<location LSID="urn:lsid:plazi:treatment:03C4331ECF1D780CF0A6A3CE2194FE06:8EB2D4D3CF1A780CF3CAA70F24F4FE93" box="[1019,1238,260,285]" country="Iceland" county="southern Rockall Trough" latitude="63.133335" longLatPrecision="1015" longitude="-15.666667" municipality="Permanent station" name="Porcupine Seabight" pageId="20" pageNumber="321">Porcupine Seabight</location>
in depth down to 
<elevation box="[151,236,295,320]" metricMagnitude="3" metricUnit="m" metricValue="3.55" pageId="20" pageNumber="321" unit="m" value="3550.0">
<quantity box="[151,236,295,320]" metricMagnitude="3" metricUnit="m" metricValue="3.55" pageId="20" pageNumber="321" unit="m" value="3550.0">3550 m</quantity>
</elevation>
(
<bibRefCitation author="Lincoln" box="[252,420,295,320]" pageId="20" pageNumber="321" refString="Lincoln, R. J. (1985 b) Deep-sea asellote isopods of the north-east Atlantic: the family Haploniscidae. Journal of Natural History, 19, 655 - 695." type="journal article" year="1985" yearSuffix="b">Lincoln 1985b</bibRefCitation>
