<subSubSection box="[199,1127,187,213]" pageId="14" pageNumber="40" type="etymology">
<paragraph blockId="14.[151,1437,151,1004]" box="[199,1127,187,213]" pageId="14" pageNumber="40">
<emphasis bold="true" box="[199,354,187,212]" pageId="14" pageNumber="40">Etymology. “</emphasis>
