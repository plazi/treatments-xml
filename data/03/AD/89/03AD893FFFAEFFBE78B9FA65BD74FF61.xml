<taxonomicName authority="Linnæus, 1758" authorityName="Linnæus" authorityYear="1758" box="[415,547,1768,1792]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="siro">Acarus siro</taxonomicName>
