) on 
<emphasis box="[980,1428,1914,1937]" italics="true" pageId="5" pageNumber="6">
<taxonomicName box="[980,1103,1914,1937]" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="genus">Nothofagus</taxonomicName>
........................... 
<taxonomicName box="[1301,1428,1914,1937]" class="Insecta" family="Cynipidae" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="tribe" tribe="Paraulacini">Paraulacini</taxonomicName>
</emphasis>
</keyLead>
</paragraph>
