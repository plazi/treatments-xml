<subSubSection box="[151,848,1807,1832]" pageId="1" pageNumber="34" type="materials_examined">
<paragraph blockId="1.[151,1436,1807,2013]" box="[151,848,1807,1832]" pageId="1" pageNumber="34">
<emphasis bold="true" box="[151,308,1807,1832]" pageId="1" pageNumber="34">
