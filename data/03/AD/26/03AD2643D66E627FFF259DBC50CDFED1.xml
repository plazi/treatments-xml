<paragraph blockId="1.[151,564,1736,1763]" box="[151,564,1736,1763]" pageId="1" pageNumber="34">
<heading bold="true" box="[151,564,1736,1763]" fontSize="11" level="1" pageId="1" pageNumber="34" reason="1">
<emphasis bold="true" box="[151,564,1736,1763]" pageId="1" pageNumber="34">
<taxonomicName authority="Kruse" authorityName="Kruse" box="[151,422,1736,1763]" class="Insecta" family="Tortricidae" genus="Amorbimorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="34" phylum="Arthropoda" rank="genus">
