<collectionCode box="[1141,1219,1238,1264]" country="Serbia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14477" lsid="urn:lsid:biocol.org:col:14477" name="University of Belgrade" pageId="1" pageNumber="58" type="Herbarium">BEOU</collectionCode>
