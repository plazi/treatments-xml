<collectingCountry box="[513,614,1605,1631]" name="Brazil" pageId="14" pageNumber="207">BRAZIL</collectingCountry>
. 
<emphasis bold="true" box="[624,747,1604,1630]" pageId="14" pageNumber="207">
<collectingRegion box="[624,742,1604,1630]" country="Brazil" name="Sao Paulo" pageId="14" pageNumber="207">São Paulo</collectingRegion>
:
