<location LSID="urn:lsid:plazi:treatment:03ADE338FF9BFFB5C9C0FA5AFCA2B357:8EDB04F5FF9BFFB5CBB9F987FCE8B23A" box="[753,854,1604,1630]" country="Brazil" name="Eldorado" pageId="14" pageNumber="207" stateProvince="Sao Paulo">Eldorado</location>
, 
<date box="[864,1045,1604,1630]" pageId="14" pageNumber="207" value="2012-10-11">
<collectingDate box="[864,1045,1604,1630]" pageId="14" pageNumber="207" value="2012-10-11">11 October 2012</collectingDate>
