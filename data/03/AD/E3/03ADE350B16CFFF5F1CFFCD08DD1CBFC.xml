<taxonomicName baseAuthorityName="Vari" baseAuthorityYear="1961" box="[412,539,1446,1472]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="127" pageNumber="128" phylum="Arthropoda" rank="species" species="melhaniae">
<emphasis bold="true" box="[412,539,1446,1472]" italics="true" pageId="127" pageNumber="128">melhaniae</emphasis>
</taxonomicName>
group based on external characters
</emphasis>
</heading>
</paragraph>
</subSubSection>
<subSubSection pageId="127" pageNumber="128" type="key">
<keyStep pageId="127" pageNumber="128">
<paragraph blockId="127.[151,1437,1517,1685]" pageId="127" pageNumber="128">
<keyLead pageId="127" pageNumber="128">
