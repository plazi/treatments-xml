<paragraph blockId="53.[151,401,151,177]" box="[151,401,151,177]" pageId="53" pageNumber="54">
<heading bold="true" box="[151,401,151,177]" fontSize="11" level="1" pageId="53" pageNumber="54" reason="1">
<emphasis bold="true" box="[151,401,151,177]" pageId="53" pageNumber="54">
The 
<taxonomicName authorityName="De Prins" authorityYear="2012" box="[207,319,151,177]" class="Magnoliopsida" family="Apocynaceae" genus="Cameraria" kingdom="Plantae" order="Gentianales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="species" species="torridella">
<emphasis bold="true" box="[207,319,151,177]" italics="true" pageId="53" pageNumber="54">torridella</emphasis>
</taxonomicName>
