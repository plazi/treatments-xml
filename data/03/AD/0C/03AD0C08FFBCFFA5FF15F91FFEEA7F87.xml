<number box="[1136,1208,152,178]" pageId="2" pageNumber="60" value="0.858">0.858</number>
. Lengths of coxae: anterior 
<number box="[250,320,190,216]" pageId="2" pageNumber="60" value="0.49">0.490</number>
; median 
<number box="[427,493,190,216]" pageId="2" pageNumber="60" value="0.385">0.385</number>
and posterior 
<number box="[664,734,190,216]" pageId="2" pageNumber="60" value="0.333">0.333</number>
. Femur lengths: anterior 
<number box="[1031,1100,190,216]" pageId="2" pageNumber="60" value="1.085">1.085</number>
, median 
<number box="[1207,1273,190,216]" pageId="2" pageNumber="60" value="1.103">1.103</number>
and posterior 
<number box="[151,221,229,255]" pageId="2" pageNumber="60" value="1.05">1.050</number>
. Tibia lengths: anterior 
<number box="[501,570,229,255]" pageId="2" pageNumber="60" value="1.435">1.435</number>
, median 
<number box="[677,743,229,255]" pageId="2" pageNumber="60" value="1.61">1.610</number>
and posterior 
<number box="[913,983,229,255]" pageId="2" pageNumber="60" value="1.715">1.715</number>
. Anterior tarsomere lengths: TI 
<number box="[1362,1431,229,255]" pageId="2" pageNumber="60" value="0.178">0.178</number>
; TII 
<number box="[197,267,268,294]" pageId="2" pageNumber="60" value="0.413">0.413</number>
; 
<collectionCode box="[280,328,268,294]" pageId="2" pageNumber="60">TIII</collectionCode>
<number box="[336,406,268,294]" pageId="2" pageNumber="60" value="0.228">0.228</number>
; TIV 
<number box="[476,543,268,294]" pageId="2" pageNumber="60" value="0.14">0.140</number>
and TV 
<number box="[650,721,268,294]" pageId="2" pageNumber="60" value="0.105">0.105</number>
. Median tarsomere lengths: TI 
<number box="[1093,1163,268,294]" pageId="2" pageNumber="60" value="0.77">0.770</number>
; TII 
<number box="[1222,1292,268,294]" pageId="2" pageNumber="60" value="0.42">0.420</number>
; 
<collectionCode box="[1305,1353,268,294]" pageId="2" pageNumber="60">TIII</collectionCode>
<number box="[1361,1431,268,294]" pageId="2" pageNumber="60" value="0.245">0.245</number>
; TIV 
<number box="[209,276,306,332]" pageId="2" pageNumber="60" value="0.14">0.140</number>
and TV 
<number box="[383,454,306,332]" pageId="2" pageNumber="60" value="0.105">0.105</number>
. Posterior tarsomeres lengths: TI 
<number box="[858,929,306,332]" pageId="2" pageNumber="60" value="0.788">0.788</number>
; TII 
<number box="[988,1059,306,332]" pageId="2" pageNumber="60" value="0.42">0.420</number>
; 
<collectionCode box="[1072,1120,306,332]" pageId="2" pageNumber="60">TIII</collectionCode>
<number box="[1128,1199,306,332]" pageId="2" pageNumber="60" value="0.252">0.252</number>
; TIV 
<number box="[1270,1337,306,332]" pageId="2" pageNumber="60" value="0.14">0.140</number>
and TV 
<number box="[151,221,345,371]" pageId="2" pageNumber="60" value="0.105">0.105</number>
. Wing 
<number box="[305,372,345,371]" pageId="2" pageNumber="60" value="2.66">2.660</number>
long, 
<number box="[449,515,345,371]" pageId="2" pageNumber="60" value="0.42">0.420</number>
