</taxonomicName>
<emphasis bold="true" box="[924,950,1184,1206]" pageId="14" pageNumber="239">3Ι</emphasis>
<taxonomicName authorityName="Smith" authorityYear="1906" box="[956,1045,1184,1206]" class="Insecta" family="Noctuidae" genus="Ursogastra" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="239" phylum="Arthropoda" rank="species" species="lunata">
