<number box="[440,498,1333,1357]" pageId="18" pageNumber="19" value="1821.0">1821</number>
</taxonomicName>
, 
<taxonomicName authority="Topsent, 1892" authorityName="Topsent" authorityYear="1892" box="[512,846,1333,1357]" class="Demospongiae" family="Coelosphaeridae" genus="Lissodendoryx" kingdom="Animalia" order="Poecilosclerida" pageId="18" pageNumber="19" phylum="Porifera" rank="genus">
