<taxonomicName baseAuthorityName="Sjostedt" baseAuthorityYear="1896" box="[1147,1302,1930,1957]" class="Insecta" family="Termitidae" genus="Cubitermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="fungifaber">
