<taxonomicName baseAuthorityName="Sjostedt" baseAuthorityYear="1896" box="[1110,1232,2001,2027]" class="Insecta" family="Termitidae" genus="Cubitermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="fungifaber">
