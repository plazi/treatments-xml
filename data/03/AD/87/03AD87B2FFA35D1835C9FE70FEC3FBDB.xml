</bibRefCitation>
, as 
<taxonomicName authority="Schmitt, 1924" authorityName="Schmitt" authorityYear="1924" box="[662,1005,826,851]" class="Malacostraca" family="Hippolytidae" genus="Hippolyte" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="curacaoense">
