<typeStatus box="[190,292,1965,1991]" pageId="3" pageNumber="52">Paratype</typeStatus>
yellowish (M 
<number box="[462,489,1965,1991]" pageId="3" pageNumber="52" value="10.0">10</number>
YR - 
<number box="[555,570,1965,1991]" pageId="3" pageNumber="52" value="7.0">7</number>
