<collectingCountry box="[478,607,188,212]" name="Philippines" pageId="2" pageNumber="276">Philippines</collectingCountry>
, 
<collectingCountry box="[626,731,188,212]" name="Malaysia" pageId="2" pageNumber="276">Malaysia</collectingCountry>
, 
<collectingCountry box="[751,863,188,212]" name="Indonesia" pageId="2" pageNumber="276">Indonesia</collectingCountry>
, 
<collectingCountry box="[883,968,188,212]" name="Taiwan" pageId="2" pageNumber="276">Taiwan</collectingCountry>
, 
<collectingCountry box="[988,1052,188,211]" name="Japan" pageId="2" pageNumber="276">Japan</collectingCountry>
