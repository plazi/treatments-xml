<materialsCitation collectingDate="2002-12-06" collectingDateMax="2002-12-12" collectingDateMin="2002-12-06" country="Brazil" county="Right" location="Universidade Federal" municipality="Deposited" pageId="6" pageNumber="31" specimenCount="1" stateProvince="Espirito Santo" typeStatus="holotype">
