<subSubSection box="[151,734,151,178]" pageId="15" pageNumber="56" type="nomenclature">
<paragraph blockId="15.[151,734,151,178]" box="[151,734,151,178]" pageId="15" pageNumber="56">
<heading bold="true" box="[151,734,151,178]" fontSize="11" level="1" pageId="15" pageNumber="56" reason="1">
<emphasis bold="true" box="[151,734,151,178]" pageId="15" pageNumber="56">
Genus 
<taxonomicName authority="Khalaim &amp; Ward" authorityName="Khalaim &amp; Ward" authorityYear="2018" box="[238,611,151,178]" class="Insecta" family="Ichneumonidae" genus="Barycnellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="56" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[238,383,151,177]" italics="true" pageId="15" pageNumber="56">Barycnellus</emphasis>
Khalaim &amp; Ward
</taxonomicName>
, 
<taxonomicNameLabel box="[625,734,152,178]" pageId="15" pageNumber="56" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
