), 
<taxonomicName box="[1338,1436,1864,1887]" class="Insecta" family="Chrysomelidae" genus="Iucetima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="69" phylum="Arthropoda" rank="genus">
