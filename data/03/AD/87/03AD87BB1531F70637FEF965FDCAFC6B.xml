<paragraph blockId="4.[151,441,1776,1837]" box="[151,441,1776,1802]" pageId="4" pageNumber="37">
<heading bold="true" box="[151,441,1776,1802]" fontSize="11" level="1" pageId="4" pageNumber="37" reason="1">
<emphasis bold="true" box="[151,441,1776,1802]" pageId="4" pageNumber="37">
<taxonomicName box="[151,368,1776,1802]" class="Insecta" family="Diadocidiidae" genus="Diadocidia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="37" phylum="Arthropoda" rank="species" species="similis" status="sp. n.">
<emphasis bold="true" box="[151,368,1776,1802]" italics="true" pageId="4" pageNumber="37">Diadocidia similis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[375,441,1776,1802]" pageId="4" pageNumber="37" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
