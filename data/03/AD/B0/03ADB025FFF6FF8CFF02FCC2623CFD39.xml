<taxonomicName authority="Mojica &amp; Jensen &amp; Caira, 2013" authorityName="Mojica &amp; Jensen &amp; Caira" authorityYear="2013" box="[1015,1213,2065,2085]" class="Cestoda" family="Polypocephalidae" genus="Anteropora" kingdom="Animalia" order="Lecanicephalidea" pageId="7" pageNumber="498" phylum="Platyhelminthes" rank="species" species="patulobothridium" status="new species">
