<quantity box="[681,777,1527,1553]" metricMagnitude="3" metricUnit="m" metricValue="3.5" pageId="13" pageNumber="14" unit="m" value="3500.0">
<pagination box="[681,740,1527,1553]" pageId="13" pageNumber="14" type="pagination">3500</pagination>
m
</quantity>
, 
<date box="[793,922,1527,1553]" pageId="13" pageNumber="14" value="1987-06-23">
<number box="[793,825,1527,1553]" pageId="13" pageNumber="14" value="23.0">23</number>
