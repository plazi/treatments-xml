<taxonomicName authorityName="Copeland" authorityYear="1947" baseAuthorityName="Kunze" box="[501,613,912,937]" class="Polypodiopsida" family="Gleicheniaceae" genus="Sticherus" kingdom="Plantae" order="Gleicheniales" pageId="48" pageNumber="49" phylum="Tracheophyta" rank="species" species="velatus">
