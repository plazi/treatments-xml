(
<figureCitation box="[159,266,186,210]" captionStart="FIGURES 7 – 8" captionStartId="7.[151,264,1961,1984]" captionTargetBox="[151,1435,700,1933]" captionTargetId="figure@7.[151,1436,700,1938]" captionTargetPageId="7" captionText="FIGURES 7 – 8. Lagideus flavus. 7, Lateral view. 8, Dorsal view of head and thorax." httpUri="https://zenodo.org/record/281908/files/figure.png" pageId="8" pageNumber="9">
Figs. 
<number box="[223,266,186,210]" pageId="8" pageNumber="9" value="7.8">7, 8</number>
</figureCitation>
, 
<figureCitation box="[278,307,186,210]" captionStart="FIGURES 19 – 23" captionStartId="14.[151,264,1839,1862]" captionTargetBox="[192,1399,200,1781]" captionTargetId="figure@14.[168,1419,193,1792]" captionTargetPageId="14" captionText="FIGURES 19 – 23. Female lancets. 19, Lagideus flavus. 20, L. albitarsis. 21, L. longicus. 22, L. tapanti. 23, L. isidro." httpUri="https://zenodo.org/record/281913/files/figure.png" pageId="8" pageNumber="9">
