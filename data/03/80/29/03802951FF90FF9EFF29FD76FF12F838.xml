<paragraph blockId="11.[151,594,727,786]" box="[151,428,762,786]" pageId="11" pageNumber="12">
(
<figureCitation box="[159,294,762,786]" captionStart="FIGURES 13 – 14" captionStartId="12.[151,264,1270,1293]" captionTargetBox="[201,1393,194,1234]" captionTargetId="figure@12.[187,1399,193,1248]" captionTargetPageId="12" captionText="FIGURES 13 – 14. Lagideus tapanti. 13, Lateral view. 14, Dorsal view of head and thorax." httpUri="https://zenodo.org/record/281911/files/figure.png" pageId="11" pageNumber="12">
Figs. 
<number box="[222,294,762,786]" pageId="11" pageNumber="12" value="13.14">13, 14</number>
</figureCitation>
, 
<number box="[306,378,762,786]" pageId="11" pageNumber="12" value="22.3">
<figureCitation box="[306,337,762,786]" captionStart="FIGURES 19 – 23" captionStartId="14.[151,264,1839,1862]" captionTargetBox="[192,1399,200,1781]" captionTargetId="figure@14.[168,1419,193,1792]" captionTargetPageId="14" captionText="FIGURES 19 – 23. Female lancets. 19, Lagideus flavus. 20, L. albitarsis. 21, L. longicus. 22, L. tapanti. 23, L. isidro." httpUri="https://zenodo.org/record/281913/files/figure.png" pageId="11" pageNumber="12">22</figureCitation>
, 
<figureCitation box="[348,378,762,786]" captionStart="FIGURES 27 – 34" captionStartId="16.[151,264,1893,1916]" captionTargetBox="[158,1427,196,1864]" captionTargetId="figure@16.[151,1436,193,1872]" captionTargetPageId="16" captionText="FIGURES 27 – 34. Male genitalia. 27 – 30, Genital capsules, ventral view. 31 – 34, Penis valves, lateral view. 27, 31, Lagideus boyaca. 28, 32, L. schmidti. 29, 33, L. longicus. 30, 34, L. tapanti." httpUri="https://zenodo.org/record/281915/files/figure.png" pageId="11" pageNumber="12">30</figureCitation>
</number>
