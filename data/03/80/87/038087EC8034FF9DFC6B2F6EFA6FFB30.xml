<taxonomicName authority="Fontes" authorityName="Fontes" authorityYear="1979" box="[1054,1304,1114,1139]" class="Insecta" family="Termitidae" genus="Atlantitermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="guarinim">
