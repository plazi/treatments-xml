Fontes
</taxonomicName>
............... 
<taxonomicName authority="Kistner" authorityName="Kistner" authorityYear="1993" box="[1362,1493,570,595]" class="Insecta" family="Staphylinidae" genus="Fonsechellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="fontesi">
