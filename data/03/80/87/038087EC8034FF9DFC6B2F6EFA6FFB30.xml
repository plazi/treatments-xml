<taxonomicName authorityName="Holmgren" authorityYear="1910" box="[1321,1425,922,947]" class="Insecta" family="Termitidae" genus="Subulitermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[1321,1425,922,947]" italics="true" pageId="2" pageNumber="3">Subulitermes</emphasis>
</taxonomicName>
sp........ .................................................................................... 
<taxonomicName authority="Seevers" authorityName="Seevers" authorityYear="1957" box="[1357,1493,954,979]" class="Insecta" family="Staphylinidae" genus="Fonsechellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="fragilis">
<emphasis bold="true" box="[1357,1426,954,979]" italics="true" pageId="2" pageNumber="3">F.fragilis</emphasis>
Seevers
</taxonomicName>
</keyLead>
</emphasis>
</paragraph>
