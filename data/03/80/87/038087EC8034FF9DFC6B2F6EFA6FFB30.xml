<emphasis bold="true" box="[1113,1194,1018,1043]" italics="true" pageId="2" pageNumber="3">
<taxonomicName authorityName="Seevers" authorityYear="1957" box="[1113,1191,1018,1043]" class="Insecta" family="Staphylinidae" genus="Fonsechellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="fragilis">F. fragilis</taxonomicName>
