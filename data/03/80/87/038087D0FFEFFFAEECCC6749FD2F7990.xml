<subSubSection box="[199,1372,791,818]" pageId="10" pageNumber="11" type="etymology">
<paragraph blockId="10.[151,1437,151,978]" box="[199,1372,791,818]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[199,339,791,817]" pageId="10" pageNumber="11">Etymology.</emphasis>
