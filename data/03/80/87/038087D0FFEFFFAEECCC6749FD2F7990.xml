<emphasis box="[1263,1372,792,817]" italics="true" pageId="10" pageNumber="11">
<taxonomicName box="[1263,1368,792,817]" class="Magnoliopsida" family="Fabaceae" genus="Dalbergia" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="hupeana">hupeana</taxonomicName>
