<collectingCountry box="[521,606,1132,1157]" name="Bolivia" pageId="2" pageNumber="392">Bolivia</collectingCountry>
, Pacific and Andean regions of 
<collectingCountry box="[990,1103,1132,1157]" name="Colombia" pageId="2" pageNumber="392">Colombia</collectingCountry>
, southeastern 
<collectingCountry box="[1270,1341,1132,1157]" name="Brazil" pageId="2" pageNumber="392">Brazil</collectingCountry>
, eastern 
<collectingCountry box="[151,246,1167,1192]" name="Ecuador" pageId="2" pageNumber="392">Ecuador</collectingCountry>
, and the central region of 
<collectingCountry box="[538,589,1168,1191]" name="Peru" pageId="2" pageNumber="392">Peru</collectingCountry>
(
<bibRefCitation box="[603,774,1167,1192]" pageId="2" pageNumber="392" refId="ref6798" refString="Carvalho, J. C. M. (1990) Mirideos Neotropicais, CCCVII: novas especies da Argentina e Bolivia (Hemiptera). Revista Brasileira de Entomologia, 34, 445 - 452." type="journal article">Carvalho 1990</bibRefCitation>
