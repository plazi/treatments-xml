</taxonomicName>
, 
<taxonomicName box="[729,898,2000,2025]" class="Polychaeta" family="Terebellidae" genus="Streblosoma" kingdom="Animalia" order="Terebellida" pageId="3" pageNumber="513" phylum="Annelida" rank="species" species="porchatensis">
