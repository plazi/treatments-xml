. 
<emphasis box="[1050,1126,188,213]" italics="true" pageId="4" pageNumber="514">hesslei</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1140,1310,188,213]" class="Polychaeta" family="Terebellidae" genus="Streblosoma" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="514" phylum="Annelida" rank="species" species="porchatensis">
