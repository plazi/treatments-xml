<emphasis box="[1324,1338,189,212]" italics="true" pageId="4" pageNumber="514">S</emphasis>
. 
<emphasis box="[1353,1432,188,213]" italics="true" pageId="4" pageNumber="514">toddae</emphasis>
</taxonomicName>
<emphasis box="[1432,1436,188,213]" italics="true" pageId="4" pageNumber="514">,</emphasis>
and 
<taxonomicName box="[200,339,224,248]" class="Polychaeta" family="Terebellidae" genus="Streblosoma" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="514" phylum="Annelida" rank="species" species="uncinatus">
<emphasis box="[200,214,224,247]" italics="true" pageId="4" pageNumber="514">S</emphasis>
. 
<emphasis box="[230,339,225,248]" italics="true" pageId="4" pageNumber="514">uncinatus</emphasis>
</taxonomicName>
