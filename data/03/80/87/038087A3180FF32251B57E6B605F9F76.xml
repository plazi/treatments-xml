<quantity box="[698,872,151,176]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" metricValueMax="1.94" metricValueMin="0.45999999999999996" pageId="25" pageNumber="406" unit="mm" value="12.0" valueMax="19.4" valueMin="4.6">4.6 to 19.4 mm</quantity>
