<emphasis box="[508,527,225,248]" italics="true" pageId="25" pageNumber="406">=</emphasis>
<number box="[534,564,224,248]" pageId="25" pageNumber="406" value="10.0">10</number>
, t 
<emphasis box="[590,609,225,248]" italics="true" pageId="25" pageNumber="406">=</emphasis>
- 
<number box="[623,705,224,248]" pageId="25" pageNumber="406" value="0.0212">0.0212</number>
, p 
<emphasis box="[736,755,225,248]" italics="true" pageId="25" pageNumber="406">=</emphasis>
