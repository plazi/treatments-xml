<quantity box="[1195,1340,880,905]" metricMagnitude="2" metricUnit="m" metricValue="4.885" metricValueMax="9.32" metricValueMin="0.45" pageId="15" pageNumber="396" unit="m" value="488.5" valueMax="932.0" valueMin="45.0">45 to 932 m</quantity>
