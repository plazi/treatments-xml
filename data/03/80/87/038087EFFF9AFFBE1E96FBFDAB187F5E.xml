<typeStatus box="[158,256,1930,1953]" pageId="19" pageNumber="20">paratype</typeStatus>
, Ural; slide KN 
<number box="[437,446,1930,1953]" pageId="19" pageNumber="20" value="1.0">1</number>
/ 
<date box="[455,583,1930,1953]" pageId="19" pageNumber="20" value="2008-04-06">
06.IV. 
<number box="[523,583,1930,1953]" pageId="19" pageNumber="20" value="2008.0">2008</number>
</date>
); 
<number box="[602,632,1930,1953]" pageId="19" pageNumber="20" value="40.0">40</number>
. Abdominal segment VIII (right: sternum; left: tergum; 
<typeStatus box="[1247,1344,1930,1953]" pageId="19" pageNumber="20">paratype</typeStatus>
