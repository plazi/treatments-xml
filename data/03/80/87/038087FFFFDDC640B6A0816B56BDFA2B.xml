. 
<taxonomicName baseAuthorityName="Hall" baseAuthorityYear="1913" box="[299,571,1882,1906]" class="Pycnogonida" family="Callipallenidae" genus="Callipallene" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="22" pageNumber="173" phylum="Arthropoda" rank="species" species="californiensis">
