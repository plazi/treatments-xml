<quantity box="[1196,1349,1162,1187]" metricMagnitude="2" metricUnit="m" metricValue="3.8899999999999997" metricValueMax="5.029999999999999" metricValueMin="2.75" pageId="19" pageNumber="400" unit="m" value="389.0" valueMax="503.0" valueMin="275.0">275 to 503 m</quantity>
