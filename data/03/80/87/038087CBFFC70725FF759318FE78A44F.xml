<collectionCode box="[1153,1234,295,321]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="42" pageNumber="43" type="Museum">BPBM</collectionCode>
