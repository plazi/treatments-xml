<subSubSection box="[198,1150,611,638]" pageId="16" pageNumber="17" type="etymology">
<paragraph blockId="16.[151,1437,456,1643]" box="[198,1150,611,638]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[198,331,611,637]" pageId="16" pageNumber="17">Etymology</emphasis>
