<collectionCode box="[454,509,1280,1304]" country="Italy" lsid="urn:lsid:biocol.org:col:15292" name="Herbarium Mediterraneum Panormitanum" pageId="67" pageNumber="2244" type="Herbarium">PAL</collectionCode>
