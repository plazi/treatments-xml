<bibRefCitation author="Mortensen, T." box="[1065,1265,1488,1512]" pageId="13" pageNumber="14" pagination="1 - 15" refId="ref35426" refString="Mortensen, T. (1948 c) Report on the Echinoidea of the Murray Expedition. II [Irregular Echinoidea]. Scientific Reports on the John Murray Expedition, 9, 1 - 15." type="journal article" year="1948">Mortensen 1948c</bibRefCitation>
), 
<collectingCountry box="[1285,1394,1487,1512]" name="Mauritius" pageId="13" pageNumber="14">Mauritius</collectingCountry>
(de 
<bibRefCitation author="De Loriol, P." box="[151,286,1521,1546]" pageId="13" pageNumber="14" pagination="1 - 64" refId="ref34555" refString="De Loriol, P. (1883) Catalogue raisonne des Echinodermes recueillis par MV de Robillard a l'ile Maurice. Memoires de la Societe de physique et d'histoire naturelle de Geneve, 28, 1 - 64." type="journal article" year="1883">Loriol 1883</bibRefCitation>
; 
<bibRefCitation author="Clark, H. L." box="[298,438,1521,1546]" pageId="13" pageNumber="14" refId="ref34415" refString="Clark, H. L. (1925 a) A Catalogue of the Recent Sea-Urchins (Echinoidea) in the Collection of the British Museum (Natural History). Oxford University Press, London, 250 pp." type="book" year="1925">Clark 1925a</bibRefCitation>
