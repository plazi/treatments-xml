<taxonomicName authorityName="Davids" authorityYear="1997" baseAuthorityName="Charp." box="[151,436,1283,1308]" class="Insecta" family="Coenagrionidae" genus="Enallagma" kingdom="Animalia" order="Odonata" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="cyathigerum">Enallagma cyathigerum</taxonomicName>
,
</emphasis>
<taxonomicName authority="(Vander L.)" baseAuthorityName="Vander L." box="[448,791,1282,1308]" class="Insecta" family="Coenagrionidae" genus="Ischnura" kingdom="Animalia" order="Odonata" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="elegans">
