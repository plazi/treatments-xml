<collectingRegion box="[358,571,1540,1566]" country="India" name="Arunachal Pradesh" pageId="4" pageNumber="567">Arunachal Pradesh</collectingRegion>
(
<collectingCountry box="[585,646,1540,1567]" name="India" pageId="4" pageNumber="567">India</collectingCountry>
) (
<figureCitation box="[669,752,1540,1567]" captionStart="FIGURES 81" captionStartId="14.[151,264,1214,1239]" captionTargetBox="[197,1390,384,1186]" captionTargetId="figure@14.[193,1394,381,1189]" captionTargetPageId="14" captionText="FIGURES 81. Distribution map of Baetiella in Arunachal Pradesh, India." figureDoi="http://doi.org/10.5281/zenodo.3762186" httpUri="https://zenodo.org/record/3762186/files/figure.png" pageId="4" pageNumber="567">Fig. 81</figureCitation>
