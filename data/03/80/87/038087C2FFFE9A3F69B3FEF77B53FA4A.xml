<collectorName box="[1136,1351,1183,1208]" pageId="27" pageNumber="328">S.E. Fond Verettes</collectorName>
, 6500’, 
<collectingDate box="[151,290,1220,1245]" pageId="27" pageNumber="328" value="1956-07-19">
<date box="[151,290,1220,1245]" pageId="27" pageNumber="328" value="1956-07-19">19.VII.1956</date>
</collectingDate>
, at night, 
<collectorName box="[400,600,1220,1245]" pageId="27" pageNumber="328">B. &amp; B. Valentine</collectorName>
(
<specimenCount box="[616,729,1220,1245]" pageId="27" pageNumber="328" type="female">2 females</specimenCount>
