<number box="[1182,1230,1874,1898]" pageId="2" pageNumber="449" value="0.13">0.13</number>
, PME–PLE 
<number box="[1380,1432,1873,1898]" pageId="2" pageNumber="449" value="0.06">0.06</number>
, ALE–PLE 
<number box="[276,328,1910,1934]" pageId="2" pageNumber="449" value="0.11">0.11</number>
. MOA 
<number box="[412,461,1910,1934]" pageId="2" pageNumber="449" value="0.27">0.27</number>
long, front width 
<number box="[665,717,1910,1934]" pageId="2" pageNumber="449" value="0.23">0.23</number>
, back width 
<number box="[860,913,1909,1934]" pageId="2" pageNumber="449" value="0.35">0.35</number>
