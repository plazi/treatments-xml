, 
<number box="[276,330,260,284]" pageId="3" pageNumber="450" value="0.8">0.80</number>
); III 
<number box="[382,431,260,285]" pageId="3" pageNumber="450" value="4.6">4.60</number>
(
<number box="[446,498,260,284]" pageId="3" pageNumber="450" value="1.2">1.20</number>
