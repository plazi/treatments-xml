<taxonomicName authority="A. Milne-Edwards, 1880" authorityName="A. Milne-Edwards" authorityYear="1880" box="[151,756,1718,1744]" class="Malacostraca" family="Chirostylidae" genus="Gastroptychus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="385" phylum="Arthropoda" rank="species" species="spinifer">
<emphasis bold="true" box="[151,756,1718,1744]" pageId="4" pageNumber="385">
