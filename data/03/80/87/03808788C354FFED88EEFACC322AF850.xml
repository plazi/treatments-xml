<subSubSection pageId="9" pageNumber="568" type="biology_ecology">
<paragraph blockId="9.[151,1437,151,1976]" box="[199,658,1483,1508]" pageId="9" pageNumber="568">
<emphasis bold="true" box="[199,295,1483,1508]" pageId="9" pageNumber="568">Biology.</emphasis>
