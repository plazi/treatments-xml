(WUSL/EI/65); B, 
<emphasis box="[346,481,1102,1124]" italics="true" pageId="58" pageNumber="59">Echinodiscus</emphasis>
<taxonomicName authorityName="L. Agassiz" authorityYear="1841" box="[486,615,1102,1125]" class="Echinoidea" family="Fibulariidae" genus="Echinocyamus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clypeasteroida" pageId="58" pageNumber="59" phylum="Echinodermata" rank="species" species="truncatus">
