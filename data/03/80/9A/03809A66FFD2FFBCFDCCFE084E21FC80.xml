<quantity box="[376,514,523,549]" metricMagnitude="-1" metricUnit="m" metricValue="3.1399999999999997" metricValueMax="3.36" metricValueMin="2.92" pageId="3" pageNumber="2539" unit="mm" value="314.0" valueMax="336.0" valueMin="292.0">292–336 mm</quantity>
