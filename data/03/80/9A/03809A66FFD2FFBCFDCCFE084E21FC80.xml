(
<typeStatus box="[840,945,524,548]" pageId="3" pageNumber="2539">holotype</typeStatus>
: 306/ 
<quantity box="[1010,1091,523,549]" metricMagnitude="-1" metricUnit="m" metricValue="1.44" pageId="3" pageNumber="2539" unit="mm" value="144.0">144 mm</quantity>
) (
<emphasis box="[1120,1133,525,549]" italics="true" pageId="3" pageNumber="2539">n</emphasis>
<specimenCount pageId="3" pageNumber="2539" type="generic" typeStatus="holotype">51 holotype</specimenCount>
