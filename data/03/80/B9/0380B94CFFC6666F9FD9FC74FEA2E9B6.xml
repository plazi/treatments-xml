<taxonomicName authorityName="S.I.Smith" authorityYear="1869" box="[363,567,1368,1394]" class="Malacostraca" family="Portunidae" genus="Callinectes" kingdom="Animalia" order="Decapoda" pageId="10" pageNumber="461" phylum="Arthropoda" rank="species" species="danae">
<emphasis box="[363,567,1368,1394]" italics="true" pageId="10" pageNumber="461">Callinectes danae</emphasis>
</taxonomicName>
is found in the Western Atlantic: 
<collectingCountry box="[967,1073,1368,1394]" name="Bermuda" pageId="10" pageNumber="461">Bermuda</collectingCountry>
, Florida, Texas, 
<collectingCountry box="[1267,1355,1368,1394]" name="Mexico" pageId="10" pageNumber="461">Mexico</collectingCountry>
, 
<collectingCountry box="[1370,1432,1368,1394]" name="Cuba" pageId="10" pageNumber="461">Cuba</collectingCountry>
, 
<collectingCountry box="[151,244,1404,1430]" name="Jamaica" pageId="10" pageNumber="461">Jamaica</collectingCountry>
, 
<collectingCountry box="[257,316,1404,1430]" name="Haiti" pageId="10" pageNumber="461">Haiti</collectingCountry>
, 
<collectingCountry box="[330,567,1404,1430]" name="Dominican Republic" pageId="10" pageNumber="461">Dominican Republic</collectingCountry>
, 
<collectingCountry box="[580,719,1404,1431]" name="Puerto Rico" pageId="10" pageNumber="461">Puerto Rico</collectingCountry>
, St. Thomas, St Croix, St Martin, Guadeloupe, Martinique, 
<collectingCountry name="Saint Lucia" pageId="10" pageNumber="461">St. Lucia</collectingCountry>
, 
<collectingCountry box="[230,339,1440,1466]" name="Barbados" pageId="10" pageNumber="461">Barbados</collectingCountry>
, 
<collectingCountry box="[351,463,1440,1466]" name="Honduras" pageId="10" pageNumber="461">Honduras</collectingCountry>
, 
<collectingCountry box="[475,567,1441,1467]" name="Panama" pageId="10" pageNumber="461">Panama</collectingCountry>
, 
<collectingCountry box="[578,676,1440,1466]" name="Trinidad and Tobago" pageId="10" pageNumber="461">Trinidad</collectingCountry>
, Isla los Roques, Isla de Providencia, Margarita, 
<collectingCountry box="[1234,1324,1441,1467]" name="Haiti" pageId="10" pageNumber="461">Tortuga</collectingCountry>
, 
<collectingCountry box="[1336,1432,1440,1466]" name="Curaçao" pageId="10" pageNumber="461">Curaçao</collectingCountry>
, 
<collectingCountry box="[151,224,1476,1502]" name="Aruba" pageId="10" pageNumber="461">Aruba</collectingCountry>
, 
<collectingCountry box="[235,348,1476,1502]" name="Colombia" pageId="10" pageNumber="461">Colombia</collectingCountry>
, 
<collectingCountry box="[358,476,1476,1502]" name="Venezuela" pageId="10" pageNumber="461">Venezuela</collectingCountry>
, and 
<collectingCountry box="[533,601,1476,1502]" name="Brazil" pageId="10" pageNumber="461">Brazil</collectingCountry>
(
<bibRefCitation author="Rathbun, M. J." box="[616,778,1476,1502]" pageId="10" pageNumber="461" pagination="1 - 609" refId="ref23377" refString="Rathbun, M. J. (1930) The cancroid crabs of America of the families Euryalidae, Portunidae, Atelecyclidae, Cancridae, and Xanthidae. Bulletin of the United States National Museum, 152, 1 - 609. https: // doi. org / 10.5479 / si. 03629236.152. i" type="journal article" year="1930">Rathbun 1930</bibRefCitation>
; 
<bibRefCitation author="Chace, F. A. Jr. &amp; Hobbs, H. H." box="[789,1032,1476,1502]" pageId="10" pageNumber="461" pagination="1 - 244" refId="ref19308" refString="Chace, F. A. Jr. &amp; Hobbs, H. H. (1969) The freshwater and terrestrial decapod crustaceans of the West Indies with special reference to Dominica. Bulletin of the United States National Museum, 292, 1 - 244. https: // doi. org / 10.5479 / si. 03629236.292.1" type="journal article" year="1969">Chace &amp; Hobbs 1969</bibRefCitation>
; 
<bibRefCitation author="Williams, A. B." box="[1043,1210,1476,1502]" pageId="10" pageNumber="461" pagination="685 - 798" refId="ref25032" refString="Williams, A. B. (1974) The swimming crabs of the genus Callinectes (Decapoda: Portunidae). Fishery Bulletin, 72 (3), 685 - 798." type="journal article" year="1974">Williams 1974</bibRefCitation>
; 
<bibRefCitation author="Melo, G. A. S." box="[1221,1346,1476,1502]" pageId="10" pageNumber="461" refId="ref22212" refString="Melo, G. A. S. (1996) Manual de identificacao dos Brachyura (caranguejos e siris) do Litoral Brasileiro. Editora, Pleiade, Sao Paulo, 604 pp." type="book" year="1996">Melo 1996</bibRefCitation>
; 
<bibRefCitation author="Poupin, J." pageId="10" pageNumber="461" refId="ref22928" refString="Poupin, J. (2018) Les Crustaces decapodes des Petites Antilles: Avec de nouvelles observations pour Saint-Martin, la Guadeloupe et la Martinique. Publications scientifiques du Museum National d'histoire Naturelle, Paris, 264 pp." type="book" year="2018">Poupin 2018</bibRefCitation>
