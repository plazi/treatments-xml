<heading bold="true" box="[264,847,283,310]" fontSize="11" level="1" pageId="7" pageNumber="44" reason="1">
<emphasis bold="true" box="[264,847,283,310]" pageId="7" pageNumber="44">
<emphasis bold="true" box="[264,558,284,310]" italics="true" pageId="7" pageNumber="44">
<taxonomicName box="[264,552,284,310]" class="Insecta" family="Bolboceratidae" genus="Neoathyreus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="44" phylum="Arthropoda" rank="species" species="moraguesi" status="new species">Neoathyreus moraguesi</taxonomicName>
