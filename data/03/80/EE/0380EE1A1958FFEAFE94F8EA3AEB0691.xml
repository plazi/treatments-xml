<collectionCode box="[603,692,1535,1561]" pageId="2" pageNumber="39">CMNC</collectionCode>
, 
<collectionCode box="[704,790,1535,1561]" pageId="2" pageNumber="39">HAHC</collectionCode>
, 
<collectionCode box="[802,890,1535,1561]" httpUri="http://grbio.org/cool/zsd4-j9gb" name="National Kweiyang Medical College" pageId="2" pageNumber="39">NKMC</collectionCode>
