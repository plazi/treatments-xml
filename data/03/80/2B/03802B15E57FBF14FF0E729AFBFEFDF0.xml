resembles 
<taxonomicName authority="Hua &amp; Chou, 1988" authorityName="Hua &amp; Chou" authorityYear="1988" box="[787,1141,441,467]" class="Insecta" family="Bittacidae" genus="Bittacus" kingdom="Animalia" order="Mecoptera" pageId="5" pageNumber="386" phylum="Arthropoda" rank="species" species="strigatus">
