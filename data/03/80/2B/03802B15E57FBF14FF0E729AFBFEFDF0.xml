<paragraph blockId="5.[151,1437,153,612]" pageId="5" pageNumber="386">
<emphasis bold="true" box="[199,315,441,467]" pageId="5" pageNumber="386">Remarks.</emphasis>
<taxonomicName authority="Zhang &amp; Du &amp; Hua, 2020" authorityName="Zhang &amp; Du &amp; Hua" authorityYear="2020" box="[323,588,441,467]" class="Insecta" family="Bittacidae" genus="Bittacus" kingdom="Animalia" order="Mecoptera" pageId="5" pageNumber="386" phylum="Arthropoda" rank="species" species="shaoguanensis" status="sp. n.">
<emphasis box="[323,588,441,467]" italics="true" pageId="5" pageNumber="386">Bittacus shaoguanensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[595,660,441,467]" pageId="5" pageNumber="386">
<taxonomicNameLabel box="[595,660,441,467]" pageId="5" pageNumber="386" rank="species">sp. n.</taxonomicNameLabel>
