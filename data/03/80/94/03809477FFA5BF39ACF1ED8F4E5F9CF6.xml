and 
<taxonomicName box="[1120,1268,1840,1865]" class="Malacostraca" family="Porcellanidae" genus="Porcellana" kingdom="Animalia" order="Decapoda" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="curvifrons">
