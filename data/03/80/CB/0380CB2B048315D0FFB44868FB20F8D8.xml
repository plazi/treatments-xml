<taxonomicName authorityName="Deshayes" authorityYear="1834" box="[397,481,1774,1800]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="138" pageNumber="139" phylum="Mollusca" rank="genus">
