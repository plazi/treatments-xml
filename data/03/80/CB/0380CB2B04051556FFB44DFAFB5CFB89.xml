<taxonomicName authority="Philippi, 1865" authorityName="Philippi" authorityYear="1865" box="[770,1059,1086,1113]" class="Gastropoda" family="Triphoridae" genus="Tetraphora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="12" pageNumber="13" phylum="Mollusca" rank="genus">
