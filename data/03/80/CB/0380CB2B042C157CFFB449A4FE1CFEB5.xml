<subSubSection pageId="38" pageNumber="39" type="discussion">
<paragraph blockId="38.[151,1436,150,321]" pageId="38" pageNumber="39">
<emphasis bold="true" box="[151,267,259,285]" pageId="38" pageNumber="39">Remarks.</emphasis>
The genus 
<taxonomicName authorityName="Deshayes" authorityYear="1830" box="[400,504,259,285]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="38" pageNumber="39" phylum="Mollusca" rank="genus">
<emphasis box="[400,504,259,285]" italics="true" pageId="38" pageNumber="39">Triphoris</emphasis>
</taxonomicName>
