<paragraph blockId="5.[151,1437,151,824]" pageId="5" pageNumber="6">
<emphasis bold="true" box="[199,462,764,789]" pageId="5" pageNumber="6">Differential diagnosis.</emphasis>
<taxonomicName box="[469,675,764,789]" class="Insecta" family="Chrysomelidae" genus="Charaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="costatum">
