<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[199,308,1272,1297]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
