<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[199,308,1891,1916]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
