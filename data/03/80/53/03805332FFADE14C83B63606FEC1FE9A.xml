<taxonomicName authorityName="Lofego &amp; De Moraes &amp; Ochoa" authorityYear="2007" box="[301,570,1610,1633]" class="Arachnida" family="Tarsonemidae" genus="Xenotarsonemus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="cerrado">
