; GL, 
<number box="[911,1031,1428,1454]" pageId="6" pageNumber="43" value="0.27" valueMax="0.29" valueMin="0.25">0.25–0.29</number>
; PL, 
<number box="[1109,1259,1428,1454]" pageId="6" pageNumber="43" value="0.0275" valueMax="0.03" valueMin="0.025">0.025–0.030</number>
