<number box="[464,586,1348,1374]" pageId="6" pageNumber="43" value="1.16" valueMax="1.19" valueMin="1.13">1.13–1.19</number>
; PW, 
<number box="[677,798,1348,1374]" pageId="6" pageNumber="43" value="0.26" valueMax="0.28" valueMin="0.24">0.24–0.28</number>
; MW, 
<number box="[899,1020,1348,1374]" pageId="6" pageNumber="43" value="0.36" valueMax="0.38" valueMin="0.34">0.34–0.38</number>
; AWV, 
<number box="[1134,1255,1348,1374]" pageId="6" pageNumber="43" value="0.5449999999999999" valueMax="0.58" valueMin="0.51">0.51–0.58</number>
; TL, 
<number box="[264,385,1388,1414]" pageId="6" pageNumber="43" value="1.925" valueMax="2.02" valueMin="1.83">1.83–2.02</number>
. Male dimensions: TW, 
<number box="[718,840,1388,1414]" pageId="6" pageNumber="43" value="0.395" valueMax="0.41" valueMin="0.38">0.38–0.41</number>
; HL, 
<number box="[929,1050,1388,1414]" pageId="6" pageNumber="43" value="0.44999999999999996" valueMax="0.48" valueMin="0.42">0.42–0.48</number>
; CI, 
<number box="[1130,1249,1388,1414]" pageId="6" pageNumber="43" value="1.1600000000000001" valueMax="1.21" valueMin="1.11">1.11–1.21</number>
; PW, 
<number box="[264,385,1428,1454]" pageId="6" pageNumber="43" value="0.22999999999999998" valueMax="0.24" valueMin="0.22">0.22–0.24</number>
