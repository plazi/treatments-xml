<number box="[638,755,1595,1621]" pageId="9" pageNumber="46" value="1.185" valueMax="1.3" valueMin="1.07">1.07–1.30</number>
(mean= 
<number box="[860,911,1595,1621]" pageId="9" pageNumber="46" value="1.13">1.13</number>
); PW, 
<number box="[993,1114,1595,1621]" pageId="9" pageNumber="46" value="0.245" valueMax="0.26" valueMin="0.23">0.23–0.26</number>
; MW, 
<number box="[1197,1318,1595,1621]" pageId="9" pageNumber="46" value="0.355" valueMax="0.38" valueMin="0.33">0.33–0.38</number>
; AWV, 
