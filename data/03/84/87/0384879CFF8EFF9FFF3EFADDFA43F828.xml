); body without head 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="2.95" metricValueMax="3.2" metricValueMin="2.7" pageId="6" pageNumber="37" unit="mm" value="2.95" valueMax="3.2" valueMin="2.7">2.7- 3.2 mm</quantity>
; wing, measured from humeral vein, 
<quantity box="[683,825,1745,1771]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="6" pageNumber="37" unit="mm" value="1.6" valueMax="1.7" valueMin="1.5">1.5–1.7 mm</quantity>
