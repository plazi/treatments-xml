</taxonomicName>
, and 
<taxonomicName authority="Weise. This" authorityName="Weise. This" authorityYear="1905" box="[1024,1322,1247,1273]" class="Insecta" family="Chrysomelidae" genus="Stilpnaspis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="genus">
