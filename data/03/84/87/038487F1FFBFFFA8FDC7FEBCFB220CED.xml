<materialsCitation ID-GBIF-Occurrence="3311767384" collectingDate="2001-08-17" collectingDateMax="2001-08-18" collectingDateMin="2001-08-17" collectionCode="INBC" collectorName="S. A. Marshall." country="Costa Rica" location="Tapanti - Macizo de la Muerte National Park" pageId="26" pageNumber="27" specimenCount="1" specimenCount-male="1" stateProvince="Cartago" typeStatus="holotype">
