<collectingCountry box="[898,960,325,350]" name="Chad" pageId="14" pageNumber="15">Chad</collectingCountry>
, 
<collectingCountry box="[973,1060,325,350]" name="Nigeria" pageId="14" pageNumber="15">Nigeria</collectingCountry>
