<emphasis box="[1224,1428,361,386]" italics="true" pageId="14" pageNumber="15">
<taxonomicName box="[1224,1424,361,386]" class="Liliopsida" family="Poaceae" genus="Leersia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="hexandra">Leersia hexandra</taxonomicName>
