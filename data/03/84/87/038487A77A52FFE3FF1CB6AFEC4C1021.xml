<collectingCountry box="[1328,1387,325,350]" name="Democratic Republic of the Congo" pageId="14" pageNumber="15">
<collectingRegion box="[1328,1387,325,350]" country="Angola" name="Zaire" pageId="14" pageNumber="15">Zaire</collectingRegion>
</collectingCountry>
and 
<collectingCountry box="[151,289,361,386]" name="Madagascar" pageId="14" pageNumber="15">Madagascar</collectingCountry>
