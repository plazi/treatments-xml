<emphasis bold="true" box="[151,681,686,712]" pageId="17" pageNumber="18">
<taxonomicName authority="Luk &amp; Marshall" authorityName="Luk &amp; Marshall" authorityYear="2014" box="[151,602,686,712]" class="Insecta" family="Sphaeroceridae" genus="Aptilotella" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="caerulea" status="sp. n.">
<emphasis bold="true" box="[151,387,686,712]" italics="true" pageId="17" pageNumber="18">Aptilotella caerulea</emphasis>
Luk &amp; Marshall
</taxonomicName>
, 
