<quantity box="[632,789,1753,1779]" metricMagnitude="-2" metricUnit="m" metricValue="1.525" metricValueMax="1.7" metricValueMin="1.35" pageId="11" pageNumber="12" unit="mm" value="15.25" valueMax="17.0" valueMin="13.5">13.5–17 mm</quantity>
