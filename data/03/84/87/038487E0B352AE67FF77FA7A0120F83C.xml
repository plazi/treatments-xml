<subSubSection pageId="11" pageNumber="12" type="diagnosis">
<paragraph blockId="11.[151,1436,1715,1966]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[197,331,1753,1779]" pageId="11" pageNumber="12">Diagnosis:</emphasis>
