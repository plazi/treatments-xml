<taxonomicName authority="Solem, 1981" authorityName="Solem" authorityYear="1981" box="[225,468,1293,1317]" class="Gastropoda" family="Camaenidae" genus="Amplirhagada" kingdom="Animalia" order="Stylommatophora" pageId="22" pageNumber="189" phylum="Mollusca" rank="species" species="pusilla">
