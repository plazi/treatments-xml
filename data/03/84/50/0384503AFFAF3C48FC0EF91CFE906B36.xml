<subSubSection pageId="21" pageNumber="188" type="etymology">
<paragraph blockId="21.[121,749,1706,1758]" pageId="21" pageNumber="188">
<emphasis bold="true" box="[121,244,1706,1730]" pageId="21" pageNumber="188">Etymology</emphasis>
