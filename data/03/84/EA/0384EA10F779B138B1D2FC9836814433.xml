<date box="[884,1017,1158,1185]" pageId="12" pageNumber="533" value="1855-08-03">
<collectingDate box="[884,1017,1158,1185]" pageId="12" pageNumber="533" value="1855-08-03">3 Aug 1855</collectingDate>
</date>
near 
<location LSID="urn:lsid:plazi:treatment:0384EA10F779B138B1D2FC9836814433:8EF20DDDF779B138B57DFB7836CC47E3" box="[1080,1269,1159,1185]" country="Sweden" county="Berlin" municipality="Okavango River" name="Okavango River" pageId="12" pageNumber="533">Okavango River</location>
; NRM 554138 [11735], a young female collected 
<date box="[551,700,1194,1221]" pageId="12" pageNumber="533" value="1855-07-23">
<collectingDate box="[551,700,1194,1221]" pageId="12" pageNumber="533" value="1855-07-23">23 July 1855</collectingDate>
</date>
near 
<location LSID="urn:lsid:plazi:treatment:0384EA10F779B138B1D2FC9836814433:8EF20DDDF779B138B3BAFB5431844787" box="[767,957,1195,1221]" country="Sweden" county="Berlin" municipality="Okavango River" name="Okavango River" pageId="12" pageNumber="533">Okavango River</location>
; and a young male [11736] also collected 
<date box="[151,297,1230,1257]" pageId="12" pageNumber="533" value="1855-07-23">
<collectingDate box="[151,297,1230,1257]" pageId="12" pageNumber="533" value="1855-07-23">23 July 1855</collectingDate>
</date>
