<location LSID="urn:lsid:plazi:treatment:0384EA10F779B138B1D2FC9836814433:8EF20DDDF779B138B463FC0837A54753" box="[1318,1436,1015,1041]" country="Sweden" county="Berlin" municipality="Okavango River" name="According" pageId="12" pageNumber="533">According</location>
