<date box="[884,1013,1122,1149]" pageId="12" pageNumber="533" value="1855-08-03">
<collectingDate box="[884,1013,1122,1149]" pageId="12" pageNumber="533" value="1855-08-03">3 Aug 1855</collectingDate>
</date>
near 
<location LSID="urn:lsid:plazi:treatment:0384EA10F779B138B1D2FC9836814433:8EF20DDDF779B138B574FB9C36D3473F" box="[1073,1258,1123,1149]" country="Sweden" county="Berlin" municipality="Okavango River" name="Okavango River" pageId="12" pageNumber="533">Okavango River</location>
. The 
<specimenCount pageId="12" pageNumber="533" type="generic" typeStatus="paralectotypes">three paralectotypes</specimenCount>
