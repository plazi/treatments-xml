<taxonomicName box="[358,496,1707,1730]" class="Insecta" family="Hybotidae" genus="Lactistomyia" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">Lactistomyia</taxonomicName>
paranaensis
</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[648,708,1707,1731]" pageId="14" pageNumber="15">
<taxonomicNameLabel box="[648,708,1707,1731]" pageId="14" pageNumber="15" rank="species">sp. n.</taxonomicNameLabel>
