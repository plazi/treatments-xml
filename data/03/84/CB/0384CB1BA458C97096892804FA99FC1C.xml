<materialsCitation ID-GBIF-Occurrence="3387360514" box="[503,1433,402,429]" collectionCode="SMF" country="Brazil" county="Freyreiss" location="Brasilia" municipality="Georg Wilhelm" pageId="44" pageNumber="45" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
