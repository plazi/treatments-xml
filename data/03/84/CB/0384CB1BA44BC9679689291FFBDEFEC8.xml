*) (
<figureCitation box="[905,990,150,177]" captionStart="FIGURE 52" captionStartId="77.[151,250,1954,1978]" captionTargetBox="[213,1373,1110,1930]" captionTargetId="figure-54@77.[213,1373,1110,1930]" captionTargetPageId="77" captionText="FIGURE 52. Syneches Walker. Geographical records of S. angulatus Menezes &amp; Ale-Rocha, 2016; S. curvipes (Fabricius, 1805) and S. repletus Bezzi, 1909 in Brazil" figureDoi="http://doi.org/10.5281/zenodo.5560734" httpUri="https://zenodo.org/record/5560734/files/figure.png" pageId="59" pageNumber="60">Fig. 52</figureCitation>
