<collectingCountry box="[359,427,1606,1632]" name="Brazil" pageId="50" pageNumber="51">Brazil</collectingCountry>
(
<collectingRegion box="[443,504,1606,1633]" country="Brazil" name="Piaui" pageId="50" pageNumber="51">Piauí</collectingRegion>
) (
<figureCitation box="[526,611,1606,1632]" captionStart="FIGURE 49" captionStartId="76.[151,250,1010,1034]" captionTargetBox="[224,1362,181,985]" captionTargetId="figure-22@76.[224,1362,181,985]" captionTargetPageId="76" captionText="FIGURE 49. Syneches Walker. Geographical records of S. atratus Soares &amp; Ale-Rocha, 2018; S. limeirai Soares &amp; Ale-Rocha and S. nordestino sp. nov. in Brazil." figureDoi="http://doi.org/10.5281/zenodo.5579372" httpUri="https://zenodo.org/record/5579372/files/figure.png" pageId="50" pageNumber="51">Fig. 49</figureCitation>
). 
<taxonomicName authority="Soares &amp; Freitas-Silva &amp; Ale-Rocha, 2021" authorityName="Soares &amp; Freitas-Silva &amp; Ale-Rocha" authorityYear="2021" box="[630,859,1606,1632]" class="Insecta" family="Hybotidae" genus="Syneches" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="nordestino" status="sp. nov.">
