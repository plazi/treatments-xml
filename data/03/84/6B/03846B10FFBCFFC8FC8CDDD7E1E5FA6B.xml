<subSubSection pageId="8" pageNumber="9" type="etymology">
<paragraph blockId="8.[151,1437,152,1484]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[197,337,823,849]" pageId="8" pageNumber="9">Etymology.</emphasis>
