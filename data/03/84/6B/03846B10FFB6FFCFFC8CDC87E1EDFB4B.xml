<taxonomicName box="[636,870,973,998]" class="Magnoliopsida" family="Rosaceae" genus="Rhaphiolepis" kingdom="Plantae" order="Rosales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="indica">
<emphasis box="[636,870,973,998]" italics="true" pageId="15" pageNumber="16">Rhaphiolepis indica</emphasis>
</taxonomicName>
<taxonomicName box="[878,1046,973,999]" pageId="15" pageNumber="16" rank="variety" variety="umbellata">
var. 
<emphasis box="[929,1046,973,998]" italics="true" pageId="15" pageNumber="16">umbellata</emphasis>
</taxonomicName>
; 
