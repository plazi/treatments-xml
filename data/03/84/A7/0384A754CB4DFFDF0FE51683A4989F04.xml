<taxonomicName box="[151,330,312,337]" class="Arachnida" family="Eriophyidae" genus="Epicecidophyes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="clerodendris">
<emphasis box="[151,330,312,337]" italics="true" pageId="9" pageNumber="10">E. clerodendris</emphasis>
</taxonomicName>
), coxal area smooth (coxal area with short lines in 
<taxonomicName box="[925,1105,312,337]" class="Arachnida" family="Eriophyidae" genus="Epicecidophyes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="clerodendris">
<emphasis box="[925,1105,312,337]" italics="true" pageId="9" pageNumber="10">E. clerodendris</emphasis>
</taxonomicName>
), dorsal annuli with fissures (dorsal annuli smooth in 
<taxonomicName box="[444,623,352,377]" class="Arachnida" family="Eriophyidae" genus="Epicecidophyes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="clerodendris">
