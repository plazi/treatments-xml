<taxonomicName authority="Huang, 2001" authorityName="Huang" authorityYear="2001" box="[636,1091,1864,1890]" class="Arachnida" family="Eriophyidae" genus="Acaphyllisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cryptomeria">
