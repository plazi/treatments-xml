<paragraph blockId="35.[212,610,997,1021]" box="[212,610,997,1021]" pageId="35" pageNumber="1981">
<typeStatus box="[212,269,997,1021]" pageId="35" pageNumber="1981">Type</typeStatus>
genus: 
<taxonomicName authority="Forbes, 1848" authorityName="Forbes" authorityYear="1848" box="[354,605,997,1021]" class="Ascidiacea" family="Molgulidae" genus="Molgula" kingdom="Animalia" order="Stolidobranchia" pageId="35" pageNumber="1981" phylum="Chordata" rank="genus">
