<emphasis bold="true" box="[569,652,1510,1534]" pageId="5" pageNumber="134">
<taxonomicNameLabel box="[569,652,1510,1534]" pageId="5" pageNumber="134" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<emphasis bold="true" box="[667,711,1509,1534]" pageId="5" pageNumber="134">A—</emphasis>
left antenna; 
<emphasis bold="true" box="[847,889,1510,1534]" pageId="5" pageNumber="134">B—</emphasis>
abdomen; 
<emphasis bold="true" box="[998,1042,1509,1534]" pageId="5" pageNumber="134">C—</emphasis>
protibia and tarsus; 
<emphasis bold="true" box="[1249,1293,1510,1534]" pageId="5" pageNumber="134">D—</emphasis>
