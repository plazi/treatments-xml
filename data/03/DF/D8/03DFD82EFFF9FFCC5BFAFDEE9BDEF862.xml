(
<number box="[959,1013,1411,1435]" pageId="10" pageNumber="59" value="0.13">0.13</number>
); III: 
<number box="[1078,1190,1410,1435]" pageId="10" pageNumber="59" value="1.6400000000000001" valueMax="1.7" valueMin="1.58">1.58–1.70</number>
(
<number box="[1206,1260,1410,1435]" pageId="10" pageNumber="59" value="1.64">1.64</number>
); IV: 
<number box="[1324,1436,1410,1435]" pageId="10" pageNumber="59" value="0.61" valueMax="0.62" valueMin="0.6">0.60–0.62</number>
(
<number box="[159,213,1446,1471]" pageId="10" pageNumber="59" value="0.61">0.61</number>
