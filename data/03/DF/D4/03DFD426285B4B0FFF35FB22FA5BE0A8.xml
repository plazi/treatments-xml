<paragraph blockId="18.[151,1436,1376,1941]" box="[515,534,1448,1473]" pageId="18" pageNumber="219">is</paragraph>
<paragraph blockId="18.[151,1436,1376,1941]" box="[539,573,1413,1436]" pageId="18" pageNumber="219">are</paragraph>
<paragraph blockId="18.[151,1436,1376,1941]" box="[541,647,1448,1473]" pageId="18" pageNumber="219">markedly</paragraph>
<paragraph blockId="18.[151,1436,1376,1941]" box="[580,678,1412,1437]" pageId="18" pageNumber="219">typically</paragraph>
<paragraph blockId="18.[151,1436,1376,1941]" box="[656,738,1448,1473]" pageId="18" pageNumber="219">tapered</paragraph>
