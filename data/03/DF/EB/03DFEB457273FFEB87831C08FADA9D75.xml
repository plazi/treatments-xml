<taxonomicName authority="Moore, 1859" authorityName="Moore" authorityYear="1859" box="[820,1098,831,856]" class="Insecta" family="Erebidae" genus="Cyana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="439" phylum="Arthropoda" rank="species" species="arama">
