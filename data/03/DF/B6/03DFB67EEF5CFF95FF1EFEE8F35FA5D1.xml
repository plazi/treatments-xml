; 
<collectingCountry box="[1039,1183,1952,1977]" name="Sierra Leone" pageId="16" pageNumber="523">Sierra Leone</collectingCountry>
Rise, 
<quantity box="[1254,1339,1952,1977]" metricMagnitude="3" metricUnit="m" metricValue="1.2029999999999998" pageId="16" pageNumber="523" unit="m" value="1203.0">1203 m</quantity>
(present material); 
<collectingCountry box="[267,396,1988,2013]" name="Cape Verde" pageId="16" pageNumber="523">Cape Verde</collectingCountry>
Islands, 
<quantity box="[497,582,1988,2013]" metricMagnitude="3" metricUnit="m" metricValue="1.4769999999999999" pageId="16" pageNumber="523" unit="m" value="1477.0">1477 m</quantity>
(
<bibRefCitation author="Chevreux, E." box="[596,780,1988,2013]" pageId="16" pageNumber="523" pagination="81 - 97" refId="ref8822" refString="Chevreux, E. (1903) Note preliminaire sur les amphipodes de la famille des Lysianassidae recueillis par la Princesse-Alice dans les eaux profondes de l'Atlantique et de la Mediterranee. Bulletin de la Societe Zoologique de France, 28, 81 - 97." type="journal article" year="1903">Chevreux, 1903</bibRefCitation>
, 
<bibRefCitation author="Chevreux, E." box="[791,851,1988,2013]" pageId="16" pageNumber="523" pagination="1 - 214" refId="ref8868" refString="Chevreux, E. (1935) Amphipodes provenant des Campagnes scientifiques du Prince Albert I er de Monaco. Resultats des Campagnes Scientifiques accomplies par le Prince Albert I er de Monaco, 90, 1 - 214." type="journal article" year="1935">1935</bibRefCitation>
).
</paragraph>
