<taxonomicName authority="Wallich" authorityName="Wallich" box="[1076,1306,1161,1184]" class="Polypodiopsida" family="Pteridaceae" genus="Pteris" kingdom="Plantae" order="Polypodiales" pageId="1" pageNumber="53" phylum="Tracheophyta" rank="species" species="ludens">
<emphasis box="[1076,1218,1161,1184]" italics="true" pageId="1" pageNumber="53">Pteris ludens</emphasis>
