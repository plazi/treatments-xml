: 
<date box="[343,372,1929,1957]" pageId="12" pageNumber="519">
V 
<number box="[364,372,1943,1957]" pageId="12" pageNumber="519" value="2.0">2</number>
</date>
with 
<number box="[436,478,1929,1953]" pageId="12" pageNumber="519" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
