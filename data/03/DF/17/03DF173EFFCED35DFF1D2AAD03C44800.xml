<materialsCitation ID-GBIF-Occurrence="1977784112" box="[199,1432,332,357]" collectingDate="2009-05" collectingDateMax="2009-06-31" collectingDateMin="2009-05" collectionCode="NMPC" country="Nicaragua" location="Cerro Musun" pageId="5" pageNumber="474" specimenCount="2" typeStatus="holotype">
