<subSubSection pageId="3" pageNumber="48" type="etymology">
<paragraph blockId="3.[264,1325,284,1870]" pageId="3" pageNumber="48">
<emphasis bold="true" box="[312,452,1684,1710]" pageId="3" pageNumber="48">Etymology.</emphasis>
<taxonomicName box="[460,540,1684,1709]" class="Insecta" family="Rhyparochromidae" genus="Neolethaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="48" phylum="Arthropoda" rank="species" species="densus">
<emphasis box="[460,540,1684,1709]" italics="true" pageId="3" pageNumber="48">densus</emphasis>
</taxonomicName>
