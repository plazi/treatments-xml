<caption httpUri="https://zenodo.org/record/273483/files/figure.png" pageId="4" pageNumber="53" targetBox="[377,1219,313,1074]" targetPageId="4">
<paragraph blockId="4.[264,1322,1081,1168]" pageId="4" pageNumber="53">
