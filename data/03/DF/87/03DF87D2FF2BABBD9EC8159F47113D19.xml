<taxonomicName authorityName="Borkent" authorityYear="2008" box="[831,912,1272,1297]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="200" pageNumber="201" phylum="Arthropoda" rank="species" species="baltica">
