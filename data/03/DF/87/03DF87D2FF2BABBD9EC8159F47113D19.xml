<paragraph blockId="199.[151,811,1466,1492]" box="[151,811,1466,1492]" pageId="199" pageNumber="200">
<heading bold="true" box="[151,811,1466,1492]" fontSize="11" level="1" pageId="199" pageNumber="200" reason="1">
<emphasis bold="true" box="[151,811,1466,1492]" pageId="199" pageNumber="200">
<taxonomicName authority="Borkent" authorityName="Borkent" authorityYear="2008" box="[151,654,1466,1492]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="baltica" status="new species" subGenus="Corethrella">
<emphasis bold="true" box="[151,289,1466,1492]" italics="true" pageId="199" pageNumber="200">Corethrella</emphasis>
(
<emphasis bold="true" box="[306,543,1466,1492]" italics="true" pageId="199" pageNumber="200">Corethrella) baltica</emphasis>
Borkent
</taxonomicName>
, 
<taxonomicNameLabel box="[668,811,1466,1492]" pageId="199" pageNumber="200" rank="species">new species</taxonomicNameLabel>
</emphasis>
