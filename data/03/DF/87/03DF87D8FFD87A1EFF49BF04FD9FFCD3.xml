<heading bold="true" box="[151,401,1868,1894]" fontSize="11" level="1" pageId="10" pageNumber="265" reason="1">
<emphasis bold="true" box="[151,401,1868,1894]" pageId="10" pageNumber="265">
<taxonomicName authority="Leduc, 2016" authorityName="Leduc" authorityYear="2016" box="[151,312,1868,1894]" class="Chromadorea" family="Microlaimidae" genus="Maragnopsia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="10" pageNumber="265" phylum="Nematoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[151,312,1868,1894]" italics="true" pageId="10" pageNumber="265">Maragnopsia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[319,401,1868,1894]" pageId="10" pageNumber="265" rank="genus">n. gen.</taxonomicNameLabel>
