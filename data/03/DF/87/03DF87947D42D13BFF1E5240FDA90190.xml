<quantity box="[411,468,401,420]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="90" pageNumber="90" unit="km" value="15.0">15km</quantity>
E 
<collectingRegion box="[503,586,401,420]" country="Peru" name="Ucayali" pageId="90" pageNumber="90">Ucayali</collectingRegion>
, 
<date pageId="90" pageNumber="90" value="1961-10-01" valueMax="1961-10-15" valueMin="1961-10-01">1- 15-X-1961</date>
,/FMHD 
<emphasis box="[328,348,428,446]" italics="true" pageId="90" pageNumber="90">#</emphasis>
61 - 3002, sweeping in for. at night, B. Malkin/ 
<typeStatus box="[333,430,454,473]" pageId="90" pageNumber="90">Holotype</typeStatus>
<taxonomicName authority="Herman." authorityName="Herman." class="Insecta" family="Staphylinidae" genus="Oedichirus" kingdom="Animalia" order="Coleoptera" pageId="90" pageNumber="90" phylum="Arthropoda" rank="species" species="hamatus">
