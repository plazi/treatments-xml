<tableCitation box="[651,745,674,700]" captionStart="TABLE 1" captionStartId="1.[152,245,1541,1565]" captionText="TABLE 1. Type material of the twighopper specimens (genus Cladonotus) with the accompanying data. Coordinates inside the brackets were approximately assigned by the authors (HT—holotype, ST—syntype)." pageId="4" pageNumber="337">
<emphasis bold="true" box="[651,745,674,700]" pageId="4" pageNumber="337">Table 1</emphasis>
</tableCitation>
), Trincomalee (
<geoCoordinate box="[935,1065,674,701]" degrees="8.594306" direction="north" orientation="latitude" pageId="4" pageNumber="337" precision="1" value="8.594306">N8.594306</geoCoordinate>
, 
<geoCoordinate box="[1083,1221,674,701]" degrees="81.167916" direction="east" orientation="longitude" pageId="4" pageNumber="337" precision="1" value="81.167915">E81.167916</geoCoordinate>
) (
<bibRefCitation author="Saussure, H. de" box="[1249,1424,674,701]" pageId="4" pageNumber="337" pagination="469 - 494" refId="ref6615" refString="Saussure, H. de ([1862] 1861) Etudes sur quelques orthopteres du Musee de Geneve. Annales de la Societe Entomologique de France, 4 (1), 469 - 494." type="journal article" year="1862">Saussure 1862</bibRefCitation>
