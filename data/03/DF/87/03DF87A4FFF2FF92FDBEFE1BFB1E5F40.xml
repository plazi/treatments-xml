<paragraph blockId="63.[189,1296,469,497]" box="[189,1296,469,497]" pageId="63" pageNumber="64">
<collectingCountry box="[189,329,471,497]" name="Ethiopia" pageId="63" pageNumber="64">ETHIOPIA</collectingCountry>
: 1 ³, 
<collectingRegion box="[398,491,470,496]" country="Ethiopia" name="Oromiya" pageId="63" pageNumber="64">Oromia</collectingRegion>
, Mt Chillalo, alt. 
<quantity box="[697,788,471,497]" metricMagnitude="3" metricUnit="m" metricValue="3.8" pageId="63" pageNumber="64" unit="m" value="3800.0">3800 m</quantity>
, 
