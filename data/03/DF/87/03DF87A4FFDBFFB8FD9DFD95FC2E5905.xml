<paragraph blockId="21.[189,765,415,444]" box="[189,765,415,444]" pageId="21" pageNumber="22">
BODY LENGTH. 
<quantity box="[357,498,417,444]" metricMagnitude="-3" metricUnit="m" metricValue="2.85" metricValueMax="3.1" metricValueMin="2.6" pageId="21" pageNumber="22" unit="mm" value="2.85" valueMax="3.1" valueMin="2.6">2.6–3.1 mm</quantity>
