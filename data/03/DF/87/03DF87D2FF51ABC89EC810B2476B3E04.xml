<location LSID="urn:lsid:plazi:treatment:03DF87D2FF51ABC89EC810B2476B3E04:8EA9601FFF51ABC89D3A147045CB3C4C" box="[869,918,1109,1135]" country="Papua New Guinea" name="One" pageId="189" pageNumber="190">One</location>
<typeStatus box="[925,1026,1109,1135]" pageId="189" pageNumber="190">paratype</typeStatus>
