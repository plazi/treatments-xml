<materialsCitation country="Australia" location="Australian Capital Territory" pageId="17" pageNumber="289" specimenCount="1" stateProvince="Australian Capital Territory">
Southeastern 
<collectingCountry box="[299,406,185,209]" name="Australia" pageId="17" pageNumber="289">Australia</collectingCountry>
, including 
<collectingRegion box="[539,871,185,209]" country="Australia" name="Australian Capital Territory" pageId="17" pageNumber="289">Australian Capital Territory</collectingRegion>
, 
<collectingRegion box="[885,1101,185,209]" country="Australia" name="New South Wales" pageId="17" pageNumber="289">New South Wales</collectingRegion>
