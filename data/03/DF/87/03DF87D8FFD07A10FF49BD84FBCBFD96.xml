<taxonomicName authority="Schulz, 1932" authorityName="Schulz" authorityYear="1932" box="[151,489,1616,1638]" class="Chromadorea" family="Microlaimidae" genus="Paracothonolaimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="2" pageNumber="257" phylum="Nematoda" rank="genus">
