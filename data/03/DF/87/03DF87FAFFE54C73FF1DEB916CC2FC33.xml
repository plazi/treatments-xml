<taxonomicName authority="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord, 2022" authorityName="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord" authorityYear="2022" box="[208,530,579,603]" class="Ostracoda" family="Cyprididae" genus="Salvadoriella" kingdom="Animalia" order="Podocopida" pageId="25" pageNumber="26" phylum="Arthropoda" rank="subSpecies" species="redunca" status="n. ssp." subSpecies="comitans">
