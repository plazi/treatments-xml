NE 
<location LSID="urn:lsid:plazi:treatment:03DF87D2FFAFAB319EC812BA47DF3DF9:8EA9601FFFA8AB319C8414A5451E3CB9" box="[731,835,1152,1178]" country="Costa Rica" name="Tarcoles" pageId="68" pageNumber="69">Tárcoles</location>
, 
<collectingCountry box="[855,994,1152,1178]" name="Costa Rica" pageId="68" pageNumber="69">Costa Rica</collectingCountry>
