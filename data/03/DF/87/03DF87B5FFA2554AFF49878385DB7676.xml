<paragraph blockId="6.[151,697,1671,1697]" box="[151,697,1671,1697]" pageId="6" pageNumber="27">
<heading bold="true" box="[151,697,1671,1697]" fontSize="11" level="1" pageId="6" pageNumber="27" reason="1">
<emphasis bold="true" box="[151,697,1671,1697]" pageId="6" pageNumber="27">
<taxonomicName authority="Nemesio" authorityName="Nemesio" box="[151,624,1671,1697]" class="Insecta" family="Apidae" genus="Euglossa" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="27" phylum="Arthropoda" rank="species" species="sovietica" status="sp. n." subGenus="Euglossa">
<emphasis bold="true" box="[151,510,1671,1697]" italics="true" pageId="6" pageNumber="27">Euglossa (Euglossa) sovietica</emphasis>
Nemésio
</taxonomicName>
<taxonomicNameLabel box="[631,697,1671,1697]" pageId="6" pageNumber="27" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
