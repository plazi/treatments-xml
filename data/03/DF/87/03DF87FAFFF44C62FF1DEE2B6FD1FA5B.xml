<emphasis italics="true" pageId="8" pageNumber="9">
<taxonomicName authorityName="Wicher" authorityYear="1959" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="kegeli">Cypridea kegeli</taxonomicName>
