<taxonomicName authority="" authorityName="Krommelbein" authorityYear="1962" box="[488,769,1121,1148]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="subGenus" subGenus="Morininoides">
