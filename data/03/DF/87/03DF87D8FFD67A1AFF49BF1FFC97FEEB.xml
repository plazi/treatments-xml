<taxonomicName authority="Leduc, 2016" authorityName="Leduc" authorityYear="2016" box="[537,721,659,680]" class="Chromadorea" family="Microlaimidae" genus="Microlaimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="8" pageNumber="263" phylum="Nematoda" rank="species" species="korari" status="n. sp.">
