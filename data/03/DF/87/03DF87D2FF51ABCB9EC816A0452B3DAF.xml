<location LSID="urn:lsid:plazi:treatment:03DF87D2FF51ABCB9EC816A0452B3DAF:8EA9601FFF52ABCB9EC813B746A23B8F" box="[151,255,914,940]" country="Solomon Islands" municipality="Santa" name="Tatamba" pageId="190" pageNumber="191" stateProvince="Isabel">Tatamba</location>
, 
<collectingMunicipality box="[268,333,914,940]" pageId="190" pageNumber="191">Santa</collectingMunicipality>
<collectingRegion box="[343,416,914,940]" country="Solomon Islands" name="Isabel" pageId="190" pageNumber="191">Isabel</collectingRegion>
, 
<collectingCountry box="[430,628,914,940]" name="Solomon Islands" pageId="190" pageNumber="191">Solomon Islands</collectingCountry>
(Fig. 116A) at an altitude of 
<quantity box="[987,1077,914,940]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="5.0" metricValueMin="0.0" pageId="190" pageNumber="191" unit="m" value="25.0" valueMax="50.0" valueMin="0.0">
<elevation box="[987,1077,914,940]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="5.0" metricValueMin="0.0" pageId="190" pageNumber="191" unit="m" value="25.0" valueMax="50.0" valueMin="0.0">0-50 m</elevation>
