<paragraph blockId="12.[151,1437,1502,1673]" pageId="12" pageNumber="13">
<emphasis bold="true" box="[151,276,1502,1528]" pageId="12" pageNumber="13">Diagnosis:</emphasis>
A small 
<taxonomicName box="[376,569,1503,1529]" class="Ostracoda" family="Cyprididae" genus="Cypridea" kingdom="Animalia" order="Podocopida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis box="[376,480,1503,1529]" italics="true" pageId="12" pageNumber="13">Cypridea</emphasis>
