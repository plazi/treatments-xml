<paragraph blockId="12.[151,585,1495,1521]" box="[151,585,1495,1521]" pageId="12" pageNumber="53">
<heading bold="true" box="[151,585,1495,1521]" fontSize="11" level="1" pageId="12" pageNumber="53" reason="1">
<emphasis bold="true" box="[151,585,1495,1521]" pageId="12" pageNumber="53">
<taxonomicName box="[151,288,1495,1521]" class="Insecta" family="Tettigoniidae" genus="Toledopizia" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="53" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,288,1495,1521]" italics="true" pageId="12" pageNumber="53">Toledopizia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[295,405,1495,1521]" pageId="12" pageNumber="53" rank="genus">gen. nov.</taxonomicNameLabel>
(
<taxonomicName box="[420,577,1495,1521]" class="Insecta" family="Tettigoniidae" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="53" phylum="Arthropoda" rank="tribe" tribe="Copiphorini">Copiphorini</taxonomicName>
