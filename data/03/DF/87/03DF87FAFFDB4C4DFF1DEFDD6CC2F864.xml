<quantity box="[549,644,1965,1992]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" pageId="39" pageNumber="40" unit="mm" value="1.25">1.25mm</quantity>
, height 
<quantity box="[733,828,1965,1991]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="39" pageNumber="40" unit="mm" value="0.8">0.80mm</quantity>
, width 
<quantity box="[910,1006,1965,1992]" metricMagnitude="-4" metricUnit="m" metricValue="5.8" pageId="39" pageNumber="40" unit="mm" value="0.58">0.58mm</quantity>
.
</paragraph>
