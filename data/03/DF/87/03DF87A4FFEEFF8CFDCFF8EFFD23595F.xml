<collectingCountry box="[189,329,889,915]" name="Ethiopia" pageId="33" pageNumber="34">ETHIOPIA</collectingCountry>
: 1 ³, 
<collectingRegion box="[391,484,889,915]" country="Ethiopia" name="Oromiya" pageId="33" pageNumber="34">Oromia</collectingRegion>
, Bale Mts, brook valley above Goba, alt. 
<quantity box="[951,1113,889,916]" metricMagnitude="3" metricUnit="m" metricValue="3.775" metricValueMax="3.8" metricValueMin="3.75" pageId="33" pageNumber="34" unit="m" value="3775.0" valueMax="3800.0" valueMin="3750.0">3750–3800 m</quantity>
, 
<geoCoordinate box="[1123,1271,887,915]" degrees="06" direction="north" minutes="54" orientation="latitude" pageId="33" pageNumber="34" precision="15" seconds="42" value="6.911667">06°54′42″ N</geoCoordinate>
, 
<geoCoordinate degrees="39" direction="east" minutes="54" orientation="longitude" pageId="33" pageNumber="34" precision="15" seconds="38" value="39.910557">39°54′38″ E</geoCoordinate>
