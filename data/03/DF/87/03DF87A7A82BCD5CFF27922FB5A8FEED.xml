<paragraph blockId="20.[151,1437,1595,1920]" box="[197,666,1632,1659]" pageId="20" pageNumber="61">
<taxonomicName authority="Piza, 1976" authorityName="Piza" authorityYear="1976" box="[197,550,1633,1659]" class="Insecta" family="Phaneropteridae" genus="Bliastes" kingdom="Animalia" order="Orthoptera" pageId="20" pageNumber="61" phylum="Arthropoda" rank="species" species="viridifrons" status="syn. nov.">
<emphasis box="[197,420,1633,1658]" italics="true" pageId="20" pageNumber="61">Bliastes viridifrons</emphasis>
Piza, 
<number box="[492,550,1633,1659]" pageId="20" pageNumber="61" value="1976.0">1976</number>
</taxonomicName>
<taxonomicNameLabel box="[558,666,1632,1658]" pageId="20" pageNumber="61">
<emphasis bold="true" box="[558,666,1632,1658]" pageId="20" pageNumber="61">syn. nov.</emphasis>
</taxonomicNameLabel>
