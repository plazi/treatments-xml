<taxonomicName authorityName="Krommelbein" authorityYear="1962" box="[524,675,741,767]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="18" pageNumber="45" phylum="Arthropoda" rank="subGenus" subGenus="Morininoides">
