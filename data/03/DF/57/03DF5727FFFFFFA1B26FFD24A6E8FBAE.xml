<subSubSection box="[264,1039,644,670]" pageId="15" pageNumber="16" type="nomenclature">
<paragraph blockId="15.[264,1039,644,670]" box="[264,1039,644,670]" pageId="15" pageNumber="16">
<heading bold="true" box="[264,1039,644,670]" fontSize="11" level="1" pageId="15" pageNumber="16" reason="1">
<emphasis bold="true" box="[264,1039,644,670]" pageId="15" pageNumber="16">
Subgenus 
<taxonomicName authority="Sayce 1902" authorityName="Sayce" authorityYear="1902" box="[392,696,644,670]" class="Branchiopoda" family="Thamnocephalidae" genus="Branchinella" kingdom="Animalia" order="Anostraca" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[392,551,644,670]" italics="true" pageId="15" pageNumber="16">Branchinella</emphasis>
Sayce 
<number box="[637,696,644,670]" pageId="15" pageNumber="16" value="1902.0">1902</number>
</taxonomicName>
(sensu stricto) subgen. nov.
</emphasis>
</heading>
