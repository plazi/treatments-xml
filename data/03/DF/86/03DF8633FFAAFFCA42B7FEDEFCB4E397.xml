</taxonomicName>
spp., including 
<emphasis italics="true" pageId="15" pageNumber="16">
<taxonomicName box="[778,971,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="camaldulensis">E. camaldulensis</taxonomicName>
, 
<taxonomicName box="[982,1112,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="globulus">E. globulus</taxonomicName>
, 
<taxonomicName box="[1122,1241,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="grandis">E. grandis</taxonomicName>
, 
<taxonomicName box="[1251,1396,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="parvifolia">E. parvifolia</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="viminalis">E. viminalis</taxonomicName>
</emphasis>
