<paragraph blockId="15.[151,1430,673,1188]" box="[151,1035,826,851]" pageId="15" pageNumber="16">
<emphasis box="[151,269,827,851]" italics="true" pageId="15" pageNumber="16">Host plant</emphasis>
: Several 
<emphasis box="[375,445,827,850]" italics="true" pageId="15" pageNumber="16">Citrus</emphasis>
spp., 
<taxonomicName authority="J.Koenig" authorityName="J.Koenig" box="[512,829,826,851]" class="Magnoliopsida" family="Rutaceae" genus="Bergera" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="koenigii" subGenus="Murraya">
<emphasis box="[512,604,828,851]" italics="true" pageId="15" pageNumber="16">Bergera</emphasis>
(
<emphasis box="[620,719,828,851]" italics="true" pageId="15" pageNumber="16">Murraya</emphasis>
) 
<emphasis box="[736,829,827,851]" italics="true" pageId="15" pageNumber="16">koenigii</emphasis>
