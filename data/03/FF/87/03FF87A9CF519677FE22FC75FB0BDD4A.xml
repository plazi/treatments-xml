<superScript attach="left" box="[588,602,905,919]" fontSize="6" pageId="11" pageNumber="12">cm-1</superScript>
, 
<quantity box="[617,727,906,933]" metricMagnitude="-8" metricUnit="kg" metricValue="6.0" pageId="11" pageNumber="12" unit="mg" value="0.06">0.060 mg</quantity>
<superScript attach="left" box="[743,758,905,919]" fontSize="6" pageId="11" pageNumber="12">l-1</superScript>
NH 
<subScript attach="left" box="[807,816,924,938]" fontSize="6" pageId="11" pageNumber="12">4</subScript>
, 
