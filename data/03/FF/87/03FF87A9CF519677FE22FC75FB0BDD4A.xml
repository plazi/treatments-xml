NO 
<subScript attach="right" box="[1021,1030,924,938]" fontSize="6" pageId="11" pageNumber="12">2</subScript>
, 
<quantity box="[1044,1154,906,933]" metricMagnitude="-7" metricUnit="kg" metricValue="4.46" pageId="11" pageNumber="12" unit="mg" value="0.446">0.446 mg</quantity>
<superScript attach="left" box="[1170,1185,905,919]" fontSize="6" pageId="11" pageNumber="12">l-1</superScript>
NO 
<subScript attach="left" box="[1234,1243,924,938]" fontSize="6" pageId="11" pageNumber="12">3</subScript>
, 
