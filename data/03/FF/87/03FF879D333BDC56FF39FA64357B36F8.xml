<taxonomicName authority="Karny." authorityName="Karny." box="[642,1019,1549,1572]" class="Insecta" family="Thripidae" genus="Hydatothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="adolfifriderici">
