<taxonomicName authority="Moser, 1913" authorityName="Moser" authorityYear="1913" box="[313,703,896,920]" class="Insecta" family="Melolonthidae" genus="Entypophana" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="158" phylum="Arthropoda" rank="species" species="apicata">
