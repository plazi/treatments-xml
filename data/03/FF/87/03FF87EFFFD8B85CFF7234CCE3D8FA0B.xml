<subSubSection pageId="9" pageNumber="10" type="diagnosis">
<paragraph blockId="9.[151,1436,763,1508]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[197,319,1447,1472]" pageId="9" pageNumber="10">Diagnosis.</emphasis>
<taxonomicName box="[328,539,1448,1472]" class="Insecta" family="Scarabaeidae" genus="Ochicanthon" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="ernei">
<emphasis box="[328,539,1448,1472]" italics="true" pageId="9" pageNumber="10">Ochicanthon ernei</emphasis>
</taxonomicName>
