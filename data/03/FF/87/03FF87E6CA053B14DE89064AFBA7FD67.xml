<subSubSection lastPageId="13" lastPageNumber="90" pageId="10" pageNumber="87" type="etymology">
<paragraph blockId="10.[140,1108,1046,1672]" pageId="10" pageNumber="87">
<emphasis bold="true" box="[185,320,1575,1601]" pageId="10" pageNumber="87">
