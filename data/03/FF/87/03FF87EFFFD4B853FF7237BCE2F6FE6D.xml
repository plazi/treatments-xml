and 
<taxonomicName box="[552,662,363,387]" class="Insecta" family="Scarabaeidae" genus="Ochicanthon" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="nitidus">
<emphasis box="[552,662,363,387]" italics="true" pageId="6" pageNumber="7">O. nitidus</emphasis>
