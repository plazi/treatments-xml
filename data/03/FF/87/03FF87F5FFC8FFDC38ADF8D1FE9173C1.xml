<collectingCountry box="[1006,1127,439,464]" name="Guatemala" pageId="34" pageNumber="35">Guatemala</collectingCountry>
(Sacatepéquez, Escuintla), 
<collectingCountry box="[151,260,476,501]" name="Honduras" pageId="34" pageNumber="35">Honduras</collectingCountry>
(Olancho), 
<collectingCountry box="[394,516,476,501]" name="Costa Rica" pageId="34" pageNumber="35">Costa Rica</collectingCountry>
(Puntarenas), 
<collectingCountry box="[679,771,477,500]" name="Panama" pageId="34" pageNumber="35">Panama</collectingCountry>
