Total length 
<number box="[568,688,1172,1198]" pageId="26" pageNumber="27" value="0.855" valueMax="0.93" valueMin="0.78">0.78–0.93</number>
, 
<quantity box="[703,814,1172,1198]" metricMagnitude="-4" metricUnit="m" metricValue="8.6" pageId="26" pageNumber="27" unit="mm" value="0.86">0.86 mm</quantity>
