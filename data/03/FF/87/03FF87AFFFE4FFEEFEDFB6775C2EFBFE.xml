<quantity box="[703,814,444,470]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="26" pageNumber="27" unit="mm" value="0.8">0.80 mm</quantity>
. 
<taxonomicName box="[826,916,444,470]" class="Gastropoda" family="Fissurellidae" genus="Scutum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeogastropoda" pageId="26" pageNumber="4" phylum="Mollusca" rank="genus">Scutum</taxonomicName>
<number box="[926,1047,444,470]" pageId="26" pageNumber="27" value="0.6799999999999999" valueMax="0.72" valueMin="0.64">0.64–0.72</number>
, 
<quantity box="[1060,1167,444,470]" metricMagnitude="-4" metricUnit="m" metricValue="6.8" pageId="26" pageNumber="27" unit="mm" value="0.68">0.68 mm</quantity>
long; scutellum 
<number box="[317,437,484,510]" pageId="26" pageNumber="27" value="0.155" valueMax="0.18" valueMin="0.13">0.13–0.18</number>
, 
<quantity box="[449,553,484,510]" metricMagnitude="-4" metricUnit="m" metricValue="1.7" pageId="26" pageNumber="27" unit="mm" value="0.17">0.17 mm</quantity>
long.
</paragraph>
<paragraph blockId="26.[264,1325,284,1478]" box="[312,952,524,550]" pageId="26" pageNumber="27">
