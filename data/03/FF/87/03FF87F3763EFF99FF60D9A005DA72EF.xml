<collectingRegion box="[441,559,1159,1185]" country="Brazil" name="Sao Paulo" pageId="4" pageNumber="515">São Paulo</collectingRegion>
, 
<collectingRegion box="[569,715,1159,1186]" country="Brazil" name="Mato Grosso" pageId="4" pageNumber="515">Mato Grosso</collectingRegion>
(?)), 
<emphasis bold="true" box="[776,896,1159,1185]" pageId="4" pageNumber="515">
<collectingCountry box="[776,896,1159,1185]" name="Venezuela" pageId="4" pageNumber="515">Venezuela</collectingCountry>
</emphasis>
, 
