<paragraph blockId="10.[264,1323,1524,1870]" lastBlockId="11.[264,1323,284,350]" lastPageId="11" lastPageNumber="66" pageId="10" pageNumber="65">
<collectingCountry box="[312,515,1564,1590]" name="Kazakhstan" pageId="10" pageNumber="65">KAZAKHSTAN</collectingCountry>
