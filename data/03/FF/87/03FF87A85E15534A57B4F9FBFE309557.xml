(
<emphasis bold="true" box="[274,403,1636,1658]" italics="true" pageId="37" pageNumber="238">Coptoderina</emphasis>
) 
<emphasis bold="true" box="[418,519,1636,1658]" italics="true" pageId="37" pageNumber="238">chaudoiri</emphasis>
Andrewes, 
<number box="[646,697,1636,1658]" pageId="37" pageNumber="238" value="1919.0">1919</number>
</emphasis>
