<taxonomicName authority="Brues" authorityName="Brues" authorityYear="1937" class="Insecta" family="Scelionidae" genus="Proteroscelio" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="antennalis">
