. 
<emphasis box="[639,777,548,572]" italics="true" pageId="6" pageNumber="467">longisetosus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[792,892,548,572]" class="Arachnida" family="Macrochelidae" genus="Longicheles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="467" phylum="Arthropoda" rank="species" species="ilvana">
