</taxonomicName>
<taxonomicNameLabel box="[825,876,1982,2003]" pageId="14" pageNumber="15" rank="species">sp. n.</taxonomicNameLabel>
(
<emphasis bold="true" box="[893,953,1981,2002]" pageId="14" pageNumber="15">70–78</emphasis>
): male habitus (
<emphasis bold="true" box="[1117,1180,1981,2002]" pageId="14" pageNumber="15">62, 70</emphasis>
); forebody (
<emphasis bold="true" box="[1305,1367,1981,2002]" pageId="14" pageNumber="15">63, 71</emphasis>
); male sternite VII (
