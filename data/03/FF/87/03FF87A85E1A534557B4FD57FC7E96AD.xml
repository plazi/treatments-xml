, 
<bibRefCitation author="Hurka" box="[918,1054,896,918]" pageId="42" pageNumber="243" refString="Hurka, K. (2003) Tribe Helluoniini Hope 1838. In: Lobl, I. &amp; Smetana, A. (Eds.). Catalogue of Palaearctic Coleoptera. Vol. 1. Archostemata - Myxophaga - Adephaga. Apollo Books, Stenstrup, pp. 407." type="book chapter" year="2003">
Hùrka (
<number box="[994,1047,896,918]" pageId="42" pageNumber="243" value="2003.0">2003</number>
)
</bibRefCitation>
and 
<bibRefCitation author="Hackel" box="[1104,1342,896,918]" pageId="42" pageNumber="243" refString="Hackel, M. &amp; Farkae, J. (2013) Checklist of the subfamily Anthiinae Bonelli, 1813 of the World. (Coleoptera: Carabidae). Studies and Reports, Taxonomical Series, 9 (2), 261 - 366." type="journal article" year="2013">
Häckel &amp; Farka (
<number box="[1279,1334,896,918]" pageId="42" pageNumber="243" value="2013.0">2013</number>
)
</bibRefCitation>
.
