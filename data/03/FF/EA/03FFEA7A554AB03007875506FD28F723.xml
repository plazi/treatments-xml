<collectingCountry box="[434,496,773,800]" name="India" pageId="6" pageNumber="217">India</collectingCountry>
(
<collectingRegion box="[511,666,773,800]" country="India" name="West Bengal" pageId="6" pageNumber="217">West Bengal</collectingRegion>
).
</paragraph>
