<materialsCitation collectionCode="NHMD" pageId="11" pageNumber="68" specimenCode="NHMD-608414" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
<typeStatus box="[189,298,1955,1981]" pageId="11" pageNumber="68">Holotype</typeStatus>
