Figs 
<number box="[209,283,532,558]" pageId="12" pageNumber="13" value="52.54">52, 54</number>
</figureCitation>
a, b, c, 
<number box="[366,441,532,558]" pageId="12" pageNumber="13" value="56.61">56, 61</number>
a, b, 
<number box="[496,569,532,558]" pageId="12" pageNumber="13" value="62.77">62, 77</number>
</paragraph>
