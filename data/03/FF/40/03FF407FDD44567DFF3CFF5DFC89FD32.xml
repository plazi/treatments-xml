<taxonomicName box="[665,727,1930,1951]" class="Insecta" family="Tettigoniidae" genus="Phlesirtes" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="458" phylum="Arthropoda" rank="species" species="nou" status="n. sp.">
<emphasis box="[665,727,1930,1951]" italics="true" pageId="7" pageNumber="458">P. nou</emphasis>
</taxonomicName>
<emphasis bold="true" box="[734,822,1929,1950]" pageId="7" pageNumber="458">
<taxonomicNameLabel box="[734,791,1929,1950]" pageId="7" pageNumber="458" rank="species">n. sp.</taxonomicNameLabel>
N.
</emphasis>
<taxonomicName box="[828,945,1929,1951]" class="Insecta" family="Tettigoniidae" genus="Phlesirtes" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="458" phylum="Arthropoda" rank="species" species="timboroa" status="n. sp.">
<emphasis box="[828,945,1929,1951]" italics="true" pageId="7" pageNumber="458">P. timboroa</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[951,1008,1929,1950]" pageId="7" pageNumber="458" rank="species">n. sp.</taxonomicNameLabel>
