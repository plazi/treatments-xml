<collectingCountry box="[1072,1172,990,1013]" name="Thailand" pageId="6" pageNumber="487">Thailand</collectingCountry>
, 
<collectingCountry box="[1187,1344,990,1013]" name="Malaysia" pageId="6" pageNumber="487">North Borneo</collectingCountry>
, South and East 
