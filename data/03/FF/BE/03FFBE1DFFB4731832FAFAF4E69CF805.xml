...................................................................... 
<taxonomicName authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[1232,1365,498,521]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="ambrense" status="n. sp.">Z. ambrense</taxonomicName>
,
