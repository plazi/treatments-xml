<paragraph blockId="31.[151,1437,1389,1552]" pageId="31" pageNumber="32">
<emphasis bold="true" box="[151,301,1389,1413]" pageId="31" pageNumber="32">FIGURE 018</emphasis>
. 
<taxonomicName authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[313,547,1390,1413]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="mitoho" status="n. sp.">
<emphasis box="[313,547,1390,1413]" italics="true" pageId="31" pageNumber="32">Zoosphaerium mitoho</emphasis>
</taxonomicName>
<emphasis bold="true" box="[553,612,1389,1413]" pageId="31" pageNumber="32">
<taxonomicNameLabel box="[553,612,1389,1413]" pageId="31" pageNumber="32" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
