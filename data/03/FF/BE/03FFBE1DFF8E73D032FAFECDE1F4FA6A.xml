<emphasis box="[314,543,1924,1947]" italics="true" pageId="127" pageNumber="128">Zoosphaerium fisheri</emphasis>
</taxonomicName>
<emphasis bold="true" box="[549,609,1923,1947]" pageId="127" pageNumber="128">
<taxonomicNameLabel box="[549,609,1923,1947]" pageId="127" pageNumber="128" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<typeStatus box="[622,717,1924,1947]" pageId="127" pageNumber="128">paratype</typeStatus>
