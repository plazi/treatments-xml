<paragraph blockId="56.[313,1268,527,609]" pageId="56" pageNumber="752">
<taxonomicName authority="LaSalle and Schauff, 1994: 255" authorityName="LaSalle and Schauff" authorityPageNumber="255" authorityYear="1994" box="[313,792,560,583]" class="Insecta" family="Eulophidae" genus="Dasyomphale" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="752" phylum="Arthropoda" rank="genus">
