<keyLead box="[313,1268,1551,1573]" pageId="10" pageNumber="706">1 Body yellow non-metallic................ 2</keyLead>
</paragraph>
<paragraph blockId="10.[313,1268,1518,1600]" box="[314,1268,1578,1600]" pageId="10" pageNumber="706">
<keyLead box="[314,1268,1578,1600]" pageId="10" pageNumber="706">– Body predominantly dark and metallic............ 3</keyLead>
</paragraph>
</keyStep>
<keyStep lastPageId="11" lastPageNumber="707" pageId="10" pageNumber="706">
<paragraph blockId="10.[313,1268,1618,1693]" pageId="10" pageNumber="706">
