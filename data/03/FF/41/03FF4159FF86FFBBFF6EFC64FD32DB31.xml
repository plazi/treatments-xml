<number box="[881,944,1202,1228]" pageId="3" pageNumber="40" value="1876.0">1876</number>
</bibRefCitation>
: 
<number box="[955,1002,1202,1228]" pageId="3" pageNumber="40" value="277.0">277</number>
; 
<bibRefCitation author="Philippi" box="[1015,1186,1202,1228]" pageId="3" pageNumber="45" refString="Philippi, F. (1887) Catalogo de los coleopteros de Chile. Anales de la Universidad de Chile, 71, 619 - 806." type="journal article" year="1887">
Philippi, 
<number box="[1123,1186,1202,1228]" pageId="3" pageNumber="40" value="1887.0">1887</number>
</bibRefCitation>
: 
<number box="[1198,1245,1202,1228]" pageId="3" pageNumber="40" value="673.0">673</number>
; 
<bibRefCitation author="Leveille" box="[1258,1432,1202,1228]" pageId="3" pageNumber="45" refString="Leveille, A. (1889) Catalogue de la famille des temnochilides. Annales de la Societe Entomologique de France, 6 e Serie, 8 (1888), 429 - 448." type="journal article" year="1889">
Léveillé, 
<number box="[1369,1432,1202,1228]" pageId="3" pageNumber="40" value="1889.0">1889</number>
</bibRefCitation>
: 
<number box="[151,198,1240,1266]" pageId="3" pageNumber="40" value="431.0">431</number>
; 
<bibRefCitation author="Leveille" box="[210,383,1240,1266]" pageId="3" pageNumber="45" refString="Leveille, A. (1910) Coleopterorum Catalogus. Pars 11. Temnochilidae. W. Junk, Berlin, 40 pp." type="book" year="1910">
Léveillé, 
<number box="[320,383,1240,1266]" pageId="3" pageNumber="40" value="1910.0">1910</number>
</bibRefCitation>
: 
<number box="[393,410,1240,1266]" pageId="3" pageNumber="40" value="4.0">4</number>
; 
<bibRefCitation author="Blackwelder" box="[423,647,1240,1266]" pageId="3" pageNumber="44" refString="Blackwelder, R. E. (1945) Checklist of the coleopterous insects of Mexico, Central America, the West Indies, and South America. Part 3. United States National Museum Bulletin, 185, i - iv, 343 - 550." type="journal article" year="1945">
Blackwelder, 
<number box="[584,647,1240,1266]" pageId="3" pageNumber="40" value="1945.0">1945</number>
</bibRefCitation>
: 
