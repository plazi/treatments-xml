long, (H) 
<number box="[637,669,786,812]" pageId="2" pageNumber="25" value="57.0">57</number>
, (P 
<number box="[711,722,786,812]" pageId="2" pageNumber="25" value="1.0">1</number>
) 
<number box="[739,771,786,812]" pageId="2" pageNumber="25" value="62.0">62</number>
, (P 
<number box="[809,824,786,812]" pageId="2" pageNumber="25" value="2.0">2</number>
) 
<number box="[841,873,786,812]" pageId="2" pageNumber="25" value="44.0">44</number>
, (P 
<number box="[912,926,786,812]" pageId="2" pageNumber="25" value="3.0">3</number>
) 
<quantity box="[942,1028,786,812]" metricMagnitude="-2" metricUnit="m" metricValue="4.2" pageId="2" pageNumber="25" unit="mm" value="42.0">42 mm</quantity>
.
</paragraph>
