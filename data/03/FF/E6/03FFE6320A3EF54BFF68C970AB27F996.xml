<collectionCode LSID="urn:lsid:biocol.org:col:34665" box="[654,760,1483,1506]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="59" pageNumber="60">NHMUK</collectionCode>
1907.10. 
<number box="[869,950,1482,1507]" pageId="59" pageNumber="60" value="28225.0">28.225</number>
