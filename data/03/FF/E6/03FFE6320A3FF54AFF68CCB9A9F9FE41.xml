<number box="[434,479,439,464]" pageId="58" pageNumber="59" value="267.0">267</number>
; 
<bibRefCitation author="Simone" box="[491,644,439,464]" pageId="58" pageNumber="59" refString="Simone, L. R. L. (2006) Land and freshwater molluscs of Brazil. EGB, Fapesp, Sao Paulo, 390 pp." type="book" year="2006">
Simone 
<number box="[583,644,439,464]" pageId="58" pageNumber="59" value="2006.0">2006</number>
