: 
<number box="[990,1021,871,896]" pageId="46" pageNumber="47" value="60.0">60</number>
; 
<bibRefCitation author="Clench" box="[1033,1179,871,896]" pageId="46" pageNumber="47" refString="Clench, W. J. (1955) Melania cancellata Say. The Nautilus, 68 (3), 107. [11 February; Coan &amp; Harasewych 1993: 179]" type="journal article" year="1955">
