<taxonomicName authority="G.B. Sowerby I" authorityName="G.B. Sowerby I" box="[879,1143,1412,1437]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="46" pageNumber="47" phylum="Mollusca" rank="species" species="rugosa">
