<taxonomicName authority="Lamarck" authorityName="Lamarck" box="[1239,1436,260,285]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="53" pageNumber="54" phylum="Mollusca" rank="species" species="fasciata">
