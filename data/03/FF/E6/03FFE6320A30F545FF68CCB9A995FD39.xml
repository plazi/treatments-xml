<taxonomicName authority="Roissy" authorityName="Roissy" box="[1053,1227,260,285]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="53" pageNumber="54" phylum="Mollusca" rank="species" species="fasciata">
