<taxonomicName authority="Bourguignat, 1879" authorityName="Bourguignat" authorityYear="1879" box="[711,1204,511,536]" class="Gastropoda" family="Ampullariidae" genus="Meladomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="19" pageNumber="20" phylum="Mollusca" rank="species" species="letourneuxi">
