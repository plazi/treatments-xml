<collectingCountry box="[932,1026,434,459]" name="Somalia" pageId="51" pageNumber="52">Somalia</collectingCountry>
, 
<collectingCountry box="[1039,1112,434,459]" name="Sudan" pageId="51" pageNumber="52">Sudan</collectingCountry>
, possibly 
<collectingCountry box="[1225,1301,435,459]" name="Kenya" pageId="51" pageNumber="52">Kenya</collectingCountry>
, Lake 
<collectingCountry box="[1377,1436,434,459]" name="Chad" pageId="51" pageNumber="52">Chad</collectingCountry>
and westwards to the 
<collectingCountry box="[409,473,470,495]" name="Niger" pageId="51" pageNumber="52">Niger</collectingCountry>
River in 
<collectingCountry box="[589,642,470,495]" name="Serbia" pageId="51" pageNumber="52">Mali</collectingCountry>
and southwestern 
<collectingCountry box="[863,950,470,495]" name="Nigeria" pageId="51" pageNumber="52">Nigeria</collectingCountry>
, and western 
<collectingCountry name="Democratic Republic of the Congo" pageId="51" pageNumber="52">Democratic Republic of the Congo</collectingCountry>
