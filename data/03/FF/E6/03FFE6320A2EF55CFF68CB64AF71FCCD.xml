<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[914,1160,332,357]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="ampullacea">
