<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[1150,1235,1335,1358]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="54" pageNumber="55">MNHN</collectionCode>
