(
<number box="[1182,1242,188,213]" pageId="11" pageNumber="494" value="8.0" valueMax="10.0" valueMin="6.0">6–10</number>
); AP 
<number box="[1305,1333,188,212]" pageId="11" pageNumber="494" value="31.0">31</number>
(
<number box="[1348,1423,188,212]" pageId="11" pageNumber="494" value="31.5" valueMax="34.0" valueMin="29.0">29–34</number>
); AM 
<number box="[205,233,223,248]" pageId="11" pageNumber="494" value="46.0">46</number>
(
<number box="[251,325,223,248]" pageId="11" pageNumber="494" value="47.0" valueMax="51.0" valueMin="43.0">43–51</number>
