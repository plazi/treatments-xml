<number box="[612,620,1693,1707]" pageId="2" pageNumber="485" value="1.0">1</number>
<number box="[627,657,1679,1703]" pageId="2" pageNumber="485" value="17.0">17</number>
, f 
<number box="[678,686,1693,1707]" pageId="2" pageNumber="485" value="1.0">1</number>
<number box="[692,708,1679,1703]" pageId="2" pageNumber="485" value="2.0">2</number>
, nude pretarsala 
<number box="[897,924,1679,1703]" pageId="2" pageNumber="485" value="11.0">11</number>
(PT´= N), nude ST 
<number box="[1156,1184,1679,1703]" pageId="2" pageNumber="485" value="21.0">21</number>
and pST 11, 2 tibialae (
<number box="[160,189,1717,1742]" pageId="2" pageNumber="485" value="15.0">15</number>
and 
<number box="[242,272,1717,1742]" pageId="2" pageNumber="485" value="15.0">15</number>
), microtibiala 3, 2 genualae—anterior 
<number box="[705,735,1718,1742]" pageId="2" pageNumber="485" value="19.0">19</number>
, posterior 
<number box="[853,883,1717,1742]" pageId="2" pageNumber="485" value="15.0">15</number>
