<number box="[1054,1070,1718,1742]" pageId="2" pageNumber="485" value="3.0">3</number>
; Leg II: Tarsus—nude pretarsala 
<number box="[151,178,1754,1778]" pageId="2" pageNumber="485" value="11.0">11</number>
