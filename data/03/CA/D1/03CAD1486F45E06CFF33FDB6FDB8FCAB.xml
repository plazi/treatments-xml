<taxonomicName class="Aves" family="Muscicapidae" genus="Larvivora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="2" pageNumber="485" phylum="Chordata" rank="species" species="sibilans">
<emphasis italics="true" pageId="2" pageNumber="485">Larvivora sibilans</emphasis>
