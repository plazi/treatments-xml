ovf (CL 9.94 mm), 
<geoCoordinate box="[977,1133,1413,1437]" direction="south" orientation="latitude" pageId="1" pageNumber="35" precision="1" value="-13.48275">
<number box="[977,1005,1413,1437]" pageId="1" pageNumber="35" value="13.0">13</number>
° 
<number box="[1015,1046,1413,1437]" pageId="1" pageNumber="35" value="28.0">28</number>
’57.9”S
</geoCoordinate>
, 
<geoCoordinate box="[1145,1309,1413,1437]" direction="west" orientation="longitude" pageId="1" pageNumber="35" precision="1" value="-38.818417">
<number box="[1145,1173,1413,1437]" pageId="1" pageNumber="35" value="38.0">38</number>
° 
<number box="[1183,1213,1413,1437]" pageId="1" pageNumber="35" value="49.0">49</number>
’06.3”W
