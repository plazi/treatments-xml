</paragraph>
<paragraph blockId="26.[151,1436,1389,1450]" pageId="26" pageNumber="27">
<taxonomicName authority="Gray, 1835" authorityName="Gray" authorityYear="1835" box="[151,467,1389,1414]" class="Gastropoda" family="Fissurellidae" genus="Macroschisma" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepetellida" pageId="26" pageNumber="27" phylum="Mollusca" rank="genus">
