<quantity box="[980,1117,1102,1126]" metricMagnitude="2" metricUnit="m" metricValue="1.45" metricValueMax="1.7" metricValueMin="1.2" pageId="16" pageNumber="417" unit="m" value="145.0" valueMax="170.0" valueMin="120.0">120–170 m</quantity>
