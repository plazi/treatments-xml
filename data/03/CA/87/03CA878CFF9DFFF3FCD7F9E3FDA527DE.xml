and 
<taxonomicName authority="(Roewer, 1928)" authorityName="Medrano &amp; Kury &amp; Mendes" authorityYear="2022" baseAuthorityName="Roewer" baseAuthorityYear="1928" box="[163,484,1599,1621]" class="Arachnida" family="Cosmetidae" genus="Protus" kingdom="Animalia" order="Opiliones" pageId="41" pageNumber="626" phylum="Arthropoda" rank="species" species="speciosus" status="comb. nov.">
