<collectingCountry box="[423,536,730,756]" name="Indonesia" pageId="17" pageNumber="94">Indonesia</collectingCountry>
, East 
<collectingCountry box="[601,703,730,756]" name="Malaysia" pageId="17" pageNumber="94">Malaysia</collectingCountry>
, West 
<collectingCountry box="[775,874,730,756]" name="Malaysia" pageId="17" pageNumber="94">Malaysia</collectingCountry>
(
<collectingRegion box="[889,977,730,757]" country="Malaysia" name="Pahang" pageId="17" pageNumber="94">Pahang</collectingRegion>
), 
<collectingCountry box="[995,1107,730,756]" name="Singapore" pageId="17" pageNumber="94">Singapore</collectingCountry>
