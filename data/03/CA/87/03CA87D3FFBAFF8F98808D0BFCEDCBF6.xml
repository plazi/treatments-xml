<paragraph blockId="5.[151,1437,1622,2007]" pageId="5" pageNumber="526">
<emphasis bold="true" box="[199,321,1622,1647]" pageId="5" pageNumber="526">Diagnosis.</emphasis>
<taxonomicName box="[332,546,1622,1647]" class="Insecta" family="Melolonthidae" genus="Miridiba" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="526" phylum="Arthropoda" rank="species" species="huesiotoi">
<emphasis box="[332,546,1622,1647]" italics="true" pageId="5" pageNumber="526">Miridiba huesiotoi</emphasis>
