<quantity box="[327,443,716,740]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" metricValueMax="1.3" metricValueMin="1.1" pageId="18" pageNumber="51" unit="mm" value="12.0" valueMax="13.0" valueMin="11.0">11–13mm</quantity>
