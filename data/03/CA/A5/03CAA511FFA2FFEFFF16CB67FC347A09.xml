female (
<collectionCode box="[664,731,1559,1585]" country="Czech Republic" httpUri="http://grbio.org/cool/fx6k-enrh" name="National Museum (Prague)" pageId="3" pageNumber="578">NMP</collectionCode>
): 
<collectingCountry box="[751,828,1559,1585]" name="Russia" pageId="3" pageNumber="578">Russia</collectingCountry>
, 
<collectingRegion box="[840,1008,1559,1585]" country="Russia" name="Altay" pageId="3" pageNumber="578">Altai Republic</collectingRegion>
, 
<collectingMunicipality box="[1020,1206,1559,1585]" pageId="3" pageNumber="578">Teletskoye Lake</collectingMunicipality>
