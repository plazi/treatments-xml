<location LSID="urn:lsid:plazi:treatment:03CAA511FFA2FFEFFF16CB67FC347A09:8EBC42DCFFA2FFEFFF36C850FEDF7B2A" box="[183,327,1595,1621]" country="Russia" municipality="Teletskoye Lake" name="Koksa River" pageId="3" pageNumber="578" stateProvince="Altay">Koksa River</location>
, 
<date box="[340,479,1595,1621]" pageId="3" pageNumber="578" value="1931-07-23">
<collectingDate box="[340,479,1595,1621]" pageId="3" pageNumber="578" value="1931-07-23">1931.VII.23</collectingDate>
