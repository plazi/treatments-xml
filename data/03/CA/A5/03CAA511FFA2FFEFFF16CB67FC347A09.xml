<materialsCitation collectingDate="1930-07-23" collectingDateMax="1931-07-23" collectingDateMin="1930-07-23" collectionCode="NMP" collectorName="S. G. Lepneva. The &amp; Czech &amp; The &amp; Koksy &amp; Koksi" country="Russia" location="Koksa River" municipality="Teletskoye Lake" pageId="3" pageNumber="578" specimenCount="1" stateProvince="Altay" typeStatus="holotype">
