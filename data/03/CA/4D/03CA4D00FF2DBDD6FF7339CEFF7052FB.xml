<typeStatus box="[287,403,477,503]" pageId="190" pageNumber="191">Lectotype</typeStatus>
: 
<specimenCount box="[417,516,477,503]" pageId="190" pageNumber="191" type="female">1 female</specimenCount>
(
<collectionCode box="[534,624,477,503]" pageId="190" pageNumber="191">BMNH</collectionCode>
), 1) Type; 2) Colombo, 
<collectingCountry box="[904,988,477,503]" name="Sri Lanka" pageId="190" pageNumber="191">Ceylon</collectingCountry>
