</paragraph>
</subSubSection>
<subSubSection box="[199,456,1222,1248]" pageId="83" pageNumber="84" type="biology_ecology">
<paragraph blockId="83.[151,1437,934,2004]" box="[199,456,1222,1248]" pageId="83" pageNumber="84">
<emphasis bold="true" box="[199,345,1222,1248]" pageId="83" pageNumber="84">
<collectingRegion box="[199,255,1222,1248]" country="Afghanistan" name="Khost" pageId="83" pageNumber="84">Host</collectingRegion>
