<collectingRegion box="[778,870,152,178]" country="Saudi Arabia" name="Ash Sharqiyah" pageId="102" pageNumber="103">Oriental</collectingRegion>
<taxonomicName authorityName="Stephens" authorityYear="1831" box="[879,1033,152,178]" class="Insecta" family="Chrysomelidae" genus="Chaetocnema" kingdom="Animalia" order="Coleoptera" pageId="102" pageNumber="103" phylum="Arthropoda" rank="genus">
