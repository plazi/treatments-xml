<subSubSection pageId="145" pageNumber="146" type="description">
<paragraph blockId="145.[151,1437,419,915]" pageId="145" pageNumber="146">
<emphasis bold="true" box="[199,347,563,590]" pageId="145" pageNumber="146">Description:</emphasis>
