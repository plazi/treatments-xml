<collectingCountry box="[304,437,1269,1292]" name="Madagascar" pageId="14" pageNumber="553">Madagascar</collectingCountry>
: former 
<collectingRegion box="[524,769,1269,1292]" country="Madagascar" name="Antananarivo" pageId="14" pageNumber="553">Antananarivo province</collectingRegion>
(
<figureCitation box="[784,864,1269,1292]" captionStart="Fig" captionStartId="17.[735,766,1563,1582]" captionTargetBox="[132,717,811,1624]" captionTargetId="figure-248@17.[129,721,809,1627]" captionTargetPageId="17" captionText="Fig. 67. Distribution of the genus Perinetella Synave, 1956 in Madagascar." figureDoi="http://doi.org/10.5281/zenodo.5372729" httpUri="https://zenodo.org/record/5372729/files/figure.png" pageId="14" pageNumber="553">Fig. 67</figureCitation>
).
</paragraph>
