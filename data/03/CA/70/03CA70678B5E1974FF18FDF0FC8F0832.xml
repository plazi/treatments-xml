<taxonomicName authorityName="Wang &amp; Zhu &amp; Zhao &amp; Yang" authorityYear="2020" baseAuthorityName="Wang" baseAuthorityYear="2004" box="[250,379,801,827]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="377" phylum="Arthropoda" rank="species" species="acutiuscula">
