</taxonomicName>
, 
<taxonomicName authority="Walker" authorityName="Walker" box="[356,574,474,499]" class="Insecta" family="Blaberidae" genus="Epilampra" kingdom="Animalia" order="Blattodea" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="dotata">
<emphasis box="[356,465,474,499]" italics="true" pageId="8" pageNumber="309">M. dotata</emphasis>
