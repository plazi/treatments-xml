<emphasis italics="true" pageId="8" pageNumber="309">M. auriculata</emphasis>
</taxonomicName>
(Brunner von Wattenwyl), 
<taxonomicName box="[575,676,438,463]" class="Insecta" family="Blaberidae" genus="Epilampra" kingdom="Animalia" order="Blattodea" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="badia">
<emphasis box="[575,676,438,463]" italics="true" pageId="8" pageNumber="309">M. badia</emphasis>
</taxonomicName>
(Brunner von Wattenwyl), 
<taxonomicName authority="Anisyutkin &amp; Gorochov" authorityName="Anisyutkin &amp; Gorochov" box="[983,1395,438,463]" class="Insecta" family="Blaberidae" genus="Morphna" kingdom="Animalia" order="Blattodea" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="clypeata">
<emphasis box="[983,1115,438,463]" italics="true" pageId="8" pageNumber="309">M. clypeata</emphasis>
Anisyutkin &amp; Gorochov
</taxonomicName>
, 
<taxonomicName authority="Bolivar" authorityName="Bolivar" class="Insecta" family="Blaberidae" genus="Morphna" kingdom="Animalia" order="Blattodea" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="decolyi">
<emphasis italics="true" pageId="8" pageNumber="309">M. decolyi</emphasis>
