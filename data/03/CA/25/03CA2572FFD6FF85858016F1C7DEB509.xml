</taxonomicName>
, 
<taxonomicName authority="Bruijning" authorityName="Bruijning" box="[585,871,474,499]" class="Insecta" family="Blaberidae" genus="Morphna" kingdom="Animalia" order="Blattodea" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="humeralis">
<emphasis box="[585,735,474,499]" italics="true" pageId="8" pageNumber="309">M. humeralis</emphasis>
