<subSubSection pageId="6" pageNumber="38" type="discussion">
<paragraph blockId="6.[151,1436,151,216]" pageId="6" pageNumber="38">
<emphasis bold="true" box="[197,328,151,177]" pageId="6" pageNumber="38">Comment.</emphasis>
