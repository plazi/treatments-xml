</taxonomicName>
: 
<taxonomicName authority="Borradaile, 1903" authorityName="Borradaile" authorityYear="1903" box="[487,822,324,350]" class="Malacostraca" family="Dromiidae" genus="Cryptodromiopsis" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="tridens">
<emphasis box="[487,608,324,349]" italics="true" pageId="11" pageNumber="12">C. tridens</emphasis>
<bibRefCitation author="Borradaile L. A." box="[621,822,324,350]" pageId="11" pageNumber="12" pagination="297 - 303" refId="ref12791" refString="Borradaile L. A. (1903) On the genera of the Dromiidae. Annals and Magazine of Natural History, (7), 11, 297 - 303." type="journal article" year="1903">Borradaile, 1903</bibRefCitation>
</taxonomicName>
(
<typeStatus box="[844,896,324,350]" pageId="11" pageNumber="12">type</typeStatus>
species), 
<taxonomicName authority="(Stimpson, 1858)" baseAuthorityName="Stimpson" baseAuthorityYear="1858" class="Malacostraca" family="Dromiidae" genus="Cryptodromiopsis" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="antillensis">
<emphasis box="[1023,1183,324,349]" italics="true" pageId="11" pageNumber="12">C. antillensis</emphasis>
(
<bibRefCitation author="Stimpson, W." pageId="11" pageNumber="12" pagination="225 - 252" refId="ref16229" refString="Stimpson, W. (1858) Prodromus descriptionis animalium evertebratorum, quae in Expeditione ad Oceanum Pacificum Septentrionalem, a Republica Federata missa, Cadwaladaro Ringgold et Johanne Rodgers ducibus, observavit et descripsit W. Stimpson Pars VII. Crustacea Anomura. Proceedings of the Academy of Natural Science, Philadelphia 10, 4, 225 - 252, figs. 1 - 60." type="journal article" year="1858">Stimpson, 1858</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Alcock, 1900)" baseAuthorityName="Alcock" baseAuthorityYear="1900" box="[359,719,364,390]" class="Malacostraca" family="Dromiidae" genus="Cryptodromia" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="bullifera">
<emphasis box="[359,507,364,389]" italics="true" pageId="11" pageNumber="12">C. bullifera</emphasis>
(
<bibRefCitation author="Alcock A." box="[534,709,364,390]" pageId="11" pageNumber="12" pagination="123 - 169" refId="ref12526" refString="Alcock A. (1900) Materials for a carcinological fauna of India. No. 5. Brachyura Primigenia or Dromiacea. Journal of the Asiatic Society, Bengal, 1899 (1900), 68, 123 - 169." type="journal article" year="1900">Alcock, 1900</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Rathbun, 1910 a)" baseAuthorityName="Rathbun" baseAuthorityYear="1910" box="[742,1153,364,390]" class="Malacostraca" family="Dromiidae" genus="Cryptodromiopsis" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="sarraburei">
<emphasis box="[742,913,364,389]" italics="true" pageId="11" pageNumber="12">C. sarraburei</emphasis>
(
<bibRefCitation author="Rathbun, M. J." box="[941,1143,364,390]" pageId="11" pageNumber="12" pagination="23 - 55" refId="ref15399" refString="Rathbun, M. J. (1910 a) The stalk-eyed Crustacea of Peru and the adjacent coast. Proceedings of United States National Museum, 26 (1307), 23 - 55, figs. 1 - 24." type="journal article" year="1910">Rathbun, 1910a</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Lewinsohn, 1984)" baseAuthorityName="Lewinsohn" baseAuthorityYear="1984" class="Malacostraca" family="Dromiidae" genus="Cryptodromiopsis" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="plumosa">
<emphasis box="[1177,1322,364,389]" italics="true" pageId="11" pageNumber="12">C. plumosa</emphasis>
(
<bibRefCitation author="Lewinsohn, C." box="[272,490,404,430]" pageId="11" pageNumber="12" pagination="89 - 129" refId="ref14081" refString="Lewinsohn, C. (1984) Dromiidae from Madagascar and the Seychelles (Crustacea Decapoda Brachyura), Bulletin du Museum national d'Histoire naturelle, Paris (4) 6 A (1), 89 - 129, figs. 1 - 4, pls. 1 - 4." type="journal article" year="1984">Lewinsohn, 1984</bibRefCitation>
