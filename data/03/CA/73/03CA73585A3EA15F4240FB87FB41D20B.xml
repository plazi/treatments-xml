<taxonomicName authority="Borradaile, 1903" authorityName="Borradaile" authorityYear="1903" class="Malacostraca" family="Dromiidae" genus="Cryptodromiopsis" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
<emphasis box="[1112,1322,284,309]" italics="true" pageId="11" pageNumber="12">Cryptodromiopsis</emphasis>
