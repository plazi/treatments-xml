<quantity box="[185,345,854,878]" metricMagnitude="-3" metricUnit="m" metricValue="3.52" metricValueMax="5.68" metricValueMin="1.36" pageId="4" pageNumber="297" unit="mm" value="3.52" valueMax="5.68" valueMin="1.36">1.36–5.68 mm</quantity>
; OsL, 
<quantity box="[418,592,854,878]" metricMagnitude="-3" metricUnit="m" metricValue="6.305" metricValueMax="10.01" metricValueMin="2.6" pageId="4" pageNumber="297" unit="mm" value="6.305" valueMax="10.01" valueMin="2.6">2.60–10.01 mm</quantity>
