<quantity box="[363,535,822,846]" metricMagnitude="-2" metricUnit="m" metricValue="1.274" metricValueMax="1.944" metricValueMin="0.604" pageId="4" pageNumber="297" unit="mm" value="12.74" valueMax="19.44" valueMin="6.04">6.04–19.44 mm</quantity>
; OH, 
<quantity box="[599,771,822,846]" metricMagnitude="-3" metricUnit="m" metricValue="9.605" metricValueMax="14.64" metricValueMin="4.57" pageId="4" pageNumber="297" unit="mm" value="9.605" valueMax="14.64" valueMin="4.57">4.57–14.64 mm</quantity>
