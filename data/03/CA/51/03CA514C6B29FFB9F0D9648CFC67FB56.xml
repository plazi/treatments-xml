<quantity box="[984,1098,839,863]" metricMagnitude="2" metricUnit="m" metricValue="6.4" metricValueMax="7.0" metricValueMin="5.8" pageId="19" pageNumber="778" unit="m" value="640.0" valueMax="700.0" valueMin="580.0">
