<taxonomicName authorityName="Cantor" authorityYear="1846" box="[967,1165,1210,1230]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="1" pageNumber="7478" phylum="Chordata" rank="species" species="galeritus">
