<taxonomicName baseAuthorityName="Mordvilko" baseAuthorityYear="1895" box="[902,1003,789,815]" class="Insecta" family="Aphididae" genus="Cinara" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="395" phylum="Arthropoda" rank="species" species="pinea">C. pinea</taxonomicName>
, 
<taxonomicName box="[1015,1220,789,815]" class="Insecta" family="Aphididae" genus="Cinara" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="395" phylum="Arthropoda" rank="species" species="piniformosana">C. piniformosana</taxonomicName>
, С. pruinosa
</emphasis>
, 
<taxonomicName box="[313,465,821,847]" class="Insecta" family="Aphididae" genus="Cinara" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="395" phylum="Arthropoda" rank="species" species="pubicornis">
