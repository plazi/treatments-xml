, 
<taxonomicName box="[478,679,821,847]" class="Insecta" family="Aphididae" genus="Cinara" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="395" phylum="Arthropoda" rank="species" species="piniformosana">
