<taxonomicName baseAuthorityName="Mordvilko" baseAuthorityYear="1895" box="[328,481,784,810]" class="Insecta" family="Aphididae" genus="Cinara" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="396" phylum="Arthropoda" rank="species" species="pinea">
