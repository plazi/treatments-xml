<taxonomicName authorityName="Meigen" authorityYear="1826" box="[535,681,813,833]" class="Insecta" family="Scathophagidae" genus="Cordylura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="147" phylum="Arthropoda" rank="species" species="macrocera">Cord. macrocera</taxonomicName>
