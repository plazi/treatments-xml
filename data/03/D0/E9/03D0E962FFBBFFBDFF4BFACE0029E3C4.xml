<collectingCountry box="[151,218,2010,2035]" name="China" pageId="1" pageNumber="146">China</collectingCountry>
and from 
<collectingCountry box="[333,474,2010,2035]" name="South Korea" pageId="1" pageNumber="146">South Korea</collectingCountry>
