<emphasis box="[654,777,441,468]" italics="true" pageId="10" pageNumber="61">C. foliacea</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Duchassaing &amp; Michelotti" baseAuthorityYear="1864" box="[791,940,441,467]" class="Demospongiae" family="Crambeidae" genus="Monanchora" kingdom="Animalia" order="Poecilosclerida" pageId="10" pageNumber="61" phylum="Porifera" rank="species" species="arbuscula">
<emphasis box="[791,940,441,467]" italics="true" pageId="10" pageNumber="61">M. arbuscula</emphasis>
</taxonomicName>
, 
<taxonomicName authority="(Duchassaing &amp; Michelotti)" authorityName="Duchassaing &amp; Michelotti" baseAuthorityName="Duchassaing &amp; Michelotti" baseAuthorityYear="1864" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="10" pageNumber="61" phylum="Porifera" rank="species" species="laxissima" subGenus="Arenochalina">
