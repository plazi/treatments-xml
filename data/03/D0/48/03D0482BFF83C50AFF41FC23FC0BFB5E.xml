, 
<number box="[368,427,1076,1102]" pageId="15" pageNumber="16" value="433.0">433</number>
/ 
<number box="[436,450,1077,1103]" pageId="15" pageNumber="16" value="4.0">4</number>
ƤƤ, 
<taxonomicName authority="Reitter., Gultekin" authorityName="Reitter., Gultekin" box="[508,886,1077,1103]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="farinifer">
