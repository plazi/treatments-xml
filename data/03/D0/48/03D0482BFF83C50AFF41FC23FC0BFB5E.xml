<typeStatus box="[161,284,1040,1066]" pageId="15" pageNumber="16">Paratypus</typeStatus>
, 
<number box="[296,358,1040,1066]" pageId="15" pageNumber="16" value="1892.0">1892</number>
, 
<taxonomicName authority="Reitter" authorityName="Reitter" box="[371,634,1040,1066]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="farinifer">
