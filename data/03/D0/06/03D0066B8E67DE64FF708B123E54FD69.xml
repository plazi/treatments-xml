<paragraph blockId="1.[151,1436,448,695]" pageId="1" pageNumber="56">
<emphasis box="[151,547,608,631]" italics="true" pageId="1" pageNumber="56">
<taxonomicName box="[151,300,608,631]" class="Insecta" family="Chrysopidae" genus="Ceraeochrysa" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="genus">Ceraeochrysa</taxonomicName>
(= 
<taxonomicName box="[341,446,608,631]" class="Insecta" family="Chrysopidae" genus="Chrysopa" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="genus">Chrysopa</taxonomicName>
) 
<taxonomicName box="[460,547,608,631]" class="Insecta" family="Chrysopidae" genus="Ceraeochrysa" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="species" species="caligata">caligata</taxonomicName>
</emphasis>
