Navás 
<number box="[473,526,152,175]" pageId="3" pageNumber="58" value="1928.0">1928</number>
</taxonomicName>
[
<number box="[544,605,152,175]" pageId="3" pageNumber="58" value="1929.0">1929</number>
]: 
<number box="[627,654,152,175]" pageId="3" pageNumber="58" value="32.0">32</number>
