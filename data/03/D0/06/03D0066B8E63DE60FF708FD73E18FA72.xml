(
<number box="[916,974,1594,1617]" pageId="5" pageNumber="60" value="2009.0">2009</number>
</bibRefCitation>
: 
<number box="[987,1031,1594,1617]" pageId="5" pageNumber="60" value="568.0">568</number>
</treatmentCitation>
