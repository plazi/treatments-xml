<number box="[1102,1161,1484,1508]" pageId="5" pageNumber="51" value="2004.0">2004</number>
)
</bibRefCitation>
except or a higher LR, LR 
<number box="[187,195,1535,1549]" pageId="5" pageNumber="51" value="1.0">1</number>
(
<number box="[209,263,1519,1543]" pageId="5" pageNumber="51" value="0.78">0.78</number>
), LR 
<number box="[318,326,1535,1549]" pageId="5" pageNumber="51" value="2.0">2</number>
(
<number box="[340,392,1519,1543]" pageId="5" pageNumber="51" value="0.5">0.50</number>
) and LR 
<number box="[490,498,1535,1549]" pageId="5" pageNumber="51" value="3.0">3</number>
<number box="[505,557,1519,1543]" pageId="5" pageNumber="51" value="0.59">0.59</number>
, in contrast with Norwegian specimens with LR 
<number box="[1095,1103,1535,1549]" pageId="5" pageNumber="51" value="1.0">1</number>
<number box="[1109,1161,1519,1543]" pageId="5" pageNumber="51" value="0.68">0.68</number>
, LR 
<number box="[1208,1216,1535,1549]" pageId="5" pageNumber="51" value="2.0">2</number>
<number box="[1220,1334,1519,1543]" pageId="5" pageNumber="51" value="0.435" valueMax="0.44" valueMin="0.43">0.43–0.44</number>
, and LR 
