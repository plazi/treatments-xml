from the 3rd section of 
<location LSID="urn:lsid:plazi:treatment:03D087B7D175FFC668F72FCEFBA2FCDE:8EA6607AD175FFC769EA2CA4FD82F918" box="[443,581,1749,1775]" country="Belgium" municipality="Antwerp area" name="Borgerhout" pageId="4" pageNumber="423">Borgerhout</location>
, 
<collectingMunicipality box="[591,753,1749,1775]" pageId="4" pageNumber="423">Antwerp area</collectingMunicipality>
