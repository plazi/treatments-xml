(); 
<taxonomicName authority="Urb." authorityName="Urb." box="[394,527,1159,1179]" class="Magnoliopsida" family="Rhamnaceae" genus="Crumenaria" kingdom="Plantae" order="Rosales" pageId="2" pageNumber="269" phylum="Tracheophyta" rank="species" species="glaziovii">C. glaziovii Urb.</taxonomicName>
