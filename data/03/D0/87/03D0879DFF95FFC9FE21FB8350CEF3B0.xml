(- 
<emphasis box="[928,941,604,627]" italics="true" pageId="8" pageNumber="69">a</emphasis>
, - 
<emphasis box="[964,997,604,627]" italics="true" pageId="8" pageNumber="69">um</emphasis>
) given after the 
<typeStatus box="[190,236,636,659]" pageId="8" pageNumber="69">type</typeStatus>
locality, 
<collectingRegion box="[335,436,636,659]" country="Ecuador" name="Cotopaxi" pageId="8" pageNumber="69">Cotopaxi</collectingRegion>
.
</paragraph>
