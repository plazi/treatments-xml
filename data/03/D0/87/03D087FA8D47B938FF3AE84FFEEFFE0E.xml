<paragraph blockId="14.[151,1436,151,393]" pageId="14" pageNumber="385">
<emphasis bold="true" box="[199,433,294,320]" pageId="14" pageNumber="385">Material examined.</emphasis>
<materialsCitation ID-GBIF-Occurrence="3383507304" collectingDate="1984-05-25" collectingDateMax="1984-05-27" collectingDateMin="1984-05-25" collectionCode="FSCA" collectorName="J. E. Wappes" country="Mexico" location="Chichen Itza" pageId="14" pageNumber="385" specimenCount="1" specimenCount-female="1" stateProvince="Yucatan">
<collectingCountry box="[441,554,295,321]" name="Mexico" pageId="14" pageNumber="385">MEXICO</collectingCountry>
, 
<emphasis box="[566,655,295,321]" italics="true" pageId="14" pageNumber="385">
<collectingRegion box="[566,655,295,321]" country="Mexico" name="Yucatan" pageId="14" pageNumber="385">Yucatán</collectingRegion>
</emphasis>
(
<emphasis bold="true" box="[672,870,295,321]" pageId="14" pageNumber="385">new state record</emphasis>
): 
