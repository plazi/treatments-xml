. MHNLCLFE 
<collectingCountry box="[583,621,368,394]" name="American Samoa" pageId="12" pageNumber="13">AS</collectingCountry>
<number box="[628,660,368,394]" pageId="12" pageNumber="13" value="15.0">15</number>
