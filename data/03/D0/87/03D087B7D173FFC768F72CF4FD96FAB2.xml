<location LSID="urn:lsid:plazi:treatment:03D087B7D173FFC768F72CF4FD96FAB2:8EA6607AD173FFC16AA02C87FBEFF8E7" box="[753,1064,1782,1808]" country="Denmark" municipality="Gram" name="claypit of Gram Teglvaerk" pageId="2" pageNumber="421" stateProvince="Jutland">claypit of Gram Teglvaerk</location>
(
<geoCoordinate box="[1080,1220,1782,1809]" pageId="2" pageNumber="421">55.29°17' N</geoCoordinate>
, 
<geoCoordinate box="[166,278,1814,1841]" pageId="2" pageNumber="421">9.05°2' E</geoCoordinate>
