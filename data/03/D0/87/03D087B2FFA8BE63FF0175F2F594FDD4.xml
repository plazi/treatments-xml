(Fig. 36), in 
<quantity box="[974,1092,1444,1468]" metricMagnitude="2" metricUnit="m" metricValue="5.3149999999999995" metricValueMax="7.13" metricValueMin="3.5" pageId="76" pageNumber="663" unit="m" value="531.5" valueMax="713.0" valueMin="350.0">
<elevation box="[974,1092,1444,1468]" metricMagnitude="2" metricUnit="m" metricValue="5.3149999999999995" metricValueMax="7.13" metricValueMin="3.5" pageId="76" pageNumber="663" unit="m" value="531.5" valueMax="713.0" valueMin="350.0">350-713 m</elevation>
</quantity>
, live in 
<quantity box="[1173,1291,1444,1468]" metricMagnitude="2" metricUnit="m" metricValue="8.315" metricValueMax="9.5" metricValueMin="7.13" pageId="76" pageNumber="663" unit="m" value="831.5" valueMax="950.0" valueMin="713.0">
<elevation box="[1173,1291,1444,1468]" metricMagnitude="2" metricUnit="m" metricValue="8.315" metricValueMax="9.5" metricValueMin="7.13" pageId="76" pageNumber="663" unit="m" value="831.5" valueMax="950.0" valueMin="713.0">713-950 m</elevation>
