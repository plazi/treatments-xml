<quantity box="[564,659,1650,1676]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" pageId="26" pageNumber="27" unit="mm" value="5.1">5.1 mm</quantity>
long, 
<quantity box="[740,835,1650,1676]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="26" pageNumber="27" unit="mm" value="2.8">2.8 mm</quantity>
