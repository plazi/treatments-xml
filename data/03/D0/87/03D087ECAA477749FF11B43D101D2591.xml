<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="26" pageNumber="27" unit="mm" value="3.1">3.1 mm</quantity>
long, 
<quantity box="[279,374,1650,1676]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="26" pageNumber="27" unit="mm" value="2.7">2.7 mm</quantity>
