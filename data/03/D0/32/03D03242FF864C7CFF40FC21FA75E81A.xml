<taxonomicName authority="McAtee &amp; Malloch, 1925" authorityName="McAtee &amp; Malloch" authorityYear="1925" box="[366,532,1377,1404]" class="Insecta" family="Schizopteridae" genus="Hoplonannus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="356" phylum="Arthropoda" rank="genus">
