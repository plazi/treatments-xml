<paragraph blockId="26.[151,386,1591,1616]" box="[151,386,1591,1616]" pageId="26" pageNumber="27">
<heading bold="true" box="[151,386,1591,1616]" fontSize="10" level="2" pageId="26" pageNumber="27" reason="2">
<taxonomicName authority="Evans" authorityName="Evans" box="[151,386,1591,1616]" class="Arachnida" family="Ascidae" genus="Neojordensia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,386,1591,1616]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[151,305,1591,1616]" italics="true" pageId="26" pageNumber="27">Neojordensia</emphasis>
Evans
</emphasis>
</taxonomicName>
</heading>
</paragraph>
