<taxonomicName authority="Berlese" authorityName="Berlese" box="[151,374,1772,1797]" class="Arachnida" family="Ascidae" genus="Hoploseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
