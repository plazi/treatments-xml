<paragraph blockId="49.[151,490,1303,1328]" box="[151,490,1303,1328]" pageId="49" pageNumber="50">
<heading bold="true" box="[151,490,1303,1328]" fontSize="10" level="2" pageId="49" pageNumber="50" reason="2">
<taxonomicName authority="Athias" authorityName="Athias" box="[151,490,1303,1328]" class="Arachnida" family="Ascidae" genus="Orthadenella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="49" pageNumber="50" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,490,1303,1328]" pageId="49" pageNumber="50">
<emphasis bold="true" box="[151,304,1303,1328]" italics="true" pageId="49" pageNumber="50">Orthadenella</emphasis>
Athias -Henriot
</emphasis>
</taxonomicName>
</heading>
</paragraph>
