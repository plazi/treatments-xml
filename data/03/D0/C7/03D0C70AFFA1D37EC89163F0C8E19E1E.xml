<taxonomicName authority="Berlese" authorityName="Berlese" box="[151,355,1484,1509]" class="Arachnida" family="Ascidae" genus="Leioseius" kingdom="Animalia" order="Mesostigmata" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,355,1484,1509]" pageId="24" pageNumber="25">
<emphasis bold="true" box="[151,260,1484,1509]" italics="true" pageId="24" pageNumber="25">Leioseius</emphasis>
Berlese
</emphasis>
</taxonomicName>
</heading>
</paragraph>
