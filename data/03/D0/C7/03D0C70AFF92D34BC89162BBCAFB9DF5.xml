<taxonomicName authority="Berlese" authorityName="Berlese" box="[151,361,1159,1184]" class="Arachnida" family="Ascidae" genus="Platyseius" kingdom="Animalia" order="Mesostigmata" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,361,1159,1184]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[151,268,1159,1184]" italics="true" pageId="43" pageNumber="44">Platyseius</emphasis>
Berlese
</emphasis>
</taxonomicName>
