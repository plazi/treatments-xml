<taxonomicName authority="Oudemans" authorityName="Oudemans" box="[785,1086,326,351]" class="Arachnida" family="Neoparasitidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="245" pageNumber="246" phylum="Arthropoda" rank="family">Neoparasitidae Oudemans</taxonomicName>
