(1); 
<taxonomicName authority="Sharp" authorityName="Sharp. The" authorityYear="1887" box="[194,356,1981,2006]" class="Insecta" family="Scydmaenidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="211" phylum="Arthropoda" rank="species" species="giraffa">
<emphasis box="[194,291,1981,2005]" italics="true" pageId="2" pageNumber="211">E. giraffa</emphasis>
Sharp
</taxonomicName>
(2); 
<taxonomicName authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[405,515,1981,2005]" class="Insecta" family="Scydmaenidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="211" phylum="Arthropoda" rank="species" species="agiraffa" status="sp. n.">
<emphasis box="[405,515,1981,2005]" italics="true" pageId="2" pageNumber="211">E. agiraffa</emphasis>
</taxonomicName>
<emphasis bold="true" box="[521,578,1981,2005]" pageId="2" pageNumber="211">
<taxonomicNameLabel box="[521,578,1981,2005]" pageId="2" pageNumber="211" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
(3–4); 
<taxonomicName authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[652,788,1982,2005]" class="Insecta" family="Scydmaenidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="211" phylum="Arthropoda" rank="species" species="oaxacanus" status="sp. n.">
<emphasis box="[652,788,1982,2005]" italics="true" pageId="2" pageNumber="211">E. oaxacanus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[795,851,1981,2005]" pageId="2" pageNumber="211">
<taxonomicNameLabel box="[795,851,1981,2005]" pageId="2" pageNumber="211" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
(5); 
<taxonomicName authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[900,1043,1981,2005]" class="Insecta" family="Scydmaenidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="211" phylum="Arthropoda" rank="species" species="bicarinatus" status="sp. n.">
<emphasis box="[900,1043,1981,2005]" italics="true" pageId="2" pageNumber="211">E. bicarinatus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1050,1107,1981,2005]" pageId="2" pageNumber="211">
<taxonomicNameLabel box="[1050,1107,1981,2005]" pageId="2" pageNumber="211" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
(6); 
<taxonomicName authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[1156,1303,1981,2005]" class="Insecta" family="Scydmaenidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="211" phylum="Arthropoda" rank="species" species="cordobanus" status="sp. n.">
<emphasis box="[1156,1303,1981,2005]" italics="true" pageId="2" pageNumber="211">E. cordobanus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1309,1365,1981,2005]" pageId="2" pageNumber="211">
<taxonomicNameLabel box="[1309,1365,1981,2005]" pageId="2" pageNumber="211" rank="species">sp. n.</taxonomicNameLabel>
