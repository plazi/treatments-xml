<paragraph blockId="3.[264,905,1161,1187]" box="[264,905,1161,1187]" pageId="3" pageNumber="42">
<heading bold="true" box="[264,905,1161,1187]" fontSize="11" level="1" pageId="3" pageNumber="42" reason="1">
<emphasis bold="true" box="[264,905,1161,1187]" pageId="3" pageNumber="42">
<taxonomicName authority="McCarthy and Gerken" authorityName="McCarthy and Gerken" authorityYear="2006" box="[264,783,1161,1187]" class="Malacostraca" family="Pseudocumatidae" genus="Monopseudocuma" kingdom="Animalia" order="Cumacea" pageId="3" pageNumber="42" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[264,487,1161,1187]" italics="true" pageId="3" pageNumber="42">Monopseudocuma</emphasis>
McCarthy and Gerken
</taxonomicName>
, 
<taxonomicNameLabel box="[796,905,1161,1187]" pageId="3" pageNumber="42" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
