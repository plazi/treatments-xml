) (
<bibRefCitation author="Riedel, M." box="[888,1030,1117,1143]" pageId="14" pageNumber="515" pagination="1477 - 1611" refId="ref47533" refString="Riedel, M. (2012) Revision der westpalaarktischen Arten der Gattung Coelichneumon Thomson (Hymenoptera, Ichneumonidae). Linzer Biologische Beitrage, 44, 1477 - 1611." type="journal article" year="2012">Riedel 2012</bibRefCitation>
),? 
<collectingCountry box="[1068,1143,1117,1143]" name="Russia" pageId="14" pageNumber="515">Russia</collectingCountry>
Far East (
<bibRefCitation author="Meyer, N. F." box="[1257,1399,1117,1143]" pageId="14" pageNumber="515" pagination="165 - 180" refId="ref47209" refString="Meyer, N. F. (1930) Scientific results of the entomological expedition of the Zoological Museum in Ussur territory: I. Hymenoptera, Ichneumonidae. Ezhegodnik Zoologicheskago Muzeya, Akademii Nauk SSR, 31, 165 - 180. [in Russian]" type="journal article" year="1930">Meyer 1930</bibRefCitation>
).
</paragraph>
