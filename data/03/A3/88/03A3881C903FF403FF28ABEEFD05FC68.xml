<emphasis box="[697,812,2015,2036]" italics="true" pageId="30" pageNumber="331">A. cristatus</emphasis>
</taxonomicName>
(
<emphasis bold="true" box="[827,845,2014,2036]" pageId="30" pageNumber="331">C</emphasis>
) and 
<taxonomicName box="[903,1070,2014,2036]" class="Insecta" family="Phaneropteridae" genus="Apteroscirtus" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="331" phylum="Arthropoda" rank="species" species="planidorsatus">
<emphasis box="[903,1070,2014,2036]" italics="true" pageId="30" pageNumber="331">A. planidorsatus</emphasis>
</taxonomicName>
(
<emphasis bold="true" box="[1085,1103,2014,2036]" pageId="30" pageNumber="331">D</emphasis>
