<taxonomicName authorityName="Dlussky" authorityYear="1996" box="[997,1134,1130,1150]" class="Hexapoda" family="Formicidae" genus="Haidomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">Haidomyrmex</taxonomicName>
