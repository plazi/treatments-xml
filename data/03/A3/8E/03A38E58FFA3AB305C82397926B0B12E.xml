<taxonomicName baseAuthorityName="Moser" baseAuthorityYear="1907" box="[293,439,937,959]" class="Nuda" family="Beroidae" genus="Beroe" kingdom="Animalia" order="Beroida" pageId="15" pageNumber="312" phylum="Ctenophora" rank="species" species="mitrata">
