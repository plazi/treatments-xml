<paragraph blockId="15.[151,1437,150,789]" box="[199,387,582,609]" pageId="15" pageNumber="424">
<emphasis bold="true" box="[199,255,582,608]" pageId="15" pageNumber="424">Host</emphasis>
. Unknown.
</paragraph>
</subSubSection>
<subSubSection box="[199,511,618,645]" pageId="15" pageNumber="424" type="distribution">
<paragraph blockId="15.[151,1437,150,789]" box="[199,511,618,645]" pageId="15" pageNumber="424">
<emphasis bold="true" box="[199,345,618,644]" pageId="15" pageNumber="424">Distribution</emphasis>
. 
<collectingCountry box="[359,427,618,644]" name="Brazil" pageId="15" pageNumber="424">Brazil</collectingCountry>
(
<collectingRegion box="[443,499,618,644]" country="Brazil" name="Para" pageId="15" pageNumber="424">Para</collectingRegion>
).
</paragraph>
