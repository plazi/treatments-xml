<collectionCode LSID="urn:lsid:biocol.org:col:34992" box="[277,365,1536,1562]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="31" pageNumber="32">RMNH</collectionCode>
