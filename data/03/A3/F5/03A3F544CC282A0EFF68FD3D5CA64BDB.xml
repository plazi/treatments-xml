<subSubSection pageId="24" type="distribution">
<paragraph blockId="24.[151,1437,829,1502]" pageId="24">
<emphasis bold="true" box="[199,352,1153,1178]" pageId="24">Distribution.</emphasis>
<taxonomicName authorityName="Byers" authorityYear="1965" box="[362,616,1153,1178]" class="Insecta" family="Panorpidae" genus="Neopanorpa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mecoptera" pageId="24" phylum="Arthropoda" rank="species" species="spatulata">
