<subSubSection pageId="3" pageNumber="593" type="etymology">
<paragraph blockId="3.[151,1436,151,680]" pageId="3" pageNumber="593">
<emphasis bold="true" box="[199,334,620,645]" pageId="3" pageNumber="593">Etymology.</emphasis>
