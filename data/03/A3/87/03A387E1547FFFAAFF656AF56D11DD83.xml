<subSubSection box="[198,1120,191,218]" pageId="22" pageNumber="23" type="etymology">
<paragraph blockId="22.[198,1120,151,218]" box="[198,1120,191,218]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[198,339,191,217]" pageId="22" pageNumber="23">Etymology.</emphasis>
