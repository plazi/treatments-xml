<quantity box="[576,685,1829,1855]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" pageId="4" pageNumber="61" unit="mm" value="2.25">2.25 mm</quantity>
, width 
<quantity box="[774,884,1829,1855]" metricMagnitude="-4" metricUnit="m" metricValue="6.7" pageId="4" pageNumber="61" unit="mm" value="0.67">0.67 mm</quantity>
. Parameres 
<quantity box="[1029,1135,1829,1855]" metricMagnitude="-4" metricUnit="m" metricValue="7.199999999999999" pageId="4" pageNumber="61" unit="mm" value="0.72">0.72 mm</quantity>
