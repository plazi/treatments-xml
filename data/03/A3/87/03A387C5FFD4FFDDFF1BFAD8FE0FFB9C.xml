</emphasis>
: 
<specimenCode box="[729,848,1473,1500]" collectionCode="HUIM" pageId="6" pageNumber="145">HUIM195</specimenCode>
,
</materialsCitation>
<materialsCitation ID-GBIF-Occurrence="3744360303" box="[859,1030,1473,1499]" collectionCode="HUIM" pageId="6" pageNumber="145" specimenCode="HUIM198-202" specimenCount="1" typeStatus="paratype">
