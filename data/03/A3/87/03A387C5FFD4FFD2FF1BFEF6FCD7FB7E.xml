<taxonomicName authority="Herbst, 1783" authorityName="Herbst" authorityYear="1783" box="[294,620,350,374]" class="Malacostraca" family="Cancridae" genus="Cancer" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="145" phylum="Arthropoda" rank="species" species="bispinosus">
