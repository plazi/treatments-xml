<number box="[614,673,1207,1233]" pageId="11" pageNumber="12" value="3600.0">3600</number>
Met. / 
<collectingCountry box="[761,880,1207,1233]" name="United States of America" pageId="11" pageNumber="12">Columbia</collectingCountry>
