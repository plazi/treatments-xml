<number box="[387,418,692,716]" pageId="9" pageNumber="547" value="36.0">36</number>
; F 
<number box="[447,460,692,716]" pageId="9" pageNumber="547" value="5.0">5</number>
<number box="[466,497,692,716]" pageId="9" pageNumber="547" value="53.0">53</number>
; F 
<number box="[525,538,692,716]" pageId="9" pageNumber="547" value="6.0">6</number>
