<number box="[1116,1176,151,175]" pageId="9" pageNumber="10" value="1899.0">1899</number>
</bibRefCitation>
), 
<collectingCountry box="[1198,1288,151,175]" name="Georgia" pageId="9" pageNumber="10">Georgia</collectingCountry>
(
<bibRefCitation author="Lohmander" pageId="9" pageNumber="10" refString="Lohmander, H. (1936) Uber die Diplopoden des Kaukasusgebietes. Goteborgs Kungliga Vetenskaps- och Vitterhets-Samhalles handlingar, 5. Foljden, Ser. B, 5 (1), 1 - 196." type="journal article" year="1936">
