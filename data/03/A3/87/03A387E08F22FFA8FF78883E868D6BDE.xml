<number box="[151,211,188,212]" pageId="9" pageNumber="10" value="1936.0">1936</number>
</bibRefCitation>
), and 
<collectingCountry box="[276,356,188,212]" name="Turkey" pageId="9" pageNumber="10">Turkey</collectingCountry>
(
<bibRefCitation author="Enghoff" box="[370,529,188,212]" pageId="9" pageNumber="10" refString="Enghoff, H. (2006) The millipedes of Turkey (Diplopoda). Steenstrupia, vol 29, no. 2, pp. 175 - 198." type="journal article" year="2006">
Enghoff 
<number box="[469,529,188,212]" pageId="9" pageNumber="10" value="2006.0">2006</number>
</bibRefCitation>
), 
<emphasis box="[548,738,188,212]" italics="true" pageId="9" pageNumber="10">M. b. dagestanus</emphasis>
<bibRefCitation author="Lohmander" box="[744,941,188,212]" pageId="9" pageNumber="10" refString="Lohmander, H. (1936) Uber die Diplopoden des Kaukasusgebietes. Goteborgs Kungliga Vetenskaps- och Vitterhets-Samhalles handlingar, 5. Foljden, Ser. B, 5 (1), 1 - 196." type="journal article" year="1936">
