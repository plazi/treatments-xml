<taxonomicName box="[607,714,513,536]" class="Gastropoda" family="Raphitomidae" genus="Teretia" kingdom="Animalia" order="Neogastropoda" pageId="5" pageNumber="565" phylum="Mollusca" rank="species" species="anceps">
<emphasis box="[607,714,513,536]" italics="true" pageId="5" pageNumber="565">T. anceps</emphasis>
</taxonomicName>
and 
<taxonomicName box="[769,849,513,536]" class="Gastropoda" family="Raphitomidae" genus="Teretia" kingdom="Animalia" order="Neogastropoda" pageId="5" pageNumber="565" phylum="Mollusca" rank="species" species="teres">
<emphasis box="[769,849,513,536]" italics="true" pageId="5" pageNumber="565">T. teres</emphasis>
</taxonomicName>
