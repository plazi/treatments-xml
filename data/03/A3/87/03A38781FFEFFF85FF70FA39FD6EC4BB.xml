<taxonomicName authority="Gorham" authorityName="Gorham" box="[1202,1436,386,408]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="nigra" subGenus="Cycadophila">
