º 
<number box="[1120,1151,1938,1962]" pageId="3" pageNumber="536" value="45.0">45</number>
’53,2’’S
</geoCoordinate>
; 
<geoCoordinate box="[1254,1424,1938,1962]" direction="west" orientation="longitude" pageId="3" pageNumber="536" precision="1" value="-42.622944">
<number box="[1254,1285,1938,1962]" pageId="3" pageNumber="536" value="42.0">42</number>
º 
<number box="[1294,1323,1938,1962]" pageId="3" pageNumber="536" value="37.0">37</number>
