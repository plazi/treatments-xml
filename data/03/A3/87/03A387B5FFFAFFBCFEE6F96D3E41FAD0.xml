<emphasis bold="true" box="[264,649,1724,1750]" pageId="47" pageNumber="48">
<taxonomicName authority="Okajima, 2006" authorityName="Okajima" authorityYear="2006" box="[264,548,1724,1750]" class="Insecta" family="Thripidae" genus="Mycterothrips" kingdom="Animalia" order="Thysanoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="fasciatus" status="sp. nov.">
