<location LSID="urn:lsid:plazi:treatment:03A387B5FFF3FFA1FEE6F9B339CAF940:8ED56078FFFCFFA1FB51FBB53BC0FB18" country="Australia" county="western Brisbane" municipality="Small" name="Cooloola National Park" pageId="41" pageNumber="42" stateProvince="Queensland">Cooloola National Park</location>
