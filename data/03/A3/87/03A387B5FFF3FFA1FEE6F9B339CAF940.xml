<location LSID="urn:lsid:plazi:treatment:03A387B5FFF3FFA1FEE6F9B339CAF940:8ED56078FFFCFFA1FCC6FB5D39E3FB18" box="[808,914,1164,1190]" country="Australia" county="western Brisbane" municipality="Small" name="February" pageId="41" pageNumber="42" stateProvince="Queensland">February</location>
and 
<location LSID="urn:lsid:plazi:treatment:03A387B5FFF3FFA1FEE6F9B339CAF940:8ED56078FFFCFFA1FC3DFB5D3E52FB18" box="[979,1059,1164,1190]" country="Australia" county="western Brisbane" municipality="Small" name="March" pageId="41" pageNumber="42" stateProvince="Queensland">March</location>
, 2006. 
<location LSID="urn:lsid:plazi:treatment:03A387B5FFF3FFA1FEE6F9B339CAF940:8ED56078FFFCFFA1FB6EFB5D3EB9FB18" box="[1152,1224,1164,1190]" country="Australia" county="western Brisbane" municipality="Small" name="Slides" pageId="41" pageNumber="42" stateProvince="Queensland">Slides</location>
