<collectingCounty box="[894,1107,1084,1110]" pageId="41" pageNumber="42">western Brisbane</collectingCounty>
, 
<collectingCountry box="[1125,1236,1084,1110]" name="Australia" pageId="41" pageNumber="42">Australia</collectingCountry>
