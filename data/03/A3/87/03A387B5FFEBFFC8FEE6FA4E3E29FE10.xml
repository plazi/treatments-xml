<subSubSection pageId="64" pageNumber="65" type="discussion">
<paragraph blockId="64.[264,1322,283,430]" pageId="64" pageNumber="65">
<emphasis bold="true" box="[312,447,363,389]" pageId="64" pageNumber="65">Comments</emphasis>
