<emphasis box="[608,753,1731,1753]" italics="true" pageId="3" pageNumber="148">D. pterophora</emphasis>
</taxonomicName>
; 19 
<taxonomicName box="[801,918,1731,1753]" class="Insecta" family="Platystictidae" genus="Drepanosticta" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="148" phylum="Arthropoda" rank="species" species="taurulus">
<emphasis box="[801,918,1731,1753]" italics="true" pageId="3" pageNumber="148">D. taurulus</emphasis>
</taxonomicName>
; 20 
<emphasis box="[965,1119,1731,1753]" italics="true" pageId="3" pageNumber="148">
<taxonomicName box="[965,1115,1731,1753]" class="Insecta" family="Platystictidae" genus="Drepanosticta" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="148" phylum="Arthropoda" rank="species" species="lepyricollis">D. lepyricollis</taxonomicName>
.
