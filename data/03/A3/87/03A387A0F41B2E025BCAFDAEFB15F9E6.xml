<subSubSection box="[199,1259,1043,1069]" pageId="5" pageNumber="364" type="etymology">
<paragraph blockId="5.[151,1436,826,1645]" box="[199,1259,1043,1069]" pageId="5" pageNumber="364">
<emphasis bold="true" box="[199,334,1043,1069]" pageId="5" pageNumber="364">Etymology.</emphasis>
