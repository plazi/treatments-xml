<subSubSection box="[199,1001,367,392]" pageId="9" pageNumber="56" type="etymology">
<paragraph blockId="9.[151,1437,151,607]" box="[199,1001,367,392]" pageId="9" pageNumber="56">
<emphasis bold="true" box="[199,334,368,392]" pageId="9" pageNumber="56">Etymology.</emphasis>
