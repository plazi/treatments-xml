<collectingCountry box="[1158,1249,1367,1388]" name="Thailand" pageId="10" pageNumber="11">Thailand</collectingCountry>
............................................................................................ 
<emphasis bold="true" box="[1141,1435,1396,1418]" pageId="10" pageNumber="11">
<taxonomicName box="[1141,1306,1396,1418]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="tansachai" status="new species" subGenus="Strobilophila">
