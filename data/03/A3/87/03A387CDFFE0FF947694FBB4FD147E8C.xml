(
<bibRefCitation author="KVIFTE G. M." box="[568,706,418,442]" pageId="36" pageNumber="825" pagination="29 - 52" refId="ref45858" refString="KVIFTE G. M. 2012: Catalogue and bibliography of Afrotropical Psychodidae: Bruchomyiinae, Psychodinae, Sycoracinae and Trichomyiinae. Zootaxa 3231: 29 - 52." type="journal article" year="2012">KVIFTE 2012</bibRefCitation>
).
