<subSubSection pageId="3" pageNumber="60" type="materials_examined">
<paragraph blockId="3.[151,1436,1433,2034]" box="[199,442,1469,1493]" pageId="3" pageNumber="60">
<typeStatus box="[199,303,1469,1493]" pageId="3" pageNumber="60">Holotype</typeStatus>
