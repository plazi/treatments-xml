<taxonomicName authorityName="Yepishin" authorityYear="2021" baseAuthorityName="Amsel" baseAuthorityYear="1950" box="[723,879,1087,1113]" class="Insecta" family="Pyralidae" genus="Salebria" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="153" phylum="Arthropoda" rank="species" species="tchahabarella">
<emphasis box="[723,879,1087,1113]" italics="true" pageId="4" pageNumber="153">tchahabarella</emphasis>
</taxonomicName>
); 
<collectingCountry box="[910,981,1087,1113]" name="China" pageId="4" pageNumber="153">China</collectingCountry>
: 
<collectingRegion box="[998,1179,1087,1113]" country="China" name="Nei Mongol" pageId="4" pageNumber="153">Inner Mongolia</collectingRegion>
Autonomous Region, 
<collectingRegion box="[151,289,1121,1147]" country="China" name="Ningxia" pageId="4" pageNumber="153">Ningxia Hui</collectingRegion>
Autonomous Region, 
<collectingRegion box="[536,713,1121,1148]" country="China" name="Shandong" pageId="4" pageNumber="153">Shandong prov.</collectingRegion>
, Sof 
<collectingRegion box="[772,924,1121,1148]" country="China" name="Shaanxi" pageId="4" pageNumber="153">Shaanxi prov.</collectingRegion>
(
<bibRefCitation author="Roesler, U." box="[936,1088,1121,1147]" journalOrPublisher="Entomologische Zeitschrift" pageId="4" pageNumber="153" pagination="245 - 260" part="79" refId="ref6927" refString="Roesler, U. (1969) Phycitinen-Studien VII (Lepidoptera, Pyralidae). Entomologische Zeitschrift, 79 (22), 245 - 260." title="Phycitinen-Studien VII (Lepidoptera, Pyralidae)" type="journal article" year="1969">Roesler 1969</bibRefCitation>
; 
