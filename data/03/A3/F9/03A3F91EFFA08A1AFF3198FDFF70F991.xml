<emphasis box="[1037,1056,367,390]" italics="true" pageId="4" pageNumber="575">si</emphasis>
and 
<emphasis box="[1109,1136,367,391]" italics="true" pageId="4" pageNumber="575">
c 
<number box="[1123,1136,367,391]" pageId="4" pageNumber="575" value="1.0">1</number>
