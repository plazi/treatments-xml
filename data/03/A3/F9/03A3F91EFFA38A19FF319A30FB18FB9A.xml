) from 
<collectingCountry box="[1146,1250,832,856]" name="Germany" pageId="7" pageNumber="578">Germany</collectingCountry>
(
<bibRefCitation author="Fritsch" box="[1267,1424,831,856]" pageId="7" pageNumber="578" refString="Fritsch, W. (1958) Die milbengattung Syringophilus Heller, 1880 (subordo Trombidiformes, Fam. Myobiidae Megnin, 1877). Zoologische Jahrbucher Systematik, 86, 227 - 234." type="journal article" year="1958">
Fritsch, 
<number box="[1364,1424,831,856]" pageId="7" pageNumber="578" value="1958.0">1958</number>
</bibRefCitation>
