</taxonomicName>
in 
<collectingCountry box="[1222,1300,868,893]" name="Jordan" pageId="7" pageNumber="578">Jordan</collectingCountry>
, 
<collectingCountry box="[1311,1389,868,893]" name="Poland" pageId="7" pageNumber="578">Poland</collectingCountry>
and 
<collectingCountry box="[151,285,903,928]" name="Kazakhstan" pageId="7" pageNumber="578">Kazakhstan</collectingCountry>
