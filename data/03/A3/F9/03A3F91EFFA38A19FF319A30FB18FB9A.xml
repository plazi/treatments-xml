(
<taxonomicName box="[437,600,940,965]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="578" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
