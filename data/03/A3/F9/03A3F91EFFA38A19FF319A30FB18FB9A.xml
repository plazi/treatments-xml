<taxonomicName authority="Vieillot" authorityName="Vieillot" box="[968,1251,940,965]" class="Aves" family="Acrocephalidae" genus="Acrocephalus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="578" phylum="Chordata" rank="species" species="paludicola">
<emphasis box="[968,1130,940,964]" italics="true" pageId="7" pageNumber="578">A. paludicola</emphasis>
(Vieillot)
</taxonomicName>
in 
<collectingCountry box="[1309,1390,940,965]" name="Poland" pageId="7" pageNumber="578">Poland</collectingCountry>
, 
<taxonomicName authority="Blyth" authorityName="Blyth" class="Aves" family="Acrocephalidae" genus="Acrocephalus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="578" phylum="Chordata" rank="species" species="dumetorum">
<emphasis italics="true" pageId="7" pageNumber="578">A. dumetorum</emphasis>
Blyth
</taxonomicName>
in 
<collectingCountry box="[382,516,975,1000]" name="Kazakhstan" pageId="7" pageNumber="578">Kazakhstan</collectingCountry>
, 
<taxonomicName authority="Bechstein" authorityName="Bechstein" box="[526,791,975,1000]" class="Aves" family="Acrocephalidae" genus="Acrocephalus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="578" phylum="Chordata" rank="species" species="palustris">
<emphasis box="[526,654,976,1000]" italics="true" pageId="7" pageNumber="578">A. palustris</emphasis>
