/ 
<number box="[447,500,1157,1183]" pageId="3" pageNumber="4" value="3.52">3.52</number>
), II 
<number box="[548,614,1157,1183]" pageId="3" pageNumber="4" value="51.71">51.71</number>
(
<number box="[631,698,1157,1183]" pageId="3" pageNumber="4" value="15.16">15.16</number>
/ 
<number box="[708,759,1157,1183]" pageId="3" pageNumber="4" value="1.68">1.68</number>
/ 
<number box="[768,833,1157,1183]" pageId="3" pageNumber="4" value="14.51">14.51</number>
