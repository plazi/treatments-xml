<number box="[743,810,1660,1686]" pageId="3" pageNumber="4" value="36.42">36.42</number>
(
<number box="[827,894,1660,1686]" pageId="3" pageNumber="4" value="10.43">10.43</number>
/ 
<number box="[903,954,1660,1686]" pageId="3" pageNumber="4" value="1.58">1.58</number>
/ 
