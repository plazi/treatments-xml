<number box="[1299,1328,1861,1887]" pageId="4" pageNumber="61" value="30.0">30</number>
(
<number box="[1344,1422,1861,1887]" pageId="4" pageNumber="61" value="29.5" valueMax="31.0" valueMin="28.0">28–31</number>
