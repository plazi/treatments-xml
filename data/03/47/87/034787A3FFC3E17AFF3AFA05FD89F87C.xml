<number box="[1239,1288,1901,1927]" pageId="4" pageNumber="61" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
); empodium simple, 
<number box="[244,259,1941,1967]" pageId="4" pageNumber="61" value="4.0">4</number>
