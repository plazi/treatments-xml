<number box="[332,347,626,652]" pageId="4" pageNumber="61" value="3.0">3</number>
from dorsal base; tarsus 
<number box="[645,660,626,652]" pageId="4" pageNumber="61" value="5.0">5</number>
(
<number box="[677,726,626,652]" pageId="4" pageNumber="61" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
); empodium simple, 
<number box="[975,989,626,652]" pageId="4" pageNumber="61" value="6.0">6</number>
­rayed, tapered solenidion. Legs 
<number box="[1407,1436,626,652]" pageId="4" pageNumber="61" value="27.0">27</number>
(
<number box="[159,238,666,692]" pageId="4" pageNumber="61" value="27.0" valueMax="29.0" valueMin="25.0">25–29</number>
