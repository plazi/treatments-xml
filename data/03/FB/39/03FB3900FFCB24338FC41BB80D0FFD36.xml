<caption httpUri="https://zenodo.org/record/242859/files/figure.png" pageId="4" pageNumber="471" targetBox="[193,1396,206,1857]" targetPageId="4">
<paragraph blockId="4.[151,1436,1897,2010]" pageId="4" pageNumber="471">
