<subSubSection pageId="2" pageNumber="260" type="materials_examined">
<paragraph blockId="2.[151,1436,1841,2010]" pageId="2" pageNumber="260">
<emphasis bold="true" box="[151,386,1841,1866]" inLineHeading="true" pageId="2" pageNumber="260" reason="1">Material examined.</emphasis>
