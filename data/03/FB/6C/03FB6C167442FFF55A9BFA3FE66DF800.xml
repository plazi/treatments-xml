. 
<taxonomicName authority="Ermilov &amp; Behan-Pelletier, 2014" authorityName="Ermilov &amp; Behan-Pelletier" authorityYear="2014" box="[320,555,1800,1822]" class="Arachnida" family="Oribatellidae" genus="Fenestrobates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="4" pageNumber="262" phylum="Arthropoda" rank="species" species="marauni" status="sp. nov.">
