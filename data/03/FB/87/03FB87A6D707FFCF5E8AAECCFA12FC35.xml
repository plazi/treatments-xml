<taxonomicName authorityName="Broun, 1893:" authorityYear="1363" box="[869,995,1886,1911]" class="Insecta" family="Curculionidae" genus="Brachyolus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="437" phylum="Arthropoda" rank="species" species="viridescens">viridescens</taxonomicName>
