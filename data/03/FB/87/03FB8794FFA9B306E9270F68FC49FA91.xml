<taxonomicName authority="ZMUC CRU" authorityName="ZMUC CRU" box="[401,845,1427,1450]" class="Malacostraca" family="Akanthophoreidae" genus="Paraleptognathia" kingdom="Animalia" order="Tanaidacea" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="brachiata">
