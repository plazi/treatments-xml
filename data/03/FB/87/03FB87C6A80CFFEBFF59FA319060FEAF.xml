<materialsCitation ID-GBIF-Occurrence="3023453308" box="[379,1297,151,178]" collectingDate="2005-09-01" collectionCode="SCAU" collectorName="Wang XM" country="China" location="Yunnan" pageId="5" pageNumber="190" specimenCount="1" specimenCount-male="1" stateProvince="Yunnan" typeStatus="holotype">
<emphasis bold="true" box="[379,693,151,177]" pageId="5" pageNumber="190">
