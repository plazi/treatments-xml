<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Insecta" family="Anthomyiidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="diophthalma">
