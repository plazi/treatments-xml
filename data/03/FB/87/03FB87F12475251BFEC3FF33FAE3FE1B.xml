<taxonomicName authority="(Kroyer, 1841)" baseAuthorityName="Kroyer" baseAuthorityYear="1841" box="[267,589,202,226]" class="Malacostraca" family="Diastylidae" genus="Diastylis" kingdom="Animalia" order="Cumacea" pageId="16" pageNumber="16" phylum="Arthropoda" rank="species" species="rathkii">
