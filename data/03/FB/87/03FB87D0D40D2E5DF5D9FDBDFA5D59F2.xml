<quantity box="[1033,1179,1120,1144]" metricMagnitude="-4" metricUnit="m" metricValue="6.95" metricValueMax="8.4" metricValueMin="5.5" pageId="11" pageNumber="662" unit="mm" value="0.695" valueMax="0.84" valueMin="0.55">0.55-0.84 mm</quantity>
, L = 
