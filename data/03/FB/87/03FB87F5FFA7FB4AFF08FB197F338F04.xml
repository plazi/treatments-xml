<subSubSection pageId="3" pageNumber="508" type="diagnosis">
<paragraph blockId="3.[151,1437,1369,2008]" pageId="3" pageNumber="508">
<emphasis bold="true" box="[151,266,1369,1395]" pageId="3" pageNumber="508">Diagnosis</emphasis>
