<accessionNumber box="[966,1052,915,939]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/J62361" pageId="26" pageNumber="26">J62361</accessionNumber>
, 
<quantity box="[1068,1162,916,940]" metricMagnitude="-3" metricUnit="m" metricValue="9.7" pageId="26" pageNumber="26" unit="mm" value="9.7">9.7 mm</quantity>
; 
