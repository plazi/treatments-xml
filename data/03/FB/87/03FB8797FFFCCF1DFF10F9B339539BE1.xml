<quantity box="[874,1052,404,428]" metricMagnitude="-3" metricUnit="m" metricValue="4.85" metricValueMax="6.2" metricValueMin="3.5" pageId="27" pageNumber="28" unit="mm" value="4.85" valueMax="6.2" valueMin="3.5">3.5 to 6.2 mm</quantity>
