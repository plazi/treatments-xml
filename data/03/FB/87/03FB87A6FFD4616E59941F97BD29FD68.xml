</taxonomicName>
and 
<taxonomicName authority="(Yasunaga 2010)" baseAuthorityName="Yasunaga" baseAuthorityYear="2010" box="[274,615,642,666]" class="Insecta" family="Miridae" genus="Moissonia" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="52" phylum="Arthropoda" rank="species" species="sakaerat">
