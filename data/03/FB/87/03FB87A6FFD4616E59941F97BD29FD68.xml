<taxonomicName authorityName="Yasunaga" authorityYear="2010" class="Insecta" family="Miridae" genus="Moissonia" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="52" phylum="Arthropoda" rank="species" species="takaii">
