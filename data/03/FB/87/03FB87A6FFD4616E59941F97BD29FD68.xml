<taxonomicName authority="(Duwal et al. 2010 b)" baseAuthorityName="Duwal" baseAuthorityYear="2010" box="[944,1392,608,633]" class="Insecta" family="Miridae" genus="Moissonia" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="52" phylum="Arthropoda" rank="species" species="schuhi">
