</taxonomicName>
. 
<taxonomicName authority="Meigen, 1803" authorityName="Meigen" authorityYear="1803" box="[582,938,764,789]" class="Insecta" family="Keroplatidae" genus="Platyura" kingdom="Animalia" order="Diptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="fasciata">
