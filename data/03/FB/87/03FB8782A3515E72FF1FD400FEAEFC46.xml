<paragraph blockId="2.[151,1437,1527,1890]" pageId="2" pageNumber="3">
<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[151,516,1527,1554]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="hesperidum">
