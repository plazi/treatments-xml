<subSubSection pageId="4" pageNumber="509" type="diagnosis">
<paragraph blockId="4.[151,1437,1338,2019]" pageId="4" pageNumber="509">
<emphasis bold="true" box="[151,266,1338,1364]" pageId="4" pageNumber="509">Diagnosis</emphasis>
