<paragraph blockId="17.[811,1457,662,848]" pageId="17" pageNumber="668">
<taxonomicName authorityName="Chen" authorityYear="1958" box="[813,1026,693,720]" class="Ostracoda" family="Bairdiidae" genus="Bairdia" kingdom="Animalia" order="Podocopida" pageId="17" pageNumber="668" phylum="Arthropoda" rank="species" species="piscariformis">
