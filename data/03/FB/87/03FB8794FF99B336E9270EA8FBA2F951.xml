<emphasis box="[415,603,1595,1618]" italics="true" pageId="39" pageNumber="40">Paraleptoganthia</emphasis>
<taxonomicName authority="ZMUC CRU" authorityName="ZMUC CRU" box="[611,889,1595,1618]" class="Malacostraca" family="Akanthophoreidae" genus="Paraleptognathia" kingdom="Animalia" order="Tanaidacea" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="multiserrata">
