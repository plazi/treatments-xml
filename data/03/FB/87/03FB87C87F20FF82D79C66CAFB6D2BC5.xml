<taxonomicName authority="Mateos, 2011" authorityName="Mateos" authorityYear="2011" box="[774,1062,1320,1345]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="10" pageNumber="398" phylum="Arthropoda" rank="species" species="barbulus">
