<taxonomicName authority="Handschin, 1924" authorityName="Handschin" authorityYear="1924" box="[1075,1402,1320,1345]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="10" pageNumber="398" phylum="Arthropoda" rank="species" species="instratus">
