<taxonomicName baseAuthorityName="Uhagon" baseAuthorityYear="1898" box="[189,466,333,359]" class="Insecta" family="Leiodidae" genus="Speonemadus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="escalerai">
