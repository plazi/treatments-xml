<taxonomicName authority="Say, 1831" authorityName="Bergroth" authorityYear="1907" class="Insecta" family="Rhyparochromidae" genus="Pamera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="294" phylum="Arthropoda" rank="species" species="noctuabunda">
