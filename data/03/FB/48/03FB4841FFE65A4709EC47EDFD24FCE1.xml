<taxonomicName baseAuthorityName="Linnavuori" baseAuthorityYear="1978" box="[875,975,765,791]" class="Insecta" family="Rhyparochromidae" genus="Stalaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="294" phylum="Arthropoda" rank="species" species="nysias">
