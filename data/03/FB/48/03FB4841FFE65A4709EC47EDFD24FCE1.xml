<collectingCountry box="[696,797,729,755]" name="Tanzania" pageId="1" pageNumber="294">Tanzania</collectingCountry>
and 
<collectingCountry box="[855,1000,729,755]" name="South Africa" pageId="1" pageNumber="294">South Africa</collectingCountry>
(
<bibRefCitation author="Slater, J. A. &amp; Zheng, L. Y." box="[1016,1289,728,755]" pageId="1" pageNumber="294" pagination="341 - 349" refId="ref2297" refString="Slater, J. A. &amp; Zheng, L. Y. (1985) A revision of the genus Stalaria Harrington (Hemiptera: Lygaeidae). Proceedings of the Entomological Society of Washington, 87 (2), 341 - 349." type="journal article" year="1985">Slater and Zheng, 1985</bibRefCitation>
