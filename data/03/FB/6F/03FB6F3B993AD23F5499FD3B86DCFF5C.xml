<materialsCitation ID-GBIF-Occurrence="3411866303" collectionCode="NHMUK" collectorName="B. M. Genitalia &amp; Meyrick" determinerName="E. Meyrick" pageId="1" pageNumber="168" specimenCount="1" specimenCount-male="1" typeStatus="holotype">
