<subSubSection box="[199,1256,1226,1252]" pageId="32" pageNumber="199" type="etymology">
<paragraph blockId="32.[151,1437,255,1252]" box="[199,1256,1226,1252]" pageId="32" pageNumber="199">
<emphasis bold="true" box="[199,334,1226,1252]" pageId="32" pageNumber="199">Etymology.</emphasis>
