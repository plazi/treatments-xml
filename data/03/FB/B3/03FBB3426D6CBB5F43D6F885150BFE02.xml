<collectingCountry box="[364,519,439,464]" name="United Kingdom" pageId="8" pageNumber="329">Great Britain</collectingCountry>
, 
<collectingCountry box="[534,616,439,464]" name="Ireland" pageId="8" pageNumber="329">Ireland</collectingCountry>
, 
<collectingCountry box="[633,714,439,464]" name="Poland" pageId="8" pageNumber="329">Poland</collectingCountry>
; 
<collectingCountry box="[730,831,440,463]" name="Morocco" pageId="8" pageNumber="329">Morocco</collectingCountry>
(
<bibRefCitation author="Oosterbroek" box="[850,1064,439,464]" pageId="8" pageNumber="329" refString="Oosterbroek, P. (2014) Catalogue of the Craneflies of the World (Insecta, Diptera, Nematocera, Tipuloidea). Version 8 Sept. 2014. Available from: http: // ip 30. eti. uva. nl / ccw / (accessed 1 September 2014)" type="journal article" year="2014">
Oosterbroek 
<number box="[1003,1064,440,464]" pageId="8" pageNumber="329" value="2014.0">2014</number>
</bibRefCitation>
