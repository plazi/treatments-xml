<collectingCountry box="[1211,1284,229,255]" name="Egypt" pageId="18" pageNumber="19">Egypt</collectingCountry>
, south 
<collectingCountry box="[1371,1436,229,255]" name="Israel" pageId="18" pageNumber="19">Israel</collectingCountry>
and 
<collectingCountry box="[200,357,269,295]" name="Saudi Arabia" pageId="18" pageNumber="19">Saudi Arabia</collectingCountry>
. New for 
<collectingCountry box="[473,548,269,295]" name="Oman" pageId="18" pageNumber="19">Oman</collectingCountry>
