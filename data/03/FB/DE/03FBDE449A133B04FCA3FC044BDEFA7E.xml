<quantity box="[295,448,1449,1473]" metricMagnitude="3" metricUnit="m" metricValue="3.735" metricValueMax="4.08" metricValueMin="3.3899999999999997" pageId="12" pageNumber="99" unit="m" value="3735.0" valueMax="4080.0" valueMin="3390.0">3390–4080 m</quantity>
