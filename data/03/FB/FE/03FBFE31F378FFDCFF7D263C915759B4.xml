<taxonomicName authority="Meyrick" authorityName="Meyrick" authorityYear="1937" box="[254,528,1694,1719]" class="Insecta" family="Lecithoceridae" genus="Homaloxestis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="562" phylum="Arthropoda" rank="species" species="pancrocopa">
