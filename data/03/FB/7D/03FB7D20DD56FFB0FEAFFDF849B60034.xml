<number box="[732,779,1119,1145]" pageId="34" pageNumber="35" value="255.0">255</number>
; Cx­ 
<number box="[837,850,1119,1145]" pageId="34" pageNumber="35" value="3.0">3</number>
width 
<number box="[929,977,1119,1145]" pageId="34" pageNumber="35" value="278.0">278</number>
