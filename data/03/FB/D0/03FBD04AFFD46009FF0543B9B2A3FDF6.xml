<paragraph blockId="16.[151,913,1483,1510]" box="[151,913,1483,1510]" pageId="16" pageNumber="17">
<heading bold="true" box="[151,913,1483,1510]" fontSize="11" level="1" pageId="16" pageNumber="17" reason="1">
<emphasis bold="true" box="[151,913,1483,1510]" pageId="16" pageNumber="17">
Key to the male 
<taxonomicName box="[354,538,1484,1510]" class="Insecta" family="Lucanidae" genus="Prismognathus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[354,538,1484,1510]" italics="true" pageId="16" pageNumber="17">Prismognathus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[600,760,1484,1510]" class="Insecta" family="Lucanidae" genus="Cladophyllus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
