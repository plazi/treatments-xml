; 
<emphasis bold="true" box="[893,923,1572,1594]" pageId="7" pageNumber="8">(4)</emphasis>
<taxonomicName baseAuthorityName="Kraatz" baseAuthorityYear="1870" box="[929,1084,1572,1594]" class="Insecta" family="Leiodidae" genus="Speonemadus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="angusticollis">
