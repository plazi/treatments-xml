<paragraph blockId="9.[151,473,602,628]" box="[151,473,602,628]" pageId="9" pageNumber="10">
<heading bold="true" box="[151,473,602,628]" fontSize="11" level="1" pageId="9" pageNumber="10" reason="1">
<emphasis bold="true" box="[151,473,602,628]" pageId="9" pageNumber="10">
<taxonomicName box="[151,288,602,628]" class="Malacostraca" family="Homolidae" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="10" phylum="Arthropoda" rank="family">Homolidae</taxonomicName>
