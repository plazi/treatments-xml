<taxonomicName authorityName="Seigler &amp; Ebinger &amp; Riggins &amp; Terra &amp; Miller" authorityYear="2017" box="[415,577,310,332]" class="Magnoliopsida" family="Fabaceae" genus="Pseudosenegalia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="20" pageNumber="200" phylum="Tracheophyta" rank="genus">
