<subSubSection pageId="37" pageNumber="488" type="etymology">
<paragraph blockId="37.[151,1437,835,1113]" box="[151,1236,835,861]" pageId="37" pageNumber="488">
<emphasis bold="true" box="[151,290,835,861]" pageId="37" pageNumber="488">Etymology:</emphasis>
<taxonomicName authorityName="Cuvier" authorityYear="1828" box="[297,403,835,861]" class="Actinopterygii" family="Ambassidae" genus="Ambassis" kingdom="Animalia" order="Perciformes" pageId="37" pageNumber="488" phylum="Chordata" rank="genus">
