<taxonomicName baseAuthorityName="Boulenger" baseAuthorityYear="1888" box="[294,520,1143,1166]" class="Actinopterygii" family="Cyprinidae" genus="Cyprinion" kingdom="Animalia" order="Cypriniformes" pageId="12" pageNumber="463" phylum="Chordata" rank="species" species="muscatense">
