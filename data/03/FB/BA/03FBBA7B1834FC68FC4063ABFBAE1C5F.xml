Table 
<number box="[1056,1070,1044,1070]" pageId="3" pageNumber="4" value="1.0">1</number>
</tableCitation>
and 
<tableCitation box="[1129,1220,1044,1070]" captionStart="TABLE 2" captionStartId="6.[264,361,283,309]" captionTargetBox="[264,1305,385,591]" captionTargetPageId="6" captionText="TABLE 2. Comparative characters for separating species of Pomerantziidae." httpUri="http://table.plazi.org/id/DF2D5BE51833FC6DFC406172FB30193F" pageId="3" pageNumber="4" tableUuid="DF2D5BE51833FC6DFC406172FB30193F">
Table 
<number box="[1202,1220,1044,1070]" pageId="3" pageNumber="4" value="2.0">2</number>
</tableCitation>
