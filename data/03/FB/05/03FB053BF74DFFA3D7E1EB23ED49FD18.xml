<paragraph blockId="4.[151,551,1584,1645]" box="[151,417,1619,1645]" pageId="4" pageNumber="57">
(
<figureCitation box="[159,272,1619,1645]" captionStart="FIGURES 6 – 9" captionStartId="5.[151,257,1718,1739]" captionTargetBox="[180,1330,764,1647]" captionTargetId="figure@5.[151,1436,764,1688]" captionTargetPageId="5" captionText="FIGURES 6 – 9. Aboilomimus guizhouensis sp. n. 6. Tegmina and wing of male; 7. End of male abdomen, dorsal view; 8. End of male abdomen, lateral view; 9. End of male abdomen, ventral view." httpUri="https://zenodo.org/record/186175/files/figure.png" pageId="4" pageNumber="57">
Figs. 
<number box="[226,272,1619,1645]" pageId="4" pageNumber="57" value="7.5" valueMax="9.0" valueMin="6.0">6–9</number>
</figureCitation>
; Plate 
<number box="[351,366,1619,1645]" pageId="4" pageNumber="57" value="1.0">1</number>
–B)
