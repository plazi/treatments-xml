, 
<location LSID="urn:lsid:plazi:treatment:03FBB532FFB3173BFF6A0F683BDEFECB:8E8D52FFFFB31735FCAD0FB13997FB0B" box="[848,957,1254,1279]" country="Mozambique" latitude="-19.116667" longLatPrecision="1273" longitude="33.45" name="Vila Pery" pageId="54" pageNumber="55" stateProvince="Manica">Vila Pery</location>
[=Chimoio, 
<geoCoordinate box="[1110,1204,1254,1278]" degrees="19" direction="south" minutes="07" orientation="latitude" pageId="54" pageNumber="55" precision="925" value="-19.116667">19°07’S</geoCoordinate>
