<taxonomicName authorityName="Assing" authorityYear="2015" baseAuthorityName="Assing" baseAuthorityYear="1874" box="[654,865,855,876]" class="Insecta" family="Staphylinidae" genus="Coimedon" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="204" phylum="Arthropoda" rank="species" species="dissimilis">
