<taxonomicName authority="(Hagen)" baseAuthorityName="Hagen" baseAuthorityYear="1861" box="[873,1166,970,995]" class="Insecta" family="Perlidae" genus="Attaneuria" kingdom="Animalia" order="Plecoptera" pageId="8" pageNumber="177" phylum="Arthropoda" rank="species" species="ruralis">
