<collectingCounty box="[502,690,260,284]" pageId="10" pageNumber="179">Carbon County</collectingCounty>
and the 
<collectingMunicipality box="[189,415,295,319]" pageId="10" pageNumber="179">Susquehanna River</collectingMunicipality>
basin in 
<collectorName box="[516,665,295,319]" pageId="10" pageNumber="179">Huntingdon</collectorName>
, 
<collectorName box="[674,793,295,319]" pageId="10" pageNumber="179">Lycoming</collectorName>
, and 
<location LSID="urn:lsid:plazi:treatment:03EA60784530FC1EFC94732CFE5CFD1A:8E9C87B54533FC1EFF097522FE1FFEBF" box="[244,441,331,355]" county="Carbon County" municipality="Susquehanna River" name="Union Counties." pageId="10" pageNumber="179" stateProvince="Maine and Quebec">Union Counties.</location>
<taxonomicName box="[451,675,331,355]" class="Insecta" family="Perlodidae" genus="Helopicus" kingdom="Animalia" order="Plecoptera" pageId="10" pageNumber="179" phylum="Arthropoda" rank="species" species="subvarian">
<emphasis box="[451,675,331,355]" italics="true" pageId="10" pageNumber="179">Helopicus subvarian</emphasis>
</taxonomicName>
