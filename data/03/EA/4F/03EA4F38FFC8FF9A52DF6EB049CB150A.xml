<emphasis box="[491,644,1329,1353]" italics="true" pageId="5" pageNumber="221">Calvolia lordi</emphasis>
(Nesbitt)
</taxonomicName>
, 
<taxonomicName authority="(Nalepa)" baseAuthorityName="Nalepa" baseAuthorityYear="1890" box="[751,1083,1329,1353]" class="Arachnida" family="Eriophyidae" genus="Aculus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="5" pageNumber="221" phylum="Arthropoda" rank="species" species="schlechtendali">
<emphasis box="[751,988,1329,1353]" italics="true" pageId="5" pageNumber="221">Aculus schlechtendali</emphasis>
(Nalepa)
</taxonomicName>
, adults of 
<taxonomicName authority="(Comstock)" baseAuthorityName="Comstock" class="Insecta" family="Diaspididae" genus="Quadraspidiotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="221" phylum="Arthropoda" rank="species" species="perniciosus">
