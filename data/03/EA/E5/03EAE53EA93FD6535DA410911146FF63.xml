<quantity box="[1317,1390,1747,1771]" metricMagnitude="2" metricUnit="m" metricValue="8.129999999999999" pageId="3" pageNumber="527" unit="m" value="813.0">813 m</quantity>
asl, N 
<number box="[180,210,1782,1807]" pageId="3" pageNumber="527" value="35.0">35</number>
째 
<number box="[228,322,1782,1807]" pageId="3" pageNumber="527" value="37.5813">37.5813</number>
, W 
<number box="[369,399,1783,1807]" pageId="3" pageNumber="527" value="83.0">83</number>
째 
<number box="[417,496,1782,1807]" pageId="3" pageNumber="527" value="5.361">5.3610</number>
) and four specimens in sediment from Cataloochee Creek, North Carolina (
<quantity box="[1362,1436,1783,1807]" metricMagnitude="2" metricUnit="m" metricValue="7.88" pageId="3" pageNumber="527" unit="m" value="788.0">788 m</quantity>
asl, N 
