<heading bold="true" box="[151,553,1051,1078]" fontSize="11" level="1" pageId="13" pageNumber="314" reason="1">
<emphasis bold="true" box="[151,553,1051,1078]" pageId="13" pageNumber="314">
Genus 
<taxonomicName authority="LeConte, 1876" authorityName="LeConte" authorityYear="1876" box="[237,553,1051,1078]" class="Insecta" family="Curculionidae" genus="Hylurgops" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="314" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[237,364,1051,1077]" italics="true" pageId="13" pageNumber="314">Hylurgops</emphasis>
<bibRefCitation author="LeConte" box="[372,553,1052,1078]" pageId="13" pageNumber="314" refString="LeConte, J. L. (1876) Family IX, Scolytidae. In: LeConte, J. L. &amp; Horn, G. H. (Eds.), The Rhynchophora of America North of Mexico. Proceedings of the American Philosophical Society, 15 (96), pp. 341 - 391." type="journal article" year="1876">
LeConte, 
<number box="[494,553,1052,1078]" pageId="13" pageNumber="314" value="1876.0">1876</number>
</bibRefCitation>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
