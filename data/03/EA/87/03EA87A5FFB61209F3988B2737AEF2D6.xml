<taxonomicName authority="Daudin" authorityName="Daudin" class="Amphibia" family="Phyllomedusidae" genus="Pithecopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="hypochondrialis">
<emphasis box="[1254,1436,1927,1952]" italics="true" pageId="31" pageNumber="32">hypochondrialis</emphasis>
