<taxonomicName authority="Miranda-Ribeiro" authorityName="Miranda-Ribeiro" box="[267,645,1964,1989]" class="Amphibia" family="Phyllomedusidae" genus="Pithecopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="megacephalus">
