<taxonomicName authority="Lutz" authorityName="Lutz" box="[482,763,1927,1952]" class="Amphibia" family="Phyllomedusidae" genus="Pithecopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="ayeaye">
<emphasis box="[482,697,1927,1952]" italics="true" pageId="31" pageNumber="32">Pithecopus ayeaye</emphasis>
Lutz
</taxonomicName>
, 
<taxonomicName authority="Cope" authorityName="Cope" box="[780,958,1928,1952]" class="Amphibia" family="Phyllomedusidae" genus="Pithecopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="azureus">
<emphasis box="[780,866,1929,1952]" italics="true" pageId="31" pageNumber="32">azureus</emphasis>
(Cope)
</taxonomicName>
