<taxonomicName authority="Dumeril" authorityName="Dumeril" box="[807,1030,500,525]" class="Amphibia" family="Hylidae" genus="Agalychnis" kingdom="Animalia" order="Anura" pageId="35" pageNumber="36" phylum="Chordata" rank="species" species="moreletii">
