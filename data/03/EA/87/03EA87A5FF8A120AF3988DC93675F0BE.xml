<taxonomicName authority="Duellman &amp; Mendelson" authorityName="Duellman &amp; Mendelson" class="Amphibia" family="Hylidae" genus="Agalychnis" kingdom="Animalia" order="Anura" pageId="35" pageNumber="36" phylum="Chordata" rank="species" species="hulli">
