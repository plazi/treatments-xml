<number box="[929,976,1047,1073]" pageId="4" pageNumber="5" value="7.5" valueMax="9.0" valueMin="6.0">6–9</number>
; II, 
<number box="[1026,1102,1047,1073]" pageId="4" pageNumber="5" value="13.5" valueMax="17.0" valueMin="10.0">10–17</number>
; III, 
<number box="[1162,1238,1047,1073]" pageId="4" pageNumber="5" value="15.5" valueMax="19.0" valueMin="12.0">12–19</number>
