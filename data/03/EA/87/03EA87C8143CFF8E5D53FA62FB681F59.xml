<emphasis box="[624,916,1084,1109]" italics="true" pageId="6" pageNumber="7">Branchinecta campestris</emphasis>
Lynch, 
<number box="[1011,1069,1084,1110]" pageId="6" pageNumber="7" value="1960.0">1960</number>
(
<bibRefCitation author="Sublette" pageId="6" pageNumber="14" refString="Sublette, J. E. &amp; Sublette, M. S. (1967) The limnology of playalakes on the Llano Estacado, New Mexico and Texas. The Southwestern Naturalist, 12, 364 - 406." type="journal article" year="1967">
Sublette &amp; Sublette, 
<number box="[264,326,1124,1150]" pageId="6" pageNumber="7" value="1967.0">1967</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="Kellogg, 1906" authorityName="Kellogg" authorityYear="1906" box="[352,783,1124,1150]" class="Branchiopoda" family="Artemiidae" genus="Artemia" kingdom="Animalia" order="Anostraca" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="franciscana">
