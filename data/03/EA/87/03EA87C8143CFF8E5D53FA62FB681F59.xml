</taxonomicName>
and 
<taxonomicName authority="Packard, 1871 (Horne, 1974)" authorityName="Packard, 1871 (Horne" authorityYear="1974" class="Branchiopoda" family="Streptocephalidae" genus="Streptocephalus" kingdom="Animalia" order="Anostraca" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="texanus">
