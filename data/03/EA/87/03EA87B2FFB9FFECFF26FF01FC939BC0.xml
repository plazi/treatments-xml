</taxonomicName>
and 
<taxonomicName authority="Wray" authorityName="Wray" box="[1224,1436,332,357]" class="Entognatha" family="Hypogastruridae" genus="Mitchellania" kingdom="Animalia" order="Collembola" pageId="7" pageNumber="308" phylum="Arthropoda" rank="genus">
<emphasis box="[1224,1367,332,357]" italics="true" pageId="7" pageNumber="308">Mitchellania</emphasis>
