<typeStatus box="[151,201,1697,1721]" pageId="6" pageNumber="379">Type</typeStatus>
species: 
<taxonomicName authority="Brain 1920" authorityName="Brain, Proteacoccu" authorityYear="1920" box="[294,597,1696,1721]" class="Insecta" family="Coccidae" genus="Lecanium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="379" phylum="Arthropoda" rank="species" species="proteae">
