), 
<collectingCountry box="[310,420,725,748]" name="Venezuela" pageId="38" pageNumber="353">Venezuela</collectingCountry>
(
<bibRefCitation author="Heppner J. B." bookContentInfo="670 pp." box="[439,609,726,750]" journalOrPublisher="Arthropods of Florida and neighbouring land areas 17" pageId="38" pageNumber="353" refString="Heppner J. B. 2003. Lepidoptera of Florida. Part 1. Introduction and catalog. Arthropods of Florida and neighbouring land areas 17: 670 pp." title="Lepidoptera of Florida. Part 1. Introduction and catalog" type="book" year="2003">Heppner, 2003</bibRefCitation>
). Also found in 
<collectingCountry box="[183,274,758,781]" name="Grenada" pageId="38" pageNumber="353">Grenada</collectingCountry>
(Caribbean), 
<collectingCountry box="[431,532,759,783]" name="Paraguay" pageId="38" pageNumber="353">
<collectingRegion box="[431,532,759,783]" country="Paraguay" name="Asuncion" pageId="38" pageNumber="353">Paraguay</collectingRegion>
</collectingCountry>
, and 
<collectingCountry box="[597,646,759,782]" name="Peru" pageId="38" pageNumber="353">Peru</collectingCountry>
