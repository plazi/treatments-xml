<emphasis box="[528,864,151,176]" italics="true" pageId="31" pageNumber="32">
<taxonomicName authority="Boulenger" authorityName="Boulenger" box="[528,853,151,176]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="albopunctulatus">Hyloscirtus albopunctulatus</taxonomicName>
*
</emphasis>
(Boulenger), 
<taxonomicName authority="Duellman" authorityName="Duellman" box="[1031,1286,151,176]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="alytolylax">
<emphasis box="[1031,1141,151,176]" italics="true" pageId="31" pageNumber="32">alytolylax</emphasis>
(Duellman)
</taxonomicName>
, 
<taxonomicName authority="Peters" authorityName="Peters" box="[1302,1422,151,176]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="bogotensis">
<emphasis box="[1302,1422,151,176]" italics="true" pageId="31" pageNumber="32">bogotensis</emphasis>
</taxonomicName>
* (Peters), 
<emphasis box="[253,354,188,213]" italics="true" pageId="31" pageNumber="32">callipeza</emphasis>
* (Duellman), 
<taxonomicName authority="Dunn" authorityName="Dunn" box="[520,707,188,213]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="colymba">
<emphasis box="[520,615,188,213]" italics="true" pageId="31" pageNumber="32">colymba</emphasis>
(Dunn)
</taxonomicName>
, 
<emphasis box="[719,868,188,213]" italics="true" pageId="31" pageNumber="32">denticulentus</emphasis>
* (Duellman), 
<emphasis box="[1035,1122,189,212]" italics="true" pageId="31" pageNumber="32">estevesi</emphasis>
* (Rivero), 
<emphasis box="[1254,1326,188,213]" italics="true" pageId="31" pageNumber="32">jahni*</emphasis>
(Rivero), 
<taxonomicName authority="Rivero" authorityName="Rivero" box="[151,361,223,248]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="lascinius">
