<paragraph blockId="29.[151,817,1086,1108]" box="[151,817,1086,1108]" pageId="29" pageNumber="30">
<emphasis box="[151,306,1086,1108]" italics="true" pageId="29" pageNumber="30">
<taxonomicName box="[151,302,1086,1108]" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="genus" status="new genus">Colomascirtus</taxonomicName>
.
</emphasis>
<typeStatus box="[311,361,1087,1108]" pageId="29" pageNumber="30">Type</typeStatus>
species: 
<taxonomicName authority="Duellman, 1973" authorityName="Duellman" authorityYear="1973" box="[454,813,1086,1108]" class="Amphibia" family="Hylidae" genus="Hyla" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="larinopygion">
