, 
<taxonomicName authority="Kizirian, Coloma &amp; Paredes-Recalde" authorityName="Kizirian, Coloma &amp; Paredes-Recalde" box="[581,1170,1513,1538]" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="tapichalaca">
