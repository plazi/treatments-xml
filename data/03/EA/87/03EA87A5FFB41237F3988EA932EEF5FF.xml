<taxonomicName authority="Duellman" authorityName="Duellman" class="Amphibia" family="Hylidae" genus="Hyla" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="larinopygion">
<emphasis box="[1291,1436,1369,1394]" italics="true" pageId="29" pageNumber="30">larinopygion</emphasis>
(Duellman)
</taxonomicName>
, 
<taxonomicName authority="Duellman &amp; Altig" authorityName="Duellman &amp; Altig" box="[304,636,1405,1430]" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="lindae">
<emphasis box="[304,374,1405,1430]" italics="true" pageId="29" pageNumber="30">lindae</emphasis>
(Duellman &amp; Altig)
</taxonomicName>
, 
<taxonomicName authority="Duellman &amp; Hillis" authorityName="Duellman &amp; Hillis" box="[658,994,1405,1430]" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="pacha">
<emphasis box="[658,727,1405,1430]" italics="true" pageId="29" pageNumber="30">pacha</emphasis>
(Duellman &amp; Hillis)
</taxonomicName>
, 
<taxonomicName authority="Duellman &amp; Berger" authorityName="Duellman &amp; Berger" box="[1015,1432,1405,1430]" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="pantostictus">
<emphasis box="[1015,1150,1406,1429]" italics="true" pageId="29" pageNumber="30">pantostictus</emphasis>
(Duellman &amp; Berger)
</taxonomicName>
, 
<taxonomicName authority="Coloma, Carvajal-Endara, Duenas, Paredes-Recalde, Morales-Mite, Almeida-Reinoso, Tapia, Hutter, Toral-Contreras &amp; Guayasamin" authorityName="Coloma, Carvajal-Endara, Duenas, Paredes-Recalde, Morales-Mite, Almeida-Reinoso, Tapia, Hutter, Toral-Contreras &amp; Guayasamin" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="princecharlesi">
<emphasis box="[151,313,1441,1466]" italics="true" pageId="29" pageNumber="30">princecharlesi</emphasis>
