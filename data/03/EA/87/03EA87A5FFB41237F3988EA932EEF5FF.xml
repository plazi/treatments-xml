</taxonomicName>
, 
<taxonomicName authority="Duellman &amp; Hillis" authorityName="Duellman &amp; Hillis" box="[622,1005,1477,1502]" class="Amphibia" family="Hylidae" genus="Colomascirtus" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="psarolaimus">
<emphasis box="[622,759,1477,1502]" italics="true" pageId="29" pageNumber="30">psarolaimus</emphasis>
