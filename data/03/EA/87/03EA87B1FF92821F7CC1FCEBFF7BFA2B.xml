<taxonomicName authority="Guenee" authorityName="Guenee" class="Insecta" family="Pyralidae" genus="Salbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="46" pageNumber="361" phylum="Arthropoda" rank="species" species="haemorrhoidalis" subFamily="Spilomelinae">
