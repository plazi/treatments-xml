</taxonomicName>
with 
<taxonomicName box="[707,878,764,789]" class="Entognatha" family="Hypogastruridae" genus="Ceratophysella" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="307" phylum="Arthropoda" rank="genus">
<emphasis box="[707,878,764,789]" italics="true" pageId="6" pageNumber="307">Ceratophysella</emphasis>
</taxonomicName>
