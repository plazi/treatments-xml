), Tasman Sea (
<quantity box="[797,886,1869,1893]" metricMagnitude="1" metricUnit="m" metricValue="4.45" metricValueMax="8.8" metricValueMin="0.1" pageId="20" pageNumber="421" unit="m" value="44.5" valueMax="88.0" valueMin="1.0">1–88 m</quantity>
