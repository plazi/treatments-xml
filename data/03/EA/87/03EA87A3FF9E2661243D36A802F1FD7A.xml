<number pageId="3" pageNumber="4" value="30.5" valueMax="34.0" valueMin="27.0">27– 34</number>
; III, 
<number box="[352,428,1500,1526]" pageId="3" pageNumber="4" value="35.5" valueMax="38.0" valueMin="33.0">33–38</number>
; IV–V, 
<number box="[518,594,1500,1526]" pageId="3" pageNumber="4" value="48.0" valueMax="54.0" valueMin="42.0">42–54</number>
; VI, 
<number box="[651,728,1500,1526]" pageId="3" pageNumber="4" value="44.0" valueMax="47.0" valueMin="41.0">41–47</number>
; VII, 
<number box="[795,872,1500,1526]" pageId="3" pageNumber="4" value="22.5" valueMax="25.0" valueMin="20.0">20–25</number>
; VIII, 
<number box="[948,1010,1500,1526]" pageId="3" pageNumber="4" value="10.5" valueMax="12.0" valueMin="9.0">9–12</number>
