<number box="[677,799,1300,1326]" pageId="3" pageNumber="4" value="0.7250000000000001" valueMax="0.77" valueMin="0.68">0.68–0.77</number>
; 
<collectionCode box="[817,894,1300,1326]" pageId="3" pageNumber="4">AWIV</collectionCode>
, 
<number box="[911,1032,1300,1326]" pageId="3" pageNumber="4" value="0.875" valueMax="0.93" valueMin="0.82">0.82–0.93</number>
; ANW, 
<number box="[1139,1260,1300,1326]" pageId="3" pageNumber="4" value="0.375" valueMax="0.4" valueMin="0.35">0.35–0.40</number>
