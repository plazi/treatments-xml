<number box="[1138,1214,1420,1446]" pageId="3" pageNumber="4" value="22.0" valueMax="25.0" valueMin="19.0">19–25</number>
; VI–VII, 
<number box="[264,341,1460,1486]" pageId="3" pageNumber="4" value="19.5" valueMax="23.0" valueMin="16.0">16–23</number>
