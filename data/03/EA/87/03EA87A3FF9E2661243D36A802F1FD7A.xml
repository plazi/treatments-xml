<number box="[1146,1267,1260,1286]" pageId="3" pageNumber="4" value="0.625" valueMax="0.66" valueMin="0.59">0.59–0.66</number>
; HL, 
<number box="[264,384,1300,1326]" pageId="3" pageNumber="4" value="0.39" valueMax="0.41" valueMin="0.37">0.37–0.41</number>
; PW, 
<number box="[466,586,1300,1326]" pageId="3" pageNumber="4" value="0.4" valueMax="0.43" valueMin="0.37">0.37–0.43</number>
