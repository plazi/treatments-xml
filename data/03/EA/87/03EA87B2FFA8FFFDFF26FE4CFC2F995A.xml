<collectingCountry box="[339,450,546,571]" name="Thailand" pageId="22" pageNumber="323">Thailand</collectingCountry>
.
</emphasis>
<collectingRegion box="[465,600,546,571]" country="Thailand" name="Chiang Mai" pageId="22" pageNumber="323">Chiang Mai</collectingRegion>
