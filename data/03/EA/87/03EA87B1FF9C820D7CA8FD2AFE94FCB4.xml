<collectingRegion box="[466,597,385,408]" country="United States of America" name="Washington" pageId="34" pageNumber="349">Washington</collectingRegion>
, and south of the 
<collectingCountry box="[197,249,418,442]" name="United States of America" pageId="34" pageNumber="349">USA</collectingCountry>
in 
<collectingCountry box="[295,405,417,440]" name="Argentina" pageId="34" pageNumber="349">Argentina</collectingCountry>
, 
<collectingCountry box="[422,523,417,440]" name="Bahamas" pageId="34" pageNumber="349">Bahamas</collectingCountry>
, 
<collectingCountry box="[540,609,417,440]" name="Brazil" pageId="34" pageNumber="349">Brazil</collectingCountry>
, 
<collectingCountry box="[626,752,417,442]" name="Costa Rica" pageId="34" pageNumber="349">Costa Rica</collectingCountry>
, 
<collectingCountry box="[151,236,449,472]" name="Mexico" pageId="34" pageNumber="349">
<collectingRegion box="[151,236,449,472]" country="Mexico" name="Mexico" pageId="34" pageNumber="349">Mexico</collectingRegion>
</collectingCountry>
