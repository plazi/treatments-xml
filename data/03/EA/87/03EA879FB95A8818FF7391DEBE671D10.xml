and 
<taxonomicName baseAuthorityName="Murray" baseAuthorityYear="1906" box="[994,1163,655,680]" class="Heterotardigrada" family="Echiniscidae" genus="Testechiniscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Echiniscoidea" pageId="5" pageNumber="266" phylum="Tardigrada" rank="species" species="meridionalis">
