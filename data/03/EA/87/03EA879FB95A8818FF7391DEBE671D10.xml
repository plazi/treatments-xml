, 
<taxonomicName authority="Gąsiorek &amp; Stec &amp; Zawierucha &amp; Kristensen &amp; Michalczyk, 2018" authorityName="Gąsiorek &amp; Stec &amp; Zawierucha &amp; Kristensen &amp; Michalczyk" authorityYear="2018" box="[1076,1381,620,645]" class="Heterotardigrada" family="Echiniscidae" genus="Testechiniscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Echiniscoidea" pageId="5" pageNumber="266" phylum="Tardigrada" rank="subSpecies" species="spitsbergensis" status="ssp. nov." subSpecies="tropicalis">
