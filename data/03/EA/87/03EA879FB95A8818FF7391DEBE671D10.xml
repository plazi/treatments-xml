<emphasis box="[371,685,620,645]" italics="true" pageId="5" pageNumber="266">
<taxonomicName box="[371,603,620,645]" class="Heterotardigrada" family="Echiniscidae" genus="Testechiniscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Echiniscoidea" pageId="5" pageNumber="266" phylum="Tardigrada" rank="subSpecies" subSpecies="sensu">
