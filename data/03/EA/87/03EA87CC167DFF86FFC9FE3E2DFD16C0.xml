<taxonomicName authority="S. I. Smith, 1873" authorityName="S. I. Smith" authorityYear="1873" box="[275,693,763,787]" class="Malacostraca" family="Mysidae" genus="Heteromysis" kingdom="Animalia" order="Mysida" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="formosa">
