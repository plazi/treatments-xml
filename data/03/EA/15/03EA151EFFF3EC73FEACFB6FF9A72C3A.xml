<taxonomicName box="[264,398,607,633]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="63" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
