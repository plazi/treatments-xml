. 
<collectingCountry box="[359,468,404,428]" name="Indonesia" pageId="9" pageNumber="76">Indonesia</collectingCountry>
(
<collectingCountry box="[483,558,404,428]" name="Papua New Guinea" pageId="9" pageNumber="76">Papua</collectingCountry>
).
