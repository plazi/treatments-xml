<subSubSection box="[199,1047,404,428]" pageId="5" pageNumber="72" type="etymology">
<paragraph blockId="5.[151,1436,151,428]" box="[199,1047,404,428]" pageId="5" pageNumber="72">
<emphasis bold="true" box="[199,334,404,428]" pageId="5" pageNumber="72">Etymology.</emphasis>
