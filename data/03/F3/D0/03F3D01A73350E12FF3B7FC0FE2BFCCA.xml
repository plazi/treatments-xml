<collectingCountry box="[806,879,772,796]" name="Oman" pageId="5" pageNumber="534">Oman</collectingCountry>
), 
<collectingCountry box="[898,996,772,797]" name="Pakistan" pageId="5" pageNumber="534">Pakistan</collectingCountry>
