, South 
<collectingCountry box="[610,677,932,957]" name="China" pageId="2" pageNumber="531">China</collectingCountry>
Sea, 
<collectingCountry box="[741,808,933,956]" name="Japan" pageId="2" pageNumber="531">Japan</collectingCountry>
, 
<collectingCountry box="[821,950,932,957]" name="Philippines" pageId="2" pageNumber="531">Philippines</collectingCountry>
, 
<collectingCountry box="[962,1057,933,956]" name="Vanuatu" pageId="2" pageNumber="531">Vanuatu</collectingCountry>
, 
<collectingCountry box="[1070,1248,932,957]" name="New Caledonia" pageId="2" pageNumber="531">New Caledonia</collectingCountry>
