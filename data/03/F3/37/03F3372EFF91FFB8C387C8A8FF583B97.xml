<collectingRegion box="[796,1002,332,357]" country="Vietnam" name="Dak Lak" pageId="7" pageNumber="352">Dak Lak Province</collectingRegion>
, 
<collectingRegion box="[1012,1095,332,357]" country="Papua New Guinea" name="Central" pageId="7" pageNumber="352">Central</collectingRegion>
<collectingRegion box="[1101,1216,332,357]" country="Papua New Guinea" name="Western Highlands" pageId="7" pageNumber="352">Highlands</collectingRegion>
of 
<collectingCountry box="[1252,1347,332,357]" name="Vietnam" pageId="7" pageNumber="352">Vietnam</collectingCountry>
(
<figureCitation captionStart="FIGURE 19" captionStartId="25.[151,250,1940,1962]" captionTargetBox="[241,1347,195,1919]" captionTargetId="figure@25.[241,1350,195,1923]" captionTargetPageId="25" captionText="FIGURE 19. Map of sampling sites of Yezoceryx spp in Vietnam: 1. Cao Bang Province; 2. Bac Kan Province; 3. Vinh Phuc Province, 4. Son La Province; 5. Hoa Binh Province; 6. Ninh Binh Province; 7. Nghe An Province; 8. Thua Thien-Hue Province; 9. Kon Tum Province; 10. Dak Lak Province; 11. Dong Nai Province." httpUri="https://zenodo.org/record/847892/files/figure.png" pageId="7" pageNumber="352">Figure 19</figureCitation>
