<emphasis box="[695,902,335,361]" italics="true" pageId="9" pageNumber="204">Falco tinnunculus</emphasis>
</taxonomicName>
<quantity box="[903,941,335,362]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="9" pageNumber="204" unit="in" value="1.0">
