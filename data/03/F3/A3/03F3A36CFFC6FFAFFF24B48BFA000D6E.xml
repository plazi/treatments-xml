<emphasis box="[1085,1104,1695,1716]" italics="true" pageId="9" pageNumber="459">j1</emphasis>
, 
<emphasis box="[1117,1138,1695,1716]" italics="true" pageId="9" pageNumber="459">z1</emphasis>
, and 
<emphasis box="[1192,1215,1695,1716]" italics="true" pageId="9" pageNumber="459">J5</emphasis>
almost equal in length and as long as 
<emphasis box="[367,386,1724,1745]" italics="true" pageId="9" pageNumber="459">j2</emphasis>
................................................. 
<taxonomicName authority="Gu, Huang &amp; Li, 1991" authorityName="Gu, Huang &amp; Li" authorityYear="1991" box="[998,1436,1724,1746]" class="Arachnida" family="Pachylaelapidae" genus="Onchodellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="9" pageNumber="459" phylum="Arthropoda" rank="species" species="torocoxus">
