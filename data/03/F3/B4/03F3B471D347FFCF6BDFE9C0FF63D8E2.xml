<paragraph blockId="11.[151,1437,152,502]" pageId="11" pageNumber="345">
<emphasis bold="true" box="[199,315,153,179]" pageId="11" pageNumber="345">Remarks.</emphasis>
<taxonomicName authority="Heffern &amp; Botero &amp; Santos-Silva, 2020" authorityName="Heffern &amp; Botero &amp; Santos-Silva" authorityYear="2020" box="[324,621,152,178]" class="Insecta" family="Cerambycidae" genus="Pentanodes" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="345" phylum="Arthropoda" rank="species" species="nicaraguensis" status="sp. nov.">
<emphasis box="[324,621,152,178]" italics="true" pageId="11" pageNumber="345">Pentanodes nicaraguensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[630,722,153,179]" pageId="11" pageNumber="345">
<taxonomicNameLabel box="[630,722,153,179]" pageId="11" pageNumber="345" rank="species">sp. nov.</taxonomicNameLabel>
