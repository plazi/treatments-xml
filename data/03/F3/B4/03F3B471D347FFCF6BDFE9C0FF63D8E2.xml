<taxonomicName authorityName="Heffern &amp; Botero &amp; Santos-Silva" authorityYear="2020" baseAuthorityName="Chemsak" baseAuthorityYear="1977" box="[1006,1172,152,178]" class="Insecta" family="Cerambycidae" genus="Pentanodes" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="345" phylum="Arthropoda" rank="species" species="xanthocollis">
