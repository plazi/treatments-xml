<paragraph blockId="7.[332,1263,393,1737]" box="[396,631,1537,1561]" pageId="7" pageNumber="28">
<emphasis box="[396,631,1537,1561]" italics="true" pageId="7" pageNumber="28">
<heading box="[396,628,1537,1561]" pageId="7" pageNumber="28">
<taxonomicName authorityName="Dallas" authorityYear="1851" baseAuthorityName="Fabricius" baseAuthorityYear="1787" box="[396,628,1537,1561]" class="Insecta" family="Pentatomidae" genus="Antiteuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="28" phylum="Arthropoda" rank="species" species="tripterus">Antiteuchus tripterus</taxonomicName>
</heading>
