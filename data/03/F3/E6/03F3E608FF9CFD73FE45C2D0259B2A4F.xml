<taxonomicName authority="Dallas, 1851" authorityName="Dallas" authorityYear="1851" baseAuthorityName="Dallas" baseAuthorityYear="1851" box="[396,561,1272,1297]" class="Insecta" family="Pentatomidae" genus="Pellaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="26" phylum="Arthropoda" rank="species" species="stictica">Pellaea stictica</taxonomicName>
