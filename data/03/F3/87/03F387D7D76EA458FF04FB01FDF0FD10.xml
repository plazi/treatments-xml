<subSubSection pageId="6" pageNumber="843" type="etymology">
<paragraph blockId="6.[173,660,194,640]" pageId="6" pageNumber="843">
<emphasis bold="true" box="[197,311,459,480]" pageId="6" pageNumber="843">Etymology.</emphasis>
<emphasis box="[329,408,458,480]" italics="true" pageId="6" pageNumber="843">Nudulus</emphasis>
