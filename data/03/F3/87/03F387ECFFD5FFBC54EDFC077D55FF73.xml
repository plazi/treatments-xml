in ♂, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.165" metricValueMax="3.53" metricValueMin="2.8" pageId="44" pageNumber="45" unit="mm" value="3.165" valueMax="3.53" valueMin="2.8">2.80–3.53 mm</quantity>
in ♀), THL = 
