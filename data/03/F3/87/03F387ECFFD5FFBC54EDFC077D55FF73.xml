in ♂, 
<quantity box="[151,314,1619,1644]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" metricValueMax="3.2" metricValueMin="2.6" pageId="44" pageNumber="45" unit="mm" value="2.9" valueMax="3.2" valueMin="2.6">2.60–3.20 mm</quantity>
in ♀), WW = 
