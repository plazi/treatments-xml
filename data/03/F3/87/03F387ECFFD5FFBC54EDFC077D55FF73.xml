(
<quantity box="[495,663,1583,1608]" metricMagnitude="-3" metricUnit="m" metricValue="1.55" metricValueMax="1.6" metricValueMin="1.5" pageId="44" pageNumber="45" unit="mm" value="1.55" valueMax="1.6" valueMin="1.5">1.50–1.60 mm</quantity>
in ♂, 
<quantity box="[743,909,1584,1608]" metricMagnitude="-3" metricUnit="m" metricValue="1.5499999999999998" metricValueMax="1.7" metricValueMin="1.4" pageId="44" pageNumber="45" unit="mm" value="1.5499999999999998" valueMax="1.7" valueMin="1.4">1.40–1.70 mm</quantity>
