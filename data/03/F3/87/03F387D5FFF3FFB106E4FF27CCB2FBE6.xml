<paragraph blockId="10.[140,1108,377,1073]" box="[185,720,1046,1073]" pageId="10" pageNumber="237">
<emphasis bold="true" box="[185,344,1046,1072]" pageId="10" pageNumber="237">
<emphasis box="[185,339,1046,1072]" inLineHeading="true" pageId="10" pageNumber="237" reason="1">Distribution</emphasis>
