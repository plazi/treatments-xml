<emphasis italics="true" pageId="248" pageNumber="249">
<taxonomicName authorityName="FIEBER, WITH" authorityYear="1858" class="Insecta" family="Miridae" genus="Plagiognathus" kingdom="Animalia" order="Hemiptera" pageId="248" pageNumber="249" phylum="Arthropoda" rank="genus">Plagiognathus</taxonomicName>
