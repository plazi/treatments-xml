Gapud, 
<number box="[405,464,947,971]" pageId="5" pageNumber="75" value="1968.0">1968</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Yosii, 1976" authorityName="Yosii" authorityYear="1976" box="[485,885,947,971]" class="Entognatha" family="Neanuridae" genus="Lobella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="5" pageNumber="75" phylum="Arthropoda" rank="species" species="penangensis">
