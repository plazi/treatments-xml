Yoshii, 
<number box="[1219,1275,947,971]" pageId="5" pageNumber="75" value="1981.0">1981</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName authority="Yosii, 1976" authorityName="Yosii" authorityYear="1976" class="Entognatha" family="Neanuridae" genus="Lobella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="5" pageNumber="75" phylum="Arthropoda" rank="species" species="selangorica">
<emphasis italics="true" pageId="5" pageNumber="75">Lobella selangorica</emphasis>
<bibRefCitation author="Yosii" box="[290,422,982,1006]" pageId="5" pageNumber="75" refString="Yosii, R. (1976) On some Neanurid Collembola of Southeast Asia. Nature and life in southeast Asia, 7, 257 - 299." type="journal article" year="1976">
Yosii, 
<number box="[362,422,982,1006]" pageId="5" pageNumber="75" value="1976.0">1976</number>
</bibRefCitation>
</taxonomicName>
