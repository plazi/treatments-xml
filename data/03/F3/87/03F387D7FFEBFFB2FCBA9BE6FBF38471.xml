<taxonomicName authorityName="Kozur &amp; Mostler" authorityYear="1990" box="[1077,1283,471,497]" class="Polycystina" family="Bagotidae" genus="Droltus" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="29" pageNumber="520" phylum="Radiozoa" rank="species" species="eurasiaticus">
