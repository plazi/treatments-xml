, 
<date pageId="253" pageNumber="254" value="1914-07-01">
<collectingDate pageId="253" pageNumber="254" value="1914-07-01">July 1, 1914</collectingDate>
</date>
, 
<collectorName box="[231,516,1063,1086]" pageId="253" pageNumber="254">WL McAtee Collector’</collectorName>
’. Deposited in the 
<location LSID="urn:lsid:plazi:treatment:03F387FCFF29FF2C2D0AFC1DF860F9E9:8E856031FF29FF2C2DB9FBBAFBD5FBA1" box="[201,647,1092,1115]" name="National Museum of Natural History" pageId="253" pageNumber="254" stateProvince="Washington">National Museum of Natural History</location>
