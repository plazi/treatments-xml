<subSubSection pageId="16" pageNumber="417" type="diagnosis">
<paragraph blockId="16.[151,1436,151,587]" pageId="16" pageNumber="417">
<emphasis bold="true" box="[199,321,487,512]" pageId="16" pageNumber="417">Diagnosis.</emphasis>
