<emphasis box="[470,605,1472,1497]" italics="true" pageId="3" pageNumber="64">P, ro s c o v i t a</emphasis>
<bibRefCitation author="Swedmark" box="[617,819,1472,1498]" pageId="3" pageNumber="67" refString="Swedmark, B. (1956) Nouveaux Gastrotriches Macrodasyoides de la region de Roscoff. Archives de Zoologie Experimentale et Generale, 94, 43 - 57." type="journal article" year="1956">
Swedmark, 
<number box="[758,819,1472,1498]" pageId="3" pageNumber="64" value="1956.0">1956</number>
</bibRefCitation>
, 
<emphasis box="[833,967,1472,1497]" italics="true" pageId="3" pageNumber="64">P. m a l a y i c a</emphasis>
<bibRefCitation author="Renaud-Mornant" box="[979,1255,1472,1498]" pageId="3" pageNumber="67" refString="Renaud-Mornant, J. (1967) Pseudostomella malayica n. sp., Gastrotriche nouveau de la cote orientale de Malaise. Bulletin du Museum National d'Historie Naturelle, 39, 209 - 212." type="journal article" year="1967">
Renaud-Mornant, 
<number box="[1194,1255,1472,1498]" pageId="3" pageNumber="64" value="1967.0">1967</number>
</bibRefCitation>
, 
<taxonomicName authority="Rao, 1970" authorityName="Rao" authorityYear="1970" family="Thaumastodermatidae" genus="Pseudostomella" kingdom="Animalia" order="Macrodasyida" pageId="3" pageNumber="64" phylum="Gastrotricha" rank="species" species="indica">
<emphasis box="[1269,1373,1472,1497]" italics="true" pageId="3" pageNumber="64">P. indica</emphasis>
<bibRefCitation author="Rao" pageId="3" pageNumber="67" refString="Rao, G. C. (1970) Three new interstitial gastrotrichs from Andhra coast, India. Cahiers de Biologie Marine, 11, 109 - 120." type="journal article" year="1970">
Rao, 
<number box="[151,212,1512,1538]" pageId="3" pageNumber="64" value="1970.0">1970</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Rao, 1993" authorityName="Rao" authorityYear="1993" box="[226,535,1512,1538]" family="Thaumastodermatidae" genus="Pseudostomella" kingdom="Animalia" order="Macrodasyida" pageId="3" pageNumber="64" phylum="Gastrotricha" rank="species" species="andamanica">
