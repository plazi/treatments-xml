<taxonomicName authority="Hochberg, 2002" authorityName="Hochberg" authorityYear="2002" box="[332,672,1432,1458]" family="Thaumastodermatidae" genus="Pseudostomella" kingdom="Animalia" order="Macrodasyida" pageId="3" pageNumber="64" phylum="Gastrotricha" rank="species" species="klauserae">
