<taxonomicName authorityName="Casey" authorityYear="1910" box="[1087,1232,897,922]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="brumalis">
