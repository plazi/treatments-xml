<subSubSection pageId="2" pageNumber="585" type="etymology">
<paragraph blockId="2.[151,1437,1470,1711]" pageId="2" pageNumber="585">
<emphasis bold="true" box="[199,334,1650,1675]" pageId="2" pageNumber="585">Etymology.</emphasis>
