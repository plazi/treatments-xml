in 
<typeStatus box="[601,699,151,176]" pageId="44" pageNumber="45">holotype</typeStatus>
(range in 
<specimenCount box="[820,855,152,176]" pageId="44" pageNumber="45" type="generic">
<number box="[820,835,152,176]" pageId="44" pageNumber="45" value="4.0">4</number>
♂
</specimenCount>
and 
<number box="[914,931,151,176]" pageId="44" pageNumber="45" value="5.0">5</number>
♀ 
<typeStatus box="[958,1070,152,176]" pageId="44" pageNumber="45">paratypes</typeStatus>
: 
<quantity box="[1082,1247,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="3.465" metricValueMax="3.6" metricValueMin="3.33" pageId="44" pageNumber="45" unit="mm" value="3.465" valueMax="3.6" valueMin="3.33">3.33–3.60 mm</quantity>
in ♂, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.065" metricValueMax="3.2" metricValueMin="2.93" pageId="44" pageNumber="45" unit="mm" value="3.065" valueMax="3.2" valueMin="2.93">2.93–3.20 mm</quantity>
in ♀), THL = 
<quantity box="[374,478,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="1.73" pageId="44" pageNumber="45" unit="mm" value="1.73">1.73 mm</quantity>
(
