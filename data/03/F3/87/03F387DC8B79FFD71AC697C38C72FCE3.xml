<subSubSection pageId="8" pageNumber="409" type="diagnosis">
<paragraph blockId="8.[151,1436,478,1386]" pageId="8" pageNumber="409">
<emphasis bold="true" box="[199,321,1251,1276]" pageId="8" pageNumber="409">Diagnosis.</emphasis>
