<taxonomicName baseAuthorityName="Temminck" baseAuthorityYear="1840" box="[968,1105,1559,1584]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="52" pageNumber="53" phylum="Chordata" rank="species" species="horsfieldii">
