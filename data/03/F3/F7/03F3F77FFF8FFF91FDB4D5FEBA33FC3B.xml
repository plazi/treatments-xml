<subSubSection pageId="19" pageNumber="20" type="distribution">
<paragraph blockId="19.[533,1532,377,575]" box="[565,1337,412,436]" pageId="19" pageNumber="20">
<emphasis bold="true" box="[565,705,412,436]" pageId="19" pageNumber="20">Recorded at:</emphasis>
<materialsCitation ID-GBIF-Occurrence="2960680326" box="[711,1332,412,436]" collectionCode="Iucn" location="Tumang Lembing Cave" municipality="Bukit Jerneh Cave" pageId="19" pageNumber="20" specimenCount="1" stateProvince="Perak">
