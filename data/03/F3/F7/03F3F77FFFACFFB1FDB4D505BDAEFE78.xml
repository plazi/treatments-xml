<subSubSection pageId="51" pageNumber="52" type="discussion">
<paragraph blockId="51.[533,1509,207,474]" pageId="51" pageNumber="52">
<taxonomicName baseAuthorityName="Peters" baseAuthorityYear="1866" box="[565,640,381,405]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="51" pageNumber="52" phylum="Chordata" rank="species" species="ater">
<emphasis box="[565,588,382,405]" italics="true" pageId="51" pageNumber="52">M</emphasis>
