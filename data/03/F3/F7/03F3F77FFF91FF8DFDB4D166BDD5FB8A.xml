<subSubSection pageId="15" pageNumber="16" type="discussion">
<paragraph blockId="15.[533,1536,276,1064]" pageId="15" pageNumber="16">
<taxonomicName authorityName="Buchannan" authorityYear="1800" box="[565,673,1005,1029]" class="Mammalia" family="Molossidae" genus="Chaerephon" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="plicatus">
