<paragraph blockId="38.[533,1527,208,648]" box="[565,1522,624,648]" pageId="38" pageNumber="39">
<taxonomicName authorityName="Miller" authorityYear="1898" box="[565,669,624,648]" class="Mammalia" family="Vespertilionidae" genus="Kerivoula" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="39" phylum="Chordata" rank="species" species="minuta">
