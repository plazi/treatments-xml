<paragraph blockId="26.[533,1525,207,474]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[565,669,208,232]" pageId="26" pageNumber="27">Remarks:</emphasis>
<taxonomicName authorityName="Francis, Kock &amp; Habersetzer" authorityYear="1999" box="[676,803,207,232]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="26" pageNumber="27" phylum="Chordata" rank="species" species="orbiculus">
<emphasis box="[676,696,208,231]" italics="true" pageId="26" pageNumber="27">H</emphasis>
. 
<emphasis box="[708,803,207,231]" italics="true" pageId="26" pageNumber="27">orbiculus</emphasis>
</taxonomicName>
