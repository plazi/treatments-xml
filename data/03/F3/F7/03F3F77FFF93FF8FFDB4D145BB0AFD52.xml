<subSubSection pageId="13" pageNumber="14" type="discussion">
<paragraph blockId="13.[533,1523,311,752]" pageId="13" pageNumber="14">
<taxonomicName authorityName="Hardwicke" authorityYear="1825" box="[565,713,658,682]" class="Mammalia" family="Emballonuridae" genus="Taphozous" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="longimanus">
