<collectingRegion box="[819,909,441,467]" country="China" name="Sichuan" pageId="28" pageNumber="229">Sichuan</collectingRegion>
and 
<collectingRegion box="[965,1089,441,467]" country="China" name="Chongqing" pageId="28" pageNumber="229">Chongqing</collectingRegion>
(
<bibRefCitation author="Huang" box="[1107,1245,441,467]" firstAuthor="Huang" pageId="28" pageNumber="229" pagination="8 - 10" refId="ref19262" refString="Huang, M. X. (1986) Description of the living environment of several kinds of Ostracoda (Crustacea) in Sichuan. Sichuan Journal of Zoology, 1, 8 - 10. [in Chinese]" type="journal article" year="1986">Huang 1986</bibRefCitation>
), 
<collectingRegion box="[1269,1340,441,467]" country="China" name="Hubei" pageId="28" pageNumber="229">Hubei</collectingRegion>
, 
<collectingRegion country="China" name="Guangdong" pageId="28" pageNumber="229">Guangdong</collectingRegion>
and 
<collectingRegion box="[262,348,477,503]" country="China" name="Yunnan" pageId="28" pageNumber="229">Yunnan</collectingRegion>
(
<bibRefCitation author="Chen" box="[365,487,477,503]" firstAuthor="Chen" pageId="28" pageNumber="229" pagination="48 - 63" refId="ref18558" refString="Chen, S. Z. (1982) Subclass, Ostracoda. In: Dong, Y. (Ed.), Atlas of animals in China, Crustacea. Vol. 1. Science Press, Beijing, pp. 48 - 63. [in Chinese]" type="book chapter" year="1982">Chen 1982</bibRefCitation>
