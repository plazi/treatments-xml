<collectingCountry box="[589,660,153,179]" name="China" pageId="10" pageNumber="211">China</collectingCountry>
. On 
<collectingRegion box="[710,791,153,179]" country="China" name="Hainan" pageId="10" pageNumber="211">Hainan</collectingRegion>
Island, 
<taxonomicName authorityName="Ma &amp; Yu" authorityYear="2020" baseAuthorityName="G. W. Muller" baseAuthorityYear="1903" box="[880,1021,153,179]" class="Ostracoda" family="Cyprididae" genus="Cypria" kingdom="Animalia" order="Podocopida" pageId="10" pageNumber="211" phylum="Arthropoda" rank="species" species="kraepelini">
<emphasis box="[880,1021,153,179]" italics="true" pageId="10" pageNumber="211">P. kraepelini</emphasis>
</taxonomicName>
