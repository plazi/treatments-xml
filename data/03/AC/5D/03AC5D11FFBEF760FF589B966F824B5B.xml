<taxonomicName authority="(DENIS &amp; SCHIFFERMULLER, 1775)" baseAuthorityName="DENIS &amp; SCHIFFERMULLER" baseAuthorityYear="1775" box="[174,778,800,828]" class="Insecta" family="Geometridae" genus="Ectropis" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="162" phylum="Arthropoda" rank="species" species="crepuscularia">
