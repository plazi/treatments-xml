<subSubSection pageId="31" pageNumber="40" type="etymology">
<paragraph blockId="31.[140,1108,1257,1672]" pageId="31" pageNumber="40">
<emphasis bold="true" box="[185,320,1399,1425]" pageId="31" pageNumber="40">
