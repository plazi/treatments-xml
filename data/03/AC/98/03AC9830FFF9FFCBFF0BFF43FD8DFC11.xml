<number box="[910,959,582,606]" pageId="35" pageNumber="36" value="2.5">2.50</number>
long, 2.00 wide; opisthosoma 
<number box="[1320,1369,582,606]" pageId="35" pageNumber="36" value="3.5">3.50</number>
long, 
<number box="[151,200,618,642]" pageId="35" pageNumber="36" value="1.65">1.65</number>
wide. Carapace yellow. Eye measurements: AME 
<number box="[786,838,618,642]" pageId="35" pageNumber="36" value="0.13">0.13</number>
, ALE 
<number box="[915,967,618,642]" pageId="35" pageNumber="36" value="0.28">0.28</number>
, PME 
<number box="[1047,1099,618,642]" pageId="35" pageNumber="36" value="0.26">0.26</number>
, PLE 
<number box="[1171,1223,618,642]" pageId="35" pageNumber="36" value="0.25">0.25</number>
, AME-AME 
<number box="[1379,1431,618,642]" pageId="35" pageNumber="36" value="0.25">0.25</number>
; AME-ALE 
<number box="[284,336,654,678]" pageId="35" pageNumber="36" value="0.08">0.08</number>
; PME-PME 
<number box="[481,534,654,678]" pageId="35" pageNumber="36" value="0.34">0.34</number>
; PME-PLE 
<number box="[670,723,654,678]" pageId="35" pageNumber="36" value="0.3">0.30</number>
. Eye row width measurement: I 
<number box="[1090,1142,654,678]" pageId="35" pageNumber="36" value="0.48">0.48</number>
, II 
<number box="[1180,1232,654,678]" pageId="35" pageNumber="36" value="0.84">0.84</number>
, III 
<number box="[1279,1331,654,678]" pageId="35" pageNumber="36" value="1.4">1.40</number>
, IV 
<number box="[1380,1433,654,678]" pageId="35" pageNumber="36" value="0.78">0.78</number>
