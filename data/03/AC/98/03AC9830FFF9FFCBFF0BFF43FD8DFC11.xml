<number box="[611,663,726,750]" pageId="35" pageNumber="36" value="4.5">4.50</number>
, 
<number box="[676,728,726,750]" pageId="35" pageNumber="36" value="4.2">4.20</number>
, 
<number box="[740,789,726,750]" pageId="35" pageNumber="36" value="1.2">1.20</number>
