<number box="[599,651,1986,2010]" pageId="10" pageNumber="11" value="0.09">0.09</number>
, ALE 
<number box="[725,777,1986,2010]" pageId="10" pageNumber="11" value="0.21">0.21</number>
, PME 
<number box="[854,906,1986,2010]" pageId="10" pageNumber="11" value="0.17">0.17</number>
, PLE 
<number box="[976,1028,1986,2010]" pageId="10" pageNumber="11" value="0.18">0.18</number>
, AME-AME 
<number box="[1181,1234,1986,2010]" pageId="10" pageNumber="11" value="0.1">0.10</number>
; AME-ALE 
<number box="[1379,1432,1986,2010]" pageId="10" pageNumber="11" value="0.06">0.06</number>
; PME-PME 
<number box="[284,336,151,175]" pageId="11" pageNumber="12" value="0.22">0.22</number>
; PME-PLE 
<number box="[475,528,151,175]" pageId="11" pageNumber="12" value="0.29">0.29</number>
. Eye row width measurement: I 
<number box="[902,954,151,175]" pageId="11" pageNumber="12" value="0.25">0.25</number>
, II 
<number box="[995,1047,151,175]" pageId="11" pageNumber="12" value="0.6">0.60</number>
, III 
<number box="[1097,1148,151,175]" pageId="11" pageNumber="12" value="1.05">1.05</number>
