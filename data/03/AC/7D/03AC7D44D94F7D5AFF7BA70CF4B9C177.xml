<paragraph blockId="17.[151,1436,1191,1372]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[197,318,1269,1295]" pageId="17" pageNumber="18">Remarks.</emphasis>
<taxonomicName box="[325,441,1269,1294]" class="Insecta" family="Eulichadidae" genus="Eulichas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="bertiae">
