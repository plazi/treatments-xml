<taxonomicName authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[307,490,766,788]" class="Insecta" family="Staphylinidae" genus="Platydomene" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="sinuosasinuosa" status="sp. n.">
<emphasis box="[307,490,766,788]" italics="true" pageId="22" pageNumber="323">Platydomene sinuosasinuosa</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[496,550,767,788]" pageId="22" pageNumber="323" rank="species">sp. n.</taxonomicNameLabel>
: habitus (
<emphasis bold="true" box="[643,666,767,784]" pageId="22" pageNumber="323">79</emphasis>
); forebody (
<emphasis bold="true" box="[786,808,767,783]" pageId="22" pageNumber="323">80</emphasis>
); antenna (
<emphasis bold="true" box="[920,941,767,783]" pageId="22" pageNumber="323">81</emphasis>
); male sternite VII (
<emphasis bold="true" box="[1138,1161,767,783]" pageId="22" pageNumber="323">82</emphasis>
); male sternite VIII (
<emphasis bold="true" box="[372,394,796,813]" pageId="22" pageNumber="323">83</emphasis>
