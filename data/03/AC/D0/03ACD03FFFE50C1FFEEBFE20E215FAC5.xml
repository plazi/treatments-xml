<taxonomicName authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[207,412,1470,1486]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="species" species="involutum" status="sp. n.">
<emphasis box="[207,412,1470,1486]" italics="true" pageId="5" pageNumber="306">Lathrobium involutum</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[418,469,1470,1491]" pageId="5" pageNumber="306" rank="species">sp. n.</taxonomicNameLabel>
(
<emphasis bold="true" box="[485,514,1470,1487]" pageId="5" pageNumber="306">1-9</emphasis>
) and 
<taxonomicName authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[569,688,1470,1486]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="species" species="alishanum" status="sp. n.">
<emphasis box="[569,688,1470,1486]" italics="true" pageId="5" pageNumber="306">L. alishanum</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[695,745,1470,1491]" pageId="5" pageNumber="306" rank="species">sp. n.</taxonomicNameLabel>
(
<emphasis bold="true" box="[761,782,1470,1486]" pageId="5" pageNumber="306">10</emphasis>
): habitus (
<emphasis bold="true" box="[886,894,1470,1486]" pageId="5" pageNumber="306">1</emphasis>
); forebody (
<emphasis bold="true" box="[1013,1024,1470,1485]" pageId="5" pageNumber="306">2</emphasis>
); male sternite VII (
<emphasis bold="true" box="[225,235,1499,1516]" pageId="5" pageNumber="306">3</emphasis>
); male sternite VIII (
<emphasis bold="true" box="[432,443,1499,1515]" pageId="5" pageNumber="306">4</emphasis>
); aedeagus in lateral view (
<emphasis bold="true" box="[691,721,1499,1516]" pageId="5" pageNumber="306">5-6</emphasis>
); female tergite VIII (
<emphasis bold="true" box="[924,935,1499,1516]" pageId="5" pageNumber="306">7</emphasis>
); female sternite VIII (
<emphasis bold="true" box="[162,172,1528,1544]" pageId="5" pageNumber="306">8</emphasis>
); female tergites IX-X (
<emphasis bold="true" box="[398,408,1528,1545]" pageId="5" pageNumber="306">9</emphasis>
); head in lateral view (
<emphasis bold="true" box="[629,650,1528,1544]" pageId="5" pageNumber="306">10</emphasis>
). Scale bars: 1-2: 1.0 mm; 3-4, 7-10: 0.5 mm; 5-6: 0.2 mm.
</paragraph>
