<taxonomicName authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[222,441,1179,1195]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="nenkaoicum" status="sp. n.">
<emphasis box="[222,441,1179,1195]" italics="true" pageId="17" pageNumber="318">Lathrobium nenkaoicum</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[447,499,1179,1200]" pageId="17" pageNumber="318" rank="species">sp. n.</taxonomicNameLabel>
: habitus (
<emphasis bold="true" box="[591,613,1179,1196]" pageId="17" pageNumber="318">58</emphasis>
); forebody (
<emphasis bold="true" box="[730,752,1179,1196]" pageId="17" pageNumber="318">59</emphasis>
); male sternite VII (
<emphasis bold="true" box="[941,964,1179,1195]" pageId="17" pageNumber="318">60</emphasis>
); male sternite VIII (
<emphasis bold="true" box="[187,208,1208,1224]" pageId="17" pageNumber="318">61</emphasis>
); aedeagus in lateral view (
