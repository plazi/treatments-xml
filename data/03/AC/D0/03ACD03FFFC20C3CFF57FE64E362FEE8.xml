</taxonomicName>
<taxonomicNameLabel box="[562,614,1096,1117]" pageId="36" pageNumber="337" rank="species">sp. n.</taxonomicNameLabel>
: habitus (
<emphasis bold="true" box="[707,740,1096,1112]" pageId="36" pageNumber="337">124</emphasis>
); forebody (
<emphasis bold="true" box="[860,892,1096,1113]" pageId="36" pageNumber="337">125</emphasis>
); antenna (
<emphasis bold="true" box="[1004,1037,1096,1112]" pageId="36" pageNumber="337">126</emphasis>
); male sternite VII (
<emphasis bold="true" box="[244,278,1125,1142]" pageId="36" pageNumber="337">127</emphasis>
); male sternite VIII (
<emphasis bold="true" box="[480,513,1125,1141]" pageId="36" pageNumber="337">128</emphasis>
); aedeagus in lateral and in ventral view (
<emphasis bold="true" box="[905,982,1125,1142]" pageId="36" pageNumber="337">129-130</emphasis>
); female tergite VIII (
<emphasis bold="true" box="[205,237,1154,1171]" pageId="36" pageNumber="337">131</emphasis>
); female sternite VIII (
<emphasis bold="true" box="[470,503,1154,1171]" pageId="36" pageNumber="337">132</emphasis>
