<quantity box="[585,688,1912,1936]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="15" pageNumber="466" unit="mm" value="1.8">1.80 mm</quantity>
; PW 
<quantity box="[748,851,1911,1936]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" pageId="15" pageNumber="466" unit="mm" value="0.75">0.75 mm</quantity>
; PL 
<quantity box="[902,1005,1911,1936]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="15" pageNumber="466" unit="mm" value="0.35">0.35 mm</quantity>
; EW 
<quantity box="[1067,1170,1912,1936]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="15" pageNumber="466" unit="mm" value="1.1">1.10 mm</quantity>
; EL 
<quantity box="[1223,1326,1911,1936]" metricMagnitude="-3" metricUnit="m" metricValue="1.45" pageId="15" pageNumber="466" unit="mm" value="1.45">1.45 mm</quantity>
.
</paragraph>
