<number box="[626,738,548,573]" pageId="23" pageNumber="474" value="1.06" valueMax="1.15" valueMin="0.97">0.97–1.15</number>
(
<number box="[753,804,548,572]" pageId="23" pageNumber="474" value="1.1">1.10</number>
