(
<number box="[1022,1074,476,500]" pageId="23" pageNumber="474" value="0.73">0.73</number>
) mm; PL 
<number box="[1189,1301,476,500]" pageId="23" pageNumber="474" value="0.355" valueMax="0.38" valueMin="0.33">0.33–0.38</number>
(
<number box="[1317,1369,476,501]" pageId="23" pageNumber="474" value="0.35">0.35</number>
