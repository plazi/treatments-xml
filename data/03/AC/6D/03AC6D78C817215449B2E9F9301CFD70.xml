<number box="[335,386,511,536]" pageId="23" pageNumber="474" value="1.15">1.15</number>
) mm; EL 
<number box="[511,622,512,536]" pageId="23" pageNumber="474" value="1.43" valueMax="1.48" valueMin="1.38">1.38–1.48</number>
(
<number box="[641,692,512,536]" pageId="23" pageNumber="474" value="1.42">1.42</number>
) mm. Female (n = 
<number box="[924,941,512,536]" pageId="23" pageNumber="474" value="9.0">9</number>
): TL 
<number box="[1010,1122,511,536]" pageId="23" pageNumber="474" value="1.73" valueMax="1.88" valueMin="1.58">1.58–1.88</number>
(
<number box="[1141,1192,512,536]" pageId="23" pageNumber="474" value="1.77">1.77</number>
) mm; PW 
<number box="[1324,1436,511,536]" pageId="23" pageNumber="474" value="0.7250000000000001" valueMax="0.8" valueMin="0.65">0.65–0.80</number>
(
<number box="[159,211,548,572]" pageId="23" pageNumber="474" value="0.74">0.74</number>
) mm; PL 
<number box="[324,436,548,572]" pageId="23" pageNumber="474" value="0.33" valueMax="0.38" valueMin="0.28">0.28–0.38</number>
(
<number box="[450,502,548,572]" pageId="23" pageNumber="474" value="0.34">0.34</number>
