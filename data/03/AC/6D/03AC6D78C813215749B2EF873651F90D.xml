; PL 
<quantity box="[1077,1180,1435,1460]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" pageId="20" pageNumber="471" unit="mm" value="0.25">0.25 mm</quantity>
; EW 
<number box="[1243,1292,1436,1460]" pageId="20" pageNumber="471" value="0.93">0.93</number>
