<quantity box="[585,688,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="1.98" pageId="6" pageNumber="457" unit="mm" value="1.98">1.98 mm</quantity>
; PW 
<quantity box="[748,851,188,213]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="6" pageNumber="457" unit="mm" value="0.85">0.85 mm</quantity>
; PL 
<quantity box="[902,1005,188,213]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="6" pageNumber="457" unit="mm" value="0.35">0.35 mm</quantity>
; EW 
<quantity box="[1067,1170,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="6" pageNumber="457" unit="mm" value="1.2">1.20 mm</quantity>
; EL 
<quantity box="[1223,1326,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="1.63" pageId="6" pageNumber="457" unit="mm" value="1.63">1.63 mm</quantity>
.
</paragraph>
</subSubSection>
