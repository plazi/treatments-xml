<emphasis bold="true" box="[510,574,780,804]" pageId="15" pageNumber="844">
<collectingCountry box="[510,571,780,804]" name="Brazil" pageId="15" pageNumber="844">Brazil</collectingCountry>
.
</emphasis>
<collectingRegion country="Brazil" name="Rio de Janeiro" pageId="15" pageNumber="844">Rio de Janeiro Prov.</collectingRegion>
