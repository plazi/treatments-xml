<taxonomicName authority="(Linnaeus)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[151,572,1676,1702]" class="Insecta" family="Eumenidae" genus="Symmorphus" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="48" phylum="Arthropoda" rank="species" species="murarius">
