<number box="[627,638,993,1017]" pageId="16" pageNumber="549" value="1.0">1</number>
)- 
<number box="[657,689,993,1017]" pageId="16" pageNumber="549" value="13.0">13</number>
, leg IV: 
<number box="[785,821,993,1017]" pageId="16" pageNumber="549" value="1.5" valueMax="2.0" valueMin="1.0">1-2</number>
- 
<number box="[830,870,993,1017]" pageId="16" pageNumber="549" value="2.5" valueMax="3.0" valueMin="2.0">2-3</number>
(
<number box="[880,891,993,1017]" pageId="16" pageNumber="549" value="1.0">1</number>
)- 
<number box="[909,941,993,1017]" pageId="16" pageNumber="549" value="10.0">10</number>
; all setae on legs, except 
