<emphasis box="[997,1208,373,398]" italics="true" pageId="15" pageNumber="548">Drepanoppia falxa</emphasis>
(
<bibRefCitation author="Kok" box="[1223,1346,373,398]" pageId="15" pageNumber="548" refString="Kok, D. J. (1967) Studies on some South African Oppiidae Grandjean, 1953 (Acarina: Oribatei). Journal of the Entomological Society of South Africa, 30 (1), 40 - 74." type="journal article" year="1967">
Kok, 
<number box="[1286,1346,373,398]" pageId="15" pageNumber="548" value="1967.0">1967</number>
</bibRefCitation>
)
</taxonomicName>
; leg IV (
<number box="[159,204,409,434]" pageId="15" pageNumber="548" value="215.0">215</number>
)&gt; leg I (
<number box="[306,350,409,434]" pageId="15" pageNumber="548" value="168.0">168</number>
)&gt; leg III (
<number box="[472,516,409,434]" pageId="15" pageNumber="548" value="150.0">150</number>
)&gt; leg II (
<number box="[629,675,409,434]" pageId="15" pageNumber="548" value="146.0">146</number>
); leg setation: leg I: 
<number box="[902,935,409,434]" pageId="15" pageNumber="548" value="3.0" valueMax="5.0" valueMin="1.0">1-5</number>
- 
<number box="[944,960,409,434]" pageId="15" pageNumber="548" value="2.0">2</number>
(
<number box="[969,979,409,434]" pageId="15" pageNumber="548" value="1.0">1</number>
)- 
<number box="[997,1013,409,434]" pageId="15" pageNumber="548" value="4.0">4</number>
(
<number box="[1022,1038,409,434]" pageId="15" pageNumber="548" value="2.0">2</number>
)- 
<number box="[1056,1090,409,434]" pageId="15" pageNumber="548" value="20.0">20</number>
(
<number box="[1099,1115,409,434]" pageId="15" pageNumber="548" value="2.0">2</number>
), leg II: 
<number box="[1210,1244,409,434]" pageId="15" pageNumber="548" value="3.0" valueMax="5.0" valueMin="1.0">1-5</number>
- 
<number box="[1253,1269,409,434]" pageId="15" pageNumber="548" value="2.0">2</number>
(
<number box="[1279,1289,409,434]" pageId="15" pageNumber="548" value="1.0">1</number>
)- 
<number box="[1308,1325,409,434]" pageId="15" pageNumber="548" value="4.0">4</number>
(
<number box="[1334,1344,409,434]" pageId="15" pageNumber="548" value="1.0">1</number>
)- 
<number box="[1363,1395,409,434]" pageId="15" pageNumber="548" value="14.0">14</number>
(
<number box="[1405,1421,409,434]" pageId="15" pageNumber="548" value="2.0">2</number>
), leg III: 
<number box="[234,275,446,470]" pageId="15" pageNumber="548" value="2.5" valueMax="3.0" valueMin="2.0">2-3</number>
- 
<number box="[284,295,446,470]" pageId="15" pageNumber="548" value="1.0">1</number>
(
<number box="[305,316,446,470]" pageId="15" pageNumber="548" value="1.0">1</number>
)- 
<number box="[335,350,446,470]" pageId="15" pageNumber="548" value="3.0">3</number>
(
<number box="[360,371,446,470]" pageId="15" pageNumber="548" value="1.0">1</number>
)- 
<number box="[390,421,446,470]" pageId="15" pageNumber="548" value="13.0">13</number>
, leg IV: 
<number box="[515,552,446,470]" pageId="15" pageNumber="548" value="1.5" valueMax="2.0" valueMin="1.0">1-2</number>
- 
<number box="[561,601,446,470]" pageId="15" pageNumber="548" value="2.5" valueMax="3.0" valueMin="2.0">2-3</number>
(
<number box="[611,622,446,470]" pageId="15" pageNumber="548" value="1.0">1</number>
)- 
