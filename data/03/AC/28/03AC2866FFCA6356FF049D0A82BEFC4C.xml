<emphasis box="[1414,1436,1930,1955]" italics="true" pageId="2" pageNumber="535">lp</emphasis>
almost on transverse line; 
<emphasis box="[445,459,1969,1994]" italics="true" pageId="2" pageNumber="535">h</emphasis>
<number box="[459,467,1984,1998]" pageId="2" pageNumber="535" value="2.0">2</number>
