<emphasis bold="true" box="[943,1133,569,590]" pageId="133" pageNumber="133">
<specimenCode box="[943,1133,569,590]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="133" pageNumber="133">AMNH 693812</specimenCode>
</emphasis>
