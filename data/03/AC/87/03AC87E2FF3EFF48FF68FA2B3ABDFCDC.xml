<specimenCount box="[1046,1228,510,532]" pageId="133" pageNumber="133" type="generic" typeStatus="paratypes">four paratypes</specimenCount>
in 
<collectionCode box="[741,837,540,561]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="133" pageNumber="133">AMNH</collectionCode>
