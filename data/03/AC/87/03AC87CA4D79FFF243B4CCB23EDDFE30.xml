<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[507,676,189,212]" class="Magnoliopsida" family="Asteraceae" genus="Tagetes" kingdom="Plantae" order="Asterales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="species" species="minuta">
