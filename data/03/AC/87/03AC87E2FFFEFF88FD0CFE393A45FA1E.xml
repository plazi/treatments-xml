<collectionCode box="[93,186,421,442]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="70" pageNumber="70">AMNH</collectionCode>
