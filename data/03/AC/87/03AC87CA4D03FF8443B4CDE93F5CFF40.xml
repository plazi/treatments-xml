and 
<taxonomicName authorityName="Dathe" authorityYear="2014" box="[1246,1436,189,212]" class="Insecta" family="Colletidae" genus="Hylaeus" kingdom="Animalia" order="Hymenoptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="camerunensis">
