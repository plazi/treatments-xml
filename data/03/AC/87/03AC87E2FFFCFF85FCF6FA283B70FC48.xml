<specimenCount box="[197,306,834,856]" pageId="72" pageNumber="72" type="male">one male</specimenCount>
and 
<specimenCount box="[374,505,834,856]" pageId="72" pageNumber="72" type="female">one female</specimenCount>
<typeStatus box="[517,620,835,856]" pageId="72" pageNumber="72">paratype</typeStatus>
