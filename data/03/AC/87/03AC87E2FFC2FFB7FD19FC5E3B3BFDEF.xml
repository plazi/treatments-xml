<location LSID="urn:lsid:plazi:treatment:03AC87E2FFC2FFB7FD19FC5E3B3BFDEF:8EDA602FFFC2FFB4FB85F9F53A47F900" county="Mathews" latitude="-17.1" longLatPrecision="767" longitude="145.52" municipality="North" name="Mount Sapphiri" pageId="121" pageNumber="121" stateProvince="Queensland">Mount Sapphiri</location>
(
<emphasis bold="true" box="[842,865,1680,1700]" italics="true" pageId="121" pageNumber="121">5</emphasis>
<location LSID="urn:lsid:plazi:treatment:03AC87E2FFC2FFB7FD19FC5E3B3BFDEF:8EDA602FFFC2FFB4FC7DF9083D2DF900" box="[898,1098,1678,1700]" county="Mathews" latitude="-17.1" longLatPrecision="767" longitude="145.52" municipality="North" name="Mount Sophia" pageId="121" pageNumber="121" stateProvince="Queensland">Mount Sophia</location>
), 
<geoCoordinate box="[1143,1223,1679,1700]" degrees="17.10" direction="south" orientation="latitude" pageId="121" pageNumber="121" precision="555" value="-17.1">17.10S</geoCoordinate>
