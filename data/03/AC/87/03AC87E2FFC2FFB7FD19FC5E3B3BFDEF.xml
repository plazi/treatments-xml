<geoCoordinate box="[701,795,1708,1730]" degrees="145.52" direction="east" orientation="longitude" pageId="121" pageNumber="121" precision="555" value="145.52">145.52E</geoCoordinate>
(
<collectionCode box="[817,922,1708,1729]" pageId="121" pageNumber="121">USBGN</collectionCode>
