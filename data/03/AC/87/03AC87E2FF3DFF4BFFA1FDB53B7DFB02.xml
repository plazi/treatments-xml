<collectionCode box="[337,430,993,1014]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="134" pageNumber="134">AMNH</collectionCode>
