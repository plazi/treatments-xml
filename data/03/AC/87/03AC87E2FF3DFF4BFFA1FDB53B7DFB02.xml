<emphasis bold="true" box="[392,611,1109,1131]" pageId="134" pageNumber="134">
<collectionCode box="[392,482,1109,1130]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="134" pageNumber="134">AMNH</collectionCode>
