<taxonomicName box="[366,559,391,414]" class="Aves" family="Meliphagidae" genus="Ptilotis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="74" pageNumber="74" phylum="Chordata" rank="species" species="montana">
