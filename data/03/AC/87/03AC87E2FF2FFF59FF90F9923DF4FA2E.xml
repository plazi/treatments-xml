<taxonomicName authorityName="Lesson" authorityYear="1830" box="[986,1129,1248,1271]" class="Aves" family="Meliphagidae" genus="Phylidonyris" kingdom="Animalia" order="Passeriformes" pageId="148" pageNumber="148" phylum="Chordata" rank="genus">
