,
</emphasis>
<taxonomicNameLabel box="[620,705,312,335]" pageId="1" pageNumber="56" rank="species">sp. nov.</taxonomicNameLabel>
, and 
<taxonomicName authority="Ma et Zheng, 1992" authorityName="Ma et Zheng" authorityYear="1992" box="[761,1142,312,335]" class="Insecta" family="Baissogryllidae" genus="Caryanda" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="species" species="rufofemorata">
