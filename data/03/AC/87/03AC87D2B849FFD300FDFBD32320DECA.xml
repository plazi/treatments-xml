<quantity box="[670,763,1241,1264]" metricMagnitude="-1" metricUnit="m" metricValue="3.937" metricValueMax="4.318" metricValueMin="3.556" pageId="22" pageNumber="23" unit="in" value="15.5" valueMax="17.0" valueMin="14.0">14–17 in</quantity>
<quantity box="[770,841,1241,1264]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="22" pageNumber="23" unit="mm" value="10.0">10 µm</quantity>
.
</emphasis>
</paragraph>
