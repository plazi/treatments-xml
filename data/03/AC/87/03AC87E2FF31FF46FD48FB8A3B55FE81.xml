<collectionCode box="[990,1086,1649,1670]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="138" pageNumber="138">AMNH</collectionCode>
