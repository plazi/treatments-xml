<subSubSection box="[199,638,656,683]" pageId="23" pageNumber="24" type="distribution">
<paragraph blockId="23.[151,1437,153,864]" box="[199,638,656,683]" pageId="23" pageNumber="24">
<emphasis bold="true" box="[199,353,656,683]" pageId="23" pageNumber="24">Distribution.</emphasis>
Oriental [
<collectingCountry box="[465,539,657,683]" name="China" pageId="23" pageNumber="24">China</collectingCountry>
: 
<collectingRegion box="[549,627,656,683]" country="China" name="Fujian" pageId="23" pageNumber="24">Fujian</collectingRegion>
