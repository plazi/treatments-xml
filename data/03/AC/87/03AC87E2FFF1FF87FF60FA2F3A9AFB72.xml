<collectionCode box="[918,1011,509,530]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH</collectionCode>
