<collectionCode box="[1092,1189,685,706]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH</collectionCode>
. 
<collectorName box="[667,788,715,737]" pageId="74" pageNumber="74">Specimens</collectorName>
