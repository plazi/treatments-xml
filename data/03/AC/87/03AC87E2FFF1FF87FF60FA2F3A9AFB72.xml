<collectionCode box="[1100,1194,627,648]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH</collectionCode>
