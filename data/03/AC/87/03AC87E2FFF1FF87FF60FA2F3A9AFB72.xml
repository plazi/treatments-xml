, 
<specimenCount pageId="74" pageNumber="74" type="female">two females</specimenCount>
from the 
<collectorName pageId="74" pageNumber="74">
<location LSID="urn:lsid:plazi:treatment:03AC87E2FFF1FF87FF60FA2F3A9AFB72:8EDA602FFFF1FF87FC83FE453D37FE73" box="[892,1104,449,471]" county="Arfak Mountains" municipality="Meek's Snow Mountains" name="Snow Mountains" pageId="74" pageNumber="74">Snow Mountains</location>
