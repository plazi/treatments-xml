<collectionCode box="[667,760,833,854]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH</collectionCode>
with the 
<collectorName box="[893,1156,833,855]" pageId="74" pageNumber="74">Rothschild Collection</collectorName>
