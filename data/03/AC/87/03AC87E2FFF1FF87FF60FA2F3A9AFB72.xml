<location LSID="urn:lsid:plazi:treatment:03AC87E2FFF1FF87FF60FA2F3A9AFB72:8EDA602FFFF1FF87FD64FE233AFFFE1E" box="[667,920,420,442]" name="Weyland Mountains" pageId="74" pageNumber="74">Weyland Mountains</location>
