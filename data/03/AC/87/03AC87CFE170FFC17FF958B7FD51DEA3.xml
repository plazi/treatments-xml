<emphasis box="[295,566,1925,1949]" italics="true" pageId="106" pageNumber="107">Dolichogenidea punctipila</emphasis>
Liu &amp; Chen
</taxonomicName>
, 
<emphasis bold="true" box="[707,787,1925,1949]" pageId="106" pageNumber="107">
<taxonomicNameLabel box="[707,787,1925,1949]" pageId="106" pageNumber="107" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, female, holotype. 
<emphasis bold="true" box="[984,1003,1925,1949]" pageId="106" pageNumber="107">a.</emphasis>
habitus, lateral view; 
<emphasis bold="true" box="[1228,1249,1925,1949]" pageId="106" pageNumber="107">b.</emphasis>
fore wing; 
<emphasis bold="true" box="[1367,1384,1925,1949]" pageId="106" pageNumber="107">c.</emphasis>
hind wing; 
<emphasis bold="true" box="[216,237,1961,1985]" pageId="106" pageNumber="107">d.</emphasis>
head, dorsal view; 
<emphasis bold="true" box="[439,457,1961,1985]" pageId="106" pageNumber="107">e.</emphasis>
mesosoma, dorsal view; 
<emphasis bold="true" box="[718,733,1960,1985]" pageId="106" pageNumber="107">f.</emphasis>
propodeum, dorsal view; 
<emphasis bold="true" box="[1003,1022,1961,1985]" pageId="106" pageNumber="107">g.</emphasis>
metasoma, dorsal view; 
<emphasis bold="true" box="[1279,1300,1961,1985]" pageId="106" pageNumber="107">h.</emphasis>
