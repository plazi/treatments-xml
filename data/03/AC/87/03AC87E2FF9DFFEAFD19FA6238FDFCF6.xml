<emphasis bold="true" pageId="39" pageNumber="39">
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="39" pageNumber="39">AMNH 701429– 701435</specimenCode>
</emphasis>
