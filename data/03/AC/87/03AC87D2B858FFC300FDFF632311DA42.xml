<taxonomicName authorityName="Levkov, D. M. Williams &amp; Cvetkoska" authorityYear="2011" box="[1395,1436,329,352]" class="Bacillariophyceae" family="Fragilariaceae" genus="Staurosirella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Fragilariales" pageId="7" pageNumber="8" phylum="Bacillariophyta" rank="species" species="lata">lata</taxonomicName>
unlateralis. Striae coarse, uniseriatae, omnino radiantes 
<quantity box="[849,950,361,384]" metricMagnitude="-1" metricUnit="m" metricValue="4.826" metricValueMax="5.08" metricValueMin="4.572" pageId="7" pageNumber="8" unit="in" value="19.0" valueMax="20.0" valueMin="18.0">18–20 in</quantity>
<quantity box="[963,1042,361,384]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="7" pageNumber="8" unit="mm" value="10.0">10 µm</quantity>
