<specimenCode box="[1048,1228,1011,1032]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="91" pageNumber="91">AMNH 695262</specimenCode>
