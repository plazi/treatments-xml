in 
<collectionCode box="[701,798,953,974]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="91" pageNumber="91">AMNH</collectionCode>
: 
<emphasis bold="true" box="[848,1066,952,973]" pageId="91" pageNumber="91">
