<taxonomicName authorityName="Gould" authorityYear="1875" box="[380,445,1019,1039]" class="Aves" family="Meliphagidae" genus="Melithreptus" kingdom="Animalia" order="Passeriformes" pageId="115" pageNumber="115" phylum="Chordata" rank="species" species="laetior">laetior</taxonomicName>
