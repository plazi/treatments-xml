<specimenCode box="[929,1110,896,917]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="23" pageNumber="23">AMNH 700728</specimenCode>
