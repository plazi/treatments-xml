<location LSID="urn:lsid:plazi:treatment:03AC87E2FF86FFF3FD05FCA73B76FD67:8EDA602FFF86FFF0FC1CFAAA3DE8FAE6" box="[995,1167,1325,1346]" county="Tabar" municipality="Mayr" name="New Hanover" pageId="61" pageNumber="61">New Hanover</location>
