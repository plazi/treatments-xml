, 
<specimenCount box="[1020,1167,1295,1317]" pageId="61" pageNumber="61" type="female">two females</specimenCount>
, and 
<specimenCount box="[701,811,1324,1346]" pageId="61" pageNumber="61" type="male">one male</specimenCount>
