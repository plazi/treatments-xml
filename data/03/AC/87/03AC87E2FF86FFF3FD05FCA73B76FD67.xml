<specimenCount box="[701,839,1354,1376]" pageId="61" pageNumber="61" type="male">eight males</specimenCount>
, 
<specimenCount box="[854,1014,1354,1376]" pageId="61" pageNumber="61" type="female">three females</specimenCount>
