, 
<collectingCountry box="[813,895,1373,1397]" name="Bolivia" pageId="3" pageNumber="632">Bolivia</collectingCountry>
, 
<collectingCountry box="[905,1006,1374,1398]" name="Paraguay" pageId="3" pageNumber="632">Paraguay</collectingCountry>
and 
<collectingCountry box="[1059,1156,1374,1398]" name="Uruguay" pageId="3" pageNumber="632">Uruguay</collectingCountry>
.
</paragraph>
