<location LSID="urn:lsid:plazi:treatment:03AC87E2FF96FFE0FF1FFA943AA6FD22:8EDA602FFF96FFE0FC65FF753AA0FEAC" box="[922,967,242,264]" name="The" pageId="45" pageNumber="45" stateProvince="Bougainville">The</location>
