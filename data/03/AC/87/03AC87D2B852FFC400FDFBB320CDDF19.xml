<paragraph blockId="13.[151,1436,1075,1338]" pageId="13" pageNumber="14">
<emphasis italics="true" pageId="13" pageNumber="14">
<collectingRegion box="[151,222,1113,1136]" country="Bulgaria" name="Vratsa" pageId="13" pageNumber="14">Valvae</collectingRegion>
late lanceolatae apicibus curte protractis rostratis, 
<quantity box="[779,888,1113,1136]" metricMagnitude="-2" metricUnit="m" metricValue="2.45" metricValueMax="2.8" metricValueMin="2.1" pageId="13" pageNumber="14" unit="mm" value="24.5" valueMax="28.0" valueMin="21.0">21–28 µm</quantity>
