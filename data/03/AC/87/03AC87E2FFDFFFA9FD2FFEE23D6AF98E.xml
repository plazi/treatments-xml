<taxonomicName authority="(Milligan, 1904)" baseAuthorityName="Milligan" baseAuthorityYear="1904" class="Aves" family="Meliphagidae" genus="Lichenostomus" kingdom="Animalia" order="Passeriformes" pageId="100" pageNumber="100" phylum="Chordata" rank="subSpecies" species="leucotis" subSpecies="novaenorciae">
