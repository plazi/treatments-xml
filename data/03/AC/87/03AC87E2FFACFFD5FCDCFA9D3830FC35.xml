<collectionCode box="[1134,1227,1678,1699]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="23" pageNumber="23">AMNH</collectionCode>
