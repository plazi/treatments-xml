<location LSID="urn:lsid:plazi:treatment:03AC87E2FFEBFF9DFF59FDEC3B7AF97B:8EDA602FFFEBFF9DFCA4FDE53AA1FDDC" box="[859,966,610,632]" country="Australia" county="Mathews" municipality="Broome Hill" name="Mathews" pageId="80" pageNumber="80">Mathews</location>
