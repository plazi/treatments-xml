</date>
and 
<specimenCode box="[667,860,1136,1158]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="80" pageNumber="80">AMNH 694818</specimenCode>
