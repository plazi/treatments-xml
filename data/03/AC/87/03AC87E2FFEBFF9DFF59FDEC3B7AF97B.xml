<date box="[957,1193,1136,1158]" pageId="80" pageNumber="80" value="1910-11-10">
<collectingDate box="[957,1193,1136,1158]" pageId="80" pageNumber="80" value="1910-11-10">10 November 1910</collectingDate>
</date>
