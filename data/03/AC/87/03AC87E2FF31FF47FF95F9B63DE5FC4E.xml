<collectionCode box="[667,763,892,913]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="138" pageNumber="138" type="Museum">RMNH</collectionCode>
