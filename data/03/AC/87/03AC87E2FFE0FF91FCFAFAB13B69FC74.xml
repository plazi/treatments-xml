<location LSID="urn:lsid:plazi:treatment:03AC87E2FFE0FF91FCFAFAB13B69FC74:8EDA602FFFE7FF91FFA2FE5839EDFE51" box="[93,138,479,501]" county="Mathews" municipality="Range." name="The" pageId="92" pageNumber="92" stateProvince="Stirling">The</location>
