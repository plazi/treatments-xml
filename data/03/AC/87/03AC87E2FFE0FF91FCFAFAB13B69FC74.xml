<collectorName box="[93,201,509,531]" pageId="92" pageNumber="92">Whitlock</collectorName>
in the 
<collectingRegion box="[317,402,509,531]" country="United Kingdom" name="Stirling" pageId="92" pageNumber="92">Stirling</collectingRegion>
<location LSID="urn:lsid:plazi:treatment:03AC87E2FFE0FF91FCFAFAB13B69FC74:8EDA602FFFE7FF91FE5AFE7A3897FDB6" box="[421,496,509,530]" county="Mathews" municipality="Range." name="Range" pageId="92" pageNumber="92" stateProvince="Stirling">Range</location>
