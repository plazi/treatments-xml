<taxonomicName authorityName="Gould" authorityYear="1867" class="Aves" family="Meliphagidae" genus="Ptilotis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="77" pageNumber="77" phylum="Chordata" rank="species" species="notata">
<emphasis italics="true" pageId="77" pageNumber="77">Ptilotis notata</emphasis>
</taxonomicName>
