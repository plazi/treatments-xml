<specimenCount box="[188,359,538,560]" pageId="77" pageNumber="77" type="generic" typeStatus="paratypes">five paratypes</specimenCount>
in 
<collectionCode box="[413,506,539,560]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="77" pageNumber="77">AMNH</collectionCode>
