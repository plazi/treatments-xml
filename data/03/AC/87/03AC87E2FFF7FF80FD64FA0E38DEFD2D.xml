<emphasis bold="true" box="[128,316,568,589]" pageId="77" pageNumber="77">
<specimenCode box="[128,316,568,589]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="77" pageNumber="77">AMNH 695917</specimenCode>
