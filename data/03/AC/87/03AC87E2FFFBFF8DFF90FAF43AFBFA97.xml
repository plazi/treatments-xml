<taxonomicName authorityName="Gould" authorityYear="1840" class="Aves" family="Meliphagidae" genus="Myzomela" kingdom="Animalia" order="Passeriformes" pageId="64" pageNumber="64" phylum="Chordata" rank="species" species="erythrocephala">
