<collectingCountry box="[364,468,637,663]" name="Malaysia" pageId="13" pageNumber="14">Sarawak</collectingCountry>
, 
<collectingCountry box="[480,591,637,663]" name="Malaysia" pageId="13" pageNumber="14">Malaysia</collectingCountry>
