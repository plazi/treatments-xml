<quantity box="[1211,1278,1406,1429]" metricMagnitude="-1" metricUnit="m" metricValue="1.143" metricValueMax="1.27" metricValueMin="1.016" pageId="4" pageNumber="5" unit="in" value="4.5" valueMax="5.0" valueMin="4.0">4–5 in</quantity>
<quantity box="[1284,1356,1406,1429]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="4" pageNumber="5" unit="mm" value="10.0">10 µm</quantity>
.
</emphasis>
</paragraph>
