<taxonomicName authority="Robiniau" authorityName="Robiniau" box="[617,871,295,320]" class="Insecta" family="Muscidae" genus="Limnophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
