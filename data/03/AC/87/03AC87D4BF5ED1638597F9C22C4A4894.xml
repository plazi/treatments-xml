<taxonomicName authority="De Geer" authorityName="De Geer" box="[151,355,1981,2003]" class="Insecta" family="Muscidae" genus="Morellia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="autumnalis">
