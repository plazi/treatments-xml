<emphasis bold="true" pageId="42" pageNumber="42">
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="42" pageNumber="42">AMNH 258608– 258611</specimenCode>
