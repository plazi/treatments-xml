<collectionCode box="[128,221,213,234]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="43" pageNumber="43">AMNH</collectionCode>
