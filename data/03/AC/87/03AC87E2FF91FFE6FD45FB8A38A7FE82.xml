<collectionCode box="[830,923,1619,1640]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="42" pageNumber="42">AMNH</collectionCode>
