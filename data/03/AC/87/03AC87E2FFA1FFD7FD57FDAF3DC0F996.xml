<taxonomicName baseAuthorityName="Gmelin" baseAuthorityYear="1789" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="26" pageNumber="26" phylum="Chordata" rank="species" species="flavifrons">
