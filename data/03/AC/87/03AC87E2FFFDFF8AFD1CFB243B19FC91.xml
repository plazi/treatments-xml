<taxonomicName authorityName="Mathews" authorityYear="1922" box="[128,226,740,763]" class="Aves" family="Meliphagidae" genus="Sugomel" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="71" pageNumber="71" phylum="Chordata" rank="genus">
