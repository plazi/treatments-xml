<taxonomicName authorityName="Mathews" authorityYear="1922" box="[375,473,798,821]" class="Aves" family="Meliphagidae" genus="Sugomel" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="71" pageNumber="71" phylum="Chordata" rank="genus">
