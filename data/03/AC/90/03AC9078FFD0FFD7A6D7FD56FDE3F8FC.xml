<location LSID="urn:lsid:plazi:treatment:03AC9078FFD0FFD7A6D7FD56FDE3F8FC:8EDA77B5FFD0FFD4A462FB4EFD26FD93" box="[546,641,1195,1218]" country="United States of America" county="Cub River" municipality="River Canyon" name="Museum" pageId="5" pageNumber="246" stateProvince="Idaho">Muséum</location>
national 
<location LSID="urn:lsid:plazi:treatment:03AC9078FFD0FFD7A6D7FD56FDE3F8FC:8EDA77B5FFD0FFD4A4ACFB4EFCF9FD92" box="[748,862,1195,1219]" country="United States of America" county="Cub River" municipality="River Canyon" name="d'Histoire" pageId="5" pageNumber="246" stateProvince="Idaho">d’Histoire</location>
naturelle
