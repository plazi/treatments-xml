<taxonomicName authority="(Fabricius, 1793)" baseAuthorityName="Fabricius" baseAuthorityYear="1793" box="[692,1146,974,1001]" class="Insecta" family="Ichneumonidae" genus="Rhyssella" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="32" phylum="Arthropoda" rank="species" species="approximator">
