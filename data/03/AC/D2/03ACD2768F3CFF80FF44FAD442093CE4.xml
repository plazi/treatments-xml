<paragraph blockId="104.[234,544,1388,1412]" box="[234,544,1388,1412]" pageId="104" pageNumber="105">
<heading box="[234,544,1388,1412]" centered="true" fontSize="10" level="2" pageId="104" pageNumber="105" reason="4">
