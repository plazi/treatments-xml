, 
<taxonomicName authorityName="Messer and Raber" authorityYear="2021" class="Insecta" family="Carabidae" genus="Selenophorus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="40" phylum="Arthropoda" rank="species" species="elytrostictus">
<emphasis italics="true" pageId="32" pageNumber="40">S. elytrostictus</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="LeConte" baseAuthorityYear="1863" box="[289,379,618,640]" class="Insecta" family="Carabidae" genus="Selenophorus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="40" phylum="Arthropoda" rank="species" species="fatuus">
