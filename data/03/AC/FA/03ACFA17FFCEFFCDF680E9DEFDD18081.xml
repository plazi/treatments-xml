<number box="[1161,1217,366,390]" pageId="27" pageNumber="28" value="1977.0">1977</number>
from Mato Grosso, 
<collectingCountry box="[151,219,402,426]" name="Brazil" pageId="27" pageNumber="28">Brazil</collectingCountry>
(
<typeStatus box="[238,288,402,426]" pageId="27" pageNumber="28">type</typeStatus>
