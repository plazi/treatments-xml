<taxonomicName authority="(Guimaraesiella)" box="[262,638,1862,1887]" class="Insecta" family="Philopteridae" genus="Guimaraesiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="subGenus" subGenus="Guimaraesiella">
