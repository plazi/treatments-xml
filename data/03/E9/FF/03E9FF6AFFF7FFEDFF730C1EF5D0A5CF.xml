. (
<taxonomicName authorityName="Gustafsson &amp; Clayton &amp; Bush" authorityYear="2019" box="[196,325,782,807]" class="Insecta" family="Philopteridae" genus="Guimaraesiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="2" pageNumber="490" phylum="Arthropoda" rank="subGenus" subGenus="Cicchinella">
