<caption ID-DOI="http://doi.org/10.5281/zenodo.3770874" ID-Zenodo-Dep="3770874" httpUri="https://zenodo.org/record/3770874/files/figure.png" pageId="5" pageNumber="64" startId="5.[121,165,680,702]" targetBox="[139,1089,165,664]" targetPageId="5">
<paragraph blockId="5.[121,1089,680,734]" pageId="5" pageNumber="64">
