<emphasis bold="true" box="[151,275,2017,2042]" pageId="7" pageNumber="152">FIGURE 6.</emphasis>
<taxonomicName authority="Feng &amp; Wynne &amp; Zhang, 2019" authorityName="Feng &amp; Wynne &amp; Zhang" authorityYear="2019" box="[281,506,2017,2041]" class="Arachnida" family="Neobisiidae" genus="Parobisium" kingdom="Animalia" order="Pseudoscorpiones" pageId="7" pageNumber="152" phylum="Arthropoda" rank="species" species="yuantongi" status="sp. n.">
<emphasis box="[281,506,2017,2041]" italics="true" pageId="7" pageNumber="152">Parobisium yuantongi</emphasis>
</taxonomicName>
<emphasis bold="true" box="[513,570,2018,2042]" pageId="7" pageNumber="152">
