<taxonomicName authorityName="Ranga Reddy &amp; Totakura" authorityYear="2012" box="[668,938,1508,1530]" class="Malacostraca" family="Bathynellidae" genus="Indobathynella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Bathynellacea" pageId="2" pageNumber="347" phylum="Arthropoda" rank="species" species="prehensilis">
