spp. in Andhra Pradesh. 
<taxonomicName authority="Shaik, 2019" authorityName="Shaik" authorityYear="2019" box="[1059,1306,1478,1500]" class="Malacostraca" family="Bathynellidae" genus="Indobathynella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Bathynellacea" pageId="2" pageNumber="347" phylum="Arthropoda" rank="species" species="socrates" status="n. sp.">
<emphasis box="[1059,1306,1478,1500]" italics="true" pageId="2" pageNumber="347">Indobathynella socrates</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1313,1373,1479,1500]" pageId="2" pageNumber="347">
<taxonomicNameLabel box="[1313,1373,1479,1500]" pageId="2" pageNumber="347" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
(
<emphasis bold="true" box="[1389,1411,1477,1500]" pageId="2" pageNumber="347">*</emphasis>
); 
<taxonomicName authorityName="Ranga Reddy &amp; Totakura" authorityYear="2012" box="[151,420,1508,1530]" class="Malacostraca" family="Bathynellidae" genus="Indobathynella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Bathynellacea" pageId="2" pageNumber="347" phylum="Arthropoda" rank="species" species="prehensilis">
