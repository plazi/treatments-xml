, 
<location LSID="urn:lsid:plazi:treatment:03E987E2FE412BA7FF3ED407FD8DFF7C:8E9F602FFE412BA6FE91D288FE25FB66" box="[312,452,1222,1249]" country="South Africa" county="Head" municipality="Uitkyk Pass" name="Uitkyk Pass" pageId="494" pageNumber="495">Uitkyk Pass</location>
