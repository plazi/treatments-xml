<subSubSection box="[222,980,1466,1490]" pageId="1" pageNumber="606" type="nomenclature">
<paragraph blockId="1.[222,980,1466,1490]" box="[222,980,1466,1490]" pageId="1" pageNumber="606">
<heading bold="true" box="[222,980,1466,1490]" centered="true" fontSize="8" level="2" pageId="1" pageNumber="606" reason="3">
<emphasis bold="true" box="[222,980,1466,1490]" pageId="1" pageNumber="606">
<taxonomicName authority="Obenberger, 1931" authorityName="Obenberger" authorityYear="1931" box="[222,818,1466,1490]" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="606" phylum="Arthropoda" rank="species" species="mashuna" subGenus="Haplanthaxia">
<emphasis bold="true" box="[222,324,1466,1490]" italics="true" pageId="1" pageNumber="606">Anthaxia</emphasis>
(
<emphasis bold="true" box="[339,492,1466,1490]" italics="true" pageId="1" pageNumber="606">Haplanthaxia</emphasis>
) 
<emphasis bold="true" box="[508,610,1466,1490]" italics="true" pageId="1" pageNumber="606">mashuna</emphasis>
<bibRefCitation author="OBENBERGER J." box="[617,818,1467,1490]" pageId="1" pageNumber="606" pagination="84 - 127" refId="ref10811" refString="OBENBERGER J. 1931: Studien uber die aethiopischen Buprestiden II. Folia Zoologica et Hydrobiologica 3 (1): 84 - 127." type="journal article" year="1931">Obenberger, 1931</bibRefCitation>
