<subSubSection pageId="243" pageNumber="244" type="etymology">
<paragraph blockId="243.[151,1437,153,828]" pageId="243" pageNumber="244">
<emphasis bold="true" box="[199,328,765,791]" pageId="243" pageNumber="244">Etymology</emphasis>
