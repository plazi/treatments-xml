<quantity box="[1224,1320,222,249]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" metricValueMax="6.0" metricValueMin="5.0" pageId="9" pageNumber="382" unit="mm" value="5.5" valueMax="6.0" valueMin="5.0">5–6 mm</quantity>
