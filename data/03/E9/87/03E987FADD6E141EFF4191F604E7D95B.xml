<emphasis bold="true" box="[199,413,222,249]" pageId="9" pageNumber="382">Description. Male</emphasis>
(
<emphasis box="[429,466,223,248]" italics="true" pageId="9" pageNumber="382">N=</emphasis>
3): body length 
<quantity box="[644,767,223,249]" metricMagnitude="-2" metricUnit="m" metricValue="1.85" metricValueMax="1.9" metricValueMin="1.8" pageId="9" pageNumber="382" unit="mm" value="18.5" valueMax="19.0" valueMin="18.0">18–19 mm</quantity>
, wing length 
<quantity box="[919,1042,223,249]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" metricValueMax="2.0" metricValueMin="1.8" pageId="9" pageNumber="382" unit="mm" value="19.0" valueMax="20.0" valueMin="18.0">18–20 mm</quantity>
