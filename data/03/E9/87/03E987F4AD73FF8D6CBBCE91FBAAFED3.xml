<subSubSection box="[151,957,258,283]" pageId="10" pageNumber="364" type="materials_examined">
<paragraph blockId="10.[151,1437,258,535]" box="[151,957,258,283]" pageId="10" pageNumber="364">
<emphasis bold="true" box="[151,384,258,283]" pageId="10" pageNumber="364">Material examined.</emphasis>
AM W. 
<number box="[474,547,258,283]" pageId="10" pageNumber="364" value="44685.0">44685</number>
, MI QLD 
<number box="[664,723,259,283]" pageId="10" pageNumber="364" value="2410.0">2410</number>
