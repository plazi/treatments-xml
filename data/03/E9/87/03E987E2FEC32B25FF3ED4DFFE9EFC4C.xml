<subSubSection pageId="365" pageNumber="366" type="etymology">
<paragraph blockId="365.[151,1437,152,971]" pageId="365" pageNumber="366">
<emphasis bold="true" box="[199,328,909,935]" pageId="365" pageNumber="366">Etymology</emphasis>
