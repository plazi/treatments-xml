<taxonomicName baseAuthorityName="Le Cerf" baseAuthorityYear="1936" box="[1153,1364,1845,1869]" class="Insecta" family="Hepialidae" genus="Pharmacis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="348" phylum="Arthropoda" rank="species" species="bertrandi">
<emphasis box="[1153,1364,1845,1869]" italics="true" pageId="9" pageNumber="348">Pharmacis bertrandi</emphasis>
</taxonomicName>
M156, (e) 
<taxonomicName baseAuthorityName="Herrich-Schäffer" baseAuthorityYear="1856" box="[186,382,1881,1905]" class="Insecta" family="Hepialidae" genus="Sthenopis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="348" phylum="Arthropoda" rank="species" species="pretiosus">
<emphasis box="[186,382,1881,1905]" italics="true" pageId="9" pageNumber="348">Sthenopis pretiosus</emphasis>
