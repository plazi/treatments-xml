<taxonomicName authority="Olmi" authorityName="Olmi" authorityYear="1992" box="[1296,1436,1943,1965]" class="Insecta" family="Dryinidae" genus="Neodryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="564" pageNumber="565" phylum="Arthropoda" rank="species" species="osteni">
