<taxonomicName authority="Ceballos" authorityName="Ceballos" box="[1266,1436,1855,1877]" class="Insecta" family="Dryinidae" genus="Chalcogonatopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="564" pageNumber="565" phylum="Arthropoda" rank="species" species="gigas">
