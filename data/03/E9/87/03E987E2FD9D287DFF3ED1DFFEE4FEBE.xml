<taxonomicName authority="Olmi" authorityName="Olmi" box="[1286,1436,2002,2024]" class="Insecta" family="Dryinidae" genus="Deinodryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="564" pageNumber="565" phylum="Arthropoda" rank="species" species="tussaci">
