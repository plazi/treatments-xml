Figures 
<number box="[251,324,691,715]" pageId="4" pageNumber="39" value="29.0" valueMax="37.0" valueMin="21.0">21–37</number>
</figureCitation>
, Table 
<number box="[404,416,691,715]" pageId="4" pageNumber="39" value="1.0">1</number>
)
</paragraph>
<paragraph blockId="4.[151,976,762,784]" box="[151,976,762,784]" pageId="4" pageNumber="39">
