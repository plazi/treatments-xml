<collectingRegion box="[822,907,1660,1685]" country="Mexico" name="Mexico" pageId="13" pageNumber="324">Mexico</collectingRegion>
</collectingCountry>
to 
<collectingCountry box="[947,1036,1661,1684]" name="Panama" pageId="13" pageNumber="324">
<collectingRegion box="[947,1036,1661,1684]" country="Panama" name="Panama" pageId="13" pageNumber="324">Panama</collectingRegion>
</collectingCountry>
Bay, 
<collectingCountry box="[1104,1193,1661,1684]" name="Panama" pageId="13" pageNumber="324">
