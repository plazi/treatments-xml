<taxonomicName authority="d'Orbigny, 1846" authorityName="d'Orbigny" authorityYear="1846" box="[407,824,1876,1901]" class="Bivalvia" family="Arcidae" genus="Barbatia" kingdom="Animalia" order="Arcoida" pageId="13" pageNumber="324" phylum="Mollusca" rank="species" species="reeveana">
