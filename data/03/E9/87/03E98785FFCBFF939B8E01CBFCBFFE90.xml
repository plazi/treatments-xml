<number box="[342,357,2010,2036]" pageId="4" pageNumber="30" value="8.0">8</number>
(L 
<number box="[400,454,2010,2036]" pageId="4" pageNumber="30" value="0.48">0.48</number>
, 
<geoCoordinate box="[467,556,2010,2036]" direction="west" orientation="longitude" pageId="4" pageNumber="30" precision="555" value="-0.17">
W 
<number box="[503,556,2010,2036]" pageId="4" pageNumber="30" value="0.17">0.17</number>
</geoCoordinate>
) bifid apically, Y-shaped; sternite 
<number box="[961,976,2010,2036]" pageId="4" pageNumber="30" value="9.0">9</number>
(L 
<number box="[1019,1074,2010,2036]" pageId="4" pageNumber="30" value="0.77">0.77</number>
, 
<geoCoordinate box="[1085,1174,2010,2036]" direction="west" orientation="longitude" pageId="4" pageNumber="30" precision="555" value="-0.22">
