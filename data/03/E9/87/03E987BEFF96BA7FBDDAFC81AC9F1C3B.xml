<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[641,722,1062,1086]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="24" pageNumber="164">USNM</collectionCode>
