<collectionCode LSID="urn:lsid:biocol.org:col:14198" box="[151,211,1062,1086]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14198" name="The Manx Museum" pageId="24" pageNumber="164">DGS</collectionCode>
