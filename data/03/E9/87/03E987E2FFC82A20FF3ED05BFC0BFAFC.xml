<taxonomicName authority="Olmi, 1984" authorityName="Olmi" authorityYear="1984" box="[340,595,1722,1747]" class="Insecta" family="Dryinidae" genus="Anteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="agile">
