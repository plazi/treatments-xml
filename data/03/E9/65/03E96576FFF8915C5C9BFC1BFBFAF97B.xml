<paragraph blockId="12.[136,1415,998,1152]" box="[136,1415,1129,1152]" pageId="12" pageNumber="224">
<taxonomicName authority=": Speidel &amp; Mey, 1999 a" authorityName="Speidel &amp; Mey" authorityYear="1999" box="[136,564,1129,1151]" class="Insecta" family="Crambidae" genus="Eoophyla" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="224" phylum="Arthropoda" rank="species" species="gibbosalis">
