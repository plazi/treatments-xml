<emphasis bold="true" box="[705,769,1790,1815]" pageId="10" pageNumber="481">
<taxonomicNameLabel box="[705,769,1790,1815]" pageId="10" pageNumber="481" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName box="[825,1047,1790,1815]" class="Insecta" family="Gelechiidae" genus="Kiwaia" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="481" phylum="Arthropoda" rank="species" species="yadongensis" status="sp. n." subGenus="Empista">
