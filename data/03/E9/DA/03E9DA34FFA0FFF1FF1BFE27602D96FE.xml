<collectingRegion box="[488,563,1897,1923]" country="Ukraine" name="Rivne" pageId="2" pageNumber="108">Rovno</collectingRegion>
amber, 
<collectingCountry box="[652,746,1896,1922]" name="Ukraine" pageId="2" pageNumber="108">Ukraine</collectingCountry>
