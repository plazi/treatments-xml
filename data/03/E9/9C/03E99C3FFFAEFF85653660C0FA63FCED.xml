<taxonomicName authority="Leidy 1868" authorityName="Leidy" baseAuthorityName="Leidy" baseAuthorityYear="1868" box="[1743,2082,567,605]" class="Reptilia" family="Tyrannosauridae" genus="Aublysodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="0" pageNumber="1" phylum="Chordata" rank="genus">
