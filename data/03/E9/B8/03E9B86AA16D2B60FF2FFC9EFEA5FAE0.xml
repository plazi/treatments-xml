<collectingCountry box="[681,865,914,940]" name="Falkland Islands" pageId="10" pageNumber="11">Falkland Islands</collectingCountry>
/ 
<collectingCountry box="[874,1035,914,940]" name="Falkland Islands" pageId="10" pageNumber="11">Islas Malvinas</collectingCountry>
