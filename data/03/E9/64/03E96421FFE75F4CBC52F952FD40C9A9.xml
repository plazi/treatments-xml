<quantity metricMagnitude="-4" metricUnit="m" metricValue="2.25" metricValueMax="3.0" metricValueMin="1.5" pageId="2" pageNumber="31" unit="mm" value="0.22499999999999998" valueMax="0.3" valueMin="0.15">0.15- 0.3 mm</quantity>
