<emphasis box="[532,615,1528,1546]" italics="true" pageId="17" pageNumber="10">L. osawai</emphasis>
<bibRefCitation author="NAKANE T." box="[621,742,1527,1546]" pageId="17" pageNumber="10" pagination="40 - 42" refId="ref73132" refString="NAKANE T. 1963: New or little known Coleoptera from Japan and its adjacent regions. XXI. Fragmenta Coleopterologica 10: 40 - 42." type="journal article" year="1963">Nakane, 1963</bibRefCitation>
</taxonomicName>
; E – 
<taxonomicName authority="Hoshina, 2012" authorityName="Hoshina" authorityYear="2012" box="[786,886,1528,1546]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="10" phylum="Arthropoda" rank="species" species="yukihikoi" status="sp. nov.">
<emphasis box="[786,886,1528,1546]" italics="true" pageId="17" pageNumber="10">L. yukihikoi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[892,961,1527,1546]" pageId="17" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
; F – 
<taxonomicName authority="Nakane, 1963" authorityName="Nakane" authorityYear="1963" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="10" phylum="Arthropoda" rank="species" species="okawai">
<emphasis box="[1002,1087,1528,1546]" italics="true" pageId="17" pageNumber="10">L. okawai</emphasis>
<bibRefCitation author="NAKANE T." box="[112,233,1554,1573]" pageId="17" pageNumber="10" pagination="40 - 42" refId="ref73132" refString="NAKANE T. 1963: New or little known Coleoptera from Japan and its adjacent regions. XXI. Fragmenta Coleopterologica 10: 40 - 42." type="journal article" year="1963">Nakane, 1963</bibRefCitation>
</taxonomicName>
; G – 
<taxonomicName authority="Hoshina, 2012" authorityName="Hoshina" authorityYear="2012" box="[281,368,1555,1573]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="10" phylum="Arthropoda" rank="species" species="tanakai" status="sp. nov.">
<emphasis box="[281,368,1555,1573]" italics="true" pageId="17" pageNumber="10">L. tanakai</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[374,444,1554,1573]" pageId="17" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
; H, J, K – 
<taxonomicName authority="(Fairmaire, 1855)" baseAuthorityName="Fairmaire" baseAuthorityYear="1855" box="[537,775,1554,1573]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="10" phylum="Arthropoda" rank="species" species="lucens">
<emphasis box="[537,616,1555,1573]" italics="true" pageId="17" pageNumber="10">L. lucens</emphasis>
(
<bibRefCitation author="FAIRMAIRE L." box="[627,769,1554,1573]" pageId="17" pageNumber="10" pagination="75 - 77" refId="ref71838" refString="FAIRMAIRE L. 1855: Communique la note suivante. Bulletin de la Societe Entomologique de France 1855: 75 - 77." type="journal article" year="1855">Fairmaire, 1855</bibRefCitation>
)
</taxonomicName>
; I – 
<taxonomicName authority="Hoshina, 2012" authorityName="Hoshina" authorityYear="2012" box="[814,910,1555,1573]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="10" phylum="Arthropoda" rank="species" species="yoshidai" status="sp. nov.">
