<taxonomicName authority="Nakane, 1989" authorityName="Nakane" authorityYear="1989" box="[173,418,943,962]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="22" phylum="Arthropoda" rank="species" species="babai">
