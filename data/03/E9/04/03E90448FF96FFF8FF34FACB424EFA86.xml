<materialsCitation ID-GBIF-Occurrence="2242483117" collectingDate="1924-04" collectionCode="HAUST" collectorName="Menglun No." country="China" elevation="920" location="Yunnan" pageId="13" pageNumber="48" specimenCount="1" stateProvince="Yunnan" typeStatus="holotype">
