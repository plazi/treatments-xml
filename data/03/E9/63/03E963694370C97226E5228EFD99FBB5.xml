<quantity box="[1261,1424,983,1007]" metricMagnitude="3" metricUnit="m" metricValue="1.4" metricValueMax="1.8" metricValueMin="1.0" pageId="23" pageNumber="24" unit="m" value="1400.0" valueMax="1800.0" valueMin="1000.0">1000–1800 m</quantity>
