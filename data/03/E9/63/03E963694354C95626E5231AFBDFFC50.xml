<quantity box="[342,489,984,1009]" metricMagnitude="2" metricUnit="m" metricValue="8.85" metricValueMax="12.7" metricValueMin="5.0" pageId="51" pageNumber="52" unit="m" value="885.0" valueMax="1270.0" valueMin="500.0">500–1270 m</quantity>
