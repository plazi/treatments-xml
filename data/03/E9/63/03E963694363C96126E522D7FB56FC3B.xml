<quantity box="[682,843,897,922]" metricMagnitude="3" metricUnit="m" metricValue="1.8" metricValueMax="2.6" metricValueMin="1.0" pageId="4" pageNumber="5" unit="m" value="1800.0" valueMax="2600.0" valueMin="1000.0">1000–2600 m</quantity>
