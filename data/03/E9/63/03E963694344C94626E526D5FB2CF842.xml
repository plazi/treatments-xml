<quantity box="[659,805,1994,2019]" metricMagnitude="2" metricUnit="m" metricValue="8.75" metricValueMax="14.0" metricValueMin="3.5" pageId="35" pageNumber="36" unit="m" value="875.0" valueMax="1400.0" valueMin="350.0">350–1400 m</quantity>
