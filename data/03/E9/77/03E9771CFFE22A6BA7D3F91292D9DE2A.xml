<subSubSection pageId="18" pageNumber="473" type="discussion">
<paragraph blockId="18.[151,1436,1154,1287]" pageId="18" pageNumber="473">
<materialsCitation ID-GBIF-Occurrence="1440156660" county="Bechuan County" location="Jiangyou City" municipality="The" pageId="18" pageNumber="473" specimenCount="1" stateProvince="Sichuan">
<emphasis bold="true" box="[199,315,1154,1179]" pageId="18" pageNumber="473">Remarks.</emphasis>
