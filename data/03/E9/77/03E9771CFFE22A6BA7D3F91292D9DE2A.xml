<location LSID="urn:lsid:plazi:treatment:03E9771CFFE22A6BA7D3F91292D9DE2A:8E9F90D1FFE02A6BA51AFB1C928ADFCE" box="[606,662,1226,1251]" municipality="Golovatch" name="Only" pageId="18" pageNumber="473" stateProvince="Sichuan">Only</location>
