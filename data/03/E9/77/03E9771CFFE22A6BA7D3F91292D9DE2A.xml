<collectingCounty box="[923,1115,1154,1179]" pageId="18" pageNumber="473">Bechuan County</collectingCounty>
, 
<collectingRegion box="[1129,1223,1154,1179]" country="China" name="Sichuan" pageId="18" pageNumber="473">Sichuan</collectingRegion>
. 
<collectingMunicipality box="[1236,1280,1154,1179]" pageId="18" pageNumber="473">The</collectingMunicipality>
