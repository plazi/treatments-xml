<collectingCountry box="[309,402,990,1012]" name="Thailand" pageId="4" pageNumber="485">Thailand</collectingCountry>
, 
<collectingCountry box="[411,463,991,1012]" name="Laos" pageId="4" pageNumber="485">Laos</collectingCountry>
, and 
<collectingCountry box="[516,603,990,1012]" name="Vietnam" pageId="4" pageNumber="485">Vietnam</collectingCountry>
