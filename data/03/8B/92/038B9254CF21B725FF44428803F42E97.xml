<collectingCountry box="[859,976,1142,1164]" name="Philippines" pageId="14" pageNumber="1289">Philippines</collectingCountry>
, new for 
<collectingCountry box="[163,218,1172,1194]" name="India" pageId="14" pageNumber="1289">India</collectingCountry>
, 
<collectingCountry box="[228,277,1172,1193]" name="Laos" pageId="14" pageNumber="1289">Laos</collectingCountry>
and 
<collectingCountry box="[326,417,1172,1194]" name="Vietnam" pageId="14" pageNumber="1289">Vietnam</collectingCountry>
