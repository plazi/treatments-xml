<subSubSection box="[163,409,1236,1258]" pageId="14" pageNumber="1289" type="nomenclature">
<paragraph blockId="14.[163,409,1236,1258]" box="[163,409,1236,1258]" pageId="14" pageNumber="1289">
<heading box="[163,409,1236,1258]" fontSize="8" level="2" pageId="14" pageNumber="1289" reason="2">
<emphasis bold="true" box="[163,409,1236,1258]" pageId="14" pageNumber="1289">
<taxonomicName authority="Riedel, 2017" authorityName="Riedel" authorityYear="2017" box="[163,312,1236,1258]" class="Insecta" family="Ichneumonidae" genus="Aulojoppoides" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="1289" phylum="Arthropoda" rank="genus" status="nov.gen.">
<emphasis bold="true" box="[163,312,1236,1258]" italics="true" pageId="14" pageNumber="1289">Aulojoppoides</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[318,409,1236,1257]" pageId="14" pageNumber="1289" rank="genus">nov.gen.</taxonomicNameLabel>
</emphasis>
