<bibRefCitation author="Barbier, Y." box="[939,1092,871,897]" pageId="7" pageNumber="76" pagination="2017" refId="ref18727" refString="Barbier, Y. (2017) Fauna Europaea: Crabronidae. In: Mitroiu, M. - D. (2017) Fauna Europaea: Hymenoptera. Fauna Europaea version 2017.06. Available from: https: // fauna-eu. org (accessed 10 December 2019)" type="book chapter" year="2017">Barbier 2017</bibRefCitation>
); 
<collectingCountry box="[1111,1191,871,897]" name="Turkey" pageId="7" pageNumber="76">Turkey</collectingCountry>
(
<bibRefCitation author="de Beaumont, J." box="[1206,1424,871,898]" pageId="7" pageNumber="76" pagination="79 - 95" refId="ref18939" refString="de Beaumont, J. (1969) Sphecidae de Turquie (Hym.). Mitteilungen der Schweizerischen Entomologischen Gesellschaft, 42, 79 - 95." type="journal article" year="1969">de Beaumont 1969</bibRefCitation>
