<taxonomicName box="[433,737,1960,1986]" class="Magnoliopsida" family="Amaranthaceae" genus="Haloxylon" kingdom="Plantae" order="Caryophyllales" pageId="10" pageNumber="39" phylum="Tracheophyta" rank="species" species="ammodendron">
<emphasis box="[433,737,1960,1986]" italics="true" pageId="10" pageNumber="39">Haloxylon ammodendron</emphasis>
</taxonomicName>
, 
<collectingRegion box="[759,1074,1960,1986]" country="Iran" name="Khorasan-e Jonubi" pageId="10" pageNumber="39">South Khorasan Province</collectingRegion>
, 
<quantity box="[1093,1173,1960,1987]" metricMagnitude="4" metricUnit="m" metricValue="5.0" pageId="10" pageNumber="39" unit="km" value="50.0">50 km</quantity>
