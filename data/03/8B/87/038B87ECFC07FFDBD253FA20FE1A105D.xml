<taxonomicName authority="Mamaev, 1972" authorityName="Mamaev" authorityYear="1972" box="[859,1280,1922,1949]" class="Insecta" family="Cecidomyiidae" genus="Stefaniola" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="species" species="similata">
<emphasis box="[859,1087,1922,1948]" italics="true" pageId="10" pageNumber="39">Stefaniola similata</emphasis>
Mamaev, 1972
</taxonomicName>
(
<taxonomicName box="[1301,1395,1922,1948]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
: 
<taxonomicName box="[189,369,1960,1987]" class="Insecta" family="Cecidomyiidae" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="family">Cecidomyiidae</taxonomicName>
