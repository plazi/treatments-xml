<collectingCountry box="[360,428,1822,1848]" name="Brazil" pageId="10" pageNumber="396">Brazil</collectingCountry>
(
<collectingRegion box="[444,511,1822,1848]" country="Brazil" name="Bahia" pageId="10" pageNumber="396">Bahia</collectingRegion>
[new record], 
<collectingRegion box="[677,837,1822,1848]" country="Brazil" name="Espirito Santo" pageId="10" pageNumber="396">Espírito Santo</collectingRegion>
[new record], 
