<collectingCountry box="[566,736,963,987]" name="Nicaragua" pageId="7" pageNumber="6">NICARAGUA</collectingCountry>
- 
<collectingRegion box="[755,887,963,987]" country="Nicaragua" name="Matagalpa" pageId="7" pageNumber="6">Matagalpa</collectingRegion>
