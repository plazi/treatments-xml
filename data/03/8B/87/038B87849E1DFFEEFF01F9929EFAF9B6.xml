<subSubSection box="[151,455,1610,1637]" pageId="1" pageNumber="566" type="nomenclature">
<paragraph blockId="1.[151,455,1610,1637]" box="[151,455,1610,1637]" pageId="1" pageNumber="566">
<heading bold="true" box="[151,455,1610,1637]" fontSize="11" level="1" pageId="1" pageNumber="566" reason="1">
<emphasis bold="true" box="[151,455,1610,1637]" pageId="1" pageNumber="566">
Genus 
<taxonomicName box="[238,365,1610,1636]" class="Adenophorea" family="Tobrilidae" genus="Neotripyla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="1" pageNumber="566" phylum="Nematoda" rank="genus" status="gen. n.">
<emphasis bold="true" box="[238,365,1610,1636]" italics="true" pageId="1" pageNumber="566">Neotripyla</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[372,455,1611,1637]" pageId="1" pageNumber="566" rank="genus">gen. n.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
