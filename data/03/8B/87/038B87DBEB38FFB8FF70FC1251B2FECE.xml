<taxonomicName box="[344,789,1956,1978]" class="Arachnida" family="Uropodidae" genus="Rotundabaloghia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="522" phylum="Arthropoda" rank="species" species="javaensis" status="sp. nov." subGenus="Circobaloghia">
<emphasis box="[344,520,1956,1977]" italics="true" pageId="7" pageNumber="522">Rotundabaloghia</emphasis>
(
<emphasis box="[534,681,1956,1977]" italics="true" pageId="7" pageNumber="522">Circobaloghia</emphasis>
) 
<emphasis box="[695,789,1957,1978]" italics="true" pageId="7" pageNumber="522">javaensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[795,875,1957,1978]" pageId="7" pageNumber="522" rank="species">
