<materialsCitation ID-GBIF-Occurrence="3462298324" collectingDate="2002-03-05" collectingDateMax="2002-03-11" collectingDateMin="2002-03-05" collectionCode="T, MZSP" collectorName="P. Amarante" country="Brazil" county="Nova Iguacu" latitude="-22.574446" location="Biological Reserve" longLatPrecision="15" longitude="-43.436306" municipality="Reserva Biologica" pageId="8" pageNumber="555" specimenCount="1" specimenCount-male="1" stateProvince="Rio de Janeiro" typeStatus="holotype">
