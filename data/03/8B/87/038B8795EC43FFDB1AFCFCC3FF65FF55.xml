. 
<taxonomicName authority="Dusbabek &amp; Capek" authorityName="Dusbabek &amp; Capek" box="[357,840,1520,1543]" class="Arachnida" family="Ascidae" genus="Proctolaelaps" kingdom="Animalia" order="Mesostigmata" pageId="9" pageNumber="60" phylum="Arthropoda" rank="species" species="chalybura" status="sp. nov.">
<taxonomicName authority="Dusbabek" authorityName="Dusbabek" authorityYear="1923" box="[357,735,1520,1543]" class="Arachnida" family="Ascidae" genus="Proctolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="9" pageNumber="60" phylum="Arthropoda" rank="species" species="chalybura">
<taxonomicName authority="A.Berlese, 1923" authorityName="A.Berlese" authorityYear="1923" box="[357,621,1520,1543]" class="Arachnida" family="Ascidae" genus="Proctolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="9" pageNumber="60" phylum="Arthropoda" rank="species" species="chalybura">
<emphasis box="[357,621,1520,1543]" italics="true" pageId="9" pageNumber="60">
