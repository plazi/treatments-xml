</taxonomicName>
(
<number box="[533,607,1974,1998]" pageId="11" pageNumber="176" value="11.5" valueMax="13.0" valueMin="10.0">10–13</number>
), 
<taxonomicName box="[627,777,1974,1999]" class="Actinopterygii" family="Claroteidae" genus="Notoglanidium" kingdom="Animalia" order="Siluriformes" pageId="11" pageNumber="176" phylum="Chordata" rank="species" species="pembetadi">
