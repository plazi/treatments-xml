<number box="[792,866,1974,1998]" pageId="11" pageNumber="176" value="11.5" valueMax="13.0" valueMin="10.0">10–13</number>
), 
<taxonomicName box="[885,1006,1974,1999]" class="Actinopterygii" family="Claroteidae" genus="Notoglanidium" kingdom="Animalia" order="Siluriformes" pageId="11" pageNumber="176" phylum="Chordata" rank="species" species="thomasi">
