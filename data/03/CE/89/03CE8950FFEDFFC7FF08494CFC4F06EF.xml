: 
<number box="[1145,1198,1945,1969]" pageId="1" pageNumber="36" value="2.96">2.96</number>
, 
<number box="[1213,1266,1945,1969]" pageId="1" pageNumber="36" value="3.75">3.75</number>
:3.0, 
<number box="[1323,1376,1945,1969]" pageId="1" pageNumber="36" value="3.62">3.62</number>
: 
<number box="[1380,1432,1945,1969]" pageId="1" pageNumber="36" value="3.25">3.25</number>
, 
<number box="[151,203,1981,2005]" pageId="1" pageNumber="36" value="3.25">3.25</number>
: 
<number box="[206,259,1981,2005]" pageId="1" pageNumber="36" value="3.72">3.72</number>
, 
<number box="[270,306,1981,2005]" pageId="1" pageNumber="36" value="3.5">3.5</number>
: 
<number box="[310,365,1981,2005]" pageId="1" pageNumber="36" value="4.62">4.62</number>
, 
<number box="[376,429,1981,2005]" pageId="1" pageNumber="36" value="4.87">4.87</number>
:6.0, 
<number box="[482,534,1981,2005]" pageId="1" pageNumber="36" value="5.25">5.25</number>
: 
<number box="[538,591,1981,2005]" pageId="1" pageNumber="36" value="6.17">6.17</number>
, 
<number box="[602,655,1981,2005]" pageId="1" pageNumber="36" value="5.12">5.12</number>
: 
<number box="[658,710,1981,2005]" pageId="1" pageNumber="36" value="5.87">5.87</number>
, 
<number box="[722,776,1981,2005]" pageId="1" pageNumber="36" value="6.75">6.75</number>
: 
<number box="[779,817,1981,2005]" pageId="1" pageNumber="36" value="4.5">4.5</number>
.
</paragraph>
<caption box="[151,1436,1958,1981]" httpUri="https://zenodo.org/record/282777/files/figure.png" pageId="2" pageNumber="37" targetBox="[156,1427,204,1957]" targetPageId="2">
<paragraph blockId="2.[151,1436,1958,1981]" box="[151,1436,1958,1981]" pageId="2" pageNumber="37">
<emphasis bold="true" box="[151,314,1958,1981]" pageId="2" pageNumber="37">FIGURES 1–8.</emphasis>
<taxonomicName box="[321,457,1958,1980]" class="Insecta" family="Scelionidae" genus="Psix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="37" phylum="Arthropoda" rank="species" species="sunithae" status="sp. nov.">
