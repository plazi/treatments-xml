<emphasis bold="true" box="[199,345,1564,1589]" pageId="8" pageNumber="534">Description.</emphasis>
Live SpecimenS 
<quantity box="[545,670,1564,1588]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="8" pageNumber="534" unit="mm" value="15.0" valueMax="20.0" valueMin="10.0">10–20 mm</quantity>
