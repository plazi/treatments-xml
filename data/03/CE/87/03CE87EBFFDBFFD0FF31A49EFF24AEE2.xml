<number box="[337,372,860,884]" pageId="12" pageNumber="13" value="1.5">1,5</number>
and 
<quantity box="[432,492,860,884]" metricMagnitude="1" metricUnit="m" metricValue="7.2" pageId="12" pageNumber="13" unit="m" value="72.0">72 m</quantity>
(
