<collectingCountry box="[576,632,679,703]" name="United States of America" pageId="12" pageNumber="13">USA</collectingCountry>
(Florida), 
<collectingCountry box="[762,872,679,703]" name="Colombia" pageId="12" pageNumber="13">Colombia</collectingCountry>
(Cape de La Vela), 
<collectingCountry box="[1121,1234,679,703]" name="Dominica" pageId="12" pageNumber="13">Dominica</collectingCountry>
, 
<collectingCountry box="[1250,1309,679,703]" name="Cuba" pageId="12" pageNumber="13">Cuba</collectingCountry>
(Batabanó Gulf), 
<collectingCountry box="[228,296,716,740]" name="Brazil" pageId="12" pageNumber="13">Brazil</collectingCountry>
(Maranhão, Ceará and Pernambuco) (
