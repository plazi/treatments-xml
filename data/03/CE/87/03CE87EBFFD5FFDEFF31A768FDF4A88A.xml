<emphasis box="[271,511,1460,1484]" italics="true" pageId="2" pageNumber="3">Bartholomea anulata</emphasis>
</taxonomicName>
and 
<taxonomicName box="[570,811,1460,1484]" class="Anthozoa" family="Actiniidae" genus="Condylactis" kingdom="Animalia" order="Actiniaria" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="gigantea">
<emphasis box="[570,811,1460,1484]" italics="true" pageId="2" pageNumber="3">Condylactis gigantea</emphasis>
