<number box="[580,625,1545,1570]" pageId="5" pageNumber="340" value="255.0">255</number>
, L/H, ratio L/H: P-1, 35/ 
<number box="[908,936,1545,1570]" pageId="5" pageNumber="340" value="21.0">21</number>
, 
<number box="[948,986,1545,1570]" pageId="5" pageNumber="340" value="1.6">1.6</number>
; P-2, 57/ 
<number box="[1089,1118,1545,1570]" pageId="5" pageNumber="340" value="48.0">48</number>
, 
<number box="[1130,1182,1546,1570]" pageId="5" pageNumber="340" value="1.19">1.19</number>
; P-3, 42/ 
<number box="[1286,1314,1546,1570]" pageId="5" pageNumber="340" value="30.0">30</number>
, 
<number box="[1326,1378,1546,1570]" pageId="5" pageNumber="340" value="1.34">1.34</number>
; P-4, 92/ 
<number box="[189,218,1582,1606]" pageId="5" pageNumber="340" value="32.0">32</number>
, 
<number box="[229,267,1582,1606]" pageId="5" pageNumber="340" value="2.8">2.8</number>
; P-5, 29/ 
<number box="[370,395,1582,1606]" pageId="5" pageNumber="340" value="11.0">11</number>
, 
<number box="[407,445,1581,1606]" pageId="5" pageNumber="340" value="2.5">2.5</number>
; P- 
<number box="[486,496,1582,1606]" pageId="5" pageNumber="340" value="1.0">1</number>
