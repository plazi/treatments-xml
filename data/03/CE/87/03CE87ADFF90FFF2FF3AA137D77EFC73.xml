<number box="[260,290,1580,1604]" pageId="11" pageNumber="12" value="10.0">10</number>
, 
<number box="[301,445,1580,1604]" pageId="11" pageNumber="12" value="0.0475" valueMax="0.05" valueMin="0.045">0.045–0.050</number>
