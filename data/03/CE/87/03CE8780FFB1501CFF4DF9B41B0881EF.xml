<paragraph blockId="9.[151,1437,151,1184]" pageId="9" pageNumber="510">
<emphasis bold="true" box="[199,308,908,933]" pageId="9" pageNumber="510">Remarks</emphasis>
. 
<taxonomicName box="[323,536,908,933]" class="Insecta" family="Bethylidae" genus="Caloapenesia" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="510" phylum="Arthropoda" rank="species" species="edas" status="sp. nov.">
<emphasis box="[323,536,908,933]" italics="true" pageId="9" pageNumber="510">Caloapenesia edas</emphasis>
</taxonomicName>
<emphasis bold="true" box="[544,634,908,933]" pageId="9" pageNumber="510">
<taxonomicNameLabel box="[544,634,908,933]" pageId="9" pageNumber="510" rank="species">sp. nov.</taxonomicNameLabel>
