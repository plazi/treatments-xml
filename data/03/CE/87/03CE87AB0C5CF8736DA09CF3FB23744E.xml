<taxonomicName authorityName="Skelley" authorityYear="2009" baseAuthorityName="Olivier" baseAuthorityYear="1807" box="[344,606,387,411]" class="Insecta" family="Erotylidae" genus="Notaepytus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="77" pageNumber="76" phylum="Arthropoda" rank="species" species="modestus">
