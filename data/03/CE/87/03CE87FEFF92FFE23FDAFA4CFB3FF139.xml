<subSubSection pageId="8" pageNumber="9" type="materials_examined">
<paragraph blockId="8.[151,1436,1654,2003]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[151,377,1654,1679]" pageId="8" pageNumber="9">Material examined</emphasis>
. 
<typeStatus box="[391,493,1655,1679]" pageId="8" pageNumber="9">
<emphasis box="[391,493,1655,1679]" italics="true" pageId="8" pageNumber="9">Holotype</emphasis>
</typeStatus>
. Colony with floatoblasts, collected 
<date box="[904,1106,1655,1679]" pageId="8" pageNumber="9" value="2007-11-02">
<number box="[904,918,1655,1679]" pageId="8" pageNumber="9" value="2.0">2</number>
