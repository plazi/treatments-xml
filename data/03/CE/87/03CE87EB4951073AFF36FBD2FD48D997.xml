<paragraph blockId="35.[151,1416,1696,1755]" box="[151,1416,1731,1755]" pageId="35" pageNumber="36">
<emphasis bold="true" box="[151,294,1731,1755]" pageId="35" pageNumber="36">FIGURE 33.</emphasis>
<taxonomicName authority="Published, 2007" authorityName="Published" authorityYear="2007" box="[301,626,1731,1754]" class="Insecta" family="Chironomidae" genus="Chasmatonotoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="deharvengi" status="n. sp.">
<emphasis box="[301,626,1731,1754]" italics="true" pageId="35" pageNumber="36">Chasmatonotoides deharvengi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[633,692,1731,1754]" pageId="35" pageNumber="36" rank="species">n. sp.</taxonomicNameLabel>
, 
