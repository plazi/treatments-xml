<taxonomicName class="Mammalia" family="Cricetidae" genus="Peromyscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="7" pageNumber="502" phylum="Chordata" rank="species" species="yucatanicus">
