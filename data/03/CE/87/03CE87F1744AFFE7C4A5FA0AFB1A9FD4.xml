<taxonomicName authority="Loew, 1843" authorityName="Loew" authorityYear="1843" box="[1017,1232,1823,1845]" class="Agaricomycetes" family="Paxillaceae" genus="Melanogaster" kingdom="Fungi" order="Boletales" pageId="5" pageNumber="22" phylum="Basidiomycota" rank="species" species="hirtella">
