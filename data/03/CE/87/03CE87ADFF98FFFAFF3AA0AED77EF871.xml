<number box="[237,311,1725,1749]" pageId="3" pageNumber="4" value="32.5" valueMax="34.0" valueMin="31.0">31–34</number>
; VII, 
<number box="[375,448,1725,1749]" pageId="3" pageNumber="4" value="13.5" valueMax="15.0" valueMin="12.0">12–15</number>
