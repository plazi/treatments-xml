<number box="[1138,1254,1725,1749]" pageId="3" pageNumber="4" value="0.47" valueMax="0.49" valueMin="0.45">0.45–0.49</number>
; HL, 
<number box="[1316,1432,1725,1749]" pageId="3" pageNumber="4" value="0.32" valueMax="0.33" valueMin="0.31">0.31–0.33</number>
; PW, 
<number box="[204,257,1761,1785]" pageId="3" pageNumber="4" value="0.28">0.28</number>
