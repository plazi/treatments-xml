<collectingCountry box="[456,573,1885,1911]" name="Indonesia" pageId="5" pageNumber="24">Indonesia</collectingCountry>
, Java, Batoerrad, (
<typeStatus box="[792,899,1885,1911]" pageId="5" pageNumber="24">holotype</typeStatus>
, 
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[912,1002,1885,1911]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="5" pageNumber="24">USNM</collectionCode>
); 
<collectingCountry box="[1023,1086,1885,1911]" name="Papua New Guinea" pageId="5" pageNumber="24">PNG</collectingCountry>
