<subSubSection pageId="16" pageNumber="517" type="description">
<paragraph blockId="16.[151,1436,151,392]" pageId="16" pageNumber="517">
<emphasis bold="true" box="[199,309,188,213]" pageId="16" pageNumber="517">Variation</emphasis>
