<collectingCountry box="[833,973,150,176]" name="Afghanistan" pageId="12" pageNumber="481">Afghanistan</collectingCountry>
, 
<collectingCountry box="[984,1042,150,176]" name="India" pageId="12" pageNumber="481">India</collectingCountry>
(Kashmir), 
<collectingCountry box="[1177,1290,150,176]" name="Tajikistan" pageId="12" pageNumber="481">Tajikistan</collectingCountry>
, 
<collectingCountry box="[1301,1432,151,177]" name="Kyrgyzstan" pageId="12" pageNumber="481">Kyrgyzstan</collectingCountry>
, 
<collectingCountry box="[151,280,186,212]" name="Uzbekistan" pageId="12" pageNumber="481">Uzbekistan</collectingCountry>
, 
<collectingCountry box="[291,425,186,212]" name="Kazakhstan" pageId="12" pageNumber="481">Kazakhstan</collectingCountry>
, 
<collectingCountry box="[436,506,186,212]" name="China" pageId="12" pageNumber="481">China</collectingCountry>
, 
<collectingCountry box="[517,629,186,212]" name="Mongolia" pageId="12" pageNumber="481">Mongolia</collectingCountry>
