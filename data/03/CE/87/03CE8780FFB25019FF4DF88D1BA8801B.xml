and 
<taxonomicName box="[913,989,1088,1112]" class="Insecta" family="Bethylidae" genus="Caloapenesia" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="513" phylum="Arthropoda" rank="species" species="ana" status="sp. nov.">
