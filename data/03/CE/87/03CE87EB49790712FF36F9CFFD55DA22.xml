<emphasis box="[389,554,724,749]" italics="true" pageId="11" pageNumber="12">Macropelopia</emphasis>
Thienemann, 1916
</taxonomicName>
, 
<taxonomicName authority="Kieffer, 1909" authorityName="Kieffer" authorityYear="1909" box="[794,1137,723,749]" class="Insecta" family="Chironomidae" genus="Psectrotanypus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
<emphasis box="[794,974,724,749]" italics="true" pageId="11" pageNumber="12">Psectrotanypus</emphasis>
