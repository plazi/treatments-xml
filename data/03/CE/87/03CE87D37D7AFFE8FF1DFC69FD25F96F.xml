<paragraph blockId="1.[151,459,975,1001]" box="[151,459,975,1001]" pageId="1" pageNumber="32">
<heading bold="true" box="[151,459,975,1001]" fontSize="11" level="1" pageId="1" pageNumber="32" reason="1">
<emphasis bold="true" box="[151,459,975,1001]" pageId="1" pageNumber="32">
<taxonomicName box="[151,322,975,1001]" class="Arachnida" family="Cranaidae" genus="Zannicranaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="1" pageNumber="32" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[151,322,975,1001]" italics="true" pageId="1" pageNumber="32">Zannicranaus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[329,459,975,1001]" pageId="1" pageNumber="32" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
</heading>
