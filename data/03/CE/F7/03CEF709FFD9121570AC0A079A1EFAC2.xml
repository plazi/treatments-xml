<number box="[1404,1415,1079,1103]" pageId="8" pageNumber="53" value="1.0">1</number>
)- 
<number box="[151,169,1116,1140]" pageId="8" pageNumber="53" value="2.0">2</number>
(
<number box="[178,189,1116,1140]" pageId="8" pageNumber="53" value="1.0">1</number>
