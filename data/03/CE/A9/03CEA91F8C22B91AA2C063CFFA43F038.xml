<taxonomicNameLabel box="[1376,1435,215,239]" pageId="6" pageNumber="7" rank="species">
<emphasis bold="true" box="[1376,1435,215,239]" pageId="6" pageNumber="7">sp. n.</emphasis>
</taxonomicNameLabel>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="6" pageNumber="7">
<paragraph blockId="6.[151,1437,152,1391]" pageId="6" pageNumber="7">
<keyLead pageId="6" pageNumber="7">
<number box="[151,165,248,271]" pageId="6" pageNumber="7" value="6.0">6</number>
(
<number box="[173,186,248,271]" pageId="6" pageNumber="7" value="5.0">5</number>
