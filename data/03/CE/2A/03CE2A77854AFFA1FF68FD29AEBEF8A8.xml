<number box="[1096,1141,1899,1925]" pageId="9" pageNumber="29" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
<number box="[1159,1175,1899,1925]" pageId="9" pageNumber="29" value="2.0">2</number>
], II (
