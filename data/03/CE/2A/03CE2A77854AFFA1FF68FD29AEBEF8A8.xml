<collectingCountry box="[560,642,1461,1487]" name="Russia" pageId="11" pageNumber="31">Russia</collectingCountry>
; two 
<typeStatus box="[705,817,1461,1487]" pageId="11" pageNumber="31">paratypes</typeStatus>
