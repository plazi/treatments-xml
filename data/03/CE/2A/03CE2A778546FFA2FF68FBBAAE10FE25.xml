– 
<number box="[1041,1072,1462,1488]" pageId="7" pageNumber="27" value="20.0">20</number>
</date>
) [
<number box="[1096,1141,1462,1488]" pageId="7" pageNumber="27" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
<number box="[1159,1175,1462,1488]" pageId="7" pageNumber="27" value="2.0">2</number>
], II (
<date box="[1232,1381,1462,1488]" pageId="7" pageNumber="27" value="1915-03-01" valueMax="1915-04-04" valueMin="1915-03-01">
<number box="[1232,1274,1462,1488]" pageId="7" pageNumber="27" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
– 
<number box="[1290,1336,1462,1488]" pageId="7" pageNumber="27" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
– 
<number box="[1353,1381,1462,1488]" pageId="7" pageNumber="27" value="15.0">15</number>
