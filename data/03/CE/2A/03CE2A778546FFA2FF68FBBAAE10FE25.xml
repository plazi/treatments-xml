– 
<number box="[151,200,1499,1525]" pageId="7" pageNumber="27" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
], III (
<date box="[271,424,1499,1525]" pageId="7" pageNumber="27" value="1915-01-01" valueMax="1915-03-02" valueMin="1915-01-01">
<number box="[271,315,1499,1525]" pageId="7" pageNumber="27" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
