<emphasis box="[403,565,1490,1512]" italics="true" pageId="9" pageNumber="404">
<taxonomicName box="[403,527,1490,1512]" class="Insecta" family="Peltoperlidae" genus="Cryptoperla" higherTaxonomySource="CoL" kingdom="Animalia" order="Plecoptera" pageId="9" pageNumber="404" phylum="Arthropoda" rank="genus">Cryptoperla</taxonomicName>
