<taxonomicName authorityName="Badonnel" authorityYear="1962" box="[321,444,1609,1633]" class="Insecta" family="Prionoglarididae" genus="Prionoglaris" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="228" phylum="Arthropoda" rank="species" species="lindbergi">
