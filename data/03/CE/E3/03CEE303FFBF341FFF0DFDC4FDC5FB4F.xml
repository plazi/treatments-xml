<subSubSection pageId="8" pageNumber="557" type="description">
<paragraph blockId="8.[136,1058,1365,1469]" pageId="8" pageNumber="557">
<taxonomicName authorityName="Li &amp; Yang" authorityYear="2013" box="[136,391,1396,1418]" class="Insecta" family="Lauxaniidae" genus="Homoneura" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="557" phylum="Arthropoda" rank="species" species="semiannulata" status="sp. n.">
