<collectionCode LSID="urn:lsid:biocol.org:col:34925" box="[593,683,596,622]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="18" pageNumber="19">AMNH</collectionCode>
