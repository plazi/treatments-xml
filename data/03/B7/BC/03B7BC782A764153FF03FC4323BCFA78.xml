</paragraph>
</subSubSection>
<subSubSection pageId="5" pageNumber="50" type="materials_examined">
<paragraph blockId="5.[132,774,1223,1299]" pageId="5" pageNumber="50">
<collectionCode box="[132,284,1224,1247]" pageId="5" pageNumber="50">TYPE LOCALITY</collectionCode>
