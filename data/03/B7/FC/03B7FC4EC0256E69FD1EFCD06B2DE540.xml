<specimenCount box="[928,1047,908,932]" pageId="24" pageNumber="349" type="generic">4 specimens</specimenCount>
(
<collectionCode box="[1061,1145,908,932]" pageId="24" pageNumber="349">GHS-W</collectionCode>
1099, 
<collectionCode box="[678,766,934,958]" pageId="24" pageNumber="349">GHS-W</collectionCode>
