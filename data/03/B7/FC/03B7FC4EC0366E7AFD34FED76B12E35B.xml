(
<collectionCode box="[1012,1092,401,425]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="11" pageNumber="336">MNHN</collectionCode>
