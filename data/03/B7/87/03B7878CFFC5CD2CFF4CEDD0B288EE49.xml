<subSubSection box="[151,466,1445,1471]" pageId="18" pageNumber="19" type="nomenclature">
<paragraph blockId="18.[151,466,1445,1471]" box="[151,466,1445,1471]" pageId="18" pageNumber="19">
<heading bold="true" box="[151,466,1445,1471]" fontSize="11" level="1" pageId="18" pageNumber="19" reason="1">
<emphasis bold="true" box="[151,466,1445,1471]" pageId="18" pageNumber="19">
Genus 
<taxonomicName box="[238,349,1445,1471]" class="Diplopoda" family="Anthroleucosomatidae" genus="Alpinella" kingdom="Animalia" order="Chordeumatida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[238,349,1445,1471]" italics="true" pageId="18" pageNumber="19">Alpinella</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[356,466,1445,1471]" pageId="18" pageNumber="19" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
