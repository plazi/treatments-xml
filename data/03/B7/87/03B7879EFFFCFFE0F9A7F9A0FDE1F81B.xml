</taxonomicName>
, 
<taxonomicName authority="Sakai, 1938" authorityName="Sakai" authorityYear="1938" class="Malacostraca" family="Inachidae" genus="Cyrtomaia" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="23" phylum="Arthropoda" rank="species" species="intermedia">
