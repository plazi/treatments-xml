Sakai, 
<number box="[230,291,912,938]" pageId="6" pageNumber="23" value="1938.0">1938</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Guinot, 1985" authorityName="Guinot" authorityYear="1985" box="[302,574,912,938]" class="Malacostraca" family="Inachidae" genus="Cyrtomaia" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="23" phylum="Arthropoda" rank="species" species="guillei">
