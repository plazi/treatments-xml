NE, 
<number box="[202,329,151,175]" pageId="10" pageNumber="539" value="9600.0" valueMax="9700.0" valueMin="9500.0">9500-9700</number>
', 
<date box="[342,491,151,176]" pageId="10" pageNumber="539" value="2006-08-24">
<number box="[342,370,151,175]" pageId="10" pageNumber="539" value="24.0">24</number>
Aug 
<number box="[432,491,151,175]" pageId="10" pageNumber="539" value="2006.0">2006</number>
</date>
, TL Griswold; 
<number box="[661,675,151,175]" pageId="10" pageNumber="539" value="1.0">1</number>
Ƥ, same except OJ Messinger. 
<typeStatus box="[1028,1132,151,175]" pageId="10" pageNumber="539">Holotype</typeStatus>
and 
<typeStatus box="[1186,1293,152,176]" pageId="10" pageNumber="539">paratypes</typeStatus>
