– 
<specimenCode box="[803,883,439,463]" collectionCode="I" pageId="23" pageNumber="24">III 323</specimenCode>
, 
<specimenCode box="[896,978,439,463]" collectionCode="I" pageId="23" pageNumber="24">IV 322</specimenCode>
; Patella 
<collectionCode box="[1074,1083,440,463]" pageId="23" pageNumber="24">I</collectionCode>
–II 000, III–IV 001; Tibia 
<date pageId="23" pageNumber="24" value="2024-01" valueMax="2024-04-31" valueMin="2024-01-00">
<collectingDate pageId="23" pageNumber="24" value="2024-01" valueMax="2024-04-31" valueMin="2024-01-00">I–IV 2024</collectingDate>
