<collectingCountry box="[870,937,1573,1597]" name="China" pageId="8" pageNumber="9">China</collectingCountry>
(Liaoning Province), 
<collectingCountry box="[1188,1255,1574,1597]" name="Japan" pageId="8" pageNumber="9">Japan</collectingCountry>
, the 
<collectingCountry box="[1310,1436,1573,1597]" name="Philippines" pageId="8" pageNumber="9">Philippines</collectingCountry>
(Luzon) and 
<collectingCountry box="[296,365,1611,1634]" name="South Korea" pageId="8" pageNumber="9">Korea</collectingCountry>
(
<bibRefCitation author="Brehm" box="[379,523,1610,1634]" pageId="8" pageNumber="9" refString="Brehm, V. (1936) Ostrakoden aus Mandschuko. Annotationes Zoologicae Japonenses, 15, 374 - 377." type="journal article" year="1936">
