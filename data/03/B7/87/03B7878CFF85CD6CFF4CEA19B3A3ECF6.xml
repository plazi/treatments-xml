<collectingRegion box="[480,573,1266,1291]" country="United States of America" name="Georgia" pageId="82" pageNumber="83">
<collectingCountry box="[480,573,1266,1291]" name="Georgia" pageId="82" pageNumber="83">Georgia</collectingCountry>
</collectingRegion>
, 
<collectingCountry box="[584,659,1266,1291]" name="Russia" pageId="82" pageNumber="83">Russia</collectingCountry>
(
<figureCitation box="[674,770,1266,1291]" captionStart="FIGURE 167" captionStartId="196.[151,250,2007,2029]" captionTargetBox="[188,1398,1136,1986]" captionTargetId="figure@196.[188,1398,1136,1986]" captionTargetPageId="196" captionText="FIGURE 167. Distribution of the Caucasian Anthroleucosomatidae: the Caucaseuma complex." httpUri="https://zenodo.org/record/272916/files/figure.png" pageId="82" pageNumber="83">Fig. 167</figureCitation>
