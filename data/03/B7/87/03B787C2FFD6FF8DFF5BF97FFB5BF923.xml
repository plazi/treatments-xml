<collectingRegion box="[151,275,1677,1702]" country="Guatemala" name="Guatemala" pageId="6" pageNumber="7">Guatemala</collectingRegion>
</collectingCountry>
, 
<collectingCountry box="[287,375,1677,1702]" name="Mexico" pageId="6" pageNumber="7">
<collectingRegion box="[287,375,1677,1702]" country="Mexico" name="Mexico" pageId="6" pageNumber="7">Mexico</collectingRegion>
</collectingCountry>
, 
<collectingCountry box="[388,477,1678,1701]" name="Panama" pageId="6" pageNumber="7">
<collectingRegion box="[388,477,1678,1701]" country="Panama" name="Panama" pageId="6" pageNumber="7">Panama</collectingRegion>
</collectingCountry>
(
<bibRefCitation box="[493,662,1677,1702]" pageId="6" pageNumber="7" refId="ref51260" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i - vi, 1 - 492." type="book chapter">Metcalf 1963a</bibRefCitation>
; Duffels &amp; van der 
<bibRefCitation box="[889,1012,1677,1702]" pageId="6" pageNumber="7" refId="ref50781" refString="Duffels, J. P. &amp; van der Laan, P. A. (1985) Catalogue of the Cicadoidea (Homoptera, Auchenorhyncha) 1956 - 1980. In: Series Entomologica. Vol. 34. Dr. W. Junk Publishers, Dordrect, pp. 1 - 414." type="book chapter">Laan 1985</bibRefCitation>
