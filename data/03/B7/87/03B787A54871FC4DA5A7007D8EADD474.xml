<quantity box="[641,734,1599,1622]" metricMagnitude="-2" metricUnit="m" metricValue="1.75" pageId="5" pageNumber="394" unit="mm" value="17.5">17.5 mm</quantity>
in 
<taxonomicName authorityName="Seidel &amp; Arriaga-Varela &amp; Sousa" authorityYear="2018" box="[151,283,1631,1654]" class="Insecta" family="Cetoniidae" genus="Archedinus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="394" phylum="Arthropoda" rank="species" species="antoshkai">
<emphasis box="[151,283,1631,1654]" italics="true" pageId="5" pageNumber="394">A. antoshkai</emphasis>
</taxonomicName>
versus 
<quantity box="[361,474,1631,1654]" metricMagnitude="-2" metricUnit="m" metricValue="2.6" metricValueMax="2.7" metricValueMin="2.5" pageId="5" pageNumber="394" unit="mm" value="26.0" valueMax="27.0" valueMin="25.0">25–27 mm</quantity>
