<taxonomicName authority="Gorochov 2014" authorityName="Gorochov" authorityYear="2014" class="Insecta" family="Tettigoniidae" genus="Pseudoteratura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="588" phylum="Arthropoda" rank="species" species="spinea">
