<emphasis bold="true" box="[650,745,1558,1584]" pageId="1" pageNumber="58">
<taxonomicNameLabel box="[650,745,1558,1584]" pageId="1" pageNumber="58" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
belongs to the 
<taxonomicName box="[929,1005,1558,1583]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="1" pageNumber="58" phylum="Annelida" rank="species" species="zebrus">
<emphasis box="[929,1005,1558,1583]" italics="true" pageId="1" pageNumber="58">zebrus</emphasis>
