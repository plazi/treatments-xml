<taxonomicName authority="Benth." authorityName="Benth." box="[539,858,351,378]" class="Magnoliopsida" family="Fabaceae" genus="Ormosia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="9" pageNumber="358" phylum="Tracheophyta" rank="species" species="pachycarpa">
