, 
<specimenCode box="[889,973,1665,1689]" collectionCode="I" pageId="65" pageNumber="66">IV 322</specimenCode>
</materialsCitation>
; 
<materialsCitation box="[987,1178,1665,1689]" collectionCode="I" pageId="65" pageNumber="66" specimenCode="IV 001" specimenCount="1">
Patella 
<collectionCode box="[1072,1081,1666,1689]" pageId="65" pageNumber="66">I</collectionCode>
– 
<specimenCode box="[1095,1178,1665,1689]" collectionCode="I" pageId="65" pageNumber="66">IV 001</specimenCode>
</materialsCitation>
/101; Tibia 
<date box="[1310,1427,1665,1689]" pageId="65" pageNumber="66" value="2124-01" valueMax="2124-04-31" valueMin="2124-01-00">I–IV 2124</date>
/ 2224; Metatarsus 
<date box="[351,469,1701,1725]" pageId="65" pageNumber="66" value="2024-01" valueMax="2024-03-31" valueMin="2024-01-00">I–III 2024</date>
