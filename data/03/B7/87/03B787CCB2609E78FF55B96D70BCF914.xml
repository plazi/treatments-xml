<collectingCountry box="[567,623,1582,1606]" name="United States of America" pageId="9" pageNumber="10">USA</collectingCountry>
(Florida) and 
<collectingCountry box="[790,858,1581,1606]" name="Brazil" pageId="9" pageNumber="10">Brazil</collectingCountry>
(Ceará, Pernambuco, 
<taxonomicName box="[1113,1181,1581,1606]" class="Malacostraca" family="Hippolytidae" genus="Lysmata" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="49" phylum="Arthropoda" rank="species" species="bahia">Bahia</taxonomicName>
