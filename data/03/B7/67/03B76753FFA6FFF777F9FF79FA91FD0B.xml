<paragraph blockId="19.[151,1435,222,275]" pageId="19" pageNumber="20">
<taxonomicName box="[151,356,222,244]" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="esarata">
