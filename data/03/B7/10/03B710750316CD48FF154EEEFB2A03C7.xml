<paragraph blockId="1.[151,674,151,244]" pageId="1" pageNumber="63">
[new Japanese name: Shinkai-sanngo-gani-zoku] 
<emphasis bold="true" box="[151,291,220,244]" pageId="1" pageNumber="63">
<typeStatus box="[151,207,220,244]" pageId="1" pageNumber="63">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName box="[305,583,221,244]" class="Malacostraca" family="Trapeziidae" genus="Hexagonaloides" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="63" phylum="Arthropoda" rank="species" species="bathyalis" status="n. sp.">
<emphasis box="[305,583,221,244]" italics="true" pageId="1" pageNumber="63">Hexagonaloides bathyalis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[589,649,220,244]" pageId="1" pageNumber="63" rank="species">
<emphasis bold="true" box="[589,649,220,244]" pageId="1" pageNumber="63">n. sp.</emphasis>
