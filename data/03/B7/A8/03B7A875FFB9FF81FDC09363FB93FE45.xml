<taxonomicName baseAuthorityName="Kirby" baseAuthorityYear="1802" box="[435,557,339,365]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="villosulum">
