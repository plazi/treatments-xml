<number box="[151,249,1605,1629]" pageId="24" pageNumber="25" value="260.0" valueMax="380.0" valueMin="140.0">140–380</number>
μm; 
<taxonomicName authority="Burton, 1930" authorityName="Burton" authorityYear="1930" box="[308,611,1605,1629]" class="Demospongiae" family="Hymedesmiidae" genus="Hymedesmia" kingdom="Animalia" order="Poecilosclerida" pageId="24" pageNumber="25" phylum="Porifera" rank="species" species="dendy" subGenus="Stylopus">
<emphasis box="[308,457,1605,1629]" italics="true" pageId="24" pageNumber="25">H. (S.) dendy</emphasis>
<bibRefCitation author="Burton" box="[464,611,1605,1629]" pageId="24" pageNumber="25" refString="Burton, M. (1930) Additions to the Sponge Fauna at Plymouth. Journal of the Marine Biological Association of the United Kingdom, 16 (2), 489 - 507. http: // dx. doi. org / 10.1017 / S 0025315400072891" type="journal article" year="1930">
Burton, 
<number box="[555,611,1605,1629]" pageId="24" pageNumber="25" value="1930.0">1930</number>
</bibRefCitation>
</taxonomicName>
has tornotes of 
<number box="[792,834,1605,1629]" pageId="24" pageNumber="25" value="700.0">700</number>
<date box="[841,890,1605,1629]" pageId="24" pageNumber="25" value="1916-10">
x 
<number box="[862,890,1605,1629]" pageId="24" pageNumber="25" value="16.0">16</number>
</date>
μm and tylostyles of 
<quantity box="[1134,1200,1605,1629]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="24" pageNumber="25" unit="mm" value="2.0">2 mm</quantity>
and acanthostyles of 
<number box="[151,249,1641,1665]" pageId="24" pageNumber="25" value="180.0" valueMax="240.0" valueMin="120.0">120–240</number>
μm; 
<taxonomicName authority="Topsent, 1928" authorityName="Topsent" authorityYear="1928" box="[309,647,1641,1665]" class="Demospongiae" family="Hymedesmiidae" genus="Hymedesmia" kingdom="Animalia" order="Poecilosclerida" pageId="24" pageNumber="25" phylum="Porifera" rank="species" species="indivisa" subGenus="Stylopus">
<emphasis box="[309,480,1641,1665]" italics="true" pageId="24" pageNumber="25">H. (S.) indivisa</emphasis>
<bibRefCitation author="Topsent" box="[488,647,1641,1665]" pageId="24" pageNumber="25" refString="Topsent, E. (1928) Spongiaires de l'Atlantique et de la Mediterranee provenant des croisieres du Prince Albert ler de Monaco. Resultats des campagnes scientifiques accomplies par le Prince Albert I. Monaco, 74, 376 pp." type="book" year="1928">
Topsent, 
<number box="[591,647,1641,1665]" pageId="24" pageNumber="25" value="1928.0">1928</number>
</bibRefCitation>
</taxonomicName>
has tylotes of 
<number box="[814,912,1641,1665]" pageId="24" pageNumber="25" value="480.0" valueMax="540.0" valueMin="420.0">420–540</number>
μm and acanthostyles of 
<number box="[1203,1301,1641,1665]" pageId="24" pageNumber="25" value="402.5" valueMax="595.0" valueMin="210.0">210–595</number>
μm; 
<taxonomicName authority="Topsent, 1904" authorityName="Topsent" authorityYear="1904" class="Demospongiae" family="Hymedesmiidae" genus="Hymedesmia" kingdom="Animalia" order="Poecilosclerida" pageId="24" pageNumber="25" phylum="Porifera" rank="species" species="obtusa" subGenus="Stylopus">
<emphasis italics="true" pageId="24" pageNumber="25">H. (S.) obtusa</emphasis>
<bibRefCitation author="Topsent" box="[238,403,1677,1701]" pageId="24" pageNumber="25" refString="Topsent, E. (1904) Spongiaires des Acores. Resultats des campagnes scientifiques accomplies par le Prince Albert I. Monaco, 25, 280 pp." type="book" year="1904">
Topsent, 
<number box="[347,403,1677,1701]" pageId="24" pageNumber="25" value="1904.0">1904</number>
</bibRefCitation>
</taxonomicName>
has acanthostyles of 
<number box="[667,765,1677,1701]" pageId="24" pageNumber="25" value="180.0" valueMax="230.0" valueMin="130.0">130–230</number>
μm and polytylote tylotes of 
<number box="[1132,1174,1677,1701]" pageId="24" pageNumber="25" value="280.0">280</number>
μm. 
<taxonomicName authority="Bergquist &amp; Fromont, 1988" authorityName="Bergquist &amp; Fromont" authorityYear="1988" class="Demospongiae" family="Chondropsidae" genus="Chondropsis" kingdom="Animalia" order="Poecilosclerida" pageId="24" pageNumber="25" phylum="Porifera" rank="species" species="australis" subGenus="Stylopus">
<emphasis box="[1245,1436,1677,1701]" italics="true" pageId="24" pageNumber="25">H. (S.) australis</emphasis>
(
<bibRefCitation author="Bergquist" box="[158,473,1713,1737]" pageId="24" pageNumber="25" refString="Bergquist, P. R. &amp; Fromont, P. J. (1988) The Marine Fauna of New Zealand: Porifera, Demospongiae, Part 4 (Poecilosclerida). New Zealand Oceanographic Institute Memoir, 96, 1 - 197." type="journal article" year="1988">
Bergquist &amp; Fromont, 
<number box="[414,473,1713,1737]" pageId="24" pageNumber="25" value="1988.0">1988</number>
</bibRefCitation>
)
</taxonomicName>
, 
