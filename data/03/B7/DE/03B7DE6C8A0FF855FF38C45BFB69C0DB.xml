<collectingCountry box="[719,790,1759,1783]" name="Iceland" pageId="32" pageNumber="33">Island</collectingCountry>
, Hawai’i Kai, depth between 
<quantity box="[1122,1216,1759,1783]" metricMagnitude="0" metricUnit="m" metricValue="1.75" metricValueMax="3.0" metricValueMin="0.5" pageId="32" pageNumber="33" unit="m" value="1.75" valueMax="3.0" valueMin="0.5">0.5–3 m</quantity>
, 
<date box="[1228,1381,1759,1784]" pageId="32" pageNumber="33" value="2007-05-31">
<number box="[1228,1256,1759,1783]" pageId="32" pageNumber="33" value="31.0">31</number>
May 
