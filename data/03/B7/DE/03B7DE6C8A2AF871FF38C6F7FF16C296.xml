<taxonomicName authority="Lamarck, 1815" authorityName="Lamarck" authorityYear="1815" box="[285,731,1352,1374]" class="Demospongiae" family="Suberitidae" genus="Protosuberites" kingdom="Animalia" order="Hadromerida" pageId="6" pageNumber="7" phylum="Porifera" rank="species" species="epiphytum">
