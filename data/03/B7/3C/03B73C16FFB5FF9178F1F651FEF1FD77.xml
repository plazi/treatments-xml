<taxonomicName authorityName="Tatarnic &amp; Cassis" authorityYear="2008" box="[518,664,1637,1656]" class="Insecta" family="Miridae" genus="Coridromius" kingdom="Animalia" order="Hemiptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="marmoreus">
