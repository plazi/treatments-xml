; 
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[1318,1408,1934,1960]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="23" pageNumber="24">USNM</collectionCode>
