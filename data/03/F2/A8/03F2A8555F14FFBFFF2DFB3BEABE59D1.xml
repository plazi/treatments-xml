<subScript attach="left" box="[1342,1357,493,507]" fontSize="6" pageId="8" pageNumber="127">e3</subScript>
), three on A7 (A 
<subScript attach="left" box="[255,263,529,543]" fontSize="6" pageId="8" pageNumber="127">3</subScript>
, B 
<subScript attach="left" box="[296,304,529,543]" fontSize="6" pageId="8" pageNumber="127">3</subScript>
, and E 
<subScript attach="right" box="[383,391,529,543]" fontSize="6" pageId="8" pageNumber="127">1</subScript>
), three on A8 (A 
<subScript attach="left" box="[581,589,529,543]" fontSize="6" pageId="8" pageNumber="127">4</subScript>
, B 
<subScript attach="left" box="[622,630,529,543]" fontSize="6" pageId="8" pageNumber="127">4</subScript>
, and C 
<subScript attach="left" box="[710,725,529,543]" fontSize="6" pageId="8" pageNumber="127">2a</subScript>
), four on A9 (A 
<subScript attach="left" box="[906,914,529,543]" fontSize="6" pageId="8" pageNumber="127">5</subScript>
, A 
<subScript attach="left" box="[947,978,529,543]" fontSize="6" pageId="8" pageNumber="127">e5pp</subScript>
, B 
<subScript attach="left" box="[1011,1019,529,543]" fontSize="6" pageId="8" pageNumber="127">5</subScript>
and an additional under B 
<subScript attach="left" box="[1315,1323,529,543]" fontSize="6" pageId="8" pageNumber="127">5</subScript>
