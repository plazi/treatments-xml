</taxonomicName>
<emphasis bold="true" box="[550,630,1806,1830]" pageId="10" pageNumber="287">
<taxonomicNameLabel box="[550,630,1806,1830]" pageId="10" pageNumber="287" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(A–J) and 
<taxonomicName authority="Ermilov &amp; Martens, 2021" authorityName="Ermilov &amp; Martens" authorityYear="2021" box="[742,1111,1806,1830]" class="Arachnida" family="Galumnidae" genus="Allogalumna" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="287" phylum="Arthropoda" rank="species" species="beateae" status="sp. nov." subGenus="Allogalumna">
<emphasis box="[742,873,1806,1830]" italics="true" pageId="10" pageNumber="287">Allogalumna</emphasis>
(
<emphasis box="[887,1018,1806,1830]" italics="true" pageId="10" pageNumber="287">Allogalumna</emphasis>
