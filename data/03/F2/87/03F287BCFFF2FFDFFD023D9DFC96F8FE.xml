<subSubSection box="[189,558,1395,1469]" pageId="18" pageNumber="19" type="distribution">
<paragraph blockId="18.[189,342,1395,1421]" box="[189,342,1395,1421]" pageId="18" pageNumber="19">
<heading bold="true" box="[189,342,1395,1421]" fontSize="11" level="3" pageId="18" pageNumber="19" reason="3">
<emphasis bold="true" box="[189,342,1395,1421]" pageId="18" pageNumber="19">Distribution</emphasis>
