<collectingCountry box="[477,619,727,752]" name="South Korea" pageId="7" pageNumber="381">South Korea</collectingCountry>
, 
<collectingCountry box="[630,697,728,751]" name="Japan" pageId="7" pageNumber="381">Japan</collectingCountry>
, northern 
<collectingCountry box="[808,878,727,752]" name="China" pageId="7" pageNumber="381">China</collectingCountry>
