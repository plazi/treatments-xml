ssp. 
<emphasis box="[422,508,1159,1185]" italics="true" pageId="66" pageNumber="67">linearis</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[521,732,1159,1185]" class="Magnoliopsida" family="Lamiaceae" genus="Sideritis" kingdom="Plantae" order="Lamiales" pageId="66" pageNumber="67" phylum="Tracheophyta" rank="species" species="perfoliata">
