<taxonomicName authority="Pers." authorityName="Pers." box="[489,667,581,607]" class="Magnoliopsida" family="Lamiaceae" genus="Hedeoma" kingdom="Plantae" order="Lamiales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="genus">
