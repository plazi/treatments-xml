<collectingCountry box="[930,1010,1519,1545]" name="Turkey" pageId="31" pageNumber="32">Turkey</collectingCountry>
to 
<collectingCountry box="[1046,1091,1519,1545]" name="Iran" pageId="31" pageNumber="32">Iran</collectingCountry>
(
<bibRefCitation author="Pauly, A." box="[1106,1252,1519,1546]" pageId="31" pageNumber="32" refId="ref105091" refString="Pauly, A. (2016 d) Genus Seladonia Robertson, 1918. Atlas Hymenoptera. Available from: http: // www. atlashymenoptera. net / page. aspx ?? ID = 67 (accessed 10 November 2020)" type="url" year="2016">Pauly 2016d</bibRefCitation>
