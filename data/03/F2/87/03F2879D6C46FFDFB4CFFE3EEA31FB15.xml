<paragraph blockId="96.[151,377,726,752]" box="[151,377,726,752]" pageId="96" pageNumber="97">
<taxonomicName baseAuthorityName="Kirby" baseAuthorityYear="1837" box="[151,377,726,752]" class="Insecta" family="Apidae" kingdom="Animalia" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="tribe" tribe="Anthophorini">
