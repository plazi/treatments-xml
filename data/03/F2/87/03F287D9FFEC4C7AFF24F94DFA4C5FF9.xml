<subSubSection pageId="13" pageNumber="214" type="description">
<paragraph blockId="13.[151,1436,1766,2010]" pageId="13" pageNumber="214">
<emphasis bold="true" box="[199,410,1805,1830]" pageId="13" pageNumber="214">Biological aspects</emphasis>
