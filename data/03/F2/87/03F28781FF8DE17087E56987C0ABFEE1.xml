(
<taxonomicName authority="Zone" authorityName="Zone" box="[566,913,1183,1208]" class="Cephalopoda" family="Placenticeratidae" genus="Placenticeras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ammonitida" pageId="6" pageNumber="267" phylum="Mollusca" rank="species" species="polyopsis">
