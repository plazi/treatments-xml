(
<collectingCountry box="[963,1068,949,973]" name="Indonesia" pageId="9" pageNumber="332">Indonesia</collectingCountry>
) (
<bibRefCitation author="STONE" box="[1089,1224,949,973]" firstAuthor="STONE" pageId="9" pageNumber="332" pagination="1 - 74" refId="ref16436" refString="STONE, B. C. (1978). Revisio Pandanacearum, Part I. Pandanus subgenera Coronata and Acrostigma. Flora Malesiana precursores. Fed. Mus. J. 23: 1 - 74." type="journal article" year="1978">
