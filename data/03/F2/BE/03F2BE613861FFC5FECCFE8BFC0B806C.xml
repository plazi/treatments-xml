apart; coxal seta II (
<emphasis box="[1284,1312,1239,1264]" italics="true" pageId="3" pageNumber="44">
<number box="[1284,1297,1239,1264]" pageId="3" pageNumber="44" value="1.0">1</number>
a
</emphasis>
) 
