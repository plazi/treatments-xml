<number box="[866,882,796,822]" pageId="13" pageNumber="54" value="2.0">2</number>
) long; antapical seta (
<number box="[1178,1238,796,822]" pageId="13" pageNumber="54" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
) long. Chelicerae 
<number box="[401,430,836,862]" pageId="13" pageNumber="54" value="16.0">16</number>
(
<number box="[451,527,836,862]" pageId="13" pageNumber="54" value="17.5" valueMax="19.0" valueMin="16.0">16–19</number>
