<emphasis box="[327,353,916,941]" italics="true" pageId="13" pageNumber="54">ve</emphasis>
) 
<number box="[370,385,916,942]" pageId="13" pageNumber="54" value="5.0">5</number>
(
<number box="[400,446,916,942]" pageId="13" pageNumber="54" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
