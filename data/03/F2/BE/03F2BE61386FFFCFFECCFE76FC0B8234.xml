<number box="[678,707,916,942]" pageId="13" pageNumber="54" value="23.0">23</number>
(
<number box="[723,799,916,942]" pageId="13" pageNumber="54" value="24.0" valueMax="26.0" valueMin="22.0">22–26</number>
