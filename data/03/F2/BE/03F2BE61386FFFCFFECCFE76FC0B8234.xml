<number box="[764,793,836,862]" pageId="13" pageNumber="54" value="12.0">12</number>
(
<number box="[814,889,836,862]" pageId="13" pageNumber="54" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
) long. Prodorsal shield 
<number box="[1189,1218,836,862]" pageId="13" pageNumber="54" value="28.0">28</number>
(
<number box="[1238,1314,836,862]" pageId="13" pageNumber="54" value="28.0" valueMax="29.0" valueMin="27.0">27–29</number>
