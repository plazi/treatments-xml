<taxonomicName authority="Li &amp; Tian 1990" authorityName="Li &amp; Tian" authorityYear="1990" box="[151,580,1898,1925]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="359" phylum="Arthropoda" rank="species" species="cerva">
<emphasis bold="true" box="[151,580,1898,1925]" pageId="1" pageNumber="359">
<emphasis bold="true" box="[151,382,1898,1925]" italics="true" pageId="1" pageNumber="359">Hydropsyche cerva</emphasis>
