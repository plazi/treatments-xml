<paragraph blockId="10.[151,611,1085,1146]" box="[151,611,1085,1111]" pageId="10" pageNumber="47">
<heading bold="true" box="[151,611,1085,1111]" fontSize="11" level="1" pageId="10" pageNumber="47" reason="1">
