<paragraph blockId="9.[151,1437,1255,2003]" box="[151,1184,1760,1787]" pageId="9" pageNumber="110">
<emphasis bold="true" box="[151,305,1760,1786]" pageId="9" pageNumber="110">Distribution.</emphasis>
<collectingCountry box="[311,378,1760,1786]" name="China" pageId="9" pageNumber="110">China</collectingCountry>
(
<collectingRegion box="[393,469,1760,1786]" country="China" name="Fujian" pageId="9" pageNumber="110">Fujian</collectingRegion>
, 
<collectingRegion box="[480,614,1760,1786]" country="China" name="Guangdong" pageId="9" pageNumber="110">Guangdong</collectingRegion>
, 
