<number box="[758,800,1024,1048]" pageId="131" pageNumber="132" value="369.0">369</number>
(BMNH): 
<number box="[926,934,1023,1048]" pageId="131" pageNumber="132" value="1.0">1</number>
/ 
<number box="[942,954,1023,1048]" pageId="131" pageNumber="132" value="8.0">8</number>
