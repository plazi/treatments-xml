</taxonomicName>
(
<taxonomicName box="[1014,1141,1255,1280]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="41" pageNumber="42" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
), 
