<paragraph blockId="40.[151,808,1821,1843]" box="[151,808,1821,1843]" pageId="40" pageNumber="41">
<emphasis bold="true" box="[151,286,1821,1843]" pageId="40" pageNumber="41">FIGURE 11.</emphasis>
<taxonomicName authority="Joubert" authorityName="Joubert" authorityYear="1901" box="[292,658,1821,1843]" class="Insecta" family="Coccidae" genus="Antecerococcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="cliffortiae">
