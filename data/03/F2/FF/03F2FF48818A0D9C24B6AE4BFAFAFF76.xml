<taxonomicName box="[534,665,224,248]" class="Insecta" family="Cerococcidae" genus="Cerococcus" kingdom="Animalia" order="Hemiptera" pageId="162" pageNumber="163" phylum="Arthropoda" rank="genus">
<emphasis box="[534,665,224,248]" italics="true" pageId="162" pageNumber="163">Cerococcus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[679,829,223,248]" class="Insecta" family="Cerococcidae" genus="Cerococcus" kingdom="Animalia" order="Hemiptera" pageId="162" pageNumber="163" phylum="Arthropoda" rank="species" species="tuberculus">
<emphasis box="[679,829,223,248]" italics="true" pageId="162" pageNumber="163">C. tuberculus</emphasis>
</taxonomicName>
keys out close to 
<taxonomicName box="[1030,1152,223,248]" class="Insecta" family="Cerococcidae" genus="Cerococcus" kingdom="Animalia" order="Hemiptera" pageId="162" pageNumber="163" phylum="Arthropoda" rank="species" species="kalmiae">
