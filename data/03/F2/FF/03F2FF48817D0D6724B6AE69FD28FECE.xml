<paragraph blockId="86.[151,1437,1538,2031]" box="[199,1149,1862,1887]" pageId="86" pageNumber="87">
<emphasis bold="true" box="[199,423,1862,1887]" pageId="86" pageNumber="87">Mounted material.</emphasis>
Body roundly pear-shaped, 
<quantity box="[740,875,1863,1887]" metricMagnitude="-3" metricUnit="m" metricValue="1.95" metricValueMax="2.2" metricValueMin="1.7" pageId="86" pageNumber="87" unit="mm" value="1.95" valueMax="2.2" valueMin="1.7">1.7–2.2 mm</quantity>
