<caption ID-DOI="http://doi.org/10.5281/zenodo.4895007" ID-Zenodo-Dep="4895007" box="[151,1254,2008,2032]" httpUri="https://zenodo.org/record/4895007/files/figure.png" pageId="14" pageNumber="25" startId="14.[151,269,2008,2032]" targetBox="[151,1436,776,1995]" targetPageId="14">
<paragraph blockId="14.[151,1254,2008,2032]" box="[151,1254,2008,2032]" pageId="14" pageNumber="25">
