<taxonomicName authority="Foged (1970: 169)" authorityName="Foged" authorityPageNumber="169" authorityYear="1970" class="Bacillariophyceae" family="Naviculaceae" genus="Pinnularia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Naviculales" pageId="20" pageNumber="31" phylum="Bacillariophyta" rank="species" species="iatriaensis">
<emphasis italics="true" pageId="20" pageNumber="31">P. iatriaensis</emphasis>
<bibRefCitation author="Foged, N." box="[282,502,190,216]" pageId="20" pageNumber="31" pagination="169 - 202" refId="ref15067" refString="Foged, N. (1970) The diatomaceous Flora in postglacial Kieselguhr Deposit in Southwestern Norway. Bacillaria 2: 169 - 202." type="journal article" year="1970">Foged (1970: 169)</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Hustedt (1954: 474)" authorityName="Hustedt" authorityPageNumber="474" authorityYear="1954" box="[514,853,190,216]" class="Bacillariophyceae" family="Naviculaceae" genus="Pinnularia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Naviculales" pageId="20" pageNumber="31" phylum="Bacillariophyta" rank="species" species="lundii">
