<paragraph blockId="75.[177,610,968,991]" box="[177,610,968,991]" pageId="75" pageNumber="1088">
<heading box="[177,610,968,991]" fontSize="8" level="2" pageId="75" pageNumber="1088" reason="2">
<emphasis bold="true" box="[177,610,968,991]" pageId="75" pageNumber="1088">
