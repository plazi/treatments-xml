<quantity box="[1072,1235,1590,1617]" metricMagnitude="-2" metricUnit="m" metricValue="1.085" metricValueMax="1.1199999999999999" metricValueMin="1.05" pageId="8" pageNumber="249" unit="mm" value="10.85" valueMax="11.2" valueMin="10.5">10.5–11.2 mm</quantity>
