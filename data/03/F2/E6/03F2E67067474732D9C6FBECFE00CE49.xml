<quantity box="[1295,1432,1591,1617]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" metricValueMax="2.4" metricValueMin="2.2" pageId="8" pageNumber="249" unit="mm" value="2.3" valueMax="2.4" valueMin="2.2">2.2–2.4 mm</quantity>
, HL 9.2–10.0 mm, HW 
<quantity box="[410,548,1627,1653]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" metricValueMax="2.3" metricValueMin="2.1" pageId="8" pageNumber="249" unit="mm" value="2.2" valueMax="2.3" valueMin="2.1">2.1–2.3 mm</quantity>
