<quantity box="[1294,1432,1555,1581]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" metricValueMax="2.2" metricValueMin="2.1" pageId="8" pageNumber="249" unit="mm" value="2.15" valueMax="2.2" valueMin="2.1">2.1–2.2 mm</quantity>
, HL 8.0– 
<quantity box="[244,331,1591,1617]" metricMagnitude="-3" metricUnit="m" metricValue="8.2" pageId="8" pageNumber="249" unit="mm" value="8.2">8.2 mm</quantity>
, HW 1.9–2.0 mm; Female: AbL 4.8–5.0 mm, BL 7.0– 
<quantity box="[935,1022,1591,1617]" metricMagnitude="-3" metricUnit="m" metricValue="7.3" pageId="8" pageNumber="249" unit="mm" value="7.3">7.3 mm</quantity>
