<subSubSection lastPageId="9" lastPageNumber="250" pageId="8" pageNumber="249" type="description">
<paragraph blockId="8.[151,1437,1231,2013]" pageId="8" pageNumber="249">
<emphasis bold="true" box="[199,382,1555,1581]" pageId="8" pageNumber="249">Measurements.</emphasis>
