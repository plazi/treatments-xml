<quantity box="[1096,1233,1555,1581]" metricMagnitude="-3" metricUnit="m" metricValue="9.25" metricValueMax="9.3" metricValueMin="9.2" pageId="8" pageNumber="249" unit="mm" value="9.25" valueMax="9.3" valueMin="9.2">9.2–9.3 mm</quantity>
