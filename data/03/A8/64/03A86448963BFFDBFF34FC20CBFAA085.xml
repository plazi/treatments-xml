<caption ID-DOI="http://doi.org/10.5281/zenodo.5361225" ID-Zenodo-Dep="5361225" box="[151,1437,1811,1835]" httpUri="https://zenodo.org/record/5361225/files/figure.png" pageId="15" pageNumber="16" startId="15.[151,255,1811,1835]" targetBox="[158,1428,213,1772]" targetPageId="15">
<paragraph blockId="15.[151,1437,1811,1835]" box="[151,1437,1811,1835]" pageId="15" pageNumber="16">
