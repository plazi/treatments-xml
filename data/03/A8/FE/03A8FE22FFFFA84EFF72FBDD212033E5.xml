<taxonomicName authority="Li &amp; Zhao &amp; Chen &amp; Wu &amp; Li, 2019" authorityName="Li &amp; Zhao &amp; Chen &amp; Wu &amp; Li" authorityYear="2019" class="Arachnida" family="Amaurobiidae" genus="Draconarius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="143" phylum="Arthropoda" rank="species" species="budanlaensis" status="sp. n.">
<emphasis italics="true" pageId="2" pageNumber="143">D. budanlaensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[305,368,1450,1476]" pageId="2" pageNumber="143" rank="species">
<emphasis bold="true" box="[305,368,1450,1476]" pageId="2" pageNumber="143">sp. n.</emphasis>
</taxonomicNameLabel>
in 
<figureCitation box="[401,484,1449,1475]" captionStart="FIGURE 2" captionStartId="4.[151,250,1970,1995]" captionTargetBox="[187,1400,166,1950]" captionTargetId="figure@4.[187,1400,166,1950]" captionTargetPageId="4" captionText="FIGURE 2. Epigyne and habitus of Draconarius budanlaensis sp. n. A Epigyne, ventral B Internal genitalia, dorsal C Female habitus, dorsal D Female habitus, ventral E Female habitus, lateral. Scale bars: equal for A and B, equal for C, D and E." httpUri="https://zenodo.org/record/3353963/files/figure.png" pageId="2" pageNumber="143">Fig. 2A</figureCitation>
and as wide as long in 
<taxonomicName baseAuthorityName="Hu &amp; Li" baseAuthorityYear="1987" box="[740,911,1449,1475]" class="Arachnida" family="Amaurobiidae" genus="Draconarius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="143" phylum="Arthropoda" rank="species" species="yadongensis">
<emphasis box="[740,911,1449,1475]" italics="true" pageId="2" pageNumber="143">D. yadongensis</emphasis>
</taxonomicName>
in fig. 
<quantity box="[990,1067,1449,1475]" metricMagnitude="-1" metricUnit="m" metricValue="5.1054" pageId="2" pageNumber="143" unit="in" value="20.1">20.1 in</quantity>
