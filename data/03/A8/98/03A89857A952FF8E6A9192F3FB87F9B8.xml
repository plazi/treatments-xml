, TL: 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="4.699999999999999" metricValueMax="4.8" metricValueMin="4.6" pageId="4" pageNumber="280" unit="mm" value="0.47" valueMax="0.48" valueMin="0.46">0.46–0.48 mm</quantity>
, PL: 
<quantity box="[256,421,583,610]" metricMagnitude="-3" metricUnit="m" metricValue="1.03" metricValueMax="1.07" metricValueMin="0.99" pageId="4" pageNumber="280" unit="mm" value="1.03" valueMax="1.07" valueMin="0.99">0.99–1.07 mm</quantity>
, PW: 
<quantity box="[488,653,583,610]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="8.7" metricValueMin="8.299999999999999" pageId="4" pageNumber="280" unit="mm" value="0.85" valueMax="0.87" valueMin="0.83">0.83–0.87 mm</quantity>
, EL: 
<quantity box="[714,879,583,610]" metricMagnitude="-3" metricUnit="m" metricValue="1.02" metricValueMax="1.03" metricValueMin="1.01" pageId="4" pageNumber="280" unit="mm" value="1.02" valueMax="1.03" valueMin="1.01">1.01–1.03 mm</quantity>
