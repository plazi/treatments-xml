<figureCitation box="[1052,1123,1520,1546]" captionStart="FIGURE 1" captionStartId="2.[151,250,1555,1579]" captionTargetBox="[156,1432,728,1529]" captionTargetId="figure-137@2.[156,1432,728,1529]" captionTargetPageId="2" captionText="FIGURE 1. General view of Helcophorus (Thibeteros) ekaterinae, sp. nov." figureDoi="http://doi.org/10.5281/zenodo.6498052" httpUri="https://zenodo.org/record/6498052/files/figure.png" pageId="3" pageNumber="87">Figs 1</figureCitation>
, 
<figureCitation box="[1137,1154,1520,1546]" captionStart="FIGURE 2" captionStartId="3.[151,250,1126,1150]" captionTargetBox="[161,1426,181,1101]" captionTargetId="figure-24@3.[161,1426,181,1101]" captionTargetPageId="3" captionText="FIGURE 2. Pronotum and the base of elytra of Helcophorus (Thibeteros) ekaterinae, sp. nov." figureDoi="http://doi.org/10.5281/zenodo.6498067" httpUri="https://zenodo.org/record/6498067/files/figure.png" pageId="3" pageNumber="87">2</figureCitation>
).
