<subSubSection pageId="1" pageNumber="6" type="materials_examined">
<paragraph blockId="1.[113,646,1445,1495]" pageId="1" pageNumber="6">
<collectionCode box="[113,239,1445,1468]" pageId="1" pageNumber="6">
<typeStatus box="[113,162,1445,1468]" pageId="1" pageNumber="6">TYPE</typeStatus>
SPECIES
</collectionCode>
. — 
<taxonomicName authority="Lee, 2013" authorityName="Lee" authorityYear="2013" box="[281,478,1445,1469]" class="Insecta" family="Cicadidae" genus="Miniterpnosia" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="6" phylum="Arthropoda" rank="species" species="chorus" status="n. sp.">
