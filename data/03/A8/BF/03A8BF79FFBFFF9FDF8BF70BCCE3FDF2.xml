<collectingCountry box="[310,376,522,545]" name="Egypt" pageId="32" pageNumber="95">Egypt</collectingCountry>
, 
<collectingRegion box="[387,554,522,545]" country="Egypt" name="Shamal Sina'" pageId="32" pageNumber="95">Sinai Peninsula</collectingRegion>
, 
<collectingCountry box="[564,638,522,545]" name="Yemen" pageId="32" pageNumber="95">Yemen</collectingCountry>
