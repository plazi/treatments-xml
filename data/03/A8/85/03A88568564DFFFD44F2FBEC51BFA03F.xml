, 
<collectingCountry box="[709,790,1214,1238]" name="Austria" pageId="19" pageNumber="524">Austria</collectingCountry>
, 
<collectingCountry box="[801,973,1214,1238]" name="Czech Republic" pageId="19" pageNumber="524">Czech Republic</collectingCountry>
, 
<collectingCountry box="[984,1034,1214,1238]" name="Italy" pageId="19" pageNumber="524">Italy</collectingCountry>
, 
<collectingCountry name="Slovenia" pageId="19" pageNumber="524">Slovenia</collectingCountry>
, 
<collectingCountry box="[184,262,1247,1271]" name="Greece" pageId="19" pageNumber="524">Greece</collectingCountry>
, 
<collectingCountry box="[272,349,1246,1270]" name="Turkey" pageId="19" pageNumber="524">Turkey</collectingCountry>
, 
<collectingCountry box="[359,405,1247,1270]" name="Iran" pageId="19" pageNumber="524">Iran</collectingCountry>
, 
<collectingCountry box="[415,477,1246,1270]" name="Israel" pageId="19" pageNumber="524">Israel</collectingCountry>
, 
<collectingCountry box="[487,593,1246,1270]" name="Mongolia" pageId="19" pageNumber="524">Mongolia</collectingCountry>
, and the 
<collectingCountry box="[688,745,1247,1271]" name="United States of America" pageId="19" pageNumber="524">USA</collectingCountry>
