<emphasis bold="true" box="[601,682,1946,1970]" pageId="166" pageNumber="167">
<taxonomicNameLabel box="[601,682,1946,1970]" pageId="166" pageNumber="167" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
Holotype. 
<emphasis bold="true" box="[796,814,1946,1970]" pageId="166" pageNumber="167">a.</emphasis>
habitus, lateral view; 
<emphasis bold="true" box="[1039,1059,1946,1970]" pageId="166" pageNumber="167">b.</emphasis>
fore wing; 
<emphasis bold="true" box="[1177,1195,1946,1970]" pageId="166" pageNumber="167">c.</emphasis>
mesonotum and scutellum, dorsal view; 
<emphasis bold="true" box="[334,354,1982,2006]" pageId="166" pageNumber="167">d.</emphasis>
head, dorsal view; 
<emphasis bold="true" box="[551,569,1982,2006]" pageId="166" pageNumber="167">e.</emphasis>
mesopleuron; 
<emphasis bold="true" box="[719,734,1981,2006]" pageId="166" pageNumber="167">f.</emphasis>
metasoma, dorsal view; 
<emphasis bold="true" box="[986,1005,1982,2006]" pageId="166" pageNumber="167">g.</emphasis>
hind wing; 
<emphasis bold="true" box="[1126,1146,1982,2006]" pageId="166" pageNumber="167">h.</emphasis>
head, frontal view; 
