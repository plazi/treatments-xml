<paragraph blockId="35.[151,1437,1627,2014]" pageId="35" pageNumber="36">
<emphasis bold="true" box="[199,264,1951,1977]" pageId="35" pageNumber="36">Note:</emphasis>
<taxonomicName authorityName="De Meyer" authorityYear="1995" box="[273,409,1952,1977]" class="Insecta" family="Pipunculidae" genus="Tomosvaryella" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="inopinata">
