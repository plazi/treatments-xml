<taxonomicName authority="Zeller." authorityName="Zeller." box="[927,1208,765,791]" class="Insecta" family="Gelechiidae" genus="Anarsia" kingdom="Animalia" order="Lepidoptera" pageId="186" pageNumber="187" phylum="Arthropoda" rank="species" species="lineatella">
