(
<bibRefCitation author="Kozanek, M." box="[879,1053,654,680]" pageId="61" pageNumber="62" pagination="1 - 16" refId="ref57349" refString="Kozanek, M. (1992) Contribution to the pipunculid fauna of Mongolia (Diptera: Pipunculidae). Akitu, Kyoto Entomological Society, New Series, 131, 1 - 16." type="journal article" year="1992">Kozánek, 1992</bibRefCitation>
, 
<bibRefCitation author="Skevington, J. H." box="[1064,1256,654,680]" pageId="61" pageNumber="62" refId="ref58350" refString="Skevington, J. H. (2020) &quot; Pipunculidae &quot;. Canadian National Collection of Insects, Arachnids and Nematodes database. Available from: https: // cnc. agr. gc. ca / taxonomy / Taxonomy. php? id = 18557 (accessed 18 October 2020) [public version of Pipunculidae catalogue maintained by J. H. Skevington]" type="url" year="2020">Skevington 2020</bibRefCitation>
) (
<figureCitation box="[1279,1363,654,681]" captionStart="FIGURE 62" captionStartId="91.[151,250,1562,1586]" captionTargetBox="[151,1436,896,1538]" captionTargetId="figure-40@91.[151,1436,896,1538]" captionTargetPageId="91" captionText="FIGURE 62. Distribution of Tomosvaryella pruinosa, T. subsylvatica, T. trichotibialis in the Middle East." figureDoi="http://doi.org/10.5281/zenodo.5122795" httpUri="https://zenodo.org/record/5122795/files/figure.png" pageId="61" pageNumber="62">Fig. 62</figureCitation>
