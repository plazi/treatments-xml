<collectionCode box="[294,351,188,212]" httpUri="http://grbio.org/cool/2vvw-hn58" name="Sammlung von Algenkulturen at Universitat Gottingen" pageId="7" pageNumber="53">SAG</collectionCode>
, 
<collectingCountry box="[362,427,188,212]" name="Chile" pageId="7" pageNumber="53">Chile</collectingCountry>
