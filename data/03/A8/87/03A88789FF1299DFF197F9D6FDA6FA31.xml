<typeStatus box="[304,412,1770,1796]" pageId="158" pageNumber="159">Holotype</typeStatus>
. Female. Body length 
<quantity box="[663,751,1769,1796]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="158" pageNumber="159" unit="mm" value="2.5">2.5 mm</quantity>
