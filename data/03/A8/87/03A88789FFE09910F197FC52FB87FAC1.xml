<emphasis box="[293,499,1889,1913]" italics="true" pageId="109" pageNumber="110">Apanteles longicoxa</emphasis>
Liu &amp; Chen
</taxonomicName>
, 
<emphasis bold="true" box="[638,718,1889,1913]" pageId="109" pageNumber="110">
<taxonomicNameLabel box="[638,718,1889,1913]" pageId="109" pageNumber="110" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
Holotype. 
<emphasis bold="true" box="[831,850,1889,1913]" pageId="109" pageNumber="110">a.</emphasis>
habitus, lateral view; 
<emphasis bold="true" box="[1073,1094,1889,1913]" pageId="109" pageNumber="110">b.</emphasis>
propodeum; 
<emphasis bold="true" box="[1228,1246,1889,1913]" pageId="109" pageNumber="110">c.</emphasis>
head, dorsal view; 
<emphasis bold="true" box="[151,172,1925,1949]" pageId="109" pageNumber="110">d.</emphasis>
antenna; 
<emphasis bold="true" box="[271,288,1925,1949]" pageId="109" pageNumber="110">e.</emphasis>
mesonotum and scutellum, dorsal view; 
<emphasis bold="true" box="[707,722,1924,1949]" pageId="109" pageNumber="110">f.</emphasis>
head, frontal view; 
<emphasis bold="true" box="[926,945,1925,1949]" pageId="109" pageNumber="110">g.</emphasis>
mesopleuron; 
<emphasis bold="true" box="[1097,1117,1925,1949]" pageId="109" pageNumber="110">h.</emphasis>
metasoma, dorsal view; 
<emphasis bold="true" box="[1371,1384,1924,1949]" pageId="109" pageNumber="110">i.</emphasis>
hind wing; 
