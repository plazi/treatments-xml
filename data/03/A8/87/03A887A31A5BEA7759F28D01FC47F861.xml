<taxonomicName authority="Pallas, 1772" authorityName="Pallas" authorityYear="1772" box="[151,619,871,896]" class="Malacostraca" family="Ocypodidae" genus="Ocypode" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="370" phylum="Arthropoda" rank="species" species="ceratophthalmus">
<emphasis bold="true" box="[151,619,871,896]" pageId="1" pageNumber="370">
<emphasis bold="true" box="[151,452,871,896]" italics="true" pageId="1" pageNumber="370">Ocypode ceratophthalmus</emphasis>
(Pallas, 
<number box="[554,611,871,896]" pageId="1" pageNumber="370" value="1772.0">1772</number>
)
</emphasis>
</taxonomicName>
