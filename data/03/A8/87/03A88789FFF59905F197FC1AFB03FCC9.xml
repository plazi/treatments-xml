<emphasis bold="true" box="[644,724,1945,1969]" pageId="122" pageNumber="123">
<taxonomicNameLabel box="[644,724,1945,1969]" pageId="122" pageNumber="123" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
Holotype. 
<emphasis bold="true" box="[835,854,1945,1969]" pageId="122" pageNumber="123">a.</emphasis>
habitus, dorsal view; 
<emphasis bold="true" box="[1073,1093,1945,1969]" pageId="122" pageNumber="123">b.</emphasis>
mesonotum and scutellum, dorsal view; 
<emphasis bold="true" box="[213,230,1981,2005]" pageId="122" pageNumber="123">c.</emphasis>
head, frontal view; 
<emphasis bold="true" box="[430,451,1981,2005]" pageId="122" pageNumber="123">d.</emphasis>
propodeum; 
<emphasis bold="true" box="[584,601,1981,2005]" pageId="122" pageNumber="123">e.</emphasis>
ovipositor sheath; 
<emphasis bold="true" box="[792,807,1980,2005]" pageId="122" pageNumber="123">f.</emphasis>
mesopleuron; 
<emphasis bold="true" box="[956,975,1981,2005]" pageId="122" pageNumber="123">g.</emphasis>
head, dorsal view; 
<emphasis bold="true" box="[1169,1189,1981,2005]" pageId="122" pageNumber="123">h.</emphasis>
