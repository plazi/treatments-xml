<collectingCountry box="[765,835,1021,1047]" name="China" pageId="38" pageNumber="39">China</collectingCountry>
and 
<collectingCountry box="[893,1011,1021,1047]" name="Mongolia" pageId="38" pageNumber="39">Mongolia</collectingCountry>
