<subSubSection pageId="76" pageNumber="77" type="etymology">
<paragraph blockId="76.[151,1436,1907,2042]" pageId="76" pageNumber="77">
<emphasis bold="true" box="[199,338,1907,1933]" pageId="76" pageNumber="77">Etymology:</emphasis>
