<number box="[196,242,1294,1317]" pageId="2" pageNumber="283" value="0.73">0.73</number>
long. Anterior eye row 
<number box="[508,555,1294,1317]" pageId="2" pageNumber="283" value="1.05">1.05</number>
wide and posterior 
<number box="[774,821,1294,1317]" pageId="2" pageNumber="283" value="0.97">0.97</number>
wide. Sternum 
<number box="[995,1042,1294,1317]" pageId="2" pageNumber="283" value="0.62">0.62</number>
long, 
<number box="[1113,1160,1294,1317]" pageId="2" pageNumber="283" value="0.49">0.49</number>
