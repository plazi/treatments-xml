Spangler, 
<number box="[151,210,979,1003]" pageId="4" pageNumber="456" value="1980.0">1980</number>
</taxonomicName>
, and 
<taxonomicName authority="Leconte, 1874" authorityName="Leconte" authorityYear="1874" box="[271,697,979,1003]" class="Insecta" family="Elmidae" genus="Heterelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="456" phylum="Arthropoda" rank="species" species="vulnerata">
