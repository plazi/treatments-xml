<taxonomicName authority="Deleve, 1968" authorityName="Deleve" authorityYear="1968" class="Insecta" family="Elmidae" genus="Heterelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="456" phylum="Arthropoda" rank="species" species="impressicollis">
