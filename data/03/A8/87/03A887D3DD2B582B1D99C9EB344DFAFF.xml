Sharp, 
<number box="[656,712,871,895]" pageId="4" pageNumber="456" value="1887.0">1887</number>
</taxonomicName>
(
<number box="[730,764,870,895]" pageId="4" pageNumber="456" value="3.5">3.5</number>
–4.0 mm), 
<taxonomicName authority="Deleve, 1968" authorityName="Deleve" authorityYear="1968" box="[897,1168,870,895]" class="Insecta" family="Elmidae" genus="Heterelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="456" phylum="Arthropoda" rank="species" species="lucida">
<emphasis box="[897,1004,870,894]" italics="true" pageId="4" pageNumber="456">H. lucida</emphasis>
