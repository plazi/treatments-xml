<taxonomicName authority="Grouvelle, 1896" authorityName="Grouvelle" authorityYear="1896" class="Insecta" family="Elmidae" genus="Heterelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="456" phylum="Arthropoda" rank="species" species="neglecta">
