<emphasis box="[226,275,1014,1038]" italics="true" pageId="4" pageNumber="456">et al</emphasis>
., 
<quantity box="[298,354,1015,1039]" metricMagnitude="1" metricUnit="m" metricValue="5.04952" pageId="4" pageNumber="456" unit="in" value="1988.0">1988</quantity>
</taxonomicName>
in dark color, and wider body form; from 
<taxonomicName authority="Grouvelle, 1889" authorityName="Grouvelle" authorityYear="1889" box="[840,1157,1014,1039]" class="Insecta" family="Elmidae" genus="Heterelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="456" phylum="Arthropoda" rank="species" species="gibbosa">
<emphasis box="[840,963,1014,1038]" italics="true" pageId="4" pageNumber="456">H. gibbosa</emphasis>
Grouvelle, 
<number box="[1099,1157,1015,1039]" pageId="4" pageNumber="456" value="1889.0">1889</number>
</taxonomicName>
, 
<taxonomicName authority="Hinton, 1940" authorityName="Hinton" authorityYear="1940" class="Insecta" family="Elmidae" genus="Heterelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="456" phylum="Arthropoda" rank="subSpecies" species="obesa" subSpecies="plana">
<emphasis box="[1170,1343,1014,1038]" italics="true" pageId="4" pageNumber="456">H. obesa plana</emphasis>
