<quantity box="[1011,1112,1768,1793]" metricMagnitude="1" metricUnit="m" metricValue="4.95" metricValueMax="8.5" metricValueMin="1.4" pageId="207" pageNumber="208" unit="m" value="49.5" valueMax="85.0" valueMin="14.0">14–85 m</quantity>
