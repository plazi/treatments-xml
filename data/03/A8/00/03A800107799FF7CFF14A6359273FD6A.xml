<subSubSection pageId="207" pageNumber="208" type="distribution">
<paragraph blockId="207.[151,1437,1444,2009]" pageId="207" pageNumber="208">
<emphasis bold="true" box="[199,503,1768,1793]" pageId="207" pageNumber="208">Distribution and ecology.</emphasis>
<collectingCountry box="[512,599,1769,1793]" name="Guyana" pageId="207" pageNumber="208">Guyana</collectingCountry>
