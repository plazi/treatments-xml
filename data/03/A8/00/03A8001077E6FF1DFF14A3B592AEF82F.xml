<quantity box="[999,1112,1407,1432]" metricMagnitude="2" metricUnit="m" metricValue="3.655" metricValueMax="6.5" metricValueMin="0.8099999999999999" pageId="177" pageNumber="178" unit="m" value="365.5" valueMax="650.0" valueMin="81.0">81–650 m</quantity>
(
