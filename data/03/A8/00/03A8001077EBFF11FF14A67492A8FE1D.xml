<figureCitation box="[348,451,1839,1864]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-10="FIGURE 11" captionStart-11="FIGURE 12" captionStart-12="FIGURE 13" captionStart-13="FIGURE 14" captionStart-14="FIGURE 15" captionStart-15="FIGURE 16" captionStart-16="FIGURE 17" captionStart-17="FIGURE 18" captionStart-18="FIGURE 19" captionStart-2="FIGURE 3" captionStart-3="FIGURE 4" captionStart-4="FIGURE 5" captionStart-5="FIGURE 6" captionStart-6="FIGURE 7" captionStart-7="FIGURE 8" captionStart-8="FIGURE 9" captionStart-9="FIGURE 10" captionStartId-0="9.[151,250,1197,1219]" captionStartId-1="10.[151,250,1108,1130]" captionStartId-10="20.[151,250,1776,1798]" captionStartId-11="21.[151,250,1849,1871]" captionStartId-12="23.[151,250,1879,1901]" captionStartId-13="24.[151,250,1628,1650]" captionStartId-14="26.[151,250,911,933]" captionStartId-15="27.[151,250,1025,1047]" captionStartId-16="28.[151,250,1883,1905]" captionStartId-17="29.[151,250,1647,1669]" captionStartId-18="31.[151,250,1727,1749]" captionStartId-2="12.[151,250,862,884]" captionStartId-3="13.[151,250,689,711]" captionStartId-4="13.[151,250,1831,1853]" captionStartId-5="14.[151,250,1744,1766]" captionStartId-6="15.[151,250,1275,1297]" captionStartId-7="16.[151,250,1454,1476]" captionStartId-8="18.[151,250,1169,1191]" captionStartId-9="19.[151,250,1365,1386]" captionTargetBox-0="[340,1247,339,1176]" captionTargetBox-1="[151,1391,374,1048]" captionTargetBox-10="[151,1436,1271,1754]" captionTargetBox-11="[151,1436,535,1827]" captionTargetBox-12="[151,1436,193,1858]" captionTargetBox-13="[151,1436,193,1607]" captionTargetBox-14="[151,1436,193,889]" captionTargetBox-15="[151,1436,193,1003]" captionTargetBox-16="[151,1436,980,1862]" captionTargetBox-17="[151,1436,465,1626]" captionTargetBox-18="[151,1436,193,1704]" captionTargetBox-2="[151,1436,267,841]" captionTargetBox-3="[151,1436,195,668]" captionTargetBox-4="[151,1436,1216,1809]" captionTargetBox-5="[151,1436,857,1722]" captionTargetBox-6="[1