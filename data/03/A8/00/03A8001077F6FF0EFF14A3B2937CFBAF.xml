<subSubSection pageId="162" pageNumber="163" type="distribution">
<paragraph blockId="162.[151,1437,151,1077]" pageId="162" pageNumber="163">
<emphasis bold="true" box="[199,502,871,896]" pageId="162" pageNumber="163">Distribution and ecology.</emphasis>
<collectingCountry box="[510,597,872,896]" name="Guyana" pageId="162" pageNumber="163">Guyana</collectingCountry>
Shelf, 
<collectingCountry box="[681,843,871,896]" name="Virgin Islands" pageId="162" pageNumber="163">Virgin Islands</collectingCountry>
, 
<collectingCountry box="[856,918,871,896]" name="Cuba" pageId="162" pageNumber="163">Cuba</collectingCountry>
, 
<collectingCountry box="[931,1024,871,896]" name="Jamaica" pageId="162" pageNumber="163">Jamaica</collectingCountry>
, 
<collectingCountry box="[1037,1133,872,896]" name="Curaçao" pageId="162" pageNumber="163">Curaçao</collectingCountry>
, NE 
<collectingCountry box="[1191,1262,871,896]" name="Brazil" pageId="162" pageNumber="163">Brazil</collectingCountry>
, in mangroves, bays and on soft bottoms at 
<quantity box="[465,550,908,933]" metricMagnitude="1" metricUnit="m" metricValue="2.95" metricValueMax="5.8" metricValueMin="0.1" pageId="162" pageNumber="163" unit="m" value="29.5" valueMax="58.0" valueMin="1.0">1–58 m</quantity>
depth (
<collectingCountry box="[634,723,908,932]" name="Guyana" pageId="162" pageNumber="163">Guyana</collectingCountry>
Shelf 
<quantity box="[796,898,908,933]" metricMagnitude="1" metricUnit="m" metricValue="2.7" metricValueMax="2.9" metricValueMin="2.5" pageId="162" pageNumber="163" unit="m" value="27.0" valueMax="29.0" valueMin="25.0">25–29 m</quantity>
).
</paragraph>
