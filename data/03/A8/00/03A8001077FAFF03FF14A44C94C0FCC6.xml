<subSubSection box="[199,1396,727,752]" pageId="174" pageNumber="175" type="etymology">
<paragraph blockId="174.[151,1436,151,752]" box="[199,1396,727,752]" pageId="174" pageNumber="175">
<emphasis bold="true" box="[199,334,727,752]" pageId="174" pageNumber="175">Etymology.</emphasis>
