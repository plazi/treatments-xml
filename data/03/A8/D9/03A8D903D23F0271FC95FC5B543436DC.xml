<taxonomicName box="[874,1009,1053,1077]" class="Bacilli" family="Lactobacillaceae" genus="Paucilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="48" pageNumber="2830" phylum="Firmicutes" rank="species" species="wasatchensis">
