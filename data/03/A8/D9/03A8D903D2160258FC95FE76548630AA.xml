<taxonomicName authority="Kashiwagi et al. 2009" authorityName="Kashiwagi" authorityYear="2009" box="[939,1403,572,596]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="25" pageNumber="2807" phylum="Firmicutes" rank="species" species="nodensis">
