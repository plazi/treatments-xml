<superScript attach="left" box="[1213,1223,1393,1407]" fontSize="6" pageId="51" pageNumber="2833">T</superScript>
= ATCC 23272 
<superScript attach="none" box="[1386,1396,1393,1407]" fontSize="6" pageId="51" pageNumber="2833">T</superScript>
=DSM 20016 
<superScript attach="left" box="[892,902,1424,1438]" fontSize="6" pageId="51" pageNumber="2833">T</superScript>
= JCM 1112 
<superScript attach="none" box="[1036,1046,1424,1438]" fontSize="6" pageId="51" pageNumber="2833">T</superScript>
=LMG 9213 
<superScript attach="left" box="[1176,1186,1424,1438]" fontSize="6" pageId="51" pageNumber="2833">T</superScript>
= LMG 13557 
<superScript attach="none" box="[1340,1348,1424,1438]" fontSize="6" pageId="51" pageNumber="2833">T</superScript>
.
</paragraph>
