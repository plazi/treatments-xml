<paragraph blockId="18.[121,762,1245,1361]" pageId="18" pageNumber="2800">
<taxonomicName box="[121,343,1245,1269]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="18" pageNumber="2800" phylum="Firmicutes" rank="species" species="psittaci">
<emphasis box="[121,343,1245,1269]" italics="true" pageId="18" pageNumber="2800">Lactobacillus psittaci</emphasis>
</taxonomicName>
