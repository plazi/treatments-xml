<taxonomicName authority="Beck et al. 1989" authorityName="Beck" authorityYear="1989" box="[937,1337,183,207]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="34" pageNumber="2816" phylum="Firmicutes" rank="species" species="graminis">
