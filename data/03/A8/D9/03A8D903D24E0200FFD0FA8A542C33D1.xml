<taxonomicName authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[232,679,1593,1618]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="gannanensis">
