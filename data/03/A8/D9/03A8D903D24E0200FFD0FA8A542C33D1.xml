<taxonomicName box="[238,368,1517,1540]" class="Bacilli" family="Lactobacillaceae" genus="Lapidilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="gannanensis">
