</paragraph>
<paragraph blockId="12.[120,760,1410,1526]" pageId="12" pageNumber="2794">
<taxonomicName box="[121,612,1410,1434]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="12" pageNumber="2794" phylum="Firmicutes" rank="subSpecies" species="delbrueckii" subSpecies="jakobsenii">
