<paragraph blockId="38.[120,760,1759,1909]" pageId="38" pageNumber="2820">
<taxonomicName box="[121,193,1760,1783]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="38" pageNumber="2820" phylum="Firmicutes" rank="species" species="oeni">
