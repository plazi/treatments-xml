<taxonomicName authority="Diaz et al. 2020" authorityName="Diaz" authorityYear="2020" box="[939,1301,1201,1225]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="66" pageNumber="2848" phylum="Firmicutes" rank="species" species="garii">
