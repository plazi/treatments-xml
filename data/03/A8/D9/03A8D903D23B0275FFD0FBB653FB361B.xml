<emphasis box="[236,494,1169,1193]" italics="true" pageId="52" pageNumber="2834">
<taxonomicName authorityName="Beijerinck" authorityYear="1901" box="[236,373,1169,1193]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="52" pageNumber="2834" phylum="Firmicutes" rank="genus">Lactobacillus</taxonomicName>
