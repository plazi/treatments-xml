<taxonomicName authorityName="Purwaningsih &amp; Smales" authorityYear="2010" box="[915,1101,975,1000]" class="Mammalia" family="Macropodidae" genus="Dorcopsis" kingdom="Animalia" order="Diprotodontia" pageId="7" pageNumber="440" phylum="Chordata" rank="species" species="salawatiensis">
