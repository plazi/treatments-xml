), with 
<taxonomicName authority="NHMUK" authorityName="NHMUK" box="[410,786,940,962]" class="Aves" family="Struthionidae" genus="Struthio" kingdom="Animalia" order="Struthioniformes" pageId="9" pageNumber="97" phylum="Chordata" rank="subSpecies" species="camelus" subSpecies="australis">
