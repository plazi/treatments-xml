<quantity box="[618,698,1138,1165]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="39" pageNumber="40" unit="m" value="100.0">100 m</quantity>
to 
<quantity box="[750,843,1137,1165]" metricMagnitude="3" metricUnit="m" metricValue="5.5" pageId="39" pageNumber="40" unit="m" value="5500.0">5500 m</quantity>
depths (
<bibRefCitation author="Molodtsova, T. N." box="[955,1174,1138,1165]" pageId="39" pageNumber="40" pagination="141 - 151" refId="ref48053" refString="Molodtsova, T. N. (2006) Black corals (Antipatharia: Anthozoa: Cnidaria) of the north-eastern Atlantic. In: Biogeography of the Atlantic Seamounts. KMK Press, Moscow, pp. 141 - 151." type="book chapter" year="2006">Molodtsova, 2006</bibRefCitation>
); off Falkland island, 
<geoCoordinate box="[151,305,1173,1200]" degrees="54" direction="south" minutes="25" orientation="latitude" pageId="39" pageNumber="40" precision="1" seconds="0.12" value="-54.416702">54º25’0.12”S</geoCoordinate>
, 
<geoCoordinate box="[316,492,1173,1201]" degrees="57" direction="west" minutes="31" orientation="longitude" pageId="39" pageNumber="40" precision="1" seconds="59.88" value="-57.5333">57º31’59.88”W</geoCoordinate>
