<caption box="[151,1435,1912,1936]" pageId="27" pageNumber="28" startId="27.[151,255,1912,1936]" targetBox="[209,1378,469,1894]" targetPageId="27">
<paragraph blockId="27.[151,1435,1912,1936]" box="[151,1435,1912,1936]" pageId="27" pageNumber="28">
