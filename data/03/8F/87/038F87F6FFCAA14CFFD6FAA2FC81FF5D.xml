<taxonomicName authority="FROM" authorityName="FROM" box="[257,462,1325,1351]" class="Magnoliopsida" family="Cunoniaceae" genus="Geissois" kingdom="Plantae" order="Oxalidales" pageId="4" pageNumber="315" phylum="Tracheophyta" rank="genus">
