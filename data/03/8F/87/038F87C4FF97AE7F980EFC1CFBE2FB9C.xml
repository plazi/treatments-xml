<taxonomicName authority="(Papp)" baseAuthorityName="Papp" baseAuthorityYear="1971" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="30" phylum="Arthropoda" rank="species" species="angustatus" subGenus="Aleiodes">
