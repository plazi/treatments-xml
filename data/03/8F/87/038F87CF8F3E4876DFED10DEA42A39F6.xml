<emphasis box="[1050,1160,1866,1892]" italics="true" pageId="41" pageNumber="40">C. adamsi</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Broun" authorityYear="1908" box="[1175,1309,1866,1892]" class="Insecta" family="Carabidae" genus="Ctenognathus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="40" phylum="Arthropoda" rank="species" species="littorellus">
<emphasis box="[1175,1309,1866,1892]" italics="true" pageId="41" pageNumber="40">C. littorellus</emphasis>
</taxonomicName>
; list), 2016: 36–37 (as 
<taxonomicName authorityName=": Broun" authorityYear="1893" box="[305,416,1900,1926]" class="Insecta" family="Carabidae" genus="Ctenognathus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="40" phylum="Arthropoda" rank="species" species="adamsi">
<emphasis box="[305,416,1900,1926]" italics="true" pageId="41" pageNumber="40">C. adamsi</emphasis>
