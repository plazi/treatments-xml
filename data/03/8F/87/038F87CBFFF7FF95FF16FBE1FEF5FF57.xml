</emphasis>
<taxonomicName authorityName=", Oke" authorityYear="1928" box="[634,817,1764,1788]" class="Insecta" family="Staphylinidae" genus="Anabaxis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="536" phylum="Arthropoda" rank="species" species="brevis">
