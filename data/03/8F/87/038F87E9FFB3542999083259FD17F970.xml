<taxonomicName authority="Howard, 1895" authorityName="Howard" authorityYear="1895" box="[466,630,1684,1708]" class="Insecta" family="Aphelinidae" genus="Physcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="369" phylum="Arthropoda" rank="species" species="flavoflagellatus">
