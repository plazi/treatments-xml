<taxonomicName authority="(De Santis)" authorityName="Howard" authorityYear="1895" baseAuthorityName="De Santis" box="[558,892,1651,1676]" class="Insecta" family="Aphelinidae" genus="Physcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="369" phylum="Arthropoda" rank="species" species="flavoflagellatus">
