<collectionCode box="[1055,1139,907,933]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="25" pageNumber="26" type="Museum">RMNH</collectionCode>
3327 (actually 
<taxonomicName baseAuthorityName="Valenciennes" baseAuthorityYear="1833" box="[1317,1444,907,933]" class="Reptilia" family="Geoemydidae" genus="Mauremys" kingdom="Animalia" order="Testudines" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="rivulata">
<emphasis box="[1317,1444,907,933]" italics="true" pageId="25" pageNumber="26">M. rivulata</emphasis>
</taxonomicName>
