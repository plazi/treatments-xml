<taxonomicName baseAuthorityName="Valenciennes" baseAuthorityYear="1833" box="[941,1159,875,901]" class="Reptilia" family="Geoemydidae" genus="Mauremys" kingdom="Animalia" order="Testudines" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="rivulata">
