<number box="[1184,1213,748,774]" pageId="6" pageNumber="7" value="40.0">40</number>
(
<number box="[1238,1314,748,774]" pageId="6" pageNumber="7" value="40.0" valueMax="42.0" valueMin="38.0">38–42</number>
) projecting forwards. 
<emphasis bold="true" box="[509,645,788,814]" pageId="6" pageNumber="7">Coxal area</emphasis>
with short lines; anterolateral setae on coxisternum I (
<emphasis box="[1284,1312,788,813]" italics="true" pageId="6" pageNumber="7">
<number box="[1284,1297,788,813]" pageId="6" pageNumber="7" value="1.0">1</number>
b
</emphasis>
) 
<number box="[264,293,828,854]" pageId="6" pageNumber="7" value="15.0">15</number>
(
