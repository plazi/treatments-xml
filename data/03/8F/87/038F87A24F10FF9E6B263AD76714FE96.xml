<number box="[728,772,628,654]" pageId="6" pageNumber="7" value="266.0">266</number>
(
<number box="[789,894,628,654]" pageId="6" pageNumber="7" value="266.0" valueMax="270.0" valueMin="262.0">262–270</number>
) long, 
<number box="[980,1009,628,654]" pageId="6" pageNumber="7" value="70.0">70</number>
(
<number box="[1027,1103,628,654]" pageId="6" pageNumber="7" value="69.5" valueMax="71.0" valueMin="68.0">68–71</number>
) wide, 
<number box="[1194,1223,628,654]" pageId="6" pageNumber="7" value="78.0">78</number>
(
<number box="[1240,1317,628,654]" pageId="6" pageNumber="7" value="78.0" valueMax="80.0" valueMin="76.0">76–80</number>
) thick; light yellow in color. Gnathosoma 
<number box="[766,795,668,694]" pageId="6" pageNumber="7" value="51.0">51</number>
(
<number box="[814,892,668,694]" pageId="6" pageNumber="7" value="51.5" valueMax="53.0" valueMin="50.0">50–53</number>
), projecting downwards. 
<emphasis bold="true" pageId="6" pageNumber="7">Prodorsal shield</emphasis>
<number box="[350,379,708,734]" pageId="6" pageNumber="7" value="33.0">33</number>
(
<number box="[400,476,708,734]" pageId="6" pageNumber="7" value="33.5" valueMax="35.0" valueMin="32.0">32–35</number>
) long, 
<number box="[570,599,708,734]" pageId="6" pageNumber="7" value="45.0">45</number>
(
<number box="[620,696,708,734]" pageId="6" pageNumber="7" value="44.5" valueMax="46.0" valueMin="43.0">43–46</number>
) wide, subtriangular; frontal lobe present; smooth. Dorsal tubercles on rear margin, 
<number box="[696,725,748,774]" pageId="6" pageNumber="7" value="25.0">25</number>
(
<number box="[750,827,748,774]" pageId="6" pageNumber="7" value="24.5" valueMax="26.0" valueMin="23.0">23–26</number>
) apart, scapular setae (
<emphasis box="[1133,1157,748,773]" italics="true" pageId="6" pageNumber="7">sc</emphasis>
