; 
<bibRefCitation author="Opresko, D. M." box="[652,824,1932,1958]" pageId="11" pageNumber="12" pagination="359 - 363" refId="ref49123" refString="Opresko, D. M. (2009) Antipatharia (Cnidaria) of the Gulf of Mexico. Gulf of Mexico Origin, Waters, and Biota: Biodiversity, 1, 359 - 363." type="journal article" year="2009">Opresko, 2009</bibRefCitation>
), off 
<collectingRegion box="[887,1030,1932,1958]" country="Brazil" name="Pernambuco" pageId="11" pageNumber="12">Pernambuco</collectingRegion>
, 
<collectingCountry box="[1043,1111,1932,1958]" name="Brazil" pageId="11" pageNumber="12">Brazil</collectingCountry>
(
<bibRefCitation author="Cordeiro, R. T. S. &amp; Maranhao, H. A. &amp; Lima, S. T. S. &amp; Perez, C. D." box="[1128,1374,1932,1959]" pageId="11" pageNumber="12" pagination="826 - 828" refId="ref46754" refString="Cordeiro, R. T. S., Maranhao, H. A., Lima, S. T. S. &amp; Perez, C. D. (2012) First record of Stichopathes occidentalis (Gray, 1860) and range extensions of Antipathes atlantica Gray, 1857 (Cnidaria: Anthozoa: Antipatharia) in the southwestern Atlantic Ocean. Check List, 8 (4), 826 - 828. https: // doi. org / 10.15560 / 8.4.826" type="journal article" year="2012">
Cordeiro 
<emphasis box="[1239,1297,1932,1958]" italics="true" pageId="11" pageNumber="12">et al.</emphasis>
, 2012
</bibRefCitation>
); and 
<collectingCountry box="[151,343,1968,1994]" name="Ascension Island" pageId="11" pageNumber="12">Ascension Island</collectingCountry>
