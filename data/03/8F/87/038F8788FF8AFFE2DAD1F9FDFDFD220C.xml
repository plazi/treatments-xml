<taxonomicName authorityName="Anderson" authorityYear="1879" class="Reptilia" family="Emydidae" genus="Emys" kingdom="Animalia" order="Testudines" pageId="28" pageNumber="29" phylum="Chordata" rank="variety" species="trijuga" variety="burmana">
<emphasis italics="true" pageId="28" pageNumber="29">Emys trijuga</emphasis>
