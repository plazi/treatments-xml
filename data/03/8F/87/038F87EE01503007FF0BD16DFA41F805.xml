<quantity box="[375,478,1510,1531]" metricMagnitude="-2" metricUnit="m" metricValue="3.65" metricValueMax="5.5" metricValueMin="1.8" pageId="1" pageNumber="505" unit="mm" value="36.5" valueMax="55.0" valueMin="18.0">18–55 mm</quantity>
