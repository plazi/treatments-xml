; 
<bibRefCitation author="Loiola, L. L." box="[728,876,404,431]" pageId="14" pageNumber="15" pagination="253 - 264" refId="ref47648" refString="Loiola, L. L. (2007) Black corals (Cnidaria: Antipatharia) from Brazil: an overview. Bulletin of Marine Science, 81 (3), 253 - 264." type="journal article" year="2007">Loiola, 2007</bibRefCitation>
; 
<bibRefCitation author="Opresko, D. M." box="[890,1059,404,431]" pageId="14" pageNumber="15" pagination="359 - 363" refId="ref49123" refString="Opresko, D. M. (2009) Antipatharia (Cnidaria) of the Gulf of Mexico. Gulf of Mexico Origin, Waters, and Biota: Biodiversity, 1, 359 - 363." type="journal article" year="2009">Opresko, 2009</bibRefCitation>
) and off 
<collectingCountry box="[1166,1234,404,430]" name="Brazil" pageId="14" pageNumber="15">Brazil</collectingCountry>
(fig. 35) (
<bibRefCitation author="Loiola, L. L." pageId="14" pageNumber="15" pagination="253 - 264" refId="ref47648" refString="Loiola, L. L. (2007) Black corals (Cnidaria: Antipatharia) from Brazil: an overview. Bulletin of Marine Science, 81 (3), 253 - 264." type="journal article" year="2007">Loiola, 2007</bibRefCitation>
