<number box="[715,744,519,545]" pageId="4" pageNumber="5" value="90.0">90</number>
(
<number box="[768,844,519,545]" pageId="4" pageNumber="5" value="89.5" valueMax="92.0" valueMin="87.0">87–92</number>
) wide, sub quadrangular, median line complete, admedian lines incomplete, submedian lines form network anterior. Dorsal tubercles near rear margin, 
<number box="[601,630,599,625]" pageId="4" pageNumber="5" value="58.0">58</number>
(
<number box="[651,726,599,625]" pageId="4" pageNumber="5" value="57.5" valueMax="60.0" valueMin="55.0">55–60</number>
