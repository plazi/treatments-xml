<number box="[899,928,479,505]" pageId="4" pageNumber="5" value="72.0">72</number>
(
<number box="[948,1026,479,505]" pageId="4" pageNumber="5" value="72.5" valueMax="75.0" valueMin="70.0">70–75</number>
