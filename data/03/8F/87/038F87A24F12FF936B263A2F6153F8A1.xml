<number box="[1281,1325,439,465]" pageId="4" pageNumber="5" value="130.0">130</number>
(
<number box="[272,378,479,505]" pageId="4" pageNumber="5" value="128.5" valueMax="132.0" valueMin="125.0">125–132</number>
