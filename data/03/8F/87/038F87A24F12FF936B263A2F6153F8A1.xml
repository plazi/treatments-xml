<emphasis bold="true" box="[264,475,518,544]" pageId="4" pageNumber="5">Prodorsal shield</emphasis>
<number box="[490,519,519,545]" pageId="4" pageNumber="5" value="82.0">82</number>
(
<number box="[542,618,519,545]" pageId="4" pageNumber="5" value="82.5" valueMax="85.0" valueMin="80.0">80–85</number>
