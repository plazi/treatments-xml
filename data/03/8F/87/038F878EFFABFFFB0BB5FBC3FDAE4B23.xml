<taxonomicName box="[284,480,1953,1975]" class="Ascidiacea" family="Ascidiidae" genus="Ascidia" kingdom="Animalia" order="Enterogona" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="corallicola" status="sp. nov.">
<emphasis box="[284,480,1953,1975]" italics="true" pageId="11" pageNumber="12">
<taxonomicName box="[284,360,1953,1975]" class="Ascidiacea" family="Ascidiidae" genus="Ascidia" higherTaxonomySource="CoL" kingdom="Animalia" order="Phlebobranchia" pageId="11" pageNumber="12" phylum="Chordata" rank="genus">Ascidia</taxonomicName>
<taxonomicName box="[369,480,1953,1975]" pageId="11" pageNumber="12" rank="species" species="corallicola" status="sp. nov.">corallicola</taxonomicName>
