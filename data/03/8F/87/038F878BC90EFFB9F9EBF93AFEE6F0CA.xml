<bibRefCitation author="Loiola, L. L. &amp; Castro, C. B." pageId="32" pageNumber="33" pagination="1 - 31" refId="ref47732" refString="Loiola, L. L. &amp; Castro, C. B. (2005) Tanacetipathes Opresko, 2001 (Cnidaria: Antipatharia: Myriopathidae) from Brazil, including two new species. Zootaxa, 1081 (1), 1 - 31. https: // doi. org / 10.11646 / zootaxa. 1081.1.1" type="journal article" year="2005">Loiola &amp; Castro, 2005</bibRefCitation>
) and Cadeia Vitória Trindade (
<bibRefCitation author="Loiola, L. L. &amp; Castro, C. B." box="[559,817,656,683]" pageId="32" pageNumber="33" pagination="1 - 31" refId="ref47732" refString="Loiola, L. L. &amp; Castro, C. B. (2005) Tanacetipathes Opresko, 2001 (Cnidaria: Antipatharia: Myriopathidae) from Brazil, including two new species. Zootaxa, 1081 (1), 1 - 31. https: // doi. org / 10.11646 / zootaxa. 1081.1.1" type="journal article" year="2005">Loiola &amp; Castro, 2005</bibRefCitation>
) (
<figureCitation box="[841,925,657,683]" captionStart="FIGURE 18" captionStartId="31.[151,250,1831,1856]" captionTargetBox="[151,1436,1165,1787]" captionTargetId="figure@31.[151,1436,1165,1787]" captionTargetPageId="31" captionText="FIGURE 18. Geographic distribution of Tanacetipathes thamnea (Warner, 1981). Yellow circles: records extracted from the literature (Warner, 1981; Loiola &amp; Castro, 2005; Opresko, 2009) and based on Ocean Biogeographic Information System (OBIS). Red circle: record made in the present work." figureDoi="http://doi.org/10.5281/zenodo.3528980" httpUri="https://zenodo.org/record/3528980/files/figure.png" pageId="32" pageNumber="33">Fig. 18</figureCitation>
); from 
<quantity box="[1010,1068,657,683]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="32" pageNumber="33" unit="m" value="30.0">30 m</quantity>
(
<bibRefCitation author="Warner, G. F." box="[1085,1237,657,683]" pageId="32" pageNumber="33" pagination="147 - 163" refId="ref50460" refString="Warner, G. F. (1981) Species descriptions and ecological observations of black corals (Antipatharia) from Trinidad. Bulletin of Marine Science, 31 (1), 147 - 163." type="journal article" year="1981">Warner, 1981</bibRefCitation>
) to 
<quantity box="[1283,1355,656,683]" metricMagnitude="2" metricUnit="m" metricValue="5.58" pageId="32" pageNumber="33" unit="m" value="558.0">558 m</quantity>
