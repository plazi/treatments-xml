<location LSID="urn:lsid:plazi:treatment:038F87C4FF93AE7B9884FC33FBE2FBBF:8EF96009FF93AE789B08FA49FCCDFA14" box="[585,841,1507,1529]" municipality="Hungarian Natural History Museum" name="Department of Zoology" pageId="12" pageNumber="33" stateProvince="Budapest">Department of Zoology</location>
), 
