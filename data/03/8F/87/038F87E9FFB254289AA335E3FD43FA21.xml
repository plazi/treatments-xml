<paragraph blockId="13.[143,1104,1403,1530]" pageId="13" pageNumber="370">
<taxonomicName authorityName="Huang" authorityYear="1994" box="[143,443,1443,1467]" class="Insecta" family="Aphelinidae" genus="Coccobius" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="370" phylum="Arthropoda" rank="species" species="longifuniculatus">
<emphasis box="[143,443,1443,1467]" italics="true" pageId="13" pageNumber="370">Coccobius longifuniculatus</emphasis>
</taxonomicName>
