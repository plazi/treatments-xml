<taxonomicName authority="(Gahan)" baseAuthorityName="Gahan" baseAuthorityYear="1927" box="[576,851,1443,1467]" class="Insecta" family="Aphelinidae" genus="Coccobius" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="370" phylum="Arthropoda" rank="species" species="intermedius">
