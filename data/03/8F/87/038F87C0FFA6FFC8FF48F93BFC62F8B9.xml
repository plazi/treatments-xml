<taxonomicName authority="(Shaw, 1802)" baseAuthorityName="Shaw" baseAuthorityYear="1802" box="[151,586,1678,1703]" class="Reptilia" family="Elapidae" genus="Hydrophis" kingdom="Animalia" order="Squamata" pageId="2" pageNumber="353" phylum="Chordata" rank="species" species="caerulescens">
