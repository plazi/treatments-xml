<collectingRegion box="[151,235,1416,1439]" country="American Samoa" name="Eastern" pageId="8" pageNumber="589">Eastern</collectingRegion>
<collectingRegion box="[243,360,1414,1439]" country="Brazil" name="Sao Paulo" pageId="8" pageNumber="589">São Paulo</collectingRegion>
