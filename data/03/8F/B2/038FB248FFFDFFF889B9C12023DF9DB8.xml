<emphasis box="[237,425,764,789]" italics="true" pageId="78" pageNumber="79">Perca flavescens</emphasis>
</taxonomicName>
(5, 6, 10); 
<taxonomicName box="[549,777,764,789]" class="Actinopterygii" family="Salmonidae" genus="Salvelinus" kingdom="Animalia" order="Salmoniformes" pageId="78" pageNumber="79" phylum="Chordata" rank="species" species="fontinalis">
