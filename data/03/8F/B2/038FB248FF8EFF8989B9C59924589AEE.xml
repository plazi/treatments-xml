<taxonomicName authority="Diesing, 1851" authorityName="Diesing" authorityYear="1851" box="[151,461,906,932]" class="Secernentea" family="Camallanidae" genus="Oncophora" kingdom="Animalia" order="Camallanida" pageId="63" pageNumber="64" phylum="Nematoda" rank="genus">
