<paragraph blockId="162.[151,1437,151,680]" box="[199,468,548,573]" pageId="162" pageNumber="163">
<taxonomicName box="[199,428,548,573]" class="Actinopterygii" family="Sebastidae" genus="Sebastes" kingdom="Animalia" order="Scorpaeniformes" pageId="162" pageNumber="163" phylum="Chordata" rank="species" species="ruberrimus">
