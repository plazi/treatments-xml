(6); 
<taxonomicName box="[535,754,1794,1819]" class="Actinopterygii" family="Salmonidae" genus="Oncorhynchus" kingdom="Animalia" order="Salmoniformes" pageId="104" pageNumber="105" phylum="Chordata" rank="species" species="keta">
<emphasis box="[535,754,1794,1819]" italics="true" pageId="104" pageNumber="105">Oncorhynchus keta</emphasis>
</taxonomicName>
(4, 6); 
<taxonomicName box="[842,1093,1794,1819]" class="Actinopterygii" family="Salmonidae" genus="Oncorhynchus" kingdom="Animalia" order="Salmoniformes" pageId="104" pageNumber="105" phylum="Chordata" rank="species" species="kisutch">
