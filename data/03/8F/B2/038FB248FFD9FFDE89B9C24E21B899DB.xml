(1, 2, 3, 4, 5); 
<taxonomicName box="[1273,1436,1794,1819]" class="Actinopterygii" family="Salmonidae" genus="Oncorhynchus" kingdom="Animalia" order="Salmoniformes" pageId="104" pageNumber="105" phylum="Chordata" rank="genus">
<emphasis box="[1273,1436,1794,1819]" italics="true" pageId="104" pageNumber="105">Oncorhynchus</emphasis>
</taxonomicName>
</paragraph>
<paragraph blockId="104.[151,1437,1254,1963]" box="[199,375,1830,1855]" pageId="104" pageNumber="105">
<taxonomicName box="[199,336,1830,1855]" class="Actinopterygii" family="Salmonidae" genus="Oncorhynchus" kingdom="Animalia" order="Salmoniformes" pageId="104" pageNumber="105" phylum="Chordata" rank="species" species="tshawytscha">
