<emphasis box="[414,599,1255,1280]" italics="true" pageId="109" pageNumber="110">Urophycis chuss</emphasis>
</taxonomicName>
; 
<taxonomicName box="[614,804,1255,1280]" class="Actinopterygii" family="Phycidae" genus="Urophycis" kingdom="Animalia" order="Gadiformes" pageId="109" pageNumber="110" phylum="Chordata" rank="species" species="tenuis">
<emphasis box="[614,804,1255,1280]" italics="true" pageId="109" pageNumber="110">Urophycis tenuis</emphasis>
