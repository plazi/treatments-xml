<taxonomicName authority="Tornquist, 1931" authorityName="Tornquist" authorityYear="1931" box="[271,525,441,463]" class="Chromadorea" family="Cucullanidae" genus="Dacnitis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="150" pageNumber="151" phylum="Nematoda" rank="genus">
