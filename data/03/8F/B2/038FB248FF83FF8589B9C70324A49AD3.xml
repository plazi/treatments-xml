<taxonomicName authority="Uhazy, 1976" authorityName="Uhazy" authorityYear="1976" box="[151,635,272,298]" class="Chromadorea" family="Philometridae" genus="Philometroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="50" pageNumber="51" phylum="Nematoda" rank="species" species="huronensis">
<emphasis bold="true" box="[151,635,272,298]" pageId="50" pageNumber="51">
<emphasis bold="true" box="[151,476,272,298]" italics="true" pageId="50" pageNumber="51">Philometroides huronensis</emphasis>
