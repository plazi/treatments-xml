<emphasis box="[478,708,1334,1357]" italics="true" pageId="143" pageNumber="144">Leptocottus armatus</emphasis>
</taxonomicName>
; 
<taxonomicName box="[722,950,1333,1358]" class="Actinopterygii" family="Hexagrammidae" genus="Ophiodon" kingdom="Animalia" order="Scorpaeniformes" pageId="143" pageNumber="144" phylum="Chordata" rank="species" species="elongatus">
