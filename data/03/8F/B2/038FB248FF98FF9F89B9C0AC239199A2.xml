<taxonomicName authority="Yamaguti, 1935" authorityName="Yamaguti" authorityYear="1935" box="[151,482,1727,1753]" class="Chromadorea" family="Philometridae" genus="Clavinema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="41" pageNumber="42" phylum="Nematoda" rank="genus">
