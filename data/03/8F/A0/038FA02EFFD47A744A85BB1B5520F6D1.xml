<paragraph blockId="72.[151,1436,780,1439]" pageId="72" pageNumber="73">
<emphasis bold="true" box="[151,269,1342,1367]" pageId="72" pageNumber="73">Remarks:</emphasis>
<emphasis box="[276,679,1342,1367]" italics="true" pageId="72" pageNumber="73">
<taxonomicName box="[276,628,1342,1367]" class="Heterotardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="72" pageNumber="73" phylum="Tardigrada" rank="subSpecies" species="occidentalis" subSpecies="sensu">
