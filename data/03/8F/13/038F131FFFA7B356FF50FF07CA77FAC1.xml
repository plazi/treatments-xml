<subSubSection box="[199,956,366,391]" pageId="2" pageNumber="560" type="etymology">
<paragraph blockId="2.[151,1437,258,1327]" box="[199,956,366,391]" pageId="2" pageNumber="560">
<emphasis bold="true" box="[199,334,366,391]" pageId="2" pageNumber="560">Etymology.</emphasis>
