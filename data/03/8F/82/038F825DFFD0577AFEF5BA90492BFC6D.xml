<number box="[1219,1281,284,310]" pageId="7" pageNumber="8" value="2000.0">2000</number>
</date>
, ex egg of 
<taxonomicName box="[351,550,324,349]" class="Insecta" family="Hesperiidae" genus="Calpodes" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="ethlius">
<emphasis box="[351,550,324,349]" italics="true" pageId="7" pageNumber="8">Calpodes ethlius</emphasis>
</taxonomicName>
on 
<taxonomicName box="[599,677,324,349]" class="Liliopsida" family="Cannaceae" genus="Canna" kingdom="Plantae" order="Zingiberales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="genus">
