<caption ID-Table-UUID="DF596182FFF23B1EFF448D9FFA20CC81" box="[151,1436,935,959]" httpUri="http://table.plazi.org/id/DF596182FFF23B1EFF448D9FFA20CC81" pageId="4" pageNumber="47" targetBox="[151,1436,1061,1809]" targetIsTable="true" targetPageId="4">
<paragraph blockId="4.[151,1436,935,1041]" box="[151,1436,935,959]" pageId="4" pageNumber="47">
