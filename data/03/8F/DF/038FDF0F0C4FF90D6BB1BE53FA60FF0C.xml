; Transcaucasia; 
<collectingCountry box="[875,1006,1939,1965]" name="Turkey" pageId="3" pageNumber="504">Asia Minor</collectingCountry>
; 
<collectingCountry box="[1016,1086,1940,1966]" name="Egypt" pageId="3" pageNumber="504">Egypt</collectingCountry>
, 
<collectingCountry box="[1095,1157,1939,1965]" name="Syria" pageId="3" pageNumber="504">Syria</collectingCountry>
, 
<collectingCountry box="[1167,1232,1939,1965]" name="Libya" pageId="3" pageNumber="504">Libya</collectingCountry>
