<subSubSection lastPageId="3" lastPageNumber="58" pageId="0" pageNumber="55" type="description">
<paragraph blockId="0.[151,1436,1745,2012]" pageId="0" pageNumber="55">
<emphasis bold="true" box="[151,214,1745,1771]" pageId="0" pageNumber="55">Male</emphasis>
. Body length: 
<quantity box="[390,476,1746,1772]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" pageId="0" pageNumber="55" unit="mm" value="16.0">16 mm</quantity>
– 
<quantity box="[491,574,1746,1772]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="0" pageNumber="55" unit="mm" value="19.0">19 mm</quantity>
; pronotum length: 
<quantity box="[799,943,1746,1772]" metricMagnitude="-3" metricUnit="m" metricValue="3.55" metricValueMax="3.7" metricValueMin="3.4" pageId="0" pageNumber="55" unit="mm" value="3.55" valueMax="3.7" valueMin="3.4">3.4–3.7 mm</quantity>
; tegmina length: 
<quantity box="[1148,1293,1746,1772]" metricMagnitude="-3" metricUnit="m" metricValue="3.05" metricValueMax="3.3" metricValueMin="2.8" pageId="0" pageNumber="55" unit="mm" value="3.05" valueMax="3.3" valueMin="2.8">2.8–3.3 mm</quantity>
; hind femur length: 
<quantity box="[239,411,1786,1812]" metricMagnitude="-2" metricUnit="m" metricValue="1.135" metricValueMax="1.15" metricValueMin="1.1199999999999999" pageId="0" pageNumber="55" unit="mm" value="11.35" valueMax="11.5" valueMin="11.2">11.2–11.5 mm</quantity>
