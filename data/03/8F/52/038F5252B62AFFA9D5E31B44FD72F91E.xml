<subSubSection pageId="3" pageNumber="2" type="etymology">
<paragraph blockId="3.[192,1440,1634,1690]" pageId="3" pageNumber="2">
<emphasis bold="true" box="[192,348,1634,1658]" pageId="3" pageNumber="2">Etymology.</emphasis>
