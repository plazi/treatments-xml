<taxonomicName authority="(Bruzelius, 1805)" baseAuthorityName="Bruzelius" baseAuthorityYear="1805" box="[1034,1420,721,749]" class="Ophiuroidea" family="Ophiodermatidae" genus="Ophioderma" kingdom="Animalia" order="Ophiurida" pageId="6" pageNumber="7" phylum="Echinodermata" rank="species" species="longicauda">
<emphasis box="[1034,1055,721,749]" italics="true" pageId="6" pageNumber="7">O</emphasis>
. 
<emphasis box="[1070,1202,721,749]" italics="true" pageId="6" pageNumber="7">longicauda</emphasis>
(
<bibRefCitation author="Bruzelius" box="[1218,1411,721,749]" firstAuthor="Bruzelius" journalOrPublisher="Berlingianis, Lund" pageId="6" pageNumber="7" refId="ref18680" refString="Bruzelius N. 1805. Dissertatio sistens species cognitas asteriarum, quamr. sub praesidio D. M. and J. Retzii, pro laurea modeste exhibet Nicolaus Bruzelius. Berlingianis, Lund." title="Dissertatio sistens species cognitas asteriarum, quamr. sub praesidio D. M. and J. Retzii, pro laurea modeste exhibet Nicolaus Bruzelius" type="book" year="1805">Bruzelius, 1805</bibRefCitation>
)
</taxonomicName>
. For 
<taxonomicName authority="Greeff, 1882" authorityName="Greeff" authorityYear="1882" box="[1480,1794,721,749]" class="Ophiuroidea" family="Ophiodermatidae" genus="Ophioderma" kingdom="Animalia" order="Ophiurida" pageId="6" pageNumber="7" phylum="Echinodermata" rank="species" species="guineense">
<emphasis box="[1480,1501,721,749]" italics="true" pageId="6" pageNumber="7">O</emphasis>
. 
<emphasis box="[1516,1633,721,749]" italics="true" pageId="6" pageNumber="7">guineense</emphasis>
<bibRefCitation author="Greeff" box="[1642,1794,721,749]" firstAuthor="Greeff" journalOrPublisher="Zoologischer Anzeiger" pageId="6" pageNumber="7" pagination="156 - 159" part="107" refId="ref18939" refString="Greeff R. 1882. Echinodermen beobachtet auf einer Reise nach der Guinea-Insel Sao-Thome. Zoologischer Anzeiger 107: 156 - 159." title="Echinodermen beobachtet auf einer Reise nach der Guinea-Insel Sao-Thome" type="journal article" year="1882">Greeff, 1882</bibRefCitation>
</taxonomicName>
