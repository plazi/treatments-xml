</taxonomicName>
; 
<taxonomicName authority="Lordello, 1955" authorityName="Lordello" authorityYear="1955" box="[613,905,296,321]" class="Adenophorea" family="Leptonchidae" genus="Proleptonchus" kingdom="Animalia" order="Dorylaimida" pageId="14" pageNumber="2004" phylum="Nematoda" rank="species" species="aestivus">
