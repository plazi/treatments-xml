<subSubSection pageId="1" pageNumber="96" type="type_taxon">
<paragraph blockId="1.[126,764,989,1039]" pageId="1" pageNumber="96">
<typeStatus box="[126,175,989,1010]" pageId="1" pageNumber="96">Type</typeStatus>
species. – 
<taxonomicName authority="Philippi, 1846" authorityName="Philippi" authorityYear="1846" box="[296,638,989,1010]" class="Gastropoda" family="Helicidae" genus="Helix" kingdom="Animalia" order="Stylommatophora" pageId="1" pageNumber="96" phylum="Mollusca" rank="species" species="resplendens">
