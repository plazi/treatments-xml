<caption ID-Table-UUID="DF6C1D9CFFD7FFC22090FE989D3EDC4C" httpUri="http://table.plazi.org/id/DF6C1D9CFFD7FFC22090FE989D3EDC4C" pageId="37" pageNumber="38" startId="37.[189,258,265,291]" targetBox="[208,1371,369,461]" targetIsTable="true" targetPageId="37">
<paragraph blockId="37.[189,1398,265,327]" pageId="37" pageNumber="38">
