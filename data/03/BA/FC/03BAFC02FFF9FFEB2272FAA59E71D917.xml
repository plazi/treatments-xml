<subSubSection pageId="11" pageNumber="12" type="diagnosis">
<paragraph blockId="11.[189,310,1511,1537]" box="[189,310,1511,1537]" pageId="11" pageNumber="12">
<heading bold="true" box="[189,310,1511,1537]" fontSize="11" level="3" pageId="11" pageNumber="12" reason="3">
