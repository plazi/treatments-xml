<subSubSection pageId="8" pageNumber="238" type="discussion">
<paragraph blockId="8.[151,1435,958,1091]" pageId="8" pageNumber="238">
<emphasis bold="true" box="[199,304,1030,1055]" pageId="8" pageNumber="238">Remark.</emphasis>
