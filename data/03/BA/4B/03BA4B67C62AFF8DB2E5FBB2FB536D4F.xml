May
</date>
, 
<date pageId="8" pageNumber="61" value="1930-05">
<number box="[1408,1436,1158,1182]" pageId="8" pageNumber="61" value="30.0">30</number>
May
</date>
, 
<date box="[214,481,1194,1218]" pageId="8" pageNumber="61" value="2009-07-09" valueMax="2009-07-21" valueMin="2009-07-09">
<number box="[214,228,1194,1218]" pageId="8" pageNumber="61" value="9.0">9</number>
July and 
<number box="[335,363,1194,1218]" pageId="8" pageNumber="61" value="21.0">21</number>
July 
<number box="[422,481,1194,1218]" pageId="8" pageNumber="61" value="2009.0">2009</number>
</date>
