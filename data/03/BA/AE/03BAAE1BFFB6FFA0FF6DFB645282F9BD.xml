, 
<collectingRegion box="[609,688,1429,1455]" country="Brazil" name="Parana" pageId="3" pageNumber="561">Paraná</collectingRegion>
, Santa Catarina, 
<collectingRegion box="[874,1082,1429,1456]" country="Brazil" name="Rio Grande do Sul" pageId="3" pageNumber="561">Rio Grande do Sul</collectingRegion>
