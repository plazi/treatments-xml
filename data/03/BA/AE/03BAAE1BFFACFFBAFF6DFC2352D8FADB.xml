<collectingCountry box="[1365,1436,1149,1175]" name="French Southern Territories" pageId="25" pageNumber="583">TAAF</collectingCountry>
MD55, st. 64 CB 105, 
<geoCoordinate box="[415,507,1186,1213]" degrees="23" direction="south" minutes="46" orientation="latitude" pageId="25" pageNumber="583" precision="925" value="-23.766666">23°46ʹS</geoCoordinate>
, 
<geoCoordinate box="[519,634,1186,1213]" degrees="042" direction="west" minutes="09" orientation="longitude" pageId="25" pageNumber="583" precision="925" value="-42.15">042°09ʹW</geoCoordinate>
, 
<quantity box="[646,777,1184,1211]" metricMagnitude="2" metricUnit="m" metricValue="6.01" metricValueMax="6.1" metricValueMin="5.92" pageId="25" pageNumber="583" unit="m" value="601.0" valueMax="610.0" valueMin="592.0">592–610 m</quantity>
