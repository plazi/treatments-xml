<collectingCountry box="[656,746,362,388]" name="Guyana" pageId="6" pageNumber="564">Guyana</collectingCountry>
, 
<collectingCountry box="[759,827,362,388]" name="Brazil" pageId="6" pageNumber="564">Brazil</collectingCountry>
(
<collectingRegion box="[844,940,362,388]" country="Brazil" name="Alagoas" pageId="6" pageNumber="564">Alagoas</collectingRegion>
