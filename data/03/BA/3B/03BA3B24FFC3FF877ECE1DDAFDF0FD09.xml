<emphasis box="[321,594,1964,1990]" italics="true" pageId="13" pageNumber="14">
Orbita 
<taxonomicName authority="Meng &amp; Wang" authorityName="Meng &amp; Wang" box="[411,792,1964,1991]" pageId="13" pageNumber="14" rank="species" species="parallelodroma">
<taxonomicName authority="Meng" authorityName="Meng" box="[411,674,1964,1991]" pageId="13" pageNumber="14" rank="species" species="parallelodroma">
