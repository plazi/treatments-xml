(
<number box="[378,467,1587,1611]" pageId="3" pageNumber="64" value="0.7" valueMax="0.9" valueMin="0.5">0.5–0.9</number>
), Tarsal claw 
<quantity box="[621,707,1587,1611]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="3" pageNumber="64" unit="mm" value="0.9">0.9 mm</quantity>
