). Abdomen: 5.0 mm (
<number box="[317,407,1551,1575]" pageId="3" pageNumber="64" value="5.05" valueMax="6.2" valueMin="3.9">3.9–6.2</number>
). Pedipalp: Femur 
<quantity box="[620,707,1551,1575]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="3" pageNumber="64" unit="mm" value="2.8">2.8 mm</quantity>
(
<number box="[723,811,1551,1575]" pageId="3" pageNumber="64" value="2.55" valueMax="3.5" valueMin="1.6">1.6–3.5</number>
), Tibia 
<quantity box="[898,984,1551,1575]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="3" pageNumber="64" unit="mm" value="2.9">2.9 mm</quantity>
(
<number box="[1000,1088,1551,1575]" pageId="3" pageNumber="64" value="2.6" valueMax="3.6" valueMin="1.6">1.6–3.6</number>
), Basitarsus 
<quantity box="[1232,1319,1551,1575]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="3" pageNumber="64" unit="mm" value="1.2">1.2 mm</quantity>
