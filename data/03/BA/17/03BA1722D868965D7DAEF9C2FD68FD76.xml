<subSubSection pageId="29" pageNumber="28" type="etymology">
<paragraph blockId="29.[192,1440,643,699]" pageId="29" pageNumber="28">
<emphasis bold="true" box="[192,343,643,667]" pageId="29" pageNumber="28">Etymology.</emphasis>
