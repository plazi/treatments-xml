<subSubSection box="[192,711,1219,1243]" pageId="73" pageNumber="72" type="type_taxon">
<paragraph blockId="73.[192,711,1219,1243]" box="[192,711,1219,1243]" pageId="73" pageNumber="72">
<emphasis bold="true" box="[192,365,1219,1243]" pageId="73" pageNumber="72">
<typeStatus box="[192,260,1219,1243]" pageId="73" pageNumber="72">Type</typeStatus>
species
</emphasis>
, 
<taxonomicName authority="Nielson, 2015" authorityName="Nielson" authorityYear="2015" box="[378,609,1219,1243]" class="Insecta" family="Cicadellidae" genus="Trinoridia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="73" pageNumber="72" phylum="Arthropoda" rank="species" species="calcaris" status="sp. nov.">
<emphasis box="[378,609,1219,1243]" italics="true" pageId="73" pageNumber="72">Trinoridia calcaris</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[622,711,1220,1243]" pageId="73" pageNumber="72" rank="species">sp. nov.</taxonomicNameLabel>
