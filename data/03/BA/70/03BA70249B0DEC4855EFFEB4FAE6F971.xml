, 
<specimenCount box="[880,917,1386,1412]" pageId="17" pageNumber="218" type="female">4♀</specimenCount>
, Fokien [= 
<collectingRegion box="[1042,1224,1385,1412]" country="China" name="Fujian" pageId="17" pageNumber="218">Fujian Province</collectingRegion>
