species:
</emphasis>
<taxonomicName authority="Clay, 1938" authorityName="in Clay" authorityYear="1938" box="[300,675,580,605]" class="Insecta" family="Philopteridae" genus="Oxylipeurus" kingdom="Animalia" order="Phthiraptera" pageId="28" pageNumber="229" phylum="Arthropoda" rank="species" species="tetraophasis">
