<collectingCountry box="[367,493,1467,1492]" name="Philippines" pageId="4">Philippines</collectingCountry>
(
<typeStatus box="[516,566,1467,1491]" pageId="4">type</typeStatus>
locality), 
<collectingRegion box="[697,750,1468,1491]" country="Indonesia" name="Jawa Timur" pageId="4">Java</collectingRegion>
, 
