<taxonomicName authority="(Haswell, 1885 a)" baseAuthorityName="Haswell" baseAuthorityYear="1885" box="[231,542,1357,1381]" class="Malacostraca" family="Caprellidae" genus="Paraproto" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="38" pageNumber="641" phylum="Arthropoda" rank="species" species="condylata">
