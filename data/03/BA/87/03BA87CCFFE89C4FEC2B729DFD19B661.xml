, 
<quantity box="[803,904,550,574]" metricMagnitude="-4" metricUnit="m" metricValue="3.3" pageId="38" pageNumber="641" unit="mm" value="0.33">0.33 mm</quantity>
; 
<emphasis box="[916,1037,550,574]" italics="true" pageId="38" pageNumber="641">pereonite 3</emphasis>
, 
<quantity box="[1051,1152,550,574]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" pageId="38" pageNumber="641" unit="mm" value="0.41">0.41 mm</quantity>
; 
<emphasis box="[160,279,584,608]" italics="true" pageId="38" pageNumber="641">pereonite 4</emphasis>
, 
<quantity box="[291,390,584,608]" metricMagnitude="-4" metricUnit="m" metricValue="4.3" pageId="38" pageNumber="641" unit="mm" value="0.43">0.43 mm</quantity>
; 
<emphasis box="[400,520,584,608]" italics="true" pageId="38" pageNumber="641">pereonite 5</emphasis>
, 
