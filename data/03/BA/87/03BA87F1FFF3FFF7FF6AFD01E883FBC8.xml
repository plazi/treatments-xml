<taxonomicName authorityName="Thorell" authorityYear="1887" box="[889,970,952,975]" class="Arachnida" family="Zodariidae" genus="Asceua" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="294" phylum="Arthropoda" rank="genus">
