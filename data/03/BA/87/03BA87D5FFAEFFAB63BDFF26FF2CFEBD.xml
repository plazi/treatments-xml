<collectingRegion box="[1208,1432,1073,1098]" country="Japan" name="Okinawa" pageId="6">Okinawa prefecture</collectingRegion>
, 
<collectingCountry box="[151,218,1110,1133]" name="Japan" pageId="6">Japan</collectingCountry>
, 
<collectingDate box="[229,367,1110,1134]" pageId="6" value="2000-05-01">
<date box="[229,367,1110,1134]" pageId="6" value="2000-05-01">1 May 2000</date>
