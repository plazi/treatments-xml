<taxonomicName authorityName="Varga" authorityYear="1972" class="Gastropoda" family="Pupinidae" genus="Pinteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="1" pageNumber="323" phylum="Mollusca" rank="species" species="croesus">
