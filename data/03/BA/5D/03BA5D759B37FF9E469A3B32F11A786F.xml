<paragraph blockId="20.[151,1436,757,854]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[151,273,757,782]" inLineHeading="true" pageId="20" pageNumber="21" reason="1">Diagnosis.</emphasis>
