<subSubSection box="[199,1167,151,176]" pageId="54" pageNumber="55" type="etymology">
<paragraph blockId="54.[199,1167,151,212]" box="[199,1167,151,176]" pageId="54" pageNumber="55">
<emphasis bold="true" box="[199,333,152,176]" inLineHeading="true" pageId="54" pageNumber="55" reason="1">Etymology.</emphasis>
