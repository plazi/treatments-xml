<subSubSection box="[189,1257,1298,1371]" pageId="62" pageNumber="63" type="discussion">
<paragraph blockId="62.[189,1257,1298,1371]" box="[189,307,1298,1324]" pageId="62" pageNumber="63">
<heading bold="true" box="[189,307,1298,1324]" fontSize="11" level="2" pageId="62" pageNumber="63" reason="6">
<emphasis bold="true" box="[189,307,1298,1324]" pageId="62" pageNumber="63">Variation</emphasis>
