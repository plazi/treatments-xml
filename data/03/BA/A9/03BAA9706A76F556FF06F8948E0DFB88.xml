<taxonomicName authorityName="Błażewicz-Paszkowycz" authorityYear="2007" baseAuthorityName="Kudinova-Pasternak" baseAuthorityYear="1978" box="[300,496,1809,1832]" class="Malacostraca" family="Typhlotanaidae" genus="Torquella" kingdom="Animalia" order="Tanaidacea" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="elegans">
