<subSubSection box="[203,667,838,865]" pageId="41" pageNumber="42" type="etymology">
<paragraph blockId="41.[151,1437,638,2025]" box="[203,667,838,865]" pageId="41" pageNumber="42">
<emphasis bold="true" box="[203,347,838,864]" pageId="41" pageNumber="42">Etymology:</emphasis>
