<emphasis bold="true" box="[151,295,1728,1752]" pageId="43" pageNumber="44">FIGURE 21.</emphasis>
<taxonomicName authority="Błażewicz-Paszkowycz, 2007" authorityName="Błażewicz-Paszkowycz" authorityYear="2007" box="[303,555,1728,1751]" class="Malacostraca" family="Typhlotanaidae" genus="Larsenotanais" kingdom="Animalia" order="Tanaidacea" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="amabilis" status="n. sp.">
<emphasis box="[303,555,1728,1751]" italics="true" pageId="43" pageNumber="44">Larsenotanais amabilis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[563,624,1728,1752]" pageId="43" pageNumber="44">
<taxonomicNameLabel box="[563,624,1728,1752]" pageId="43" pageNumber="44" rank="species">n. sp.</taxonomicNameLabel>
