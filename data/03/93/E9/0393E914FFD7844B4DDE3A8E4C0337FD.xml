<location LSID="urn:lsid:plazi:treatment:0393E914FFD7844B4DDE3A8E4C0337FD:8EE50ED9FFD7844B4F65383F4F1D3745" box="[556,600,550,575]" country="Serbia" county="Rtanj-Mirovo" municipality="Bogovinska" name="The" pageId="2" pageNumber="158" stateProvince="Zajecar">The</location>
