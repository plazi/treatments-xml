III 
<number box="[387,418,1123,1147]" pageId="3" pageNumber="4" value="44.0">44</number>
</date>
; distance between their bases 
<number box="[760,791,1123,1147]" pageId="3" pageNumber="4" value="21.0">21</number>
; postocular setae (outer) 
<number box="[1077,1107,1122,1147]" pageId="3" pageNumber="4" value="54.0">54</number>
, (inner) 
<number box="[1204,1236,1123,1147]" pageId="3" pageNumber="4" value="40.0">40</number>
. Pronotum length 
<number box="[151,195,1159,1183]" pageId="3" pageNumber="4" value="131.0">131</number>
, width 
<number box="[278,323,1159,1183]" pageId="3" pageNumber="4" value="204.0">204</number>
. Fore wing length 
<number box="[534,579,1159,1183]" pageId="3" pageNumber="4" value="920.0">920</number>
, width 
<number box="[662,707,1159,1183]" pageId="3" pageNumber="4" value="104.0">104</number>
. Antennal segments length (width): I, 
<number box="[1138,1166,1159,1183]" pageId="3" pageNumber="4" value="34.0">34</number>
(
<number box="[1182,1214,1159,1183]" pageId="3" pageNumber="4" value="32.0">32</number>
); II, 
<number box="[1269,1297,1159,1183]" pageId="3" pageNumber="4" value="44.0">44</number>
(
<number box="[1313,1345,1159,1183]" pageId="3" pageNumber="4" value="30.0">30</number>
); III, 
<number box="[1408,1436,1158,1183]" pageId="3" pageNumber="4" value="67.0">67</number>
(
<number box="[159,191,1195,1219]" pageId="3" pageNumber="4" value="22.0">22</number>
); IV, 
<number box="[252,280,1194,1219]" pageId="3" pageNumber="4" value="61.0">61</number>
(
<number box="[296,328,1195,1219]" pageId="3" pageNumber="4" value="22.0">22</number>
); V, 
