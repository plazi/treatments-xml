<number box="[999,1065,1087,1111]" pageId="3" pageNumber="4" value="1.81">1,810</number>
; head length 
<number box="[1241,1286,1087,1111]" pageId="3" pageNumber="4" value="202.0">202</number>
, width 
<number box="[1386,1431,1086,1111]" pageId="3" pageNumber="4" value="160.0">160</number>
; interocellar setae 
