<taxonomicName authority="Pelikan, 1964: 267" authorityName="Pelikan" authorityPageNumber="267" authorityYear="1964" box="[151,658,1841,1867]" class="Insecta" family="Aeolothripidae" genus="Dactuliothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="kaszabi">
<emphasis bold="true" box="[151,658,1841,1867]" pageId="3" pageNumber="4">
<emphasis bold="true" box="[151,422,1841,1867]" italics="true" pageId="3" pageNumber="4">Dactuliothrips kaszabi</emphasis>
Pelikán, 
<number box="[538,601,1841,1867]" pageId="3" pageNumber="4" value="1964.0">1964</number>
: 
<number box="[614,658,1841,1867]" pageId="3" pageNumber="4" value="267.0">267</number>
