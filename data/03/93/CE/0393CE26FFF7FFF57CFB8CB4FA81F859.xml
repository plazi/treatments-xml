species: 
<taxonomicName authority="Stephens" authorityName="Stephens" box="[301,624,1341,1364]" class="Insecta" family="Polycentropodidae" genus="Plectrocnemia" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="senex">
<emphasis box="[301,521,1341,1364]" italics="true" pageId="9" pageNumber="10">Plectrocnemia senex</emphasis>
Stephens
</taxonomicName>
<emphasis box="[631,668,1341,1364]" italics="true" pageId="9" pageNumber="10">nec</emphasis>
Pictet = 
<taxonomicName authority="McLachlan" authorityName="McLachlan" authorityYear="1871" box="[764,1168,1341,1364]" class="Insecta" family="Polycentropodidae" genus="Plectrocnemia" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="geniculata">
<emphasis box="[764,1037,1341,1364]" italics="true" pageId="9" pageNumber="10">Plectrocnemia geniculata</emphasis>
McLachlan
</taxonomicName>
, monotypic. 
<taxonomicName authority="Neboiss, 1977: 64" authorityName="Neboiss" authorityPageNumber="64" authorityYear="1977" box="[151,518,1373,1396]" class="Insecta" family="Polycentropodidae" genus="Tasmanoplegas" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis box="[151,316,1373,1396]" italics="true" pageId="9" pageNumber="10">Tasmanoplegas</emphasis>
<bibRefCitation author="Neboiss, A." box="[322,518,1373,1396]" pageId="9" pageNumber="10" pagination="1 - 208" refId="ref29250" refString="Neboiss, A. (1977) A taxonomic and zoogeographic study of Tasmanian caddis-flies (Insecta: Trichoptera). Memoirs National Museum Victoria, 38, 1 - 208." type="journal article" year="1977">Neboiss, 1977: 64</bibRefCitation>
