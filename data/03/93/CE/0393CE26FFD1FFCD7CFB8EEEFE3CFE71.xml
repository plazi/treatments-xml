(
<emphasis box="[522,716,1309,1332]" italics="true" pageId="48" pageNumber="49">Paranyctiophylax</emphasis>
) 
<emphasis box="[732,820,1309,1332]" italics="true" pageId="48" pageNumber="49">lancelot</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[835,970,1309,1332]" pageId="48" pageNumber="49" rank="species">new species</taxonomicNameLabel>
