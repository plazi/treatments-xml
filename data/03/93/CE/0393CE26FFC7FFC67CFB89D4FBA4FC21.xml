<taxonomicName authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[718,879,939,964]" class="Insecta" family="Polycentropodidae" genus="Pahamunaya" kingdom="Animalia" order="Trichoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="espelandae">
<emphasis box="[718,879,939,964]" italics="true" pageId="58" pageNumber="59">P. espelandae</emphasis>
</taxonomicName>
and 
<taxonomicName box="[935,1086,939,964]" class="Insecta" family="Polycentropodidae" genus="Pahamunaya" kingdom="Animalia" order="Trichoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="nacesiveci">
<emphasis box="[935,1086,939,964]" italics="true" pageId="58" pageNumber="59">P. nacesiveci</emphasis>
</taxonomicName>
.
</paragraph>
