<subSubSection pageId="42" pageNumber="43" type="discussion">
<paragraph blockId="42.[151,1436,1596,1846]" pageId="42" pageNumber="43">
<emphasis bold="true" box="[196,324,1745,1771]" pageId="42" pageNumber="43">Remarks:</emphasis>
<taxonomicName authorityName="Malicky" authorityYear="1999" box="[333,701,1746,1772]" class="Insecta" family="Polycentropodidae" genus="Nyctiophylax" kingdom="Animalia" order="Trichoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="archemoros" subGenus="Paranyctiophylax">
