<subSubSection box="[196,815,637,663]" pageId="54" pageNumber="55" type="etymology">
<paragraph blockId="54.[151,1436,152,700]" box="[196,815,637,663]" pageId="54" pageNumber="55">
<emphasis bold="true" box="[196,340,637,663]" pageId="54" pageNumber="55">Etymology:</emphasis>
