<taxonomicName class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="blakeii">
