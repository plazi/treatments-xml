</taxonomicName>
near 
<taxonomicName class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="blakeii">
<emphasis italics="true" pageId="6" pageNumber="7">Sphaeropthalma blakeii</emphasis>
</taxonomicName>
and 
<taxonomicName authority="Schuster" authorityName="Schuster" box="[287,513,1084,1110]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="papaga">
<emphasis box="[287,405,1084,1109]" italics="true" pageId="6" pageNumber="7">S. papaga</emphasis>
