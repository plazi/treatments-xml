<taxonomicName box="[938,1106,500,525]" class="Insecta" family="Mutillidae" genus="Photomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="clandestina">
<emphasis box="[938,1106,500,525]" italics="true" pageId="34" pageNumber="35">P. clandestina</emphasis>
</taxonomicName>
in 
<number box="[1145,1208,500,526]" pageId="34" pageNumber="35" value="1928.0">1928</number>
