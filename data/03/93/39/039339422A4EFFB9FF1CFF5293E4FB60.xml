<geoCoordinate box="[1286,1446,914,940]" degrees="62" direction="north" minutes="03" orientation="latitude" pageId="13" pageNumber="31" precision="1" seconds="39.2" value="62.060886">62°03’39.2”N</geoCoordinate>
