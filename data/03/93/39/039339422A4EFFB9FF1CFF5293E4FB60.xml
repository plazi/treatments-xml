<taxonomicName authorityName="Du Tour" authorityYear="1803" box="[1033,1194,878,904]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="13" pageNumber="31" phylum="Tracheophyta" rank="species" species="sibirica">
