(
<bibRefCitation author="StOp-Bowitz, C." box="[1072,1263,1986,2012]" pageId="2" pageNumber="41" pagination="1 - 59" refId="ref2359" refString="StOp-Bowitz, C. 1948. Les Flabelligeriens Norvegiens. Bergens Museums Arbok 1946: 1 - 59." type="journal article" year="1948">StØp-Bowitz, 1948</bibRefCitation>
)
