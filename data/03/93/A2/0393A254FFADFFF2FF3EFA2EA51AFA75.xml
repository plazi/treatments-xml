<taxonomicName authority="Machado and Rafael" authorityName="Machado and Rafael" box="[291,794,1612,1635]" class="Insecta" family="Mantispidae" genus="Leptomantispa" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="39" phylum="Arthropoda" rank="species" species="catarinae" status="new species">
