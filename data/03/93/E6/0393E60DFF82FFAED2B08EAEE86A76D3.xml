<location LSID="urn:lsid:plazi:treatment:0393E60DFF82FFAED2B08EAEE86A76D3:8EE501C0FF82FFAED3C18993EF1C75FB" box="[486,533,1219,1243]" country="Philippines" county="The Philippine" municipality="Fujita's" name="One" pageId="53" pageNumber="54">One</location>
