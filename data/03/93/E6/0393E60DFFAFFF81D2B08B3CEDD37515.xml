<materialsCitation ID-GBIF-Occurrence="1503739489" box="[837,1112,1894,1919]" country="Philippines" elevation="225" location="Philippines" pageId="24" pageNumber="25" specimenCount="1">
<collectingCountry box="[837,967,1894,1919]" name="Philippines" pageId="24" pageNumber="25">Philippines</collectingCountry>
. 
