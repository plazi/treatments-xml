is recorded only from Central America (
<collectingRegion box="[1068,1194,1015,1040]" country="Guatemala" name="Guatemala" pageId="7" pageNumber="299">
<collectingCountry box="[1068,1194,1015,1040]" name="Guatemala" pageId="7" pageNumber="299">Guatemala</collectingCountry>
</collectingRegion>
, 
