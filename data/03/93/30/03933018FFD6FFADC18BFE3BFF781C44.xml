<collectingCountry box="[1338,1403,1426,1452]" name="China" pageId="6" pageNumber="7">
<collectionCode box="[1338,1403,1426,1452]" httpUri="http://grbio.org/cool/ze71-842p" name="Tel-Aviv University" pageId="6" pageNumber="7">TAU</collectionCode>
