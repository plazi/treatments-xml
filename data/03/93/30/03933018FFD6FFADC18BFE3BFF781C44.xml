S., 
<quantity box="[930,1018,1426,1452]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="6" pageNumber="7" unit="m" value="1800.0">1800m</quantity>
., 
<date box="[1034,1162,1426,1452]" pageId="6" pageNumber="7" value="1987-11-22">
