, 
<collectingCountry box="[151,244,744,770]" name="Mexico" pageId="5" pageNumber="46">Mexico</collectingCountry>
, 
<collectingCountry box="[256,313,744,770]" name="Peru" pageId="5" pageNumber="46">Peru</collectingCountry>
