<collectingCountry box="[829,903,705,731]" name="Brazil" pageId="5" pageNumber="46">Brazil</collectingCountry>
, 
<collectingCountry box="[921,1040,705,731]" name="Colombia" pageId="5" pageNumber="46">Colombia</collectingCountry>
, 
<collectingCountry box="[1057,1122,705,731]" name="Cuba" pageId="5" pageNumber="46">Cuba</collectingCountry>
, 
<collectingCountry box="[1140,1285,705,731]" name="Guadeloupe" pageId="5" pageNumber="46">Guadeloupe</collectingCountry>
