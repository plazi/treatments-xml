Yang &amp; Zhang, 
<number box="[704,763,1587,1611]" pageId="1" pageNumber="590" value="1990.0">1990</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Yang, Zhang &amp; Yang, 1993" authorityName="Yang, Zhang &amp; Yang" authorityYear="1993" box="[774,1241,1586,1611]" class="Insecta" family="Sciaridae" genus="Sciara" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="590" phylum="Arthropoda" rank="species" species="sclerocerci">
<emphasis box="[774,924,1586,1611]" italics="true" pageId="1" pageNumber="590">S. sclerocerci</emphasis>
<bibRefCitation author="Yang" box="[932,1241,1586,1611]" pageId="1" pageNumber="590" refString="Yang, C. - K., Zhang, X. &amp; Yang, C. - Q. (1993) Taxonomic studies on the Sciaridae (Diptera: Nematocera) of Guizhou. Entomotaxonomia, 15, 283 - 318." type="journal article" year="1993">
Yang, Zhang &amp; Yang, 
<number box="[1182,1241,1587,1611]" pageId="1" pageNumber="590" value="1993.0">1993</number>
</bibRefCitation>
</taxonomicName>
, and the Nearctic 
<taxonomicName authority="Johannsen, 1912" authorityName="Johannsen" authorityYear="1912" box="[151,435,1622,1647]" class="Insecta" family="Sciaridae" genus="Sciara" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="590" phylum="Arthropoda" rank="species" species="futilis">
<emphasis box="[151,242,1622,1647]" italics="true" pageId="1" pageNumber="590">S. futilis</emphasis>
Johannsen, 
<quantity box="[379,435,1623,1647]" metricMagnitude="1" metricUnit="m" metricValue="4.85648" pageId="1" pageNumber="590" unit="in" value="1912.0">1912</quantity>
