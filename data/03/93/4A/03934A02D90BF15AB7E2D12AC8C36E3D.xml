Yang &amp; Zhang, 
<number box="[330,389,1587,1611]" pageId="1" pageNumber="590" value="1987.0">1987</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Yang &amp; Zhang, 1990" authorityName="Yang &amp; Zhang" authorityYear="1990" box="[400,763,1586,1611]" class="Insecta" family="Sciaridae" genus="Sciara" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="590" phylum="Arthropoda" rank="species" species="isopalpi">
