<paragraph blockId="22.[151,509,1516,1542]" box="[151,509,1516,1542]" pageId="22" pageNumber="377">
<heading bold="true" box="[151,509,1516,1542]" fontSize="11" level="1" pageId="22" pageNumber="377" reason="1">
<emphasis bold="true" box="[151,509,1516,1542]" pageId="22" pageNumber="377">
Genus 
<taxonomicName authority="Iredale, 1936" authorityName="Iredale" authorityYear="1936" box="[238,509,1516,1542]" class="Malacostraca" family="Atyidae" genus="Diniatys" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="377" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,339,1516,1542]" italics="true" pageId="22" pageNumber="377">Diniatys</emphasis>
<bibRefCitation author="Iredale, T." box="[346,509,1516,1542]" pageId="22" pageNumber="377" pagination="267 - 340" refId="ref19120" refString="Iredale, T. (1936) Australia molluscan notes No. 2. Records of the Australian Museum, 19, 267 - 340. http: // dx. doi. org / 10.3853 / j. 0067 - 1975.19.1936.704" type="journal article" year="1936">Iredale, 1936</bibRefCitation>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
