, 
<collectingCountry box="[953,1023,1620,1645]" name="China" pageId="28" pageNumber="383">China</collectingCountry>
, 
<collectingCountry box="[1038,1105,1620,1643]" name="Japan" pageId="28" pageNumber="383">Japan</collectingCountry>
, 
<collectingCountry box="[1119,1346,1620,1645]" name="Papua New Guinea" pageId="28" pageNumber="383">Papua New Guinea</collectingCountry>
, 
<collectingCountry box="[1361,1432,1620,1645]" name="Guam" pageId="28" pageNumber="383">Guam</collectingCountry>
, 
<collectingCountry box="[151,328,1655,1680]" name="New Caledonia" pageId="28" pageNumber="383">New Caledonia</collectingCountry>
, Hawaii (
<bibRefCitation author="Habe, T." box="[434,561,1655,1680]" pageId="28" pageNumber="383" pagination="137 - 152" refId="ref18981" refString="Habe, T. (1952) Atyidae in Japan. In: Kuroda, T., (Ed.), Illustrated Catalogue of Japanese shells, 20, pp. 137 - 152." type="journal article" year="1952">Habe 1952</bibRefCitation>
