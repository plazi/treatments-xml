<paragraph blockId="4.[151,669,151,177]" box="[151,669,151,177]" pageId="4" pageNumber="309">
<heading bold="true" box="[151,669,151,177]" fontSize="11" level="1" pageId="4" pageNumber="309" reason="1">
<emphasis bold="true" box="[151,669,151,177]" pageId="4" pageNumber="309">
<taxonomicName authority="Willem, 1902" authorityName="Willem" authorityYear="1902" box="[151,540,151,177]" class="Entognatha" family="Isotomidae" genus="Folsomia" kingdom="Animalia" order="Collembola" pageId="4" pageNumber="309" phylum="Arthropoda" rank="species" sensu="lato" species="candida">
<emphasis bold="true" box="[151,368,151,177]" italics="true" pageId="4" pageNumber="309">Folsomia candida</emphasis>
<bibRefCitation author="Willem" box="[375,540,151,177]" pageId="4" pageNumber="309" refString="Willem, V. (1902) Note preliminaire sur les Collemboles des grottes de Han et de Rochefort. Annales de la Societe entomologique de Belgique, 46, 275 - 283." type="journal article" year="1902">
Willem, 
<number box="[481,540,151,177]" pageId="4" pageNumber="309" value="1902.0">1902</number>
</bibRefCitation>
</taxonomicName>
<taxonomicNameLabel box="[547,669,151,177]" pageId="4" pageNumber="309" sensu="lato">sensu lato</taxonomicNameLabel>
