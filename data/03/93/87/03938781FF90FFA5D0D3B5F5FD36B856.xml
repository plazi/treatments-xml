<quantity box="[332,388,335,359]" metricMagnitude="1" metricUnit="m" metricValue="4.80314" pageId="7" pageNumber="8" unit="in" value="1891.0">1891</quantity>
</bibRefCitation>
</taxonomicName>
in the 
<taxonomicName box="[475,642,334,359]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="splendidula">
