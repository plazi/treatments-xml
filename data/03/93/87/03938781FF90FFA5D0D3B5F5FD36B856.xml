and 
<taxonomicName authority="Radoszkowski, 1891" authorityName="Radoszkowski" authorityYear="1891" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="acceptabilis">
