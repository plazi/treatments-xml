<number box="[1254,1267,1788,1813]" pageId="1" pageNumber="2" value="1.0">1</number>
b
</emphasis>
) 
<number box="[1300,1317,1788,1814]" pageId="1" pageNumber="2" value="7.0">7</number>
, proximal setae on coxisternum I (
<emphasis box="[660,689,1828,1853]" italics="true" pageId="1" pageNumber="2">
<number box="[660,673,1828,1853]" pageId="1" pageNumber="2" value="1.0">1</number>
a
</emphasis>
) 
<number box="[707,739,1828,1854]" pageId="1" pageNumber="2" value="15.0">15</number>
, proximal setae on coxisternum II (
<emphasis box="[1157,1187,1828,1853]" italics="true" pageId="1" pageNumber="2">
<number box="[1157,1171,1828,1853]" pageId="1" pageNumber="2" value="2.0">2</number>
a
