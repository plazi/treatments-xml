) 
<number box="[1111,1128,1479,1505]" pageId="14" pageNumber="15" value="7.0">7</number>
, setae located at dorsomedially; tarsus 
<number box="[526,541,1519,1545]" pageId="14" pageNumber="15" value="7.0">7</number>
long; tarsal empodium simple, 
<number box="[920,933,1519,1545]" pageId="14" pageNumber="15" value="5.0">5</number>
­rayed, tarsal solenidion tapered. Legs 
<date box="[331,389,1559,1585]" pageId="14" pageNumber="15" value="1929-02">
II 
<number box="[360,389,1559,1585]" pageId="14" pageNumber="15" value="29.0">29</number>
</date>
long, femur 
<number box="[549,565,1559,1585]" pageId="14" pageNumber="15" value="8.0">8</number>
, basiventral femoral setae (
<emphasis box="[900,928,1559,1584]" italics="true" pageId="14" pageNumber="15">bv</emphasis>
) 
<number box="[947,964,1559,1585]" pageId="14" pageNumber="15" value="8.0">8</number>
; genu 
<number box="[1047,1062,1559,1585]" pageId="14" pageNumber="15" value="4.0">4</number>
long, antaxial genual setae (
