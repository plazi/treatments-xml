<emphasis box="[412,438,1679,1704]" italics="true" pageId="14" pageNumber="15">
c 
<number box="[424,438,1679,1704]" pageId="14" pageNumber="15" value="2.0">2</number>
</emphasis>
<number box="[448,477,1679,1705]" pageId="14" pageNumber="15" value="27.0">27</number>
on ventral annulus 
<number box="[715,747,1679,1705]" pageId="14" pageNumber="15" value="12.0">12</number>
; setae 
<emphasis box="[829,844,1679,1704]" italics="true" pageId="14" pageNumber="15">d</emphasis>
<number box="[852,881,1679,1705]" pageId="14" pageNumber="15" value="26.0">26</number>
on ventral annulus 
<number box="[1119,1151,1679,1705]" pageId="14" pageNumber="15" value="31.0">31</number>
; setae 
<emphasis box="[1233,1246,1679,1704]" italics="true" pageId="14" pageNumber="15">e</emphasis>
<number box="[1255,1284,1679,1705]" pageId="14" pageNumber="15" value="26.0">26</number>
on ventral annulus 
<number box="[455,487,1719,1745]" pageId="14" pageNumber="15" value="51.0">51</number>
; setae 
<emphasis box="[571,579,1719,1744]" italics="true" pageId="14" pageNumber="15">f</emphasis>
<number box="[588,617,1719,1745]" pageId="14" pageNumber="15" value="16.0">16</number>
