: (n = 
<number box="[446,461,1199,1225]" pageId="14" pageNumber="15" value="5.0">5</number>
) Body fusiform, 
<number box="[683,727,1199,1225]" pageId="14" pageNumber="15" value="190.0">190</number>
long, 
<number box="[814,843,1199,1225]" pageId="14" pageNumber="15" value="70.0">70</number>
wide, 
<number box="[935,964,1199,1225]" pageId="14" pageNumber="15" value="80.0">80</number>
