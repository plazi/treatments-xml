<emphasis box="[1104,1134,1719,1744]" italics="true" pageId="14" pageNumber="15">
h 
<number box="[1121,1134,1719,1744]" pageId="14" pageNumber="15" value="1.0">1</number>
</emphasis>
<number box="[1143,1158,1719,1745]" pageId="14" pageNumber="15" value="5.0">5</number>
