<emphasis box="[799,924,1327,1350]" italics="true" pageId="2" pageNumber="3">
<taxonomicName box="[799,918,1327,1350]" class="Diplopoda" family="Conotylidae" genus="Idagona" kingdom="Animalia" order="Chordeumatida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="westcotti">I. westcotti</taxonomicName>
;
</emphasis>
square, 
<emphasis box="[1014,1119,1327,1350]" italics="true" pageId="2" pageNumber="3">
<taxonomicName box="[1014,1112,1327,1350]" class="Diplopoda" family="Conotylidae" genus="Idagona" kingdom="Animalia" order="Chordeumatida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="jasperi">I. jasperi</taxonomicName>
;
</emphasis>
circle, 
<emphasis box="[1197,1358,1327,1350]" italics="true" pageId="2" pageNumber="3">
<taxonomicName box="[1197,1354,1327,1350]" class="Diplopoda" family="Conotylidae" genus="Idagona" kingdom="Animalia" order="Chordeumatida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="lehmanensis">I. lehmanensis</taxonomicName>
.
</emphasis>
Triangle 
<number box="[191,204,1362,1385]" pageId="2" pageNumber="3" value="1.0">1</number>
