<emphasis box="[1284,1312,1878,1903]" italics="true" pageId="4" pageNumber="5">
<number box="[1284,1297,1878,1903]" pageId="4" pageNumber="5" value="1.0">1</number>
b
</emphasis>
) 
<number box="[264,296,284,310]" pageId="5" pageNumber="6" value="10.0">10</number>
, proximal setae on coxisternum I (
<emphasis box="[721,750,284,309]" italics="true" pageId="5" pageNumber="6">
<number box="[721,734,284,309]" pageId="5" pageNumber="6" value="1.0">1</number>
a
</emphasis>
) 
<number box="[770,801,284,310]" pageId="5" pageNumber="6" value="15.0">15</number>
, proximal setae on coxisternum II (
<emphasis box="[1236,1266,284,309]" italics="true" pageId="5" pageNumber="6">
