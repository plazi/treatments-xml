; prosoma 
<number box="[897,949,928,954]" pageId="6" pageNumber="27" value="2.75">2.75</number>
long, 
<number box="[1025,1077,928,954]" pageId="6" pageNumber="27" value="2.2">2.20</number>
wide; opisthosoma 
<number box="[1316,1368,928,954]" pageId="6" pageNumber="27" value="2.62">2.62</number>
long, 
