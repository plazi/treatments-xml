, holotype; 2. 
<taxonomicName box="[1143,1340,1593,1616]" class="Insecta" family="Glaphyridae" genus="Amphicoma" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="aurata">
<emphasis box="[1143,1340,1593,1616]" italics="true" pageId="3" pageNumber="26">Amphicoma aurata</emphasis>
</taxonomicName>
, male; 3. 
<taxonomicName box="[151,346,1624,1646]" class="Insecta" family="Glaphyridae" genus="Amphicoma" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="aurata">
<emphasis box="[151,346,1624,1646]" italics="true" pageId="3" pageNumber="26">Amphicoma aurata</emphasis>
</taxonomicName>
, male; 4. 
<taxonomicName box="[448,644,1624,1646]" class="Insecta" family="Glaphyridae" genus="Amphicoma" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="aurata">
<emphasis box="[448,644,1624,1646]" italics="true" pageId="3" pageNumber="26">Amphicoma aurata</emphasis>
</taxonomicName>
, female; 5. 
<taxonomicName box="[766,1002,1624,1646]" class="Insecta" family="Glaphyridae" genus="Amphicoma" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="formosana">
<emphasis box="[766,1002,1624,1646]" italics="true" pageId="3" pageNumber="26">Amphicoma formosana</emphasis>
</taxonomicName>
, male; 6. 
<taxonomicName box="[1104,1340,1624,1646]" class="Insecta" family="Glaphyridae" genus="Amphicoma" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="formosana">
