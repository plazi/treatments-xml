<caption httpUri="https://zenodo.org/record/206621/files/figure.png" pageId="4" pageNumber="27" targetBox="[158,1432,199,609]" targetPageId="4">
<paragraph blockId="4.[151,1436,638,691]" pageId="4" pageNumber="27">
