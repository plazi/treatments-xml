<subSubSection pageId="3" pageNumber="599" type="etymology">
<paragraph blockId="3.[151,1437,1037,1674]" pageId="3" pageNumber="599">
<emphasis bold="true" box="[199,334,1145,1170]" pageId="3" pageNumber="599">Etymology.</emphasis>
