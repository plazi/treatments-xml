<taxonomicName authorityName="Volynkin &amp; Černý" authorityYear="2019" box="[290,507,1917,1941]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="283" phylum="Arthropoda" rank="species" species="hausmanni">
