: 
<collectingRegion box="[434,500,655,681]" country="India" name="Delhi" pageId="11" pageNumber="128">Delhi</collectingRegion>
; 
<collectingRegion box="[511,630,655,681]" country="India" name="Karnataka" pageId="11" pageNumber="128">Karnataka</collectingRegion>
; 
<collectingRegion box="[640,776,655,681]" country="India" name="Tamil Nadu" pageId="11" pageNumber="128">Tamil Nadu</collectingRegion>
; 
<collectingCountry box="[788,929,655,681]" name="India" pageId="11" pageNumber="128">Pondicherry</collectingCountry>
; 
