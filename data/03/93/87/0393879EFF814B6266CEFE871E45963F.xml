, HW 
<quantity box="[326,484,451,475]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" metricValueMax="6.8" metricValueMin="6.2" pageId="20" pageNumber="185" unit="mm" value="0.65" valueMax="0.68" valueMin="0.62">0.62-0.68 mm</quantity>
, PL 
