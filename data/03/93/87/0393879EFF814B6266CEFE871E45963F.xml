, EL 
<quantity box="[351,503,484,508]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="8.8" metricValueMin="8.2" pageId="20" pageNumber="185" unit="mm" value="0.85" valueMax="0.88" valueMin="0.82">0.82-0.88 mm</quantity>
, EW 
<quantity box="[560,713,483,507]" metricMagnitude="-3" metricUnit="m" metricValue="1.33" metricValueMax="1.46" metricValueMin="1.2" pageId="20" pageNumber="185" unit="mm" value="1.33" valueMax="1.46" valueMin="1.2">1.20-1.46 mm</quantity>
, AL 1.00- 
<quantity box="[206,304,516,540]" metricMagnitude="-3" metricUnit="m" metricValue="1.29" pageId="20" pageNumber="185" unit="mm" value="1.29">1.29 mm</quantity>
, AW 
<quantity box="[362,516,515,539]" metricMagnitude="-3" metricUnit="m" metricValue="1.505" metricValueMax="1.57" metricValueMin="1.44" pageId="20" pageNumber="185" unit="mm" value="1.505" valueMax="1.57" valueMin="1.44">1.44-1.57 mm</quantity>
.
