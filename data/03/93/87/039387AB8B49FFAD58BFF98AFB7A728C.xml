<number box="[1338,1380,2010,2035]" pageId="3" pageNumber="417" value="580.0">580</number>
– 
<quantity lastPageId="4" lastPageNumber="418" metricMagnitude="2" metricUnit="m" metricValue="5.64" pageId="3" pageNumber="417" unit="m" value="564.0">564 m</quantity>
(
<bibRefCitation author="Tabachnick" box="[192,402,151,176]" pageId="4" pageNumber="418" refString="Tabachnick, K. R. (1990) Hexactinellid sponges from the Nasca and Sala-y-Gomez. Trudi of the Institute of Oceanology, Academy of Sciences of USSR, 124, 161 - 173." type="journal article" year="1990">
Tabachnick, 
<number box="[342,402,152,176]" pageId="4" pageNumber="418" value="1990.0">1990</number>
</bibRefCitation>
; Tabachnick, 
<number box="[567,628,152,176]" pageId="4" pageNumber="418" value="2002.0">2002</number>
