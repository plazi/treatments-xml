(
<bibRefCitation author="Radoszkowski" box="[633,873,439,464]" pageId="13" pageNumber="14" refString="Radoszkowski, O. (1877) Chrysidiformes, Mutillidae et Sphegidae. In: Putieshestvie v Turkestan A. P. Fedtshenko [Voyage au Turkestan d'Alexis Fedtschenko], (14) 2 (5), Sankt-Petersburg, pp. 1 - 87. + 8 plates." type="book chapter" year="1877">
Radoszkowski, 
<number box="[816,873,440,464]" pageId="13" pageNumber="14" value="1877.0">1877</number>
</bibRefCitation>
)
</taxonomicName>
(transferred to 
<taxonomicName authority="Semenov" authorityName="Semenov" box="[1064,1311,439,464]" class="Insecta" family="Chrysididae" genus="Spintharina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis box="[1064,1196,439,464]" italics="true" pageId="13" pageNumber="14">Spintharina</emphasis>
Semenov
</taxonomicName>
by 
<bibRefCitation author="Bohart" pageId="13" pageNumber="14" refString="Bohart, R. M. (1987) A Key to the species of Spintharina with descriptions of new species and indication of species groups (Hymenoptera, Chrysididae). Psyche, 94, 93 - 101. http: // dx. doi. org / 10.1155 / 1987 / 59023" type="journal article" year="1987">
Bohart 
<number box="[151,211,476,500]" pageId="13" pageNumber="14" value="1987.0">1987</number>
</bibRefCitation>
: 
<number box="[221,253,476,500]" pageId="13" pageNumber="14" value="93.0">93</number>
).
</paragraph>
