<number box="[726,739,324,349]" pageId="13" pageNumber="14" value="1.0">1</number>
b
</emphasis>
) 
<number box="[778,795,324,350]" pageId="13" pageNumber="14" value="7.0">7</number>
, proximal setae on coxisternum I (
<emphasis box="[1234,1263,324,349]" italics="true" pageId="13" pageNumber="14">
<number box="[1234,1247,324,349]" pageId="13" pageNumber="14" value="1.0">1</number>
a
</emphasis>
) 
<number box="[1286,1318,324,350]" pageId="13" pageNumber="14" value="18.0">18</number>
