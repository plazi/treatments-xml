<paragraph blockId="2.[151,565,364,390]" box="[151,565,364,390]" pageId="2" pageNumber="357">
<heading bold="true" box="[151,565,364,390]" fontSize="11" level="1" pageId="2" pageNumber="357" reason="1">
<emphasis bold="true" box="[151,565,364,390]" pageId="2" pageNumber="357">
Genus 
<taxonomicName authority="Pilsbry, 1896" authorityName="Pilsbry" authorityYear="1896" box="[238,565,364,390]" class="Malacostraca" family="Atyidae" genus="Aliculastrum" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="357" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,396,364,390]" italics="true" pageId="2" pageNumber="357">Aliculastrum</emphasis>
<bibRefCitation author="Pilsbry, H. A." box="[403,565,364,390]" pageId="2" pageNumber="357" refId="ref20531" refString="Pilsbry, H. A. (1896) Manual of Conchology, structural and systematic. Vol. 16. Academy of Natural Sciences of Philadelphia, Philadelphia, 237 pp." type="book" year="1896">Pilsbry, 1896</bibRefCitation>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
