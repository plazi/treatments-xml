<subSubSection box="[199,1241,367,392]" pageId="10" pageNumber="11" type="etymology">
<paragraph blockId="10.[151,1436,151,464]" box="[199,1241,367,392]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[199,326,368,392]" pageId="10" pageNumber="11">Etymology</emphasis>
