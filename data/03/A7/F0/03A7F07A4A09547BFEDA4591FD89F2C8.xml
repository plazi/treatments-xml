<taxonomicName authorityName="Bolz &amp; Wagner" authorityYear="2012" baseAuthorityName="Jacoby" baseAuthorityYear="1899" box="[373,583,656,680]" class="Insecta" family="Chrysomelidae" genus="Neobarombiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="punctatolineata">
