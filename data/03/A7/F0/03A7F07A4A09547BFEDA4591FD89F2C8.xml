<collectingCountry box="[327,471,692,716]" name="South Africa" pageId="82" pageNumber="83">South Africa</collectingCountry>
(
<figureCitation box="[486,570,692,716]" captionStart="FIGURE 51" captionStartId="35.[151,250,1178,1201]" captionTargetBox="[151,1436,193,1157]" captionTargetId="figure-343@35.[151,1436,193,1157]" captionTargetPageId="35" captionText="FIGURE 51. Distribution of Neobarombiella punctatolineata (Jacoby, 1899)." pageId="82" pageNumber="83">Fig. 51</figureCitation>
).
