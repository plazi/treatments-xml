<typeStatus box="[485,535,1189,1210]" pageId="13" pageNumber="358">Type</typeStatus>
species 
<taxonomicName authority="Castelnau, 1873" authorityName="Castelnau" authorityYear="1873" box="[621,967,1188,1210]" class="Chondrichthyes" family="Rajidae" genus="Raya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="13" pageNumber="358" phylum="Chordata" rank="species" species="trigonoides">
