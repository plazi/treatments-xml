<taxonomicName authority="Wagner, 1965" authorityName="Wagner" authorityYear="1965" box="[414,741,1364,1389]" class="Insecta" family="Miridae" genus="Megacoelum" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="187" phylum="Arthropoda" rank="species" species="angustum" status="n. comb">
