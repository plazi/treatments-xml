(Wagner, 
<number box="[676,733,1364,1389]" pageId="36" pageNumber="187" value="1965.0">1965</number>
)
</taxonomicName>
<taxonomicNameLabel box="[751,851,1364,1389]" pageId="36" pageNumber="187">n. comb</taxonomicNameLabel>
., 
<taxonomicName authority="Fieber, 1870" authorityName="Fieber" authorityYear="1870" box="[868,1153,1364,1389]" class="Insecta" family="Miridae" genus="Calocoris" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="187" phylum="Arthropoda" rank="species" species="beckeri" status="n. comb">
<emphasis box="[868,981,1364,1389]" italics="true" pageId="36" pageNumber="187">P. beckeri</emphasis>
(Fieber, 
<number box="[1088,1145,1364,1388]" pageId="36" pageNumber="187" value="1870.0">1870</number>
)
</taxonomicName>
<taxonomicNameLabel box="[1164,1265,1364,1389]" pageId="36" pageNumber="187">n. comb</taxonomicNameLabel>
.*, 
<taxonomicName authority="Linnavuori, 1988" authorityName="Linnavuori" authorityYear="1988" class="Insecta" family="Miridae" genus="Pseudomegacoelum" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="187" phylum="Arthropoda" rank="species" species="irbilanum" status="n. comb">
<emphasis box="[1295,1435,1364,1389]" italics="true" pageId="36" pageNumber="187">P. irbilanum</emphasis>
(Linnavuori, 
<number box="[299,356,1400,1424]" pageId="36" pageNumber="187" value="1988.0">1988</number>
)
</taxonomicName>
<taxonomicNameLabel box="[371,469,1399,1424]" pageId="36" pageNumber="187">n. comb</taxonomicNameLabel>
.*, 
<taxonomicName authority="Linnavuori (1965)" authorityName="Linnavuori" authorityYear="1965" box="[496,855,1399,1424]" class="Insecta" family="Miridae" genus="Pseudomegacoelum" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="187" phylum="Arthropoda" rank="species" species="quercicola" status="n. comb">
<emphasis box="[496,643,1399,1424]" italics="true" pageId="36" pageNumber="187">P. quercicola</emphasis>
Linnavuori (
<number box="[789,847,1399,1424]" pageId="36" pageNumber="187" value="1965.0">1965</number>
)
</taxonomicName>
<taxonomicNameLabel box="[862,958,1399,1424]" pageId="36" pageNumber="187">n. comb</taxonomicNameLabel>
*.
</paragraph>
</subSubSection>
<subSubSection pageId="36" pageNumber="187" type="distribution">
<paragraph blockId="36.[151,1437,1004,1640]" pageId="36" pageNumber="187">
<emphasis bold="true" box="[199,352,1436,1461]" pageId="36" pageNumber="187">Distribution.</emphasis>
Europe (
<collectingCountry box="[456,543,1436,1461]" name="Austria" pageId="36" pageNumber="187">Austria</collectingCountry>
, 
<collectingCountry box="[554,653,1436,1461]" name="Bulgaria" pageId="36" pageNumber="187">Bulgaria</collectingCountry>
, 
<collectingCountry box="[665,748,1436,1460]" name="Cyprus" pageId="36" pageNumber="187">Cyprus</collectingCountry>
, 
<collectingCountry box="[761,840,1437,1460]" name="France" pageId="36" pageNumber="187">France</collectingCountry>
, 
<collectingCountry box="[851,956,1436,1460]" name="Germany" pageId="36" pageNumber="187">Germany</collectingCountry>
, 
<collectingCountry box="[968,1119,1436,1461]" name="United Kingdom" pageId="36" pageNumber="187">Great Britain</collectingCountry>
, 
<collectingCountry box="[1130,1212,1436,1460]" name="Greece" pageId="36" pageNumber="187">Greece</collectingCountry>
, 
<collectingCountry box="[1224,1284,1436,1461]" name="Italy" pageId="36" pageNumber="187">Italia</collectingCountry>
, 
<collectingCountry box="[1296,1364,1436,1461]" name="Spain" pageId="36" pageNumber="187">Spain</collectingCountry>
), 
<collectingCountry box="[1384,1432,1437,1460]" name="Iraq" pageId="36" pageNumber="187">Iraq</collectingCountry>
