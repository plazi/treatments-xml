<bibRefCitation author="Rothschild, W. &amp; E. Hartert" pageId="24" pageNumber="24" pagination="117 - 118" refId="ref143714" refString="Rothschild, W., and E. Hartert. 1923. [Lord Rothschild and Dr. Ernst Hartert exhibited and described a new bird from Buru as follows: -]. Bulletin of the British Ornithologists' Club 43: 117 - 118." type="journal article" year="1923">Rothschild and Hartert (1923: 118)</bibRefCitation>
listed 
<collectingMunicipality box="[316,488,1012,1034]" pageId="24" pageNumber="24">Mount Fogha</collectingMunicipality>
<emphasis bold="true" box="[501,524,1011,1034]" italics="true" pageId="24" pageNumber="24">5</emphasis>
<location LSID="urn:lsid:plazi:treatment:03A787A2F223F14AED8E159DFDDA595B:8ED1606FF22CF14AED401599FF45595B" latitude="-3.14" longLatPrecision="784" longitude="126.04" municipality="Mount Fogha" name="Mount Tomahu" pageId="24" pageNumber="24" stateProvince="Madang">Mount Tomahu</location>
, 
<geoCoordinate box="[209,289,1042,1063]" degrees="03.14" direction="south" orientation="latitude" pageId="24" pageNumber="24" precision="555" value="-3.14">03.14S</geoCoordinate>
, 
<geoCoordinate box="[302,396,1042,1064]" degrees="126.04" direction="east" orientation="longitude" pageId="24" pageNumber="24" precision="555" value="126.04">126.04E</geoCoordinate>
(
<collectionCode box="[413,518,1042,1063]" pageId="24" pageNumber="24">USBGN</collectionCode>
, 1982)
