<collectingCountry box="[1020,1148,260,284]" name="Costa Rica" pageId="28" pageNumber="29">Costa Rica</collectingCountry>
, from 
<number box="[1227,1351,260,284]" pageId="28" pageNumber="29" value="1475.0" valueMax="1800.0" valueMin="1150.0">1150–1800</number>
