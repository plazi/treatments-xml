<collectionCode box="[416,512,1620,1641]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="58" pageNumber="58">AMNH</collectionCode>
