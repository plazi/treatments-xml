<location LSID="urn:lsid:plazi:treatment:03A787A2F20EF168EFD0150CFC1F5E40:8ED1606FF20EF168EED0101DFD8B5BF4" box="[394,522,1649,1671]" country="Guinea-Bissau" name="Rothschild" pageId="58" pageNumber="58">Rothschild</location>
