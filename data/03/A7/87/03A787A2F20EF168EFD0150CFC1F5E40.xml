<location LSID="urn:lsid:plazi:treatment:03A787A2F20EF168EFD0150CFC1F5E40:8ED1606FF20EF168EFAA1299FED75879" box="[240,342,1269,1290]" country="Guinea-Bissau" county="Rothschild" municipality="Berlin" name="Museum" pageId="58" pageNumber="58">Museum</location>
