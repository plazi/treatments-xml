<emphasis bold="true" pageId="34" pageNumber="34">
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="34" pageNumber="34">AMNH 658154</specimenCode>
