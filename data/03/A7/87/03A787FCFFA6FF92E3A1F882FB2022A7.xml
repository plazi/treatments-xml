<emphasis box="[974,1168,188,214]" italics="true" pageId="80" pageNumber="81">Cydonia oblonga</emphasis>
, 
<emphasis box="[1182,1299,189,215]" italics="true" pageId="80" pageNumber="81">Crateagus</emphasis>
</taxonomicName>
spp., 
<taxonomicName authorityName="P.Miller" authorityYear="1754" box="[1366,1436,188,214]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="80" pageNumber="81" phylum="Tracheophyta" rank="genus">
<emphasis box="[1366,1436,188,214]" italics="true" pageId="80" pageNumber="81">Malus</emphasis>
