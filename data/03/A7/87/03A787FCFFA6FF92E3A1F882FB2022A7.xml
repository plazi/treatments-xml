</taxonomicName>
spp., and 
<taxonomicName box="[450,515,225,250]" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" kingdom="Plantae" order="Rosales" pageId="80" pageNumber="81" phylum="Tracheophyta" rank="genus">
<emphasis box="[450,515,225,250]" italics="true" pageId="80" pageNumber="81">Pyrus</emphasis>
</taxonomicName>
