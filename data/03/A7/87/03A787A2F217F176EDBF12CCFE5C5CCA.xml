in the species 
<taxonomicName authorityName="Latham" authorityYear="1801" box="[186,335,360,383]" class="Aves" family="Pachycephalidae" genus="Pachycephala" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="36" phylum="Chordata" rank="species" species="pectoralis">
<emphasis box="[186,335,360,383]" italics="true" pageId="36" pageNumber="36">P. pectoralis</emphasis>
</taxonomicName>
