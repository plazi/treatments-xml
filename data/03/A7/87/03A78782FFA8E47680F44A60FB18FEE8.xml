<taxonomicName authority="Waters, 1904" authorityName="Waters" authorityYear="1904" class="Gymnolaemata" family="Membraniporidae" genus="Membranipora" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="203" phylum="Bryozoa" rank="species" species="uniserialis">
<emphasis italics="true" pageId="2" pageNumber="203">P. uniserialis</emphasis>
(
<bibRefCitation author="Waters" box="[292,454,1910,1934]" pageId="2" pageNumber="203" refString="Waters, A. W. (1904) Bryozoa. Resultats du Voyage du S. Y. Belgica, Zoologie. Expedition Antarctique Belge, 4, 1 - 114, 4 pls." type="journal article" year="1904">Waters, 1904</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="Gordon, 1989" authorityName="Gordon" authorityYear="1989" box="[482,828,1909,1934]" class="Gymnolaemata" family="Calloporidae" genus="Daisyella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="203" phylum="Bryozoa" rank="species" species="circularis">
<emphasis box="[482,626,1909,1933]" italics="true" pageId="2" pageNumber="203">P. circularis</emphasis>
(
<bibRefCitation author="Gordon" box="[650,820,1909,1934]" pageId="2" pageNumber="203" refString="Gordon, D. P. (1989) New and little-known genera of cheilostome Bryozoa from the New Zealand region. Journal of Natural History, 23, 1319 - 1339." type="journal article" year="1989">Gordon, 1989</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="Gordon, 1989" authorityName="Gordon" authorityYear="1989" box="[849,1270,1909,1934]" class="Gymnolaemata" family="Calloporidae" genus="Pyriporoides" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="203" phylum="Bryozoa" rank="species" species="libita">
