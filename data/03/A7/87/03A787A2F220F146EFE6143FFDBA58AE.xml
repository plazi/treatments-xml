<emphasis bold="true" box="[279,464,795,816]" pageId="20" pageNumber="20">
<specimenCode box="[279,464,795,816]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="20" pageNumber="20">AMNH 658629</specimenCode>
