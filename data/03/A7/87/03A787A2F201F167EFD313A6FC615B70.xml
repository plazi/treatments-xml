<materialsCitation collectingDate="1910-07-12" collectingDateMax="1912-03" collectingDateMin="1910-07-12" collectionCode="AMNH" collectorName="E. Olive &amp; Tolga &amp; Atherton Tableland &amp; Mathews" county="Four" location="Cooktown" municipality="Mathews Collection" pageId="53" pageNumber="53" specimenCode="AMNH 656718, AMNH 656724, AMNH 656719-656722, AMNH 656725" specimenCount="1" stateProvince="Rivers" typeStatus="paratype">
<emphasis bold="true" box="[809,990,866,887]" pageId="53" pageNumber="53">
<specimenCode box="[809,990,866,887]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="53" pageNumber="53">AMNH 656718</specimenCode>
