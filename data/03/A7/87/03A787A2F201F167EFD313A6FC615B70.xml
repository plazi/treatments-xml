<materialsCitation collectingDate="1913-05" collectionCode="AMNH" collectorName="Bowyer-Bower &amp; Cairns &amp; Mathews &amp; Mathews Collection" location="Mathews Collection" municipality="Three" pageId="53" pageNumber="53" specimenCode="AMNH 656727-656729" specimenCount="2" stateProvince="Rivers" typeStatus="paratype">
