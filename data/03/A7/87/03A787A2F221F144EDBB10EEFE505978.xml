<emphasis bold="true" box="[93,288,808,829]" pageId="22" pageNumber="22">
<specimenCode box="[93,288,808,829]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="22" pageNumber="22">AMNH 657413</specimenCode>
