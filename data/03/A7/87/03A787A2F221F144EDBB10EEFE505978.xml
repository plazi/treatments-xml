<specimenCode box="[329,522,720,741]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="22" pageNumber="22">AMNH 657409</specimenCode>
</emphasis>
(Kühn no. 5497), 
<emphasis bold="true" box="[231,419,750,771]" pageId="22" pageNumber="22">
<specimenCode box="[231,419,750,771]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="22" pageNumber="22">AMNH 657410</specimenCode>
</emphasis>
(5500), 
<emphasis bold="true" pageId="22" pageNumber="22">
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="22" pageNumber="22">AMNH 657411</specimenCode>
</emphasis>
(5499), 
<emphasis bold="true" box="[282,468,779,800]" pageId="22" pageNumber="22">
<specimenCode box="[282,468,779,800]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="22" pageNumber="22">AMNH 657412</specimenCode>
