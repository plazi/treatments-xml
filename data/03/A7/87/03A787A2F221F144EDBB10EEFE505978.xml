<collectionCode box="[330,424,691,712]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="22" pageNumber="22">AMNH</collectionCode>
