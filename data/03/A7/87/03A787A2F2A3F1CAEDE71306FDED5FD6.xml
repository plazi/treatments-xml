</materialsCitation>
; 
<materialsCitation collectingDate="1893-09" collectingDateMax="1893-10-31" collectingDateMin="1893-09" collectionCode="AMNH" location="Everett on Bunguran Island" pageId="152" pageNumber="152" specimenCode="AMNH 685339-685346" specimenCount="2" specimenCount-female="1" specimenCount-male="1" typeStatus="lectotype">
