), 
<collectingCountry box="[1133,1278,298,322]" name="South Africa" pageId="14" pageNumber="15">South Africa</collectingCountry>
(
<bibRefCitation author="Stuckenberg" pageId="14" pageNumber="15" refString="Stuckenberg, B. R. (1973) New and little-known Clusiidae (Diptera) from South Africa and Mocambique. Annals of the Natal Museum, 21, 579 - 593." type="journal article" year="1973">
Stuckenberg 
<number box="[151,210,335,359]" pageId="14" pageNumber="15" value="1973.0">1973</number>
