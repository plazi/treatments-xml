and 
<quantity box="[197,295,1281,1308]" metricMagnitude="0" metricUnit="m" metricValue="1.3208" metricValueMax="1.4224" metricValueMin="1.2192" pageId="15" pageNumber="514" unit="in" value="52.0" valueMax="56.0" valueMin="48.0">48–56 in</quantity>
