<taxonomicName authorityName="E. P. Ramsay" authorityYear="1876" box="[449,617,1195,1216]" class="Aves" family="Pachycephalidae" genus="Pachycephala" kingdom="Animalia" order="Passeriformes" pageId="31" pageNumber="31" phylum="Chordata" rank="species" species="citreogaster">
