<paragraph blockId="7.[151,1436,692,935]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[198,254,837,863]" pageId="7" pageNumber="8">Host</emphasis>
: 
<taxonomicName authorityName="Fosberg" authorityYear="1964" baseAuthorityName="Mill." box="[270,472,837,863]" class="Magnoliopsida" family="Sapotaceae" genus="Manilkara" kingdom="Plantae" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="achras">
<emphasis box="[270,472,837,863]" italics="true" pageId="7" pageNumber="8">Manilkara achras</emphasis>
</taxonomicName>
(
<taxonomicName box="[489,618,837,863]" class="Magnoliopsida" family="Sapotaceae" kingdom="Plantae" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="family">Sapotaceae</taxonomicName>
