<quantity box="[827,924,357,379]" metricMagnitude="-3" metricUnit="m" metricValue="9.25" metricValueMax="9.5" metricValueMin="9.0" pageId="31" pageNumber="232" unit="mm" value="9.25" valueMax="9.5" valueMin="9.0">9–9.5 mm</quantity>
