<location LSID="urn:lsid:plazi:treatment:03A787A2F278F11FEDAB13A4FF225EA1:8ED1606FF279F11FED3A150FFD0C5E0A" box="[608,653,867,889]" county="Rothschild Collection" municipality="Specimens" name="The" pageId="77" pageNumber="77" stateProvince="Okinawa">The</location>
<typeStatus box="[128,230,897,919]" pageId="77" pageNumber="77">holotype</typeStatus>
