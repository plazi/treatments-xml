<location LSID="urn:lsid:plazi:treatment:03A787A2F278F11FEDAB13A4FF225EA1:8ED1606FF279F11FEFDA1566FE015E53" box="[128,384,778,800]" county="Rothschild Collection" municipality="Specimens" name="Rothschild Collection" pageId="77" pageNumber="77" stateProvince="Okinawa">Rothschild Collection</location>
