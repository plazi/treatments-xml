<location LSID="urn:lsid:plazi:treatment:03A787A2F278F11FEDAB13A4FF225EA1:8ED1606FF279F11FEE7C152AFEFC5E28" box="[294,381,838,859]" county="Rothschild Collection" municipality="Specimens" name="Hartert" pageId="77" pageNumber="77" stateProvince="Okinawa">Hartert</location>
