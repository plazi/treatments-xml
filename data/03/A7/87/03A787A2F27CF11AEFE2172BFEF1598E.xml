<location LSID="urn:lsid:plazi:treatment:03A787A2F27CF11AEFE2172BFEF1598E:8ED1606FF27CF11AEFF915CAFF785EC8" box="[163,249,934,955]" country="Philippines" county="Whitney Hall of Oceanic Birds" municipality="Mt. Cayapo" name="Lamao" pageId="72" pageNumber="72">Lamao</location>
