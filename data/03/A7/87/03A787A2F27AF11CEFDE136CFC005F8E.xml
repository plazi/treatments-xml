<taxonomicName authorityName="Blyth" authorityYear="1855" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="78" pageNumber="78" phylum="Chordata" rank="subSpecies" species="spilonotus" subSpecies="subviridis">
