<quantity box="[400,556,1598,1622]" metricMagnitude="3" metricUnit="m" metricValue="1.605" metricValueMax="1.8699999999999999" metricValueMin="1.34" pageId="11" pageNumber="162" unit="m" value="1605.0" valueMax="1870.0" valueMin="1340.0">1340-1870 m</quantity>
., 
<date box="[571,750,1597,1622]" pageId="11" pageNumber="162" value="2008-04-15" valueMax="2008-05-15" valueMin="2008-04-15">
<number box="[571,599,1597,1622]" pageId="11" pageNumber="162" value="15.0">15</number>
.iv- 
<number box="[636,664,1597,1622]" pageId="11" pageNumber="162" value="15.0">15</number>
.v. 
