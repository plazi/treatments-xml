<materialsCitation collectingDate="1911-08-18" collectionCode="AMNH" collectorName="Whitlock" location="Range" municipality="Mathews" pageId="85" pageNumber="85" specimenCode="AMNH 684252" specimenCount="1" stateProvince="Stirling" typeStatus="holotype">
