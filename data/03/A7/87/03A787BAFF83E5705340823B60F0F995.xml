<taxonomicName authority="sensu" authorityName="SenSU" class="Insecta" family="Staphylinidae" genus="Zyras" kingdom="Animalia" order="Coleoptera" pageId="66" pageNumber="186" phylum="Arthropoda" rank="genus">
