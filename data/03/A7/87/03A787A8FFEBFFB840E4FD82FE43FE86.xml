<paragraph blockId="17.[212,1172,731,1011]" lastBlockId="18.[144,1104,168,383]" lastPageId="18" lastPageNumber="2406" pageId="17" pageNumber="2405">
<emphasis box="[212,433,732,756]" italics="true" pageId="17" pageNumber="2405">
