</figureCitation>
) length 
<number box="[1303,1432,1869,1894]" pageId="8" pageNumber="9" value="1625.0" valueMax="1862.0" valueMin="1388.0">
<date box="[1303,1432,1869,1894]" pageId="8" pageNumber="9" value="1388" valueMax="1862">1388–1862</date>
