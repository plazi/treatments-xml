<number box="[542,556,1599,1623]" pageId="2" pageNumber="3" value="2.0">2</number>
) 
<number box="[572,674,1599,1624]" pageId="2" pageNumber="3" value="284.0" valueMax="306.0" valueMin="262.0">262–306</number>
