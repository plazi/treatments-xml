<subSubSection box="[264,1208,1201,1227]" pageId="1" pageNumber="2" type="materials_examined">
<paragraph blockId="1.[264,1208,1201,1227]" box="[264,1208,1201,1227]" pageId="1" pageNumber="2">
<emphasis bold="true" box="[264,414,1201,1227]" italics="true" pageId="1" pageNumber="2">
<typeStatus box="[264,321,1201,1227]" pageId="1" pageNumber="2">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Minei, 1973" authorityName="Minei" authorityYear="1973" box="[428,924,1201,1227]" class="Malacostraca" family="Potamidae" genus="Candidiopotamon" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="amamense">
<emphasis box="[428,771,1201,1226]" italics="true" pageId="1" pageNumber="2">Candidiopotamon amamense</emphasis>
<bibRefCitation author="Minei" box="[778,924,1201,1227]" pageId="1" pageNumber="6" refString="Minei, H. (1973) Potamoid crabs of the Ryukyu Islands, with descriptions of five new species (Crustacea, Decapoda, Potamoidea). Journal of the Faculty of Agriculture, Kyushu University, 17, 203 - 226." type="journal article" year="1973">
