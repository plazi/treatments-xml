<paragraph blockId="0.[151,578,1951,2010]" lastBlockId="1.[151,1436,152,313]" lastPageId="1" lastPageNumber="378" pageId="0" pageNumber="377">
<emphasis bold="true" lastPageId="1" lastPageNumber="378" pageId="0" pageNumber="377">
<typeStatus box="[151,259,1987,2010]" pageId="0" pageNumber="377">Holotype</typeStatus>
: CNU-ORT-NN2011010PC Diagnosis
</emphasis>
