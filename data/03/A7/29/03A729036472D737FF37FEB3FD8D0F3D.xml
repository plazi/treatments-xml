<location LSID="urn:lsid:plazi:treatment:03A729036472D737FF37FEB3FD8D0F3D:8ED1CECE6472D737FEC1FE1CFE380AFD" box="[380,505,477,500]" country="United Kingdom" county="Caerfyrddin" latitude="51.86" longLatPrecision="652" longitude="-4.31" municipality="Anglicized" name="Carmarthen" pageId="12" pageNumber="2301">Carmarthen</location>
