<number box="[829,859,268,294]" pageId="5" pageNumber="55" value="18.0">18</number>
verticals and 
<number box="[1024,1039,268,294]" pageId="5" pageNumber="55" value="4.0">4</number>
postorbitals. Clypeus with 
<number box="[1362,1435,268,294]" pageId="5" pageNumber="55" value="44.5" valueMax="46.0" valueMin="43.0">43–46</number>
setae. Tentorium 
<number box="[371,480,306,332]" pageId="5" pageNumber="55" value="190.0" valueMax="200.0" valueMin="180.0">180–200</number>
µm long, 
<number box="[613,644,306,332]" pageId="5" pageNumber="55" value="40.0">40</number>
µm wide. Length of palpomeres 
<number box="[1069,1116,306,332]" pageId="5" pageNumber="55" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
(in µm): 
<number box="[1240,1273,306,332]" pageId="5" pageNumber="55" value="70.0">70</number>
–75, 90– 
<number pageId="5" pageNumber="55" value="172707.5" valueMax="235235.0" valueMin="110180.0">110, 180–235, 235</number>
– 
<number box="[329,429,345,371]" pageId="5" pageNumber="55" value="295315.0">295, 315</number>
– 
<number box="[445,491,345,371]" pageId="5" pageNumber="55" value="400.0">400</number>
.
</paragraph>
