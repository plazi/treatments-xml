<taxonomicName authority="Beier. The" authorityName="Beier. The" class="Arachnida" family="Chernetidae" genus="Asterochernes" kingdom="Animalia" order="Pseudoscorpiones" pageId="12" pageNumber="13" phylum="Arthropoda" rank="subSpecies" species="kuscheli" subSpecies="patagonicus">
