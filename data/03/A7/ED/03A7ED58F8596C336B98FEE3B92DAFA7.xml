<subSubSection box="[199,1225,439,464]" pageId="32" pageNumber="33" type="etymology">
<paragraph blockId="32.[151,1437,151,501]" box="[199,1225,439,464]" pageId="32" pageNumber="33">
<emphasis bold="true" box="[199,334,439,464]" pageId="32" pageNumber="33">Etymology.</emphasis>
