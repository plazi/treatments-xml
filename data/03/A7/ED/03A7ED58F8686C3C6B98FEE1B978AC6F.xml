<subSubSection box="[199,1009,511,536]" pageId="47" pageNumber="48" type="etymology">
<paragraph blockId="47.[151,1437,151,573]" box="[199,1009,511,536]" pageId="47" pageNumber="48">
<emphasis bold="true" box="[199,334,511,536]" pageId="47" pageNumber="48">Etymology.</emphasis>
