<taxonomicName box="[474,548,513,536]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="grossa">
<emphasis box="[474,548,513,536]" italics="true" pageId="47" pageNumber="48">grossa</emphasis>
</taxonomicName>
