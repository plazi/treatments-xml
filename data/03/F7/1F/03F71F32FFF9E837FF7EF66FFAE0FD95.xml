<paragraph blockId="10.[151,1437,151,2035]" box="[199,1315,1617,1642]" pageId="10" pageNumber="393">
<emphasis box="[199,333,1617,1642]" inLineHeading="true" pageId="10" pageNumber="393" reason="1">
<emphasis bold="true" box="[199,326,1617,1642]" pageId="10" pageNumber="393">Etymology</emphasis>
