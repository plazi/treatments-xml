<subSubSection box="[199,1300,655,680]" pageId="5" pageNumber="62" type="etymology">
<paragraph blockId="5.[151,1437,151,896]" box="[199,1300,655,680]" pageId="5" pageNumber="62">
<emphasis bold="true" box="[199,334,656,680]" pageId="5" pageNumber="62">Etymology.</emphasis>
