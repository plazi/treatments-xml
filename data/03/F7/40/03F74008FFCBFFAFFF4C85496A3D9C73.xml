Central 
<collectingCountry box="[448,552,1269,1295]" name="Tanzania" pageId="24" pageNumber="175">Tanzania</collectingCountry>
, 
<collectingRegion box="[563,751,1269,1295]" country="Tanzania" name="Dodoma" pageId="24" pageNumber="175">Dodoma Region</collectingRegion>
