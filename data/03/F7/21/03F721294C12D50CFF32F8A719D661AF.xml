<tr box="[159,1411,724,746]" gridrow="4" pageId="7" pageNumber="134" rowspan-0="1">
<td box="[331,443,724,746]" gridcol="1" gridrow="4" pageId="7" pageNumber="134">Triactine II</td>
<td box="[511,605,724,746]" gridcol="2" gridrow="4" pageId="7" pageNumber="134">Basal</td>
<td box="[663,720,724,746]" gridcol="3" gridrow="4" pageId="7" pageNumber="134">67.5</td>
<td box="[778,833,724,746]" gridcol="4" gridrow="4" pageId="7" pageNumber="134">77.9</td>
<td box="[873,905,724,746]" gridcol="5" gridrow="4" pageId="7" pageNumber="134">5.7</td>
<td box="[938,995,724,746]" gridcol="6" gridrow="4" pageId="7" pageNumber="134">86.4</td>
<td box="[1039,1079,724,746]" gridcol="7" gridrow="4" pageId="7" pageNumber="134">8.1</td>
<td box="[1138,1193,724,746]" gridcol="8" gridrow="4" pageId="7" pageNumber="134">11.1</td>
