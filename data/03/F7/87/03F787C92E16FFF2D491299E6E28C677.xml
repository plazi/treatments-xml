<taxonomicName authority="Nutt." authorityName="Nutt." box="[679,964,1827,1851]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="species" species="parviflorus">
<emphasis box="[679,894,1827,1851]" italics="true" pageId="46" pageNumber="45">Rubus parviflorus</emphasis>
Nutt.
</taxonomicName>
(
<taxonomicName box="[978,1093,1827,1851]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
