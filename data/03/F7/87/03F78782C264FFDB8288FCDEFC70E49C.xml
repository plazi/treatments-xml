<number box="[528,589,1076,1100]" pageId="11" pageNumber="45" value="1987.0">1987</number>
</bibRefCitation>
</bibRefCitation>
: 
<number box="[599,630,1076,1100]" pageId="11" pageNumber="45" value="20.0">20</number>
, sub 
<taxonomicName box="[686,877,1076,1100]" class="Insecta" family="Cixiidae" genus="Oliarus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="45" phylum="Arthropoda" rank="species" species="zairensis">
<emphasis box="[686,877,1076,1100]" italics="true" pageId="11" pageNumber="45">Oliarus zairensis</emphasis>
</taxonomicName>
