in largest cells, cell wall 
<quantity box="[205,305,836,860]" metricMagnitude="-5" metricUnit="m" metricValue="6.0" pageId="5" pageNumber="588" unit="mm" value="0.06">0.06 mm</quantity>
thick; for the nest Nest#VN-HL- 
<number box="[677,733,836,860]" pageId="5" pageNumber="588" value="2015.0">2015</number>
-P-02 each cell expanded towards open end 
<quantity box="[1218,1303,836,860]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="5" pageNumber="588" unit="mm" value="3.7">3.7 mm</quantity>
(range 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.8499999999999996" metricValueMax="4.3" metricValueMin="3.4" pageId="5" pageNumber="588" unit="mm" value="3.8499999999999996" valueMax="4.3" valueMin="3.4">3.4– 4.3 mm</quantity>
