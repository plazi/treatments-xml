<paragraph blockId="1.[151,677,580,606]" box="[151,677,580,606]" pageId="1" pageNumber="406">
<typeStatus box="[151,201,582,606]" pageId="1" pageNumber="406">Type</typeStatus>
species: 
<taxonomicName authority="Niisato &amp; Liu, 2019" authorityName="Niisato &amp; Liu" authorityYear="2019" box="[294,536,581,605]" class="Insecta" family="Cerambycidae" genus="Sichuanortia" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="406" phylum="Arthropoda" rank="species" species="zhouchaoi" status="new species">
