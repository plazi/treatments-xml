<taxonomicName authority="Liebherr" authorityName="Liebherr" authorityYear="2017" box="[735,1060,790,811]" class="Insecta" family="Carabidae" genus="Mecyclothorax" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="691" phylum="Arthropoda" rank="species" species="brispex" status="new species">
