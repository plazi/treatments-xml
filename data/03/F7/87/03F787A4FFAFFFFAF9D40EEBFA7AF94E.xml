<taxonomicName authority="Jacoby" authorityName="Jacoby" authorityYear="1884" box="[1182,1440,898,922]" class="Insecta" family="Chrysomelidae" genus="Phrynocepha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="capitata">
