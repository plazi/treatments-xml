(
<bibRefCitation author="Ivinskis P" box="[884,1040,1504,1530]" pageId="7" pageNumber="8" refId="ref39388" refString="Ivinskis P (1993) Check-list of Lithuanian Lepidoptera. Institute of Ecology, Vilnius, 210 pp." type="book" year="1993">Ivinskis 1993</bibRefCitation>
; 
<bibRefCitation author="Ivinskis P" pageId="7" pageNumber="8" refId="ref39407" refString="Ivinskis P (2004) Lepidoptera of Lithuania. Annotated catalogue. Lietuvos drugiai (Lepidoptera). Katalogas. Vilniaus Universiteto Ekologijos Institutas, Vilnius, 379 pp." type="book" year="2004">Ivinskis 2004</bibRefCitation>
; 
<bibRefCitation author="Ivinskis P &amp; Pakalniskis S &amp; Puplesis R" box="[261,476,1539,1566]" pageId="7" pageNumber="8" pagination="1 - 240" refId="ref39437" refString="Ivinskis P, Pakalniskis S, Puplesis R (1985) Augalus Minuojantys Vabzdziai [Leafmining insects]. Mokslas, Vilnius, 1 - 240 pp." type="book chapter" year="1985">Ivinskis et al. 1985</bibRefCitation>
