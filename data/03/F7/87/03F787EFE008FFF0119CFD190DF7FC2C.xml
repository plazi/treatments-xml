<collectingCountry box="[396,489,1626,1651]" name="Albania" pageId="21" pageNumber="126">Albania</collectingCountry>
, 
<collectingCountry box="[500,579,1627,1650]" name="France" pageId="21" pageNumber="126">France</collectingCountry>
, 
<collectingCountry box="[590,695,1627,1651]" name="Germany" pageId="21" pageNumber="126">Germany</collectingCountry>
, 
<collectingCountry box="[707,805,1626,1651]" name="Portugal" pageId="21" pageNumber="126">Portugal</collectingCountry>
, 
<collectingCountry box="[815,882,1626,1651]" name="Spain" pageId="21" pageNumber="126">Spain</collectingCountry>
, 
<collectingCountry box="[893,987,1626,1651]" name="Sweden" pageId="21" pageNumber="126">Sweden</collectingCountry>
). Introduced in 
<collectingCountry box="[1162,1268,1626,1651]" name="Australia" pageId="21" pageNumber="126">Australia</collectingCountry>
.
</paragraph>
