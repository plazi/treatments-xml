or.,
</emphasis>
<collectingCountry box="[549,614,1698,1722]" name="Lebanon" pageId="4" pageNumber="441">Libáň</collectingCountry>
, 
<date box="[628,750,1698,1722]" pageId="4" pageNumber="441" value="2012-06-15">
