<quantity box="[794,932,1073,1098]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" metricValueMax="3.8" metricValueMin="3.6" pageId="14" pageNumber="375" unit="mm" value="3.7" valueMax="3.8" valueMin="3.6">3.6–3.8 mm</quantity>
