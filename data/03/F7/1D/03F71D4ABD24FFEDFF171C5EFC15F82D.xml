, 
<emphasis bold="true" box="[501,680,1789,1814]" pageId="12" pageNumber="373">Zhejiang Prov.</emphasis>
, Hangzhouwan Wetland, 
<date box="[977,1115,1789,1814]" pageId="12" pageNumber="373" value="2010-08-15">
<number box="[977,1004,1789,1814]" pageId="12" pageNumber="373" value="15.0">15</number>
