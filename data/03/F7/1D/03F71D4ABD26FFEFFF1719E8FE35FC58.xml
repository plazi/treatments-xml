<quantity box="[794,933,404,429]" metricMagnitude="-3" metricUnit="m" metricValue="3.35" metricValueMax="3.5" metricValueMin="3.2" pageId="14" pageNumber="375" unit="mm" value="3.35" valueMax="3.5" valueMin="3.2">3.2–3.5 mm</quantity>
