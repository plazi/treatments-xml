<quantity box="[554,692,404,428]" metricMagnitude="-3" metricUnit="m" metricValue="3.25" metricValueMax="3.4" metricValueMin="3.1" pageId="14" pageNumber="375" unit="mm" value="3.25" valueMax="3.4" valueMin="3.1">3.1–3.4 mm</quantity>
