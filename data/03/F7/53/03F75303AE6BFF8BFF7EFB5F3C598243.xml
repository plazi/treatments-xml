<taxonomicName authority="Verrill, 1900" authorityName=", Verrill" authorityYear="1900" box="[300,849,1270,1293]" class="Polychaeta" family="Terebellidae" genus="Eupolymnia" kingdom="Animalia" order="Terebellida" pageId="53" pageNumber="54" phylum="Annelida" rank="species" species="aurantiaca" subGenus="Polymniella">
