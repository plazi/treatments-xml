species:
</emphasis>
<taxonomicName authority="Ehlers, 1897" authorityName="Ehlers" authorityYear="1897" box="[300,669,2014,2037]" class="Polychaeta" family="Terebellidae" genus="Leprea" kingdom="Animalia" order="Terebellida" pageId="41" pageNumber="42" phylum="Annelida" rank="species" species="streptochaeta">
