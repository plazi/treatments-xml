<paragraph blockId="11.[151,1437,711,1206]" pageId="11" pageNumber="294">
<emphasis bold="true" box="[199,315,1143,1169]" pageId="11" pageNumber="294">Remarks.</emphasis>
<taxonomicName authorityName="Cartwright" authorityYear="1990" box="[324,499,1143,1169]" class="Insecta" family="Ecnomidae" genus="Ecnomus" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="294" phylum="Arthropoda" rank="species" species="blythi">
<emphasis box="[324,499,1143,1169]" italics="true" pageId="11" pageNumber="294">Ecnomus blythi</emphasis>
