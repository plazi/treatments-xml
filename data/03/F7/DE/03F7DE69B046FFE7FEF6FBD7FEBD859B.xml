<collectionCode LSID="urn:lsid:biocol.org:col:34838" box="[777,836,1292,1318]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="13" pageNumber="48">SMF</collectionCode>
