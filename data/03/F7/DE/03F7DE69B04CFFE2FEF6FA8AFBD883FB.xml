<number box="[644,706,364,390]" pageId="8" pageNumber="43" value="1000.0">1000</number>
; chelicera L 
<number box="[861,923,364,390]" pageId="8" pageNumber="43" value="2200.0">2200</number>
, palp total L 
<number box="[1083,1144,364,390]" pageId="8" pageNumber="43" value="1730.0">1730</number>
, segments L/H P­ 
<number box="[295,305,404,430]" pageId="8" pageNumber="43" value="1.0">1</number>
