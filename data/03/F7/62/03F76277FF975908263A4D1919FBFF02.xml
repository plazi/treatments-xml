<number box="[791,912,1479,1505]" pageId="1" pageNumber="28" value="0.14500000000000002" valueMax="0.15" valueMin="0.14">0.14–0.15</number>
; parameres 
<number box="[1054,1109,1479,1505]" pageId="1" pageNumber="28" value="0.04">0.04</number>
