, G. Steyskal (
<number box="[1103,1115,1378,1402]" pageId="16" pageNumber="61" value="1.0">1</number>
<number box="[1123,1144,1379,1403]" pageId="16" pageNumber="61" value="3.0">3</number>
, 
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[1158,1244,1378,1402]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="16" pageNumber="61">USNM</collectionCode>
