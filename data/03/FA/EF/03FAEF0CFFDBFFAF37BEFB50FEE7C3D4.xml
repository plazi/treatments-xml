<number box="[1015,1131,223,247]" pageId="4" pageNumber="87" value="30.15" valueMax="30.5" valueMin="29.8">29.8–30.5</number>
; fore wing width: 
<number box="[1344,1432,223,247]" pageId="4" pageNumber="87" value="9.7" valueMax="9.8" valueMin="9.6">9.6–9.8</number>
; head width: 
<number box="[289,324,260,284]" pageId="4" pageNumber="87" value="6.8">6.8</number>
