<quantity box="[716,854,2009,2034]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" metricValueMax="5.5" metricValueMin="4.5" pageId="6" pageNumber="247" unit="mm" value="5.0" valueMax="5.5" valueMin="4.5">4.5–5.5 mm</quantity>
