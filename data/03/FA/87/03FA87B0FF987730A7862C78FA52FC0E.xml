<emphasis bold="true" box="[1212,1440,803,827]" pageId="9" pageNumber="8">
<taxonomicName authority="Sokolov, 2011" authorityName="Sokolov" authorityYear="2011" box="[1212,1334,803,827]" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="species" species="unicoi" status="sp.nov.">
