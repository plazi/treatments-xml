<paragraph blockId="2.[151,596,359,385]" box="[151,596,359,385]" pageId="2" pageNumber="54">
<heading bold="true" box="[151,596,359,385]" fontSize="11" level="1" pageId="2" pageNumber="54" reason="1">
<emphasis bold="true" box="[151,596,359,385]" pageId="2" pageNumber="54">
Subfamily 
<taxonomicName authority="Banks, 1895" authorityName="Banks" authorityYear="1895" box="[287,596,359,385]" class="Arachnida" family="Syarinidae" kingdom="Animalia" order="Pseudoscorpiones" pageId="2" pageNumber="54" phylum="Arthropoda" rank="subFamily" subFamily="Ideobisiinae">Ideobisiinae Banks, 1895</taxonomicName>
