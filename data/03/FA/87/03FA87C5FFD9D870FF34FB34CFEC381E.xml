<taxonomicName authority="Roelofs 1876" authorityName="Roelofs" authorityYear="1876" box="[840,1159,1645,1671]" class="Insecta" family="Curculionidae" genus="Pentarthrum" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="51" phylum="Arthropoda" rank="species" species="wollastoni">
