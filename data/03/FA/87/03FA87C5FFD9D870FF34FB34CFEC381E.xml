<taxonomicName authority="Schaufuss 1887" authorityName="Schaufuss" authorityYear="1887" box="[923,1273,1720,1746]" class="Insecta" family="Curculionidae" genus="Pentarthrum" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="51" phylum="Arthropoda" rank="species" species="celebensis">
