<paragraph blockId="15.[151,1437,1337,1758]" pageId="15" pageNumber="208">
<materialsCitation ID-GBIF-Occurrence="3325974330" collectingDate="1998-11" collectingDateMax="1998-12-31" collectingDateMin="1998-11" location="Loxton" municipality="Described" pageId="15" pageNumber="208" specimenCount="3" specimenCount-female="2" specimenCount-male="1" stateProvince="New South Wales">
