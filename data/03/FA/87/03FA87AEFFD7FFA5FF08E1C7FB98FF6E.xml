and 
<taxonomicName baseAuthorityName="Pic" baseAuthorityYear="1906" box="[542,609,887,911]" class="Insecta" family="Aderidae" genus="Cobososia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="369" phylum="Arthropoda" rank="species" species="feai">
