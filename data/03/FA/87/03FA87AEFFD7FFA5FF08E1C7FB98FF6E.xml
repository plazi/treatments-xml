</bibRefCitation>
as 
<taxonomicName authorityName="Baguena Corella" authorityYear="1948" box="[421,528,1132,1156]" class="Insecta" family="Aderidae" genus="Cobososia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="369" phylum="Arthropoda" rank="genus">
