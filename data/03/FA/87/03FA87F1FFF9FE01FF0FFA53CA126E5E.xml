<location LSID="urn:lsid:plazi:treatment:03FA87F1FFF9FE01FF0FFA53CA126E5E:8E8C603CFFF9FE03FD39F9C5C8796A40" box="[673,781,1624,1647]" country="Virgin Islands" county="The" municipality="Jerusalem'" name="Bailiwick" pageId="0" pageNumber="586" stateProvince="Brandenburg">Bailiwick</location>
of 
<collectingRegion box="[821,959,1624,1647]" country="Germany" name="Brandenburg" pageId="0" pageNumber="586">Brandenburg</collectingRegion>
of the 
<location LSID="urn:lsid:plazi:treatment:03FA87F1FFF9FE01FF0FFA53CA126E5E:8E8C603CFFF9FE03FB88F9C5CFCE6A40" box="[1040,1210,1624,1647]" country="Virgin Islands" county="The" municipality="Jerusalem'" name="Chivalric Order" pageId="0" pageNumber="586" stateProvince="Brandenburg">Chivalric Order</location>
of 
<collectingCountry box="[1249,1363,1624,1647]" name="Virgin Islands" pageId="0" pageNumber="586">Saint John</collectingCountry>
