, 
<quantity box="[1010,1071,2005,2028]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="0" pageNumber="586" unit="m" value="300.0">
<elevation box="[1010,1071,2005,2028]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="0" pageNumber="586" unit="m" value="300.0">300m</elevation>
</quantity>
a
</materialsCitation>
. 
<materialsCitation ID-GBIF-Occurrence="1306553257" country="Turkey" elevation="375" lastPageId="1" lastPageNumber="587" location="Ezrin" municipality="Antakya" pageId="0" pageNumber="586" specimenCount="1" stateProvince="Adana">
s.l. [above sea level] in the year 1967 and 
<collectingMunicipality box="[263,353,152,175]" pageId="1" pageNumber="587">Antakya</collectingMunicipality>
<locationDeviation box="[363,587,152,175]" pageId="1" pageNumber="587">
<quantity box="[363,420,152,175]" metricMagnitude="3" metricUnit="m" metricValue="7.0" pageId="1" pageNumber="587" unit="km" value="7.0">7 km</quantity>
E of Yeşilkent
</locationDeviation>
[
<location LSID="urn:lsid:plazi:treatment:03FA87F1FFF9FE01FF0FFA53CA126E5E:8E8C603CFFF8FE02FDC5FF05C9EA6C80" box="[605,670,152,175]" country="Turkey" municipality="Antakya" name="Ezrin" pageId="1" pageNumber="587" stateProvince="Adana">Ezrin</location>
], 
<quantity box="[690,804,152,175]" metricMagnitude="2" metricUnit="m" metricValue="3.75" metricValueMax="4.0" metricValueMin="3.5" pageId="1" pageNumber="587" unit="m" value="375.0" valueMax="400.0" valueMin="350.0">
<elevation box="[690,804,152,175]" metricMagnitude="2" metricUnit="m" metricValue="3.75" metricValueMax="4.0" metricValueMin="3.5" pageId="1" pageNumber="587" unit="m" value="375.0" valueMax="400.0" valueMin="350.0">350–400m</elevation>
</quantity>
a
