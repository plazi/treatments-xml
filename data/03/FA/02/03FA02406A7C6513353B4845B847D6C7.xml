; length of thorax,? 
<quantity box="[999,1154,1504,1527]" metricMagnitude="-4" metricUnit="m" metricValue="8.3" metricValueMax="9.3" metricValueMin="7.3" pageId="57" pageNumber="200" unit="mm" value="0.83" valueMax="0.93" valueMin="0.73">0.73–0.93 mm</quantity>
,! 
<quantity box="[1190,1346,1504,1527]" metricMagnitude="-4" metricUnit="m" metricValue="9.15" metricValueMax="9.7" metricValueMin="8.6" pageId="57" pageNumber="200" unit="mm" value="0.915" valueMax="0.97" valueMin="0.86">0.86–0.97 mm</quantity>
; length of wing,? 
<quantity box="[918,1049,1533,1556]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="1.9" metricValueMin="1.6" pageId="57" pageNumber="200" unit="mm" value="1.75" valueMax="1.9" valueMin="1.6">1.6–1.9 mm</quantity>
,! 
<quantity box="[1089,1220,1533,1556]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" metricValueMax="2.2" metricValueMin="2.1" pageId="57" pageNumber="200" unit="mm" value="2.15" valueMax="2.2" valueMin="2.1">2.1–2.2 mm</quantity>
