<taxonomicName authority="Nikitsky" authorityName="Nikitsky" authorityYear="1983" class="Insecta" family="Tetratomidae" genus="Pseudoholostrophus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="267" phylum="Arthropoda" rank="genus">
