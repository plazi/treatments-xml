<date box="[887,950,1901,1926]" pageId="5" pageNumber="500" value="1958-03">
III 
<number box="[922,950,1901,1926]" pageId="5" pageNumber="500" value="58.0">
<emphasis bold="true" box="[922,950,1901,1926]" pageId="5" pageNumber="500">58</emphasis>
</number>
</date>
