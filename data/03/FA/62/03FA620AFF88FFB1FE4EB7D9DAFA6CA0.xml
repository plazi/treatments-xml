<collectingCountry box="[562,630,348,372]" name="Oman" pageId="2" pageNumber="171">Oman</collectingCountry>
, 
<collectingCountry box="[639,682,348,371]" name="Iran" pageId="2" pageNumber="171">Iran</collectingCountry>
and the specimen from Avaran in 
<collectingCountry box="[1036,1126,347,371]" name="Pakistan" pageId="2" pageNumber="171">Pakistan</collectingCountry>
