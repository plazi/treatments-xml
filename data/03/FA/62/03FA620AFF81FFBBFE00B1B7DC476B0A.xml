), 
<collectingCountry box="[693,787,1313,1337]" name="Thailand" pageId="8" pageNumber="177">Thailand</collectingCountry>
(
<bibRefCitation author="UHMANN G." box="[800,948,1313,1337]" pageId="8" pageNumber="177" pagination="243 - 252" refId="ref9069" refString="UHMANN G. 1989: Anthicidae (Coleoptera der orientalischen Region aus dem Naturhistorischen Museum in Genf, II. Revue Suisse de Zoologie 96: 243 - 252." type="journal article" year="1989">UHMANN 1989</bibRefCitation>
as 
<taxonomicName box="[981,1119,1314,1337]" class="Insecta" family="Anthicidae" genus="Stenidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="177" phylum="Arthropoda" rank="species" species="longitarsis">
<emphasis box="[981,1119,1314,1337]" italics="true" pageId="8" pageNumber="177">S. longitarsis</emphasis>
</taxonomicName>
; 
<bibRefCitation author="UHMANN G." box="[151,305,1345,1369]" pageId="8" pageNumber="177" pagination="19 - 36" refId="ref9208" refString="UHMANN G. 1996: Anthiciden aus dem Naturhistorischen Museum in Wien (Coleoptera, Anthicidae). Entomologische Blatter 92: 19 - 36." type="journal article" year="1996">UHMANN 1996</bibRefCitation>
, 
<bibRefCitation author="UHMANN G." box="[317,374,1345,1369]" pageId="8" pageNumber="177" pagination="145 - 160" refId="ref9234" refString="UHMANN G. 2000: Anthicidae (Coleoptera) aus verschiedenen Regionen. Annales Historico-Naturales Musei Nationalis Hungarici 92: 145 - 160." type="journal article" year="2000">2000</bibRefCitation>
), 
<collectingCountry box="[393,483,1345,1369]" name="Vietnam" pageId="8" pageNumber="177">Vietnam</collectingCountry>
(
<bibRefCitation author="UHMANN G." box="[498,667,1345,1369]" pageId="8" pageNumber="177" pagination="655 - 676" refId="ref9153" refString="UHMANN G. 1994 b: Sudostasiatische Anthiciden aus dem Naturhistorischen Museum in Genf, 4 (Coleoptera Anthicidae). Revue Suisse de Zoologie 101: 655 - 676." type="journal article" year="1994">UHMANN 1994b</bibRefCitation>
; 
<bibRefCitation author="TELNOV D." box="[678,823,1345,1369]" pageId="8" pageNumber="177" pagination="81 - 100" refId="ref8795" refString="TELNOV D. 1998: Anthicidae (Coleoptera) der Sammlung Sergej Kurbatov, mit Beschreibung von sechs neuen Arten aus der Orientalis. Bulletin et Annales de la Societe Royale Belge d'Entomologie 134: 81 - 100." type="journal article" year="1998">TELNOV 1998</bibRefCitation>
