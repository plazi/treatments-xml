<taxonomicName authority="Coiffait, 1970" authorityName="Coiffait" authorityYear="1970" box="[661,996,546,572]" class="Insecta" family="Staphylinidae" genus="Xantholinus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="66" phylum="Arthropoda" rank="species" species="sublinearis">
