<subSubSection pageId="5" pageNumber="64" type="nomenclature">
<paragraph blockId="5.[151,1142,1893,1919]" box="[151,1142,1893,1919]" pageId="5" pageNumber="64">
<heading bold="true" box="[151,1142,1893,1919]" fontSize="11" level="1" pageId="5" pageNumber="64" reason="1">
<emphasis bold="true" box="[151,1142,1893,1919]" pageId="5" pageNumber="64">
Key to Mediterranean species of 
<taxonomicName box="[562,712,1893,1919]" class="Insecta" family="Staphylinidae" genus="Xantholinus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[562,712,1893,1919]" italics="true" pageId="5" pageNumber="64">Xantholinus</emphasis>
</taxonomicName>
, subgenus 
<taxonomicName authority="Coiffait, 1956" authorityName="Coiffait" authorityYear="1956" box="[849,1142,1893,1919]" class="Insecta" family="Staphylinidae" genus="Calolinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="genus">
