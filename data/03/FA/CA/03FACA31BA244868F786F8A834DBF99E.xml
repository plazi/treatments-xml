</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="6" pageNumber="65">
<paragraph blockId="6.[151,1437,152,1618]" pageId="6" pageNumber="65">
<keyLead pageId="6" pageNumber="65">
<number box="[151,168,392,418]" pageId="6" pageNumber="65" value="3.0">3</number>
