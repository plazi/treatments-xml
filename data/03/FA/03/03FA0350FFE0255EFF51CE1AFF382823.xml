<materialsCitation ID-GBIF-Occurrence="3407812449" collectionCode="SVL, R" county="The" location="Fraser's Hill" municipality="Gap" pageId="79" pageNumber="80" specimenCode="LSUHC 8079, ZRC 2.6777" specimenCount="1" stateProvince="Pahang" typeStatus="holotype">
