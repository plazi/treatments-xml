<quantity box="[808,927,1054,1075]" metricMagnitude="-3" metricUnit="m" metricValue="6.45" metricValueMax="7.2" metricValueMin="5.7" pageId="2" pageNumber="110" unit="mm" value="6.45" valueMax="7.2" valueMin="5.7">5.7–7.2 mm</quantity>
