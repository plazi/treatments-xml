<collectingRegion box="[1018,1088,1665,1691]" country="China" name="Anhui" pageId="1" pageNumber="128">Anhui</collectingRegion>
and 
<collectingRegion box="[1145,1245,1665,1691]" country="China" name="Zhejiang" pageId="1" pageNumber="128">Zhejiang</collectingRegion>
