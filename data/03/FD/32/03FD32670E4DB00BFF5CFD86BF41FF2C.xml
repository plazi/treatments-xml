<paragraph blockId="5.[151,1437,924,2029]" pageId="5" pageNumber="252">
<materialsCitation ID-GBIF-Occurrence="1290989891" box="[199,1379,1140,1165]" country="Madagascar" location="A" pageId="5" pageNumber="252" specimenCount="1" stateProvince="A">
<emphasis bold="true" box="[199,282,1140,1165]" pageId="5" pageNumber="252">Range.</emphasis>
