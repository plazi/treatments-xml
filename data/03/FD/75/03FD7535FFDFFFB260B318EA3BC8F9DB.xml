<caption ID-DOI="http://doi.org/10.5281/zenodo.4687656" ID-Zenodo-Dep="4687656" httpUri="https://zenodo.org/record/4687656/files/figure.png" pageId="16" pageNumber="217" startId="16.[144,223,1749,1768]" targetBox="[369,1197,196,1717]" targetPageId="16">
<paragraph blockId="16.[144,1422,1749,1886]" pageId="16" pageNumber="217">
