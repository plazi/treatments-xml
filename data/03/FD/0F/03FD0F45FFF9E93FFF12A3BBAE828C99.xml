<number box="[269,283,512,536]" pageId="5" pageNumber="48" value="2.0">2</number>
</emphasis>
<number box="[292,334,511,535]" pageId="5" pageNumber="48" value="160.0">160</number>
(
<number box="[350,452,511,535]" pageId="5" pageNumber="48" value="170.0" valueMax="180.0" valueMin="160.0">160–180</number>
), 
<emphasis box="[475,502,512,536]" italics="true" pageId="5" pageNumber="48">
h 
<number box="[488,502,512,536]" pageId="5" pageNumber="48" value="3.0">3</number>
</emphasis>
<number box="[511,539,511,535]" pageId="5" pageNumber="48" value="28.0">28</number>
(
<number box="[556,630,511,535]" pageId="5" pageNumber="48" value="30.5" valueMax="33.0" valueMin="28.0">28–33</number>
), 
<emphasis box="[652,691,512,536]" italics="true" pageId="5" pageNumber="48">
