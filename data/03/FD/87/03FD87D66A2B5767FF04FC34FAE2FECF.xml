<taxonomicName box="[349,553,1596,1618]" class="Entognatha" family="Entomobryidae" genus="Willowsia" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="143" phylum="Arthropoda" rank="species" species="variabilis" status="sp. nov.">
<emphasis box="[349,553,1596,1618]" italics="true" pageId="6" pageNumber="143">
<taxonomicName box="[349,449,1596,1618]" class="Collembola" family="Entomobryidae" genus="Willowsia" higherTaxonomySource="CoL" kingdom="Animalia" order="Entomobryomorpha" pageId="6" pageNumber="143" phylum="Arthropoda" rank="genus">Willowsia</taxonomicName>
