<emphasis bold="true" box="[199,345,1705,1730]" pageId="78" pageNumber="79">Distribution</emphasis>
. Western Boreal Pacific, 
<geoCoordinate box="[630,714,1705,1730]" degrees="59.2" direction="north" orientation="latitude" pageId="78" pageNumber="79" precision="5555" value="59.2">59.2°N</geoCoordinate>
, 
<geoCoordinate box="[725,835,1705,1730]" degrees="143" direction="east" minutes="15" orientation="longitude" pageId="78" pageNumber="79" precision="925" value="143.25">143°15’E</geoCoordinate>
