<taxonomicName authorityName="Haller" authorityYear="1768" box="[624,705,1643,1667]" class="Myxomycetes" family="Trichiaceae" genus="Trichia" kingdom="Animalia" order="Trichiales" pageId="1" pageNumber="202" phylum="Mycetozoa" rank="genus">
