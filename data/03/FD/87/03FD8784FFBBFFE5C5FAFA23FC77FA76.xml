(
<figureCitation box="[639,723,1444,1470]" captionStart="FIGURES 1 – 9" captionStartId="4.[151,264,1896,1919]" captionTargetBox="[182,1436,264,1832]" captionTargetId="figure@4.[151,1436,247,1843]" captionTargetPageId="4" captionText="FIGURES 1 – 9. Habitus images (lateral). 1. Megaselia armstrongorum. 2. Megaselia bradyi. 3. Megaselia brejchaorum. 4. Megaselia carthayensis. 5. Megaselia ciancii. 6. Megaselia creasoni. 7. Megaselia defibaughorum. 8. Megaselia donahuei. 9. Megaselia francoae." httpUri="https://zenodo.org/record/242978/files/figure.png" pageId="2" pageNumber="453">
Figs. 
<number box="[708,723,1444,1470]" pageId="2" pageNumber="453" value="1.33">1</number>
</figureCitation>
, 
<figureCitation box="[737,768,1444,1470]" captionStart="FIGURES 33 – 42" captionStartId="17.[151,264,1863,1886]" captionTargetBox="[180,1416,264,1796]" captionTargetId="figure@17.[151,1436,251,1820]" captionTargetPageId="17" captionText="FIGURES 33 – 42. Right wing, dorsal. 33. Megaselia armstrongorum. 34. Megaselia bradyi. 35. Megaselia brejchaorum. 36. Megaselia carthayensis. 37. Megaselia ciancii. 38. Megaselia creasoni. 39. Megaselia defibaughorum. 40. Megaselia donahuei. 41. Megaselia francoae. 42. Megaselia fujiokai." httpUri="https://zenodo.org/record/242981/files/figure.png" pageId="2" pageNumber="453">33</figureCitation>
, 
<number box="[781,856,1444,1470]" pageId="2" pageNumber="453" value="63.93">
<figureCitation box="[781,811,1444,1470]" captionStart="FIGURES 63 – 68" captionStartId="23.[151,264,1732,1755]" captionTargetBox="[168,1417,207,1696]" captionTargetId="figure@23.[151,1436,193,1712]" captionTargetPageId="23" captionText="FIGURES 63 – 68. Male genitalia, left lateral. 63. Megaselia armstrongorum. 64. Megaselia bradyi. 65. Megaselia brejchaorum. 66. Megaselia carthayensis. 67. Megaselia ciancii. 68. Megaselia creasoni." httpUri="https://zenodo.org/record/242984/files/figure.png" pageId="2" pageNumber="453">63</figureCitation>
