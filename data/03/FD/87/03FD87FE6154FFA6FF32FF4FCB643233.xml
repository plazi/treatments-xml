<number box="[1064,1111,949,972]" pageId="2" pageNumber="63" value="4.06">4.06</number>
(
<number box="[1125,1173,949,972]" pageId="2" pageNumber="63" value="1.13">1.13</number>
+ 
<number box="[1201,1248,949,972]" pageId="2" pageNumber="63" value="1.23">1.23</number>
