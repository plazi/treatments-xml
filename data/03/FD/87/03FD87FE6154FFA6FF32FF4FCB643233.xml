<number box="[844,890,914,937]" pageId="2" pageNumber="63" value="0.93">0.93</number>
long, 
<number box="[965,1012,914,937]" pageId="2" pageNumber="63" value="0.73">0.73</number>
wide, abdomen 
<number box="[1199,1245,914,937]" pageId="2" pageNumber="63" value="1.33">1.33</number>
long, 
<number box="[1320,1367,914,937]" pageId="2" pageNumber="63" value="0.93">0.93</number>
wide. Clypeus height 
<number box="[317,367,949,972]" pageId="2" pageNumber="63" value="0.1">0.10</number>
. Tibia dorsal spines: 
<number box="[591,606,949,972]" pageId="2" pageNumber="63" value="2.0">2</number>
- 
<number box="[614,629,949,972]" pageId="2" pageNumber="63" value="2.0">2</number>
- 
<number box="[636,651,949,972]" pageId="2" pageNumber="63" value="2.0">2</number>
