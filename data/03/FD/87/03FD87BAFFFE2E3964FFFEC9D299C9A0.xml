<emphasis bold="true" box="[199,345,716,741]" pageId="18" pageNumber="19">Distribution</emphasis>
. Departure Bay, Vancouver Island, 
<collectingCountry box="[774,861,716,741]" name="Canada" pageId="18" pageNumber="19">Canada</collectingCountry>
, 
<geoCoordinate box="[878,998,716,741]" degrees="49" direction="north" minutes="12.6" orientation="latitude" pageId="18" pageNumber="19" precision="92" value="49.21">49°12.6’N</geoCoordinate>
, 
<geoCoordinate box="[1015,1144,716,741]" degrees="123" direction="east" minutes="57.6" orientation="longitude" pageId="18" pageNumber="19" precision="92" value="123.96">123°57.6’E</geoCoordinate>
