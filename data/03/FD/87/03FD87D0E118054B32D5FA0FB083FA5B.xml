<paragraph blockId="14.[151,1437,1376,1437]" box="[199,1437,1412,1437]" pageId="14" pageNumber="15">
<taxonomicName box="[199,312,1412,1436]" class="Adenophorea" family="Tripylidae" genus="Trischistoma" kingdom="Animalia" order="Enoplida" pageId="14" pageNumber="21" phylum="Nematoda" rank="species" species="waiotama" status="sp. nov.">Waiotama</taxonomicName>
