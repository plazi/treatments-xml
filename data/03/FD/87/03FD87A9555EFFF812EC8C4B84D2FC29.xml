. Central 
<collectingCountry box="[448,515,932,957]" name="Nepal" pageId="12" pageNumber="573">Nepal</collectingCountry>
(de 
<bibRefCitation box="[565,785,932,957]" pageId="12" pageNumber="573" refId="ref6093" refString="Freina, J. de &amp; Witt, T. J. (1994) Zur Kenntnis der Gattung Lithosarctia Daniel, 1954 mit BesChreiBung zWeier neuer Taxa. Atalanta, 25 (3 / 4), 535 - 542." type="journal article">Freina &amp; Witt 1994</bibRefCitation>
) (
<figureCitation box="[809,893,932,957]" captionStart="FIGURE 40" captionStartId="12.[151,250,1757,1779]" captionTargetBox="[163,1427,1012,1742]" captionTargetId="figure@12.[163,1427,1012,1742]" captionTargetPageId="12" captionText="FIGURE 40. Distribution map of the species of Lithosarctia." httpUri="https://zenodo.org/record/1437294/files/figure.png" pageId="12" pageNumber="573">Fig. 40</figureCitation>
).
</paragraph>
