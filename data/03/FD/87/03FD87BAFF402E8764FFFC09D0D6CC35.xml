<taxonomicName authority="Reyss, 1975" authorityName="Reyss" authorityYear="1975" box="[770,1056,1369,1394]" class="Malacostraca" family="Lampropidae" genus="Pseudodiastylis" kingdom="Animalia" order="Cumacea" pageId="172" pageNumber="173" phylum="Arthropoda" rank="species" species="delamarei">
