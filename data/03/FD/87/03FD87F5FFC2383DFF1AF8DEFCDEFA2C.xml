<quantity box="[545,676,1972,1996]" metricMagnitude="-3" metricUnit="m" metricValue="7.699999999999999" metricValueMax="8.2" metricValueMin="7.2" pageId="1" pageNumber="60" unit="mm" value="7.699999999999999" valueMax="8.2" valueMin="7.2">7.2–8.2mm</quantity>
