<subSubSection pageId="173" pageNumber="174" type="diagnosis">
<paragraph blockId="173.[151,1436,1141,1418]" pageId="173" pageNumber="174">
<emphasis bold="true" box="[199,321,1177,1202]" pageId="173" pageNumber="174">Diagnosis.</emphasis>
