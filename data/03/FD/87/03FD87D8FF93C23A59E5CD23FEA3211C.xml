<taxonomicName authorityName="Karaman &amp; Sket" authorityYear="2019" box="[554,747,973,997]" class="Malacostraca" family="Niphargidae" genus="Chaetoniphargus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="250" phylum="Arthropoda" rank="genus">
