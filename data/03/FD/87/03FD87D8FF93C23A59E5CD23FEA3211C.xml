<taxonomicName authorityName="Schiodte" authorityYear="1849" box="[1017,1134,973,997]" class="Malacostraca" family="Niphargidae" genus="Niphargus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="250" phylum="Arthropoda" rank="genus">
