<paragraph blockId="10.[151,609,151,210]" box="[151,295,186,210]" pageId="10" pageNumber="49">
(
<figureCitation box="[159,287,186,210]" captionStart-0="FIGURE 23" captionStart-1="FIGURES 24 – 29" captionStartId-0="9.[151,250,1842,1865]" captionStartId-1="11.[151,264,1083,1106]" captionTargetBox-0="[152,1434,884,1818]" captionTargetBox-1="[151,1422,203,1030]" captionTargetId-0="figure@9.[152,1434,884,1818]" captionTargetId-1="figure@11.[151,1436,195,1060]" captionTargetPageId-0="9" captionTargetPageId-1="11" captionText-0="FIGURE 23. Distribution of Acylophorus lomaensis Levaisseur." captionText-1="FIGURES 24 – 29. Acylophorus acufer sp. n. 24, fore body; 25. mandibles; 26, maxillary palp; 27, antenna; 28, aedeagus (ventral and lateral views); 29, internal face of paramere." httpUri-0="https://zenodo.org/record/279796/files/figure.png" httpUri-1="https://zenodo.org/record/279797/files/figure.png" pageId="10" pageNumber="49">
Figs 
<number box="[215,287,186,210]" pageId="10" pageNumber="49" value="25.5" valueMax="28.0" valueMin="23.0">23–28</number>
</figureCitation>
