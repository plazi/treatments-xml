<taxonomicName authority="Seidenstucker, 1948" authorityName="Seidenstucker" authorityYear="1948" box="[351,836,131,155]" class="Insecta" family="Berytidae" genus="Gampsocoris" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="culicinus">
