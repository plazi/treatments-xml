. 
<taxonomicName authorityName="Gerken" authorityYear="2018" baseAuthorityName="Gladfelter" baseAuthorityYear="1975" box="[321,568,860,885]" class="Malacostraca" family="Lampropidae" genus="Alamprops" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Cumacea" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="triserratus">
<emphasis box="[321,568,860,885]" italics="true" pageId="19" pageNumber="20">Alamprops triserratus</emphasis>
