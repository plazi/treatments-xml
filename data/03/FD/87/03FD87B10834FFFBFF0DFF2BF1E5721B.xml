<taxonomicName authority="Spinola" authorityName="Spinola" box="[1263,1436,956,977]" class="Insecta" family="Pentatomidae" genus="Dichelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="62" phylum="Arthropoda" rank="species" species="punctatus">
