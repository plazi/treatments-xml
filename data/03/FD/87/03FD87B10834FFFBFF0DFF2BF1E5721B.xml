<taxonomicName authority="Grazia" authorityName="Grazia" box="[1274,1436,1161,1182]" class="Insecta" family="Pentatomidae" genus="Dichelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="62" phylum="Arthropoda" rank="species" species="miriamae">
