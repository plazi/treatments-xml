μm; 
<emphasis box="[537,551,1052,1075]" italics="true" pageId="18" pageNumber="19">a</emphasis>
= 
<number box="[580,653,1050,1074]" pageId="18" pageNumber="19" value="53.5" valueMax="58.0" valueMin="49.0">49–58</number>
; 
<emphasis box="[665,679,1051,1075]" italics="true" pageId="18" pageNumber="19">b</emphasis>
= 
<number box="[709,796,1050,1074]" pageId="18" pageNumber="19" value="4.85" valueMax="5.3" valueMin="4.4">4.4–5.3</number>
; 
<emphasis box="[808,820,1052,1075]" italics="true" pageId="18" pageNumber="19">c</emphasis>
= 
<number box="[850,923,1050,1074]" pageId="18" pageNumber="19" value="13.0" valueMax="14.0" valueMin="12.0">12–14</number>
; 
<emphasis box="[935,956,1051,1074]" italics="true" pageId="18" pageNumber="19">c’</emphasis>
= 
<number box="[986,1074,1050,1074]" pageId="18" pageNumber="19" value="5.300000000000001" valueMax="5.9" valueMin="4.7">4.7–5.9</number>
; 
<emphasis box="[1085,1102,1052,1075]" italics="true" pageId="18" pageNumber="19">V</emphasis>
= 
<number box="[1132,1206,1050,1074]" pageId="18" pageNumber="19" value="75.5" valueMax="78.0" valueMin="73.0">73–78</number>
%. Male: 
<emphasis box="[271,287,1088,1111]" italics="true" pageId="18" pageNumber="19">L</emphasis>
= 
<number box="[316,428,1086,1110]" pageId="18" pageNumber="19" value="950.0" valueMax="1100.0" valueMin="800.0">800–1100</number>
μm; 
<emphasis box="[493,507,1088,1111]" italics="true" pageId="18" pageNumber="19">a</emphasis>
= 
<number box="[537,610,1086,1110]" pageId="18" pageNumber="19" value="49.0" valueMax="57.0" valueMin="41.0">41–57</number>
; 
