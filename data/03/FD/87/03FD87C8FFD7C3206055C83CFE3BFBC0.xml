<subSubSection pageId="11" pageNumber="12" type="discussion">
<paragraph blockId="11.[264,1325,284,1030]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[312,426,924,950]" pageId="11" pageNumber="12">Remarks</emphasis>
. 
<taxonomicName box="[444,744,924,949]" class="Arachnida" family="Ologamasidae" genus="Euryparasitus" kingdom="Animalia" order="Mesostigmata" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="longicheta">
