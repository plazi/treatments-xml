<paragraph blockId="5.[264,1325,1216,1895]" pageId="5" pageNumber="42">
Total length 
<quantity box="[468,580,1336,1362]" metricMagnitude="-3" metricUnit="m" metricValue="4.98" pageId="5" pageNumber="42" unit="mm" value="4.98">4.98 mm</quantity>
