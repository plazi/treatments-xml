<number box="[776,852,924,950]" pageId="7" pageNumber="44" value="14.0" valueMax="15.0" valueMin="13.0">13–15</number>
; including 
<number box="[991,1036,924,950]" pageId="7" pageNumber="44" value="5.0" valueMax="6.0" valueMin="4.0">4–6</number>
inner verticals, 
<number box="[1237,1252,924,950]" pageId="7" pageNumber="44" value="5.0">5</number>
outer verticals and 
<number box="[425,439,964,990]" pageId="7" pageNumber="44" value="4.0">4</number>
postorbitals. Clypeus with 
<number box="[772,845,964,990]" pageId="7" pageNumber="44" value="14.0" valueMax="18.0" valueMin="10.0">10–18</number>
setae. Tentorium 
<quantity box="[1062,1214,964,990]" metricMagnitude="2" metricUnit="m" metricValue="1.4" metricValueMax="1.55" metricValueMin="1.25" pageId="7" pageNumber="44" unit="m" value="140.0" valueMax="155.0" valueMin="125.0">125–155 m</quantity>
long, 
<quantity metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="7" pageNumber="44" unit="m" value="50.0">50 m</quantity>
wide. Palpomere lengths (in m): 
<number box="[726,756,1004,1030]" pageId="7" pageNumber="44" value="45.0">45</number>
– 
<number box="[773,803,1004,1030]" pageId="7" pageNumber="44" value="45.0">45</number>
; 
<number box="[818,895,1004,1030]" pageId="7" pageNumber="44" value="42.5" valueMax="45.0" valueMin="40.0">40–45</number>
; 
<number box="[911,1016,1004,1030]" pageId="7" pageNumber="44" value="142.5" valueMax="145.0" valueMin="140.0">140–145</number>
; 
<number box="[1032,1138,1004,1030]" pageId="7" pageNumber="44" value="192.5" valueMax="195.0" valueMin="190.0">190–195</number>
; 
<number box="[1153,1281,1004,1030]" pageId="7" pageNumber="44" value="243.75" valueMax="275.0" valueMin="212.5">212.5–275</number>
. L: 
<number box="[264,279,1052,1078]" pageId="7" pageNumber="44" value="5.0">5</number>
