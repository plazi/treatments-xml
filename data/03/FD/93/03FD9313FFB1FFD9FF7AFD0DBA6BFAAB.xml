<number box="[926,968,788,813]" pageId="1" pageNumber="420" value="570.0">570</number>
(
<number box="[985,1089,788,813]" pageId="1" pageNumber="420" value="585.0" valueMax="615.0" valueMin="555.0">555–615</number>
