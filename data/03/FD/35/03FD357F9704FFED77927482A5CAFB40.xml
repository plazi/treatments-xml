<materialsCitation location="northeastern North America" pageId="27" pageNumber="26" specimenCount="1" stateProvince="Florida">
<location LSID="urn:lsid:plazi:treatment:03FD357F9704FFED77927482A5CAFB40:8E8BD2B29704FFED76797641A6C1FCC0" box="[299,661,802,826]" name="northeastern North America" pageId="27" pageNumber="26" stateProvince="Florida">northeastern North America</location>
