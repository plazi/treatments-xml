<materialsCitation ID-GBIF-Occurrence="1701201004" box="[199,1433,1792,1817]" collectionCode="NPC" collectorName="Coll. H. L. &amp; Andrewes" country="India" location="Nilgiri Hills" pageId="2" pageNumber="201" specimenCount="1" stateProvince="Tamil Nadu" typeStatus="holotype">
