. 
<taxonomicName authority="(Enderlein, 1938)" baseAuthorityName="Enderlein" baseAuthorityYear="1938" box="[370,851,1944,1968]" class="Insecta" family="Syrphidae" genus="Pseudoscaeva" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="141" pageNumber="142" phylum="Arthropoda" rank="species" species="schoenemanni">
