<quantity box="[848,969,1974,2001]" metricMagnitude="3" metricUnit="m" metricValue="1.4775" metricValueMax="2.95" metricValueMin="0.005" pageId="153" pageNumber="154" unit="m" value="1477.5" valueMax="2950.0" valueMin="5.0">5–2950 m</quantity>
