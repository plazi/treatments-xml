<quantity box="[594,701,1909,1935]" metricMagnitude="3" metricUnit="m" metricValue="1.232" pageId="125" pageNumber="126" unit="km" value="1.232">1,232 km</quantity>
<superScript attach="left" box="[700,708,1909,1923]" fontSize="6" pageId="125" pageNumber="126">2</superScript>
