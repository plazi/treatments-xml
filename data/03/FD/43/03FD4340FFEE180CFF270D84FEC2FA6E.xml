<collectingRegion box="[521,729,1189,1216]" country="Chile" name="Arica y Parinacota" pageId="84" pageNumber="85">Arica y Parinacota</collectingRegion>
to 
<collectingRegion box="[765,945,1189,1215]" country="Chile" name="Tarapaca" pageId="84" pageNumber="85">Tarapacá region</collectingRegion>
