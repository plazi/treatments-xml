<quantity box="[1141,1314,1535,1562]" metricMagnitude="0" metricUnit="m" metricValue="3.65" metricValueMax="3.8" metricValueMin="3.5" pageId="81" pageNumber="82" unit="m" value="3.65" valueMax="3.8" valueMin="3.5">
