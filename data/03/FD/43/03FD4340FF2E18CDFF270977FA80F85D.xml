<taxonomicName authority="(J. D. Brandis ex Molina) Pers." authorityName="Pers." baseAuthorityName="Molina" box="[451,1044,1830,1857]" class="Magnoliopsida" family="Gesneriaceae" genus="Sarmienta" kingdom="Plantae" order="Lamiales" pageId="149" pageNumber="150" phylum="Tracheophyta" rank="species" species="scandens">
