<paragraph blockId="68.[151,1437,901,1504]" pageId="68" pageNumber="69">
<emphasis bold="true" box="[199,418,1297,1323]" pageId="68" pageNumber="69">Biology and notes.</emphasis>
<taxonomicName baseAuthorityName="Shannon" baseAuthorityYear="1927" box="[425,639,1297,1323]" class="Insecta" family="Syrphidae" genus="Valdiviomyia" kingdom="Animalia" order="Diptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="nigra">
<emphasis box="[425,639,1297,1323]" italics="true" pageId="68" pageNumber="69">Valdiviomyia nigra</emphasis>
</taxonomicName>
