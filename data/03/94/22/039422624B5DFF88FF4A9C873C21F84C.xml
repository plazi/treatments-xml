<paragraph blockId="1.[151,622,1007,1067]" box="[151,398,1042,1067]" pageId="1" pageNumber="96">
(
<figureCitation box="[159,236,1042,1067]" captionStart="FIGURE 1" captionStartId="2.[151,250,1183,1205]" captionText="FIGURE 1. Grossander (Oculoander) eylesi holotype male." pageId="1" pageNumber="96">Figs. 1</figureCitation>
, 
<figureCitation box="[250,266,1043,1067]" captionStart="FIGURE 2" captionStartId="3.[151,250,1365,1387]" captionText="FIGURE 2. Grossander (Oculoander) papuanus holotype male." pageId="1" pageNumber="96">2</figureCitation>
, 
<figureCitation box="[278,391,1042,1067]" captionStart-0="FIGURES 4 – 10. 4 – 6" captionStart-1="FIGURE 11" captionStartId-0="5.[151,264,1216,1238]" captionStartId-1="7.[151,250,1698,1720]" captionTargetBox-0="[162,1429,273,1185]" captionTargetBox-1="[188,1405,820,1670]" captionTargetId-0="figure@5.[151,1436,267,1192]" captionTargetId-1="figure@7.[170,1421,810,1670]" captionTargetPageId-0="5" captionTargetPageId-1="7" captionText-0="FIGURES 4 – 10. 4 – 6 Pygophores of: 4, Grossander (Oculoander) eylesi paratype; 5, G. (O.) papuanus holotype; 6, Grossander major (picture of Gross 1965). 7 – 10 Parameres of: 7, 8, Grossander (Oculoander) papuanus holotype; 9, 10, G. (O.) eylesi paratype. 7 and 9 dorsal, 8 and 10 ventral view." captionText-1="FIGURE 11. Distribution area of Grossander (Oculoander) papuanus and G. (O.) eylesi." httpUri-0="https://zenodo.org/record/241313/files/figure.png" httpUri-1="https://zenodo.org/record/241314/files/figure.png" pageId="1" pageNumber="96">
4, 5, 
<number box="[334,391,1043,1067]" pageId="1" pageNumber="96" value="9.0" valueMax="11.0" valueMin="7.0">7–11</number>
</figureCitation>
