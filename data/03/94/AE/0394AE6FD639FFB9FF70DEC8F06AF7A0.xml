<paragraph blockId="7.[136,1451,1721,1784]" pageId="7" pageNumber="164">
<taxonomicName authority="Metzeltin &amp; Lange-Bertalot" authorityName="Metzeltin &amp; Lange-Bertalot" authorityYear="1998" box="[136,716,1721,1748]" class="Bacillariophyceae" family="Eunotiaceae" genus="Eunotia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Eunotiales" pageId="7" pageNumber="164" phylum="Bacillariophyta" rank="species" species="neomundana">
