<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[669,758,1882,1908]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="17" pageNumber="18">USNM</collectionCode>
