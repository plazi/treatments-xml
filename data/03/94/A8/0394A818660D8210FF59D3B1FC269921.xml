<collectingCountry box="[810,881,264,290]" name="Japan" pageId="15" pageNumber="16">Japan</collectingCountry>
, 
<collectingCountry box="[895,967,264,290]" name="China" pageId="15" pageNumber="16">China</collectingCountry>
