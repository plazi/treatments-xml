<emphasis bold="true" box="[192,536,1251,1275]" pageId="31" pageNumber="30">
<taxonomicName authorityName="Erichson" authorityYear="1848" box="[192,305,1251,1275]" class="Insecta" family="Glaresidae" genus="Glaresis" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[192,305,1251,1275]" italics="true" pageId="31" pageNumber="30">Glaresis</emphasis>
