<taxonomicName authority="Kuscer, 1933" authorityName="Kuscer" authorityYear="1933" box="[531,804,913,938]" class="Gastropoda" family="Hydrobiidae" genus="Costellina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="20" pageNumber="331" phylum="Mollusca" rank="genus">
