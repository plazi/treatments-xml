<taxonomicName authorityName="Ratcliffe and Cave" authorityYear="2008" box="[1267,1440,956,977]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="dolichotarsa">
