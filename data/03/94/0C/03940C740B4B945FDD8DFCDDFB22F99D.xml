<taxonomicName authorityName="Engel &amp; Michener &amp; Boontop" authorityYear="2017" box="[711,867,828,854]" class="Insecta" family="Apidae" genus="Tarsalia" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="subGenus" subGenus="Astibomelissa">
