<quantity box="[1214,1277,1534,1555]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" pageId="5" pageNumber="6" unit="mm" value="12.0">12mm</quantity>
)................................................................................................... 
<taxonomicName authority="Reid &amp; Beatson, 2011" authorityName="Reid &amp; Beatson" authorityYear="2011" box="[1275,1356,1564,1585]" class="Insecta" family="Chrysomelidae" genus="Bohumiljania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tango" status="sp. nov.">
