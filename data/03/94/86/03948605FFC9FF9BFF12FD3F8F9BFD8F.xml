<taxonomicName box="[318,427,1954,1977]" class="Insecta" family="Cleridae" genus="Strotocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="werneri">
<emphasis box="[318,427,1954,1977]" italics="true" pageId="51" pageNumber="52">S. werneri</emphasis>
</taxonomicName>
, aedeagus, terminal sternite and 
<emphasis bold="true" box="[809,946,1954,1978]" pageId="51" pageNumber="52">FIGURE 97</emphasis>
: 
<taxonomicName box="[960,1070,1954,1977]" class="Insecta" family="Cleridae" genus="Strotocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="werneri">
