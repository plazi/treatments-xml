<subSubSection pageId="10" pageNumber="11" type="description">
<paragraph blockId="10.[151,1437,421,1066]" box="[151,521,421,447]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[151,250,421,447]" pageId="10" pageNumber="11">Length:</emphasis>
<quantity box="[257,354,421,447]" metricMagnitude="-2" metricUnit="m" metricValue="1.8399999999999999" pageId="10" pageNumber="11" unit="mm" value="18.4">18.4mm</quantity>
(
