, 
<collectingCountry box="[797,906,2012,2038]" name="Tanzania" pageId="33" pageNumber="34">Tanzania</collectingCountry>
, 
<collectingCountry box="[917,1046,2012,2038]" name="Zimbabwe" pageId="33" pageNumber="34">Zimbabwe</collectingCountry>
