<paragraph blockId="0.[151,987,1836,1861]" box="[151,987,1836,1861]" pageId="0" pageNumber="190">
<taxonomicName authority="Leraut, 2017: 155" authorityName="Leraut" authorityPageNumber="155" authorityYear="2017" box="[151,470,1836,1861]" class="Insecta" family="Pyralidae" genus="Vietnamodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="190" phylum="Arthropoda" rank="genus">
