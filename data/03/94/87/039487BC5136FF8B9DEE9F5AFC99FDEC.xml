<taxonomicName authorityName="Oliveira &amp; Brescovit" authorityYear="2021" baseAuthorityName="Keyserling" baseAuthorityYear="1891" box="[328,521,1836,1862]" class="Arachnida" family="Anyphaenidae" genus="Patrera" kingdom="Animalia" order="Araneae" pageId="30" pageNumber="231" phylum="Arthropoda" rank="species" species="concolor">
<emphasis box="[328,521,1836,1862]" italics="true" pageId="30" pageNumber="231">Patrera concolor</emphasis>
