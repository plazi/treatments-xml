<taxonomicName authority="(Keyserling, 1891)" authorityName="Oliveira &amp; Brescovit" authorityYear="2021" baseAuthorityName="Keyserling" baseAuthorityYear="1891" box="[343,755,1730,1754]" class="Arachnida" family="Anyphaenidae" genus="Patrera" kingdom="Animalia" order="Araneae" pageId="35" pageNumber="236" phylum="Arthropoda" rank="species" species="recentissima">
