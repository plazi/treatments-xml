<taxonomicName authority="Latreille" authorityName="Latreille" box="[535,770,583,610]" class="Insecta" family="Pompilidae" genus="Ceropales" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="218" phylum="Arthropoda" rank="subGenus" subGenus="Ceropales">
