<taxonomicName authorityName="Masi Z. Boucek" authorityYear="1931" box="[667,787,729,755]" class="Insecta" family="Pteromalidae" genus="Stenetra" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="198" phylum="Arthropoda" rank="species" species="ligustica">
