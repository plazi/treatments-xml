<taxonomicName authorityName="Gunther" authorityYear="1858" box="[1010,1131,1249,1272]" class="Diplopoda" family="Chelodesmidae" genus="Rhacophorus" kingdom="Animalia" order="Polydesmida" pageId="2" pageNumber="259" phylum="Arthropoda" rank="species" species="pardalis">R. pardalis</taxonomicName>
, 
<taxonomicName authority=", Leptolalax" authorityName="Leptolalax" box="[1140,1473,1249,1272]" class="Diplopoda" family="Chelodesmidae" genus="Rhacophorus" kingdom="Animalia" order="Polydesmida" pageId="2" pageNumber="259" phylum="Arthropoda" rank="species" species="kajau">Rhacophorus kajau, Leptolalax</taxonomicName>
