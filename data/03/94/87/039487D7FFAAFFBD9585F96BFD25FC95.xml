<paragraph blockId="3.[151,1437,153,791]" pageId="3" pageNumber="80">
<emphasis bold="true" box="[199,332,368,395]" pageId="3" pageNumber="80">Discussion.</emphasis>
<taxonomicName authorityName=": Polhemus &amp; Polhemus" authorityYear="1993" box="[341,572,369,395]" class="Insecta" family="Veliidae" genus="Steinovelia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="80" phylum="Arthropoda" rank="species" species="permista">
