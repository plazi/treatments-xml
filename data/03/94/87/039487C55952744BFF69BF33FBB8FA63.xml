<taxonomicName authority="ASSING, 2011" authorityName="ASSING" authorityYear="2011" box="[1126,1398,1065,1089]" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="292" phylum="Arthropoda" rank="species" species="baculatus">
<emphasis box="[1126,1251,1065,1089]" italics="true" pageId="5" pageNumber="292">S. baculatus</emphasis>
ASSING, 2011
</taxonomicName>
(East 
<collectingCountry box="[858,925,1097,1121]" name="Nepal" pageId="5" pageNumber="292">Nepal</collectingCountry>
), 
<taxonomicName authority="(Darjeeling)" baseAuthorityName="Darjeeling" box="[949,1214,1097,1121]" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="292" phylum="Arthropoda" rank="species" species="cameroni">
<emphasis box="[949,1071,1097,1121]" italics="true" pageId="5" pageNumber="292">S. cameroni</emphasis>
(Darjeeling)
</taxonomicName>
, and 
<taxonomicName authority="(COIFFAIT, 1978)" baseAuthorityName="COIFFAIT" baseAuthorityYear="1978" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="292" phylum="Arthropoda" rank="species" species="bouddha">
<emphasis box="[1281,1399,1097,1121]" italics="true" pageId="5" pageNumber="292">S. bouddha</emphasis>
(COIFFAIT, 1978)
</taxonomicName>
(
<collectingCountry box="[992,1075,1129,1153]" name="Bhutan" pageId="5" pageNumber="292">Bhutan</collectingCountry>
). The aedeagus of 
<taxonomicName authorityName="Assing" authorityYear="2015" box="[1279,1400,1129,1153]" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="292" phylum="Arthropoda" rank="species" species="marratus">
<emphasis box="[1279,1400,1129,1153]" italics="true" pageId="5" pageNumber="292">S. marratus</emphasis>
</taxonomicName>
