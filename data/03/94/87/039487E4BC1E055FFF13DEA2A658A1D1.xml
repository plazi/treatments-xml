<subSubSection pageId="1" pageNumber="191" type="diagnosis">
<paragraph blockId="1.[151,1437,1681,2033]" pageId="1" pageNumber="191">
<emphasis bold="true" box="[199,314,1933,1960]" pageId="1" pageNumber="191">Diagnosis</emphasis>
