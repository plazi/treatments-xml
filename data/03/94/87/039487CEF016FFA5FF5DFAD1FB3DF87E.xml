<subSubSection pageId="21" pageNumber="488" type="etymology">
<paragraph blockId="21.[151,1437,1475,2042]" pageId="21" pageNumber="488">
<emphasis bold="true" box="[199,319,1619,1645]" pageId="21" pageNumber="488">Etymology</emphasis>
