<taxonomicName authority="Gressitt, 1946" authorityName="Gressitt" authorityYear="1946" box="[318,705,1979,2001]" class="Insecta" family="Chrysomelidae" genus="Chlamisus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="ferrugineus">
<emphasis box="[318,550,1979,2001]" italics="true" pageId="36" pageNumber="37">
<taxonomicName box="[318,425,1979,2001]" class="Insecta" family="Chrysomelidae" genus="Chlamisus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">Chlamisus</taxonomicName>
