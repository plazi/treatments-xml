: 
<taxonomicName baseAuthorityName="COIFFAIT" baseAuthorityYear="1970" box="[320,545,1097,1121]" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="289" phylum="Arthropoda" rank="species" species="fulgocephalus">
