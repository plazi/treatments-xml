<taxonomicName authorityName="Tournier" authorityYear="1889" box="[221,354,619,643]" class="Insecta" family="Pompilidae" genus="Ceropales" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="212" phylum="Arthropoda" rank="species" species="helvetica">
