<subSubSection lastPageId="72" lastPageNumber="73" pageId="68" pageNumber="69" type="description">
<paragraph blockId="68.[151,1437,151,608]" pageId="68" pageNumber="69">
<emphasis bold="true" box="[199,381,295,320]" pageId="68" pageNumber="69">Measurements.</emphasis>
BL = 
<quantity box="[452,554,295,320]" metricMagnitude="-3" metricUnit="m" metricValue="3.25" pageId="68" pageNumber="69" unit="mm" value="3.25">3.25 mm</quantity>
, BW = 
