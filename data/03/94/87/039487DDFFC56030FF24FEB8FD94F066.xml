<taxonomicName authority="SZEPLIGETI 1905" authorityName="SZEPLIGETI" authorityYear="1905" box="[265,677,576,598]" class="Insecta" family="Braconidae" genus="Iphiaulax" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="62" phylum="Arthropoda" rank="species" species="annulicornis">
