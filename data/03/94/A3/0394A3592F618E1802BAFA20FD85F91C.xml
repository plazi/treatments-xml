<taxonomicName baseAuthorityName="Kinzelbach" baseAuthorityYear="1971" box="[176,297,1505,1527]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Strepsiptera" pageId="5" pageNumber="182" phylum="Arthropoda" rank="order">Strepsiptera</taxonomicName>
