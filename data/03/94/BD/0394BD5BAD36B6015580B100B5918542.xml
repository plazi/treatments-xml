<quantity box="[361,506,1417,1438]" metricMagnitude="-3" metricUnit="m" metricValue="1.4449999999999998" metricValueMax="1.46" metricValueMin="1.43" pageId="3" pageNumber="222" unit="mm" value="1.4449999999999998" valueMax="1.46" valueMin="1.43">1.43–1.46mm</quantity>
(
<emphasis box="[524,536,1418,1439]" italics="true" pageId="3" pageNumber="222">x</emphasis>
