n = 3, TL = 
<quantity box="[780,937,1391,1412]" metricMagnitude="-3" metricUnit="m" metricValue="1.95" metricValueMax="1.97" metricValueMin="1.93" pageId="3" pageNumber="222" unit="mm" value="1.95" valueMax="1.97" valueMin="1.93">1.93–1.97 mm</quantity>
