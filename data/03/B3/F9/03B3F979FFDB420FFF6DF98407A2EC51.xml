</taxonomicName>
<taxonomicNameLabel box="[527,590,1628,1653]" pageId="41" pageNumber="42" rank="species">
<emphasis bold="true" box="[527,590,1628,1653]" pageId="41" pageNumber="42">n. sp.</emphasis>
</taxonomicNameLabel>
resembles 
<taxonomicName box="[715,863,1628,1653]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="connudata" status="n. sp.">
