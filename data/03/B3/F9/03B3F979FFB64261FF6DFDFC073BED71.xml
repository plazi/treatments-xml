°N
</geoCoordinate>
, 
<geoCoordinate box="[586,708,1029,1054]" direction="east" orientation="longitude" pageId="69" pageNumber="70" precision="5" value="35.3062">
<number box="[586,682,1029,1054]" pageId="69" pageNumber="70" value="35.3062">35.3062</number>
°E
</geoCoordinate>
, 
<quantity box="[719,778,1030,1054]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="69" pageNumber="70" unit="m" value="10.0">10 m</quantity>
, 
