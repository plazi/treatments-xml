<paragraph blockId="267.[151,1437,151,896]" box="[199,847,871,896]" pageId="267" pageNumber="268">
<emphasis bold="true" box="[199,345,871,896]" pageId="267" pageNumber="268">Distribution</emphasis>
. 
<collectingCountry box="[359,401,871,896]" name="Fiji" pageId="267" pageNumber="268">Fiji</collectingCountry>
, 
<collectingCountry box="[412,485,872,896]" name="Tonga" pageId="267" pageNumber="268">Tonga</collectingCountry>
, 
<collectingCountry box="[495,702,871,896]" name="Wallis and Futuna" pageId="267" pageNumber="268">Wallis and Futuna</collectingCountry>
; 
<quantity box="[713,843,871,896]" metricMagnitude="2" metricUnit="m" metricValue="4.055" metricValueMax="5.25" metricValueMin="2.86" pageId="267" pageNumber="268" unit="m" value="405.5" valueMax="525.0" valueMin="286.0">
286– 
<specimenCount box="[770,843,871,896]" pageId="267" pageNumber="268" type="generic">525 m</specimenCount>
</quantity>
.
</paragraph>
