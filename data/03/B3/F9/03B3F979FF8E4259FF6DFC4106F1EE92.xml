<taxonomicName box="[292,477,1921,1943]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="126" pageNumber="127" phylum="Arthropoda" rank="species" species="gladiola" status="n. sp.">
<emphasis box="[292,477,1921,1943]" italics="true" pageId="126" pageNumber="127">
Galathea 
<taxonomicName box="[392,477,1921,1943]" pageId="126" pageNumber="127" rank="species" species="gladiola" status="n. sp.">gladiola</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[483,539,1921,1942]" pageId="126" pageNumber="127" rank="species">
