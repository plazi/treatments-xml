<taxonomicName box="[747,889,583,608]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="rhaphidia" status="n. sp.">
<emphasis box="[747,889,583,608]" italics="true" pageId="229" pageNumber="230">G. rhaphidia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[897,961,583,608]" pageId="229" pageNumber="230" rank="species">
<emphasis bold="true" box="[897,961,583,608]" pageId="229" pageNumber="230">n. sp.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName authority="Henderson, 1885" authorityName="Henderson" authorityYear="1885" box="[1018,1386,583,608]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="inconspicua">
<emphasis box="[1018,1185,584,608]" italics="true" pageId="229" pageNumber="230">G. inconspicua</emphasis>
<bibRefCitation author="Henderson" box="[1193,1386,583,608]" pageId="229" pageNumber="230" refString="Henderson, J. R. (1885) Diagnoses of new species of Galatheidae collected during the &quot; Challenger &quot; expedition. Annals and Magazine of Natural History, Series 5, 16, 407 - 421." type="journal article" year="1885">
Henderson, 
<number box="[1330,1386,583,608]" pageId="229" pageNumber="230" value="1885.0">1885</number>
</bibRefCitation>
</taxonomicName>
(for differentiation, see Remarks of 
<taxonomicName box="[504,670,621,644]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="inconspicua">
<emphasis box="[504,670,621,644]" italics="true" pageId="229" pageNumber="230">G. inconspicua</emphasis>
</taxonomicName>
).
</paragraph>
</subSubSection>
