<collectionCode LSID="urn:lsid:biocol.org:col:14498" box="[795,846,1196,1220]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14498" name="University of Coimbra Botany Department" pageId="80" pageNumber="81">COI</collectionCode>
) and 
