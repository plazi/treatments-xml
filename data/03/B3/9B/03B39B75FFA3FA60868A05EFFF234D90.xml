<paragraph blockId="8.[151,1436,151,418]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[198,450,271,297]" pageId="8" pageNumber="9">Material Examined.</emphasis>
<quantity box="[459,499,272,298]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="8" pageNumber="9" unit="m" value="5.0">5m</quantity>
