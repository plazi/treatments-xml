st canonical function = 
<number box="[1372,1432,1437,1459]" pageId="4" pageNumber="321" value="0.028">0.028</number>
, p = 0.00; 
<number box="[248,261,1470,1492]" pageId="4" pageNumber="321" value="2.0">2</number>
nd canonical function = 
