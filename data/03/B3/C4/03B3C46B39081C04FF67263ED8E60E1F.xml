<number box="[1079,1118,1890,1916]" pageId="7" pageNumber="8" value="3.8">3.8</number>
× [
<number box="[1150,1193,1890,1916]" pageId="7" pageNumber="8" value="3.8">3.8</number>
×], flagellomere VII 
<number box="[151,189,1928,1954]" pageId="7" pageNumber="8" value="2.8">2.8</number>
× [
<number box="[219,314,1928,1954]" pageId="7" pageNumber="8" value="2.75" valueMax="2.8" valueMin="2.7">2.7–2.8</number>
