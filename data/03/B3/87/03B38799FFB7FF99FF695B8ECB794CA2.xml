<taxonomicName authority="araxenus" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="351" phylum="Arthropoda" rank="subSpecies" species="araxenus" subSpecies="araxenus">
