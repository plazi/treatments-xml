<collectingCountry box="[580,674,1867,1909]" name="India" pageId="18" pageNumber="181">India</collectingCountry>
, 
<collectingCountry box="[706,828,1867,1909]" name="Sri Lanka" pageId="18" pageNumber="181">Ceylon</collectingCountry>
, 
<collectingCountry box="[858,981,1867,1909]" name="Myanmar" pageId="18" pageNumber="181">Burma</collectingCountry>
