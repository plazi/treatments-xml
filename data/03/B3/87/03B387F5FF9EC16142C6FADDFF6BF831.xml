); 
<typeStatus box="[494,595,1296,1320]" pageId="3" pageNumber="248">holotype</typeStatus>
: 
<quantity box="[604,706,1296,1320]" metricMagnitude="-3" metricUnit="m" metricValue="1.95" pageId="3" pageNumber="248" unit="mm" value="1.95">1.95 mm</quantity>
