<quantity box="[151,235,883,909]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="6" pageNumber="30" unit="m" value="1000.0">1000m</quantity>
, 
<date box="[248,408,883,909]" pageId="6" pageNumber="30" value="2007-05-11">
<number box="[248,276,883,909]" pageId="6" pageNumber="30" value="11.0">11</number>
May 
