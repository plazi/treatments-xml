<collectingCountry box="[531,578,883,909]" name="Niue" pageId="6" pageNumber="30">Niu</collectingCountry>
, sweep net. 
<typeStatus box="[723,837,883,909]" pageId="6" pageNumber="30">Paratypes</typeStatus>
(
