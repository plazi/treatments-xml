<number box="[813,828,980,1004]" pageId="7" pageNumber="27" value="2.0">2</number>
way to posterior margin, 
<number box="[1115,1129,980,1004]" pageId="7" pageNumber="27" value="2.0">2</number>
katepisternal setae, tergites 
<number box="[151,165,1015,1039]" pageId="7" pageNumber="27" value="3.0">3</number>
and 
<number box="[219,233,1015,1039]" pageId="7" pageNumber="27" value="4.0">4</number>
