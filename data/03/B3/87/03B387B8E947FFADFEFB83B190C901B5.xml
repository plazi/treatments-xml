<collectingCountry box="[1040,1164,668,711]" name="Sri Lanka" pageId="12" pageNumber="175">Ceylon</collectingCountry>
, 
<collectingCountry box="[1189,1313,668,711]" name="Myanmar" pageId="12" pageNumber="175">Burma</collectingCountry>
