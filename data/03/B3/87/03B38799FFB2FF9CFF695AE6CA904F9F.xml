: 
<collectingRegion box="[456,507,1303,1326]" country="Iran" name="Fars" pageId="3" pageNumber="348">Fars</collectingRegion>
, 
<collectingRegion box="[525,646,1302,1327]" country="Iran" name="Khuzestan" pageId="3" pageNumber="348">Khuzestan</collectingRegion>
