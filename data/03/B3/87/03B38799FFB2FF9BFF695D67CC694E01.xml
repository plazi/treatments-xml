<subSubSection pageId="4" pageNumber="349" type="etymology">
<paragraph blockId="4.[151,1437,151,1149]" pageId="4" pageNumber="349">
<emphasis bold="true" box="[199,334,1052,1077]" inLineHeading="true" pageId="4" pageNumber="349" reason="1">Etymology.</emphasis>
