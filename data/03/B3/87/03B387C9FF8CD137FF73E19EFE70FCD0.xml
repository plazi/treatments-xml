<taxonomicName authorityName="Deichmann" authorityYear="1941" box="[295,431,405,428]" class="Holothuroidea" family="Sclerodactylidae" genus="Neopentamera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dendrochirotida" pageId="5" pageNumber="162" phylum="Echinodermata" rank="species" species="anexigua">
