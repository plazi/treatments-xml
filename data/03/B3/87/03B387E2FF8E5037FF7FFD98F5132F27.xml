<typeStatus box="[197,267,191,217]" pageId="4" pageNumber="28">
<emphasis bold="true" box="[197,267,191,217]" pageId="4" pageNumber="28">Types</emphasis>
</typeStatus>
. 
<typeStatus box="[286,395,192,218]" pageId="4" pageNumber="28">Holotype</typeStatus>
(
<collectionCode box="[420,487,192,218]" pageId="4" pageNumber="28">ZJFC</collectionCode>
<date box="[496,586,192,218]" pageId="4" pageNumber="28" value="12-07-04">070412</date>
) 
<collectingCountry box="[602,699,192,218]" name="China" pageId="4" pageNumber="28">CHINA</collectingCountry>
: Zhejiang Tianmu Montain National Natural Reserve, 
<quantity box="[1347,1431,192,218]" metricMagnitude="3" metricUnit="m" metricValue="1.09" pageId="4" pageNumber="28" unit="m" value="1090.0">1090m</quantity>
, 
<date box="[151,378,232,258]" pageId="4" pageNumber="28" value="1998-11-14">
<number box="[151,180,232,258]" pageId="4" pageNumber="28" value="14.0">14</number>
November 
<number box="[317,378,232,258]" pageId="4" pageNumber="28" value="1998.0">1998</number>
</date>
, Mingshui, Zhao, Malaise trap. 
<typeStatus box="[750,864,232,258]" pageId="4" pageNumber="28">Paratypes</typeStatus>
(
