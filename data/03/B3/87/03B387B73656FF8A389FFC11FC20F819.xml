male chelicera; 
<emphasis box="[568,596,1614,1635]" italics="true" pageId="21" pageNumber="36">(d)</emphasis>
female chelicera; 
<emphasis box="[768,795,1614,1635]" italics="true" pageId="21" pageNumber="36">(e)</emphasis>
epigyne; 
<emphasis box="[886,909,1614,1635]" italics="true" pageId="21" pageNumber="36">(f)</emphasis>
female internal genitalia, dorsal; 
<emphasis box="[1224,1252,1614,1635]" italics="true" pageId="21" pageNumber="36">(g)</emphasis>
female genitalia, lateral, from centre; 
<emphasis box="[510,538,1640,1661]" italics="true" pageId="21" pageNumber="36">(h)</emphasis>
female internal genitalia, dorsal exploded view; 
