<collectingCountry box="[1251,1334,588,614]" name="France" pageId="17" pageNumber="18">France</collectingCountry>
, 
<collectingCountry box="[1346,1432,588,614]" name="Greece" pageId="17" pageNumber="18">Greece</collectingCountry>
, 
<collectingCountry box="[151,219,626,652]" name="Israel" pageId="17" pageNumber="18">Israel</collectingCountry>
