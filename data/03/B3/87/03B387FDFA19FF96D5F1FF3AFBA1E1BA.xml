<collectingCountry box="[1125,1216,549,575]" name="Algeria" pageId="17" pageNumber="18">Algeria</collectingCountry>
, 
<collectingCountry box="[1231,1335,549,575]" name="Bulgaria" pageId="17" pageNumber="18">Bulgaria</collectingCountry>
, Canary Islands (Fuerteventura, Lanzarote), 
<collectingCountry box="[571,664,588,614]" name="France" pageId="17" pageNumber="18">Corsica</collectingCountry>
, Crete, Crimea, 
<collectingCountry box="[856,945,588,614]" name="Croatia" pageId="17" pageNumber="18">Croatia</collectingCountry>
