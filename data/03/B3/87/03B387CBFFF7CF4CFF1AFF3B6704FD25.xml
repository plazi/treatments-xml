<collectingRegion box="[1066,1142,602,627]" country="China" name="Fujian" pageId="8" pageNumber="309">Fujian</collectingRegion>
, 
<collectingRegion box="[1155,1254,602,627]" country="China" name="Guangxi" pageId="8" pageNumber="309">Guangxi</collectingRegion>
, 
<collectingRegion box="[1268,1358,603,627]" country="China" name="Yunnan" pageId="8" pageNumber="309">Yunnan</collectingRegion>
), 
<collectingCountry box="[1378,1436,602,627]" name="India" pageId="8" pageNumber="309">India</collectingCountry>
(Bengal), 
<collectingCountry box="[263,372,638,663]" name="Indonesia" pageId="8" pageNumber="309">Indonesia</collectingCountry>
(
<collectingRegion box="[386,439,639,663]" country="Indonesia" name="Jawa Timur" pageId="8" pageNumber="309">Java</collectingRegion>
