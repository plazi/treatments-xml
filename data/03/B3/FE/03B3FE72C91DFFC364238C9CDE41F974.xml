<quantity box="[215,319,1293,1317]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" pageId="36" pageNumber="436" unit="mm" value="0.25">0.25 mm</quantity>
wide, 
<quantity box="[402,490,1293,1317]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="36" pageNumber="436" unit="mm" value="2.5">2.5 mm</quantity>
