<taxonomicName authority="(Haller, 1877)" baseAuthorityName="Haller" baseAuthorityYear="1877" box="[752,1143,501,526]" class="Arachnida" family="Tenuipalpidae" genus="Pentamerismus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="992" phylum="Arthropoda" rank="species" species="taxi">
