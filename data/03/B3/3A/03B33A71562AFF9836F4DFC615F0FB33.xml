<taxonomicName authority="Deshayes, 1856" authorityName="Deshayes" authorityYear="1856" box="[726,1115,1038,1062]" class="Coccolithophyceae" family="Isochrysidaceae" genus="Scintilla" kingdom="Chromista" order="Isochrysidales" pageId="31" pageNumber="3205" phylum="Haptophyta" rank="species" species="philippinensis">
