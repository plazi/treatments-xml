<quantity box="[1023,1186,295,319]" metricMagnitude="-2" metricUnit="m" metricValue="1.345" metricValueMax="1.45" metricValueMin="1.24" pageId="10" pageNumber="11" unit="mm" value="13.45" valueMax="14.5" valueMin="12.4">12.4–14.5 mm</quantity>
