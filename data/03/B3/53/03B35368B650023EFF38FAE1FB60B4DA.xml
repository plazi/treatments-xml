<number box="[846,906,1804,1828]" pageId="2" pageNumber="3" value="1841.0">1841</number>
</bibRefCitation>
); length of forewing 
<quantity box="[1140,1288,1804,1828]" metricMagnitude="-2" metricUnit="m" metricValue="1.055" metricValueMax="1.1199999999999999" metricValueMin="0.99" pageId="2" pageNumber="3" unit="mm" value="10.55" valueMax="11.2" valueMin="9.9">9.9–11.2 mm</quantity>
