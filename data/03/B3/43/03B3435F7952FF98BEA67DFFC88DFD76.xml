<emphasis bold="true" box="[880,937,1698,1722]" pageId="12" pageNumber="51">A–E,</emphasis>
male; 
<emphasis bold="true" box="[1013,1032,1698,1722]" pageId="12" pageNumber="51">A</emphasis>
, wing; 
<emphasis bold="true" box="[1116,1134,1698,1722]" pageId="12" pageNumber="51">B</emphasis>
