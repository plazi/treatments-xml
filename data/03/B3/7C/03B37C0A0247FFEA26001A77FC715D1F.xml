µm); femur 
<number box="[1186,1201,364,390]" pageId="22" pageNumber="23" value="3.0">3</number>
or 
<number box="[1240,1257,364,390]" pageId="22" pageNumber="23" value="4.0">4</number>
; tibia 
<number box="[264,278,404,430]" pageId="22" pageNumber="23" value="6.0">6</number>
or 
<number box="[318,333,404,430]" pageId="22" pageNumber="23" value="7.0">7</number>
and tarsus 
<number box="[467,481,404,430]" pageId="22" pageNumber="23" value="4.0">4</number>
