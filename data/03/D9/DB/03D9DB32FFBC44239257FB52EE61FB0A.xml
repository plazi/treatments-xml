<taxonomicName authority="(Brandt, 1833)" baseAuthorityName="Brandt" baseAuthorityYear="1833" box="[593,1066,234,258]" class="Malacostraca" family="Porcellionidae" genus="Porcellionides" kingdom="Animalia" order="Isopoda" pageId="6" pageNumber="1879" phylum="Arthropoda" rank="species" species="pruinosus">
