de Santa Marta, 
<collectingRegion box="[799,926,1053,1078]" country="Colombia" name="Magdalena" pageId="55" pageNumber="56">Magdalena</collectingRegion>
, 
<collectingCountry box="[937,1051,1053,1078]" name="Colombia" pageId="55" pageNumber="56">Colombia</collectingCountry>
