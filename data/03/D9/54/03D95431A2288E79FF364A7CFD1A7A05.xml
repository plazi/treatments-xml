<emphasis bold="true" box="[562,626,476,501]" pageId="26" pageNumber="27">n. sp.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName authority="Passanha &amp; Brescovit, 2018" authorityName="Passanha &amp; Brescovit" authorityYear="2018" box="[641,814,477,500]" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="amarumayu" status="n. sp.">
