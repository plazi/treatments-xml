<taxonomicName authority="Passanha &amp; Brescovit, 2018" authorityName="Passanha &amp; Brescovit" authorityYear="2018" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" lastPageId="27" lastPageNumber="28" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="amarumayu" status="n. sp.">
