, 
<taxonomicName authority="Passanha &amp; Brescovit, 2018" authorityName="Passanha &amp; Brescovit" authorityYear="2018" box="[1173,1314,2012,2037]" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="yacambu" status="n. sp.">
