<taxonomicName authorityName="Raven" authorityYear="1981" box="[829,1085,1770,1795]" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="colombiensis">
