<subSubSection pageId="3" pageNumber="54" type="etymology">
<paragraph blockId="3.[264,1324,283,1870]" pageId="3" pageNumber="54">
<emphasis bold="true" box="[312,456,844,870]" pageId="3" pageNumber="54">Etymology:</emphasis>
