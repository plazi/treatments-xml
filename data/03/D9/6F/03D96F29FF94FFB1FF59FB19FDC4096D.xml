<collectionCode box="[307,386,1529,1550]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="49" pageNumber="494" type="Museum">USNM</collectionCode>
