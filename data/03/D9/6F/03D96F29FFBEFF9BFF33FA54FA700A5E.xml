locality
</emphasis>
: 
<location LSID="urn:lsid:plazi:treatment:03D96F29FFBEFF9BFF33FA54FA700A5E:8EAF88E4FFBEFF9BFEAFF9A4FE320830" box="[325,481,1560,1582]" country="Greenland" name="Arctic Ocean" pageId="27" pageNumber="472">Arctic Ocean</location>
, off 
<collectingCountry box="[540,663,1560,1582]" name="Greenland" pageId="27" pageNumber="472">Greenland</collectingCountry>
, north of 
<collectingCountry box="[162,372,1591,1613]" name="Jan Mayen Island" pageId="27" pageNumber="472">Jan Mayen Island</collectingCountry>
; ∼
</materialsCitation>
<geoCoordinate box="[398,458,1591,1612]" degrees="74" direction="north" orientation="latitude" pageId="27" pageNumber="472" precision="55555" value="74.0">74°N</geoCoordinate>
, 
