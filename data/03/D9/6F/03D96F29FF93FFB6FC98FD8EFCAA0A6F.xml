: 
<location LSID="urn:lsid:plazi:treatment:03D96F29FF93FFB6FC98FD8EFCAA0A6F:8EAF88E4FF93FFB6FC38FDDEFB9C0C65" box="[978,1103,610,632]" country="Sweden" latitude="57.883335" longLatPrecision="1048" longitude="11.533334" name="Marstrand" pageId="54" pageNumber="499">Marstrand</location>
