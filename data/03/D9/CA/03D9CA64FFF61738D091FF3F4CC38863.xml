<taxonomicName baseAuthorityName="Zhang et al." baseAuthorityYear="2010" box="[1314,1436,662,683]" class="Insecta" family="Thripidae" genus="Yaobinthrips" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="145" phylum="Arthropoda" rank="genus">
