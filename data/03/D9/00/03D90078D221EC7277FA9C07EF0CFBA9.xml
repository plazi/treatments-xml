by Bullock (
<number box="[674,734,1088,1112]" pageId="3" pageNumber="38" value="1968.0">1968</number>
), Sutty (
<number box="[831,889,1088,1112]" pageId="3" pageNumber="38" value="1986.0">1986</number>
) and Vermeij and Snyder (
<number box="[1194,1253,1088,1112]" pageId="3" pageNumber="38" value="2006.0">2006</number>
) in 
<taxonomicName class="Gastropoda" family="Fasciolariidae" genus="Pustulatirus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="3" pageNumber="38" phylum="Mollusca" rank="species" species="virginensis">
