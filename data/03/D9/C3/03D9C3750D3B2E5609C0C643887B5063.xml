<typeStatus box="[439,548,1344,1369]" pageId="14" pageNumber="347">Holotype</typeStatus>
: ♂, 
<collectingCountry box="[593,664,1344,1369]" name="China" pageId="14" pageNumber="347">China</collectingCountry>
: Yunnan Prov., Lancang, 
<quantity box="[950,1029,1344,1368]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="14" pageNumber="347" unit="m" value="1100.0">1100m</quantity>
, 
<date box="[1040,1149,1344,1369]" pageId="14" pageNumber="347" value="1999-11-02">
<number box="[1040,1054,1344,1368]" pageId="14" pageNumber="347" value="2.0">2</number>
xi 
