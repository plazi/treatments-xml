Entomological Museum, 
<collectionCode box="[624,726,1381,1405]" pageId="14" pageNumber="347">NWAFU</collectionCode>
, Yangling, 
<collectingCountry box="[853,924,1380,1405]" name="China" pageId="14" pageNumber="347">China</collectingCountry>
