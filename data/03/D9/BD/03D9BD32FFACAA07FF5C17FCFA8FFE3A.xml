<number box="[1366,1398,1844,1868]" pageId="10" pageNumber="39" value="22.0">22</number>
. V. 
<number box="[151,210,1880,1904]" pageId="10" pageNumber="39" value="1996.0">1996</number>
</date>
, 
<quantity box="[221,308,1880,1904]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="10" pageNumber="39" unit="m" value="1700.0">1700 m</quantity>
, Leyi Zheng leg. 
<typeStatus box="[505,607,1881,1905]" pageId="10" pageNumber="39">Paratype</typeStatus>
: 
<specimenCount box="[617,717,1880,1904]" pageId="10" pageNumber="39" type="female">
<number box="[617,631,1880,1904]" pageId="10" pageNumber="39" value="1.0">1</number>
female
