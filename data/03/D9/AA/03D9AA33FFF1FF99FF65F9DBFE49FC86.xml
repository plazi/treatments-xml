<subSubSection pageId="4" pageNumber="387" type="etymology">
<paragraph blockId="4.[151,1437,151,843]" pageId="4" pageNumber="387">
<emphasis bold="true" box="[199,334,785,810]" pageId="4" pageNumber="387">Etymology.</emphasis>
