of 
<taxonomicName authorityName="Curletti" authorityYear="2001" box="[524,623,1672,1696]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="385" phylum="Arthropoda" rank="species" species="dingo">
