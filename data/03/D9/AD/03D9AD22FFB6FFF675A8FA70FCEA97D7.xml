<taxonomicName box="[191,316,1026,1048]" class="Insecta" family="Noteridae" genus="Liocanthydrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="237" phylum="Arthropoda" rank="species" species="uniformis">
<emphasis box="[191,316,1026,1048]" italics="true" pageId="6" pageNumber="237">L. uniformis</emphasis>
</taxonomicName>
; 18) 
