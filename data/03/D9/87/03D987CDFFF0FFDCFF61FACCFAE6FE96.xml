<taxonomicName authority="Casto, 1980" authorityName="Casto" authorityYear="1980" box="[1015,1307,151,176]" class="Arachnida" family="Syringophilidae" genus="Kalamotrypetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="19" phylum="Arthropoda" rank="species" species="colinastes">
