distinctly longer than 
<emphasis box="[907,921,1096,1121]" italics="true" pageId="5" pageNumber="554">h</emphasis>
<quantity box="[921,960,1096,1125]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="5" pageNumber="554" unit="in" value="2.0">2 in</quantity>
<taxonomicName box="[969,1147,1096,1121]" class="Arachnida" family="Pygmephoridae" genus="Petalomium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="554" phylum="Arthropoda" rank="species" species="heterotrichus">
<emphasis box="[969,1147,1096,1121]" italics="true" pageId="5" pageNumber="554">P. heterotrichus</emphasis>
</taxonomicName>
) and by smooth setae 
<number box="[1410,1424,1097,1121]" pageId="5" pageNumber="554" value="4.0">4</number>
<emphasis box="[1423,1435,1097,1120]" italics="true" pageId="5" pageNumber="554">c</emphasis>
(barbed in 
<taxonomicName box="[272,448,1135,1160]" class="Arachnida" family="Pygmephoridae" genus="Petalomium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="554" phylum="Arthropoda" rank="species" species="heterotrichus">
