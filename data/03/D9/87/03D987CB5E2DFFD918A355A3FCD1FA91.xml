<quantity box="[742,920,1278,1303]" metricMagnitude="2" metricUnit="m" metricValue="7.0" metricValueMax="10.0" metricValueMin="4.0" pageId="21" pageNumber="176" unit="m" value="700.0" valueMax="1000.0" valueMin="400.0">400 to 1000 m</quantity>
