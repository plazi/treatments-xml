<subSubSection box="[199,1005,260,285]" pageId="7" pageNumber="158" type="etymology">
<paragraph blockId="7.[199,1281,151,285]" box="[199,1005,260,285]" pageId="7" pageNumber="158">
<emphasis bold="true" box="[199,334,260,285]" pageId="7" pageNumber="158">Etymology.</emphasis>
