<collectingCountry box="[359,413,1340,1366]" name="Laos" pageId="6" pageNumber="311">Laos</collectingCountry>
<emphasis bold="true" box="[421,430,1339,1365]" pageId="6" pageNumber="311">(</emphasis>
<collectingRegion box="[430,577,1339,1366]" country="Laos" name="Bolikhamxai" pageId="6" pageNumber="311">Bolikhamsai</collectingRegion>
