<quantity metricMagnitude="3" metricUnit="m" metricValue="2.0" metricValueMax="2.2" metricValueMin="1.8" pageId="29" pageNumber="184" unit="m" value="2000.0" valueMax="2200.0" valueMin="1800.0">1800- 2200 m</quantity>
