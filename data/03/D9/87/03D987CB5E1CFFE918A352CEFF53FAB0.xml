Rchb.
</taxonomicName>
f.: 
<emphasis bold="true" box="[1008,1021,774,791]" pageId="37" pageNumber="192">A</emphasis>
, 
<emphasis bold="true" box="[1035,1048,774,791]" pageId="37" pageNumber="192">B</emphasis>
, flower; 
<emphasis bold="true" box="[1128,1142,774,791]" pageId="37" pageNumber="192">C</emphasis>
, lip; 
<emphasis bold="true" box="[1189,1203,774,791]" pageId="37" pageNumber="192">D</emphasis>
, 
<taxonomicName authority="Harv." authorityName="Harv." box="[113,294,795,812]" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="37" pageNumber="192" phylum="Tracheophyta" rank="species" species="cardiophora">
