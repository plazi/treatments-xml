<emphasis bold="true" box="[694,730,774,791]" pageId="37" pageNumber="192">A -C</emphasis>
, 
<taxonomicName authority="Rchb." authorityName="Rchb." box="[743,983,774,791]" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="37" pageNumber="192" phylum="Tracheophyta" rank="species" species="lindleyana">
