<quantity box="[891,1080,696,721]" metricMagnitude="3" metricUnit="m" metricValue="1.8" metricValueMax="2.25" metricValueMin="1.35" pageId="28" pageNumber="183" unit="m" value="1800.0" valueMax="2250.0" valueMin="1350.0">1350 to 2250 m</quantity>
