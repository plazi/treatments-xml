<quantity box="[277,463,1217,1242]" metricMagnitude="3" metricUnit="m" metricValue="1.6" metricValueMax="2.0" metricValueMin="1.2" pageId="21" pageNumber="176" unit="m" value="1600.0" valueMax="2000.0" valueMin="1200.0">1200 to 2000 m</quantity>
