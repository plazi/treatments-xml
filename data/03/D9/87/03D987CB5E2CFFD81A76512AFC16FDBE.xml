<quantity box="[1096,1201,389,414]" metricMagnitude="-3" metricUnit="m" metricValue="8.5" metricValueMax="11.0" metricValueMin="6.0" pageId="20" pageNumber="175" unit="mm" value="8.5" valueMax="11.0" valueMin="6.0">6-11 mm</quantity>
, basally joined (up to third); lip 
<quantity box="[1022,1146,420,445]" metricMagnitude="-3" metricUnit="m" metricValue="8.75" metricValueMax="10.0" metricValueMin="7.5" pageId="20" pageNumber="175" unit="mm" value="8.75" valueMax="10.0" valueMin="7.5">7.5-10 mm</quantity>
