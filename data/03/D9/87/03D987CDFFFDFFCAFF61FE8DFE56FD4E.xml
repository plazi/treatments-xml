<emphasis box="[1339,1360,1706,1730]" italics="true" pageId="15" pageNumber="28">
f 
<number box="[1351,1360,1706,1730]" pageId="15" pageNumber="28" value="2.0">2</number>
</emphasis>
. Setae 
<emphasis box="[151,178,1742,1766]" italics="true" pageId="15" pageNumber="28">
h 
<number box="[166,178,1742,1766]" pageId="15" pageNumber="28" value="1.0">1</number>
</emphasis>
