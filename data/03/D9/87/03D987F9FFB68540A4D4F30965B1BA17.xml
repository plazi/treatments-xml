<collectingRegion box="[877,952,1961,1986]" country="Nigeria" name="Rivers" pageId="13" pageNumber="14">Rivers</collectingRegion>
State) (
<bibRefCitation author="Powell, C. B." box="[1037,1182,1961,1986]" journalOrPublisher="Revue de Zoologie Africaine" pageId="13" pageNumber="14" pagination="883 – 902" part="90" refId="ref44535" refString="Powell, C. B. (1976) Two new freshwater shrimps from West Africa: the first euryrhynchinids (Decapoda Palaemonidae) reported from the Old World. Revue de Zoologie Africaine, 90, 883 - 902." title="Two new freshwater shrimps from West Africa: the first euryrhynchinids (Decapoda Palaemonidae) reported from the Old World" type="journal article" year="1976">Powell 1976</bibRefCitation>
; 
<bibRefCitation author="Nwadiaro, C. S." box="[1193,1368,1961,1986]" journalOrPublisher="Hydrobiological Bulletin" pageId="13" pageNumber="14" pagination="133 – 140" part="18" publicationUrl="https://doi.org/10.1007/BF02257052" refId="ref44023" refString="Nwadiaro, C. S. (1984) The longitudinal distribution of macroinvertebrates and fish in a lower Niger Delta River (River Sombreiro) in Nigeria. Hydrobiological Bulletin, 18, 133 - 140. https: // doi. org / 10.1007 / BF 02257052" title="The longitudinal distribution of macroinvertebrates and fish in a lower Niger Delta River (River Sombreiro) in Nigeria" type="journal article" year="1984">Nwadiaro 1984</bibRefCitation>
) (
<figureCitation captionStart="FIGURE 62" captionStartId="90.[151,250,1882,1904]" captionTargetBox="[151,1436,193,1861]" captionTargetId="figure@90.[151,1436,193,1861]" captionTargetPageId="90" captionText="FIGURE 62. Distribution maps for the African euryrhynchids. (A–B) Euryrhynchoides holthuisi Powell, 1976 (blue circles). (B) Euryrhynchina edingtonae Powell, 1976 (yellow circles) and Euryrhynchina puteola De Grave, Piscart, Tuekam Kayo &amp; Anker, 2017 (orange circle).The corresponding localities for each point on the maps are indicated on the Appendix 1A–C." httpUri="https://zenodo.org/record/1175774/files/figure.png" pageId="13" pageNumber="14">Fig. 62</figureCitation>
, blue circles).
</paragraph>
</subSubSection>
<caption httpUri="https://zenodo.org/record/1175664/files/figure.png" pageId="14" pageNumber="15" startId="14.[151,250,1833,1855]" targetBox="[185,1409,205,1797]" targetPageId="14">
