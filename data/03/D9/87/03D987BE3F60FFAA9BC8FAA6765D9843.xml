<paragraph blockId="6.[144,382,1559,1584]" box="[144,382,1559,1584]" pageId="6" pageNumber="2672">
<emphasis box="[144,284,1560,1584]" italics="true" pageId="6" pageNumber="2672">Body length.</emphasis>
<quantity box="[290,378,1559,1583]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="6" pageNumber="2672" unit="mm" value="4.5">4.5 mm</quantity>
