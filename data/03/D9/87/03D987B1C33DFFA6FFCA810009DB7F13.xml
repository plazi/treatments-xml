<subSubSection box="[112,415,1959,2014]" pageId="3" pageNumber="4" type="diagnosis">
<paragraph blockId="3.[112,415,1959,2014]" box="[112,217,1959,1985]" pageId="3" pageNumber="4">
<heading bold="true" box="[112,217,1959,1985]" fontSize="10" level="3" pageId="3" pageNumber="4" reason="6">
<emphasis bold="true" box="[112,217,1959,1985]" italics="true" pageId="3" pageNumber="4">Diagnosis</emphasis>
</heading>
</paragraph>
<paragraph blockId="3.[112,415,1959,2014]" box="[112,415,1990,2014]" pageId="3" pageNumber="4">
