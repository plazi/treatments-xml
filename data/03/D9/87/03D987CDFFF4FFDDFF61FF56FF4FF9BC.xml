<number box="[1192,1234,330,354]" pageId="7" pageNumber="20" value="155.0">155</number>
(
<number box="[1256,1357,330,354]" pageId="7" pageNumber="20" value="157.5" valueMax="165.0" valueMin="150.0">150–165</number>
