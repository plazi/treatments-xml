<taxonomicName authority="(Sm.) Thouars, Hist. Orchid." authorityName="Thouars, Hist. Orchid." baseAuthorityName="Sm." class="Liliopsida" family="Orchidaceae" genus="Dryopeia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="25" pageNumber="180" phylum="Tracheophyta" rank="species" species="oppositifolia">
