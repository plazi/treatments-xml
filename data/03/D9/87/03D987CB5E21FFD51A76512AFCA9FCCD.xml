<taxonomicName authority="Thouars, Hist. Orchid." authorityName="Thouars, Hist. Orchid." class="Liliopsida" family="Orchidaceae" genus="Dryorkis" kingdom="Plantae" order="Asparagales" pageId="25" pageNumber="180" phylum="Tracheophyta" rank="species" species="antidris">
