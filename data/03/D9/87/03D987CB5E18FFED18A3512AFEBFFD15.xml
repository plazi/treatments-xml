<collectingCountry box="[434,492,573,598]" name="Democratic Republic of the Congo" pageId="33" pageNumber="188">DRC</collectingCountry>
; in grassland, woodland and bog from 
<quantity box="[430,637,604,629]" metricMagnitude="3" metricUnit="m" metricValue="2.025" metricValueMax="2.4" metricValueMin="1.65" pageId="33" pageNumber="188" unit="m" value="2025.0" valueMax="2400.0" valueMin="1650.0">1650 to 2400 m</quantity>
