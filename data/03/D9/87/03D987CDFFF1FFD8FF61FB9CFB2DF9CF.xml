<paragraph blockId="2.[151,842,1187,1240]" box="[151,842,1218,1240]" pageId="2" pageNumber="15">
<typeStatus box="[151,200,1219,1240]" pageId="2" pageNumber="15">Type</typeStatus>
species: 
<taxonomicName authority="Heller, 1880" authorityName="Heller" authorityYear="1880" box="[294,697,1218,1240]" class="Arachnida" family="Syringophilidae" genus="Syringophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="15" phylum="Arthropoda" rank="species" species="bipectinatus">
<emphasis box="[294,565,1218,1240]" italics="true" pageId="2" pageNumber="15">Syringophilus bipectinatus</emphasis>
<bibRefCitation author="Heller" box="[571,697,1218,1240]" pageId="2" pageNumber="15" refString="Heller, A. (1880) Die Schmarotzer, Munich und Leipzig. 186 - 188 pp." type="book chapter" year="1880">
