<subSubSection pageId="44" pageNumber="45" type="type_taxon">
<paragraph blockId="44.[189,344,1519,1545]" box="[189,344,1519,1545]" pageId="44" pageNumber="45">
<heading bold="true" box="[189,344,1519,1545]" fontSize="11" level="3" pageId="44" pageNumber="45" reason="3">
