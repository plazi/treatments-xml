Inner 
<collectingCountry box="[641,746,1088,1111]" name="Mongolia" pageId="3" pageNumber="148">Mongolia</collectingCountry>
</collectingRegion>
) and 
<collectingCountry box="[806,910,1088,1111]" name="Mongolia" pageId="3" pageNumber="148">Mongolia</collectingCountry>
(
<collectingRegion box="[923,1083,1088,1111]" country="Mongolia" name="Bayanhongor" pageId="3" pageNumber="148">Bayankhongor</collectingRegion>
