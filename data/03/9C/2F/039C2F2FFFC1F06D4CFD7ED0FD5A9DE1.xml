<subSubSection pageId="8" pageNumber="67" type="etymology">
<paragraph blockId="8.[151,1436,151,248]" pageId="8" pageNumber="67">
<emphasis bold="true" box="[199,334,152,176]" pageId="8" pageNumber="67">Etymology.</emphasis>
