<paragraph blockId="12.[362,553,994,1020]" box="[362,553,994,1020]" pageId="12" pageNumber="266">
<heading box="[362,553,994,1020]" centered="true" fontSize="36" level="2" pageId="12" pageNumber="266" reason="5">
<taxonomicName box="[362,553,994,1020]" class="Mammalia" family="Vespertilionidae" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="266" phylum="Chordata" rank="family">Vespertilionidae</taxonomicName>
