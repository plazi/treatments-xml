<collectingCountry box="[824,914,1224,1249]" name="Uganda" pageId="9" pageNumber="410">Uganda</collectingCountry>
, 
<collectingCountry box="[925,1029,1224,1249]" name="Tanzania" pageId="9" pageNumber="410">Tanzania</collectingCountry>
, 
<collectingCountry box="[1039,1389,1224,1249]" name="Democratic Republic of the Congo" pageId="9" pageNumber="410">
Democratic Republic of 
<collectingRegion box="[1314,1389,1224,1248]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="9" pageNumber="410">Congo</collectingRegion>
</collectingCountry>
and 
<collectingCountry box="[151,236,1260,1285]" name="Zambia" pageId="9" pageNumber="410">Zambia</collectingCountry>
(
<bibRefCitation author="Karsch" box="[251,398,1260,1285]" pageId="9" pageNumber="410" refString="KarSch, F. (1896) Neue Orthopteren auS dem tropiSchen Afrika. Stettiner Entomologische Zeitung 57, 242 - 359. pteridae parS 2 a (reliquae SpecieS omneS). Memoires de la Societe entomologique Belgique, 15, 201 - 226. Available from: http: // www. biodiverSitylibrary. org / item / 36013  page / 250 / mode / 1 up (AcceSSed 20 Nov. 2017)" type="journal article" year="1896">Karsch 1896</bibRefCitation>
, 
<bibRefCitation author="Heller" box="[409,608,1260,1285]" pageId="9" pageNumber="410" refString="Heller, K. G., Hemp, C., Liu, C. &amp; Volleth, M. (2014) Taxonomic, bioacouSticS and fauniStic data on a collection of Tettigonioidea from EaStern Congo (InSecta: Orthoptera). Zootaxa, 3785 (3), 343 - 376." type="journal article" year="2014">
Heller 
<emphasis box="[486,535,1260,1285]" italics="true" pageId="9" pageNumber="410">et al</emphasis>
. 2014
</bibRefCitation>
, 
<bibRefCitation author="Massa" box="[619,756,1260,1285]" pageId="9" pageNumber="410" refString="MaSSa, B. (2015) New genera, SpecieS and recordS of Phaneropterinae (Orthoptera, Phaneropteridae) from Sub-Saharan Africa. Zookeys 472, 77 - 102." type="journal article" year="2015">Massa 2015</bibRefCitation>
