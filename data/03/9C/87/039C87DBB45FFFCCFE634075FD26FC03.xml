</taxonomicName>
<taxonomicNameLabel box="[713,782,220,239]" pageId="10" pageNumber="641" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName authority="Růžička &amp; Pütz, 2009" authorityName="Růžička &amp; Pütz" authorityYear="2009" box="[826,915,220,238]" class="Insecta" family="Agyrtidae" genus="Apteroloma" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="641" phylum="Arthropoda" rank="species" species="jelineki" status="sp. nov.">
