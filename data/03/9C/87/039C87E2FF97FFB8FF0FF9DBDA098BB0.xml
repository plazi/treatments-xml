, 
<taxonomicName box="[151,366,658,683]" class="Ostracoda" family="Candonidae" genus="Dentocypria" kingdom="Animalia" order="Podocopida" pageId="2" pageNumber="331" phylum="Arthropoda" rank="species" species="smithi" status="n. gen.">
<emphasis box="[151,366,658,683]" italics="true" pageId="2" pageNumber="331">Dentocypria smithi</emphasis>
</taxonomicName>
<emphasis bold="true" box="[373,521,658,683]" pageId="2" pageNumber="331">
<taxonomicNameLabel box="[373,452,658,683]" pageId="2" pageNumber="331" rank="genus">n. gen.</taxonomicNameLabel>
