and 
<taxonomicName box="[576,835,658,683]" class="Ostracoda" family="Candonidae" genus="Dentocypria" kingdom="Animalia" order="Podocopida" pageId="2" pageNumber="331" phylum="Arthropoda" rank="species" species="aequiloba" status="n. gen.">
