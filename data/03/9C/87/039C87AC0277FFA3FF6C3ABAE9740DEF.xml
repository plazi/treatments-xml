<number box="[867,916,1094,1118]" pageId="9" pageNumber="10" value="0.48">0.48</number>
long, 
<number box="[987,1036,1094,1118]" pageId="9" pageNumber="10" value="0.4">0.40</number>
wide. Opisthosoma 
<number box="[1267,1316,1094,1118]" pageId="9" pageNumber="10" value="0.69">0.69</number>
long, 
<number box="[1387,1436,1094,1118]" pageId="9" pageNumber="10" value="0.46">0.46</number>
wide. Sternum 
<number box="[325,374,1130,1154]" pageId="9" pageNumber="10" value="0.25">0.25</number>
long, 
<number box="[450,499,1130,1154]" pageId="9" pageNumber="10" value="0.27">0.27</number>
