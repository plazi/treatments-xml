<number box="[554,603,1572,1595]" pageId="1" pageNumber="148" value="0.43">0.43</number>
, PME 
<number box="[681,730,1572,1595]" pageId="1" pageNumber="148" value="0.09">0.09</number>
, PLE 
<number box="[800,850,1572,1595]" pageId="1" pageNumber="148" value="0.35">0.35</number>
; AME-AME 
<number box="[1000,1049,1572,1595]" pageId="1" pageNumber="148" value="0.06">0.06</number>
, AME-ALE 
<number box="[1192,1241,1572,1595]" pageId="1" pageNumber="148" value="0.07">0.07</number>
, PME-PME 
<number box="[1382,1431,1572,1595]" pageId="1" pageNumber="148" value="1.94">1.94</number>
, PME-PLE 
<number box="[274,323,1607,1630]" pageId="1" pageNumber="148" value="0.5">0.50</number>
, ALE-PLE 
<number box="[460,510,1607,1630]" pageId="1" pageNumber="148" value="1.04">1.04</number>
. Clypeus 
<number box="[625,672,1607,1630]" pageId="1" pageNumber="148" value="0.17">0.17</number>
high. Chelicera with 
<number box="[923,936,1607,1630]" pageId="1" pageNumber="148" value="2.0">2</number>
promarginal and 
<number box="[1141,1154,1607,1630]" pageId="1" pageNumber="148" value="1.0">1</number>
retromarginal teeth. Leg measurements: I 
<number box="[333,380,1641,1664]" pageId="1" pageNumber="148" value="9.95">9.95</number>
(
<number box="[394,445,1641,1664]" pageId="1" pageNumber="148" value="2.96">2.96</number>
, 
<number box="[456,505,1641,1664]" pageId="1" pageNumber="148" value="4.08">4.08</number>
, 
<number box="[516,565,1641,1664]" pageId="1" pageNumber="148" value="1.84">1.84</number>
, 
<number box="[576,626,1641,1664]" pageId="1" pageNumber="148" value="1.07">1.07</number>
); II 
<number box="[670,717,1641,1664]" pageId="1" pageNumber="148" value="9.37">9.37</number>
(
<number box="[731,781,1641,1664]" pageId="1" pageNumber="148" value="2.83">2.83</number>
, 
<number box="[792,841,1641,1664]" pageId="1" pageNumber="148" value="3.71">3.71</number>
, 
<number box="[853,902,1641,1664]" pageId="1" pageNumber="148" value="1.76">1.76</number>
, 
<number box="[913,963,1641,1664]" pageId="1" pageNumber="148" value="1.07">1.07</number>
); III 
<number box="[1016,1075,1641,1664]" pageId="1" pageNumber="148" value="11.17">11.17</number>
(
<number box="[1089,1139,1641,1664]" pageId="1" pageNumber="148" value="3.52">3.52</number>
, 
<number box="[1151,1200,1641,1664]" pageId="1" pageNumber="148" value="3.72">3.72</number>
, 
<number box="[1211,1260,1641,1664]" pageId="1" pageNumber="148" value="2.61">2.61</number>
, 
<number box="[1271,1321,1641,1664]" pageId="1" pageNumber="148" value="1.32">1.32</number>
); IV 
<number box="[1375,1435,1641,1664]" pageId="1" pageNumber="148" value="10.79">10.79</number>
(
<number box="[158,208,1676,1699]" pageId="1" pageNumber="148" value="3.2">3.20</number>
, 
<number box="[222,271,1676,1699]" pageId="1" pageNumber="148" value="3.65">3.65</number>
, 
<number box="[283,332,1676,1699]" pageId="1" pageNumber="148" value="2.78">2.78</number>
, 
<number box="[345,395,1676,1699]" pageId="1" pageNumber="148" value="1.16">1.16</number>
). Leg formula: 
<number box="[567,624,1676,1699]" pageId="1" pageNumber="148" value="3412.0">3412</number>
. When alive (
<figureCitation box="[776,843,1676,1699]" captionStart="FIGURES 1 – 4" captionStartId="1.[151,258,1379,1401]" captionTargetBox="[151,1436,281,1358]" captionTargetId="figure@1.[151,1436,281,1358]" captionTargetPageId="1" captionText="FIGURES 1 – 4. Epeus spp. 1 – 2 Epeus bicuspidatus, 1 male, anterolateral view; 2 female, dorsal view; 3 – 4 Epeus glorius, 3 male, dorsal view; 4 same, frontal view." httpUri="https://zenodo.org/record/240943/files/figure.png" pageId="1" pageNumber="148">
Fig. 
<number box="[828,843,1676,1699]" pageId="1" pageNumber="148" value="2.0">2</number>
</figureCitation>
