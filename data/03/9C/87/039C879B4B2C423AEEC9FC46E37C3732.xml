<number box="[258,290,1987,2013]" pageId="14" pageNumber="15" value="38.0">38</number>
; primary chamber external width h 
<number box="[711,743,1987,2013]" pageId="14" pageNumber="15" value="13.0">13</number>
, c 
<number box="[776,805,1987,2013]" pageId="14" pageNumber="15" value="13.0">13</number>
(
<number box="[822,899,1987,2013]" pageId="14" pageNumber="15" value="13.0" valueMax="14.0" valueMin="12.0">12–14</number>
