, 
<collectorName box="[1106,1268,1663,1689]" pageId="63" pageNumber="64">EE Honeycutt</collectorName>
, 
<collectionCode box="[1278,1334,1663,1689]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" lsid="urn:lsid:biocol.org:col:34700" name="California Academy of Sciences" pageId="63" pageNumber="64" type="Museum">CAS</collectionCode>
) through mid-August (
<date box="[300,455,1698,1725]" pageId="63" pageNumber="64" value="2015-08-13">
