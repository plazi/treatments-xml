<emphasis box="[345,690,1953,1975]" italics="true" pageId="9" pageNumber="266">
<taxonomicName box="[345,497,1953,1975]" class="Insecta" family="Limoniidae" genus="Dicranoptycha" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="266" phylum="Arthropoda" rank="genus">Dicranoptycha</taxonomicName>
