<subSubSection pageId="26" pageNumber="27" type="description">
<paragraph blockId="26.[264,1323,473,1899]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[312,448,793,819]" pageId="26" pageNumber="27">Size range.</emphasis>
