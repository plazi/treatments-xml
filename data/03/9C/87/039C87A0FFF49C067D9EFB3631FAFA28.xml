<taxonomicName authority="Wygodzinsky, 1966" authorityName="Wygodzinsky" authorityYear="1966" box="[1118,1435,737,759]" class="Insecta" family="Reduviidae" genus="Phasmatocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="505" phylum="Arthropoda" rank="species" species="breddini">
