with 
<taxonomicName box="[192,304,1572,1595]" class="Insecta" family="Glossosomatidae" genus="Agapetus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="42" phylum="Arthropoda" rank="species" species="tomus">
