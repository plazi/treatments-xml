<taxonomicName baseAuthorityName="Betten and Mosely" baseAuthorityYear="1940" box="[1254,1379,1539,1563]" class="Insecta" family="Glossosomatidae" genus="Agapetus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="42" phylum="Arthropoda" rank="species" species="walkeri">
<emphasis box="[1254,1379,1539,1563]" italics="true" pageId="43" pageNumber="42">A. walkeri</emphasis>
