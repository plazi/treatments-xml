<taxonomicName box="[694,808,1539,1563]" class="Insecta" family="Glossosomatidae" genus="Agapetus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="42" phylum="Arthropoda" rank="species" species="baueri">A. baueri</taxonomicName>
, 
<taxonomicName box="[818,950,1540,1563]" class="Insecta" family="Glossosomatidae" genus="Agapetus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="42" phylum="Arthropoda" rank="species" species="jocassee">A. jocassee</taxonomicName>
, 
<taxonomicName box="[959,1087,1540,1563]" class="Insecta" family="Glossosomatidae" genus="Agapetus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="42" phylum="Arthropoda" rank="species" species="pinatus">A. pinatus</taxonomicName>
