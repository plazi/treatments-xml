<subSubSection pageId="14" pageNumber="645" type="distribution">
<paragraph blockId="14.[151,1127,219,564]" pageId="14" pageNumber="645">
<emphasis bold="true" box="[151,298,475,499]" pageId="14" pageNumber="645">Distribution.</emphasis>
<taxonomicName box="[307,564,477,500]" class="Insecta" family="Agyrtidae" genus="Apteroloma" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="645" phylum="Arthropoda" rank="species" species="qinlingense">
<emphasis box="[307,564,477,500]" italics="true" pageId="14" pageNumber="645">Apteroloma qinlingense</emphasis>
