<quantity box="[1199,1301,2009,2035]" metricMagnitude="-4" metricUnit="m" metricValue="8.8" pageId="45" pageNumber="46" unit="mm" value="0.88">0.88 mm</quantity>
. 
<taxonomicName authority=": Body" authorityName="Body" class="Liliopsida" family="Poaceae" genus="Doratura" kingdom="Plantae" lastPageId="46" lastPageNumber="47" order="Poales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="rusaevi">
