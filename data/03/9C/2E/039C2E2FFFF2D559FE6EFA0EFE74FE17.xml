<taxonomicName authority="FMNH" authorityName="FMNH" box="[546,756,1464,1488]" class="Mammalia" family="Echimyidae" genus="Toromys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="rhipidurus">
