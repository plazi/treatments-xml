<taxonomicName box="[282,299,1281,1303]" class="Chilopoda" family="Cryptopidae" genus="Cryptops" kingdom="Animalia" order="Scolopendromorpha" pageId="7" pageNumber="439" phylum="Arthropoda" rank="species" species="hephaestus" status="n. sp." subGenus="Trigonocryptops">
<emphasis bold="true" box="[282,299,1281,1303]" italics="true" pageId="7" pageNumber="439">C</emphasis>
</taxonomicName>
</emphasis>
. (
<emphasis bold="true" box="[320,334,1281,1302]" italics="true" pageId="7" pageNumber="439">T</emphasis>
.) 
<emphasis bold="true" box="[355,536,1281,1303]" pageId="7" pageNumber="439">
<emphasis bold="true" box="[355,472,1281,1303]" italics="true" pageId="7" pageNumber="439">hephaestus</emphasis>
