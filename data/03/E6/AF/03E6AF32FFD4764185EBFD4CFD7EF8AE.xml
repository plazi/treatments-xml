<taxonomicName authority="(Fairmaire, 1891)" authorityName="Fairmaire" authorityYear="1891" baseAuthorityName="Fairmaire" baseAuthorityYear="1891" box="[1049,1432,1628,1653]" class="Insecta" family="Scarabaeidae" genus="Garreta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="243" phylum="Arthropoda" rank="species" species="lugens">
<emphasis box="[1049,1220,1628,1652]" italics="true" pageId="1" pageNumber="243">Garreta lugens</emphasis>
(Fairmaire, 1891)
</taxonomicName>
, det
</materialsCitation>
. CSIRO, DBRU and 
<taxonomicName authority="(Fairmaire, 1891)" authorityName="Fairmaire" authorityYear="1891" baseAuthorityName="Fairmaire" baseAuthorityYear="1891" box="[428,804,1663,1688]" class="Insecta" family="Scarabaeidae" genus="Garreta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="243" phylum="Arthropoda" rank="species" species="lugens">
