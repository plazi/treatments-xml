<taxonomicName authority="Gorochov, 2004" authorityName="Gorochov" authorityYear="2004" box="[633,898,1338,1363]" class="Insecta" family="Haglidae" genus="Neolarnaca" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="230" phylum="Arthropoda" rank="species" species="vera">
