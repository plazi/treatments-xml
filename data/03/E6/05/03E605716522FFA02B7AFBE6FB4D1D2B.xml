<quantity box="[491,590,1385,1408]" metricMagnitude="-2" metricUnit="m" metricValue="2.5" metricValueMax="2.6" metricValueMin="2.4" pageId="1" pageNumber="975" unit="mm" value="25.0" valueMax="26.0" valueMin="24.0">24-26 µm</quantity>
