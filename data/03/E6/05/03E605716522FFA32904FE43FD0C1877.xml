<quantity box="[1219,1371,652,675]" metricMagnitude="-4" metricUnit="m" metricValue="6.85" metricValueMax="8.299999999999999" metricValueMin="5.4" pageId="1" pageNumber="975" unit="mm" value="0.685" valueMax="0.83" valueMin="0.54">0.54-0.83 mm</quantity>
