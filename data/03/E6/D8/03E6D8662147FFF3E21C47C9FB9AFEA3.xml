<subSubSection pageId="8" pageNumber="89" type="distribution">
<paragraph blockId="8.[190,1180,175,230]" pageId="8" pageNumber="89">
<emphasis bold="true" box="[190,328,176,194]" pageId="8" pageNumber="89">Distribution:</emphasis>
<taxonomicName box="[336,576,176,194]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="89" phylum="Arthropoda" rank="species" species="mahakamensismahakamensis">
<emphasis box="[336,576,176,194]" italics="true" pageId="8" pageNumber="89">Anomala mahakamensismahakamensis</emphasis>
</taxonomicName>
