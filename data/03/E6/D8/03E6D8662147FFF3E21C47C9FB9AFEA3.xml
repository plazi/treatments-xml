<collectingRegion box="[190,382,206,230]" country="Indonesia" name="Kalimantan Timur" pageId="8" pageNumber="89">Kalimantan Timur</collectingRegion>
(
<collectingRegion box="[396,570,206,230]" country="Indonesia" name="Kalimantan Timur" pageId="8" pageNumber="89">East Kalimantan</collectingRegion>
) (
<figureCitation box="[591,666,206,230]" captionStart="Fig" captionStartId="9.[146,177,1120,1142]" captionTargetBox="[144,1054,174,1104]" captionTargetId="figure-290@9.[144,1054,174,1112]" captionTargetPageId="9" captionText="Fig. 22:Tistribution map." figureDoi="http://doi.org/10.5281/zenodo.4752739" httpUri="https://zenodo.org/record/4752739/files/figure.png" pageId="8" pageNumber="89">Fig. 22</figureCitation>
