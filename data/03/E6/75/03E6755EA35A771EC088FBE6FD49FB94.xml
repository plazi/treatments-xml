<quantity box="[1073,1201,1996,2019]" metricMagnitude="-3" metricUnit="m" metricValue="6.3" metricValueMax="7.4" metricValueMin="5.2" pageId="11" pageNumber="308" unit="mm" value="6.3" valueMax="7.4" valueMin="5.2">5.2–7.4 mm</quantity>
(LT 
<quantity box="[1253,1339,1996,2019]" metricMagnitude="-3" metricUnit="m" metricValue="6.2" pageId="11" pageNumber="308" unit="mm" value="6.2">6.2 mm</quantity>
