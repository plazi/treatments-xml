<taxonomicName box="[1138,1242,439,464]" class="Insecta" family="Erebidae" genus="Arctiinae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="423" phylum="Arthropoda" rank="genus">Arctiinae</taxonomicName>
specialist from 
<collectingRegion box="[151,291,476,501]" country="Russia" name="Novosibirsk" pageId="5" pageNumber="416">Novosibirsk</collectingRegion>
, 
<collectingCountry box="[302,380,476,501]" name="Russia" pageId="5" pageNumber="416">Russia</collectingCountry>
