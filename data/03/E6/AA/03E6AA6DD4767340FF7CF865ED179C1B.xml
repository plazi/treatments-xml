<collectingCountry box="[308,492,338,364]" name="New Caledonia" pageId="32" pageNumber="33">New Caledonia</collectingCountry>
, 
<collectingCountry box="[504,567,338,364]" name="India" pageId="32" pageNumber="33">India</collectingCountry>
, 
<collectingCountry box="[579,689,338,364]" name="Australia" pageId="32" pageNumber="33">Australia</collectingCountry>
, New 
<collectingCountry box="[763,851,338,364]" name="Guinea-Bissau" pageId="32" pageNumber="33">Guinea</collectingCountry>
.
</paragraph>
