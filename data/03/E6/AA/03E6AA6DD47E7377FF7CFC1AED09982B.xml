<collectingCountry box="[383,500,1346,1372]" name="Sri Lanka" pageId="23" pageNumber="24">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[512,584,1346,1372]" name="Oman" pageId="23" pageNumber="24">Oman</collectingCountry>
(
<bibRefCitation author="Smit" box="[599,823,1344,1372]" pageId="23" pageNumber="24" refString="Smit, H. &amp; Pesic, V. (2010) New species of water mites from Oman, with some zoogeographical notes (Acari: Hydrachnidia). Acarologia, 50 (2), 151 - 195." type="journal article" year="2010">
Smit &amp; Pešić 
<number box="[763,823,1346,1372]" pageId="23" pageNumber="24" value="2010.0">2010</number>
</bibRefCitation>
).
</paragraph>
</subSubSection>
