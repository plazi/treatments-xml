<taxonomicNameLabel box="[617,718,1663,1686]" pageId="8" pageNumber="319" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName authority="Ernst &amp; Jones &amp; Whittington, 2001" authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[800,925,1662,1686]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="8" pageNumber="319" phylum="Platyhelminthes" rank="species" species="shieldsi" status="sp. nov.">
<emphasis box="[800,925,1662,1686]" italics="true" pageId="8" pageNumber="319">A. shieldsi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[945,1047,1663,1686]" pageId="8" pageNumber="319" rank="species">sp. nov.</taxonomicNameLabel>
<taxonomicName authority="Ernst &amp; Jones &amp; Whittington, 2001" authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[92,349,132,156]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="10" pageNumber="321" phylum="Platyhelminthes" rank="species" species="sigani" status="sp. nov.">
<emphasis box="[92,349,132,156]" italics="true" pageId="10" pageNumber="321">Acanthoplacatus sigani</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[356,447,133,156]" pageId="10" pageNumber="321" rank="species">sp. nov.</taxonomicNameLabel>
can be distinguished from 
<taxonomicName authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[760,871,132,156]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="10" pageNumber="321" phylum="Platyhelminthes" rank="species" species="adlardi">
<emphasis box="[760,871,132,156]" italics="true" pageId="10" pageNumber="321">A. adlardi</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[932,1045,132,156]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="10" pageNumber="321" phylum="Platyhelminthes" rank="species" species="shieldsi">
