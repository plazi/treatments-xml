<taxonomicName authority="Ernst &amp; Jones &amp; Whittington, 2001" authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[718,815,196,220]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="10" pageNumber="321" phylum="Platyhelminthes" rank="species" species="sigani" status="sp. nov.">
<emphasis box="[718,815,196,220]" italics="true" pageId="10" pageNumber="321">A. sigani</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[823,912,197,220]" pageId="10" pageNumber="321" rank="species">sp. nov.</taxonomicNameLabel>
is 
<taxonomicName authority="Ernst &amp; Jones &amp; Whittington, 2001" authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[943,1045,196,220]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="10" pageNumber="321" phylum="Platyhelminthes" rank="species" species="brauni" status="sp. nov.">
<emphasis box="[943,1045,196,220]" italics="true" pageId="10" pageNumber="321">A. brauni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[92,185,228,251]" pageId="10" pageNumber="321" rank="species">sp. nov.</taxonomicNameLabel>
<taxonomicName authority="Ernst &amp; Jones &amp; Whittington, 2001" authorityName="Ernst &amp; Jones &amp; Whittington" authorityYear="2001" box="[196,456,227,251]" class="Monogenea" family="Gyrodactylidae" genus="Acanthoplacatus" kingdom="Animalia" order="Gyrodactylidea" pageId="10" pageNumber="321" phylum="Platyhelminthes" rank="species" species="sigani" status="sp. nov.">
<emphasis box="[196,456,227,251]" italics="true" pageId="10" pageNumber="321">Acanthoplacatus sigani</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[467,560,228,251]" pageId="10" pageNumber="321" rank="species">sp. nov.</taxonomicNameLabel>
