<geoCoordinate box="[551,604,419,443]" degrees="5" direction="north" minutes="7" orientation="latitude" pageId="18" pageNumber="329" precision="925" value="5.116667">
<emphasis box="[551,564,420,443]" italics="true" pageId="18" pageNumber="329">n</emphasis>
