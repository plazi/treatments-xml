<emphasis box="[189,202,324,347]" italics="true" pageId="18" pageNumber="329">n</emphasis>
5 7
</geoCoordinate>
) long, 32.5 (22±50, 
<geoCoordinate box="[485,538,324,348]" degrees="5" direction="north" minutes="7" orientation="latitude" pageId="18" pageNumber="329" precision="925" value="5.116667">
<emphasis box="[485,498,324,347]" italics="true" pageId="18" pageNumber="329">n</emphasis>
5 7
</geoCoordinate>
) wide, contained within 
<collectionCode box="[833,908,323,347]" pageId="18" pageNumber="329">ECFR</collectionCode>
. Testis 17.5 (9±23, 
<geoCoordinate box="[174,227,355,380]" degrees="5" direction="north" minutes="8" orientation="latitude" pageId="18" pageNumber="329" precision="925" value="5.133333">
<emphasis box="[174,187,356,379]" italics="true" pageId="18" pageNumber="329">n</emphasis>
5 8
</geoCoordinate>
) long, 27 (21±34, 
<geoCoordinate box="[446,499,355,380]" degrees="5" direction="north" minutes="8" orientation="latitude" pageId="18" pageNumber="329" precision="925" value="5.133333">
<emphasis box="[446,459,356,379]" italics="true" pageId="18" pageNumber="329">n</emphasis>
5 8
</geoCoordinate>
