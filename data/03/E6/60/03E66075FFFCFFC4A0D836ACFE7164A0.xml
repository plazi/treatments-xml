<geoCoordinate box="[384,450,479,504]" degrees="5" direction="north" minutes="15" orientation="latitude" pageId="12" pageNumber="323" precision="925" value="5.25">
<emphasis box="[384,397,480,503]" italics="true" pageId="12" pageNumber="323">n</emphasis>
