<subSubSection lastPageId="10" lastPageNumber="275" pageId="9" pageNumber="274" type="nomenclature">
<paragraph blockId="9.[151,673,1353,1413]" box="[151,673,1353,1379]" pageId="9" pageNumber="274">
<heading bold="true" box="[151,673,1353,1379]" fontSize="11" level="1" pageId="9" pageNumber="274" reason="1">
<emphasis bold="true" box="[151,673,1353,1379]" pageId="9" pageNumber="274">
<taxonomicName authority="Zhang" authorityName="Zhang" box="[151,566,1353,1379]" class="Insecta" family="Crambidae" genus="Thliptoceras" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="274" phylum="Arthropoda" rank="species" species="filamentosum" status="sp. nov.">
