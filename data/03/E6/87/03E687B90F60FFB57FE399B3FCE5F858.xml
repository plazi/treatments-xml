pseudospurs; hind ta 
<number box="[839,847,816,830]" pageId="4" pageNumber="431" value="1.0">1</number>
with 
<number box="[910,924,802,826]" pageId="4" pageNumber="431" value="2.0">2</number>
pseudospurs, ta 
<number box="[1102,1110,816,830]" pageId="4" pageNumber="431" value="2.0">2</number>
with 
<number box="[1173,1187,802,826]" pageId="4" pageNumber="431" value="1.0">1</number>
