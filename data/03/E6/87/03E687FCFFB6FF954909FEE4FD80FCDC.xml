<quantity box="[443,570,350,374]" metricMagnitude="-2" metricUnit="m" metricValue="3.95" metricValueMax="4.1" metricValueMin="3.8" pageId="22" pageNumber="887" unit="mm" value="39.5" valueMax="41.0" valueMin="38.0">
38 
<emphasis box="[471,484,350,374]" italics="true" pageId="22" pageNumber="887">–</emphasis>
41 mm
</quantity>
[mean (
<emphasis box="[679,693,350,374]" italics="true" pageId="22" pageNumber="887">n</emphasis>
= 2) 
<quantity box="[775,897,350,374]" metricMagnitude="-2" metricUnit="m" metricValue="3.94" pageId="22" pageNumber="887" unit="mm" value="39.4">39.40 mm</quantity>
, SD = 1.154]; female 
<quantity box="[192,314,385,409]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" metricValueMax="4.7" metricValueMin="4.3" pageId="22" pageNumber="887" unit="mm" value="45.0" valueMax="47.0" valueMin="43.0">
43 
<emphasis box="[219,232,385,409]" italics="true" pageId="22" pageNumber="887">–</emphasis>
47 mm
</quantity>
[mean (
<emphasis box="[413,427,385,409]" italics="true" pageId="22" pageNumber="887">n</emphasis>
= 2) 
<quantity box="[494,611,385,409]" metricMagnitude="-2" metricUnit="m" metricValue="4.51" pageId="22" pageNumber="887" unit="mm" value="45.1">45.10 mm</quantity>
, SD = 1.310]. 
<bibRefCitation author="van Someren VGL" box="[774,997,385,409]" pageId="22" pageNumber="887" pagination="277 - 316" refId="ref24058" refString="van Someren VGL. 1967. Revisional notes on African Charaxes (Lepidoptera: Nymphalidae). Part IV. Bull Br Mus (Nat Hist) Entomol. 18: 277 - 316, 9 pls." type="book chapter" year="1967">van Someren (1967</bibRefCitation>
, p. 309) gave male forewing length as 
<quantity box="[479,562,420,444]" metricMagnitude="-2" metricUnit="m" metricValue="3.8" pageId="22" pageNumber="887" unit="mm" value="38.0">38 mm</quantity>
, female 
<quantity box="[658,741,420,444]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" pageId="22" pageNumber="887" unit="mm" value="45.0">45 mm</quantity>
