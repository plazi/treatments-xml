</taxonomicName>
(
<collectingCountry box="[1305,1382,1272,1298]" name="Japan" pageId="7" pageNumber="8">Japan</collectingCountry>
), 
<taxonomicName authority="Khodkina, 1973" authorityName="Khodkina" authorityYear="1973" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="palmatus">
