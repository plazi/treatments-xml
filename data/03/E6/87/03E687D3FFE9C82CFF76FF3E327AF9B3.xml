FIGURES 
<number box="[277,319,1972,1998]" pageId="2" pageNumber="41" value="6.0" valueMax="7.0" valueMin="5.0">5–7</number>
.
</emphasis>
<taxonomicName box="[332,708,1974,1997]" class="Insecta" family="Cerambycidae" genus="Parandra" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="41" phylum="Arthropoda" rank="species" species="antioquensis" status="new species" subGenus="Parandra">
