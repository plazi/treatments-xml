<typeStatus box="[151,201,1659,1680]" pageId="7" pageNumber="8">Type</typeStatus>
species: 
<taxonomicName authority="(Haan, 1842)" baseAuthorityName="Haan" baseAuthorityYear="1842" box="[294,736,1658,1680]" class="Insecta" family="Haglotettigoniidae" genus="Cardiodactylus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="novaeguineae">
