<paragraph blockId="2.[175,661,944,966]" box="[175,661,944,966]" pageId="2" pageNumber="201">
<heading box="[175,661,944,966]" centered="true" fontSize="9" level="2" pageId="2" pageNumber="201" reason="2">
<taxonomicName authority="Lacordaire 1869: 174" authorityName="Lacordaire" authorityPageNumber="174" authorityYear="1869" box="[175,657,944,966]" class="Insecta" family="Cerambycidae" genus="Phoenicus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="201" phylum="Arthropoda" rank="species" species="sanguinipennis">
<emphasis box="[175,428,944,965]" italics="true" pageId="2" pageNumber="201">Phoenicus sanguinipennis</emphasis>
Lacordaire 1869: 174
</taxonomicName>
