<subSubSection pageId="48" pageNumber="49" type="diagnosis">
<paragraph blockId="48.[151,1437,151,428]" pageId="48" pageNumber="49">
<emphasis bold="true" box="[199,321,367,392]" inLineHeading="true" pageId="48" pageNumber="49" reason="1">Diagnosis.</emphasis>
<taxonomicName authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[330,493,368,392]" class="Insecta" family="Derbidae" genus="Levu" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="psittacus">
