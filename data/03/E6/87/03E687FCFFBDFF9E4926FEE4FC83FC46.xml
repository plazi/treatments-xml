<quantity box="[282,365,385,409]" metricMagnitude="-2" metricUnit="m" metricValue="4.9" pageId="29" pageNumber="894" unit="mm" value="49.0">49 mm</quantity>
; female 
<quantity box="[462,542,385,409]" metricMagnitude="-2" metricUnit="m" metricValue="5.5" pageId="29" pageNumber="894" unit="mm" value="55.0">55 mm</quantity>
(
<bibRefCitation author="van Someren VGL" box="[557,772,385,409]" pageId="29" pageNumber="894" pagination="75 - 166" refId="ref24096" refString="van Someren VGL. 1969. Revisional notes on African Charaxes (Lepidoptera: Nymphalidae). Part V. Bull Br Mus (Nat Hist) Entomol. 23: 75 - 166, 29 pls." type="book chapter" year="1969">van Someren 1969</bibRefCitation>
