(van Someren, 1936) from 
<collectingCountry box="[610,681,1711,1735]" name="Kenya" pageId="20" pageNumber="885">Kenya</collectingCountry>
. 
<materialsCitation ID-GBIF-Occurrence="2832026342" collectionCode="OUMNH" country="Tanzania" location="Kilimanjaro area" municipality="There" pageId="20" pageNumber="885" specimenCount="1">
