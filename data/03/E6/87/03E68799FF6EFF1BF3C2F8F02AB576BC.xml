(Muir)
</taxonomicName>
from 
<collectingCountry box="[1107,1189,548,572]" name="Taiwan" pageId="142" pageNumber="143">Taiwan</collectingCountry>
and 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1247,1395,548,572]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="142" pageNumber="143" phylum="Arthropoda" rank="species" species="chinensis" status="sp. nov.">
<emphasis box="[1247,1395,548,572]" italics="true" pageId="142" pageNumber="143">Sa. chinensis</emphasis>
</taxonomicName>
<taxonomicNameLabel pageId="142" pageNumber="143" rank="species">sp. nov.</taxonomicNameLabel>
