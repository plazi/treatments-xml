resembles 
<taxonomicName authority="(Muir)" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Muir" baseAuthorityYear="1915" box="[769,1035,548,572]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="142" pageNumber="143" phylum="Arthropoda" rank="species" species="matsumurae">
