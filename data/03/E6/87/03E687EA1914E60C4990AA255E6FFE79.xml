<geoCoordinate box="[1155,1268,219,245]" degrees="25" direction="east" minutes="0" orientation="longitude" pageId="3" pageNumber="767" precision="15" seconds="43" value="25.011944">25°0ʹ43ʺE</geoCoordinate>
