<location LSID="urn:lsid:plazi:treatment:03E687EA1914E60C4990AA255E6FFE79:8E9060271916E60C4A15AC8558D5FF59" box="[835,989,221,245]" country="Taiwan" county="In" latitude="121.46555" longLatPrecision="17" longitude="25.011944" municipality="Subsequently" name="Longdong bay" pageId="3" pageNumber="767">Longdong bay</location>
(
<geoCoordinate box="[1003,1145,219,245]" degrees="121" direction="north" minutes="27" orientation="latitude" pageId="3" pageNumber="767" precision="15" seconds="56" value="121.46555">121°27ʹ56ʺN</geoCoordinate>
