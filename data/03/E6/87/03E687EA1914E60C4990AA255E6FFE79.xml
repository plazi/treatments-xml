<elevation box="[1313,1440,189,213]" metricMagnitude="1" metricUnit="m" metricValue="1.2" pageId="3" pageNumber="767" unit="m" value="12.0">
<quantity box="[1313,1370,189,213]" metricMagnitude="1" metricUnit="m" metricValue="1.2" pageId="3" pageNumber="767" unit="m" value="12.0">12 m</quantity>
