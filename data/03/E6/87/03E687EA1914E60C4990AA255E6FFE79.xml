<location LSID="urn:lsid:plazi:treatment:03E687EA1914E60C4990AA255E6FFE79:8E9060271916E60C4A15ACA55856FEB9" box="[835,862,253,277]" country="Taiwan" county="In" latitude="121.46555" longLatPrecision="17" longitude="25.011944" municipality="Subsequently" name="At" pageId="3" pageNumber="767">At</location>
