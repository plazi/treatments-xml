</taxonomicName>
resembles in structure and coloration 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[983,1094,1590,1614]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="crispa" status="sp. nov.">
<emphasis box="[983,1094,1590,1614]" italics="true" pageId="160" pageNumber="161">Sa. crispa</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1101,1187,1591,1614]" pageId="160" pageNumber="161" rank="species">sp. nov.</taxonomicNameLabel>
(also from 
<collectingCountry name="New Caledonia" pageId="160" pageNumber="161">New Caledonia</collectingCountry>
), 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[236,344,1627,1651]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="joycei" status="sp. nov.">
<emphasis box="[236,344,1627,1651]" italics="true" pageId="160" pageNumber="161">Sa. joycei</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[351,438,1627,1650]" pageId="160" pageNumber="161" rank="species">sp. nov.</taxonomicNameLabel>
(from 
<collectingCountry box="[514,557,1626,1650]" name="Fiji" pageId="160" pageNumber="161">Fiji</collectingCountry>
), and 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[623,729,1627,1651]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="doddi" status="sp. nov.">
<emphasis box="[623,729,1627,1651]" italics="true" pageId="160" pageNumber="161">Sa. doddi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[735,822,1627,1650]" pageId="160" pageNumber="161" rank="species">sp. nov.</taxonomicNameLabel>
(from 
<collectingCountry box="[898,1006,1626,1650]" name="Australia" pageId="160" pageNumber="161">Australia</collectingCountry>
