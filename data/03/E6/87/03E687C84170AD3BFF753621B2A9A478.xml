(
<taxonomicName authority="HUFN." authorityName="HUFN." box="[592,827,1574,1596]" class="Insecta" family="Noctuidae" genus="Apamea" kingdom="Animalia" order="Lepidoptera" pageId="40" pageNumber="637" phylum="Arthropoda" rank="species" species="crenata">
<emphasis box="[592,759,1574,1596]" italics="true" pageId="40" pageNumber="637">Apamea crenata</emphasis>
HUFN.
</taxonomicName>
, 
<taxonomicName authority="DEN. et SCHIFF." authorityName="DEN. et SCHIFF." class="Insecta" family="Noctuidae" genus="Cerastis" kingdom="Animalia" lastPageId="41" lastPageNumber="638" order="Lepidoptera" pageId="40" pageNumber="637" phylum="Arthropoda" rank="species" species="rubricosa">
<emphasis box="[841,1030,1574,1596]" italics="true" pageId="40" pageNumber="637">Cerastis rubricosa</emphasis>
DEN. et SCHIFF.
</taxonomicName>
, 
<taxonomicName authority="DEN. et SCHIFF." authorityName="DEN. et SCHIFF." box="[287,616,219,241]" class="Insecta" family="Noctuidae" genus="Diarsia" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="638" phylum="Arthropoda" rank="species" species="brunnea">
<emphasis box="[287,456,219,241]" italics="true" pageId="41" pageNumber="638">Diarsia brunnea</emphasis>
DEN. et SCHIFF.
</taxonomicName>
, 
<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[631,786,219,241]" class="Insecta" family="Noctuidae" genus="Plusia" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="638" phylum="Arthropoda" rank="species" species="festucae">
<emphasis box="[631,786,219,241]" italics="true" pageId="41" pageNumber="638">Plusia festucae</emphasis>
</taxonomicName>
L., 
<taxonomicName authority="HUFN." authorityName="HUFN." box="[830,1049,219,241]" class="Insecta" family="Noctuidae" genus="Polia" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="638" phylum="Nemertea" rank="species" species="nebulosa">
<emphasis box="[830,983,219,241]" italics="true" pageId="41" pageNumber="638">Polia nebulosa</emphasis>
HUFN.
