<taxonomicName authority="Zelazny" authorityName="Zelazny" authorityYear="1981" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="157" pageNumber="158" phylum="Arthropoda" rank="species" species="spinosa">
