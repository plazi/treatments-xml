<subSubSection pageId="85" pageNumber="86" type="diagnosis">
<paragraph blockId="85.[151,1436,258,714]" pageId="85" pageNumber="86">
<emphasis bold="true" box="[199,321,618,643]" inLineHeading="true" pageId="85" pageNumber="86" reason="1">Diagnosis.</emphasis>
<taxonomicName authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,497,619,643]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="aequa">
<emphasis box="[328,497,619,643]" italics="true" pageId="85" pageNumber="86">Rhotana aequa</emphasis>
</taxonomicName>
resembles closely 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[710,812,619,643]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="dextra" status="sp. nov.">
