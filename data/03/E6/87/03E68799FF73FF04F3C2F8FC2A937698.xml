<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[330,631,584,608]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="vietnamensis" status="sp. nov.">
<emphasis box="[330,631,584,608]" italics="true" pageId="145" pageNumber="146">Saccharodite vietnamensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[639,728,584,607]" pageId="145" pageNumber="146" rank="species">sp. nov.</taxonomicNameLabel>
show similarities 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[938,1053,584,608]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="timida" status="sp. nov.">
<emphasis box="[938,1053,584,608]" italics="true" pageId="145" pageNumber="146">Sa. timida</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1063,1154,584,607]" pageId="145" pageNumber="146" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1167,1288,584,608]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="aurora" status="sp. nov.">
<emphasis box="[1167,1288,584,608]" italics="true" pageId="145" pageNumber="146">Sa. aurora</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1297,1388,584,607]" pageId="145" pageNumber="146" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="propinqua" status="sp. nov.">
<emphasis italics="true" pageId="145" pageNumber="146">Sa. propinqua</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[274,363,620,643]" pageId="145" pageNumber="146" rank="species">sp. nov.</taxonomicNameLabel>
, as well as 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[493,642,620,644]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="perlucida" status="sp. nov.">
<emphasis box="[493,642,620,644]" italics="true" pageId="145" pageNumber="146">Sa. perlucida</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[650,739,620,643]" pageId="145" pageNumber="146" rank="species">sp. nov.</taxonomicNameLabel>
