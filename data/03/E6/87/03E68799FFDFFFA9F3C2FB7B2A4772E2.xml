<subSubSection pageId="60" pageNumber="61" type="description">
<paragraph blockId="60.[151,566,1158,1217]" box="[151,315,1193,1217]" pageId="60" pageNumber="61">
(
<figureCitation box="[159,253,1193,1217]" captionStart="FIGURE 24" captionStartId="197.[151,244,931,952]" captionTargetBox="[156,1433,189,908]" captionTargetId="figure-59@197.[151,1436,191,909]" captionTargetPageId="197" captionText="FIGURE 24. Muiralevu telum, holotype in BMNH, dorsal view." pageId="60" pageNumber="61">Figs. 24</figureCitation>
, 
<figureCitation box="[264,307,1193,1217]" captionStart="FIGURE 189" captionStartId="244.[151,244,627,648]" captionTargetBox="[184,1402,207,594]" captionTargetId="figure-44@244.[178,1410,191,605]" captionTargetPageId="244" captionText="FIGURE 189. Muiralevu telum, holotype ♂." pageId="60" pageNumber="61">189</figureCitation>
