<taxonomicName authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[330,539,152,176]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="brevis">
<emphasis box="[330,539,152,176]" italics="true" pageId="32" pageNumber="33">Dichotropis brevis</emphasis>
</taxonomicName>
is indistinguishable externally from 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[962,1060,152,176]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="nivea" status="sp. nov.">
<emphasis box="[962,1060,152,176]" italics="true" pageId="32" pageNumber="33">D. nivea</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1069,1160,152,175]" pageId="32" pageNumber="33" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1173,1331,152,176]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="straatmani" status="sp. nov.">
<emphasis box="[1173,1331,152,176]" italics="true" pageId="32" pageNumber="33">D. straatmani</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1340,1432,152,175]" pageId="32" pageNumber="33" rank="species">sp. nov.</taxonomicNameLabel>
, and 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[198,341,189,212]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="peregrina" status="sp. nov.">
<emphasis box="[198,341,189,212]" italics="true" pageId="32" pageNumber="33">D. peregrina</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[348,436,188,211]" pageId="32" pageNumber="33" rank="species">sp. nov.</taxonomicNameLabel>
, all three also found in New 
<collectingCountry box="[755,839,188,212]" name="Guinea-Bissau" pageId="32" pageNumber="33">Guinea</collectingCountry>
