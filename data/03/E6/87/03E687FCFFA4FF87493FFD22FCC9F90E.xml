<taxonomicName authorityName="Kluk" authorityYear="1780" box="[160,243,947,971]" class="Insecta" family="Nymphalidae" genus="Danaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="869" phylum="Arthropoda" rank="genus">
