<taxonomicName authority="(Fennah)" authorityName="Zelazny &amp; Webb" authorityYear="2011" baseAuthorityName="Fennah" baseAuthorityYear="1956" class="Insecta" family="Derbidae" genus="Levu" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="haedulus">
