<specimenCode box="[563,732,1789,1813]" collectionCode="BPBM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="142" pageNumber="143" type="Museum">BPBM # 5208</specimenCode>
