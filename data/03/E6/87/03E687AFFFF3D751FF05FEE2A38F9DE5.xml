<typeStatus box="[977,1025,722,746]" pageId="9" pageNumber="444">type</typeStatus>
locality, Parque Municipal da Lagoa do 
<taxonomicName box="[186,234,757,782]" class="Arachnida" family="Nemesiidae" genus="Stenoterommata" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="454" phylum="Arthropoda" rank="species" species="peri" status="sp. nov.">Peri</taxonomicName>
, 
<collectingRegion box="[245,409,757,782]" country="Brazil" name="Santa Catarina" pageId="9" pageNumber="444">Santa Catarina</collectingRegion>
<collectingCountry box="[416,487,757,782]" name="Iceland" pageId="9" pageNumber="444">Island</collectingCountry>
, Florianópolis, 
<collectingRegion box="[662,829,757,782]" country="Brazil" name="Santa Catarina" pageId="9" pageNumber="444">Santa Catarina</collectingRegion>
, 
<collectingCountry box="[840,912,757,782]" name="Brazil" pageId="9" pageNumber="444">Brazil</collectingCountry>
