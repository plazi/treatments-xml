<quantity box="[479,602,1320,1345]" metricMagnitude="-2" metricUnit="m" metricValue="4.1" metricValueMax="4.2" metricValueMin="4.0" pageId="26" pageNumber="891" unit="mm" value="41.0" valueMax="42.0" valueMin="40.0">
