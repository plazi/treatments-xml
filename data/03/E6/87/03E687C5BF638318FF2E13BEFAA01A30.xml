<typeStatus box="[412,515,152,176]" pageId="22" pageNumber="23">holotype</typeStatus>
</emphasis>
: height 
<quantity box="[608,711,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="2.28" pageId="22" pageNumber="23" unit="mm" value="2.28">2.28 mm</quantity>
, width 
<quantity box="[793,896,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="1.45" pageId="22" pageNumber="23" unit="mm" value="1.45">1.45 mm</quantity>
