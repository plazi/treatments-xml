<quantity box="[425,546,662,686]" metricMagnitude="-2" metricUnit="m" metricValue="3.45" metricValueMax="3.8" metricValueMin="3.1" pageId="7" pageNumber="872" unit="mm" value="34.5" valueMax="38.0" valueMin="31.0">
31 
<emphasis box="[453,466,662,686]" italics="true" pageId="7" pageNumber="872">–</emphasis>
38 mm
</quantity>
[mean (
<emphasis box="[643,657,662,686]" italics="true" pageId="7" pageNumber="872">n</emphasis>
= 13) 
<quantity box="[735,851,662,686]" metricMagnitude="-2" metricUnit="m" metricValue="3.475" pageId="7" pageNumber="872" unit="mm" value="34.75">34.75 mm</quantity>
, SD = 1.689]; female 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="3.775" metricValueMax="4.0" metricValueMin="3.55" pageId="7" pageNumber="872" unit="mm" value="37.75" valueMax="40.0" valueMin="35.5">
