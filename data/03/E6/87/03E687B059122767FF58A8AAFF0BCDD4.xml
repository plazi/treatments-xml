<quantity box="[1268,1379,1956,1982]" metricMagnitude="-1" metricUnit="m" metricValue="5.842" metricValueMax="6.096" metricValueMin="5.588" pageId="21" pageNumber="22" unit="in" value="23.0" valueMax="24.0" valueMin="22.0">22–24 in</quantity>
