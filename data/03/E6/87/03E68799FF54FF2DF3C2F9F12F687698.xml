<subSubSection pageId="184" pageNumber="185" type="distribution">
<paragraph blockId="184.[151,1436,151,679]" pageId="184" pageNumber="185">
<emphasis box="[199,352,439,464]" inLineHeading="true" pageId="184" pageNumber="185" reason="1">
<emphasis bold="true" box="[199,345,439,464]" pageId="184" pageNumber="185">Distribution</emphasis>
.
</emphasis>
<collectingCountry box="[359,472,439,463]" name="Sri Lanka" pageId="184" pageNumber="185">Sri Lanka</collectingCountry>
. 
<bibRefCitation author="Muir" box="[483,614,439,463]" firstAuthor="Muir" pageId="184" pageNumber="185" pagination="335 - 342" refId="ref202422" refString="Muir, F. (1922) On some Indian Derbidae (Homoptera). Records of the Indian Museum, 24, 335 - 342." type="journal article" year="1922">Muir (1922</bibRefCitation>
