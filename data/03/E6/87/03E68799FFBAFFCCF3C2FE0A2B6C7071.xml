<subSubSection pageId="89" pageNumber="90" type="diagnosis">
<paragraph blockId="89.[151,1436,610,1103]" pageId="89" pageNumber="90">
<emphasis bold="true" box="[199,321,1042,1067]" inLineHeading="true" pageId="89" pageNumber="90" reason="1">Diagnosis.</emphasis>
<taxonomicName authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,516,1043,1067]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis box="[328,516,1043,1067]" italics="true" pageId="89" pageNumber="90">Rhotana gracilis</emphasis>
</taxonomicName>
resembles closely 
<taxonomicName authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[728,830,1043,1067]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="dextra" status="sp. nov.">
