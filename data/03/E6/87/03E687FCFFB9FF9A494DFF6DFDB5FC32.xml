<quantity box="[301,420,264,288]" metricMagnitude="-2" metricUnit="m" metricValue="4.287" pageId="25" pageNumber="890" unit="mm" value="42.87">42.87 mm</quantity>
, SD = 1.646]; female 
<quantity box="[680,842,264,288]" metricMagnitude="-2" metricUnit="m" metricValue="4.9" metricValueMax="5.25" metricValueMin="4.55" pageId="25" pageNumber="890" unit="mm" value="49.0" valueMax="52.5" valueMin="45.5">
