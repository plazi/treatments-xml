<quantity box="[942,1069,230,254]" metricMagnitude="-2" metricUnit="m" metricValue="4.25" metricValueMax="4.6" metricValueMin="3.9" pageId="25" pageNumber="890" unit="mm" value="42.5" valueMax="46.0" valueMin="39.0">
