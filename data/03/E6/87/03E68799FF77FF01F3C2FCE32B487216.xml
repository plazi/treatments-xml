<subSubSection box="[151,609,798,824]" pageId="148" pageNumber="149" type="nomenclature">
<paragraph blockId="148.[151,609,798,857]" box="[151,609,798,824]" pageId="148" pageNumber="149">
<heading bold="true" box="[151,609,798,824]" fontSize="11" level="1" pageId="148" pageNumber="149" reason="1">
<emphasis bold="true" box="[151,609,798,824]" pageId="148" pageNumber="149">
<taxonomicName authority="Zelazny" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[151,503,798,824]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="148" pageNumber="149" phylum="Arthropoda" rank="species" species="carens" status="sp. nov.">
<emphasis bold="true" box="[151,396,798,824]" italics="true" pageId="148" pageNumber="149">Saccharodite carens</emphasis>
Zelazny
</taxonomicName>
, 
<taxonomicNameLabel box="[516,609,798,824]" pageId="148" pageNumber="149" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
