; head 
<number box="[1153,1186,1195,1221]" pageId="17" pageNumber="18" value="54.0">54</number>
; trunk 
<number box="[1273,1321,1195,1221]" pageId="17" pageNumber="18" value="252.0">252</number>
; tail 
<number box="[1384,1431,1195,1221]" pageId="17" pageNumber="18" value="363.0">363</number>
; predorsal distance 
<number box="[369,416,1233,1259]" pageId="17" pageNumber="18" value="101.0">101</number>
; pectoral-fin length 
<number box="[651,706,1233,1259]" pageId="17" pageNumber="18" value="13.4">13.4</number>
; pectoral-fin base 
<number box="[920,960,1233,1259]" pageId="17" pageNumber="18" value="3.8">3.8</number>
; body depth at gill openings ~ 
<number box="[1311,1365,1233,1259]" pageId="17" pageNumber="18" value="16.5">16.5</number>
; body width at gill openings ~ 
<number box="[432,463,1270,1296]" pageId="17" pageNumber="18" value="15.0">15</number>
; body depth at anus ~ 
<number box="[723,754,1270,1296]" pageId="17" pageNumber="18" value="16.0">16</number>
; body width at anus ~ 
<number box="[1016,1047,1270,1296]" pageId="17" pageNumber="18" value="15.0">15</number>
; snout 
<number box="[1131,1186,1270,1296]" pageId="17" pageNumber="18" value="10.7">10.7</number>
; tip of snout to rictus 
<number box="[151,206,1307,1333]" pageId="17" pageNumber="18" value="20.4">20.4</number>
; eye diameter 5.0; interorbital distance 
<number box="[668,708,1307,1333]" pageId="17" pageNumber="18" value="7.5">7.5</number>
; gill-opening height 
<number box="[952,992,1307,1333]" pageId="17" pageNumber="18" value="6.2">6.2</number>
; isthmus width 
<number box="[1177,1217,1307,1333]" pageId="17" pageNumber="18" value="7.5">7.5</number>
. Vertebral formula 
<number box="[151,179,1345,1371]" pageId="17" pageNumber="18" value="19.0">19</number>
/ 
