<taxonomicName box="[460,580,1780,1801]" class="Bacillariophyceae" family="Gomphonemataceae" genus="Gomphoneis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Cymbellales" pageId="15" pageNumber="16" phylum="Ochrophyta" rank="species" species="stoermeri">
