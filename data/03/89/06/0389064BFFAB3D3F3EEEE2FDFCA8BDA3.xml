<taxonomicName authority="d'Orbigny 1839" authorityName="d'Orbigny" authorityYear="1839" box="[330,646,510,535]" class="Globothalamea" family="Discorbidae" genus="Rotorbis" kingdom="Chromista" order="Rotaliida" pageId="81" pageNumber="82" phylum="Foraminifera" rank="species" species="auberii">
