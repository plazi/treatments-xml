<date pageId="85" pageNumber="86" value="1984-06-04">4–6. 1984</date>
<taxonomicName authority="Said" authorityName="Said" box="[208,531,1904,1926]" class="Globothalamea" family="Anomalinidae" genus="Epistomaroides" kingdom="Chromista" order="Rotaliida" pageId="85" pageNumber="86" phylum="Foraminifera" rank="species" species="punctata">
