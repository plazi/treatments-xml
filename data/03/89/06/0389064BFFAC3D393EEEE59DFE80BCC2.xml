<quantity box="[897,1027,1589,1613]" metricMagnitude="2" metricUnit="m" metricValue="1.775" metricValueMax="1.8199999999999998" metricValueMin="1.73" pageId="87" pageNumber="88" unit="m" value="177.5" valueMax="182.0" valueMin="173.0">
