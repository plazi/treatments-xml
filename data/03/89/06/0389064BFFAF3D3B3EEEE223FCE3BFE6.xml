<taxonomicName box="[151,293,358,383]" class="Globothalamea" family="Anomalinidae" genus="Cibicidoides" kingdom="Chromista" order="Rotaliida" pageId="85" pageNumber="86" phylum="Foraminifera" rank="genus">
<emphasis bold="true" box="[151,293,358,383]" italics="true" pageId="85" pageNumber="86">Cibicidoides</emphasis>
</taxonomicName>
sp. cf. 
<taxonomicName authority="McCulloch 1977" authorityName="McCulloch" authorityYear="1977" box="[374,744,358,383]" class="Globothalamea" family="Anomalinidae" genus="Cibicidoides" kingdom="Chromista" order="Rotaliida" pageId="85" pageNumber="86" phylum="Foraminifera" rank="species" species="basilanensis">
<emphasis bold="true" box="[374,548,358,383]" pageId="85" pageNumber="86">
<emphasis bold="true" box="[374,393,358,383]" italics="true" pageId="85" pageNumber="86">C</emphasis>
. 
