<emphasis bold="true" box="[199,354,619,644]" pageId="2" pageNumber="49">
<typeStatus box="[199,258,620,644]" pageId="2" pageNumber="49">Type</typeStatus>
species.
</emphasis>
<taxonomicName box="[361,583,620,644]" class="Insecta" family="Baetidae" genus="Parakari" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="49" phylum="Arthropoda" rank="species" species="churiensis" status="n. sp.">
<emphasis box="[361,583,620,644]" italics="true" pageId="2" pageNumber="49">Parakari churiensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[591,654,620,644]" pageId="2" pageNumber="49">
<taxonomicNameLabel box="[591,654,620,644]" pageId="2" pageNumber="49" rank="species">n. sp.</taxonomicNameLabel>
