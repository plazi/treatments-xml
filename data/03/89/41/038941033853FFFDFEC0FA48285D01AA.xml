<taxonomicName baseAuthorityName="Valenciennes" baseAuthorityYear="1836" box="[625,775,1746,1770]" class="Actinopterygii" family="Anostomidae" genus="Megaleporinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Characiformes" pageId="20" pageNumber="21" phylum="Chordata" rank="species" species="obtusidens">
