<taxonomicName baseAuthorityName="Ihering" baseAuthorityYear="1898" box="[916,995,208,232]" class="Actinopterygii" family="Pimelodidae" genus="Zungaro" kingdom="Animalia" order="Siluriformes" pageId="86" pageNumber="87" phylum="Chordata" rank="species" species="jahu">
