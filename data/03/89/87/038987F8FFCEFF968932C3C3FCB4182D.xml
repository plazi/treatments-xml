<paragraph blockId="7.[189,914,313,375]" box="[189,914,348,375]" pageId="7" pageNumber="8">
LENGTH. Body 
<quantity box="[362,455,349,375]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="7" pageNumber="8" unit="mm" value="2.7">2.7 mm</quantity>
, fore wing 
<quantity box="[587,680,349,375]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="7" pageNumber="8" unit="mm" value="2.7">2.7 mm</quantity>
