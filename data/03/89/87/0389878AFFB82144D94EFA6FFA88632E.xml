<taxonomicName authority="Chao et al. 2010" authorityName="Chao" authorityYear="2010" box="[232,677,1747,1771]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="53" pageNumber="2835" phylum="Firmicutes" rank="species" species="odoratitofui">
