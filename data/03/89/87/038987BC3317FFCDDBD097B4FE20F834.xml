and 
<taxonomicName box="[535,668,1878,1903]" class="Insecta" family="Thripidae" genus="Neohydatothrips" kingdom="Animalia" order="Thysanoptera" pageId="8" pageNumber="193" phylum="Arthropoda" rank="species" species="tabulifer">
