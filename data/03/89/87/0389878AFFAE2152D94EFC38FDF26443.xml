<paragraph blockId="35.[121,760,942,1572]" pageId="35" pageNumber="2817">
<taxonomicName authorityName="Abo-Elnaga &amp; Kandler" authorityYear="1965" box="[121,458,1211,1235]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="35" pageNumber="2817" phylum="Firmicutes" rank="subSpecies" species="coryniformis" subSpecies="torquens">
