<taxonomicName authority="Jung et al. 2017" authorityName="Jung" authorityYear="2017" box="[232,585,1605,1629]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="21" pageNumber="2803" phylum="Firmicutes" rank="species" species="allii">
