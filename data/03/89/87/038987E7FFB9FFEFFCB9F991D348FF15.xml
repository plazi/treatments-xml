<emphasis italics="true" pageId="14" pageNumber="13">
<taxonomicName class="Magnoliopsida" family="Menispermaceae" genus="Cissampelos" kingdom="Plantae" order="Ranunculales" pageId="14" pageNumber="13" phylum="Tracheophyta" rank="species" species="pareira">Cissampelos pareira</taxonomicName>
, 
<taxonomicName box="[299,587,1635,1659]" class="Magnoliopsida" family="Rubiaceae" genus="Psychotria" kingdom="Plantae" order="Gentianales" pageId="14" pageNumber="13" phylum="Tracheophyta" rank="species" species="horizontalis">Psychotria horizontalis</taxonomicName>
</emphasis>
, 3 (F) adultas (
<collectionCode box="[781,861,1635,1659]" pageId="14" pageNumber="13">CZAC</collectionCode>
); Sierra del Infierno; Viñales, 
<date box="[1241,1347,1635,1659]" pageId="14" pageNumber="13" value="1999-05-01">1.v.1999</date>
, col. N. Mestre, 
<taxonomicName box="[290,448,1667,1691]" class="Magnoliopsida" family="Malvaceae" genus="Urena" kingdom="Plantae" order="Malvales" pageId="14" pageNumber="13" phylum="Tracheophyta" rank="species" species="lobata">
<emphasis box="[290,448,1667,1691]" italics="true" pageId="14" pageNumber="13">Urena lobata</emphasis>
</taxonomicName>
, 10 (F) adultas (
<collectionCode box="[640,719,1667,1691]" pageId="14" pageNumber="13">CZAC</collectionCode>
); Viñales, 
<date box="[840,961,1667,1691]" pageId="14" pageNumber="13" value="1969-02-21">21.ii.1969</date>
, col. 
<collectionCode box="[1017,1040,1668,1691]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="14" pageNumber="13">R</collectionCode>
. Alayo, 
<taxonomicName box="[1130,1288,1667,1691]" class="Magnoliopsida" family="Malvaceae" genus="Urena" kingdom="Plantae" order="Malvales" pageId="14" pageNumber="13" phylum="Tracheophyta" rank="species" species="lobata">
