<taxonomicName authority="Torriani et al. 1996" authorityName="Torriani" authorityYear="1996" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="33" pageNumber="2815" phylum="Firmicutes" rank="subSpecies" species="sakei" subSpecies="carnosus">
