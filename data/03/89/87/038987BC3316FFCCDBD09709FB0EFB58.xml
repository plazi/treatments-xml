<collectingCountry box="[344,408,715,738]" name="Japan" pageId="9" pageNumber="194">Japan</collectingCountry>
and recorded from 
<collectingCountry box="[652,737,714,739]" name="Taiwan" pageId="9" pageNumber="194">Taiwan</collectingCountry>
