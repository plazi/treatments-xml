<subSubSection box="[151,1203,581,606]" pageId="2" pageNumber="35" type="materials_examined">
<paragraph blockId="2.[151,1436,581,1973]" box="[151,1203,581,606]" pageId="2" pageNumber="35">
<emphasis bold="true" box="[151,308,581,606]" pageId="2" pageNumber="35">
<typeStatus box="[151,210,581,605]" pageId="2" pageNumber="35">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Tang, Melika &amp; Yang" authorityName="Tang, Melika &amp; Yang" box="[315,833,581,606]" class="Hexapoda" family="Cynipidae" genus="Cycloneuroterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="35" phylum="Arthropoda" rank="species" species="lilungi" status="new species">
<emphasis box="[315,581,581,605]" italics="true" pageId="2" pageNumber="35">Cycloneuroterus lilungi</emphasis>
Tang, Melika &amp; Yang
</taxonomicName>
, 
<taxonomicNameLabel box="[844,983,581,605]" pageId="2" pageNumber="35" rank="species">new species</taxonomicNameLabel>
