<paragraph blockId="38.[826,1469,1153,1654]" pageId="38" pageNumber="2820">
<taxonomicName box="[828,921,1324,1347]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="38" pageNumber="2820" phylum="Firmicutes" rank="species" species="sicerae">
<emphasis box="[828,921,1324,1347]" italics="true" pageId="38" pageNumber="2820">L. sicerae</emphasis>
</taxonomicName>
