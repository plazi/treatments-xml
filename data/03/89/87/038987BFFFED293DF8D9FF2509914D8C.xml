<taxonomicName authority="Ricci, Shiel, Fontaneto &amp; Melone" authorityName="Ricci, Shiel, Fontaneto &amp; Melone" authorityYear="2003" box="[985,1436,544,566]" class="Eurotatoria" family="Philodinavidae" genus="Philodinavus" kingdom="Animalia" order="Bdelloidea" pageId="3" pageNumber="402" phylum="Rotifera" rank="species" species="aussiensis">
