<taxonomicName authority="(Murray)" baseAuthorityName="Murray" baseAuthorityYear="1905" box="[1217,1437,632,654]" class="Eurotatoria" family="Philodinavidae" genus="Philodinavus" kingdom="Animalia" order="Bdelloidea" pageId="3" pageNumber="402" phylum="Rotifera" rank="species" species="paradoxus">
