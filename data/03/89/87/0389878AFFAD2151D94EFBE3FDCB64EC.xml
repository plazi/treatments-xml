<taxonomicName box="[403,508,1375,1398]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="32" pageNumber="2814" phylum="Firmicutes" rank="species" species="saniviri">
