<taxonomicName authority="Liu and Gu 2019" authorityName="Liu and Gu" authorityYear="2019" box="[229,681,1028,1052]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="45" pageNumber="2827" phylum="Firmicutes" rank="species" species="nangangensis">
