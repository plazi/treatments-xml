<taxonomicName box="[538,807,387,411]" class="Magnoliopsida" family="Euphorbiaceae" genus="Jatropha" kingdom="Plantae" order="Malpighiales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="integerrima">
<emphasis box="[538,807,387,411]" italics="true" pageId="8" pageNumber="7">Jatropha integerrima</emphasis>
</taxonomicName>
, 
<taxonomicName box="[824,1042,387,411]" class="Magnoliopsida" family="Myrtaceae" genus="Syzygium" kingdom="Plantae" order="Myrtales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="jambos">
<emphasis box="[824,1042,387,411]" italics="true" pageId="8" pageNumber="7">Syzygium jambos</emphasis>
