<taxonomicName box="[353,639,1689,1711]" class="Hexapoda" family="Cynipidae" genus="Cycloneuroterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="51" phylum="Arthropoda" rank="species" species="fortuitusus" status="new species">
<emphasis box="[353,639,1689,1711]" italics="true" pageId="18" pageNumber="51">Cycloneuroterus fortuitusus</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[656,783,1689,1712]" pageId="18" pageNumber="51">
<taxonomicNameLabel box="[656,783,1689,1712]" pageId="18" pageNumber="51" rank="species">new species</taxonomicNameLabel>
