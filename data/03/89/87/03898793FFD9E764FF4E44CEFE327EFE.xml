<paragraph blockId="15.[151,1437,491,2036]" pageId="15" pageNumber="16">
<emphasis bold="true" box="[151,290,491,516]" pageId="15" pageNumber="16">Dimensions</emphasis>
. 
<typeStatus box="[306,410,492,516]" pageId="15" pageNumber="16">Holotype</typeStatus>
female length 
<number box="[583,625,492,516]" pageId="15" pageNumber="16" value="525.0">525</number>
μm, breadth 
<number box="[781,823,492,516]" pageId="15" pageNumber="16" value="303.0">303</number>
μm. 
<typeStatus box="[885,983,493,517]" pageId="15" pageNumber="16">Paratype</typeStatus>
females lengths 
<number box="[1179,1279,492,516]" pageId="15" pageNumber="16" value="504517.0">504, 517</number>
