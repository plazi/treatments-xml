<taxonomicName authority="Watanabe et al. 2009" authorityName="Watanabe" authorityYear="2009" box="[232,664,340,364]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="sunkii">
