<taxonomicName authority="Watanabe et al. 2009" authorityName="Watanabe" authorityYear="2009" box="[939,1350,922,946]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="64" pageNumber="2846" phylum="Firmicutes" rank="species" species="rapi">
