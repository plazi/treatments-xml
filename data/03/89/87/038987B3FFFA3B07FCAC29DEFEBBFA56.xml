<number box="[433,442,533,547]" pageId="11" pageNumber="39" value="2.0">2</number>
+ 
<number box="[451,458,533,547]" pageId="11" pageNumber="39" value="3.0">3</number>
with basal extremity beyond 
<emphasis box="[802,875,518,543]" italics="true" pageId="11" pageNumber="39">dm-cu</emphasis>
; a stripe–like spot on 
<emphasis box="[1132,1205,518,543]" italics="true" pageId="11" pageNumber="39">dm-cu</emphasis>
, subcostal cell hyaline; costa with 
<number box="[341,355,561,587]" pageId="11" pageNumber="39" value="2.0">2</number>
nd (between R 
<number box="[530,538,577,591]" pageId="11" pageNumber="39" value="1.0">1</number>
and R 
<number box="[618,626,577,591]" pageId="11" pageNumber="39" value="2.0">2</number>
