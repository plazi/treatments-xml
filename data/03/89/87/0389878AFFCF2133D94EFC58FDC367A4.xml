<taxonomicName authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[232,664,1160,1185]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="66" pageNumber="2848" phylum="Firmicutes" rank="species" species="suibinensis">
