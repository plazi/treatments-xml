<materialsCitation ID-GBIF-Occurrence="2609029270" pageId="29" pageNumber="386">
<emphasis bold="true" box="[518,718,624,650]" pageId="29" pageNumber="386">
<collectingRegion box="[518,714,624,650]" country="Canada" name="Nova Scotia" pageId="29" pageNumber="386">NOVA SCOTIA</collectingRegion>
