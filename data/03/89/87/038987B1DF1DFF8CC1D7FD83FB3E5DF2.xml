<emphasis bold="true" box="[151,587,535,561]" pageId="12" pageNumber="13">
Genus 
<taxonomicName authority="Kiefer, 1932" authorityName="Kiefer" authorityYear="1932" box="[238,587,535,561]" class="Maxillopoda" family="Diaptomidae" genus="Arctodiaptomus" kingdom="Animalia" order="Calanoida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
