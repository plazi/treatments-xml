San Vicente, Viñales, 
<date box="[896,1002,1155,1179]" pageId="13" pageNumber="12" value="1999-05-02">2.v.1999</date>
, col. N. Mestre, 
<taxonomicName box="[1203,1367,1155,1179]" class="Magnoliopsida" family="Fabaceae" genus="Delonix" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="12" phylum="Tracheophyta" rank="species" species="regia">
<emphasis box="[1203,1367,1155,1179]" italics="true" pageId="13" pageNumber="12">Delonix regia</emphasis>
</taxonomicName>
, 6 (F) adultas (
<collectionCode box="[296,374,1187,1211]" pageId="13" pageNumber="12">CZAC</collectionCode>
); Soroa, 
<date box="[472,578,1187,1211]" pageId="13" pageNumber="12" value="1968-02-07">7.ii.1968</date>
, col. A. Huba, hospedante desconocido, 1 (F) adulta (
<collectionCode box="[1194,1272,1187,1211]" pageId="13" pageNumber="12">CZAC</collectionCode>
); San Vicente, Viñales, 
<date box="[296,418,1219,1243]" pageId="13" pageNumber="12" value="1969-02-20">20.ii.1969</date>
, col. 
<collectionCode box="[474,497,1220,1243]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="13" pageNumber="12">R</collectionCode>
. Alayo, 
<emphasis box="[588,1120,1219,1243]" italics="true" pageId="13" pageNumber="12">
<taxonomicName box="[588,834,1219,1243]" class="Magnoliopsida" family="Calophyllaceae" genus="Calophyllum" kingdom="Plantae" order="Malpighiales" pageId="13" pageNumber="12" phylum="Tracheophyta" rank="species" species="calaba">Calophyllum calaba</taxonomicName>
, 
<taxonomicName box="[844,1120,1219,1243]" class="Magnoliopsida" family="Fabaceae" genus="Leucaena" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="12" phylum="Tracheophyta" rank="species" species="leucocephala">Leucaena leucocephala</taxonomicName>
</emphasis>
, 
<taxonomicName box="[1133,1303,1220,1243]" class="Magnoliopsida" family="Rubiaceae" genus="Ixora" kingdom="Plantae" order="Gentianales" pageId="13" pageNumber="12" phylum="Tracheophyta" rank="species" species="coccinea">
<emphasis box="[1133,1303,1220,1243]" italics="true" pageId="13" pageNumber="12">Ixora coccinea</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Malvaceae" genus="Corchorus" kingdom="Plantae" order="Malvales" pageId="13" pageNumber="12" phylum="Tracheophyta" rank="species" species="siliquosus">
<emphasis italics="true" pageId="13" pageNumber="12">Corchorus siliquosus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[328,618,1251,1275]" class="Magnoliopsida" family="Acanthaceae" genus="Thunbergia" kingdom="Plantae" order="Lamiales" pageId="13" pageNumber="12" phylum="Tracheophyta" rank="species" species="grandiflora">
<emphasis box="[328,618,1251,1275]" italics="true" pageId="13" pageNumber="12">Thunbergia grandiflora</emphasis>
</taxonomicName>
, 5 (F) adultas (
<collectionCode box="[797,877,1251,1275]" pageId="13" pageNumber="12">CZAC</collectionCode>
