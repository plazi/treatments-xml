<taxonomicName authority="Watanabe et al. 2009" authorityName="Watanabe" authorityYear="2009" box="[936,1400,495,519]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="63" pageNumber="2845" phylum="Firmicutes" rank="species" species="kisonensis">
