<number box="[873,915,1711,1733]" pageId="3" pageNumber="566" value="5.5" valueMax="6.0" valueMin="5.0">5−6</number>
); 
<taxonomicName box="[940,1265,1711,1733]" class="Insecta" family="Staphylinidae" genus="Pseudocephennium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="566" phylum="Arthropoda" rank="species" species="araguanum">
<emphasis box="[940,1265,1711,1733]" italics="true" pageId="3" pageNumber="566">Pseudocephennium araguanum</emphasis>
</taxonomicName>
, 
<typeStatus box="[1283,1374,1711,1733]" pageId="3" pageNumber="566">holotype</typeStatus>
