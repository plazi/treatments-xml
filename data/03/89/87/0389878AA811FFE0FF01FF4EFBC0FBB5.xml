<paragraph blockId="9.[151,1437,429,1129]" pageId="9" pageNumber="68">
<emphasis bold="true" box="[151,269,429,454]" pageId="9" pageNumber="68">
<typeStatus box="[151,210,429,454]" pageId="9" pageNumber="68">Type</typeStatus>
