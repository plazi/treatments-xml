<taxonomicName authority="Endo et al. 2010" authorityName="Endo" authorityYear="2010" box="[232,625,1251,1275]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="59" pageNumber="2841" phylum="Firmicutes" rank="species" species="florum">
