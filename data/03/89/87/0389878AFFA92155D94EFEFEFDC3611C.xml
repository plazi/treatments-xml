<taxonomicName authority="Long and Gu 2019" authorityName="Long and Gu" authorityYear="2019" box="[230,680,546,571]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="36" pageNumber="2818" phylum="Firmicutes" rank="species" species="jiayinensis">
