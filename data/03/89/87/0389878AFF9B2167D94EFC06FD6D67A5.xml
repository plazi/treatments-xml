<taxonomicName authority="Chang et al. 2015" authorityName="Chang" authorityYear="2015" box="[232,688,1099,1123]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="22" pageNumber="2804" phylum="Firmicutes" rank="species" species="formosensis">
