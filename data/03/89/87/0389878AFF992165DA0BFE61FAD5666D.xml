<taxonomicName class="Bacilli" family="Lactobacillaceae" genus="Apilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="20" pageNumber="2802" phylum="Firmicutes" rank="species" species="mellifera">
