<taxonomicName authority="Killer et al. 2014" authorityName="Killer" authorityYear="2014" box="[232,622,1779,1803]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="20" pageNumber="2802" phylum="Firmicutes" rank="species" species="bombi">
