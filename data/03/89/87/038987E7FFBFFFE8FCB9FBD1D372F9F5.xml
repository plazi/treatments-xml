<emphasis box="[192,674,1187,1211]" italics="true" pageId="8" pageNumber="7">
<taxonomicName box="[192,442,1187,1211]" class="Magnoliopsida" family="Annonaceae" genus="Oxandra" kingdom="Plantae" order="Magnoliales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="lanceolata">Oxandra lanceolata</taxonomicName>
