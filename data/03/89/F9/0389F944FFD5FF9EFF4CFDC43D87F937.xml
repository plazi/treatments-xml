<location LSID="urn:lsid:plazi:treatment:0389F944FFD5FF9EFF4CFDC43D87F937:8EFF1E89FFD5FF9EFE29FB063B6BFB3A" box="[498,516,1240,1266]" county="Pancas county" municipality="Laginha" name="It" pageId="22" pageNumber="23" stateProvince="Espirito Santo">It</location>
grows on inselbergs in 
<location LSID="urn:lsid:plazi:treatment:0389F944FFD5FF9EFF4CFDC43D87F937:8EFF1E89FFD5FF9EFCC0FB063A16FB3A" box="[795,889,1240,1266]" county="Pancas county" municipality="Laginha" name="Atlantic" pageId="22" pageNumber="23" stateProvince="Espirito Santo">Atlantic</location>
rain forest between 
<quantity box="[1128,1261,1240,1266]" metricMagnitude="2" metricUnit="m" metricValue="4.9" metricValueMax="5.5" metricValueMin="4.3" pageId="22" pageNumber="23" unit="m" value="490.0" valueMax="550.0" valueMin="430.0">
<elevation box="[1128,1261,1240,1266]" metricMagnitude="2" metricUnit="m" metricValue="4.9" metricValueMax="5.5" metricValueMin="4.3" pageId="22" pageNumber="23" unit="m" value="490.0" valueMax="550.0" valueMin="430.0">430–550 m</elevation>
</quantity>
elevation
</materialsCitation>
.
</paragraph>
