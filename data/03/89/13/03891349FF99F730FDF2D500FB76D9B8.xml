<taxonomicName authorityName="Latreille" authorityYear="1804" box="[859,983,1184,1210]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
