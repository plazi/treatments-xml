<taxonomicName authority="" authorityName="Robertson" authorityYear="1902" box="[189,487,1184,1210]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="subGenus" subGenus="Dialictus">
