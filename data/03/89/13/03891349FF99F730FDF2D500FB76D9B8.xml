</taxonomicName>
<taxonomicNameLabel box="[814,905,1220,1246]" pageId="23" pageNumber="24" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName authorityName="Gibbs" authorityYear="2012" box="[962,1220,1219,1245]" class="Insecta" family="Halictidae" genus="Habralictus" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="gonzalezi">
