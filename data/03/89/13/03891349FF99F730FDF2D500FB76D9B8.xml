<taxonomicName authority="Gibbs, 2016" authorityName="Gibbs" authorityYear="2016" box="[384,518,1219,1246]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="kalinago" status="sp. nov.">
<emphasis box="[384,400,1220,1245]" italics="true" pageId="23" pageNumber="24">L</emphasis>
. 
<emphasis box="[415,518,1219,1245]" italics="true" pageId="23" pageNumber="24">kalinago</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[525,618,1220,1246]" pageId="23" pageNumber="24" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName authority="Gibbs, 2016" authorityName="Gibbs" authorityYear="2016" box="[629,807,1219,1246]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="dominicense" status="sp. nov.">
<emphasis box="[629,645,1220,1245]" italics="true" pageId="23" pageNumber="24">L</emphasis>
. 
