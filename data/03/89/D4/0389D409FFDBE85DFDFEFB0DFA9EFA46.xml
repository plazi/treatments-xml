</taxonomicName>
, 
<taxonomicName authority="(Linnaeus) J. V. Lamouroux, 1816" authorityName="J. V. Lamouroux" authorityYear="1816" baseAuthorityName="Linnaeus" box="[234,856,1380,1403]" family="Corallinaceae" genus="Jania" kingdom="Plantae" order="Cryptonemiales" pageId="1" pageNumber="342" phylum="Rhodophyta" rank="species" species="rubens">
