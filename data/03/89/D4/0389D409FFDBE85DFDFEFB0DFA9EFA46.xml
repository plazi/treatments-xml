</taxonomicName>
, 
<taxonomicName authority="(Philippi) Cabioch &amp; M. L. Mendoza, 2003" authorityName="Cabioch &amp; M. L. Mendoza" authorityYear="2003" baseAuthorityName="Philippi" class="Florideophyceae" family="Hapalidiaceae" genus="Mesophyllum" kingdom="Plantae" order="Corallinales" pageId="1" pageNumber="342" phylum="Rhodophyta" rank="species" species="expansum">
