</subSubSection>
<subSubSection box="[199,1184,475,500]" pageId="22" pageNumber="23" type="distribution">
<paragraph blockId="22.[151,1436,151,607]" box="[199,1184,475,500]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[199,352,475,500]" pageId="22" pageNumber="23">Distribution.</emphasis>
<taxonomicName box="[359,576,476,500]" class="Hydrozoa" family="Lafoeidae" genus="Filellum" kingdom="Animalia" order="Leptothecata" pageId="22" pageNumber="23" phylum="Cnidaria" rank="species" species="contortum">
