and 
<taxonomicName authority="H. Karst." authorityName="H. Karst." box="[617,818,1843,1869]" class="Liliopsida" family="Arecaceae" genus="Socratea" kingdom="Plantae" order="Arecales" pageId="56" pageNumber="57" phylum="Tracheophyta" rank="genus">
<emphasis box="[617,717,1843,1869]" italics="true" pageId="56" pageNumber="57">Socratea</emphasis>
H.Karst.
