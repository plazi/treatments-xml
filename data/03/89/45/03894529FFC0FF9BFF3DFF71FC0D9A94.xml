<treatmentCitationGroup box="[151,622,1072,1095]" pageId="7" pageNumber="22">
<taxonomicName authority="Lichtenstein, 1823a: 132" authorityName="Lichtenstein" authorityPageNumber="132" authorityYear="1823" box="[151,618,1072,1095]" class="Aves" family="Fringillidae" genus="Fringilla" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="22" phylum="Chordata" rank="species" species="obsoleta">
