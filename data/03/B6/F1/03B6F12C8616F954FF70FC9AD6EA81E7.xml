<subSubSection box="[199,972,151,177]" pageId="2" pageNumber="359" type="etymology">
<paragraph blockId="2.[151,1437,151,537]" box="[199,972,151,177]" pageId="2" pageNumber="359">
<emphasis bold="true" box="[199,334,151,177]" pageId="2" pageNumber="359">Etymology.</emphasis>
