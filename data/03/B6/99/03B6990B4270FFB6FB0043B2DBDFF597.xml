, width eye region 
<quantity box="[1286,1439,1432,1466]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="18" pageNumber="296" unit="mm" value="0.5">0.50 mm</quantity>
; abdomen length 
<quantity box="[1781,1928,1432,1466]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="18" pageNumber="296" unit="mm" value="1.5">1.50 mm</quantity>
, width 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="18" pageNumber="296" unit="mm" value="1.1">1.10 mm</quantity>
. Leg length: leg 
<date box="[956,1056,1485,1519]" pageId="18" pageNumber="296" value="1975-01-04">
I 
<quantity box="[985,1138,1485,1519]" metricMagnitude="-3" metricUnit="m" metricValue="4.75" pageId="18" pageNumber="296" unit="mm" value="4.75">4.75 mm</quantity>
</date>
(1.20-1. 
<date box="[1301,1432,1485,1519]" pageId="18" pageNumber="296" value="1975-01-30">75-1.30</date>
-0.50); leg 
<date box="[1644,1758,1485,1519]" pageId="18" pageNumber="296" value="1945-02-03">
II 
<quantity box="[1688,1841,1485,1519]" metricMagnitude="-3" metricUnit="m" metricValue="3.45" pageId="18" pageNumber="296" unit="mm" value="3.45">3.45 mm</quantity>
