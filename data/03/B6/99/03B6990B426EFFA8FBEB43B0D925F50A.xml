<quantity box="[1689,1840,1484,1518]" metricMagnitude="-3" metricUnit="m" metricValue="6.05" pageId="12" pageNumber="290" unit="mm" value="6.05">6.05 mm</quantity>
</date>
(
<date box="[1875,1997,1484,1518]" pageId="12" pageNumber="290" value="1950-01-01">1.50-1.</date>
90-1.75­ 0.90), palp 0.85-0.35-0.45- 
<quantity box="[1035,1187,1537,1571]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="12" pageNumber="290" unit="mm" value="0.85">0.85 mm</quantity>
