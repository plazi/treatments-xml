<emphasis box="[1199,1316,2535,2569]" italics="true" pageId="0" pageNumber="276">
<taxonomicName authorityName="Deeleman-Reinhold" authorityYear="2001" box="[1199,1312,2535,2569]" class="Arachnida" family="Corinnidae" genus="Koppe" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="276" phylum="Arthropoda" rank="genus">Koppe</taxonomicName>
