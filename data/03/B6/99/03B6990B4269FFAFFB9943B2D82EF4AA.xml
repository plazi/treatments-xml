II 
<quantity box="[1529,1683,1382,1416]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="11" pageNumber="289" unit="mm" value="5.3">5.30 mm</quantity>
</date>
(
<date box="[1722,1845,1382,1416]" pageId="11" pageNumber="289" value="1940-01-01">1.40-1.</date>
90-1.40­ 0.60); leg 
