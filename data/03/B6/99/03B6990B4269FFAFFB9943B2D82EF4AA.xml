<quantity box="[1834,1979,1278,1312]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="11" pageNumber="289" unit="mm" value="1.2">1.20 mm</quantity>
, width head 
<quantity box="[621,775,1329,1363]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="11" pageNumber="289" unit="mm" value="0.8">0.80 mm</quantity>
, eye group width 
<quantity box="[1122,1276,1329,1363]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="11" pageNumber="289" unit="mm" value="0.55">0.55 mm</quantity>
; abdomen length 
<quantity box="[1621,1769,1329,1363]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="11" pageNumber="289" unit="mm" value="1.6">1.60 mm</quantity>
, width 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="11" pageNumber="289" unit="mm" value="1.2">1.20 mm</quantity>
. Leg length: leg 
<date box="[796,896,1382,1416]" pageId="11" pageNumber="289" value="1985-06-01">
1 
<quantity box="[825,980,1382,1416]" metricMagnitude="-3" metricUnit="m" metricValue="6.85" pageId="11" pageNumber="289" unit="mm" value="6.85">6.85 mm</quantity>
</date>
(
