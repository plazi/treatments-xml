</date>
(
<date box="[1214,1378,2114,2148]" pageId="11" pageNumber="289" value="2000-01-30" valueMax="2000-01-02" valueMin="2000-01-30">1.30-2.00</date>
-1.50-0.60). leg 
<date box="[1675,1789,2114,2148]" pageId="11" pageNumber="289" value="1930-02-04">
II 
<quantity box="[1716,1870,2114,2148]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="11" pageNumber="289" unit="mm" value="4.3">4.30 mm</quantity>
</date>
(
<date pageId="11" pageNumber="289" value="1960-01-10" valueMax="1960-01-01" valueMin="1960-01-10">1.10­ 1.60</date>
-1.1 0-0.50); leg 
<date box="[775,904,2167,2201]" pageId="11" pageNumber="289" value="1950-03-03">
III 
<quantity box="[831,983,2167,2201]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="11" pageNumber="289" unit="mm" value="3.5">3.50 mm</quantity>
