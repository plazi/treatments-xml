<number box="[581,623,151,175]" pageId="5" pageNumber="63" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
anterior; III, 
<number box="[776,849,151,175]" pageId="5" pageNumber="63" value="50.0" valueMax="55.0" valueMin="45.0">45–55</number>
