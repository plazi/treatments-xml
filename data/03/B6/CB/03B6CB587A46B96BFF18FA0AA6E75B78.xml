species:
</emphasis>
<taxonomicName authority="Brenske, 1896" authorityName="Brenske" authorityYear="1896" box="[323,792,1631,1657]" class="Insecta" family="Scarabaeidae" genus="Trigonochilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="596" phylum="Arthropoda" rank="species" species="coriaceus">
