<emphasis box="[408,537,760,785]" italics="true" pageId="6" pageNumber="207">Physiphora</emphasis>
Fall√©n
</taxonomicName>
, 
<taxonomicName authority="Wiedemann" authorityName="Wiedemann" box="[628,833,760,785]" class="Insecta" family="Ulidiidae" genus="Timia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="207" phylum="Arthropoda" rank="genus">
<emphasis box="[628,692,761,784]" italics="true" pageId="6" pageNumber="207">Timia</emphasis>
Wiedemann
</taxonomicName>
and 
<taxonomicName authority="Meigen." authorityName="Meigen." box="[887,1059,760,785]" class="Insecta" family="Lonchaeidae" genus="Ulidia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="207" phylum="Arthropoda" rank="genus">
<emphasis box="[887,959,760,785]" italics="true" pageId="6" pageNumber="207">Ulidia</emphasis>
Meigen.
