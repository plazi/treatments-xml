<paragraph blockId="3.[151,1437,1556,1979]" box="[151,1203,1556,1583]" pageId="3" pageNumber="114">
<emphasis bold="true" box="[151,262,1556,1583]" pageId="3" pageNumber="114">Material.</emphasis>
<collectionCode box="[269,313,1557,1583]" country="China" name="Chongqing Museum" pageId="3" pageNumber="114">CM</collectionCode>
