<collectingCountry box="[357,424,548,572]" name="China" pageId="6" pageNumber="32">China</collectingCountry>
(Hainan), 
<collectingCountry box="[544,673,548,572]" name="Philippines" pageId="6" pageNumber="32">Philippines</collectingCountry>
, 
<collectingCountry box="[685,790,548,572]" name="Malaysia" pageId="6" pageNumber="32">Malaysia</collectingCountry>
, 
<collectingCountry box="[801,914,548,572]" name="Indonesia" pageId="6" pageNumber="32">Indonesia</collectingCountry>
.
</paragraph>
