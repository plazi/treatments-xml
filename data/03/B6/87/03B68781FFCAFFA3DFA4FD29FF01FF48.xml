<date box="[177,282,1840,1863]" pageId="1" pageNumber="286" value="2015-12-07">7.xii.2015</date>
– 
<date box="[297,380,1840,1863]" pageId="1" pageNumber="286" value="2016-01-09">9.i.2016</date>
