<collectingCountry box="[1021,1126,693,717]" name="Paraguay" pageId="30" pageNumber="80">Paraguay</collectingCountry>
, 
<collectingCountry box="[1139,1268,692,716]" name="Philippines" pageId="30" pageNumber="80">Philippines</collectingCountry>
