, 
<collectingCountry box="[448,560,692,716]" name="Indonesia" pageId="30" pageNumber="80">Indonesia</collectingCountry>
, 
<collectingCountry box="[571,677,692,716]" name="Malaysia" pageId="30" pageNumber="80">Malaysia</collectingCountry>
