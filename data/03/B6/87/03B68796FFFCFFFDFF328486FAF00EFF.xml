<emphasis bold="true" box="[151,267,1549,1571]" pageId="12" pageNumber="351">FIGURE 9</emphasis>
(ĀD) 
<taxonomicName box="[348,524,1549,1572]" class="Insecta" family="Ichneumonidae" genus="Eucremastus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="351" phylum="Arthropoda" rank="species" species="rufoantennalis" status="sp. nov.">
<emphasis box="[348,524,1549,1572]" italics="true" pageId="12" pageNumber="351">E. rufoantennalis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[530,610,1550,1571]" pageId="12" pageNumber="351">
<taxonomicNameLabel box="[530,610,1550,1571]" pageId="12" pageNumber="351" rank="species">sp. nov.</taxonomicNameLabel>
