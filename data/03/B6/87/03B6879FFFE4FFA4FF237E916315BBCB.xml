<emphasis box="[320,572,1951,1973]" italics="true" pageId="1" pageNumber="275">Notosacantha timorensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[578,685,1951,1972]" pageId="1" pageNumber="275">
<taxonomicNameLabel box="[578,658,1951,1972]" pageId="1" pageNumber="275" rank="species">sp. nov.</taxonomicNameLabel>
1.
</emphasis>
dorsal view; 
<emphasis bold="true" box="[821,840,1951,1972]" pageId="1" pageNumber="275">2.</emphasis>
lateral view; 
<emphasis bold="true" box="[979,997,1951,1972]" pageId="1" pageNumber="275">3.</emphasis>
hind view; 
<emphasis bold="true" box="[1118,1137,1951,1972]" pageId="1" pageNumber="275">4.</emphasis>
head; 
<emphasis bold="true" box="[1204,1223,1951,1972]" pageId="1" pageNumber="275">5.</emphasis>
antenna.
</paragraph>
</caption>
<caption box="[151,1302,1801,1824]" httpUri="https://zenodo.org/record/160661/files/figure.png" pageId="2" pageNumber="276" targetBox="[219,1374,209,1776]" targetPageId="2">
<paragraph blockId="2.[151,1302,1801,1824]" box="[151,1302,1801,1824]" pageId="2" pageNumber="276">
<emphasis bold="true" box="[151,327,1801,1824]" pageId="2" pageNumber="276">FIGURES 6–10.</emphasis>
<taxonomicName box="[333,626,1802,1823]" class="Insecta" family="Chrysomelidae" genus="Notosacantha" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="276" phylum="Arthropoda" rank="species" species="sumbawaensis">
<emphasis box="[333,626,1802,1823]" italics="true" pageId="2" pageNumber="276">Notosacantha sumbawaensis</emphasis>
