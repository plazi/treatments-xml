, HW: 
<number box="[810,924,1551,1575]" pageId="19" pageNumber="120" value="2.81" valueMax="3.13" valueMin="2.49">2.49–3.13</number>
, CI: 
<number box="[987,1060,1551,1575]" pageId="19" pageNumber="120" value="88.5" valueMax="92.0" valueMin="85.0">85–92</number>
, SL: 
<number box="[1126,1241,1551,1575]" pageId="19" pageNumber="120" value="2.71" valueMax="2.95" valueMin="2.47">2.47–2.95</number>
, SI: 
<number box="[1299,1373,1551,1575]" pageId="19" pageNumber="120" value="96.5" valueMax="99.0" valueMin="94.0">94–99</number>
, PW: 
<number box="[151,266,1588,1612]" pageId="19" pageNumber="120" value="2.125" valueMax="2.3" valueMin="1.95">1.95–2.30</number>
