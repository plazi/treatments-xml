<subSubSection lastPageId="21" lastPageNumber="122" pageId="19" pageNumber="120" type="description">
<paragraph blockId="19.[151,1437,1371,1612]" box="[151,371,1371,1396]" pageId="19" pageNumber="120">
<emphasis bold="true" box="[151,371,1371,1396]" pageId="19" pageNumber="120">Worker diagnosis:</emphasis>
</paragraph>
<paragraph blockId="19.[151,1437,1371,1612]" pageId="19" pageNumber="120">
Larger species (HW: 
<quantity box="[390,556,1407,1431]" metricMagnitude="-3" metricUnit="m" metricValue="2.81" metricValueMax="3.13" metricValueMin="2.49" pageId="19" pageNumber="120" unit="mm" value="2.81" valueMax="3.13" valueMin="2.49">2.49–3.13 mm</quantity>
