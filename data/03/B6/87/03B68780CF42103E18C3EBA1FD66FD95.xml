<paragraph blockId="18.[151,705,152,178]" box="[151,705,152,178]" pageId="18" pageNumber="519">
<heading bold="true" box="[151,705,152,178]" fontSize="11" level="1" pageId="18" pageNumber="519" reason="1">
<taxonomicName authority="Mertens, 1937" authorityName="Mertens" authorityYear="1937" box="[151,705,152,178]" class="Reptilia" family="Pelomedusidae" genus="Pelomedusa" kingdom="Animalia" order="Testudines" pageId="18" pageNumber="519" phylum="Chordata" rank="subSpecies" species="subrufa" subSpecies="wettsteini">
<emphasis bold="true" box="[151,705,152,178]" pageId="18" pageNumber="519">
<emphasis bold="true" box="[151,520,152,178]" italics="true" pageId="18" pageNumber="519">Pelomedusa subrufa wettsteini</emphasis>
<bibRefCitation author="Mertens, R." box="[527,705,152,178]" pageId="18" pageNumber="519" pagination="139 - 142" refId="ref12650" refString="Mertens, R. (1937) Bemerkungen uber die Rassen von Pelomedusa subrufa (La Cepede). Zoologischer Anzeiger, 117, 139 - 142." type="journal article" year="1937">Mertens, 1937</bibRefCitation>
</emphasis>
</taxonomicName>
</heading>
</paragraph>
