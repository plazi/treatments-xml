<subSubSection box="[140,502,1046,1073]" pageId="8" pageNumber="41" type="nomenclature">
<paragraph blockId="8.[140,502,1046,1108]" box="[140,502,1046,1073]" pageId="8" pageNumber="41">
<heading box="[140,502,1046,1073]" fontSize="11" level="2" pageId="8" pageNumber="41" reason="3">
<emphasis bold="true" box="[140,502,1046,1073]" pageId="8" pageNumber="41">
