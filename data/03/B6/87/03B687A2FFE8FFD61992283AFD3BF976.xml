<paragraph blockId="9.[140,439,1117,1214]" box="[140,364,1117,1143]" pageId="9" pageNumber="42">
<emphasis bold="true" box="[140,364,1117,1143]" pageId="9" pageNumber="42">
<emphasis box="[140,203,1117,1143]" inLineHeading="true" pageId="9" pageNumber="42" reason="1">Tribe</emphasis>
