– 
<number box="[494,509,406,431]" pageId="5" pageNumber="598" value="6.0">6</number>
] Females [
<number box="[631,645,406,431]" pageId="5" pageNumber="598" value="7.0">7</number>
– 
<number box="[662,677,406,431]" pageId="5" pageNumber="598" value="6.0">6</number>
* 
<number box="[689,704,406,431]" pageId="5" pageNumber="598" value="2.0">2</number>
