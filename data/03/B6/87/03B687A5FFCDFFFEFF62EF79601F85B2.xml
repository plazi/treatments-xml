<number box="[716,729,406,431]" pageId="5" pageNumber="598" value="3.0">3</number>
– 
<number box="[746,761,406,431]" pageId="5" pageNumber="598" value="2.0">2</number>
] [
<number box="[782,811,407,431]" pageId="5" pageNumber="598" value="10.0">10</number>
* 
<number box="[824,837,407,431]" pageId="5" pageNumber="598" value="3.0">3</number>
