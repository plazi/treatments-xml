<paragraph blockId="18.[151,1436,367,1184]" pageId="18" pageNumber="219">
<emphasis bold="true" box="[199,262,1087,1112]" pageId="18" pageNumber="219">Note.</emphasis>
<taxonomicName authority="Puplesis, 1984" authorityName="Puplesis" authorityYear="1984" box="[278,602,1087,1112]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="dentatae">
