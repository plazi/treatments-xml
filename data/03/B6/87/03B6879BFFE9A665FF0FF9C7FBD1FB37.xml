<emphasis box="[1137,1436,1949,1973]" italics="true" pageId="17" pageNumber="218">Stigmella quercipulchella</emphasis>
(Chambers, 
<number box="[288,348,1986,2010]" pageId="17" pageNumber="218" value="1878.0">1878</number>
)
</taxonomicName>
; 
<taxonomicName authority="Braun, 1910" authorityName="Braun" authorityYear="1910" box="[368,650,1985,2010]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="218" phylum="Arthropoda" rank="species" species="variella">
<emphasis box="[368,483,1985,2009]" italics="true" pageId="17" pageNumber="218">S. variella</emphasis>
(Braun, 
<number box="[581,641,1986,2010]" pageId="17" pageNumber="218" value="1910.0">1910</number>
)
</taxonomicName>
; 
<taxonomicName authority="Braun, 1914" authorityName="Braun" authorityYear="1914" box="[662,928,1985,2010]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="218" phylum="Arthropoda" rank="species" species="altella">
<emphasis box="[662,762,1985,2009]" italics="true" pageId="17" pageNumber="218">S. altella</emphasis>
(Braun, 
<number box="[860,920,1986,2010]" pageId="17" pageNumber="218" value="1914.0">1914</number>
)
</taxonomicName>
.
</paragraph>
<paragraph blockId="18.[151,1436,151,212]" pageId="18" pageNumber="219">
Neotropical (Central American): 
<taxonomicName authority="Diskus &amp; Stonis, 2013" authorityName="Diskus &amp; Stonis" authorityYear="2013" box="[573,1113,151,176]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="guatemalensis">
