<heading bold="true" box="[151,390,151,178]" fontSize="11" level="1" pageId="6" pageNumber="567" reason="1">
<emphasis bold="true" box="[151,390,151,178]" pageId="6" pageNumber="567">
<taxonomicName box="[151,315,151,177]" class="Insecta" family="Baetidae" genus="Cloeon" kingdom="Animalia" order="Ephemeroptera" pageId="6" pageNumber="567" phylum="Arthropoda" rank="species" species="sidadi" status="sp. n.">
<emphasis bold="true" box="[151,315,151,177]" italics="true" pageId="6" pageNumber="567">Cloeon sidadi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[323,390,152,178]" pageId="6" pageNumber="567" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
