<taxonomicName authority="CAMERON" authorityName="CAMERON" authorityYear="1903" box="[579,913,1440,1461]" class="Insecta" family="Ichneumonidae" genus="Aglaojoppa" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="899" phylum="Arthropoda" rank="species" species="femorata">
