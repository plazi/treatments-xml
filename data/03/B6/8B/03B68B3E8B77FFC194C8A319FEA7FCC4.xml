<subSubSection pageId="18" pageNumber="19" type="etymology">
<paragraph blockId="18.[151,1437,151,789]" pageId="18" pageNumber="19">
<emphasis bold="true" box="[199,334,727,752]" pageId="18" pageNumber="19">Etymology.</emphasis>
