<paragraph blockId="1.[151,589,151,177]" box="[151,589,151,177]" pageId="1" pageNumber="28">
<heading bold="true" box="[151,589,151,177]" fontSize="11" level="1" pageId="1" pageNumber="28" reason="1">
<emphasis bold="true" box="[151,589,151,177]" pageId="1" pageNumber="28">
Genus 
<taxonomicName authority="Den Heyer, 1978" authorityName="Den Heyer" authorityYear="1978" box="[238,589,151,177]" class="Arachnida" family="Cunaxidae" genus="Armascirus" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="28" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,378,151,177]" italics="true" pageId="1" pageNumber="28">Armascirus</emphasis>
<bibRefCitation author="Den" box="[385,589,151,177]" pageId="1" pageNumber="39" refString="Den Heyer, J. (1978) Four new species of Armascirus gen. nov. (Prostigmata: Acari) from the Ethiopian Region. Journal of Entomological Society of South Africa, 41, 217 - 239." type="journal article" year="1978">
