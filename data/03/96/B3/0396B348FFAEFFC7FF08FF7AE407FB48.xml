<paragraph blockId="28.[151,1436,1920,2030]" pageId="28" pageNumber="29">
<emphasis bold="true" box="[151,259,1920,1941]" pageId="28" pageNumber="29">
<collectionCode box="[151,229,1920,1941]" pageId="28" pageNumber="29">PLATE</collectionCode>
<number box="[235,259,1920,1941]" pageId="28" pageNumber="29" value="15.0">15</number>
