<emphasis box="[199,341,1203,1227]" italics="true" pageId="12" pageNumber="2969">Distribution.</emphasis>
<collectingCountry box="[353,462,1202,1226]" name="Australia" pageId="12" pageNumber="2969">Australia</collectingCountry>
, Chagos Archipelago (Egmont Atoll), 
<collectingCountry name="Christmas Island" pageId="12" pageNumber="2969">Christmas Island</collectingCountry>
, 
<collectingCountry box="[255,318,1234,1258]" name="India" pageId="12" pageNumber="2969">India</collectingCountry>
, 
<collectingCountry box="[340,391,1234,1258]" name="Iran" pageId="12" pageNumber="2969">Iran</collectingCountry>
, 
<collectingCountry box="[413,501,1234,1258]" name="Kuwait" pageId="12" pageNumber="2969">Kuwait</collectingCountry>
, 
<collectingCountry box="[523,624,1234,1258]" name="Pakistan" pageId="12" pageNumber="2969">Pakistan</collectingCountry>
