<collectingCountry box="[947,1111,1234,1258]" name="Saudi Arabia" pageId="12" pageNumber="2969">Saudi Arabia</collectingCountry>
, 
<collectingCountry box="[159,313,1268,1292]" name="South Africa" pageId="12" pageNumber="2969">South Africa</collectingCountry>
