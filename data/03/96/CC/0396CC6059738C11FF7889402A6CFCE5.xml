<subSubSection box="[151,949,1712,1735]" pageId="4" pageNumber="51" type="materials_examined">
<paragraph blockId="4.[151,949,1712,1735]" box="[151,949,1712,1735]" pageId="4" pageNumber="51">
<typeStatus box="[151,204,1712,1735]" pageId="4" pageNumber="51">Type</typeStatus>
species: 
<taxonomicName authority="Berkov and Monne" authorityName="Berkov and Monne" box="[301,811,1712,1735]" class="Insecta" family="Cerambycidae" genus="Baryssiniella" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="51" phylum="Arthropoda" rank="species" species="hieroglyphica" status="new species">
<emphasis box="[301,596,1712,1735]" italics="true" pageId="4" pageNumber="51">Baryssiniella hieroglyphica</emphasis>
Berkov and Monné
</taxonomicName>
, 
<taxonomicNameLabel box="[821,949,1712,1735]" pageId="4" pageNumber="51" rank="species">new species</taxonomicNameLabel>
</paragraph>
