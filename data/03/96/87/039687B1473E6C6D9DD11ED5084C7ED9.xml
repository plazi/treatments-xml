<taxonomicNameLabel box="[551,632,1949,1973]" pageId="67" pageNumber="68" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, female. 
<emphasis bold="true" box="[727,745,1949,1973]" pageId="67" pageNumber="68">A</emphasis>
, fore wing; 
<emphasis bold="true" box="[870,887,1949,1973]" pageId="67" pageNumber="68">B</emphasis>
, mesopleuron; 
<emphasis bold="true" box="[1044,1062,1949,1973]" pageId="67" pageNumber="68">C</emphasis>
, propodeum; 
<emphasis bold="true" box="[1205,1223,1949,1973]" pageId="67" pageNumber="68">D</emphasis>
, ovipositor; 
<emphasis bold="true" box="[1352,1369,1949,1973]" pageId="67" pageNumber="68">E</emphasis>
