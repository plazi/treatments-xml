<taxonomicName box="[1084,1205,1622,1645]" class="Adenophorea" family="Desmodoridae" genus="Acanthopharynx" kingdom="Animalia" order="Desmodorida" pageId="5" pageNumber="88" phylum="Nematoda" rank="species" species="arcanus">
