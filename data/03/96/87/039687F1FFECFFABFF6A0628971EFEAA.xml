. 
<emphasis box="[1375,1436,188,212]" italics="true" pageId="6" pageNumber="95">lictor</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Burton" baseAuthorityYear="1954" box="[199,318,224,248]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="topsenti">
