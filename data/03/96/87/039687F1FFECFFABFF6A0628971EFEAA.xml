<taxonomicName baseAuthorityName="Topsent" baseAuthorityYear="1904" box="[151,305,188,212]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="hispidulum">
<emphasis box="[151,168,189,212]" italics="true" pageId="6" pageNumber="95">E</emphasis>
. 
<emphasis box="[181,305,188,212]" italics="true" pageId="6" pageNumber="95">hispidulum</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Topsent" authorityYear="1927" box="[320,451,189,212]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="incipiens">
<emphasis box="[320,337,189,212]" italics="true" pageId="6" pageNumber="95">E</emphasis>
. 
<emphasis box="[350,451,189,212]" italics="true" pageId="6" pageNumber="95">incipiens</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Topsent" baseAuthorityYear="1927" box="[465,614,188,212]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="scabiosum">
<emphasis box="[465,482,189,212]" italics="true" pageId="6" pageNumber="95">E</emphasis>
. 
<emphasis box="[496,614,188,212]" italics="true" pageId="6" pageNumber="95">scabiosum</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Bowerbank" baseAuthorityYear="1874" box="[668,784,188,212]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="simplex">
