<taxonomicName authorityName="Santos, Franca &amp; Pinheiro" authorityYear="2014" box="[328,469,152,176]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="suassunai">
