<taxonomicName baseAuthorityName="Topsent" baseAuthorityYear="1904" box="[1344,1436,188,212]" class="Demospongiae" family="Raspailiidae" genus="Eurypon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="6" pageNumber="95" phylum="Porifera" rank="species" species="lictor">
