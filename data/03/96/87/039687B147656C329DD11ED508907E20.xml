, female. 
<emphasis bold="true" box="[709,727,1966,1990]" pageId="26" pageNumber="27">A</emphasis>
, fore wing; 
<emphasis bold="true" box="[857,874,1967,1991]" pageId="26" pageNumber="27">B</emphasis>
, mesopleuron; 
<emphasis bold="true" box="[1035,1053,1966,1990]" pageId="26" pageNumber="27">C</emphasis>
, propodeum; 
<emphasis bold="true" box="[1199,1217,1967,1991]" pageId="26" pageNumber="27">D</emphasis>
