<emphasis bold="true" box="[1041,1059,1962,1986]" pageId="103" pageNumber="104">C</emphasis>
, propodeum; 
<emphasis bold="true" box="[1203,1221,1962,1986]" pageId="103" pageNumber="104">D</emphasis>
, ovipositor; 
<emphasis bold="true" box="[1352,1369,1962,1986]" pageId="103" pageNumber="104">E</emphasis>
, head, anterior view; 
<emphasis bold="true" box="[299,314,1998,2022]" pageId="103" pageNumber="104">F</emphasis>
