<date box="[1278,1390,1863,1888]" pageId="18" pageNumber="569" value="1969-05-12">
<number box="[1278,1307,1863,1888]" pageId="18" pageNumber="569" value="12.0">12</number>
.v. 
<number box="[1329,1390,1863,1888]" pageId="18" pageNumber="569" value="1969.0">1969</number>
</date>
, on 
<taxonomicName box="[151,288,1901,1924]" class="Magnoliopsida" family="Rosaceae" genus="Cotoneaster" kingdom="Plantae" order="Rosales" pageId="18" pageNumber="569" phylum="Tracheophyta" rank="genus">
