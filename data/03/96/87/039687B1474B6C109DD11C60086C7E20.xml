<emphasis bold="true" box="[1038,1056,1958,1982]" pageId="56" pageNumber="57">C</emphasis>
, propodeum; 
<emphasis bold="true" box="[1201,1219,1959,1983]" pageId="56" pageNumber="57">D</emphasis>
, ovipositor; 
<emphasis bold="true" box="[1351,1368,1959,1983]" pageId="56" pageNumber="57">E</emphasis>
, head, anterior view; 
<emphasis bold="true" box="[299,314,1995,2019]" pageId="56" pageNumber="57">F</emphasis>
, head, dorsal view; 
<emphasis bold="true" box="[518,538,1994,2018]" pageId="56" pageNumber="57">G</emphasis>
