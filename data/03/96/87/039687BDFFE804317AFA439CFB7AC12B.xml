<subSubSection lastPageId="8" lastPageNumber="91" pageId="7" pageNumber="90" type="materials_examined">
<paragraph blockId="7.[151,1437,1621,2006]" box="[199,1402,1837,1862]" pageId="7" pageNumber="90">
<emphasis bold="true" box="[199,310,1837,1862]" pageId="7" pageNumber="90">Material.</emphasis>
