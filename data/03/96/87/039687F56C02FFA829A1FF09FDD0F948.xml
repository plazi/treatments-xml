, I-L-5/6; 
<emphasis bold="true" box="[1145,1159,1657,1675]" pageId="16" pageNumber="751">C</emphasis>
, palp; 
<emphasis bold="true" box="[113,145,1680,1698]" pageId="16" pageNumber="751">D -F</emphasis>
, 1 ♀, I 1165: 
<emphasis bold="true" box="[257,271,1680,1698]" pageId="16" pageNumber="751">D</emphasis>
, venter; 
<emphasis bold="true" box="[343,355,1680,1698]" pageId="16" pageNumber="751">E</emphasis>
, palp; 
<emphasis bold="true" box="[412,423,1680,1698]" pageId="16" pageNumber="751">F</emphasis>
