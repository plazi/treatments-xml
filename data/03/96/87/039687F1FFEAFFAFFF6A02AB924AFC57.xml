<paragraph blockId="2.[151,858,222,275]" box="[151,858,253,275]" pageId="2" pageNumber="91">
<emphasis bold="true" box="[151,294,253,275]" pageId="2" pageNumber="91">Type species:</emphasis>
<taxonomicName authority="Bowerbank, 1866" authorityName="Bowerbank" authorityYear="1866" box="[299,693,253,275]" class="Demospongiae" family="Raspailiidae" genus="Hymeraphia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="2" pageNumber="91" phylum="Porifera" rank="species" species="clavata">
