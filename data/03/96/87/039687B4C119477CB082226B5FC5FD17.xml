<taxonomicName box="[313,457,548,573]" class="Insecta" family="Aphididae" genus="Aspidophorodon" kingdom="Animalia" order="Aphidomorpha" pageId="17" pageNumber="568" phylum="Arthropoda" rank="species" species="sinisalicis">
