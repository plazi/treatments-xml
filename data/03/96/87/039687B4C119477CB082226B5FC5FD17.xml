described 
<taxonomicName authority="Zhang, 1980" authorityName="Zhang" authorityYear="1980" box="[766,1231,439,464]" class="Insecta" family="Aphididae" genus="Aspidophorodon" kingdom="Animalia" order="Aphidomorpha" pageId="17" pageNumber="568" phylum="Arthropoda" rank="species" species="sinisalicis">
