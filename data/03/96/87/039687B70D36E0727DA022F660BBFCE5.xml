</taxonomicName>
, 
<date box="[1261,1384,1693,1719]" pageId="18" pageNumber="19" value="2002-09-17">17.ix.2002</date>
, one colony, 
<quantity box="[240,317,1729,1756]" metricMagnitude="-2" metricUnit="m" metricValue="1.7" pageId="18" pageNumber="19" unit="cm" value="1.7">1.7 cm</quantity>
high, with gonothecae, coll. A. Faucci, 
<collectionCode box="[767,857,1729,1755]" pageId="18" pageNumber="19">ROMIZ</collectionCode>
