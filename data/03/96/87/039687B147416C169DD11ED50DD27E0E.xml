<emphasis bold="true" box="[1121,1139,1956,1980]" pageId="62" pageNumber="63">D</emphasis>
, ovipositor; 
<emphasis bold="true" box="[1268,1285,1956,1980]" pageId="62" pageNumber="63">E</emphasis>
, head, anterior view; 
<emphasis bold="true" box="[214,229,1992,2016]" pageId="62" pageNumber="63">F</emphasis>
