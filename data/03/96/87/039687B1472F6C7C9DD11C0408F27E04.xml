, head, anterior view; 
<emphasis bold="true" box="[299,314,1987,2011]" pageId="84" pageNumber="85">F</emphasis>
, head, dorsal view; 
<emphasis bold="true" box="[518,538,1986,2010]" pageId="84" pageNumber="85">G</emphasis>
, mesoscutum; 
<emphasis bold="true" box="[691,711,1987,2011]" pageId="84" pageNumber="85">H</emphasis>
