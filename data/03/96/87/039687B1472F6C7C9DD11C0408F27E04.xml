<taxonomicName authority="Han &amp; Achterberg &amp; Chen, 2021" authorityName="Han &amp; Achterberg &amp; Chen" authorityYear="2021" box="[296,533,1951,1975]" class="Insecta" family="Ichneumonidae" genus="Campoplex" kingdom="Animalia" order="Hymenoptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="parassosae" status="sp. nov.">
<emphasis box="[296,533,1951,1975]" italics="true" pageId="84" pageNumber="85">Campoplex parassosae</emphasis>
</taxonomicName>
<emphasis bold="true" box="[540,622,1951,1975]" pageId="84" pageNumber="85">
<taxonomicNameLabel box="[540,622,1951,1975]" pageId="84" pageNumber="85" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, female. 
<emphasis bold="true" box="[719,737,1950,1974]" pageId="84" pageNumber="85">A</emphasis>
, fore wing; 
<emphasis bold="true" box="[864,881,1951,1975]" pageId="84" pageNumber="85">B</emphasis>
, mesopleuron; 
<emphasis bold="true" box="[1040,1058,1950,1974]" pageId="84" pageNumber="85">C</emphasis>
, propodeum; 
<emphasis bold="true" box="[1202,1220,1951,1975]" pageId="84" pageNumber="85">D</emphasis>
