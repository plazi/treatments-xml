<taxonomicName box="[311,484,1334,1357]" class="Insecta" family="Elateridae" genus="Dima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="moraveci">
