, 
<taxonomicName authorityName="E. M. FRIIS, P. R. CRANE et K. R. PEDERSEN" authorityYear="2019" class="Magnoliopsida" family="Chloranthaceae" genus="Goczania" kingdom="Plantae" order="Chloranthales" pageId="46" pageNumber="199" phylum="Tracheophyta" rank="species" species="punctata">
<emphasis italics="true" pageId="46" pageNumber="199">Goczania punctata</emphasis>
</taxonomicName>
