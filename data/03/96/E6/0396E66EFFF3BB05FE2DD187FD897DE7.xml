<subSubSection pageId="27" pageNumber="2516" type="materials_examined">
<paragraph blockId="27.[144,1103,887,943]" pageId="27" pageNumber="2516">
<materialsCitation box="[144,1031,887,911]" country="Guinea-Bissau" location="Guinee Forestiere" municipality="Foret Classee de Dieke" pageId="27" pageNumber="2516" specimenCount="1" typeStatus="holotype">
<collectingCountry box="[144,262,887,911]" name="Guinea-Bissau" pageId="27" pageNumber="2516">GUINEA</collectingCountry>
