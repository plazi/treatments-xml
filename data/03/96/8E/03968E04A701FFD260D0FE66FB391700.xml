<number box="[195,291,952,976]" pageId="8" pageNumber="8" value="202.5" valueMax="205.0" valueMin="200.0">
200 
<emphasis box="[236,249,953,976]" italics="true" pageId="8" pageNumber="8">–</emphasis>
205
</number>
; WL 1.00) (
<collectingCountry box="[417,461,952,976]" name="Saudi Arabia" pageId="8" pageNumber="8">KSA</collectingCountry>
