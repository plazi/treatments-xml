<taxonomicName authority="(REITTER, 1878)" baseAuthorityName="REITTER" baseAuthorityYear="1878" box="[504,832,1553,1575]" class="Insecta" family="Cryptophagidae" genus="Curelius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1141" phylum="Arthropoda" rank="species" species="japonicus">
