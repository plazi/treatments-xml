<paragraph blockId="7.[151,1437,866,1932]" pageId="7" pageNumber="58">
<materialsCitation collectingDate="1984-11-14" collectionCode="MNHN" collectorName="I. Foldi" country="Brazil" location="Curitiba" pageId="7" pageNumber="58" specimenCount="3" specimenCount-adult="3" stateProvince="Parana" typeStatus="paratype">
