<taxonomicName box="[609,792,1284,1310]" kingdom="Fungi" pageId="6" pageNumber="267" phylum="Ascomycota" rank="subPhylum" subPhylum="Pezizomycotina">Pezizomycotina</taxonomicName>
; 
<taxonomicName authorityName="O.E.Erikss. &amp; Winka" authorityYear="1997" box="[805,997,1284,1310]" class="Sordariomycetes" kingdom="Fungi" pageId="6" pageNumber="267" phylum="Ascomycota" rank="class">Sordariomycetes</taxonomicName>
; Sordariomycetidae; 
<taxonomicName authorityName="Huhndorf, A.N.Mill. &amp; F.A.Fernandez" authorityYear="2004" box="[1240,1447,1284,1310]" class="Sordariomycetes" kingdom="Fungi" order="Chaetosphaeriales" pageId="6" pageNumber="267" phylum="Ascomycota" rank="order">Chaetosphaeriales</taxonomicName>
; 
<taxonomicName box="[136,362,1320,1346]" class="Sordariomycetes" family="Chaetosphaeriaceae" kingdom="Fungi" order="Chaetosphaeriales" pageId="6" pageNumber="267" phylum="Ascomycota" rank="family">Chaetosphaeriaceae</taxonomicName>
; 
<taxonomicName baseAuthorityName="Index Fungorum" baseAuthorityYear="2020" box="[373,544,1320,1346]" class="Sordariomycetes" family="Chaetosphaeriaceae" genus="Stanjehughesia" kingdom="Fungi" order="Chaetosphaeriales" pageId="6" pageNumber="267" phylum="Ascomycota" rank="genus">
<emphasis box="[373,544,1320,1346]" italics="true" pageId="6" pageNumber="267">Stanjehughesia</emphasis>
</taxonomicName>
.
</paragraph>
