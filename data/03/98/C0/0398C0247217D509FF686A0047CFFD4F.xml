<paragraph blockId="22.[151,510,1594,1619]" box="[151,510,1594,1619]" pageId="22" pageNumber="523">
<heading bold="true" box="[151,510,1594,1619]" fontSize="10" level="2" pageId="22" pageNumber="523" reason="2">
<emphasis bold="true" box="[151,510,1594,1619]" pageId="22" pageNumber="523">
<taxonomicName authorityName="Kier" authorityYear="1968" box="[151,294,1594,1619]" class="Echinoidea" family="Fibulariidae" genus="Leniechinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clypeasteroida" pageId="22" pageNumber="523" phylum="Echinodermata" rank="genus">
<emphasis bold="true" box="[151,294,1594,1619]" italics="true" pageId="22" pageNumber="523">Leniechinus</emphasis>
</taxonomicName>
and 
<emphasis bold="true" box="[353,510,1594,1619]" italics="true" pageId="22" pageNumber="523">Lenicyamidia</emphasis>
</emphasis>
