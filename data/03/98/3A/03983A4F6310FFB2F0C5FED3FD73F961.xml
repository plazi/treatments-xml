<emphasis box="[970,1105,976,1002]" italics="true" pageId="3" pageNumber="776">
<taxonomicName authorityName="Du Petithouarsii Neboux" authorityYear="1840" box="[970,1067,976,1002]" class="Aves" family="Columbidae" genus="Columba" kingdom="Animalia" order="Columbiformes" pageId="3" pageNumber="776" phylum="Chordata" rank="genus">Columba</taxonomicName>
