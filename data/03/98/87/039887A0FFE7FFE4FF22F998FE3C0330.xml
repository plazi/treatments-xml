<paragraph blockId="6.[151,1436,223,539]" pageId="6" pageNumber="465">
<emphasis bold="true" box="[199,315,406,431]" pageId="6" pageNumber="465">Remarks.</emphasis>
<collectingRegion box="[322,415,406,431]" country="Portugal" name="Madeira" pageId="6" pageNumber="465">Madeira</collectingRegion>
