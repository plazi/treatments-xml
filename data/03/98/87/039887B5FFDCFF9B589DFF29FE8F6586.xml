<number box="[562,615,800,822]" pageId="10" pageNumber="242" value="1920.0">1920</number>
, 
<emphasis box="[628,994,800,822]" italics="true" pageId="10" pageNumber="242">Zoologische Mededeelingen Leiden</emphasis>
<number box="[1003,1016,800,822]" pageId="10" pageNumber="242" value="5.0">
<emphasis bold="true" box="[1003,1016,800,822]" pageId="10" pageNumber="242">5</emphasis>
