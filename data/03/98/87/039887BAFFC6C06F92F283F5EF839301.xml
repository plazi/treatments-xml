<quantity box="[419,499,857,879]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="33" pageNumber="34" unit="mm" value="2.4">2.4 mm</quantity>
, EW 
<emphasis bold="true" box="[568,588,856,878]" pageId="33" pageNumber="34">5</emphasis>
<quantity box="[601,681,857,879]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="33" pageNumber="34" unit="mm" value="1.2">1.2 mm</quantity>
, PNW 
<emphasis bold="true" box="[769,789,856,878]" pageId="33" pageNumber="34">5</emphasis>
<quantity box="[802,882,857,879]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="33" pageNumber="34" unit="mm" value="1.1">1.1 mm</quantity>
