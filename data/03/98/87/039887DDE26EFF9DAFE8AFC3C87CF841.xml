</taxonomicName>
<emphasis bold="true" box="[664,753,474,499]" pageId="4" pageNumber="5">
<taxonomicNameLabel box="[664,753,474,499]" pageId="4" pageNumber="5" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName box="[806,1012,475,499]" class="Arachnida" family="Salticidae" genus="Corusca" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="wuzhishanensis" status="sp. nov.">
