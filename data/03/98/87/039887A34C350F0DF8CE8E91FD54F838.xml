<taxonomicName box="[151,290,1861,1883]" class="Insecta" family="Staphylinidae" genus="Cephennomicrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="367" phylum="Arthropoda" rank="species" species="perexiguus">
<emphasis box="[151,290,1861,1883]" italics="true" pageId="10" pageNumber="367">C. perexiguus</emphasis>
</taxonomicName>
(32‾33), 
<taxonomicName box="[391,490,1860,1882]" class="Insecta" family="Staphylinidae" genus="Cephennomicrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="367" phylum="Arthropoda" rank="species" species="politus">
