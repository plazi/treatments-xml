<paragraph blockId="1.[151,811,1319,1379]" box="[151,358,1354,1379]" pageId="1" pageNumber="243">
(
<figureCitation box="[159,267,1354,1379]" captionStart="FIGURES 1 – 2" captionStartId="2.[151,264,1612,1635]" captionTargetBox="[162,1422,450,1582]" captionTargetId="figure@2.[150,1436,445,1591]" captionTargetPageId="2" captionText="FIGURES 1 – 2. Hylotribus fluminensis Queiroz &amp; Mermudes sp. nov., holotype male: 1, dorsal; 2, lateral." httpUri="https://zenodo.org/record/253180/files/figure.png" pageId="1" pageNumber="243">
Figs. 
<number box="[222,267,1355,1379]" pageId="1" pageNumber="243" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
</figureCitation>
; 
<number box="[279,350,1355,1379]" pageId="1" pageNumber="243" value="18.0" valueMax="23.0" valueMin="13.0">13–23</number>
)
</paragraph>
