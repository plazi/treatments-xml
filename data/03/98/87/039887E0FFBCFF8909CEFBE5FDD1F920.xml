<collectingCountry box="[359,426,1590,1616]" name="China" pageId="7" pageNumber="78">China</collectingCountry>
(
<collectingRegion box="[441,526,1590,1616]" country="China" name="Hainan" pageId="7" pageNumber="78">Hainan</collectingRegion>
, 
