, and 
<taxonomicName authority="Hutchings &amp; Glasby, 1988" authorityName="Hutchings &amp; Glasby" authorityYear="1988" box="[492,1009,1015,1040]" class="Polychaeta" family="Terebellidae" genus="Lanicides" kingdom="Animalia" order="Terebellida" pageId="42" pageNumber="526" phylum="Annelida" rank="species" species="tribranchiata">
