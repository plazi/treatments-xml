<figureCitation box="[592,694,367,392]" captionStart="FIGURES 232 – 235" captionStartId="77.[151,269,1963,1987]" captionTargetBox="[195,1375,234,1949]" captionTargetId="figure@77.[194,1391,194,1954]" captionTargetPageId="77" captionText="FIGURES 232 – 235. Insula tumida sp. nov., male holotype and female paratype. 232, palp, ventral view; 233, palp, retrolateral view; 234, epigyne; 235, vulva. Scale bars: 0.1 mm." httpUri="https://zenodo.org/record/284797/files/figure.png" pageId="18" pageNumber="19">
Figs 
<number box="[649,694,367,392]" pageId="18" pageNumber="19" value="235241.0">235</number>
</figureCitation>
, 
<figureCitation box="[704,750,368,392]" captionStart="FIGURES 240 – 244" captionStartId="79.[151,269,1961,1985]" captionTargetBox="[200,1397,194,1948]" captionTargetId="figure@79.[200,1397,194,1954]" captionTargetPageId="79" captionText="FIGURES 240 – 244. Insula tumida sp. nov., male holotype and female paratype. 240, epigyne; 241, vulva; 242, male habitus, dorsal view; 243, female habitus, dorsal view; 244, female habitus, ventral view." httpUri="https://zenodo.org/record/284799/files/figure.png" pageId="18" pageNumber="19">241</figureCitation>
