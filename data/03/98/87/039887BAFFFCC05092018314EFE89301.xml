<taxonomicName authorityName="Seago" authorityYear="2009" box="[777,881,1205,1224]" class="Insecta" family="Leiodidae" genus="Agyrtodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="globosus" status="new species">
