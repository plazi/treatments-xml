<paragraph blockId="28.[175,1005,1205,1249]" box="[175,628,1229,1249]" pageId="28" pageNumber="29">
<taxonomicName authorityName="Zwick" authorityYear="1979" box="[175,266,1230,1249]" class="Insecta" family="Leiodidae" genus="Agyrtodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="crassus">
<emphasis box="[175,266,1230,1249]" italics="true" pageId="28" pageNumber="29">A. crassus</emphasis>
