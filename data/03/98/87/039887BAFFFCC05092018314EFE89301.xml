<taxonomicName authorityName="Zwick" authorityYear="1979" box="[279,387,1230,1249]" class="Insecta" family="Leiodidae" genus="Agyrtodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="eucalypti">
<emphasis box="[279,387,1230,1249]" italics="true" pageId="28" pageNumber="29">A. eucalypti</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Seago" authorityYear="2009" box="[400,504,1230,1249]" class="Insecta" family="Leiodidae" genus="Agyrtodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="flaviceps" status="new species">
<emphasis box="[400,504,1230,1249]" italics="true" pageId="28" pageNumber="29">A. flaviceps</emphasis>
