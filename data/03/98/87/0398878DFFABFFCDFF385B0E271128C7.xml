<quantity box="[489,623,1729,1756]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" metricValueMax="7.5" metricValueMin="6.5" pageId="6" pageNumber="575" unit="mm" value="7.0" valueMax="7.5" valueMin="6.5">6.5–7.5 mm</quantity>
