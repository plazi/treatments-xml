<taxonomicName class="Malacostraca" family="Hyalidae" genus="Insula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="scutata" status="sp. nov.">
<emphasis italics="true" pageId="11" pageNumber="12">I. scutata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[240,330,1096,1121]" pageId="11" pageNumber="12">
<taxonomicNameLabel box="[240,330,1096,1121]" pageId="11" pageNumber="12" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<figureCitation box="[345,447,1096,1121]" captionStart="FIGURES 206 – 209" captionStartId="71.[151,269,1965,1989]" captionTargetBox="[211,1378,196,1949]" captionTargetId="figure@71.[196,1393,194,1954]" captionTargetPageId="71" captionText="FIGURES 206 – 209. Insula scutata sp. nov., male holotype and female paratype. 206, palp, ventral view; 207, palp, retrolateral view; 208, epigyne; 209, vulva. Scale bars: 0.1 mm." httpUri="https://zenodo.org/record/284791/files/figure.png" pageId="11" pageNumber="12">
Figs 
<number box="[402,447,1096,1120]" pageId="11" pageNumber="12" value="208214.0">208</number>
