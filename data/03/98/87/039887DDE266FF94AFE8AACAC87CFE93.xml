<figureCitation box="[837,940,1761,1786]" captionStart="FIGURES 148 – 151" captionStartId="57.[151,269,1967,1991]" captionTargetBox="[198,1382,200,1949]" captionTargetId="figure@57.[185,1401,194,1954]" captionTargetPageId="57" captionText="FIGURES 148 – 151. Insula longa sp. nov., male holotype and female paratype. 148, palp, ventral view; 149, palp, retrolateral view; 150, epigyne; 151, vulva. Scale bars: 0.1 mm." httpUri="https://zenodo.org/record/284777/files/figure.png" pageId="12" pageNumber="13">
Figs 
<number box="[896,940,1762,1786]" pageId="12" pageNumber="13" value="148152.0">148</number>
</figureCitation>
, 
<figureCitation box="[953,1056,1761,1786]" captionStart="FIGURES 152 – 155" captionStartId="58.[151,269,1971,1995]" captionTargetBox="[199,1384,216,1954]" captionTargetId="figure@58.[194,1391,194,1954]" captionTargetPageId="58" captionText="FIGURES 152 – 155. Insula longa sp. nov., male holotype. 152, palp, prolateral view; 153, palp, retrolateral view; 154, palp, ventral view; 155, bulbus, prolateral view." httpUri="https://zenodo.org/record/284778/files/figure.png" pageId="12" pageNumber="13">
152– 
<number box="[1013,1056,1761,1786]" pageId="12" pageNumber="13" value="155.0">155</number>
</figureCitation>
