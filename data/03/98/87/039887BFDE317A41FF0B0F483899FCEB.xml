<paragraph blockId="17.[204,1437,1726,1820]" pageId="17" pageNumber="468">
<emphasis bold="true" box="[205,343,1727,1750]" pageId="17" pageNumber="468">DESCRIPTION</emphasis>
— MALE: Body length 
<quantity box="[591,712,1727,1751]" metricMagnitude="-3" metricUnit="m" metricValue="3.55" metricValueMax="3.7" metricValueMin="3.4" pageId="17" pageNumber="468" unit="mm" value="3.55" valueMax="3.7" valueMin="3.4">3.4-3.7 mm</quantity>
