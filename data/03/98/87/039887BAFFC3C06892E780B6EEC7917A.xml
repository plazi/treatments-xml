<quantity box="[346,470,1198,1220]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" metricValueMax="2.4" metricValueMin="2.1" pageId="36" pageNumber="37" unit="mm" value="2.25" valueMax="2.4" valueMin="2.1">2.1–2.4 mm</quantity>
