<paragraph blockId="4.[151,464,1173,1200]" box="[151,464,1173,1200]" pageId="4">
<heading bold="true" box="[151,464,1173,1200]" fontSize="11" level="1" pageId="4" reason="1">
<taxonomicName authority="Rondani, 1856" authorityName="Rondani" authorityYear="1856" box="[151,464,1173,1200]" class="Insecta" family="Carnidae" genus="Meoneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" phylum="Arthropoda" rank="genus">
