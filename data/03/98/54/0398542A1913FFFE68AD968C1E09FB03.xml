<paragraph blockId="45.[128,654,362,1158]" pageId="45" pageNumber="45">
<materialsCitation ID-GBIF-Occurrence="3059913685" collectingDate="1908-08-24" collectingDateMax="1912-04-02" collectingDateMin="1908-08-24" collectionCode="AMNH" collectorName="J. P. Rogers &amp; Other Mathews" country="Australia" county="Parry Creek" location="In" municipality="The" pageId="45" pageNumber="45" specimenCode="AMNH 671194, 671195, 671198-671201" specimenCount="2" typeStatus="holotype">
