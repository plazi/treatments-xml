<paragraph blockId="126.[93,620,1291,1759]" lastBlockId="126.[667,1193,213,621]" pageId="126" pageNumber="126">
<materialsCitation ID-GBIF-Occurrence="3059913784" collectingDate="1904-07-05" collectingDateMax="1904-07-07" collectingDateMin="1904-07-05" collectionCode="AMNH" county="In" location="Stresemann" municipality="Rothschild Collection" pageId="126" pageNumber="126" specimenCode="AMNH 674322- 674324" specimenCount="23" stateProvince="Okinawa">
