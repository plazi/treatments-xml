, female, 
<date box="[413,615,1410,1432]" pageId="104" pageNumber="104" value="1954-03-22">
<collectingDate box="[413,615,1410,1432]" pageId="104" pageNumber="104" value="1954-03-22">22 March 1954</collectingDate>
</date>
. 
<collectorName box="[93,293,1440,1462]" pageId="104" pageNumber="104">Because Gilliard</collectorName>
