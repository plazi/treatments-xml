<location LSID="urn:lsid:plazi:treatment:0398542A1916FFFA6AB390DB1B27FE5F:8EEEB3E71916FFFA6AFC93EB1E2CFC7D" box="[128,269,997,1019]" country="Solomon Islands" county="Hartert's" municipality="All" name="Fortunately" pageId="41" pageNumber="41" stateProvince="Guadalcanal">Fortunately</location>
, 
<location LSID="urn:lsid:plazi:treatment:0398542A1916FFFA6AB390DB1B27FE5F:8EEEB3E71916FFFA6B6193E81E55FC7D" box="[285,372,998,1019]" country="Solomon Islands" county="Hartert's" municipality="All" name="Hartert" pageId="41" pageNumber="41" stateProvince="Guadalcanal">Hartert</location>
