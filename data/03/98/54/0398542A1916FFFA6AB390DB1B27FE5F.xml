<collectingRegion box="[128,280,909,931]" country="Solomon Islands" name="Guadalcanal" pageId="41" pageNumber="41">Guadalcanal</collectingRegion>
. 
<location LSID="urn:lsid:plazi:treatment:0398542A1916FFFA6AB390DB1B27FE5F:8EEEB3E71916FFFA6B5793831E6FFC24" box="[299,334,909,930]" country="Solomon Islands" county="Hartert's" municipality="All" name="He" pageId="41" pageNumber="41" stateProvince="Guadalcanal">He</location>
