: 
<emphasis bold="true" box="[831,1023,283,304]" pageId="26" pageNumber="26">AMNH 725957</emphasis>
, adult male, collected at Lado, 
<geoCoordinate box="[909,997,313,335]" degrees="05.10" direction="north" orientation="latitude" pageId="26" pageNumber="26" precision="555" value="5.1">05.10N</geoCoordinate>
, 
<geoCoordinate box="[1017,1097,313,334]" degrees="31.32" direction="east" orientation="longitude" pageId="26" pageNumber="26" precision="555" value="31.32">31.32E</geoCoordinate>
(Times 
<taxonomicName box="[667,732,342,364]" class="Mollusca (awaiting allocation)" family="Mollusca (awaiting allocation)" genus="Atlas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca (awaiting allocation)" pageId="26" pageNumber="3" phylum="Mollusca" rank="genus">Atlas</taxonomicName>
), 
<collectingCountry box="[757,834,342,364]" name="Sudan" pageId="26" pageNumber="26">Sudan</collectingCountry>
, on 
<date box="[893,1073,343,364]" pageId="26" pageNumber="26" value="1879-08-07">7 August 1879</date>
