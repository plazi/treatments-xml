<collectionCode box="[423,516,1737,1758]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="55" pageNumber="55">AMNH</collectionCode>
