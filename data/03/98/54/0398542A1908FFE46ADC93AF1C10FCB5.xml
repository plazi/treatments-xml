, 
<date box="[202,342,1708,1730]" pageId="55" pageNumber="55" value="1888-04">
<collectingDate box="[202,342,1708,1730]" pageId="55" pageNumber="55" value="1888-04">April 1888</collectingDate>
</date>
, 
<collectorName box="[366,451,1708,1729]" pageId="55" pageNumber="55">Bourns</collectorName>
and 
<collectorName box="[535,653,1708,1729]" pageId="55" pageNumber="55">Worcester</collectorName>
