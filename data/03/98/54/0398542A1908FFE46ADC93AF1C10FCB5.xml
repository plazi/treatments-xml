; no locality, 
<collectingRegion box="[520,598,1532,1553]" country="Philippines" name="Samar" pageId="55" pageNumber="55">Samar</collectingRegion>
, one adult male [?]; 
<materialsCitation ID-GBIF-Occurrence="3059913863" collectingDate="1888-04" collectionCode="AMNH, USNM" collectorName="Bourns &amp; Worcester" location="Leyte" pageId="55" pageNumber="55" specimenCode="AMNH 69043" specimenCount="1" stateProvince="Southern Leyte" typeStatus="paratype">
