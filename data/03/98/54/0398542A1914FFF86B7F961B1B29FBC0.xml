<emphasis bold="true" box="[701,889,568,589]" pageId="43" pageNumber="43">
<specimenCode box="[701,889,568,589]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="43" pageNumber="43">AMNH 671112</specimenCode>
</emphasis>
(Kühn no. 5601), 
<emphasis bold="true" pageId="43" pageNumber="43">
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="43" pageNumber="43">AMNH 671113</specimenCode>
</emphasis>
(5600), males, 29 September
</materialsCitation>
; 
<materialsCitation ID-GBIF-Occurrence="3059913837" collectionCode="AMNH" pageId="43" pageNumber="43" specimenCode="AMNH 671114, AMNH 671115" specimenCount="1" typeStatus="holotype">
<emphasis bold="true" pageId="43" pageNumber="43">
