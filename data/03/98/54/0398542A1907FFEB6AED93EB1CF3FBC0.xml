<collectionCode box="[980,1077,213,234]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="56" pageNumber="56">AMNH</collectionCode>
