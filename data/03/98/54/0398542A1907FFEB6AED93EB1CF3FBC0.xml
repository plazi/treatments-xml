label, a 
<location LSID="urn:lsid:plazi:treatment:0398542A1907FFEB6AED93EB1CF3FBC0:8EEEB3E71907FFEB6B2D963A1EF0F9CC" box="[337,465,1588,1610]" county="In" municipality="Mathews" name="Rothschild" pageId="56" pageNumber="56" stateProvince="Northern Territory">Rothschild</location>
label, 
<location LSID="urn:lsid:plazi:treatment:0398542A1907FFEB6AED93EB1CF3FBC0:8EEEB3E71907FFEB685D963A1FA6F9EE" county="In" municipality="Mathews" name="Mathews" pageId="56" pageNumber="56" stateProvince="Northern Territory">Mathews</location>
and 
<location LSID="urn:lsid:plazi:treatment:0398542A1907FFEB6AED93EB1CF3FBC0:8EEEB3E71907FFEB6AA3965C1E7FF9EE" box="[223,350,1618,1640]" county="In" municipality="Mathews" name="Rothschild" pageId="56" pageNumber="56" stateProvince="Northern Territory">Rothschild</location>
