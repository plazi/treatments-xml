<emphasis box="[419,494,1528,1551]" italics="true" pageId="56" pageNumber="56">baileyi</emphasis>
as 
<collectingRegion country="Australia" name="Northern Territory" pageId="56" pageNumber="56">Northern Territory</collectingRegion>
