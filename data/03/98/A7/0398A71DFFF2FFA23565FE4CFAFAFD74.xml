<paragraph blockId="1.[264,508,483,510]" box="[264,508,483,510]" pageId="1" pageNumber="2">
<heading bold="true" box="[264,508,483,510]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<emphasis bold="true" box="[264,508,483,510]" pageId="1" pageNumber="2">
<taxonomicName box="[264,370,484,510]" class="Insecta" family="Phoridae" genus="Tapantia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[264,370,484,510]" italics="true" pageId="1" pageNumber="2">Tapantia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[378,508,483,509]" pageId="1" pageNumber="2" rank="genus">new genus</taxonomicNameLabel>
