<collectionCode LSID="urn:lsid:biocol.org:col:34802" box="[1173,1258,732,758]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34802" name="Natural History Museum of Los Angeles County" pageId="6" pageNumber="7">LACM</collectionCode>
<collectionCode LSID="urn:lsid:biocol.org:col:13589" box="[1265,1322,732,758]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13589" name="Ministry of Natural Resources" pageId="6" pageNumber="7">ENT</collectionCode>
<number box="[264,358,772,798]" pageId="6" pageNumber="7" value="180789.0">
