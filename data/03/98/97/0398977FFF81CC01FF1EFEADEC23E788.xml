(
<typeStatus box="[287,337,296,320]" pageId="2" pageNumber="355">type</typeStatus>
species 
<taxonomicName authority="Regan" authorityName="Regan" box="[432,754,295,320]" class="Actinopterygii" family="Cichlidae" genus="Tropheops" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="355" phylum="Chordata" rank="species" species="tropheops">
