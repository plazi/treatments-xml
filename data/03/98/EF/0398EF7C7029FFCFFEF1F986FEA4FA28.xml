<collectingCountry box="[586,679,1004,1030]" name="Zambia" pageId="3" pageNumber="4">Zambia</collectingCountry>
, 
<collectingCountry box="[691,819,1004,1030]" name="Zimbabwe" pageId="3" pageNumber="4">Zimbabwe</collectingCountry>
, 
<collectingCountry box="[831,939,1004,1030]" name="Tanzania" pageId="3" pageNumber="4">Tanzania</collectingCountry>
, 
<collectingCountry box="[952,1032,1004,1030]" name="Kenya" pageId="3" pageNumber="4">Kenya</collectingCountry>
, 
<collectingCountry box="[1044,1198,1004,1030]" name="South Africa" pageId="3" pageNumber="4">South Africa</collectingCountry>
, 
<collectingCountry box="[1210,1318,1004,1030]" name="Tanzania" pageId="3" pageNumber="4">Zanzibar</collectingCountry>
