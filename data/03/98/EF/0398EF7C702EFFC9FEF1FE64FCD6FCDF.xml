<number box="[355,414,759,785]" pageId="5" pageNumber="6" value="1958.0">1958</number>
</bibRefCitation>
</taxonomicName>
with 
<taxonomicName authority="Olave,1953" authorityName="Olave" authorityYear="1953" box="[487,926,759,785]" class="Insecta" family="Buprestidae" genus="Cylindrophora" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Ascomycota" rank="species" species="iricolor">
