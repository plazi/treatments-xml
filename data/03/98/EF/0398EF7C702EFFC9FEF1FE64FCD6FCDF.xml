<taxonomicName authority="Obenberger, 1958" authorityName="Obenberger" authorityYear="1958" box="[990,1323,759,785]" class="Insecta" family="Buprestidae" genus="Brasilaxia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="jacobi">
