species:
</emphasis>
<taxonomicName authority="O. Pickard-Cambridge, 1871" authorityName="O. Pickard-Cambridge" authorityYear="1871" class="Arachnida" family="Linyphiidae" genus="Linyphia" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="204" phylum="Arthropoda" rank="species" species="pallida">
