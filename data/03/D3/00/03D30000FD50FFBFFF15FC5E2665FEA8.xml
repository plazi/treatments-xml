<subSubSection pageId="7" pageNumber="367" type="distribution">
<paragraph blockId="7.[151,1436,152,287]" box="[199,788,188,215]" pageId="7" pageNumber="367">
<emphasis bold="true" box="[199,353,188,215]" pageId="7" pageNumber="367">Distribution.</emphasis>
<collectingCountry box="[359,426,189,215]" name="China" pageId="7" pageNumber="367">China</collectingCountry>
(
<collectingRegion box="[441,542,189,215]" country="China" name="Guangxi" pageId="7" pageNumber="367">Guangxi</collectingRegion>
, 
