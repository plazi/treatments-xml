<taxonomicName authority="Coutiere, 1909" authorityName="Coutiere" authorityYear="1909" class="Malacostraca" family="Alpheidae" genus="Synalpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="19" pageNumber="350" phylum="Arthropoda" rank="subSpecies" species="latastei" subSpecies="tenuispina">
