(Col. 
<taxonomicName box="[1229,1398,498,524]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="310" pageNumber="311" phylum="Arthropoda" rank="family">Staphylinidae</taxonomicName>
