(Col. 
<taxonomicName box="[591,752,1998,2024]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="309" pageNumber="310" phylum="Arthropoda" rank="family">Staphylinidae</taxonomicName>
<taxonomicName authorityName="Fleming" authorityYear="1821" box="[760,891,1998,2025]" class="Insecta" family="Oxytelinae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="309" pageNumber="310" phylum="Arthropoda" rank="family">Oxytelinae</taxonomicName>
