<emphasis bold="true" box="[1014,1105,447,473]" pageId="79" pageNumber="80">Europe</emphasis>
(
<collectingCountry box="[1120,1189,447,473]" name="Spain" pageId="79" pageNumber="80">Spain</collectingCountry>
, 
<collectingCountry box="[1199,1395,447,473]" name="United Kingdom" pageId="79" pageNumber="80">United Kingdom</collectingCountry>
, 
<collectingCountry box="[189,271,483,509]" name="France" pageId="79" pageNumber="80">France</collectingCountry>
, 
<collectingCountry box="[280,334,482,508]" name="Italy" pageId="79" pageNumber="80">Italy</collectingCountry>
, 
<collectingCountry box="[344,453,483,509]" name="Germany" pageId="79" pageNumber="80">Germany</collectingCountry>
, 
<collectingCountry box="[463,546,482,508]" name="Poland" pageId="79" pageNumber="80">Poland</collectingCountry>
, 
<collectingCountry box="[556,647,482,508]" name="Finland" pageId="79" pageNumber="80">Finland</collectingCountry>
, 
<collectingCountry box="[657,738,482,509]" name="Russia" pageId="79" pageNumber="80">Russia</collectingCountry>
), 
<emphasis bold="true" box="[756,835,482,508]" pageId="79" pageNumber="80">Africa</emphasis>
(
<collectingCountry box="[848,940,482,509]" name="Algeria" pageId="79" pageNumber="80">Algeria</collectingCountry>
), 
<emphasis bold="true" box="[959,1014,482,508]" pageId="79" pageNumber="80">Asia</emphasis>
(
<collectingCountry box="[1027,1145,482,509]" name="Sri Lanka" pageId="79" pageNumber="80">Sri Lanka</collectingCountry>
), and 
<emphasis bold="true" box="[1210,1316,482,508]" pageId="79" pageNumber="80">America</emphasis>
(
<collectingCountry box="[1329,1390,483,509]" name="United States of America" pageId="79" pageNumber="80">USA</collectingCountry>
) (
<bibRefCitation author="Majewski T." box="[199,376,265,291]" pageId="80" pageNumber="81" pagination="1 - 240" refId="ref217228" refString="Majewski T. 2008. Laboulbeniales. In: Chlebicki A. (ed.) Atlas of the Geographical Distribution of Fungi in Poland 4: 1 - 240. W. Szafer Institute of Botany, Polish Academy of Sciences, Krakow." type="journal article" year="2008">Majewski 2008</bibRefCitation>
). Reported later from 
<collectingCountry box="[627,718,265,291]" name="Sweden" pageId="80" pageNumber="81">Sweden</collectingCountry>
(
<bibRefCitation author="Huggert L." box="[733,891,265,291]" pageId="80" pageNumber="81" refId="ref214370" refString="Huggert L. 2010. Laboulbeniales i Sverige. Ove Eriksson (ed.). Umea University, Umea." type="book" year="2010">Huggert 2010</bibRefCitation>
), 
<collectingCountry box="[913,1012,265,291]" name="Bulgaria" pageId="80" pageNumber="81">Bulgaria</collectingCountry>
(
<bibRefCitation author="Rossi W. &amp; Gueorguiev B. &amp; Georgiev G. &amp; Stoianova D." box="[1027,1228,265,291]" pageId="80" pageNumber="81" pagination="48 - 59" refId="ref219483" refString="Rossi W., Gueorguiev B., Georgiev G. &amp; Stoianova D. 2019 a. Laboulbeniales (Ascomycota) from Bulgaria and other countries. Plant Biosystems 153 (1): 48 - 59. https: // doi. org / 10.1080 / 11263504.2018.1454531" type="journal article" year="2019">
Rossi 
<emphasis box="[1096,1152,265,291]" italics="true" pageId="80" pageNumber="81">et al.</emphasis>
2019a
</bibRefCitation>
), and 
<collectingCountry box="[1298,1399,265,291]" name="Hungary" pageId="80" pageNumber="81">Hungary</collectingCountry>
(
<bibRefCitation author="Rossi W. &amp; Christian E." box="[199,473,302,328]" pageId="80" pageNumber="81" pagination="149 - 161" refId="ref218957" refString="Rossi W. &amp; Christian E. 2020. Laboulbeniales (Ascomycota) from Austria and neighbouring areas. Sydowia 72: 149 - 161." type="journal article" year="2020">Rossi &amp; Christian 2020</bibRefCitation>
). The record of 
<taxonomicName authorityName="Thaxt. (Thaxter" authorityYear="1894" box="[659,816,302,328]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Teratomyces" kingdom="Fungi" order="Laboulbeniales" pageId="80" pageNumber="81" phylum="Ascomycota" rank="species" species="brevicaulis">
<emphasis box="[659,816,302,328]" italics="true" pageId="80" pageNumber="81">T. brevicaulis</emphasis>
</taxonomicName>
