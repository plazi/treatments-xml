<typeStatus box="[1106,1162,1730,1756]" pageId="275" pageNumber="276">type</typeStatus>
), 
<collectingCountry box="[1182,1249,1730,1756]" name="Spain" pageId="275" pageNumber="276">Spain</collectingCountry>
