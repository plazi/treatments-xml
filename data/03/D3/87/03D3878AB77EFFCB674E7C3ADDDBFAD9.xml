<collectingCountry box="[346,427,751,777]" name="Poland" pageId="47" pageNumber="48">Poland</collectingCountry>
(
<typeStatus box="[443,497,752,778]" pageId="47" pageNumber="48">type</typeStatus>
) and Spain on 
<taxonomicName authorityName="Leach" authorityYear="1815" box="[670,792,751,777]" class="Insecta" family="Hydraenidae" genus="Limnebius" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis box="[670,792,751,777]" italics="true" pageId="47" pageNumber="48">Limnebius</emphasis>
</taxonomicName>
(Col. 
<taxonomicName authorityName="Mulsant" authorityYear="1844" box="[866,1018,751,778]" class="Insecta" family="Hydraenidae" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="family">Hydraenidae</taxonomicName>
) (
<bibRefCitation author="Santamaria S." box="[1042,1252,751,778]" pageId="47" pageNumber="48" pagination="375 - 389" refId="ref220345" refString="Santamaria S. 2001 a. New and interesting Laboulbeniales (Fungi, Ascomycota) from Spain, IV. Nova Hedwigia 72 (3 - 4): 375 - 389. https: // doi. org / 10.1127 / nova. hedwigia / 72 / 2001 / 375" type="journal article" year="2001">Santamaria 2001a</bibRefCitation>
