</taxonomicName>
and relatives like 
<taxonomicName authority="Mulsant, 1850" authorityName="Mulsant" authorityYear="1850" class="Insecta" family="Coccinellidae" genus="Diomus" kingdom="Animalia" order="Coleoptera" pageId="238" pageNumber="239" phylum="Arthropoda" rank="genus">
