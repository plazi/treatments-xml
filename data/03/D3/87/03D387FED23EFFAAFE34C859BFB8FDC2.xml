<collectorName box="[419,532,906,930]" pageId="5" pageNumber="800">Station Z</collectorName>
, Long Kei Wan, off the mouth of 
<location LSID="urn:lsid:plazi:treatment:03D387FED23EFFAAFE34C859BFB8FDC2:8EA56033D23EFFA9FC4EC8C2BBE7FC51" country="Hong Kong" latitude="114.0" name="Conic Island Cave" pageId="5" pageNumber="800">Conic Island Cave</location>
, 
<collectingCountry box="[218,357,939,963]" name="Hong Kong" pageId="5" pageNumber="800">Hong Kong</collectingCountry>
, 22 
<emphasis bold="true" box="[396,406,939,963]" italics="true" pageId="5" pageNumber="800">°</emphasis>
229030 
<geoCoordinate box="[477,557,939,963]" degrees="114" direction="north" orientation="latitude" pageId="5" pageNumber="800" precision="55555" value="114.0">
N 114 
<emphasis bold="true" box="[547,557,939,963]" italics="true" pageId="5" pageNumber="800">°</emphasis>
</geoCoordinate>
229080E; sediment 1% gravel, 96% sand, 3% silt/ clay, depth 
<quantity box="[281,358,971,995]" metricMagnitude="1" metricUnit="m" metricValue="1.3699999999999999" pageId="5" pageNumber="800" unit="m" value="13.7">
<elevation box="[281,358,971,995]" metricMagnitude="1" metricUnit="m" metricValue="1.3699999999999999" pageId="5" pageNumber="800" unit="m" value="13.7">13.7 m</elevation>
