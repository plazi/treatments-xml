<subSubSection pageId="29" pageNumber="30" type="diagnosis">
<paragraph blockId="29.[189,428,864,890]" box="[189,428,864,890]" pageId="29" pageNumber="30">
<heading bold="true" box="[189,428,864,890]" fontSize="11" level="3" pageId="29" pageNumber="30" reason="3">
