<emphasis box="[697,986,832,857]" italics="true" pageId="10" pageNumber="11">
<taxonomicName box="[697,847,832,857]" class="Insecta" family="Scelionidae" genus="Heptascelio" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="watshami">H. watshami</taxonomicName>
, 
<taxonomicName box="[858,981,832,857]" class="Insecta" family="Scelionidae" genus="Heptascelio" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="albipes">H. albipes</taxonomicName>
,
</emphasis>
and 
<taxonomicName box="[1041,1180,832,857]" class="Insecta" family="Scelionidae" genus="Heptascelio" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="strigatus">
<emphasis box="[1041,1180,832,857]" italics="true" pageId="10" pageNumber="11">H. strigatus</emphasis>
