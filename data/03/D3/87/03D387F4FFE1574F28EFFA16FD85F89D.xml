<collectingCountry name="Cameroon" pageId="1" pageNumber="2">Cameroon</collectingCountry>
, phoretic on 
<taxonomicName box="[364,511,1744,1769]" class="Insecta" family="Phlaeothripidae" genus="Bactrothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis box="[364,511,1744,1769]" italics="true" pageId="1" pageNumber="2">Bactrothrips</emphasis>
</taxonomicName>
sp. (Insecta: 
<taxonomicName box="[671,833,1744,1770]" class="Insecta" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="order">Thysanoptera</taxonomicName>
) from dead leaves of 
<taxonomicName box="[1094,1300,1744,1769]" class="Magnoliopsida" family="Anacardiaceae" genus="Mangifera" kingdom="Plantae" order="Sapindales" pageId="1" pageNumber="2" phylum="Tracheophyta" rank="species" species="indica">
<emphasis box="[1094,1300,1744,1769]" italics="true" pageId="1" pageNumber="2">Mangifera indica</emphasis>
</taxonomicName>
(
<taxonomicName class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="1" pageNumber="2" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
