<number box="[1102,1116,848,874]" pageId="13" pageNumber="14" value="9.0">9</number>
A: 
<number box="[1142,1155,848,874]" pageId="13" pageNumber="14" value="5.0">5</number>
D/JV- 
<number box="[1231,1244,848,874]" pageId="13" pageNumber="14" value="3.0">3</number>
:ZV- 
<number box="[1300,1313,848,874]" pageId="13" pageNumber="14" value="3.0">3</number>
for 
<taxonomicName authority="Evans" authorityName="Evans" class="Arachnida" family="Phytoseiidae" genus="Phytoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="longipes">
<emphasis italics="true" pageId="13" pageNumber="14">P. longipes</emphasis>
Evans
</taxonomicName>
, 
<number box="[319,347,888,914]" pageId="13" pageNumber="14" value="10.0">10</number>
A: 
<number box="[373,387,888,914]" pageId="13" pageNumber="14" value="6.0">6</number>
E/JV- 
<number box="[459,497,888,914]" pageId="13" pageNumber="14" value="3.4">3,4</number>
:ZV- 
<number box="[551,564,888,914]" pageId="13" pageNumber="14" value="3.0">3</number>
