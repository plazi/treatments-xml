</taxonomicName>
and 
<taxonomicName authorityName="Reitter" authorityYear="1891" box="[189,355,348,374]" class="Insecta" family="Staphylinidae" genus="Plectophloeus" kingdom="Animalia" order="Coleoptera" pageId="233" pageNumber="234" phylum="Arthropoda" rank="genus">
<emphasis box="[189,355,348,374]" italics="true" pageId="233" pageNumber="234">Plectophloeus</emphasis>
</taxonomicName>
(Col. 
<taxonomicName box="[430,591,348,374]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="233" pageNumber="234" phylum="Arthropoda" rank="family">Staphylinidae</taxonomicName>
