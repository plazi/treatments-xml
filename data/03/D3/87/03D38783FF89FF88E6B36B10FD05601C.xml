</taxonomicName>
, 
<taxonomicName authority="(Simon, 1898)" authorityName="Simon" authorityYear="1898" baseAuthorityName="Simon" baseAuthorityYear="1898" box="[1136,1431,1268,1293]" class="Arachnida" family="Zoropsidae" genus="Devendra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="65" phylum="Arthropoda" rank="species" species="seriatus">
