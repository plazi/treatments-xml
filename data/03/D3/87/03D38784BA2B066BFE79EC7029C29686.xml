<paragraph blockId="1.[264,851,359,385]" box="[264,851,359,385]" pageId="1" pageNumber="2">
<typeStatus box="[264,322,359,385]" pageId="1" pageNumber="2">Type</typeStatus>
species: 
<taxonomicName box="[430,695,359,384]" class="Insecta" family="Miridae" genus="Sulawesifulvius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="schuhi" status="new species">
