<emphasis bold="true" box="[530,551,1857,1883]" pageId="54" pageNumber="55">A</emphasis>
</taxonomicName>
. 
<taxonomicName authority="(Maire) I. I. Tav. Paired" authorityName="I. I. Tav. Paired" baseAuthorityName="Maire" box="[567,987,1857,1884]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Euphoriomyces" kingdom="Fungi" order="Laboulbeniales" pageId="54" pageNumber="55" phylum="Ascomycota" rank="species" species="agathidii">
