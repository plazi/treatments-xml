<emphasis bold="true" box="[768,860,386,412]" pageId="177" pageNumber="178">Europe</emphasis>
: 
<collectingCountry box="[878,933,386,412]" name="Italy" pageId="177" pageNumber="178">Italy</collectingCountry>
, 
<collectingCountry box="[945,1028,387,413]" name="France" pageId="177" pageNumber="178">France</collectingCountry>
