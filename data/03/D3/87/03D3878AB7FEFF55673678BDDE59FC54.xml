<subSubSection pageId="177" pageNumber="178" type="distribution">
<paragraph blockId="177.[189,468,264,291]" box="[189,468,264,291]" pageId="177" pageNumber="178">
<heading bold="true" box="[189,468,264,291]" fontSize="11" level="3" pageId="177" pageNumber="178" reason="3">
<emphasis bold="true" box="[189,468,264,291]" pageId="177" pageNumber="178">Distribution and hosts</emphasis>
</heading>
</paragraph>
<paragraph blockId="177.[189,1399,313,560]" pageId="177" pageNumber="178">
Mostly on 
<taxonomicName box="[315,581,313,339]" class="Insecta" family="Carabidae" genus="Diachromus" kingdom="Animalia" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="species" species="germanus">
<emphasis box="[315,581,313,339]" italics="true" pageId="177" pageNumber="178">Diachromus germanus</emphasis>
</taxonomicName>
(Col. 
<taxonomicName authorityName="Latreille" authorityYear="1802" box="[655,779,313,340]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="family">Carabidae</taxonomicName>
), but also reported on other carabids belonging to the 
<taxonomicName authorityName="Bonelli" authorityYear="1810" box="[189,318,349,375]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="subFamily" subFamily="Harpalinae">Harpalinae</taxonomicName>
like 
<taxonomicName baseAuthorityName="Hoess &amp; Senn-Irlet" baseAuthorityYear="2009" box="[377,541,349,375]" class="Insecta" family="Carabidae" genus="Anisodactylus" kingdom="Animalia" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="genus">
<emphasis box="[377,541,349,375]" italics="true" pageId="177" pageNumber="178">Anisodactylus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Dejean" authorityYear="1821" box="[556,662,349,375]" class="Insecta" family="Carabidae" genus="Ophonus" kingdom="Animalia" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="genus">
<emphasis box="[556,662,349,375]" italics="true" pageId="177" pageNumber="178">Ophonus</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Motschulsky" authorityYear="1844" box="[719,890,349,375]" class="Insecta" family="Carabidae" genus="Pseudophonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="genus">
<emphasis box="[719,890,349,375]" italics="true" pageId="177" pageNumber="178">Pseudophonus</emphasis>
