<collectingCountry box="[373,488,1848,1874]" name="Indonesia" pageId="255" pageNumber="256">Indonesia</collectingCountry>
(
<typeStatus box="[506,562,1848,1874]" pageId="255" pageNumber="256">type</typeStatus>
