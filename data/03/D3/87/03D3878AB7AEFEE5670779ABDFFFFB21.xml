and 
<taxonomicName authorityName="Hulden (Hulden" authorityYear="1983" box="[236,389,1923,1949]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Stigmatomyces" kingdom="Fungi" order="Laboulbeniales" pageId="255" pageNumber="256" phylum="Ascomycota" rank="species" species="chthonicus">
