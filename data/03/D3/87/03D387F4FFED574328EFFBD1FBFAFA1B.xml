, 
<collectingCountry box="[665,732,1442,1468]" name="Chile" pageId="13" pageNumber="14">Chile</collectingCountry>
, 
<collectingCountry box="[743,894,1442,1468]" name="South Africa" pageId="13" pageNumber="14">South Africa</collectingCountry>
and 
<collectingCountry box="[951,1080,1442,1468]" name="Zimbabwe" pageId="13" pageNumber="14">Zimbabwe</collectingCountry>
.
</paragraph>
