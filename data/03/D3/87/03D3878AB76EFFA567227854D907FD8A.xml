<subSubSection box="[189,506,264,291]" pageId="65" pageNumber="66" type="multiple">
<paragraph blockId="65.[189,506,264,291]" box="[189,506,264,291]" pageId="65" pageNumber="66">
<heading bold="true" box="[189,506,264,291]" fontSize="11" level="3" pageId="65" pageNumber="66" reason="3">
<emphasis bold="true" box="[189,506,264,291]" pageId="65" pageNumber="66">Key to the Danish species</emphasis>
