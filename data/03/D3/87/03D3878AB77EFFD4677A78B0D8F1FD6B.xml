) in 
<collectingCountry box="[962,1032,312,338]" name="Spain" pageId="48" pageNumber="49">Spain</collectingCountry>
, 
<collectingCountry box="[1043,1153,313,339]" name="Germany" pageId="48" pageNumber="49">Germany</collectingCountry>
, 
<collectingCountry box="[1165,1219,312,338]" name="Italy" pageId="48" pageNumber="49">Italy</collectingCountry>
