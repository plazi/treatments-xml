<taxonomicName authorityName="Dejean" authorityYear="1833" box="[382,473,592,618]" class="Insecta" family="Staphylinidae" genus="Astenus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
<emphasis box="[382,473,592,618]" italics="true" pageId="41" pageNumber="42">Astenus</emphasis>
</taxonomicName>
(Col. 
<taxonomicName box="[547,710,591,618]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="family">Staphylinidae</taxonomicName>
) ......................................... 
<taxonomicName authority="(Thaxt.) Thaxt." authorityName="Thaxt." baseAuthorityName="Thaxt." box="[1037,1399,591,618]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Compsomyces" kingdom="Fungi" order="Laboulbeniales" pageId="41" pageNumber="42" phylum="Ascomycota" rank="species" species="verticillatus">
<emphasis bold="true" box="[1037,1213,591,618]" italics="true" pageId="41" pageNumber="42">C. verticillatus</emphasis>
(Thaxt.) Thaxt.
