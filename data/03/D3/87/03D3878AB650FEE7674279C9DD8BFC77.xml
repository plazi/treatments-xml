</taxonomicName>
(
<taxonomicName box="[704,795,358,384]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Diptera" pageId="259" pageNumber="260" phylum="Tracheophyta" rank="order">Diptera</taxonomicName>
