<taxonomicName authority="Oliveira¹ &amp; Cipola² &amp; Almeida¹, 2020" authorityName="Oliveira¹ &amp; Cipola² &amp; Almeida¹" authorityYear="2020" box="[333,440,464,490]" class="Entognatha" family="Paronellidae" genus="Salina" kingdom="Animalia" order="Collembola" pageId="35" pageNumber="116" phylum="Arthropoda" rank="species" species="serrana" status="sp.n.">
<emphasis box="[333,440,464,490]" italics="true" pageId="35" pageNumber="116">S. serrana</emphasis>
</taxonomicName>
<emphasis bold="true" box="[448,506,464,490]" pageId="35" pageNumber="116">
<taxonomicNameLabel box="[448,506,464,490]" pageId="35" pageNumber="116" rank="species">sp.n.</taxonomicNameLabel>
</emphasis>
