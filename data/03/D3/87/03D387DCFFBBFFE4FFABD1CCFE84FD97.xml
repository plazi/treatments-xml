<taxonomicName authority="Oliveira¹ &amp; Cipola² &amp; Almeida¹, 2020" authorityName="Oliveira¹ &amp; Cipola² &amp; Almeida¹" authorityYear="2020" box="[823,931,395,421]" class="Entognatha" family="Paronellidae" genus="Salina" kingdom="Animalia" order="Collembola" pageId="35" pageNumber="116" phylum="Arthropoda" rank="species" species="serrana" status="sp.n.">
