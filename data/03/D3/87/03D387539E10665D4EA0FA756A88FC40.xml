(female) “ 
<collectingRegion box="[639,715,451,475]" country="United States of America" name="Texas" pageId="11" pageNumber="10">Texas</collectingRegion>
, 
<collectingCounty box="[725,882,451,475]" pageId="11" pageNumber="10">Brewster Co</collectingCounty>
