<location LSID="urn:lsid:plazi:treatment:03D387539E10665D4EA0FA756A88FC40:8EA5609E9E11665D4F4AFE5368C3FE20" box="[298,395,483,507]" county="Brewster Co" municipality="Buttrail" name="Springs" pageId="11" pageNumber="10" stateProvince="Texas">Springs</location>
, 
<date box="[405,550,483,507]" pageId="11" pageNumber="10" value="1991-07-13">
