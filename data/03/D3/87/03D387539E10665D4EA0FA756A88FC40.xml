<location LSID="urn:lsid:plazi:treatment:03D387539E10665D4EA0FA756A88FC40:8EA5609E9E11665D4EA0FE536856FE20" box="[192,286,483,507]" county="Brewster Co" municipality="Buttrail" name="Buttrill" pageId="11" pageNumber="10" stateProvince="Texas">Buttrill</location>
