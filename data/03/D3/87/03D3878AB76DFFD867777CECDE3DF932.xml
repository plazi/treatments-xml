(
<typeStatus box="[486,542,1079,1105]" pageId="60" pageNumber="61">type</typeStatus>
), 
<collectingCountry box="[561,642,1079,1105]" name="Poland" pageId="60" pageNumber="61">Poland</collectingCountry>
