on 
<taxonomicName authority="Thomson, 1858" authorityName="Thomson" authorityYear="1858" box="[530,809,549,576]" class="Insecta" family="Staphylinidae" genus="Acrulia" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
<emphasis box="[530,618,549,575]" italics="true" pageId="57" pageNumber="58">Acrulia</emphasis>
Thomson, 1858
</taxonomicName>
and 
<taxonomicName authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" box="[865,1158,549,575]" class="Insecta" family="Staphylinidae" genus="Proteinus" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
<emphasis box="[865,978,550,575]" italics="true" pageId="57" pageNumber="58">Proteinus</emphasis>
Latreille, 1796
</taxonomicName>
and also from Spain on 
<taxonomicName authority="(Paykull, 1789) (Santamaria 1989)" authorityName="Santamaria" authorityYear="1989" baseAuthorityName="Paykull" baseAuthorityYear="1789" box="[226,850,588,614]" class="Insecta" family="Staphylinidae" genus="Baptolinus" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="affinis">
<emphasis box="[226,433,588,614]" italics="true" pageId="57" pageNumber="58">Baptolinus affinis</emphasis>
