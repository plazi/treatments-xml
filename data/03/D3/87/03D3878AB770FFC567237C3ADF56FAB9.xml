<subSubSection pageId="33" pageNumber="34" type="discussion">
<paragraph blockId="33.[189,303,1146,1172]" box="[189,303,1146,1172]" pageId="33" pageNumber="34">
<heading bold="true" box="[189,303,1146,1172]" fontSize="11" level="3" pageId="33" pageNumber="34" reason="3">
