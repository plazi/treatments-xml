<taxonomicName box="[1069,1239,1545,1568]" class="Insecta" family="Cixiidae" genus="Kotonisia" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="67" phylum="Arthropoda" rank="species" species="kanoi">
