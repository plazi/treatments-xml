<taxonomicName authority="Wiles" authorityName="Wiles" class="Arachnida" family="Torrenticolidae" genus="Pseudotorrenticola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="259" phylum="Arthropoda" rank="species" species="sharpae">
