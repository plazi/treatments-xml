<collectingCountry box="[551,606,405,431]" name="Italy" pageId="32" pageNumber="33">Italy</collectingCountry>
, 
<collectingCountry box="[618,699,405,431]" name="Poland" pageId="32" pageNumber="33">Poland</collectingCountry>
(
<bibRefCitation author="Majewski T." box="[716,898,405,431]" pageId="32" pageNumber="33" pagination="1 - 240" refId="ref217228" refString="Majewski T. 2008. Laboulbeniales. In: Chlebicki A. (ed.) Atlas of the Geographical Distribution of Fungi in Poland 4: 1 - 240. W. Szafer Institute of Botany, Polish Academy of Sciences, Krakow." type="journal article" year="2008">Majewski 2008</bibRefCitation>
), 
<collectingCountry box="[922,1023,405,431]" name="Belgium" pageId="32" pageNumber="33">Belgium</collectingCountry>
(
<bibRefCitation author="De Kesel A. &amp; Werbrouck T." box="[1040,1382,405,431]" pageId="32" pageNumber="33" pagination="48 - 54" refId="ref212183" refString="De Kesel A. &amp; Werbrouck T. 2008. Belgian records of Laboulbeniales from aquatic insects. Sterbeeckia 28: 48 - 54." type="journal article" year="2008">De Kesel &amp; Werbrouck 2008</bibRefCitation>
), 
<collectingCountry box="[189,298,448,474]" name="Denmark" pageId="32" pageNumber="33">Denmark</collectingCountry>
