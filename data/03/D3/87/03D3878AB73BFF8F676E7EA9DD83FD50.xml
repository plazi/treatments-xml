<taxonomicName authorityName="Latreille" authorityYear="1802" box="[860,961,1601,1627]" class="Insecta" family="Haliplidae" genus="Haliplus" kingdom="Animalia" order="Coleoptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="genus">
