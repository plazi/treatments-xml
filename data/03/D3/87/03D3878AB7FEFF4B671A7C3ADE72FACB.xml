<collectingCountry box="[887,1005,924,950]" name="Sri Lanka" pageId="175" pageNumber="176">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[1016,1130,924,950]" name="Indonesia" pageId="175" pageNumber="176">Indonesia</collectingCountry>
(
<bibRefCitation author="Thaxter R." box="[1145,1308,924,951]" pageId="175" pageNumber="176" pagination="3 - 51" refId="ref223416" refString="Thaxter R. 1915. New Indo-Malayan Laboulbeniales. Proceedings of the American Academy of Arts and Sciences 51 (1): 3 - 51. https: // doi. org / 10.2307 / 20025560" type="journal article" year="1915">Thaxter 1915</bibRefCitation>
), 
<collectingCountry box="[1328,1398,924,950]" name="China" pageId="175" pageNumber="176">China</collectingCountry>
(
<bibRefCitation author="Rossi W." box="[197,348,960,987]" pageId="175" pageNumber="176" pagination="1023 - 1026" refId="ref218327" refString="Rossi W. 1982 b. New or interesting Laboulbeniales from China. Mycologia 74 (6): 1023 - 1026. https: // doi. org / 10.1080 / 00275514.1982.12021623" type="journal article" year="1982">Rossi 1982b</bibRefCitation>
