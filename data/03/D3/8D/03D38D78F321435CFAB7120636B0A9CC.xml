<taxonomicName authority="Hecher" authorityName="Hecher" box="[860,1067,572,597]" class="Insecta" family="Macroveliidae" genus="Pseudovelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="438" phylum="Arthropoda" rank="species" species="intonsa">
