[compared with 21.93– 29.09 (
<emphasis box="[888,900,941,962]" italics="true" pageId="23" pageNumber="901">x</emphasis>
= 23.80) in 
<specimenCount box="[1037,1062,944,962]" pageId="23" pageNumber="901" type="male">♂</specimenCount>
and 18.92–22.60 (
<emphasis box="[1279,1291,941,962]" italics="true" pageId="23" pageNumber="901">x</emphasis>
= 20.42) in 
<specimenCount box="[804,825,975,993]" pageId="23" pageNumber="901" type="female">♀</specimenCount>
in 
<taxonomicName authorityName="Anderson" authorityYear="1872" box="[865,988,973,994]" class="Reptilia" family="Colubridae" genus="Cyclophis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="23" pageNumber="901" phylum="Chordata" rank="species" species="persicus">
<emphasis box="[865,988,973,994]" italics="true" pageId="23" pageNumber="901">E. persicus</emphasis>
