<specimenCount box="[975,1000,822,840]" pageId="23" pageNumber="901" type="male">♂</specimenCount>
and 54–77 (
<emphasis box="[1186,1198,819,840]" italics="true" pageId="23" pageNumber="901">x</emphasis>
= 66.25) in 
<specimenCount box="[1355,1376,822,840]" pageId="23" pageNumber="901" type="female">♀</specimenCount>
in 
<taxonomicName authorityName="Anderson" authorityYear="1872" box="[804,924,850,871]" class="Reptilia" family="Colubridae" genus="Cyclophis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="23" pageNumber="901" phylum="Chordata" rank="species" species="persicus">
