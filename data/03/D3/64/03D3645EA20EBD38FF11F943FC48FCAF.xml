<heading bold="true" box="[151,546,1683,1710]" fontSize="11" level="1" pageId="1" pageNumber="51" reason="1">
<emphasis bold="true" box="[151,546,1683,1710]" pageId="1" pageNumber="51">
<taxonomicName authority="Gibbs" authorityName="Gibbs" box="[151,466,1683,1710]" class="Insecta" family="Bombyliidae" genus="Mythenteles" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="51" phylum="Arthropoda" rank="species" species="rameli" status="sp. n.">
<emphasis bold="true" box="[151,383,1684,1710]" italics="true" pageId="1" pageNumber="51">Mythenteles rameli</emphasis>
Gibbs
</taxonomicName>
, 
<taxonomicNameLabel box="[480,546,1683,1709]" pageId="1" pageNumber="51" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
