<tr box="[166,1415,227,252]" gridrow="1" pageId="2" pageNumber="394">
<th box="[166,520,227,252]" gridcol="0" gridrow="1" pageId="2" pageNumber="394">Krauss’ organ of male</th>
<td box="[562,909,227,252]" gridcol="1" gridrow="1" pageId="2" pageNumber="394">Without ramified keel</td>
<td box="[953,1415,227,252]" gridcol="2" gridrow="1" pageId="2" pageNumber="394">With 3 ramified keels</td>
</tr>
<tr box="[166,1415,265,290]" gridrow="2" pageId="2" pageNumber="394">
<th box="[166,520,265,290]" gridcol="0" gridrow="2" pageId="2" pageNumber="394">Tegmina of male</th>
<td box="[562,909,265,290]" gridcol="1" gridrow="2" pageId="2" pageNumber="394">Narrower, length 2.5 times width</td>
<td box="[953,1415,265,290]" gridcol="2" gridrow="2" pageId="2" pageNumber="394">Wider, length 1.8 times width,</td>
