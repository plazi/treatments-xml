<heading bold="true" box="[151,617,1158,1185]" fontSize="11" level="1" pageId="1" pageNumber="229" reason="1">
<emphasis bold="true" box="[151,617,1158,1185]" pageId="1" pageNumber="229">
<taxonomicName authority="Tulande-M &amp; Prado &amp; Jiménez &amp; Chagas-Jr, 2020" authorityName="Tulande-M &amp; Prado &amp; Jiménez &amp; Chagas-Jr" authorityYear="2020" box="[151,483,1158,1185]" class="Chilopoda" family="Scolopocryptopidae" genus="Newportia" kingdom="Animalia" order="Scolopendromorpha" pageId="1" pageNumber="229" phylum="Arthropoda" rank="subGenus" status="subgen. n." subGenus="Andeocryptops">
<emphasis bold="true" box="[151,276,1159,1185]" italics="true" pageId="1" pageNumber="229">Newportia</emphasis>
(
<emphasis bold="true" box="[294,473,1158,1185]" italics="true" pageId="1" pageNumber="229">Andeocryptops</emphasis>
