<quantity box="[445,610,794,820]" metricMagnitude="-3" metricUnit="m" metricValue="3.06" metricValueMax="3.33" metricValueMin="2.79" pageId="3" pageNumber="430" unit="mm" value="3.06" valueMax="3.33" valueMin="2.79">2.79–3.33 mm</quantity>
, fore wing length 
<quantity box="[814,980,793,820]" metricMagnitude="-3" metricUnit="m" metricValue="3.085" metricValueMax="3.15" metricValueMin="3.02" pageId="3" pageNumber="430" unit="mm" value="3.085" valueMax="3.15" valueMin="3.02">3.02–3.15 mm</quantity>
