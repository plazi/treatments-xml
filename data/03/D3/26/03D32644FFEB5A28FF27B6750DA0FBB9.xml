<subSubSection pageId="20" pageNumber="537" type="description">
<paragraph blockId="20.[151,1436,476,1115]" box="[199,666,512,540]" pageId="20" pageNumber="537">
<emphasis bold="true" box="[199,374,513,539]" pageId="20" pageNumber="537">Measurements</emphasis>
