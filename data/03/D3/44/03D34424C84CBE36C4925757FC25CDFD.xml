<collectingRegion box="[380,469,896,920]" country="Greece" name="Voreio Aigaio" pageId="7" pageNumber="8">Aegean</collectingRegion>
&quot; lineage of 
<taxonomicName authority="ROTTENBERG, 1874" authorityName="ROTTENBERG" authorityYear="1874" box="[606,977,896,920]" class="Insecta" family="Carabidae" genus="Duvalidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="diaphanus">
<emphasis box="[606,758,896,920]" italics="true" pageId="7" pageNumber="8">D. diaphanus</emphasis>
ROTTENBERG, 1874
</taxonomicName>
, and 
<taxonomicName authority="DEUVE, 2001" authorityName="DEUVE" authorityYear="2001" class="Insecta" family="Carabidae" genus="Duvalidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="mirei">
<emphasis italics="true" pageId="7" pageNumber="8">D. mirei</emphasis>
DEUVE, 2001
