in male (n = 
<number box="[808,824,1719,1745]" pageId="2" pageNumber="29" value="5.0">5</number>
); 
<quantity box="[848,1018,1719,1745]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" metricValueMax="4.95" metricValueMin="4.25" pageId="2" pageNumber="29" unit="mm" value="4.6" valueMax="4.95" valueMin="4.25">4.25–4.95 mm</quantity>
