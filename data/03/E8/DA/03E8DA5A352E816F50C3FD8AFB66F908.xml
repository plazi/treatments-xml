<taxonomicName authority="Keifer, 1964" authorityName="Keifer" authorityYear="1964" box="[625,1030,1613,1639]" class="Arachnida" family="Diptilomiopidae" genus="Trimeroptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="7" pageNumber="66" phylum="Arthropoda" rank="species" species="ilicifolia">
