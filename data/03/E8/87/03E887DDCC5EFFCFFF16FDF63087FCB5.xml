<subSubSection pageId="11" pageNumber="192" type="diagnosis">
<paragraph blockId="11.[151,1437,656,1043]" pageId="11" pageNumber="192">
<emphasis bold="true" box="[151,274,656,683]" pageId="11" pageNumber="192">Diagnosis.</emphasis>
<taxonomicName authority="Alvarado, 2020" authorityName="Alvarado" authorityYear="2020" box="[280,437,658,683]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="192" phylum="Arthropoda" rank="species" species="pusaq" status="sp. nov.">
