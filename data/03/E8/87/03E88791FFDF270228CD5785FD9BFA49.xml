<taxonomicName authority="Kóbor, 2018" authorityName="Kóbor" authorityYear="2018" box="[361,596,1151,1172]" class="Insecta" family="Lygaeidae" genus="Geocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="583" phylum="Arthropoda" rank="species" species="margaretarum" status="new species">
