<taxonomicName authority="Townes, 1946" authorityName="Townes" authorityYear="1946" box="[151,415,441,467]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,415,441,467]" pageId="1" pageNumber="182">
<emphasis bold="true" box="[151,241,441,467]" italics="true" pageId="1" pageNumber="182">Mnioes</emphasis>
Townes, 1946
</emphasis>
</taxonomicName>
</heading>
</paragraph>
<paragraph blockId="1.[151,1180,508,533]" box="[151,1180,508,533]" pageId="1" pageNumber="182">
<taxonomicName authority="Townes, 1946: 58" authorityName="Townes" authorityPageNumber="58" authorityYear="1946" box="[151,415,508,533]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="genus">
<emphasis box="[151,226,510,533]" italics="true" pageId="1" pageNumber="182">Mnioes</emphasis>
Townes, 1946: 58
</taxonomicName>
. Type-species 
<taxonomicName authority="Cresson, 1874" authorityName="Cresson" authorityYear="1874" box="[563,928,509,533]" class="Insecta" family="Ichneumonidae" genus="Lampronota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="species" species="jacunda">
