<typeStatus box="[503,549,1192,1216]" pageId="5" pageNumber="334">type</typeStatus>
locality, 
<collectingCountry box="[647,755,1191,1215]" name="Colombia" pageId="5" pageNumber="334">Colombia</collectingCountry>
, 
<collectingRegion box="[766,870,1191,1215]" country="Colombia" name="Risaralda" pageId="5" pageNumber="334">Risaralda</collectingRegion>
.
</paragraph>
