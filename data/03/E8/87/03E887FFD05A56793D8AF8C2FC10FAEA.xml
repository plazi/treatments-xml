<quantity box="[780,877,511,537]" metricMagnitude="-1" metricUnit="m" metricValue="4.62" pageId="4" pageNumber="437" unit="mm" value="462.0">462 mm</quantity>
; TaL: 
<quantity box="[950,1047,511,537]" metricMagnitude="-1" metricUnit="m" metricValue="1.8399999999999999" pageId="4" pageNumber="437" unit="mm" value="184.0">184 mm</quantity>
; TL: 
<quantity box="[1110,1207,511,537]" metricMagnitude="-1" metricUnit="m" metricValue="6.459999999999999" pageId="4" pageNumber="437" unit="mm" value="646.0">646 mm</quantity>
; Tal/TL: 0.285; LsI: 
<quantity box="[151,240,547,573]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="4" pageNumber="437" unit="mm" value="1.6">1.6 mm</quantity>
; LsP: 
<quantity box="[309,398,547,573]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="4" pageNumber="437" unit="mm" value="1.7">1.7 mm</quantity>
; HW: 
<quantity box="[469,558,547,573]" metricMagnitude="-3" metricUnit="m" metricValue="9.2" pageId="4" pageNumber="437" unit="mm" value="9.2">9.2 mm</quantity>
; HL: 
<quantity box="[621,724,546,573]" metricMagnitude="-2" metricUnit="m" metricValue="1.65" pageId="4" pageNumber="437" unit="mm" value="16.5">16.5 mm</quantity>
; HH: 
<quantity box="[790,879,547,573]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" pageId="4" pageNumber="437" unit="mm" value="6.1">6.1 mm</quantity>
; DE: 
<quantity box="[942,1031,546,573]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="4" pageNumber="437" unit="mm" value="5.5">5.5 mm</quantity>
; EL: 
<quantity box="[1091,1180,547,573]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="4" pageNumber="437" unit="mm" value="3.8">3.8 mm</quantity>
; EW: 
<quantity box="[1248,1337,547,573]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="4" pageNumber="437" unit="mm" value="3.8">3.8 mm</quantity>
; RH: 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="4" pageNumber="437" unit="mm" value="1.1">1.1 mm</quantity>
; RW: 
<quantity box="[267,356,583,609]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="4" pageNumber="437" unit="mm" value="2.9">2.9 mm</quantity>
; MH: 
<quantity box="[427,516,583,609]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="437" unit="mm" value="0.9">0.9 mm</quantity>
; MW: 
<quantity box="[592,681,583,609]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="4" pageNumber="437" unit="mm" value="1.8">1.8 mm</quantity>
