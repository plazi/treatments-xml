39. 
<taxonomicName authority="Anderson" authorityName="Anderson" authorityYear="2010" box="[557,917,1610,1636]" class="Insecta" family="Curculionidae" genus="Theognete" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="maziaki" status="new species">
<emphasis bold="true" box="[557,785,1610,1636]" italics="true" pageId="45" pageNumber="46">Theognete maziaki</emphasis>
Anderson
</taxonomicName>
, 
<taxonomicNameLabel box="[930,1074,1610,1636]" pageId="45" pageNumber="46" rank="species">new species</taxonomicNameLabel>
</emphasis>
(
<figureCitation box="[693,836,1645,1671]" captionStart="FIGURES 165–168" captionStartId="107.[151,269,966,990]" captionTargetBox="[536,1053,188,936]" captionTargetId="figure-62@107.[531,1056,178,942]" captionTargetPageId="107" captionText="FIGURES 165–168. Habitus, dorsal view, Theognete tzotzil species group. 165. T. illuviosa. 166. T. maziaki. 167. T. evelynae. 168. T. barringerorum." figureDoi="http://doi.org/10.5281/zenodo.5362283" httpUri="https://zenodo.org/record/5362283/files/figure.png" pageId="45" pageNumber="46">Figures 166</figureCitation>
, 
<figureCitation box="[848,893,1645,1671]" captionStart="FIGURES 200–201" captionStartId="113.[151,269,1879,1903]" captionTargetBox="[293,1308,1103,1860]" captionTargetId="figure-56@113.[285,1314,1095,1863]" captionTargetPageId="113" captionText="FIGURES 200–201. Aedeagus, dorsal and lateral view, Theognete tzotzil species group. 200. T. illuviosa. 201. T. maziaki." figureDoi="http://doi.org/10.5281/zenodo.5362293" httpUri="https://zenodo.org/record/5362293/files/figure.png" pageId="45" pageNumber="46">201</figureCitation>
