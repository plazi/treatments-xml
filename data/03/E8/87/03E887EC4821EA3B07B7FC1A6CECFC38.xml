<subSubSection pageId="3" pageNumber="44" type="etymology">
<paragraph blockId="3.[151,1436,151,978]" pageId="3" pageNumber="44">
<emphasis bold="true" box="[198,339,311,337]" pageId="3" pageNumber="44">Etymology.</emphasis>
