, 
<taxonomicName authorityName="Anderson" authorityYear="2010" box="[875,992,1861,1886]" class="Insecta" family="Curculionidae" genus="Theognete" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="cristata">
<emphasis box="[875,992,1861,1886]" italics="true" pageId="25" pageNumber="26">T. cristata</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Anderson" authorityYear="2010" box="[1007,1161,1861,1886]" class="Insecta" family="Curculionidae" genus="Theognete" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="tomneyeae">
