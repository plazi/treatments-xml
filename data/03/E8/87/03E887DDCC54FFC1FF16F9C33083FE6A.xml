<emphasis bold="true" box="[1300,1436,1855,1877]" pageId="1" pageNumber="182">
<taxonomicName authority="Alvarado, 2020" authorityName="Alvarado" authorityYear="2020" box="[1300,1353,1855,1877]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="species" species="kinsa" status="sp. nov.">
