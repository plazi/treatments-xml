<paragraph blockId="2.[151,962,1161,1223]" box="[151,541,1161,1188]" pageId="2" pageNumber="558">
<heading bold="true" box="[151,541,1161,1188]" fontSize="11" level="1" pageId="2" pageNumber="558" reason="1">
<emphasis bold="true" box="[151,541,1161,1188]" pageId="2" pageNumber="558">
Genus 
<taxonomicName authority="Mordukhovich &amp; Zograf &amp; Saulenko &amp; Fadeeva, 2020" authorityName="Mordukhovich &amp; Zograf &amp; Saulenko &amp; Fadeeva" authorityYear="2020" box="[238,424,1161,1188]" class="Enoplea" family="Oncholaimidae" genus="Oloncholaimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="2" pageNumber="558" phylum="Nematoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[238,424,1161,1188]" italics="true" pageId="2" pageNumber="558">Oloncholaimus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[432,541,1162,1188]" pageId="2" pageNumber="558" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
