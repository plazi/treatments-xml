<number box="[1077,1106,1906,1932]" pageId="0" pageNumber="29" value="29.0">29</number>
(
<number box="[1121,1197,1906,1932]" pageId="0" pageNumber="29" value="28.0" valueMax="30.0" valueMin="26.0">26–30</number>
