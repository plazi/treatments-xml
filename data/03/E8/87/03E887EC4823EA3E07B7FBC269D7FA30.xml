<number box="[964,979,1386,1412]" pageId="3" pageNumber="44" value="5.0">5</number>
(
<number box="[996,1042,1386,1412]" pageId="3" pageNumber="44" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
) 
<number box="[1059,1072,1386,1412]" pageId="3" pageNumber="44" value="5.0">5</number>
