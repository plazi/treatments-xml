<number box="[544,588,1146,1172]" pageId="3" pageNumber="44" value="199.0">199</number>
(
<number box="[604,710,1146,1172]" pageId="3" pageNumber="44" value="217.5" valueMax="255.0" valueMin="180.0">180–255</number>
, n = 
<number box="[768,785,1146,1172]" pageId="3" pageNumber="44" value="7.0">7</number>
), (
<number box="[816,892,1146,1172]" pageId="3" pageNumber="44" value="45.0" valueMax="49.0" valueMin="41.0">41–49</number>
) thick, 
<number box="[981,1010,1146,1172]" pageId="3" pageNumber="44" value="50.0">50</number>
(
<number box="[1026,1102,1146,1172]" pageId="3" pageNumber="44" value="48.5" valueMax="50.0" valueMin="47.0">47–50</number>
