<number box="[1132,1161,1346,1372]" pageId="3" pageNumber="44" value="28.0">28</number>
(
<number box="[1179,1258,1346,1372]" pageId="3" pageNumber="44" value="26.5" valueMax="28.0" valueMin="25.0">25–28</number>
), tibia 
<number box="[1341,1356,1346,1372]" pageId="3" pageNumber="44" value="5.0">5</number>
(
<number box="[1374,1422,1346,1372]" pageId="3" pageNumber="44" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
), tarsus 
<number box="[227,242,1386,1412]" pageId="3" pageNumber="44" value="7.0">7</number>
(
<number box="[260,307,1386,1412]" pageId="3" pageNumber="44" value="7.0" valueMax="8.0" valueMin="6.0">6–8</number>
), ω 
<number box="[356,385,1386,1412]" pageId="3" pageNumber="44" value="10.0">10</number>
(
<number box="[402,463,1386,1412]" pageId="3" pageNumber="44" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
