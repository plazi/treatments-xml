<emphasis box="[214,243,1427,1452]" italics="true" pageId="3" pageNumber="44">
<number box="[214,227,1427,1452]" pageId="3" pageNumber="44" value="1.0">1</number>
b
</emphasis>
setae 
<number box="[319,334,1426,1452]" pageId="3" pageNumber="44" value="9.0">9</number>
(
<number box="[351,399,1426,1452]" pageId="3" pageNumber="44" value="7.5" valueMax="9.0" valueMin="6.0">6–9</number>
), 
<emphasis box="[420,449,1427,1452]" italics="true" pageId="3" pageNumber="44">
<number box="[420,433,1427,1452]" pageId="3" pageNumber="44" value="1.0">1</number>
b
