<number box="[997,1026,1306,1332]" pageId="3" pageNumber="44" value="30.0">30</number>
(
<number box="[1042,1120,1306,1332]" pageId="3" pageNumber="44" value="28.5" valueMax="30.0" valueMin="27.0">27–30</number>
), tibia 
<number box="[1200,1215,1306,1332]" pageId="3" pageNumber="44" value="6.0">6</number>
(
<number box="[1231,1278,1306,1332]" pageId="3" pageNumber="44" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), tarsus 
<number box="[1375,1390,1306,1332]" pageId="3" pageNumber="44" value="7.0">7</number>
(
<number pageId="3" pageNumber="44" value="7.0" valueMax="8.0" valueMin="6.0">6– 8</number>
), ω 
<number box="[219,234,1346,1372]" pageId="3" pageNumber="44" value="9.0">9</number>
(
<number box="[251,312,1346,1372]" pageId="3" pageNumber="44" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
) distally just rounded, empodium simple, 
<number box="[819,834,1346,1372]" pageId="3" pageNumber="44" value="6.0">6</number>
(
<number box="[852,899,1346,1372]" pageId="3" pageNumber="44" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), 
<number box="[923,936,1346,1372]" pageId="3" pageNumber="44" value="5.0">5</number>
