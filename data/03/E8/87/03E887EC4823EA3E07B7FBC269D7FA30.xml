<paragraph blockId="3.[151,1436,1146,1852]" pageId="3" pageNumber="44">
<emphasis bold="true" box="[151,242,1146,1172]" pageId="3" pageNumber="44">Female</emphasis>
(fig. 
<number box="[306,323,1146,1172]" pageId="3" pageNumber="44" value="2.0">2</number>
