<number box="[714,743,1186,1212]" pageId="3" pageNumber="44" value="18.0">18</number>
(
<number box="[759,836,1186,1212]" pageId="3" pageNumber="44" value="18.5" valueMax="21.0" valueMin="16.0">16–21</number>
