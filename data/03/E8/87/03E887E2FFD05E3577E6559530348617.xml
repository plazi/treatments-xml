<paragraph blockId="11.[168,562,1280,1300]" box="[168,562,1280,1300]" pageId="11" pageNumber="398">
<taxonomicName authority="Heyer 1994: 82" authorityName="* (Heyer" authorityPageNumber="82" authorityYear="1994" box="[168,558,1280,1300]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="11" pageNumber="398" phylum="Chordata" rank="species" species="colombiensis">
