<typeStatus box="[151,207,1443,1467]" pageId="2" pageNumber="259">Type</typeStatus>
species: 
<taxonomicName box="[309,573,1443,1467]" class="Actinopterygii" family="Cyprinidae" genus="Paraqianlabeo" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Cypriniformes" pageId="2" pageNumber="259" phylum="Chordata" rank="species" species="lineatus" status="new species">
<emphasis box="[309,573,1443,1467]" italics="true" pageId="2" pageNumber="259">Paraqianlabeo lineatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[587,721,1442,1467]" pageId="2" pageNumber="259" rank="species">new species</taxonomicNameLabel>
(
<figureCitation box="[736,816,1442,1467]" captionStart="FIGURE 2" captionStartId="5.[151,250,1283,1305]" captionTargetBox="[175,1412,231,1258]" captionTargetId="figure@5.[175,1412,231,1258]" captionTargetPageId="5" captionText="FIGURE 2. Oromanibular structures of Paraqianlabeo lineatus, SWU 20130710005, holotype, 65.1 mm SL. lj = lower jaw; lll = lateral lobe of lower lip; mb = maxillary barbel; mg = mental groove; ml l = median lobe of lower lip; pg = postlabial groove; rb = rostral barbel; rc = rostral cap; uj = upper jaw; ul = upper lip." httpUri="https://zenodo.org/record/230271/files/figure.png" pageId="2" pageNumber="259">Figs. 2</figureCitation>
, 
<figureCitation box="[828,843,1443,1467]" captionStart="FIGURE 4" captionStartId="7.[151,250,999,1021]" captionTargetBox="[151,1436,193,977]" captionTargetId="figure@7.[151,1436,193,978]" captionTargetPageId="7" captionText="FIGURE 4. Lateral view of (a) Paraqianlabeo lineatus, SWU 20130710007, paratype, 65.1 mm SL and (b) Qianlabeo striatus, SWU 20130917005, 68.1 mm SL, Beipan-Jiang drainage at An’shun County, Guizhou Province." httpUri="https://zenodo.org/record/230273/files/figure.png" pageId="2" pageNumber="259">4</figureCitation>
)
</paragraph>
