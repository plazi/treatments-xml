<emphasis box="[387,565,1391,1417]" italics="true" pageId="42" pageNumber="443">N. fuscaspecula</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Kallal" authorityYear="2019" box="[578,678,1392,1417]" class="Insecta" family="Formicidae" genus="Nylanderia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="443" phylum="Arthropoda" rank="species" species="sierra">
<emphasis box="[578,678,1392,1417]" italics="true" pageId="42" pageNumber="443">N. sierra</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Kallal" authorityYear="2019" box="[692,788,1391,1417]" class="Insecta" family="Formicidae" genus="Nylanderia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="443" phylum="Arthropoda" rank="species" species="wardi">
