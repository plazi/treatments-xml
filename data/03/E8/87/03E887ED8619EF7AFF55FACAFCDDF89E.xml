<collectingDate box="[926,1034,1508,1532]" pageId="48" pageNumber="49" value="1945-06-05">5.vi.1945</collectingDate>
</date>
, 
<collectorName box="[1046,1206,1508,1532]" pageId="48" pageNumber="49">S.H. Coleman</collectorName>
, ex Orange rind 
<emphasis italics="true" pageId="48" pageNumber="49">Citrus</emphasis>
sp. (
<taxonomicName box="[243,349,1543,1567]" class="Magnoliopsida" family="Rutaceae" kingdom="Plantae" order="Sapindales" pageId="48" pageNumber="49" phylum="Tracheophyta" rank="family">Rutaceae</taxonomicName>
