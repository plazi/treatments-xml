<paragraph blockId="1.[151,395,970,996]" box="[151,395,970,996]" pageId="1" pageNumber="45">
<heading bold="true" box="[151,395,970,996]" fontSize="11" level="1" pageId="1" pageNumber="45" reason="1">
<emphasis bold="true" box="[151,395,970,996]" pageId="1" pageNumber="45">
<taxonomicName box="[151,278,970,996]" class="Magnoliopsida" family="Asclepiadaceae" genus="Calliparus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="1" pageNumber="45" phylum="Tracheophyta" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,278,970,996]" italics="true" pageId="1" pageNumber="45">Calliparus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[285,395,970,996]" pageId="1" pageNumber="45" rank="genus">gen. nov.</taxonomicNameLabel>
