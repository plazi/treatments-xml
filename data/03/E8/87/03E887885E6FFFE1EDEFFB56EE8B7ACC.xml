<date box="[1388,1436,1530,1556]" pageId="4" pageNumber="48" value="1929-01">
І 
<number box="[1406,1436,1530,1556]" pageId="4" pageNumber="48" value="29.0">29</number>
</date>
(
<number box="[159,240,1568,1594]" pageId="4" pageNumber="48" value="29.0" valueMax="30.0" valueMin="28.0">28−30</number>
