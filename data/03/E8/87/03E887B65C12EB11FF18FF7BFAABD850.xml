<quantity box="[484,588,692,717]" metricMagnitude="-3" metricUnit="m" metricValue="7.37" pageId="3" pageNumber="88" unit="mm" value="7.37">7.37 mm</quantity>
, width 2.96– 
<quantity box="[736,840,692,717]" metricMagnitude="-3" metricUnit="m" metricValue="2.26" pageId="3" pageNumber="88" unit="mm" value="2.26">2.26 mm</quantity>
