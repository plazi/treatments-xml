<subSubSection pageId="1" pageNumber="86" type="etymology">
<paragraph blockId="1.[151,1437,260,501]" pageId="1" pageNumber="86">
<emphasis bold="true" box="[199,334,439,464]" inLineHeading="true" pageId="1" pageNumber="86" reason="1">Etymology.</emphasis>
