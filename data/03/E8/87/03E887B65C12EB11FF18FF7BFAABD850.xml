<taxonomicName authority="Tenore" authorityName="Tenore" box="[687,911,800,824]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="88" phylum="Tracheophyta" rank="species" species="brutia">
