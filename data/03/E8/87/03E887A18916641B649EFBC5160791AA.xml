horizontal layer), sandy silt and 
<quantity box="[783,855,1336,1360]" metricMagnitude="2" metricUnit="m" metricValue="1.7" pageId="1" pageNumber="388" unit="m" value="170.0">170 m</quantity>
(
<quantity box="[872,961,1336,1360]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" metricValueMax="10.0" metricValueMin="0.0" pageId="1" pageNumber="388" unit="cm" value="0.5" valueMax="1.0" valueMin="0.0">0–1 cm</quantity>
), silt; Sevastopolskaya Bay, depths 
<quantity metricMagnitude="1" metricUnit="m" metricValue="1.55" metricValueMax="1.7" metricValueMin="1.4" pageId="1" pageNumber="388" unit="m" value="15.5" valueMax="17.0" valueMin="14.0">14–17 m</quantity>
