Known from 
<collectingCountry box="[525,598,752,778]" name="Egypt" pageId="8" pageNumber="42">Egypt</collectingCountry>
, 
<collectingCountry box="[610,672,752,778]" name="Syria" pageId="8" pageNumber="42">Syria</collectingCountry>
and 
<collectingCountry box="[729,815,752,778]" name="Turkey" pageId="8" pageNumber="42">Turkey</collectingCountry>
