<number box="[1153,1194,1805,1831]" pageId="9" pageNumber="43" value="9.2">9.2</number>
. Eye tubercle length 
<number box="[151,190,1845,1871]" pageId="9" pageNumber="43" value="1.5">1.5</number>
, width 
<number box="[277,317,1845,1871]" pageId="9" pageNumber="43" value="1.9">1.9</number>
; anterior row procurved, posterior row recurved, clypeus absent. Labium length 
<number box="[1265,1304,1845,1871]" pageId="9" pageNumber="43" value="1.4">1.4</number>
, width 
<number box="[1392,1432,1845,1871]" pageId="9" pageNumber="43" value="1.9">1.9</number>
