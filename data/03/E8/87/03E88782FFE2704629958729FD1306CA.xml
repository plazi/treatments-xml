(2, 7), 
<taxonomicName authority="(Kusigemati)" authorityName="Watanabe" authorityYear="2019" baseAuthorityName="Kusigemati" baseAuthorityYear="1982" box="[723,974,1679,1704]" class="Insecta" family="Ichneumonidae" genus="Xanthocampoplex" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="581" phylum="Arthropoda" rank="species" species="kumatai">
