<number box="[1404,1431,1560,1582]" pageId="26" pageNumber="477" value="87.0">87</number>
, mesoscutellum, dorsal view, 
<number box="[444,471,1593,1615]" pageId="26" pageNumber="477" value="88.0">88</number>
, fore wing, part, 89, 1st metasomal tergite (petiole), dorsal view, 
<number box="[1123,1151,1593,1615]" pageId="26" pageNumber="477" value="90.0">90</number>
