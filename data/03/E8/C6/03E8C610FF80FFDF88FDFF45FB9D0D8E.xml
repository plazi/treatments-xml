<collectingCountry box="[512,583,389,415]" name="China" pageId="12" pageNumber="523">China</collectingCountry>
: 
<collectingRegion box="[594,751,389,415]" country="China" name="Jilin" pageId="12" pageNumber="523">Jilin Province</collectingRegion>
, Changbaishan National Natural Reserve, Pond of the Visitor Center (
<figureCitation box="[242,325,425,452]" captionStart="FIGURE 15" captionStartId="10.[151,250,1970,1995]" captionTargetBox="[167,1421,1323,1950]" captionTargetId="figure@10.[167,1421,1323,1950]" captionTargetPageId="10" captionText="FIGURE 15. Locality of Arcynopteryx polaris Klapálek, 1912 and Pictetiella asiatica Zwick &amp; Levanidova, 1971 in Jilin Province of northeastern China." figureDoi="http://doi.org/10.5281/zenodo.3471232" httpUri="https://zenodo.org/record/3471232/files/figure.png" pageId="12" pageNumber="523">Fig. 15</figureCitation>
), 
<geoCoordinate box="[347,480,425,452]" degrees="42.07111" direction="north" orientation="latitude" pageId="12" pageNumber="523" precision="1" value="42.07111">42.07111 N</geoCoordinate>
, 
<geoCoordinate box="[492,638,425,452]" degrees="128.06358" direction="east" orientation="longitude" pageId="12" pageNumber="523" precision="1" value="128.06358">128.06358 E</geoCoordinate>
, 
<quantity box="[651,738,425,452]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="12" pageNumber="523" unit="m" value="1700.0">1700 m</quantity>
, 
