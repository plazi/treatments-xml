<taxonomicName authority="Miller" authorityName="Miller" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="8" pageNumber="465" phylum="Tracheophyta" rank="species" species="echinata">
