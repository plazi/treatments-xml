<taxonomicName baseAuthorityName="Westcott and Bellamy" baseAuthorityYear="2013" box="[703,918,1229,1247]" class="Insecta" family="Buprestidae" genus="Chalcophora" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="464" phylum="Arthropoda" rank="species" species="virginiensis">
