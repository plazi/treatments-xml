, 
<quantity box="[178,238,1542,1563]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="1" pageNumber="326" unit="m" value="300.0">
<elevation box="[178,238,1542,1563]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="1" pageNumber="326" unit="m" value="300.0">300m</elevation>
