<quantity box="[415,537,597,621]" metricMagnitude="-2" metricUnit="m" metricValue="9.55" metricValueMax="11.2" metricValueMin="7.9" pageId="3" pageNumber="114" unit="cm" value="9.55" valueMax="11.2" valueMin="7.9">7.9-11.2 cm</quantity>
