<paragraph blockId="3.[113,647,572,825]" pageId="3" pageNumber="114">
<emphasis italics="true" pageId="3" pageNumber="114">
Frutex ad 
<quantity box="[239,276,572,596]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="3" pageNumber="114" unit="m" value="3.0">3 m</quantity>
