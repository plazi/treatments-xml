<quantity box="[485,579,648,672]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.2" metricValueMin="0.9" pageId="3" pageNumber="114" unit="mm" value="10.5" valueMax="12.0" valueMin="9.0">9-12 mm</quantity>
