</taxonomicName>
<taxonomicNameLabel box="[346,409,1827,1846]" pageId="5" pageNumber="410" rank="species">sp. nov.</taxonomicNameLabel>
<emphasis bold="true" box="[413,441,1826,1845]" pageId="5" pageNumber="410">(A)</emphasis>
vista ventral, 
<emphasis bold="true" box="[555,583,1826,1845]" pageId="5" pageNumber="410">(B)</emphasis>
vista dorsal interna, 
<emphasis bold="true" box="[754,784,1826,1845]" pageId="5" pageNumber="410">(C)</emphasis>
vista lateral de la glándula calcífera, 
<emphasis bold="true" box="[1083,1114,1826,1845]" pageId="5" pageNumber="410">(D)</emphasis>
cerda genital, 
<emphasis bold="true" box="[94,121,1852,1871]" pageId="5" pageNumber="410">(E)</emphasis>
espermateca, 
<emphasis bold="true" box="[237,263,1852,1871]" pageId="5" pageNumber="410">(F)</emphasis>
nefridio, 
<emphasis bold="true" box="[344,374,1852,1871]" pageId="5" pageNumber="410">Gc:</emphasis>
glándula calcífera, 
<emphasis bold="true" box="[533,558,1852,1871]" pageId="5" pageNumber="410">M:</emphasis>
molleja, 
<emphasis bold="true" box="[634,659,1852,1871]" pageId="5" pageNumber="410">In:</emphasis>
