<td box="[1109,1151,1535,1561]" gridcol="9" gridrow="3" pageId="16" pageNumber="45">2.91</td>
<td box="[1222,1264,1535,1561]" gridcol="10" gridrow="3" pageId="16" pageNumber="45">2.69</td>
