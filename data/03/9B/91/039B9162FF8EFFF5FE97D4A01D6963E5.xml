<number box="[1115,1124,582,597]" pageId="4" pageNumber="33" value="2.0">2</number>
and f 
<number box="[1192,1201,582,597]" pageId="4" pageNumber="33" value="3.0">3</number>
brown on distal end and t 
<number box="[442,451,626,641]" pageId="4" pageNumber="33" value="2.0">2</number>
, t 
<number box="[474,483,626,641]" pageId="4" pageNumber="33" value="3.0">3</number>
, ta 
<number box="[518,544,626,641]" pageId="4" pageNumber="33" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
