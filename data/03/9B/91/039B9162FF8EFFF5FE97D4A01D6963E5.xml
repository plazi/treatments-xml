<number box="[1314,1323,539,554]" pageId="4" pageNumber="33" value="1.0">1</number>
yellow, except brown on distal 
<number box="[636,646,567,593]" pageId="4" pageNumber="33" value="1.0">1</number>
/ 
<number box="[657,673,567,593]" pageId="4" pageNumber="33" value="2.0">2</number>
f 
<number box="[692,701,582,597]" pageId="4" pageNumber="33" value="1.0">1</number>
and t 
<number box="[768,777,582,597]" pageId="4" pageNumber="33" value="1.0">1</number>
, ta 
<number box="[814,840,582,597]" pageId="4" pageNumber="33" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
; P 
<number box="[873,882,582,597]" pageId="4" pageNumber="33" value="2.0">2</number>
and P 
<number box="[958,967,582,597]" pageId="4" pageNumber="33" value="3.0">3</number>
