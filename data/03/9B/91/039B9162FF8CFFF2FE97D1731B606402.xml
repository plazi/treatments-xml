<quantity box="[489,613,853,879]" metricMagnitude="1" metricUnit="m" metricValue="6.55" metricValueMax="6.8" metricValueMin="6.3" pageId="2" pageNumber="31" unit="m" value="65.5" valueMax="68.0" valueMin="63.0">63–68 m</quantity>
