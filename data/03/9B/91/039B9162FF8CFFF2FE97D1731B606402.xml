<quantity box="[778,862,893,919]" metricMagnitude="1" metricUnit="m" metricValue="9.5" pageId="2" pageNumber="31" unit="m" value="95.0">95 m</quantity>
, width 
<quantity box="[959,1044,893,919]" metricMagnitude="1" metricUnit="m" metricValue="3.2" pageId="2" pageNumber="31" unit="m" value="32.0">32 m</quantity>
