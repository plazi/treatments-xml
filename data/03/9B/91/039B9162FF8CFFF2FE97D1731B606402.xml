<quantity box="[445,538,933,959]" metricMagnitude="2" metricUnit="m" metricValue="2.03" pageId="2" pageNumber="31" unit="m" value="203.0">203 m</quantity>
, width 
<quantity box="[624,704,933,959]" metricMagnitude="1" metricUnit="m" metricValue="3.2" pageId="2" pageNumber="31" unit="m" value="32.0">32 m</quantity>
), curved, apically pointed, widest at about distal 
<number box="[1277,1289,933,959]" pageId="2" pageNumber="31" value="1.0">1</number>
