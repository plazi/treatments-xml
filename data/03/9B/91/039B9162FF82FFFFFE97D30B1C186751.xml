<number box="[1087,1096,929,944]" pageId="15" pageNumber="44" value="2.0">2</number>
and f 
<number box="[1162,1171,929,944]" pageId="15" pageNumber="44" value="3.0">3</number>
brownish on distal 
<number box="[337,348,957,983]" pageId="15" pageNumber="44" value="1.0">1</number>
/ 
<number box="[359,374,957,983]" pageId="15" pageNumber="44" value="3.0">3</number>
and t 
<number box="[444,453,972,987]" pageId="15" pageNumber="44" value="2.0">2</number>
and t 
<number box="[523,532,972,987]" pageId="15" pageNumber="44" value="3.0">3</number>
