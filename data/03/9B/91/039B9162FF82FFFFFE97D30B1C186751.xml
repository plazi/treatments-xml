<number box="[1220,1229,886,901]" pageId="15" pageNumber="44" value="1.0">1</number>
yellow, except brown on distal end f 
<number box="[595,604,929,944]" pageId="15" pageNumber="44" value="1.0">1</number>
and t 
<number box="[668,677,929,944]" pageId="15" pageNumber="44" value="1.0">1</number>
, ta 
<number box="[712,738,929,944]" pageId="15" pageNumber="44" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
; P 
<number box="[769,778,929,944]" pageId="15" pageNumber="44" value="2.0">2</number>
and P 
<number box="[850,859,929,944]" pageId="15" pageNumber="44" value="3.0">3</number>
