r, 
<taxonomicName authorityName="Sharp" authorityYear="1889" box="[682,759,1182,1203]" class="Insecta" family="Staphylinidae" genus="Liusus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="755" phylum="Arthropoda" rank="genus">
