<emphasis bold="true" box="[380,445,781,798]" italics="true" pageId="62" pageNumber="373">FIG. 27.</emphasis>
<taxonomicName authority="(Gerstaecker)" baseAuthorityName="Gerstaecker" baseAuthorityYear="1889" box="[450,849,781,798]" class="Insecta" family="Acrididae" genus="Gastrimargus" kingdom="Animalia" order="Orthoptera" pageId="62" pageNumber="373" phylum="Arthropoda" rank="subSpecies" species="determinatus" subSpecies="procerus">
