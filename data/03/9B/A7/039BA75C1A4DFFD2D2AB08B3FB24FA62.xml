<typeStatus box="[566,650,589,609]" pageId="19" pageNumber="330">Holotype</typeStatus>
mâle du 
<collectingCountry box="[737,799,589,609]" name="Ghana" pageId="19" pageNumber="330">Ghana</collectingCountry>
