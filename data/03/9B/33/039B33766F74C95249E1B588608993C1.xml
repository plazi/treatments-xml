<emphasis bold="true" box="[248,312,658,684]" pageId="29" pageNumber="30">
<taxonomicNameLabel box="[248,312,658,684]" pageId="29" pageNumber="30" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="Volynkin &amp; Černý &amp; Huang, 2019" authorityName="Volynkin &amp; Černý &amp; Huang" authorityYear="2019" box="[325,498,657,683]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="syntypicoida" status="sp. n.">
<emphasis box="[325,498,657,683]" italics="true" pageId="29" pageNumber="30">B. syntypicoida</emphasis>
</taxonomicName>
<emphasis bold="true" box="[505,568,658,684]" pageId="29" pageNumber="30">
<taxonomicNameLabel box="[505,568,658,684]" pageId="29" pageNumber="30" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName authorityName="♀ Swinhoe" authorityYear="1906" box="[622,759,658,683]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="syntypica">
