<emphasis bold="true" box="[431,495,730,756]" pageId="29" pageNumber="30">
<taxonomicNameLabel box="[431,495,730,756]" pageId="29" pageNumber="30" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
