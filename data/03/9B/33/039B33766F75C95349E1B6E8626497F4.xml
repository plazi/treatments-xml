<quantity box="[550,713,1905,1933]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" metricValueMax="1.15" metricValueMin="1.05" pageId="28" pageNumber="29" unit="mm" value="11.0" valueMax="11.5" valueMin="10.5">10.5–11.5 mm</quantity>
