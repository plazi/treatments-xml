— 
<taxonomicName authority="(Hampson, 1898)" authorityName="Hampson" authorityYear="1898" baseAuthorityName="Hampson" baseAuthorityYear="1898" box="[411,753,621,647]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="mesortha">
