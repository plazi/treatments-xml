species. 
<taxonomicName box="[297,467,863,885]" class="Arachnida" family="Filistatidae" genus="Antilloides" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="413" phylum="Arthropoda" rank="species" species="abeli" status="new species">
