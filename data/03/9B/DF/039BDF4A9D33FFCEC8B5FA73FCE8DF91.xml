; II 2.0/ 
<number box="[151,185,1974,1999]" pageId="13" pageNumber="414" value="0.5">0.5</number>
/ 
<number box="[203,237,1975,1999]" pageId="13" pageNumber="414" value="1.8">1.8</number>
