, 
<collectingRegion box="[867,932,1569,1594]" country="Ireland" name="Laois" pageId="62" pageNumber="63">Laois</collectingRegion>
, 
<collectingRegion box="[950,1055,1569,1594]" country="Ireland" name="Limerick" pageId="62" pageNumber="63">Limerick</collectingRegion>
, 
<collectingRegion box="[1074,1142,1570,1594]" country="Ireland" name="Mayo" pageId="62" pageNumber="63">Mayo</collectingRegion>
, 
<collectingRegion box="[1160,1304,1570,1593]" country="Ireland" name="Roscommon" pageId="62" pageNumber="63">Roscommon</collectingRegion>
, 
<collectingRegion box="[1322,1381,1569,1594]" country="Ireland" name="Sligo" pageId="62" pageNumber="63">Sligo</collectingRegion>
and 
<collectingRegion box="[245,348,1605,1630]" country="Ireland" name="Wicklow" pageId="62" pageNumber="63">Wicklow</collectingRegion>
.
</paragraph>
