<emphasis bold="true" box="[245,360,1724,1749]" pageId="24" pageNumber="25">Counties:</emphasis>
<collectingRegion box="[367,448,1724,1749]" country="Ireland" name="South Dublin" pageId="24" pageNumber="25">Dublin</collectingRegion>
