<emphasis box="[323,525,836,861]" italics="true" pageId="23" pageNumber="24">Cumella nodulata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[533,623,836,861]" pageId="23" pageNumber="24">
<taxonomicNameLabel box="[533,623,836,861]" pageId="23" pageNumber="24" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
resembles 
<taxonomicName authority="Petrescu, 2002" authorityName="Petrescu" authorityYear="2002" box="[751,1053,836,861]" class="Malacostraca" family="Nannastacidae" genus="Cumella" kingdom="Animalia" order="Cumacea" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="ruetzleri">
<emphasis box="[751,880,836,861]" italics="true" pageId="23" pageNumber="24">C. ruetzleri</emphasis>
<bibRefCitation author="Petrescu" box="[888,1053,836,860]" pageId="23" pageNumber="24" refString="Petrescu, I. (2002) Cumacea (Crustacea: Peracarida) from Belize. Travaux du Museum d'Histoire naturelle &quot; Grigore Antipa &quot;, 64, 141 - 203." type="journal article" year="2002">Petrescu, 2002</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[1124,1196,836,861]" name="Belize" pageId="23" pageNumber="24">
<collectingRegion box="[1124,1196,836,861]" country="Belize" name="Belize" pageId="23" pageNumber="24">Belize</collectingRegion>
</collectingCountry>
(
<bibRefCitation author="Petrescu" box="[1211,1380,836,860]" pageId="23" pageNumber="24" refString="Petrescu, I. (2002) Cumacea (Crustacea: Peracarida) from Belize. Travaux du Museum d'Histoire naturelle &quot; Grigore Antipa &quot;, 64, 141 - 203." type="journal article" year="2002">Petrescu, 2002</bibRefCitation>
