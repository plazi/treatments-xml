<emphasis bold="true" box="[1353,1367,1706,1730]" pageId="7" pageNumber="360">d</emphasis>
, head, frontolateral; 
<emphasis bold="true" box="[290,301,1742,1766]" pageId="7" pageNumber="360">e</emphasis>
, antenna, outer face; 
<emphasis bold="true" box="[519,527,1742,1766]" pageId="7" pageNumber="360">f</emphasis>
