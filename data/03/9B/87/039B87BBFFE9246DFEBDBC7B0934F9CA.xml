<subSubSection pageId="8" pageNumber="763" type="diagnosis">
<paragraph blockId="8.[151,1127,1212,1601]" pageId="8" pageNumber="763">
<emphasis bold="true" box="[151,398,1386,1409]" pageId="8" pageNumber="763">Differential diagnosis.</emphasis>
<taxonomicName authorityName="Cho &amp; Borowiec" authorityYear="2016" box="[402,710,1386,1409]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="mausonensis" subGenus="Asiphytodecta">
<emphasis box="[402,525,1386,1409]" italics="true" pageId="8" pageNumber="763">Gonioctena</emphasis>
(
<emphasis box="[539,562,1386,1409]" italics="true" pageId="8" pageNumber="763">A.</emphasis>
) 
<emphasis box="[575,710,1386,1409]" italics="true" pageId="8" pageNumber="763">mausonensis</emphasis>
</taxonomicName>
can be easily distinguished from 
<taxonomicName baseAuthorityName="Achard" baseAuthorityYear="1924" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="eburoides" subGenus="Asiphytodecta">
<emphasis box="[1055,1074,1386,1409]" italics="true" pageId="8" pageNumber="763">G</emphasis>
. (
<emphasis box="[1095,1118,1386,1409]" italics="true" pageId="8" pageNumber="763">A.</emphasis>
) 
<emphasis box="[151,255,1418,1441]" italics="true" pageId="8" pageNumber="763">eburoides</emphasis>
</taxonomicName>
, 
<taxonomicName box="[269,491,1418,1441]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="flavoplagiata" subGenus="Asiphytodecta">
<emphasis box="[269,288,1418,1441]" italics="true" pageId="8" pageNumber="763">G</emphasis>
. (
<emphasis box="[312,335,1418,1441]" italics="true" pageId="8" pageNumber="763">A.</emphasis>
) 
<emphasis box="[351,491,1418,1441]" italics="true" pageId="8" pageNumber="763">flavoplagiata</emphasis>
</taxonomicName>
, 
<taxonomicName authority="Cho &amp; Borowiec, 2016" authorityName="Cho &amp; Borowiec" authorityYear="2016" box="[506,660,1418,1441]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="jindrai" status="sp. nov." subGenus="Asiphytodecta">
<emphasis box="[506,525,1418,1441]" italics="true" pageId="8" pageNumber="763">G</emphasis>
. (
<emphasis box="[548,572,1418,1441]" italics="true" pageId="8" pageNumber="763">A.</emphasis>
) 
<emphasis box="[587,660,1418,1441]" italics="true" pageId="8" pageNumber="763">jindrai</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[667,751,1418,1441]" pageId="8" pageNumber="763" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName authority="Cho &amp; Borowiec, 2016" authorityName="Cho &amp; Borowiec" authorityYear="2016" box="[804,1036,1418,1441]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="truncaticornis" status="sp. nov." subGenus="Asiphytodecta">
<emphasis box="[804,823,1418,1441]" italics="true" pageId="8" pageNumber="763">G</emphasis>
. (
<emphasis box="[846,869,1418,1441]" italics="true" pageId="8" pageNumber="763">A.</emphasis>
) 
<emphasis box="[885,1036,1418,1441]" italics="true" pageId="8" pageNumber="763">truncaticornis</emphasis>
</taxonomicName>
