<emphasis italics="true" pageId="10" pageNumber="2509">Amynthas sexpectatus</emphasis>
Tsai, Shen and Tsai
</taxonomicName>
, 
<taxonomicName authority="Tsai, Shen and Tsai" authorityName="Tsai, Shen and Tsai" box="[535,963,211,236]" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="10" pageNumber="2509" phylum="Annelida" rank="species" species="tayalis">
<emphasis box="[535,728,211,236]" italics="true" pageId="10" pageNumber="2509">Amynthas tayalis</emphasis>
Tsai, Shen and Tsai
</taxonomicName>
, 
<taxonomicName authority="Tsai, Shen and Tsai" authorityName="Tsai, Shen and Tsai" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="10" pageNumber="2509" phylum="Annelida" rank="species" species="tungpuensis">
<emphasis italics="true" pageId="10" pageNumber="2509">Amynthas tungpuensis</emphasis>
Tsai, Shen and Tsai
</taxonomicName>
, 
<taxonomicName authority="Hong and Kim" authorityName="Hong and Kim" box="[497,920,245,270]" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="10" pageNumber="2509" phylum="Annelida" rank="species" species="moakensis">
<emphasis box="[497,735,245,270]" italics="true" pageId="10" pageNumber="2509">Amynthas moakensis</emphasis>
Hong and Kim
</taxonomicName>
, 
<taxonomicName authority="Shen, Tsai and Tsai" authorityName="Shen, Tsai and Tsai" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="10" pageNumber="2509" phylum="Annelida" rank="species" species="fenestrus">
<emphasis box="[934,1155,245,270]" italics="true" pageId="10" pageNumber="2509">Amynthas fenestrus</emphasis>
Shen, Tsai and Tsai
