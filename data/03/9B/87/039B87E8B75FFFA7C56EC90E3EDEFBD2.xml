<quantity box="[727,834,924,950]" metricMagnitude="-3" metricUnit="m" metricValue="2.06" pageId="6" pageNumber="7" unit="mm" value="2.06">2.06 mm</quantity>
PL 
<quantity box="[886,996,924,950]" metricMagnitude="-3" metricUnit="m" metricValue="1.22" pageId="6" pageNumber="7" unit="mm" value="1.22">1.22 mm</quantity>
, PW 
<quantity box="[1063,1172,924,950]" metricMagnitude="-3" metricUnit="m" metricValue="1.83" pageId="6" pageNumber="7" unit="mm" value="1.83">1.83 mm</quantity>
, Ediam 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="5.699999999999999" pageId="6" pageNumber="7" unit="mm" value="0.57">0.57 mm</quantity>
