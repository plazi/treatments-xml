<paragraph blockId="17.[151,409,1417,1477]" box="[151,409,1417,1443]" pageId="17" pageNumber="118">
<heading bold="true" box="[151,409,1417,1443]" fontSize="11" level="1" pageId="17" pageNumber="118" reason="1">
<emphasis bold="true" box="[151,409,1417,1443]" pageId="17" pageNumber="118">
<taxonomicName box="[151,335,1417,1443]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="118" phylum="Arthropoda" rank="species" species="micula" status="sp. n.">
<emphasis bold="true" box="[151,335,1417,1443]" italics="true" pageId="17" pageNumber="118">Manota micula</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[342,409,1417,1443]" pageId="17" pageNumber="118" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
</heading>
