<taxonomicName authority="Pariselle &amp; Euzet, 2003" authorityName="Pariselle &amp; Euzet" authorityYear="2003" box="[359,918,332,356]" class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="4" phylum="Platyhelminthes" rank="species" species="berradae">
<emphasis box="[359,632,332,356]" italics="true" pageId="4">Cichlidogyrus berradae</emphasis>
Pariselle &amp; Euzet, 
<number box="[862,918,332,356]" pageId="4" value="2003.0">2003</number>
</taxonomicName>
and that of 
<taxonomicName authority="Pariselle &amp; Euzet, 1995" authorityName="Pariselle &amp; Euzet" authorityYear="1995" class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="4" phylum="Platyhelminthes" rank="species" species="yanni">
<emphasis box="[1065,1296,332,356]" italics="true" pageId="4">Cichlidogyrus yanni</emphasis>
