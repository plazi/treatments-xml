</taxonomicName>
all have a much longer penis (&gt; 
<number box="[632,676,223,247]" pageId="4" value="100.0">100</number>
,&gt; 
<number box="[713,744,223,247]" pageId="4" value="60.0">60</number>
,&gt; 
<number box="[780,824,223,247]" pageId="4" value="100.0">100</number>
,&gt; 
<number box="[861,905,223,247]" pageId="4" value="190.0">190</number>
,&gt; 
<number box="[942,972,223,247]" pageId="4" value="60.0">60</number>
,&gt; 
<number box="[1009,1051,223,247]" pageId="4" value="120.0">120</number>
respectively 
<emphasis box="[1202,1225,225,248]" italics="true" pageId="4">vs</emphasis>
. 
<number box="[1241,1273,223,247]" pageId="4" value="27.0">27</number>
). 
<taxonomicName box="[1293,1436,224,248]" class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="4" phylum="Platyhelminthes" rank="species" species="nshomboi">
