<number box="[897,911,367,391]" pageId="4" value="1.0">1</number>
(
<number box="[926,955,367,391]" pageId="4" value="26.0">26</number>
and 
<number box="[1010,1038,367,391]" pageId="4" value="24.0">24</number>
<emphasis box="[1044,1067,369,392]" italics="true" pageId="4">vs</emphasis>
. 
<number box="[1082,1113,367,391]" pageId="4" value="18.0">18</number>
).
