), U IV = 
<number box="[1109,1137,1718,1742]" pageId="3" value="19.0">19</number>
± 
<number box="[1167,1202,1718,1742]" pageId="3" value="0.8">0.8</number>
(
<number box="[1217,1288,1718,1742]" pageId="3" value="19.0" valueMax="21.0" valueMin="17.0">17–21</number>
) (
<number box="[1313,1344,1718,1742]" pageId="3" value="48.0">48</number>
), U V = 
<number box="[151,179,1753,1777]" pageId="3" value="19.0">19</number>
± 
<number box="[212,247,1753,1777]" pageId="3" value="0.8">0.8</number>
(
<number box="[265,336,1753,1777]" pageId="3" value="19.5" valueMax="21.0" valueMin="18.0">18–21</number>
) (
<number box="[363,395,1753,1777]" pageId="3" value="50.0">50</number>
), U VI = 
<number box="[510,538,1753,1777]" pageId="3" value="19.0">19</number>
