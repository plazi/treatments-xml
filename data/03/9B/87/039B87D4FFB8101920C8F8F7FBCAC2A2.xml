</taxonomicName>
and 
<taxonomicName class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="4" phylum="Platyhelminthes" rank="species" species="nandidae">
