<number box="[624,637,1700,1724]" pageId="2" value="2.0">2</number>
</emphasis>
</date>
spaced, subventral; 
<emphasis box="[868,894,1700,1724]" italics="true" pageId="2">
<date box="[868,894,1700,1724]" pageId="2">
v 
<number box="[880,894,1700,1724]" pageId="2" value="4.0">4</number>
</date>
