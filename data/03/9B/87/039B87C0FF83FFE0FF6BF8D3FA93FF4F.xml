<taxonomicName authority="Suarez, 1963" authorityName="Suarez" authorityYear="1963" box="[151,436,1907,1932]" class="Insecta" family="Mutillidae" genus="Paramyrmosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="527" phylum="Arthropoda" rank="genus">
