<caption httpUri="https://zenodo.org/record/1207739/files/figure.png" pageId="75" pageNumber="76" startId="75.[151,250,1553,1575]" targetBox="[151,1436,193,1511]" targetPageId="75">
<paragraph blockId="75.[151,1436,1553,1636]" pageId="75" pageNumber="76">
