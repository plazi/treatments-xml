<subSubSection pageId="4" pageNumber="5" type="etymology">
<paragraph blockId="4.[264,1324,284,550]" pageId="4" pageNumber="5">
<emphasis box="[312,435,484,509]" italics="true" pageId="4" pageNumber="5">Etymology</emphasis>
