<subSubSection box="[312,979,724,750]" pageId="8" pageNumber="9" type="distribution">
<paragraph blockId="8.[264,1324,284,830]" box="[312,979,724,750]" pageId="8" pageNumber="9">
<emphasis box="[312,455,724,749]" italics="true" pageId="8" pageNumber="9">Distribution</emphasis>
