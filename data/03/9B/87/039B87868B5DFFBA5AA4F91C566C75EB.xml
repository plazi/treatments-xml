<taxonomicName authority="Brescovit, 1993: 1 - 2" authorityName="Brescovit" authorityPageNumber="1 - 2" authorityYear="1993" box="[312,622,1979,2001]" class="Arachnida" family="Anyphaenidae" genus="Hibana" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="393" phylum="Arthropoda" rank="species" species="talmina">
