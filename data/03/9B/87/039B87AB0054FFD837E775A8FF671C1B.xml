<emphasis box="[283,394,1483,1507]" italics="true" pageId="15" pageNumber="116">
<typeStatus box="[283,389,1483,1507]" pageId="15" pageNumber="116">Holotype</typeStatus>
.
</emphasis>
Male, 
<collectingCountry box="[473,612,1482,1506]" name="Ecuador" pageId="15" pageNumber="116">ECUADOR</collectingCountry>
, Cotopaxi, San Francisco de Las Pampas, Otonga Reserve, Malaise trap, 
<date box="[151,270,1518,1542]" pageId="15" pageNumber="116" value="2004-06-19">
<number box="[151,178,1518,1542]" pageId="15" pageNumber="116" value="19.0">19</number>
.vi. 
<number box="[208,270,1518,1542]" pageId="15" pageNumber="116" value="2004.0">2004</number>
</date>
(on slide, in 
<collectionCode box="[426,515,1518,1542]" pageId="15" pageNumber="116">BMNH</collectionCode>
). 
<typeStatus box="[535,638,1520,1543]" pageId="15" pageNumber="116">
<emphasis box="[535,638,1520,1543]" italics="true" pageId="15" pageNumber="116">Paratype</emphasis>
</typeStatus>
. 
<specimenCount box="[654,735,1518,1543]" pageId="15" pageNumber="116" type="male">
<emphasis bold="true" box="[654,735,1518,1543]" pageId="15" pageNumber="116">
<number box="[654,668,1518,1543]" pageId="15" pageNumber="116" value="1.0">1</number>
male
</emphasis>
