<subSubSection pageId="17" pageNumber="18" type="discussion">
<paragraph blockId="17.[151,1435,1166,1298]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[201,317,1239,1263]" pageId="17" pageNumber="18">Remarks.</emphasis>
<taxonomicName box="[325,447,1239,1262]" class="Insecta" family="Cicadellidae" genus="Onukigallia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="arisana">
<emphasis box="[325,447,1239,1262]" italics="true" pageId="17" pageNumber="18">O. arisana</emphasis>
</taxonomicName>
