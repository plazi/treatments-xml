<taxonomicName authorityName="Chaowasku" authorityYear="2012" box="[1346,1435,673,698]" class="Magnoliopsida" family="Annonaceae" genus="Hubera" kingdom="Plantae" order="Magnoliales" pageId="18" pageNumber="51" phylum="Tracheophyta" rank="genus">
<emphasis box="[1346,1435,673,698]" italics="true" pageId="18" pageNumber="51">Hubera</emphasis>
