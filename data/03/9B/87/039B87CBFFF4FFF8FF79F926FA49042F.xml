<collectingCountry box="[226,302,357,378]" name="Panama" pageId="13" pageNumber="400">Panama</collectingCountry>
(
<collectingRegion box="[314,488,357,379]" country="Panama" name="Chiriqui" pageId="13" pageNumber="400">Chiriqui province</collectingRegion>
)........................................................... 
<taxonomicName authority="Nascimento &amp; Bressan &amp; Bocakova, 2020" authorityName="Nascimento &amp; Bressan &amp; Bocakova" authorityYear="2020" box="[1224,1353,357,378]" class="Insecta" family="Lycidae" genus="Currhaeus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="400" phylum="Arthropoda" rank="species" species="championi" status="sp. nov.">
