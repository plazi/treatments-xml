<number box="[1223,1236,1425,1449]" pageId="5" value="3.0">3</number>
</date>
d, 
<date box="[1263,1292,1425,1449]" pageId="5">
v 
<number box="[1276,1292,1425,1449]" pageId="5" value="4.0">4</number>
</date>
, ad, (
<date box="[1357,1386,1425,1449]" pageId="5">
v 
<number box="[1370,1386,1425,1449]" pageId="5" value="5.0">5</number>
</date>
+ 
