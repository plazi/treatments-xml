. 
<taxonomicName box="[281,476,1985,2007]" class="Arachnida" family="Tenuipalpidae" genus="Tenuipalpus" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="494" phylum="Arthropoda" rank="species" species="bacuri" status="sp. nov.">
<emphasis box="[281,476,1985,2007]" italics="true" pageId="1" pageNumber="494">Tenuipalpus bacuri</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[482,562,1985,2006]" pageId="1" pageNumber="494" rank="species">
<emphasis bold="true" box="[482,562,1985,2006]" pageId="1" pageNumber="494">sp. nov.</emphasis>
