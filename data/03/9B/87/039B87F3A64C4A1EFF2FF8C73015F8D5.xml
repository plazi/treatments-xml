<paragraph blockId="35.[151,1437,151,1808]" pageId="35" pageNumber="36">
<materialsCitation box="[201,1279,1428,1453]" collectingDate="1901-06-06" collectingDateMax="1901-06-21" collectingDateMin="1901-06-06" collectorName="Huelva &amp; Coto" country="Spain" location="Spain" pageId="35" pageNumber="36" specimenCount="16" specimenCount-female="6" specimenCount-male="10">
<emphasis bold="true" box="[201,269,1428,1453]" pageId="35" pageNumber="36">
<collectingCountry box="[201,269,1428,1453]" name="Spain" pageId="35" pageNumber="36">Spain</collectingCountry>
