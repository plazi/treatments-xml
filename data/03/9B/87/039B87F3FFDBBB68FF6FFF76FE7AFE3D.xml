. 
<taxonomicName authority="Saguiah &amp; Molin &amp; Tavares, 2020" authorityName="Saguiah &amp; Molin &amp; Tavares" authorityYear="2020" box="[327,513,1885,1909]" class="Insecta" family="Chalcididae" genus="Chalcis" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="378" phylum="Arthropoda" rank="species" species="winstonae" status="sp. nov.">
