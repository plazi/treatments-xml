<taxonomicNameLabel box="[519,599,1885,1909]" pageId="25" pageNumber="378" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype female: 
<emphasis bold="true" box="[788,801,1885,1909]" pageId="25" pageNumber="378">a</emphasis>
, habitus, lateral; 
<emphasis bold="true" box="[974,988,1885,1909]" pageId="25" pageNumber="378">b</emphasis>
, habitus, dorsal; 
<emphasis bold="true" box="[1161,1172,1885,1909]" pageId="25" pageNumber="378">c</emphasis>
, head, frontal; 
<emphasis bold="true" box="[1325,1339,1885,1909]" pageId="25" pageNumber="378">d</emphasis>
, antenna, inner face; 
<emphasis bold="true" box="[265,276,1921,1945]" pageId="25" pageNumber="378">e</emphasis>
