<number box="[898,926,870,894]" pageId="6" value="28.0">28</number>
(
<number box="[941,1014,870,894]" pageId="6" value="28.0" valueMax="30.0" valueMin="26.0">26–30</number>
) (
<number box="[1038,1069,870,894]" pageId="6" value="27.0">27</number>
), c = 
