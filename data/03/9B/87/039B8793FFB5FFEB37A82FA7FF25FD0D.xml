</taxonomicName>
<taxonomicNameLabel box="[1179,1259,1929,1950]" pageId="9" pageNumber="459" rank="species">
<emphasis bold="true" box="[1179,1259,1929,1950]" pageId="9" pageNumber="459">sp. nov.</emphasis>
</taxonomicNameLabel>
(male, length 5.7 mm); 43, 
<taxonomicName box="[255,398,1959,1981]" class="Insecta" family="Cicadellidae" genus="Subrasaca" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="459" phylum="Arthropoda" rank="species" species="bimaculata" status="sp. nov.">
<emphasis box="[255,398,1959,1981]" italics="true" pageId="9" pageNumber="459">S. bimaculata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[406,490,1960,1981]" pageId="9" pageNumber="459">
<taxonomicNameLabel box="[406,490,1960,1981]" pageId="9" pageNumber="459" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(male, length 6.1 mm); 44, 
<taxonomicName authority="Signoret, 1855" authorityName="Signoret" authorityYear="1855" box="[790,1116,1959,1981]" class="Insecta" family="Cicadellidae" genus="Subrasaca" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="459" phylum="Arthropoda" rank="species" species="flavolineata">
<emphasis box="[790,939,1959,1981]" italics="true" pageId="9" pageNumber="459">S. flavolineata</emphasis>
