<emphasis box="[323,546,1947,1969]" italics="true" pageId="1" pageNumber="594">Epicnemis linguiformis</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[558,613,1948,1969]" pageId="1" pageNumber="594">
<taxonomicNameLabel box="[558,613,1948,1969]" pageId="1" pageNumber="594" rank="species">sp. n.</taxonomicNameLabel>
