<taxonomicName authority="Meyrick" authorityName="Meyrick" box="[1210,1436,1021,1044]" class="Insecta" family="Oecophoridae" genus="Acria" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="227" phylum="Arthropoda" rank="species" species="ceramitis">
