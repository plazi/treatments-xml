<collectingRegion box="[554,599,1102,1126]" country="South Korea" name="Jeju" pageId="8" pageNumber="45">Jeju</collectingRegion>
<collectingMunicipality box="[606,675,1102,1126]" pageId="8" pageNumber="45">Island</collectingMunicipality>
