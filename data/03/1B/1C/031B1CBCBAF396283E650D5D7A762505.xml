<subSubSection pageId="null" pageNumber="557" type="vernacular_name">
<paragraph pageId="null" pageNumber="557">
Echte 
<normalizedToken originalValue="Geißraute" pageId="null" pageNumber="557">Geissraute</normalizedToken>
</paragraph>
