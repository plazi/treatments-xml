<heading bold="true" box="[151,716,151,177]" fontSize="11" level="1" pageId="4" pageNumber="63" reason="1">
<emphasis bold="true" box="[151,716,151,177]" pageId="4" pageNumber="63">
<taxonomicName authority="Nemesio" authorityName="Nemesio" box="[151,636,151,177]" class="Insecta" family="Apidae" genus="Euglossa" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="63" phylum="Arthropoda" rank="species" species="marianae" status="sp. n." subGenus="Euglossa">
<emphasis bold="true" box="[151,522,151,177]" italics="true" pageId="4" pageNumber="63">Euglossa (Euglossa) marianae</emphasis>
Nemésio
</taxonomicName>
, 
<taxonomicNameLabel box="[650,716,151,177]" pageId="4" pageNumber="63" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
