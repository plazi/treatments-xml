(= 
<taxonomicName authority="Guilding, 1829" authorityName="Guilding" authorityYear="1829" box="[708,1079,621,647]" class="Polyplacophora" family="Cryptoplacidae" genus="Chitonellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chitonida" pageId="34" pageNumber="435" phylum="Mollusca" rank="species" species="latus">
