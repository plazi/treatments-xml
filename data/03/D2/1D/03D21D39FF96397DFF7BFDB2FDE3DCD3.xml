<subSubSection pageId="36" pageNumber="437" type="discussion">
<paragraph blockId="36.[151,1436,584,720]" pageId="36" pageNumber="437">
<emphasis bold="true" box="[199,315,621,647]" pageId="36" pageNumber="437">Remarks.</emphasis>
<taxonomicName authorityName="Blainville" authorityYear="1818" box="[322,448,621,647]" class="Polyplacophora" family="Cryptoplacidae" genus="Cryptoplax" kingdom="Animalia" order="Chitonida" pageId="36" pageNumber="437" phylum="Mollusca" rank="genus">
<emphasis box="[322,448,621,647]" italics="true" pageId="36" pageNumber="437">Cryptoplax</emphasis>
</taxonomicName>
