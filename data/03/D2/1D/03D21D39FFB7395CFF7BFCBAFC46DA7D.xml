<paragraph blockId="5.[151,1436,845,1150]" box="[151,1191,845,873]" pageId="5" pageNumber="406">
<emphasis bold="true" box="[151,306,845,872]" pageId="5" pageNumber="406">
<typeStatus box="[151,210,846,872]" pageId="5" pageNumber="406">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Thiele, 1909" authorityName="Thiele" authorityYear="1909" box="[313,914,846,873]" class="Polyplacophora" family="Leptochitonidae" genus="Lepidopleurus" kingdom="Animalia" order="Lepidopleurida" pageId="5" pageNumber="406" phylum="Mollusca" rank="species" species="acuminatus" subGenus="Parachiton">
