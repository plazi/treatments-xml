</emphasis>
female (
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[681,765,1754,1778]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="3" pageNumber="98">USNM</collectionCode>
) 
<collectingCountry box="[789,884,1754,1779]" name="Ecuador" pageId="3" pageNumber="98">Ecuador</collectingCountry>
