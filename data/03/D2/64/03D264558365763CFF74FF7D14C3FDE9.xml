<collectingCountry box="[729,805,362,385]" name="France" pageId="16" pageNumber="317">France</collectingCountry>
to 
<collectingCountry box="[841,889,362,385]" name="Iran" pageId="16" pageNumber="317">Iran</collectingCountry>
