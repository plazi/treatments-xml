<emphasis box="[288,532,1383,1406]" italics="true" pageId="6" pageNumber="51">
<taxonomicName authority="Kinberg, 1866" authorityName="Kinberg" authorityYear="1866" box="[288,398,1383,1406]" class="Oligochaeta" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="6" pageNumber="51" phylum="Annelida" rank="genus">Pheretima</taxonomicName>
