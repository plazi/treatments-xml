<emphasis box="[528,651,691,715]" italics="true" pageId="17" pageNumber="18">
<taxonomicName authorityName="Pruthi" authorityYear="1930" box="[528,647,691,715]" class="Insecta" family="Cicadellidae" genus="Agallia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="robusta">A. robusta</taxonomicName>
