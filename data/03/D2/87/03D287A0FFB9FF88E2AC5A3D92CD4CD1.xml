<paragraph blockId="1.[159,1269,1730,1839]" lastBlockId="2.[151,1437,152,402]" lastPageId="2" lastPageNumber="64" pageId="1" pageNumber="63">
Eye diameter 10.0 
<number box="[824,916,1773,1796]" pageId="1" pageNumber="63" value="10.6" valueMax="11.6" valueMin="9.6">9.6–11.6</number>
<number box="[1073,1119,1773,1796]" pageId="1" pageNumber="63" value="10.6">10.6</number>
