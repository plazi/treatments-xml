<taxonomicName authority="(Hardy)" authorityName="O’Grady &amp; Bonacum &amp; Desalle &amp; Val" authorityYear="2003" baseAuthorityName="O’Grady &amp; Bonacum &amp; Desalle &amp; Val" baseAuthorityYear="2003" box="[1123,1322,1644,1670]" class="Insecta" family="Drosophilidae" genus="Scaptomyza" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="vittigera">
