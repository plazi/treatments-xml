, for 
<taxonomicName authority="(Hardy, 1965)" authorityName="Knab" authorityYear="1914" baseAuthorityName="Hardy" baseAuthorityYear="1965" box="[613,1047,1558,1584]" class="Insecta" family="Drosophilidae" genus="Titanochaeta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="silvicola">
