<emphasis italics="true" pageId="12" pageNumber="13">... .........................................................................................................</emphasis>
<taxonomicName authority="(Hardy)" authorityName="O’Grady &amp; Bonacum &amp; Desalle &amp; Val" authorityYear="2003" baseAuthorityName="O’Grady &amp; Bonacum &amp; Desalle &amp; Val" baseAuthorityYear="2003" box="[1091,1322,1404,1430]" class="Insecta" family="Drosophilidae" genus="Scaptomyza" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="chauliodon">
