), and 
<taxonomicName authority="O'Grady et al." authorityName="O'Grady" authorityYear="2003" box="[829,1317,1518,1544]" class="Insecta" family="Drosophilidae" genus="Scaptomyza" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="neosilvicola" status="new replacement name">
<emphasis box="[829,1123,1519,1544]" italics="true" pageId="11" pageNumber="12">Scaptomyza neosilvicola</emphasis>
