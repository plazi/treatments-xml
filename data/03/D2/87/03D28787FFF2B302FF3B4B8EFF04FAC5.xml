<quantity box="[457,621,1052,1076]" metricMagnitude="-3" metricUnit="m" metricValue="3.25" metricValueMax="3.3" metricValueMin="3.2" pageId="16" pageNumber="17" unit="mm" value="3.25" valueMax="3.3" valueMin="3.2">3.20–3.30 mm</quantity>
long, 
<quantity box="[696,861,1052,1076]" metricMagnitude="-3" metricUnit="m" metricValue="1.08" metricValueMax="1.1" metricValueMin="1.06" pageId="16" pageNumber="17" unit="mm" value="1.08" valueMax="1.1" valueMin="1.06">1.06–1.10 mm</quantity>
wide across eyes. Female 
<quantity box="[1171,1258,1052,1076]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="16" pageNumber="17" unit="mm" value="3.7">3.7 mm</quantity>
