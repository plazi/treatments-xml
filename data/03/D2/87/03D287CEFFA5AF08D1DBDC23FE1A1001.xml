<paragraph blockId="17.[151,511,855,881]" box="[151,511,855,881]" pageId="17" pageNumber="18">
<heading bold="true" box="[151,511,855,881]" fontSize="11" level="1" pageId="17" pageNumber="18" reason="1">
<emphasis bold="true" box="[151,511,855,881]" pageId="17" pageNumber="18">
2. 
<taxonomicName authority="Vaz-De-Mello, 2008" authorityName="Vaz-De-Mello" authorityYear="2008" box="[180,367,855,881]" class="Insecta" family="Scarabaeidae" genus="Bradypodidium" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[180,367,855,881]" italics="true" pageId="17" pageNumber="18">Bradypodidium</emphasis>
