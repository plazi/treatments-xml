<paragraph blockId="40.[151,499,1151,1177]" box="[151,499,1151,1177]" pageId="40" pageNumber="41">
<heading bold="true" box="[151,499,1151,1177]" fontSize="11" level="1" pageId="40" pageNumber="41" reason="1">
<emphasis bold="true" box="[151,499,1151,1177]" pageId="40" pageNumber="41">
12. 
<emphasis bold="true" box="[195,361,1151,1177]" italics="true" pageId="40" pageNumber="41">
<taxonomicName authority="Vaz-De-Mello, 2008" authorityName="Vaz-De-Mello" authorityYear="2008" box="[195,356,1151,1177]" class="Insecta" family="Scarabaeidae" genus="Pereiraidium" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus" status="new genus">Pereiraidium</taxonomicName>
,
</emphasis>
<taxonomicNameLabel box="[369,499,1151,1177]" pageId="40" pageNumber="41" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
