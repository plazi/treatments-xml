<taxonomicName authority="Hook." authorityName="Hook." box="[832,1137,1337,1362]" class="Magnoliopsida" family="Asteraceae" genus="Senecio" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="species" species="triangularis">
