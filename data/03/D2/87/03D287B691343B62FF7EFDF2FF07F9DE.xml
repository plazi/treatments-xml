<taxonomicName authority="(Carter, 1874)" authorityName="Vosmaer" authorityYear="1887" baseAuthorityName="Carter" baseAuthorityYear="1874" box="[746,1106,1145,1171]" class="Demospongiae" family="Mycalidae" genus="Esperella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poecilosclerida" pageId="122" pageNumber="123" phylum="Porifera" rank="species" species="cupressiformis">
