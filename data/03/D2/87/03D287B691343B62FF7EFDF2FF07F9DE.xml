<taxonomicName authorityName="Lundbeck" authorityYear="1905" box="[662,793,1109,1135]" class="Demospongiae" family="Cladorhizidae" genus="Lycopodina" higherTaxonomySource="CoL" kingdom="Plantae" order="Poecilosclerida" pageId="122" pageNumber="123" phylum="Porifera" rank="genus">
