<paragraph blockId="40.[189,1179,1530,1646]" lastBlockId="41.[106,1097,1200,1311]" lastPageId="41" lastPageNumber="130" pageId="40" pageNumber="129">
<taxonomicName authorityName="Assing" authorityYear="2011" box="[189,362,1530,1554]" class="Insecta" family="Staphylinidae" genus="Neosclerus" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="129" phylum="Arthropoda" rank="species" species="glaber">
<emphasis box="[189,362,1530,1554]" italics="true" pageId="40" pageNumber="129">Neosclerus glaber</emphasis>
</taxonomicName>
<materialsCitation country="China" county="Fujian" elevation="1100" lastPageId="41" lastPageNumber="130" location="Oriental" municipality="The" pageId="40" pageNumber="129" specimenCount="570" stateProvince="Fujian and Yunnan">
