<taxonomicName authority="Benth." authorityName="Benth." box="[436,809,356,381]" class="Magnoliopsida" family="Boraginaceae" genus="Emmenanthe" higherTaxonomySource="GBIF" kingdom="Plantae" order="Boraginales" pageId="60" pageNumber="61" phylum="Tracheophyta" rank="species" species="penduliflora">
