<emphasis bold="true" box="[199,315,1164,1188]" inLineHeading="true" pageId="86" pageNumber="87" reason="1">Remarks.</emphasis>
<taxonomicName authorityName="C.A.Viraktamath" authorityYear="2011" box="[321,443,1164,1188]" class="Insecta" family="Cicadellidae" genus="Japanagallia" kingdom="Animalia" order="Hemiptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="malaisei">
