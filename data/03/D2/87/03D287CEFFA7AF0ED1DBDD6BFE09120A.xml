<subSubSection box="[151,498,543,569]" pageId="19" pageNumber="20" type="nomenclature">
<paragraph blockId="19.[151,498,543,569]" box="[151,498,543,569]" pageId="19" pageNumber="20">
<heading bold="true" box="[151,498,543,569]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis bold="true" box="[151,498,543,569]" pageId="19" pageNumber="20">
3. 
<taxonomicName authority="Vaz-De-Mello, 2008" authorityName="Vaz-De-Mello" authorityYear="2008" box="[180,362,543,569]" class="Insecta" family="Scarabaeidae" genus="Degallieridium" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[180,362,543,569]" italics="true" pageId="19" pageNumber="20">Degallieridium</emphasis>
