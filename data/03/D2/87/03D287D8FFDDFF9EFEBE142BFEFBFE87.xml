, 
<collectingCountry box="[401,545,219,243]" name="Mozambique" pageId="30" pageNumber="617">Mozambique</collectingCountry>
, 
<collectingCountry box="[555,827,219,243]" name="South Africa" pageId="30" pageNumber="617">Republic of South Africa</collectingCountry>
, 
<collectingCountry box="[838,921,219,243]" name="Uganda" pageId="30" pageNumber="617">Uganda</collectingCountry>
