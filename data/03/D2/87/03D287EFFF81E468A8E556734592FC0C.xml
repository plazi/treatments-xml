<emphasis box="[673,812,1878,1903]" italics="true" pageId="33" pageNumber="34">M. scandens</emphasis>
</taxonomicName>
(L.) Willd., * 
<taxonomicName box="[967,1194,1878,1903]" class="Magnoliopsida" family="Asteraceae" genus="Silphium" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="laciniatum">
<emphasis box="[967,1194,1878,1903]" italics="true" pageId="33" pageNumber="34">Silphium laciniatum</emphasis>
</taxonomicName>
L., 
<taxonomicName box="[1239,1398,1878,1903]" class="Magnoliopsida" family="Asteraceae" genus="Silphium" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="perfoliatum">
