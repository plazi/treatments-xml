<collectionCode LSID="urn:lsid:biocol.org:col:14382" box="[182,250,152,176]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="13" pageNumber="433">MOD</collectionCode>
