<number box="[590,622,1983,2007]" pageId="12" pageNumber="432" value="24.0">24</number>
: 
<number box="[626,658,1983,2007]" pageId="12" pageNumber="432" value="20.0">20</number>
), 
<number box="[678,713,1983,2007]" pageId="12" pageNumber="432" value="1.8">1.8</number>
