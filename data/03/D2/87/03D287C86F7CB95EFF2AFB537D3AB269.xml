<taxonomicName authorityName="SCHEERPELTZ" authorityYear="1974" box="[339,630,969,991]" class="Insecta" family="Staphylinidae" genus="Stenectinobregma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="1115" phylum="Arthropoda" rank="species" species="rudebecki">
