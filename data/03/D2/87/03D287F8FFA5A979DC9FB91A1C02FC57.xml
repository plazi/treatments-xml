<taxonomicName box="[314,488,619,643]" class="Arachnida" family="Phytoseiidae" genus="Paraphytoseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
<emphasis box="[314,488,619,643]" italics="true" pageId="6" pageNumber="299">Paraphytoseius</emphasis>
</taxonomicName>
, 
<taxonomicName box="[515,658,619,643]" class="Arachnida" family="Phytoseiidae" genus="Phytoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
<emphasis box="[515,658,619,643]" italics="true" pageId="6" pageNumber="299">Phytoseiulus</emphasis>
</taxonomicName>
