J 
<number box="[463,475,511,535]" pageId="6" pageNumber="299" value="2.0">2</number>
</emphasis>
, 
<emphasis box="[491,517,511,534]" italics="true" pageId="6" pageNumber="299">
J 
<number box="[504,517,511,534]" pageId="6" pageNumber="299" value="5.0">5</number>
