<taxonomicName box="[686,829,619,642]" class="Arachnida" family="Phytoseiidae" genus="Proprioseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
<emphasis box="[686,829,619,642]" italics="true" pageId="6" pageNumber="299">Proprioseius</emphasis>
</taxonomicName>
, and some species of 
<taxonomicName authority="Berlese" authorityName="Berlese" box="[1143,1375,618,643]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
<emphasis box="[1143,1270,619,643]" italics="true" pageId="6" pageNumber="299">Amblyseius</emphasis>
Berlese
</taxonomicName>
and 
<taxonomicName box="[151,327,655,678]" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
<emphasis box="[151,327,655,678]" italics="true" pageId="6" pageNumber="299">Proprioseiopsis</emphasis>
</taxonomicName>
