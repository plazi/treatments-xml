<taxonomicName authority="Gauthier, 1939" authorityName="Gauthier" authorityYear="1939" box="[284,643,1368,1390]" class="Branchiopoda" genus="Leydigia" kingdom="Animalia" order="Diplostraca" pageId="7" pageNumber="52" phylum="Arthropoda" rank="species" species="ciliata">
<taxonomicName authority="Gauthier" authorityName="Gauthier" box="[284,578,1368,1390]" class="Branchiopoda" genus="Leydigia" kingdom="Animalia" order="Diplostraca" pageId="7" pageNumber="52" phylum="Arthropoda" rank="species" species="ciliata">
