<paragraph blockId="7.[264,1324,1735,1881]" pageId="7" pageNumber="58">
<taxonomicName box="[264,376,1735,1760]" class="Arachnida" family="Syringophilidae" genus="Creagonycha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="58" phylum="Arthropoda" rank="species" species="totana">
