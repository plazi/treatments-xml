<taxonomicName authority="Leisler" authorityName="Leisler" box="[556,850,1775,1801]" class="Aves" family="Scolopacidae" genus="Calidris" kingdom="Animalia" order="Charadriiformes" pageId="7" pageNumber="58" phylum="Chordata" rank="species" species="minuta">
