<taxonomicName authority="Pallas" authorityName="Pallas" box="[454,706,1815,1841]" class="Aves" family="Scolopacidae" genus="Calidris" kingdom="Animalia" order="Charadriiformes" pageId="7" pageNumber="58" phylum="Chordata" rank="species" species="alba">
