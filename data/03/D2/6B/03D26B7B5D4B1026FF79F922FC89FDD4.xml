<taxonomicName authorityName="Boiteau &amp; Mannoni" authorityYear="1948" baseAuthorityName="Perrier de la Bathie" baseAuthorityYear="1924" box="[136,517,1904,1925]" class="Magnoliopsida" family="Crassulaceae" genus="Kalanchoe" kingdom="Plantae" order="Saxifragales" pageId="3" pageNumber="121" phylum="Tracheophyta" rank="variety" species="miniata" variety="andringitrensis">
