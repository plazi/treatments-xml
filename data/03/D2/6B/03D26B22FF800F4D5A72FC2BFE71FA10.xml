<taxonomicName authority="Temminck" authorityName="Temminck" box="[880,1323,1105,1131]" class="Aves" family="Laridae" genus="Chlidonias" kingdom="Animalia" order="Charadriiformes" pageId="11" pageNumber="62" phylum="Chordata" rank="species" species="leucopterus">
