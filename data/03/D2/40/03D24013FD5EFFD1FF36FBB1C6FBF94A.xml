; body depth at anus 
<number box="[1117,1155,1646,1670]" pageId="13" pageNumber="62" value="3.9">3.9</number>
; body width at anus 
<number box="[1393,1431,1646,1670]" pageId="13" pageNumber="62" value="4.1">4.1</number>
; head depth at branchial basket 
<number box="[494,532,1681,1706]" pageId="13" pageNumber="62" value="5.8">5.8</number>
