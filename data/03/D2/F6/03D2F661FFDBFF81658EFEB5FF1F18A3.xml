<collectingCountry box="[359,430,1853,1878]" name="China" pageId="16" pageNumber="281">China</collectingCountry>
: West 
<collectingRegion box="[503,588,1854,1877]" country="China" name="Yunnan" pageId="16" pageNumber="281">Yunnan</collectingRegion>
(
<figureCitation box="[604,672,1853,1878]" captionStart="FIGURE 3" captionStartId="5.[151,250,1914,1936]" captionTargetBox="[211,1345,495,1880]" captionTargetId="figure@5.[211,1345,495,1895]" captionTargetPageId="5" captionText="FIGURE 3. Distribution map of Ctenoneura species in Yunnan Province and Myanmar." httpUri="https://zenodo.org/record/343799/files/figure.png" pageId="16" pageNumber="281">Fig. 3</figureCitation>
