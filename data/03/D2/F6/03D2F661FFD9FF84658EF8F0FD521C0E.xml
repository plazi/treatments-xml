<collectingCountry box="[359,430,1018,1043]" name="China" pageId="20" pageNumber="285">China</collectingCountry>
: South 
<collectingRegion box="[513,598,1019,1042]" country="China" name="Yunnan" pageId="20" pageNumber="285">Yunnan</collectingRegion>
