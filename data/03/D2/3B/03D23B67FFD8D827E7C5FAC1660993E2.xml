<caption box="[151,1412,1400,1423]" httpUri="https://zenodo.org/record/279867/files/figure.png" pageId="2" pageNumber="56" targetBox="[151,1436,155,1379]" targetPageId="2">
<paragraph blockId="2.[151,1412,1400,1423]" box="[151,1412,1400,1423]" pageId="2" pageNumber="56">
