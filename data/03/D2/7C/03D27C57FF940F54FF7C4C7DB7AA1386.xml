, PWb 
<quantity box="[321,477,602,625]" metricMagnitude="-4" metricUnit="m" metricValue="8.25" metricValueMax="8.4" metricValueMin="8.1" pageId="3" pageNumber="486" unit="mm" value="0.825" valueMax="0.84" valueMin="0.81">0.81–0.84 mm</quantity>
, PWm 
<quantity box="[557,714,602,625]" metricMagnitude="-4" metricUnit="m" metricValue="9.299999999999999" metricValueMax="10.2" metricValueMin="8.4" pageId="3" pageNumber="486" unit="mm" value="0.9299999999999999" valueMax="1.02" valueMin="0.84">0.84–1.02 mm</quantity>
; EL 
<quantity box="[766,923,602,625]" metricMagnitude="-3" metricUnit="m" metricValue="2.525" metricValueMax="2.6" metricValueMin="2.45" pageId="3" pageNumber="486" unit="mm" value="2.525" valueMax="2.6" valueMin="2.45">2.45–2.60 mm</quantity>
, EW 
<quantity box="[983,1140,602,625]" metricMagnitude="-3" metricUnit="m" metricValue="1.4449999999999998" metricValueMax="1.46" metricValueMin="1.43" pageId="3" pageNumber="486" unit="mm" value="1.4449999999999998" valueMax="1.46" valueMin="1.43">1.43–1.46 mm</quantity>
, EI 
<number box="[1184,1292,602,625]" pageId="3" pageNumber="486" value="1.75" valueMax="1.78" valueMin="1.72">1.72–1.78</number>
