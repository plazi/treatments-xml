<collectingCountry box="[353,470,824,848]" name="Argentina" pageId="4" pageNumber="2922">Argentina</collectingCountry>
, 
<collectingRegion box="[483,611,824,848]" country="Argentina" name="Entre Rios" pageId="4" pageNumber="2922">Entre Rios</collectingRegion>
