<subSubSection pageId="4" pageNumber="165" type="etymology">
<paragraph blockId="4.[151,1437,152,971]" box="[199,541,369,395]" pageId="4" pageNumber="165">
<emphasis bold="true" box="[199,334,369,395]" pageId="4" pageNumber="165">Etymology.</emphasis>
