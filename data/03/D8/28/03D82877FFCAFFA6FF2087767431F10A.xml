<accessionNumber box="[395,485,2009,2034]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/P10400" pageId="23" pageNumber="390">
P 
<number box="[412,485,2009,2034]" pageId="23" pageNumber="390" value="10400.0">10400</number>
</accessionNumber>
