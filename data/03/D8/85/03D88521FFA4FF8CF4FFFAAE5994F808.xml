<emphasis box="[1408,1421,1924,1945]" italics="true" pageId="2" pageNumber="114">P</emphasis>
.) 
<emphasis box="[151,325,1953,1975]" italics="true" pageId="2" pageNumber="114">damingshanensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[332,412,1954,1975]" pageId="2" pageNumber="114" rank="species">
<emphasis bold="true" box="[332,412,1954,1975]" pageId="2" pageNumber="114">sp. nov.</emphasis>
</taxonomicNameLabel>
; D–F. 
<taxonomicName authority="Distant" authorityName="Distant" authorityYear="1916" box="[481,721,1953,1975]" class="Insecta" family="Cicadellidae" genus="Pediopsis" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="114" phylum="Arthropoda" rank="species" species="garuda" subGenus="Pedionis">
<emphasis box="[481,494,1954,1975]" italics="true" pageId="2" pageNumber="114">P</emphasis>
