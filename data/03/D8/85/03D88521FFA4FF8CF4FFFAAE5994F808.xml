<taxonomicName authorityName="Hamilton" authorityYear="1980" box="[410,499,1923,1945]" class="Insecta" family="Cicadellidae" genus="Pedionis" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="114" phylum="Arthropoda" rank="genus">
<emphasis box="[410,499,1923,1945]" italics="true" pageId="2" pageNumber="114">Pedionis</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Hamilton" authorityYear="1980" box="[513,601,1923,1945]" class="Insecta" family="Cicadellidae" genus="Pedionis" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="114" phylum="Arthropoda" rank="genus">
