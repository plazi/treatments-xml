<quantity box="[512,583,1600,1627]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="3" pageNumber="456" unit="mm" value="5.0">5 mm</quantity>
, width, 
<quantity box="[681,774,1600,1626]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="3" pageNumber="456" unit="mm" value="1.7">1.7 mm</quantity>
. 
