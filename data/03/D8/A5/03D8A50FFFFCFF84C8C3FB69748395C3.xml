(
<figureCitation box="[159,231,1205,1230]" captionStart="FIGURES 5 – 6" captionStartId="5.[151,264,1042,1065]" captionTargetBox="[158,1418,195,1009]" captionTargetId="figure@5.[150,1436,195,1021]" captionTargetPageId="5" captionText="FIGURES 5 – 6. Habitus, dorsal view. 5. Duolandrevus (Eulandrevus) hainanensis, female; 6. Duolandrevus (Eulandrevus) ziyunensis sp. nov., male." httpUri="https://zenodo.org/record/884347/files/figure.png" pageId="5" pageNumber="315">Figs 6</figureCitation>
, 
<figureCitation box="[243,301,1206,1230]" captionStart="FIGURES 9 – 14" captionStartId="7.[151,264,1759,1782]" captionTargetBox="[151,1429,1345,1731]" captionTargetId="figure@7.[150,1436,1345,1739]" captionTargetPageId="7" captionText="FIGURES 9 – 14. Duolandrevus (Eulandrevus) spp. Male, dorsal view. 9, 12. Duolandreuv (Eulandrevus) ziyunensis sp. nov.; 10, 13. Duolandrevus (Eulandrevus) gorochovi sp. nov.; 11, 14. Duolandrevus (Eulandrevus) axinus sp. nov. 9 – 11. Supra-anal plate; 12 – 14, Metanotal gland." httpUri="https://zenodo.org/record/884353/files/figure.png" pageId="5" pageNumber="315">9, 12</figureCitation>
, 
<figureCitation box="[313,342,1205,1230]" captionStart="FIGURES 15 – 17" captionStartId="8.[151,264,1700,1723]" captionTargetBox="[160,1430,201,1671]" captionTargetId="figure@8.[150,1436,193,1679]" captionTargetPageId="8" captionText="FIGURES 15 – 17. Genitalia. 15. Duolandrevus (Eulandrevus) ziyunensis sp. nov.; 16, 17. Duolandrevus (Eulandrevus) gorochovi sp. nov.; a, dorsal view; b, ventral view; c, lateral view." httpUri="https://zenodo.org/record/884355/files/figure.png" pageId="5" pageNumber="315">15</figureCitation>
)
</paragraph>
<paragraph blockId="5.[151,1436,1277,2022]" box="[151,1055,1277,1302]" pageId="5" pageNumber="315">
http://lsid.speciesfile.org/urn:lsid: 
<taxonomicName box="[521,649,1277,1302]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="315" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
