<quantity box="[473,640,908,932]" metricMagnitude="-3" metricUnit="m" metricValue="3.88" metricValueMax="4.05" metricValueMin="3.71" pageId="25" pageNumber="26" unit="mm" value="3.88" valueMax="4.05" valueMin="3.71">3.71–4.05 mm</quantity>
