<emphasis box="[956,1121,2062,2088]" italics="true" pageId="47" pageNumber="48">
<taxonomicName box="[956,1117,2062,2088]" class="Liliopsida" family="Orchidaceae" genus="Bulbophyllum" kingdom="Plantae" order="Asparagales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="genus">Bulbophyllum</taxonomicName>
