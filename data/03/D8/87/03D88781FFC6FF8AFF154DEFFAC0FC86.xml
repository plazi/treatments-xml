<collectingRegion box="[821,907,693,719]" country="China" name="Beijing" pageId="12" pageNumber="13">Beijing</collectingRegion>
municipality 
<taxonomicName authorityName="Zhu &amp; Song" authorityYear="1999" box="[1075,1246,693,718]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="beijingensis">
