<taxonomicName authority="Lowry &amp; Stoddart, 1995" authorityName="Lowry &amp; Stoddart" authorityYear="1995" class="Malacostraca" family="Lysianassidae" genus="Riwo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="4" phylum="Arthropoda" rank="species" species="mizeui">
