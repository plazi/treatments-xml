<number box="[615,667,1917,1941]" pageId="5" value="8.96">8.96</number>
; abdominal width 
<number box="[893,946,1917,1941]" pageId="5" value="5.44">5.44</number>
; head length 
<number box="[1112,1164,1917,1941]" pageId="5" value="1.92">1.92</number>
, width 2.0; interocular distance 
<number box="[252,304,1953,1977]" pageId="5" value="0.96">0.96</number>
; length of antennal segments: I— 
<number box="[692,742,1953,1977]" pageId="5" value="0.72">0.72</number>
, II— 
<number box="[805,840,1953,1977]" pageId="5" value="0.8">0.8</number>
, III— 
<number box="[912,947,1953,1977]" pageId="5" value="0.8">0.8</number>
, IV— 
<number box="[1021,1070,1953,1977]" pageId="5" value="1.12">1.12</number>
