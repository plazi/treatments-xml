<collectingCountry box="[891,958,446,472]" name="China" pageId="25" pageNumber="26">China</collectingCountry>
and 
<collectingCountry box="[1011,1097,446,472]" name="Taiwan" pageId="25" pageNumber="26">Taiwan</collectingCountry>
