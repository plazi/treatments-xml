<subSubSection pageId="25" pageNumber="26" type="distribution">
<paragraph blockId="25.[136,1452,302,616]" box="[189,1101,446,473]" pageId="25" pageNumber="26">
<emphasis bold="true" box="[189,373,446,472]" pageId="25" pageNumber="26">Distribution:—</emphasis>
<collectingCountry box="[373,484,447,473]" name="Myanmar" pageId="25" pageNumber="26">Myanmar</collectingCountry>
