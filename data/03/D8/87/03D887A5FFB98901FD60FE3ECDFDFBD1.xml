<taxonomicName authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Werner" baseAuthorityYear="1901" box="[700,943,487,510]" class="Amphibia" family="Hylidae" genus="Hypsiboas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="86" pageNumber="87" phylum="Chordata" rank="species" species="pellucens">
