<paragraph blockId="33.[106,1097,1499,1585]" pageId="33" pageNumber="34">
<taxonomicName baseAuthorityName="KLUG" baseAuthorityYear="1816" box="[106,293,1499,1523]" class="Insecta" family="Tenthredinidae" genus="Claremontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="uncta">
<emphasis box="[106,293,1499,1523]" italics="true" pageId="33" pageNumber="34">Claremontia uncta</emphasis>
</taxonomicName>
, together with 
<taxonomicName baseAuthorityName="Klug" baseAuthorityYear="1816" box="[456,595,1500,1523]" class="Insecta" family="Tenthredinidae" genus="Claremontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="tenuicornis">
