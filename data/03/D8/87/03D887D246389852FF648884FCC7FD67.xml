<paragraph blockId="6.[151,1436,1887,2020]" lastBlockId="7.[151,1437,151,645]" lastPageId="7" lastPageNumber="88" pageId="6" pageNumber="87">
<emphasis bold="true" box="[199,267,1887,1912]" pageId="6" pageNumber="87">Male.</emphasis>
<emphasis box="[277,448,1888,1911]" italics="true" pageId="6" pageNumber="87">Measurements:</emphasis>
Approximately body length: 
<number box="[795,847,1887,1912]" pageId="6" pageNumber="87" value="16.7">16.7</number>
; head width: 
<number box="[1009,1047,1887,1912]" pageId="6" pageNumber="87" value="5.8">5.8</number>
; inter-tegular span: 
<number box="[1281,1319,1887,1912]" pageId="6" pageNumber="87" value="5.8">5.8</number>
; forewing length: 
<number box="[239,291,1923,1948]" pageId="6" pageNumber="87" value="15.2">15.2</number>
; UID: 
<number box="[375,413,1924,1948]" pageId="6" pageNumber="87" value="1.8">1.8</number>
; LID: 
<number box="[494,532,1923,1948]" pageId="6" pageNumber="87" value="2.5">2.5</number>
; mandible length: 2.0; mandible basal width: 
<number box="[1071,1109,1924,1948]" pageId="6" pageNumber="87" value="1.2">1.2</number>
; labrum length: 1.0; labrum width: 
<number box="[231,269,1960,1984]" pageId="6" pageNumber="87" value="1.8">1.8</number>
; F 
<number box="[302,312,1960,1984]" pageId="6" pageNumber="87" value="1.0">1</number>
length: 
<number box="[408,446,1960,1984]" pageId="6" pageNumber="87" value="1.1">1.1</number>
