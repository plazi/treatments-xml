<paragraph blockId="5.[151,1436,1913,2010]" lastBlockId="6.[151,1436,151,536]" lastPageId="6" lastPageNumber="87" pageId="5" pageNumber="86">
<emphasis bold="true" box="[151,407,1913,1938]" pageId="5" pageNumber="86">Morphology. Female.</emphasis>
<emphasis box="[415,586,1915,1938]" italics="true" pageId="5" pageNumber="86">Measurements:</emphasis>
Approximately body length: 
<number box="[923,975,1913,1938]" pageId="5" pageNumber="86" value="18.5">18.5</number>
; head width: 6.0; inter-tegular span: 
<number box="[1393,1431,1913,1938]" pageId="5" pageNumber="86" value="5.5">5.5</number>
; forewing length: 14.0; UID: 
<number box="[482,520,1950,1974]" pageId="5" pageNumber="86" value="3.8">3.8</number>
; LID: 
<number box="[598,636,1950,1974]" pageId="5" pageNumber="86" value="2.8">2.8</number>
; mandible length: 
<number box="[851,889,1950,1974]" pageId="5" pageNumber="86" value="3.2">3.2</number>
; mandible basal width: 
<number box="[1165,1203,1950,1974]" pageId="5" pageNumber="86" value="1.7">1.7</number>
; labrum length: 
<number box="[1393,1431,1950,1974]" pageId="5" pageNumber="86" value="0.8">0.8</number>
; labrum width: 
<number box="[316,354,1986,2010]" pageId="5" pageNumber="86" value="1.8">1.8</number>
; F 
<number box="[385,395,1986,2010]" pageId="5" pageNumber="86" value="1.0">1</number>
length: 
<number box="[487,525,1986,2010]" pageId="5" pageNumber="86" value="1.2">1.2</number>
