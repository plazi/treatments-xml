and 
<emphasis box="[791,1049,1699,1723]" italics="true" pageId="4" pageNumber="105">
<taxonomicName box="[791,1045,1699,1723]" class="Gastropoda" family="Camaenidae" genus="Australocosmica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="4" pageNumber="105" phylum="Mollusca" rank="species" species="sanctumpatriciusae">A. sanctumpatriciusae</taxonomicName>
