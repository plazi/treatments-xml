<quantity box="[295,405,1281,1303]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="3.8" metricValueMin="3.2" pageId="135" pageNumber="748" unit="mm" value="3.5" valueMax="3.8" valueMin="3.2">3,2-3,8mm</quantity>
