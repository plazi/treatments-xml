<quantity box="[626,741,1281,1303]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" metricValueMax="2.3" metricValueMin="2.1" pageId="135" pageNumber="748" unit="mm" value="2.2" valueMax="2.3" valueMin="2.1">2,1-2,3mm</quantity>
