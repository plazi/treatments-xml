, 
<figureCitation box="[257,287,1590,1614]" captionStart="FIGURES 84–92" captionStartId="70.[151,264,1931,1954]" captionTargetBox="[161,1425,181,1910]" captionTargetId="figure-72@70.[161,1425,181,1910]" captionTargetPageId="70" captionText="FIGURES 84–92. Mesosoma in dorsal view. 84. Eupsenella kokatha sp. nov. 85. E. larrakia sp. nov. 86. E. maya sp. nov. 87." pageId="50" pageNumber="51">85</figureCitation>
, 
<figureCitation box="[299,342,1590,1614]" captionStart="FIGURES 123–132" captionStartId="74.[151,263,1911,1934]" captionTargetBox="[151,1436,193,1890]" captionTargetId="figure-77@74.[151,1436,193,1890]" captionTargetPageId="74" captionText="FIGURES 123–132. Right forewing. 123. Eupsenella jagara sp. nov. 124. E. janda sp. nov. 125. E. kabalbara sp. nov. 126. E." pageId="50" pageNumber="51">130</figureCitation>
