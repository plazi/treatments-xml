</taxonomicName>
, 
<taxonomicNameLabel box="[968,1101,1660,1683]" pageId="89" pageNumber="90" rank="species">new comb.</taxonomicNameLabel>
; 
<taxonomicName authority="(Duellman and Trueb, 1983)" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Duellman and Trueb" baseAuthorityYear="1983" class="Amphibia" family="Hylidae" genus="Dendropsophus" kingdom="Animalia" order="Anura" pageId="89" pageNumber="90" phylum="Chordata" rank="species" species="praestans" status="new comb.">
<emphasis italics="true" pageId="89" pageNumber="90">Dendropsophus praestans</emphasis>
