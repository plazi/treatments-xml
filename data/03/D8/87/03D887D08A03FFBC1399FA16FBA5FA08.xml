<subSubSection box="[199,1355,1339,1364]" pageId="49" pageNumber="50" type="description">
<paragraph blockId="49.[151,1436,151,1508]" box="[199,1355,1339,1364]" pageId="49" pageNumber="50">
<emphasis bold="true" box="[199,319,1339,1364]" inLineHeading="true" pageId="49" pageNumber="50" reason="1">Variation.</emphasis>
