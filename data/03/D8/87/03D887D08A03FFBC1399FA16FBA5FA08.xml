<quantity box="[470,636,1340,1364]" metricMagnitude="-3" metricUnit="m" metricValue="5.59" metricValueMax="5.71" metricValueMin="5.47" pageId="49" pageNumber="50" unit="mm" value="5.59" valueMax="5.71" valueMin="5.47">5.47–5.71 mm</quantity>
