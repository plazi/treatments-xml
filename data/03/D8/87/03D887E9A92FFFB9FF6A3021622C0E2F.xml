<subSubSection pageId="15" pageNumber="464" type="discussion">
<paragraph blockId="15.[151,1436,151,429]" pageId="15" pageNumber="464">
<emphasis bold="true" box="[199,315,332,357]" pageId="15" pageNumber="464">Remarks.</emphasis>
<taxonomicName box="[323,630,332,356]" class="Insecta" family="Chironomidae" genus="Stenochironomus" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="464" phylum="Arthropoda" rank="species" species="bacrionis">
