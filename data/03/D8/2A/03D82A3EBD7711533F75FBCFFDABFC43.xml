<paragraph blockId="30.[816,1084,1084,1132]" box="[816,1084,1084,1105]" pageId="30" pageNumber="31">
<heading box="[816,1084,1084,1105]" centered="true" fontSize="10" level="2" pageId="30" pageNumber="31" reason="2">
<emphasis bold="true" box="[816,1084,1084,1105]" pageId="30" pageNumber="31">
4. 
<taxonomicName authority="SHARP" box="[843,935,1084,1105]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[843,935,1084,1105]" italics="true" pageId="30" pageNumber="31">baiknami</emphasis>
</taxonomicName>
