<subSubSection pageId="67" pageNumber="68" type="etymology">
<paragraph blockId="67.[151,638,938,1730]" pageId="67" pageNumber="68">
<emphasis bold="true" box="[175,290,1576,1597]" pageId="67" pageNumber="68">Etymology.</emphasis>
