<emphasis box="[297,392,1686,1704]" italics="true" pageId="63" pageNumber="64">S. lescheni</emphasis>
</taxonomicName>
: black circles; 
<taxonomicName box="[534,631,1686,1704]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="gourlayi">
