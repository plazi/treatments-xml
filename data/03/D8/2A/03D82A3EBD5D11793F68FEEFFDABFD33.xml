10. 
<taxonomicName authority="SHARP" box="[852,939,284,305]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="helmorei">
<emphasis bold="true" box="[852,939,284,305]" italics="true" pageId="52" pageNumber="53">helmorei</emphasis>
</taxonomicName>
