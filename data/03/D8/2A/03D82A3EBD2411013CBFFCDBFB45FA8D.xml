<taxonomicName authorityName="Broun, 1893 b:" authorityYear="1053" box="[289,392,808,829]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="anisarthra">
