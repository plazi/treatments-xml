<taxonomicName authorityName="Broun Desig. Park and Carlton" authorityYear="2013" box="[854,938,1241,1262]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="notabilis">
<emphasis bold="true" box="[854,938,1241,1262]" italics="true" pageId="96" pageNumber="97">notabilis</emphasis>
