<collectingRegion box="[245,475,1518,1543]" country="Brazil" name="Mato Grosso do Sul" pageId="14" pageNumber="275">Mato Grosso do Sul</collectingRegion>
, 
<collectingCountry box="[487,557,1518,1543]" name="Brazil" pageId="14" pageNumber="275">Brazil</collectingCountry>
