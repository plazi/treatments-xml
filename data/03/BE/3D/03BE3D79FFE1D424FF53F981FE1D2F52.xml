(
<collectingRegion box="[399,475,547,573]" country="China" name="Fujian" pageId="7" pageNumber="119">Fujian</collectingRegion>
, 
<collectingRegion box="[487,621,547,573]" country="China" name="Guangdong" pageId="7" pageNumber="119">Guangdong</collectingRegion>
, 
<collectingRegion box="[632,731,547,573]" country="China" name="Guangxi" pageId="7" pageNumber="119">Guangxi</collectingRegion>
, 
<collectingRegion box="[742,841,547,573]" country="China" name="Guizhou" pageId="7" pageNumber="119">Guizhou</collectingRegion>
, 
<collectingRegion box="[853,937,547,573]" country="China" name="Hainan" pageId="7" pageNumber="119">Hainan</collectingRegion>
, 
<collectingCountry box="[948,1083,547,573]" name="Hong Kong" pageId="7" pageNumber="119">Hong Kong</collectingCountry>
, Jiangsu, Jiangxi, Sichuan, 
<collectingCountry name="Taiwan" pageId="7" pageNumber="119">Taiwan</collectingCountry>
