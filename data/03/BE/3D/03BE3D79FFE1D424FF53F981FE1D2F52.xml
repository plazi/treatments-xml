<emphasis box="[1255,1304,403,429]" italics="true" pageId="7" pageNumber="119">et al</emphasis>
. 2016). For Chinese records, 
<taxonomicName box="[343,537,439,465]" class="Magnoliopsida" family="Anacardiaceae" genus="Mangifera" kingdom="Plantae" order="Sapindales" pageId="7" pageNumber="119" phylum="Tracheophyta" rank="species" species="indica">
<emphasis box="[343,537,439,465]" italics="true" pageId="7" pageNumber="119">Mangifera indica</emphasis>
</taxonomicName>
(
<taxonomicName box="[551,722,439,466]" class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="7" pageNumber="119" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
), 
<taxonomicName box="[739,932,439,465]" class="Magnoliopsida" family="Myrtaceae" genus="Syzygium" kingdom="Plantae" order="Myrtales" pageId="7" pageNumber="119" phylum="Tracheophyta" rank="species" species="jambos">
<emphasis box="[739,932,439,465]" italics="true" pageId="7" pageNumber="119">Syzygium jambos</emphasis>
</taxonomicName>
(
<taxonomicName box="[946,1068,439,465]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="7" pageNumber="119" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
) (
<bibRefCitation author="Tang, F. D." box="[1088,1209,439,465]" pageId="7" pageNumber="119" refId="ref13126" refString="Tang, F. D. (1991) The Coccidae of China. Shanxi United Universities Press, Taiyuan, China, 377 pp. [in Chinese]" type="book" year="1991">Tang 1991</bibRefCitation>
) and 
<taxonomicName box="[1270,1436,439,465]" class="Cycadopsida" family="Cycadaceae" genus="Cycas" kingdom="Plantae" order="Cycadales" pageId="7" pageNumber="119" phylum="Tracheophyta" rank="species" species="revoluta">
<emphasis box="[1270,1436,439,465]" italics="true" pageId="7" pageNumber="119">Cycas revoluta</emphasis>
</taxonomicName>
(
<taxonomicName box="[159,300,475,502]" class="Cycadopsida" family="Cycadaceae" kingdom="Plantae" order="Cycadales" pageId="7" pageNumber="119" phylum="Tracheophyta" rank="family">Cycadaceae</taxonomicName>
).
</paragraph>
</subSubSection>
<subSubSection pageId="7" pageNumber="119" type="distribution">
<paragraph blockId="7.[151,1437,150,610]" pageId="7" pageNumber="119">
<emphasis bold="true" box="[199,353,510,536]" pageId="7" pageNumber="119">Distribution.</emphasis>
