<taxonomicName authority="Bertrand &amp; Bagheri &amp; Akbari &amp; Yazdanian &amp; Irani-Nejad &amp; Mohajer &amp; Saboori, 2012" authorityName="Bertrand &amp; Bagheri &amp; Akbari &amp; Yazdanian &amp; Irani-Nejad &amp; Mohajer &amp; Saboori" authorityYear="2012" box="[272,612,1352,1371]" class="Arachnida" family="Labidostommatidae" genus="Labidostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="6" pageNumber="238" phylum="Arthropoda" rank="species" species="intermedia" status="n. sp." subGenus="Labidostoma">
