(
<quantity box="[1241,1333,1519,1546]" metricMagnitude="2" metricUnit="m" metricValue="7.0104000000000015" pageId="217" pageNumber="218" unit="ft" value="2300.0">2,300 ft</quantity>
) (
<bibRefCitation author="Kim" etAl="et al." firstAuthor="Kim" pageId="217" pageNumber="218" pagination="17 - 22" refId="ref291068" refString="Kim, Y. U., Kim, Y. S., Ahn, G. &amp; Kim, J. K. (1999) New record of the two carangid fishes (Perciformes, Carangidae) from Korea. Korean Journal of Ichthyology, 11, 17 - 22." type="journal article" year="1999">
