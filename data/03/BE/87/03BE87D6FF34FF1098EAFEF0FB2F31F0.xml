<taxonomicName authority="(Steindachner 1866)" baseAuthorityName="Steindachner" baseAuthorityYear="1866" box="[864,1331,439,466]" class="Actinopterygii" family="Sciaenidae" genus="Isopisthus" kingdom="Animalia" order="Perciformes" pageId="165" pageNumber="166" phylum="Chordata" rank="species" species="altipinnis">
