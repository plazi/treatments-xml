<quantity box="[1129,1228,367,393]" metricMagnitude="1" metricUnit="m" metricValue="6.4" metricValueMax="12.4" metricValueMin="0.4" pageId="165" pageNumber="166" unit="m" value="64.0" valueMax="124.0" valueMin="4.0">4–124 m</quantity>
