<taxonomicName authorityName="Schuh &amp; P. Pedraza" authorityYear="2010" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="pinocchii">
<emphasis italics="true" pageId="32" pageNumber="33">W. pinocchii</emphasis>
</taxonomicName>
and the Lamiaceae-feeding 
<taxonomicName authorityName="Schuh &amp; P. Pedraza" authorityYear="2010" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="cassisi">
<emphasis italics="true" pageId="32" pageNumber="33">W. cassisi</emphasis>
</taxonomicName>
