<taxonomicName box="[798,996,329,352]" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="lachnostachy">
<emphasis box="[798,996,329,352]" italics="true" pageId="32" pageNumber="33">W. lachnostachy</emphasis>
</taxonomicName>
os, 
<taxonomicName box="[1046,1219,329,352]" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="newcastelii">
<emphasis box="[1046,1219,329,352]" italics="true" pageId="32" pageNumber="33">W. newcastelii</emphasis>
</taxonomicName>
, 
<taxonomicName box="[701,905,358,381]" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="paradicrastyli">
<emphasis box="[701,905,358,381]" italics="true" pageId="32" pageNumber="33">W. paradicrastyli</emphasis>
</taxonomicName>
