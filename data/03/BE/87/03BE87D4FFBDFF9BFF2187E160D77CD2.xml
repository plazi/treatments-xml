. Allotype, female, 
<collectionCode box="[964,1050,776,799]" httpUri="http://grbio.org/cool/ppqr-es1r" name="United States National Parasite Collection" pageId="2" pageNumber="67">USNPC</collectionCode>
<number box="[1061,1131,776,799]" pageId="2" pageNumber="67" value="98047.0">98047</number>
. 
<typeStatus box="[1145,1251,776,799]" pageId="2" pageNumber="67">Paratypes</typeStatus>
, 
<collectionCode box="[1266,1352,776,799]" httpUri="http://grbio.org/cool/ppqr-es1r" name="United States National Parasite Collection" pageId="2" pageNumber="67">USNPC</collectionCode>
