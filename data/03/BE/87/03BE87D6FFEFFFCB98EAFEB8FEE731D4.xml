<quantity box="[694,816,402,429]" metricMagnitude="2" metricUnit="m" metricValue="6.77" metricValueMax="13.54" metricValueMin="0.0" pageId="126" pageNumber="127" unit="m" value="677.0" valueMax="1354.0" valueMin="0.0">0–1,354 m</quantity>
