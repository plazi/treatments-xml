<quantity box="[899,941,691,717]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="120" pageNumber="121" unit="m" value="2.0">2 m</quantity>
(
<quantity box="[957,997,691,718]" metricMagnitude="0" metricUnit="m" metricValue="2.1336" pageId="120" pageNumber="121" unit="ft" value="7.0">7 ft</quantity>
) to 
<quantity box="[1039,1110,690,717]" metricMagnitude="2" metricUnit="m" metricValue="5.56" pageId="120" pageNumber="121" unit="m" value="556.0">556 m</quantity>
(
<quantity box="[1124,1214,691,718]" metricMagnitude="2" metricUnit="m" metricValue="5.559552" pageId="120" pageNumber="121" unit="ft" value="1824.0">1,824 ft</quantity>
