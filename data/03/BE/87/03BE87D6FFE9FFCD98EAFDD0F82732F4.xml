<collectingRegion box="[1109,1193,655,681]" country="United States of America" name="Oregon" pageId="120" pageNumber="121">Oregon</collectingRegion>
(
<geoCoordinate box="[1208,1307,655,681]" degrees="44" direction="north" minutes="02" orientation="latitude" pageId="120" pageNumber="121" precision="925" value="44.033333">44°02’N</geoCoordinate>
