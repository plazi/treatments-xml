or Pacific Thread Herring. To about 
<quantity box="[1365,1436,691,717]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="43" pageNumber="44" unit="cm" value="30.0">30 cm</quantity>
(
<quantity box="[206,286,727,754]" metricMagnitude="-1" metricUnit="m" metricValue="2.9972" pageId="43" pageNumber="44" unit="in" value="11.8">11.8 in</quantity>
) TL (
<bibRefCitation author="Amezcua Linares" box="[350,617,727,754]" firstAuthor="Amezcua Linares" pageId="43" pageNumber="44" refId="ref260157" refString="Amezcua Linares, F. (1996) Peces Demersales de la Platforma Continental del Pacifico Central de Mexico. Instituto de Ciencias del Mar y Limnologica, Universidad Nacional Autonoma de Mexico." type="book" year="1996">Amezcua Linares 1996</bibRefCitation>
