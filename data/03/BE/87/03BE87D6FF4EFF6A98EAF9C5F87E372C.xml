<quantity box="[1225,1337,1735,1761]" metricMagnitude="1" metricUnit="m" metricValue="9.55" metricValueMax="17.1" metricValueMin="2.0" pageId="223" pageNumber="224" unit="m" value="95.5" valueMax="171.0" valueMin="20.0">20–171 m</quantity>
