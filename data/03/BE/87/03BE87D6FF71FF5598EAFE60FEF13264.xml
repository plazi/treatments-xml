<geoCoordinate box="[347,468,475,501]" degrees="23" direction="north" minutes="24.6" orientation="latitude" pageId="224" pageNumber="225" precision="92" value="23.41">23°24.6’N</geoCoordinate>
, 
<geoCoordinate box="[484,622,475,501]" degrees="110" direction="west" minutes="13.8" orientation="longitude" pageId="224" pageNumber="225" precision="92" value="-110.23">110°13.8’W</geoCoordinate>
