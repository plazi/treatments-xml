<collectingCountry box="[469,520,783,809]" name="Peru" pageId="65" pageNumber="66">Peru</collectingCountry>
(
<geoCoordinate box="[539,619,783,809]" degrees="9" direction="south" minutes="07" orientation="latitude" pageId="65" pageNumber="66" precision="925" value="-9.116667">9°07’S</geoCoordinate>
