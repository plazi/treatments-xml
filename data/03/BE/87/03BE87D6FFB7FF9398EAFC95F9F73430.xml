<quantity box="[1231,1355,1015,1042]" metricMagnitude="1" metricUnit="m" metricValue="9.95172" metricValueMax="14.99616" metricValueMin="4.90728" pageId="38" pageNumber="39" unit="ft" value="326.5" valueMax="492.0" valueMin="161.0">161–492 ft</quantity>
