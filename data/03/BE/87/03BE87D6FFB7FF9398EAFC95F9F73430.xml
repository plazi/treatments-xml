<quantity box="[1106,1218,1014,1041]" metricMagnitude="1" metricUnit="m" metricValue="9.95" metricValueMax="15.0" metricValueMin="4.9" pageId="38" pageNumber="39" unit="m" value="99.5" valueMax="150.0" valueMin="49.0">49–150 m</quantity>
