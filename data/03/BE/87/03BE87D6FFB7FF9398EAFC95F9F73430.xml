<quantity box="[806,877,871,897]" metricMagnitude="-1" metricUnit="m" metricValue="7.4" pageId="38" pageNumber="39" unit="cm" value="74.0">74 cm</quantity>
(
<quantity box="[894,977,871,898]" metricMagnitude="-1" metricUnit="m" metricValue="7.3914" pageId="38" pageNumber="39" unit="in" value="29.1">29.1 in</quantity>
) TL (John Snow, pers. comm. to M.L.). Todos Santos (
<geoCoordinate box="[368,489,907,933]" degrees="23" direction="north" minutes="24.6" orientation="latitude" pageId="38" pageNumber="39" precision="92" value="23.41">23°24.6’N</geoCoordinate>
