<quantity box="[828,956,653,680]" metricMagnitude="2" metricUnit="m" metricValue="2.45" metricValueMax="3.0" metricValueMin="1.9" pageId="105" pageNumber="106" unit="m" value="245.0" valueMax="300.0" valueMin="190.0">190–300 m</quantity>
