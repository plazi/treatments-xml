<taxonomicName box="[365,496,1778,1799]" class="Malacostraca" family="Talitridae" genus="Africorchestia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="57" phylum="Arthropoda" rank="species" species="tricornuta">
