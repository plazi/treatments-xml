<quantity box="[1345,1436,1902,1929]" metricMagnitude="3" metricUnit="m" metricValue="2.7439999999999998" pageId="69" pageNumber="70" unit="m" value="2744.0">2,744 m</quantity>
(
<quantity box="[205,296,1938,1965]" metricMagnitude="3" metricUnit="m" metricValue="2.7432" pageId="69" pageNumber="70" unit="ft" value="9000.0">9,000 ft</quantity>
