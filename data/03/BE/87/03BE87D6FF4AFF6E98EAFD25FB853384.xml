<collectingRegion box="[1145,1317,799,825]" country="Mexico" name="Baja California" pageId="219" pageNumber="220">Baja California</collectingRegion>
(
<geoCoordinate box="[1332,1431,798,825]" degrees="25" direction="north" minutes="59" orientation="latitude" pageId="219" pageNumber="220" precision="925" value="25.983334">25°59’N</geoCoordinate>
, 
