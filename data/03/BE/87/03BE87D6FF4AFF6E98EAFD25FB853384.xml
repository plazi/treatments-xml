<quantity box="[952,1071,834,862]" metricMagnitude="2" metricUnit="m" metricValue="7.008876" metricValueMax="13.996416" metricValueMin="0.021336" pageId="219" pageNumber="220" unit="ft" value="2299.5" valueMax="4592.0" valueMin="7.0">7–4,592 ft</quantity>
