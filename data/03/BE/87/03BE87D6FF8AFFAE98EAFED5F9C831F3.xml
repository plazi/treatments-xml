<quantity box="[378,477,439,465]" metricMagnitude="1" metricUnit="m" metricValue="5.15" metricValueMax="10.0" metricValueMin="0.3" pageId="27" pageNumber="28" unit="m" value="51.5" valueMax="100.0" valueMin="3.0">3–100 m</quantity>
