<quantity box="[640,725,1970,1997]" metricMagnitude="1" metricUnit="m" metricValue="2.25" metricValueMax="4.0" metricValueMin="0.5" pageId="16" pageNumber="17" unit="m" value="22.5" valueMax="40.0" valueMin="5.0">5–40 m</quantity>
