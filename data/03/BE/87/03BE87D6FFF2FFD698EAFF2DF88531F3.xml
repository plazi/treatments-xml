<bibRefCitation author="Masuda" etAl="et al." firstAuthor="Masuda" pageId="99" pageNumber="100" refId="ref299291" refString="Masuda, H., Amaoka, K., Araga, C., Uyeno, T. &amp; Yoshino, T. (Eds). (1984) The Fishes of the Japanese Archipelago. Tokai University Press, Tokyo." type="book" year="1984">
Masuda 
<emphasis box="[198,257,295,321]" italics="true" pageId="99" pageNumber="100">et al.</emphasis>
1984
</bibRefCitation>
) and Sea of Okhotsk (
<geoCoordinate box="[586,686,295,321]" degrees="44" direction="north" minutes="20" orientation="latitude" pageId="99" pageNumber="100" precision="925" value="44.333332">44°20’N</geoCoordinate>
, 
<geoCoordinate box="[700,808,295,321]" degrees="148" direction="east" minutes="37" orientation="longitude" pageId="99" pageNumber="100" precision="925" value="148.61667">148°37’E</geoCoordinate>
) (
<bibRefCitation author="Kharin" box="[834,998,295,322]" firstAuthor="Kharin" pageId="99" pageNumber="100" pagination="545 - 546" refId="ref290166" refString="Kharin, V. E. (2006 b) Rondeletiidae - a new family of fish of the Russian fauna. Journal of Ichthyology, 46, 545 - 546. https: // doi. org / 10.1134 / s 003294520607006 x" type="journal article" year="2006" yearSuffix="b">Kharin 2006b</bibRefCitation>
); Washington (
<geoCoordinate box="[1171,1268,295,321]" degrees="47" direction="north" minutes="12" orientation="latitude" pageId="99" pageNumber="100" precision="925" value="47.2">47°12’N</geoCoordinate>
