<quantity box="[1332,1403,223,250]" metricMagnitude="-1" metricUnit="m" metricValue="1.5" pageId="99" pageNumber="100" unit="cm" value="15.0">15 cm</quantity>
(
<quantity metricMagnitude="-1" metricUnit="m" metricValue="1.524" pageId="99" pageNumber="100" unit="in" value="6.0">6 in</quantity>
) TL (
<bibRefCitation author="Fitch and Lavenberg" box="[288,594,259,286]" firstAuthor="Fitch" pageId="99" pageNumber="100" refId="ref279229" refString="Fitch, J. E. &amp; Lavenberg, R. J. (1968) Deep-Water Fishes of California. California Natural History Guides, No. 25, University of California Press, Berkeley." type="book" year="1968">Fitch and Lavenberg 1968</bibRefCitation>
