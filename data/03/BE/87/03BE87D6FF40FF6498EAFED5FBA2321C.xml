. To 
<quantity box="[680,767,294,321]" metricMagnitude="0" metricUnit="m" metricValue="3.05" pageId="209" pageNumber="210" unit="cm" value="305.0">305 cm</quantity>
(
<quantity box="[787,846,295,322]" metricMagnitude="0" metricUnit="m" metricValue="3.048" pageId="209" pageNumber="210" unit="ft" value="10.0">10 ft</quantity>
) TL (
<bibRefCitation author="Eschmeyer and Herald" box="[915,1251,295,321]" firstAuthor="Eschmeyer" pageId="209" pageNumber="210" refId="ref277744" refString="Eschmeyer, W. N. &amp; Herald, E. S. (1983) A Field Guide to Pacific Coast Fishes of North America from the Gulf of Alaska to Baja California. Houghton Mifflin, Boston." type="book" year="1983">Eschmeyer and Herald 1983</bibRefCitation>
