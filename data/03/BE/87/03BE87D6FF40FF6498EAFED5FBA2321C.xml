<geoCoordinate box="[549,648,366,393]" degrees="38" direction="north" minutes="15" orientation="latitude" pageId="209" pageNumber="210" precision="925" value="38.25">38°15’N</geoCoordinate>
, 
<geoCoordinate box="[665,785,367,393]" degrees="123" direction="west" minutes="30" orientation="longitude" pageId="209" pageNumber="210" precision="925" value="-123.5">123°30’W</geoCoordinate>
), northern California (Personal communication: Scripps Institution of Oceanography Fish Collection, La Jolla, California) to southern Baja California and 
<collectingCountry box="[1313,1436,403,429]" name="Costa Rica" pageId="209" pageNumber="210">Costa Rica</collectingCountry>
(
<bibRefCitation author="Robertson and Allen" box="[205,509,438,465]" firstAuthor="Robertson" pageId="209" pageNumber="210" refId="ref316799" refString="Robertson, D. R. and Allen, G. R. (2015) Shorefishes of the Tropical Eastern Pacific: an Information System. Version 2.0 (2008). Smithsonian Tropical Research Institute, Balboa. http: // biogeodb. stri. si. edu / sftep / en / pages" type="url" year="2015">Robertson and Allen 2015</bibRefCitation>
) to 
<collectingCountry box="[556,617,439,465]" name="Chile" pageId="209" pageNumber="210">Chile</collectingCountry>
(
<bibRefCitation author="Pequeno" box="[634,801,439,465]" firstAuthor="Pequeno" pageId="209" pageNumber="210" pagination="1 - 132" refId="ref311468" refString="Pequeno R., G. (1989) Peces de Chile lista sistematica revisada y comentada. Revista de Biologia Marina y Oceanografia, 24, 1 - 132." type="journal article" year="1989">Pequeño 1989</bibRefCitation>
). Oceanic benthopelagic; depth: 
<quantity box="[1177,1313,439,465]" metricMagnitude="2" metricUnit="m" metricValue="5.91" metricValueMax="11.6" metricValueMin="0.22" pageId="209" pageNumber="210" unit="m" value="591.0" valueMax="1160.0" valueMin="22.0">22–1,160 m</quantity>
(
<quantity metricMagnitude="2" metricUnit="m" metricValue="5.910072" metricValueMax="11.600688" metricValueMin="0.219456" pageId="209" pageNumber="210" unit="ft" value="1939.0" valueMax="3806.0" valueMin="72.0">72–3,806 ft</quantity>
) (min.: Personal communication: Scripps Institution of Oceanography Fish Collection, La Jolla, California; max.: 
<bibRefCitation author="Pakhorukov" box="[268,470,511,537]" firstAuthor="Pakhorukov" pageId="209" pageNumber="210" pagination="626 - 633" refId="ref309088" refString="Pakhorukov, N. P. (1999) Underwater observations on deepwater fish of the Atlantic Ocean in the region of the Sierra Leone Rise. Journal of Ichthyology, 39, 626 - 633." type="journal article" year="1999">Pakhorukov 1999</bibRefCitation>
).
