<subSubSection pageId="88" pageNumber="89" type="description">
<paragraph blockId="88.[151,1437,150,1797]" pageId="88" pageNumber="89">
To 
<quantity box="[741,833,619,645]" metricMagnitude="-1" metricUnit="m" metricValue="1.13" pageId="88" pageNumber="89" unit="cm" value="11.3">11.3 cm</quantity>
