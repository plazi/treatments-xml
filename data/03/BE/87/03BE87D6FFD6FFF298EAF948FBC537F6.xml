(
<quantity box="[787,959,1937,1964]" metricMagnitude="3" metricUnit="m" metricValue="2.249424" metricValueMax="2.49936" metricValueMin="1.999488" pageId="71" pageNumber="72" unit="ft" value="7380.0" valueMax="8200.0" valueMin="6560.0">
