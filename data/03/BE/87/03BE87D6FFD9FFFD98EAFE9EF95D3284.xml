<geoCoordinate box="[661,761,436,462]" degrees="24" direction="north" minutes="26" orientation="latitude" pageId="72" pageNumber="73" precision="925" value="24.433332">24°26’N</geoCoordinate>
, 
<geoCoordinate box="[777,894,437,463]" degrees="113" direction="west" minutes="18" orientation="longitude" pageId="72" pageNumber="73" precision="925" value="-113.3">113°18’W</geoCoordinate>
