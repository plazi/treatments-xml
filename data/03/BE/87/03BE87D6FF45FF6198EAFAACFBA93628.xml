. To 
<quantity box="[771,816,1374,1401]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="212" pageNumber="213" unit="m" value="5.0">5 m</quantity>
(
<quantity box="[832,910,1375,1402]" metricMagnitude="0" metricUnit="m" metricValue="4.99872" pageId="212" pageNumber="213" unit="ft" value="16.4">16.4 ft</quantity>
