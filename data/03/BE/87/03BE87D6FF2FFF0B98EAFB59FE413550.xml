<quantity box="[557,654,1303,1329]" metricMagnitude="2" metricUnit="m" metricValue="4.005" metricValueMax="8.01" metricValueMin="0.0" pageId="190" pageNumber="191" unit="m" value="400.5" valueMax="801.0" valueMin="0.0">0–801 m</quantity>
