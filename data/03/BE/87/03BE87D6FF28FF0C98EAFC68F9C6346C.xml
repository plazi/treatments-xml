<quantity box="[1254,1353,1066,1094]" metricMagnitude="2" metricUnit="m" metricValue="1.002792" metricValueMax="1.999488" metricValueMin="0.006096" pageId="185" pageNumber="186" unit="ft" value="329.0" valueMax="656.0" valueMin="2.0">2–656 ft</quantity>
