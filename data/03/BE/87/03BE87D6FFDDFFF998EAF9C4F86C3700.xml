<quantity box="[1207,1354,1663,1690]" metricMagnitude="2" metricUnit="m" metricValue="6.248399999999999" metricValueMax="11.996928" metricValueMin="0.499872" pageId="76" pageNumber="77" unit="ft" value="2050.0" valueMax="3936.0" valueMin="164.0">164–3,936 ft</quantity>
