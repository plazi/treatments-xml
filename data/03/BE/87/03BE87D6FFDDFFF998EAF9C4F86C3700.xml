<quantity box="[1059,1193,1662,1689]" metricMagnitude="2" metricUnit="m" metricValue="6.25" metricValueMax="12.0" metricValueMin="0.5" pageId="76" pageNumber="77" unit="m" value="625.0" valueMax="1200.0" valueMin="50.0">50–1,200 m</quantity>
