<quantity box="[423,556,1483,1510]" metricMagnitude="3" metricUnit="m" metricValue="1.1399519999999999" metricValueMax="2.2500335999999996" metricValueMin="0.0298704" pageId="63" pageNumber="64" unit="ft" value="3740.0" valueMax="7382.0" valueMin="98.0">98–7,382 ft</quantity>
