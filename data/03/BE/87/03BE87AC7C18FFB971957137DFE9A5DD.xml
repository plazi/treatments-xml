<number box="[1268,1310,1435,1459]" pageId="1" pageNumber="108" value="140.0">140</number>
(
<number box="[1327,1427,1434,1459]" pageId="1" pageNumber="108" value="150.0" valueMax="160.0" valueMin="140.0">140–160</number>
) long, 
<number box="[216,257,1471,1495]" pageId="1" pageNumber="108" value="110.0">110</number>
