<paragraph blockId="3.[151,1437,151,1184]" pageId="3" pageNumber="110">
<taxonomicName box="[199,342,152,176]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="3" pageNumber="110" phylum="Nematoda" rank="genus">
<emphasis box="[199,342,152,176]" italics="true" pageId="3" pageNumber="110">Gnathosoma</emphasis>
