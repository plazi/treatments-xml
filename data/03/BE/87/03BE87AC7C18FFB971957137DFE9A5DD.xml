<number box="[1304,1377,152,176]" pageId="3" pageNumber="110" value="22.0" valueMax="24.0" valueMin="20.0">20–24</number>
, palp coxal seta 
<number box="[280,353,188,212]" pageId="3" pageNumber="110" value="21.0" valueMax="22.0" valueMin="20.0">20–22</number>
