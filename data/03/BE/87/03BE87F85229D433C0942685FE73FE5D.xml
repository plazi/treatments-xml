</paragraph>
<paragraph blockId="25.[151,1436,151,2013]" pageId="25" pageNumber="26">
<emphasis box="[199,305,1556,1580]" italics="true" pageId="25" pageNumber="26">Abdomen</emphasis>
: syntergite 
<number box="[445,455,1556,1580]" pageId="25" pageNumber="26" value="1.0">1</number>
+ 
<number box="[473,489,1556,1580]" pageId="25" pageNumber="26" value="2.0">2</number>
with base brown; syntergite 
<number box="[834,844,1556,1580]" pageId="25" pageNumber="26" value="1.0">1</number>
+ 
<number box="[863,880,1556,1580]" pageId="25" pageNumber="26" value="2.0">2</number>
, tergites 
<number box="[990,1031,1556,1580]" pageId="25" pageNumber="26" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
