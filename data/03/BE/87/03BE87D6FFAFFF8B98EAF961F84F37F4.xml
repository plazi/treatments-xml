<geoCoordinate box="[1332,1432,1755,1782]" degrees="58" direction="north" minutes="21" orientation="latitude" pageId="62" pageNumber="63" precision="925" value="58.35">58°21’N</geoCoordinate>
, 
<geoCoordinate box="[198,316,1791,1818]" degrees="147" direction="west" minutes="51" orientation="longitude" pageId="62" pageNumber="63" precision="925" value="-147.85">147°51’W</geoCoordinate>
