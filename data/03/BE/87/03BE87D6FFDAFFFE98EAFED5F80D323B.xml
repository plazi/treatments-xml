<geoCoordinate box="[206,306,475,501]" degrees="22" direction="north" minutes="27" orientation="latitude" pageId="75" pageNumber="76" precision="925" value="22.45">22°27’N</geoCoordinate>
, 
<geoCoordinate box="[317,436,475,502]" degrees="110" direction="west" minutes="59" orientation="longitude" pageId="75" pageNumber="76" precision="925" value="-110.98333">110°59’W</geoCoordinate>
