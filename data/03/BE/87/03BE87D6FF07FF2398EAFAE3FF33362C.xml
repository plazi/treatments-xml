<quantity box="[619,732,1514,1540]" metricMagnitude="2" metricUnit="m" metricValue="1.3" metricValueMax="2.3" metricValueMin="0.3" pageId="150" pageNumber="151" unit="m" value="130.0" valueMax="230.0" valueMin="30.0">30–230 m</quantity>
