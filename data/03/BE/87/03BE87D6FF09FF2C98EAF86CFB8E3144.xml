or Yellow Jack. To 
<quantity box="[1034,1116,1951,1977]" metricMagnitude="0" metricUnit="m" metricValue="1.2" pageId="152" pageNumber="153" unit="cm" value="120.0">120 cm</quantity>
(
<quantity box="[1130,1209,1951,1978]" metricMagnitude="0" metricUnit="m" metricValue="1.19888" pageId="152" pageNumber="153" unit="in" value="47.2">47.2 in</quantity>
) TL (
<bibRefCitation author="Randall" box="[1269,1424,1951,1978]" firstAuthor="Randall" pageId="152" pageNumber="153" refId="ref315737" refString="Randall, J. (1996) Shore Fishes of Hawaii. National World Press, Vida." type="book" year="1996">Randall 1996</bibRefCitation>
