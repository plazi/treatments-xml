<subSubSection pageId="3" pageNumber="67" type="etymology">
<paragraph blockId="3.[151,1436,151,402]" pageId="3" pageNumber="67">
<emphasis bold="true" box="[197,338,338,364]" pageId="3" pageNumber="67">Etymology.</emphasis>
