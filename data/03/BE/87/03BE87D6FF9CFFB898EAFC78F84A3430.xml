<quantity box="[1107,1248,942,969]" metricMagnitude="0" metricUnit="m" metricValue="5.785" metricValueMax="6.17" metricValueMin="5.4" pageId="13" pageNumber="14" unit="m" value="5.785" valueMax="6.17" valueMin="5.4">5.40–6.17 m</quantity>
