<collectingRegion box="[492,610,1259,1286]" country="United States of America" name="California" pageId="69" pageNumber="70">California</collectingRegion>
), 
<quantity box="[630,702,1259,1286]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="69" pageNumber="70" unit="m" value="200.0">200 m</quantity>
(
<quantity box="[717,788,1259,1286]" metricMagnitude="2" metricUnit="m" metricValue="1.999488" pageId="69" pageNumber="70" unit="ft" value="656.0">656 ft</quantity>
) or more (
<bibRefCitation author="Wisner" box="[908,1058,1259,1286]" firstAuthor="Wisner" pageId="69" pageNumber="70" refId="ref334240" refString="Wisner, R. L. (1976) The taxonomy and distribution of lanternfishes (family Myctophidae) of the eastern Pacific Ocean. Navy Ocean Research and Development Activity, Report 3." type="book" year="1976">Wisner 1976</bibRefCitation>
