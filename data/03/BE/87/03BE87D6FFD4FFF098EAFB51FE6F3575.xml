<quantity box="[1214,1307,1259,1286]" metricMagnitude="3" metricUnit="m" metricValue="4.939" pageId="69" pageNumber="70" unit="m" value="4939.0">4,939 m</quantity>
(
<quantity box="[1323,1429,1259,1286]" metricMagnitude="3" metricUnit="m" metricValue="4.93776" pageId="69" pageNumber="70" unit="ft" value="16200.0">16,200 ft</quantity>
