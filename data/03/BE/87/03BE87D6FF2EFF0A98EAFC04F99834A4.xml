<quantity box="[791,959,1123,1150]" metricMagnitude="3" metricUnit="m" metricValue="1.6745711999999997" metricValueMax="2.749296" metricValueMin="0.5998464" pageId="191" pageNumber="192" unit="ft" value="5494.0" valueMax="9020.0" valueMin="1968.0">
