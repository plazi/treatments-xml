<quantity box="[629,777,1122,1149]" metricMagnitude="3" metricUnit="m" metricValue="1.675" metricValueMax="2.75" metricValueMin="0.6" pageId="191" pageNumber="192" unit="m" value="1675.0" valueMax="2750.0" valueMin="600.0">600–2,750 m</quantity>
