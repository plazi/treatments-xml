<quantity box="[1142,1234,1223,1249]" metricMagnitude="3" metricUnit="m" metricValue="3.9" pageId="142" pageNumber="143" unit="m" value="3900.0">3,900 m</quantity>
(
<quantity box="[1248,1354,1223,1250]" metricMagnitude="3" metricUnit="m" metricValue="3.8990016" pageId="142" pageNumber="143" unit="ft" value="12792.0">12,792 ft</quantity>
