<quantity box="[234,302,439,465]" metricMagnitude="-1" metricUnit="m" metricValue="8.0" pageId="170" pageNumber="171" unit="cm" value="80.0">80 cm</quantity>
(
<quantity box="[317,397,438,466]" metricMagnitude="-1" metricUnit="m" metricValue="8.001" pageId="170" pageNumber="171" unit="in" value="31.5">31.5 in</quantity>
