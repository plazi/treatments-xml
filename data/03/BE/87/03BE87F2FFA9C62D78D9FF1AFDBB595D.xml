<number box="[486,516,732,758]" pageId="27" pageNumber="28" value="25.0">25</number>
–35, 31 µm and 
<number box="[706,735,732,758]" pageId="27" pageNumber="28" value="15.0">15</number>
–18, 16 µm long, of mid tibia 
<number box="[1085,1113,732,758]" pageId="27" pageNumber="28" value="13.0">13</number>
–20, 15 (
<number box="[1218,1233,732,758]" pageId="27" pageNumber="28" value="3.0">3</number>
) µm long, and of hind tibia 
<number box="[272,302,770,796]" pageId="27" pageNumber="28" value="28.0">28</number>
–35, 31 µm long. Width at apex of fore tibia 
<number box="[838,868,770,796]" pageId="27" pageNumber="28" value="25.0">25</number>
–30, 27 µm, of mid tibia 
<number box="[1168,1198,770,796]" pageId="27" pageNumber="28" value="23.0">23</number>
–28, 25 µm, of hind tibia 
<number box="[215,246,809,835]" pageId="27" pageNumber="28" value="45.0">45</number>
