<number box="[657,672,538,564]" pageId="27" pageNumber="28" value="5.0">5</number>
/ 
<number box="[695,713,538,564]" pageId="27" pageNumber="28" value="3.0">3</number>
: 
<number box="[725,777,538,564]" pageId="27" pageNumber="28" value="2.45">2.45</number>
–3.0, 
<number box="[842,882,538,564]" pageId="27" pageNumber="28" value="2.7">2.7</number>
