<collectingCountry box="[777,838,979,1005]" name="Chile" pageId="78" pageNumber="79">Chile</collectingCountry>
(
<geoCoordinate box="[856,951,978,1005]" degrees="21" direction="south" minutes="05" orientation="latitude" pageId="78" pageNumber="79" precision="925" value="-21.083334">21°05’S</geoCoordinate>
, 
<geoCoordinate box="[965,1070,979,1005]" degrees="71" direction="west" minutes="07" orientation="longitude" pageId="78" pageNumber="79" precision="925" value="-71.11667">71°07’W</geoCoordinate>
) (
<bibRefCitation author="Melendez and Sielfeld" box="[1095,1424,979,1005]" firstAuthor="Melendez" pageId="78" pageNumber="79" pagination="113 - 116" refId="ref301490" refString="Melendez C. R. &amp; Sielfeld K, W. (1991) Melanonus zugmayeri Norman, 1930, primer registro para la costa de Chile (Pisces: Gadiformes: Melanonidae). Estudios Oceanograficos, 10, 113 - 116." type="journal article" year="1991">Meléndez and Sielfeld 1991</bibRefCitation>
). Oceanic, mainly mesopelagic and bathypelagic; depth: 
<quantity box="[831,953,1015,1041]" metricMagnitude="3" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="78" pageNumber="79" unit="m" value="1500.0" valueMax="3000.0" valueMin="0.0">0–3,000 m</quantity>
