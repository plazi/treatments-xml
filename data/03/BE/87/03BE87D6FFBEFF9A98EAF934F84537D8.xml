<taxonomicName authorityName="Gilbert" authorityYear="1892" box="[358,509,1987,2013]" class="Actinopterygii" family="Alepocephalidae" genus="Alepocephalus" kingdom="Animalia" order="Osmeriformes" pageId="47" pageNumber="48" phylum="Chordata" rank="species" species="tenebrosus">
