<taxonomicName authority="Surrentina A." authorityName="Surrentina A." class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="19" pageNumber="362" phylum="Mollusca" rank="genus">
