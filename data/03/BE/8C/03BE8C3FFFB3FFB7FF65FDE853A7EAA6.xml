<collectingCountry box="[876,1237,929,955]" name="Democratic Republic of the Congo" pageId="6" pageNumber="407">Democratic Republic of Congo</collectingCountry>
, 
<collectingCountry box="[1251,1343,929,955]" name="Gambia" pageId="6" pageNumber="407">Gambia</collectingCountry>
, 
<collectingCountry box="[1356,1432,929,955]" name="Kenya" pageId="6" pageNumber="407">Kenya</collectingCountry>
