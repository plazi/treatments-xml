<taxonomicName authority="Loew" authorityName="Loew" box="[1273,1437,1722,1744]" class="Insecta" family="Ephydridae" genus="Paralimna" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="411" phylum="Arthropoda" rank="subGenus" subGenus="Paralimna">
