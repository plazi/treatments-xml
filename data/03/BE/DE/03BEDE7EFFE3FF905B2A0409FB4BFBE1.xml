<subSubSection pageId="19" pageNumber="20" type="diagnosis">
<paragraph blockId="19.[189,310,1030,1056]" box="[189,310,1030,1056]" pageId="19" pageNumber="20">
<heading bold="true" box="[189,310,1030,1056]" fontSize="11" level="3" pageId="19" pageNumber="20" reason="3">
