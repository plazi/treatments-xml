<paragraph blockId="52.[151,1437,1970,2028]" pageId="52" pageNumber="53">
<emphasis bold="true" box="[151,297,1970,1994]" pageId="52" pageNumber="53">FIGURE 33.</emphasis>
<taxonomicName authority="Champion" authorityName="Champion" authorityYear="1918" box="[306,621,1970,1993]" class="Insecta" family="Scirtidae" genus="Scirtes" kingdom="Animalia" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="sulcigerus">
