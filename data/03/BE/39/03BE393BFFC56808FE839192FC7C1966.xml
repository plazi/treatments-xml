<subSubSection box="[264,1112,999,1025]" pageId="1" pageNumber="2" type="materials_examined">
<paragraph blockId="1.[264,1112,999,1025]" box="[264,1112,999,1025]" pageId="1" pageNumber="2">
<emphasis box="[264,408,999,1024]" italics="true" pageId="1" pageNumber="2">
<typeStatus box="[264,316,999,1024]" pageId="1" pageNumber="2">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Owen and Pinto" authorityName="Owen and Pinto" box="[433,887,999,1025]" class="Insecta" family="Trichogrammatidae" genus="Pachamama" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="speciosa">
