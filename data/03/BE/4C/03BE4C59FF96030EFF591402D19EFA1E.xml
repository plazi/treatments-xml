<taxonomicName authority="Scheerpeltz, 1974" authorityName="Scheerpeltz" authorityYear="1974" box="[179,557,1372,1395]" class="Insecta" family="Staphylinidae" genus="Ischnopoderona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="163" phylum="Arthropoda" rank="genus">
