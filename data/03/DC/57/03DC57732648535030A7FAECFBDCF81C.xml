<number box="[991,1020,1505,1530]" pageId="10" pageNumber="237" value="36.0">36</number>
° 
<number box="[1028,1055,1505,1530]" pageId="10" pageNumber="237" value="21.0">21</number>
′ 
<number box="[1061,1128,1505,1530]" pageId="10" pageNumber="237" value="55.68">55.68</number>
″ N, 
<number box="[1181,1210,1505,1530]" pageId="10" pageNumber="237" value="52.0">52</number>
°06′ 
