<paragraph blockId="5.[151,1437,1681,1884]" box="[151,952,1681,1706]" pageId="5" pageNumber="515">
<emphasis bold="true" box="[151,365,1681,1706]" pageId="5" pageNumber="515">Southeast Pacific:</emphasis>
ex. 
<emphasis bold="true" box="[412,510,1681,1706]" pageId="5" pageNumber="515">1090383</emphasis>
, 5/280, [
<geoCoordinate box="[609,715,1681,1706]" direction="south" orientation="latitude" pageId="5" pageNumber="515" precision="55" value="-67.325">67.325°S</geoCoordinate>
<geoCoordinate box="[722,842,1681,1706]" direction="west" orientation="longitude" pageId="5" pageNumber="515" precision="55" value="-74.875">74.875°W</geoCoordinate>
], 
<quantity box="[860,948,1681,1706]" metricMagnitude="3" metricUnit="m" metricValue="2.577" pageId="5" pageNumber="515" unit="m" value="2577.0">2577 m</quantity>
.
