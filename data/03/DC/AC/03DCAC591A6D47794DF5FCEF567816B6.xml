<taxonomicName box="[858,982,1357,1378]" class="Insecta" family="Mantispidae" genus="Tuberonotha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="15" pageNumber="516" phylum="Arthropoda" rank="genus">
