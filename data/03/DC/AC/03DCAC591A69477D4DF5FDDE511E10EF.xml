<taxonomicName authority="Mats." authorityName="Mats." box="[995,1233,618,640]" class="Insecta" family="Mantispidae" genus="Mantispa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="10" pageNumber="511" phylum="Arthropoda" rank="species" species="suzukii">
