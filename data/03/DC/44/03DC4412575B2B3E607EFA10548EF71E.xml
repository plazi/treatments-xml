<subSubSection box="[559,825,1499,1524]" pageId="4" pageNumber="1861" type="nomenclature">
<paragraph blockId="4.[559,825,1499,1524]" box="[559,825,1499,1524]" pageId="4" pageNumber="1861">
<heading box="[559,825,1499,1524]" centered="true" fontSize="36" level="2" pageId="4" pageNumber="1861" reason="7">
<taxonomicName authority="Monné &amp; Monné, 2011" authorityName="Monné &amp; Monné" authorityYear="2011" box="[559,718,1499,1523]" class="Insecta" family="Cerambycidae" genus="Acanthocornis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="1861" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[559,718,1499,1523]" italics="true" pageId="4" pageNumber="1861">Acanthocornis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[724,825,1500,1524]" pageId="4" pageNumber="1861" rank="genus">gen. nov.</taxonomicNameLabel>
</heading>
