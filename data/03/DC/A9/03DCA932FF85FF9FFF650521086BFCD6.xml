<number box="[1268,1316,1719,1744]" pageId="1" pageNumber="175" value="4.85">4.85</number>
–5.00 mm (male), 
<quantity box="[238,338,1756,1781]" metricMagnitude="-3" metricUnit="m" metricValue="5.95" pageId="1" pageNumber="175" unit="mm" value="5.95">5.95 mm</quantity>
(female); forewing length 
<quantity box="[639,739,1756,1781]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="1" pageNumber="175" unit="mm" value="4.5">4.50 mm</quantity>
(male), 
<quantity box="[833,933,1756,1781]" metricMagnitude="-3" metricUnit="m" metricValue="5.4" pageId="1" pageNumber="175" unit="mm" value="5.4">5.40 mm</quantity>
