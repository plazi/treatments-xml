). 
<taxonomicName authority="Botosaneanu" authorityName="Botosaneanu" box="[327,763,584,608]" class="Insecta" family="Limnephilidae" genus="Consorophylax" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="384" phylum="Arthropoda" rank="species" species="styriacus">
