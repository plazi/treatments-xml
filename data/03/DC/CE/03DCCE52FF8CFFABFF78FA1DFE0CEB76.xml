<number box="[1168,1224,476,500]" pageId="3" pageNumber="384" value="1992.0">1992</number>
</taxonomicName>
(Karnische Alpen, southern Alps), 
<taxonomicName authority="Malicky 2008" authorityName="Malicky" authorityYear="2008" box="[334,599,511,536]" class="Insecta" family="Limnephilidae" genus="Consorophylax" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="384" phylum="Arthropoda" rank="species" species="corvo">
<emphasis box="[334,433,512,536]" italics="true" pageId="3" pageNumber="384">C. corvo</emphasis>
<bibRefCitation author="Malicky" box="[441,599,511,536]" pageId="3" pageNumber="384" refString="Malicky, H. (2008) Eine neue Consorophylax - Art aus dem Piemont (Italien) (Trichoptera, Limnephilidae). Braueria, 35, 1 - 40." type="journal article" year="2008">
Malicky 
<number box="[543,599,512,536]" pageId="3" pageNumber="384" value="2008.0">2008</number>
</bibRefCitation>
</taxonomicName>
(Piemont, western Alps), 
<taxonomicName authority="Malicky 2004" authorityName="Malicky" authorityYear="2004" box="[901,1221,511,536]" class="Insecta" family="Limnephilidae" genus="Consorophylax" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="384" phylum="Arthropoda" rank="species" species="delmastroi">
<emphasis box="[901,1054,512,536]" italics="true" pageId="3" pageNumber="384">C. delmastroi</emphasis>
<bibRefCitation author="Malicky" box="[1063,1221,511,536]" pageId="3" pageNumber="384" refString="Malicky, H. (2004) Atlas of European Trichoptera. Springer, Dordrecht, The Netherlands, second edition, 359 pp." type="book" year="2004">
Malicky 
<number box="[1165,1221,512,536]" pageId="3" pageNumber="384" value="2004.0">2004</number>
</bibRefCitation>
</taxonomicName>
(Piemont, western Alps), 
<taxonomicName authority="McLachlan 1867" authorityName="McLachlan" authorityYear="1867" box="[227,604,548,573]" class="Insecta" family="Limnephilidae" genus="Consorophylax" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="384" phylum="Arthropoda" rank="species" species="montivagus">
<emphasis box="[227,389,548,572]" italics="true" pageId="3" pageNumber="384">C. montivagus</emphasis>
(McLachlan 
<number box="[539,596,548,573]" pageId="3" pageNumber="384" value="1867.0">1867</number>
)
</taxonomicName>
(Koralpe, Saualpe, southeastern Alps), 
<taxonomicName box="[1050,1225,548,572]" class="Insecta" family="Limnephilidae" genus="Consorophylax" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="384" phylum="Arthropoda" rank="species" species="piemontanus">
<emphasis box="[1050,1225,548,572]" italics="true" pageId="3" pageNumber="384">C. piemontanus</emphasis>
</taxonomicName>
(Piemont, western Alps) (
<figureCitation box="[231,303,583,608]" captionStart="FIGURE 2" captionStartId="3.[151,250,1708,1730]" captionTargetBox="[161,1436,879,1678]" captionTargetId="figure@3.[161,1446,879,1678]" captionTargetPageId="3" captionText="FIGURE 2. Known distributions of Consorophylax species." httpUri="https://zenodo.org/record/244689/files/figure.png" pageId="3" pageNumber="384">
Fig. 
<number box="[286,303,584,608]" pageId="3" pageNumber="384" value="2.0">2</number>
