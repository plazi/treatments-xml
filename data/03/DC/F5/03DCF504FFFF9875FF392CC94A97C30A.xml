</taxonomicName>
, 
<taxonomicName authority="(Smith, 1855)" authorityName="Smith" authorityYear="1855" baseAuthorityName="Smith" baseAuthorityYear="1855" class="Insecta" family="Mutillidae" genus="Orientilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="desponsa">
<emphasis box="[1294,1436,1225,1250]" italics="true" pageId="10" pageNumber="11">O. desponsa</emphasis>
