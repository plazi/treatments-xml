<subSubSection box="[199,1386,930,955]" pageId="38" pageNumber="39" type="etymology">
<paragraph blockId="38.[151,1437,151,1027]" box="[199,1386,930,955]" pageId="38" pageNumber="39">
<emphasis bold="true" box="[199,334,930,955]" pageId="38" pageNumber="39">Etymology.</emphasis>
