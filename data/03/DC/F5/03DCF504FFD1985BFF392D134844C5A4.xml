<collectingRegion box="[263,398,768,793]" country="China" name="Guangdong" pageId="36" pageNumber="37">Guangdong</collectingRegion>
), 
<collectingCountry box="[420,529,768,793]" name="Indonesia" pageId="36" pageNumber="37">Indonesia</collectingCountry>
(
<collectingRegion box="[546,601,768,792]" country="Indonesia" name="Jawa Timur" pageId="36" pageNumber="37">Java</collectingRegion>
