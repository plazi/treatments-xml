<collectingCountry box="[622,717,768,793]" name="Vietnam" pageId="36" pageNumber="37">Vietnam</collectingCountry>
(
<collectingRegion box="[734,808,768,793]" country="Vietnam" name="Ha Noi" pageId="36" pageNumber="37">Hanoi</collectingRegion>
