<collectingCountry box="[1261,1328,732,757]" name="China" pageId="36" pageNumber="37">China</collectingCountry>
(
<collectingRegion box="[1346,1431,732,757]" country="China" name="Hainan" pageId="36" pageNumber="37">Hainan</collectingRegion>
, 
<collectingRegion box="[151,250,768,793]" country="China" name="Guangxi" pageId="36" pageNumber="37">Guangxi</collectingRegion>
