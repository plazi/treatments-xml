<subSubSection box="[199,904,1604,1629]" pageId="27" pageNumber="28" type="distribution">
<paragraph blockId="27.[151,1437,693,1845]" box="[199,904,1604,1629]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[199,352,1604,1629]" pageId="27" pageNumber="28">Distribution.</emphasis>
<collectingCountry box="[359,459,1604,1629]" name="Thailand" pageId="27" pageNumber="28">Thailand</collectingCountry>
(
<collectingRegion box="[473,541,1604,1629]" country="Thailand" name="Krabi" pageId="27" pageNumber="28">Krabi</collectingRegion>
, 
<collectingRegion box="[553,811,1604,1629]" country="Thailand" name="Nakhon Si Thammarat" pageId="27" pageNumber="28">Nakhon Si Thammarat</collectingRegion>
, 
