55, 
<emphasis box="[193,232,1191,1215]" italics="true" pageId="336" pageNumber="337">ps2</emphasis>
45, 
<emphasis box="[282,310,1191,1215]" italics="true" pageId="336" pageNumber="337">g1</emphasis>
30, 
<emphasis box="[360,389,1191,1215]" italics="true" pageId="336" pageNumber="337">1a</emphasis>
50–65, 
<emphasis box="[481,508,1191,1215]" italics="true" pageId="336" pageNumber="337">3a</emphasis>
65–75, 
<emphasis box="[601,643,1191,1215]" italics="true" pageId="336" pageNumber="337">ag1</emphasis>
80–95, 
