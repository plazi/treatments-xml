, 
<taxonomicName authority="Koehler, 1907" authorityName="Koehler" authorityYear="1907" box="[646,927,1186,1213]" class="Asteroidea" family="Ganeriidae" genus="Scotiaster" kingdom="Animalia" order="Valvatida" pageId="14" pageNumber="81" phylum="Echinodermata" rank="genus">
