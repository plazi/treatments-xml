<number box="[290,362,937,962]" pageId="10" pageNumber="367" value="57821.0">57821</number>
</accessionNumber>
, QM 
<accessionNumber box="[425,515,937,962]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/F57822" pageId="10" pageNumber="367">
F 
<number box="[441,515,937,962]" pageId="10" pageNumber="367" value="57822.0">57822</number>
</accessionNumber>
+; ACC: I. 
<number box="[628,675,938,962]" pageId="10" pageNumber="367" value="110.0">110</number>
