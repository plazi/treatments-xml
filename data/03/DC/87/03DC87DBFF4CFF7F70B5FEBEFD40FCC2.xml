<taxonomicName authority="Linnaeus" authorityName="Linnaeus" authorityYear="1758" box="[505,795,1351,1376]" class="Aves" family="Hirundinidae" genus="Hirundo" kingdom="Animalia" order="Passeriformes" pageId="192" pageNumber="193" phylum="Chordata" rank="species" species="rustica">
