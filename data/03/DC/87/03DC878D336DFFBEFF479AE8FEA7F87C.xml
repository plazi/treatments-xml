<subSubSection pageId="11" pageNumber="552" type="distribution">
<paragraph blockId="11.[151,1437,1873,2006]" pageId="11" pageNumber="552">
<emphasis bold="true" box="[199,352,1945,1970]" pageId="11" pageNumber="552">Distribution.</emphasis>
<collectingCountry box="[359,472,1945,1970]" name="Colombia" pageId="11" pageNumber="552">Colombia</collectingCountry>
