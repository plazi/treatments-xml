230–245, 
<emphasis box="[831,857,1277,1301]" italics="true" pageId="353" pageNumber="354">c2</emphasis>
155–210, 
<emphasis box="[976,1005,1277,1301]" italics="true" pageId="353" pageNumber="354">d1</emphasis>
115–140, 
<emphasis box="[1123,1150,1277,1301]" italics="true" pageId="353" pageNumber="354">d2</emphasis>
155–175, 
<emphasis box="[1269,1296,1277,1301]" italics="true" pageId="353" pageNumber="354">e2</emphasis>
100–110, 
<emphasis box="[1414,1435,1277,1301]" italics="true" pageId="353" pageNumber="354">f1</emphasis>
40–45, 
<emphasis box="[236,258,1313,1337]" italics="true" pageId="353" pageNumber="354">f2</emphasis>
85–95, 
<emphasis box="[350,378,1313,1337]" italics="true" pageId="353" pageNumber="354">h1</emphasis>
variable 95–180, 
<emphasis box="[583,622,1313,1337]" italics="true" pageId="353" pageNumber="354">ps1</emphasis>
30–35, 
<emphasis box="[715,754,1313,1337]" italics="true" pageId="353" pageNumber="354">ps2</emphasis>
30–35, 
<emphasis box="[846,874,1313,1337]" italics="true" pageId="353" pageNumber="354">g1</emphasis>
35–45, 
<emphasis box="[967,995,1313,1337]" italics="true" pageId="353" pageNumber="354">1a</emphasis>
