30, 
<emphasis box="[1124,1166,1313,1337]" italics="true" pageId="353" pageNumber="354">ag1</emphasis>
70–75, 
<emphasis box="[1259,1300,1313,1337]" italics="true" pageId="353" pageNumber="354">ag2</emphasis>
70–75, 
<emphasis box="[1394,1436,1313,1337]" italics="true" pageId="353" pageNumber="354">ag3</emphasis>
145–160, 
