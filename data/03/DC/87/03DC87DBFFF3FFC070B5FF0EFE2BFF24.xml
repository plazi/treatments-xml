<typeStatus box="[151,203,218,239]" pageId="127" pageNumber="128">Type</typeStatus>
species: 
<taxonomicName authority="Skoracki, 2004" authorityName="Skoracki" authorityYear="2004" box="[294,665,217,239]" class="Arachnida" family="Syringophilidae" genus="Aulonastus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="127" pageNumber="128" phylum="Arthropoda" rank="species" species="buczekae">
<emphasis box="[294,506,217,239]" italics="true" pageId="127" pageNumber="128">Aulonastus buczekae</emphasis>
Skoracki, 2004
</taxonomicName>
