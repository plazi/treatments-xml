<emphasis box="[151,179,780,804]" italics="true" pageId="94" pageNumber="95">d1</emphasis>
200–205, 
<emphasis box="[297,325,780,804]" italics="true" pageId="94" pageNumber="95">d2</emphasis>
190–220, 
<emphasis box="[443,469,780,804]" italics="true" pageId="94" pageNumber="95">e2</emphasis>
170–230, 
<emphasis box="[587,608,780,804]" italics="true" pageId="94" pageNumber="95">f1</emphasis>
and 
<emphasis box="[662,690,780,804]" italics="true" pageId="94" pageNumber="95">h1</emphasis>
25–30, 
<emphasis box="[780,801,780,804]" italics="true" pageId="94" pageNumber="95">f2</emphasis>
215–230, 
<emphasis box="[920,948,780,804]" italics="true" pageId="94" pageNumber="95">h2</emphasis>
330–370, 
<emphasis box="[1066,1104,780,804]" italics="true" pageId="94" pageNumber="95">ps1</emphasis>
15–20, 
