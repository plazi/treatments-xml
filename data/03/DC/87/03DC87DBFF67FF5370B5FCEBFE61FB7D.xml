165, 
<emphasis box="[1092,1119,1649,1673]" italics="true" pageId="235" pageNumber="236">c1</emphasis>
130, 
<emphasis box="[1180,1207,1649,1673]" italics="true" pageId="235" pageNumber="236">c2</emphasis>
120–170, 
<emphasis box="[1324,1353,1649,1673]" italics="true" pageId="235" pageNumber="236">d1</emphasis>
20–25, 
<emphasis box="[151,179,1685,1709]" italics="true" pageId="235" pageNumber="236">d2</emphasis>
30–45, 
<geoCoordinate box="[270,332,1685,1709]" degrees="2" direction="east" minutes="20" orientation="longitude" pageId="235" pageNumber="236" precision="925" value="2.3333333">
<emphasis box="[270,296,1685,1709]" italics="true" pageId="235" pageNumber="236">e2</emphasis>
20
</geoCoordinate>
–25, 
<emphasis box="[387,416,1685,1709]" italics="true" pageId="235" pageNumber="236">h2</emphasis>
245–255, 
<emphasis box="[534,556,1685,1709]" italics="true" pageId="235" pageNumber="236">f2</emphasis>
40, 
<emphasis box="[605,702,1685,1709]" italics="true" pageId="235" pageNumber="236">tc’III–IV</emphasis>
and 
<emphasis box="[757,861,1685,1709]" italics="true" pageId="235" pageNumber="236">tc”III–IV</emphasis>
50, 
