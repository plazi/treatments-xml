<taxonomicName authority="(Linnaeus)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[510,839,1751,1775]" class="Aves" family="Motacillidae" genus="Anthus" kingdom="Animalia" order="Passeriformes" pageId="189" pageNumber="190" phylum="Chordata" rank="species" species="spinoletta">
