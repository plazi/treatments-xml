295–315, 
<emphasis box="[788,817,1393,1417]" italics="true" pageId="75" pageNumber="76">d1</emphasis>
235–275, 
<emphasis box="[938,966,1393,1417]" italics="true" pageId="75" pageNumber="76">d2</emphasis>
215–265, 
<emphasis box="[1087,1113,1393,1417]" italics="true" pageId="75" pageNumber="76">e2</emphasis>
200–240, 
<emphasis box="[1235,1256,1393,1417]" italics="true" pageId="75" pageNumber="76">f1</emphasis>
40–45, 
<emphasis box="[1350,1372,1393,1417]" italics="true" pageId="75" pageNumber="76">f2</emphasis>
330– 380, 
