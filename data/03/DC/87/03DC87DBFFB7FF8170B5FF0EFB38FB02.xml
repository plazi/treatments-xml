<typeStatus box="[816,866,282,303]" pageId="59" pageNumber="60">Type</typeStatus>
locality 
<collectingCountry box="[954,1053,281,303]" name="Kyrgyzstan" pageId="59" pageNumber="60">Kirghizia</collectingCountry>
. FEMALE (
