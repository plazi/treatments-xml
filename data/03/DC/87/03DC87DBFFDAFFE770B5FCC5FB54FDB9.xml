<emphasis box="[508,531,1627,1650]" italics="true" pageId="86" pageNumber="87">se</emphasis>
and 
<emphasis box="[585,612,1626,1650]" italics="true" pageId="86" pageNumber="87">c1</emphasis>
. Length ratio of setae 
<emphasis box="[863,883,1627,1650]" italics="true" pageId="86" pageNumber="87">vi</emphasis>
: 
<emphasis box="[891,916,1627,1650]" italics="true" pageId="86" pageNumber="87">ve</emphasis>
: 
<emphasis box="[924,943,1627,1650]" italics="true" pageId="86" pageNumber="87">si</emphasis>
