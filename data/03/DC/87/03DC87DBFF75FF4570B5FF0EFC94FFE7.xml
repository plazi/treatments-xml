<taxonomicName authorityName="Skoracki" authorityYear="2011" box="[460,588,1027,1050]" class="Arachnida" family="Syringophilidae" genus="Torotrogla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="250" pageNumber="251" phylum="Arthropoda" rank="species" species="pycnonotus">
