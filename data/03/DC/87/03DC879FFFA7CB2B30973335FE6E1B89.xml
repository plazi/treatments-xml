<paragraph blockId="1.[140,981,1363,1496]" box="[140,959,1434,1460]" pageId="1" pageNumber="56">
<typeStatus box="[140,195,1434,1460]" pageId="1" pageNumber="56">Type</typeStatus>
species: 
<taxonomicName authority="Mesibov" authorityName="Mesibov" authorityYear="2009" box="[294,610,1434,1460]" class="Diplopoda" family="Not" genus="Noteremus" higherTaxonomySource="CoL" kingdom="Animalia" order="Polydesmida" pageId="1" pageNumber="56" phylum="Arthropoda" rank="species" species="summus" status="sp. n.">
<emphasis box="[294,502,1435,1460]" italics="true" pageId="1" pageNumber="56">Noteremus summus</emphasis>
Mesibov
</taxonomicName>
, 
<taxonomicNameLabel box="[621,684,1434,1460]" pageId="1" pageNumber="56" rank="species">sp. n.</taxonomicNameLabel>
, by present designation.
</paragraph>
<paragraph blockId="1.[140,981,1363,1496]" box="[140,705,1469,1496]" pageId="1" pageNumber="56">
