<heading bold="true" box="[151,709,620,652]" fontSize="11" level="1" pageId="2" pageNumber="511" reason="1">
<emphasis bold="true" box="[151,709,620,652]" pageId="2" pageNumber="511">
Genus 
<taxonomicName authority="Griffini, 1913" authorityName="Griffini" authorityYear="1913" box="[238,585,625,652]" class="Insecta" family="Chorotypidae" genus="Paterdecolyus" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="511" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,407,625,652]" italics="true" pageId="2" pageNumber="511">Paterdecolyus</emphasis>
<bibRefCitation author="Griffini, A." box="[415,585,625,651]" pageId="2" pageNumber="511" pagination="61 - 104" refId="ref5870" refString="Griffini, A. (1913) Sopra alcuni Grillacridi e Stenopelmatidi della collezione Pantel. Atti della Societa Italiana di Scienze Naturali e del Museo Civico di Storia Naturale di Milano, 52, 61 - 104." type="journal article" year="1913">Griffini, 1913</bibRefCitation>
