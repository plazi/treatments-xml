255– 265, 
<emphasis box="[209,235,1173,1197]" italics="true" pageId="374" pageNumber="375">c1</emphasis>
270–280, 
<emphasis box="[358,385,1173,1197]" italics="true" pageId="374" pageNumber="375">c2</emphasis>
250–265, 
<emphasis box="[508,535,1173,1197]" italics="true" pageId="374" pageNumber="375">d1</emphasis>
190–220, 
<emphasis box="[658,686,1173,1197]" italics="true" pageId="374" pageNumber="375">d2</emphasis>
230–255, 
<emphasis box="[809,836,1173,1197]" italics="true" pageId="374" pageNumber="375">e2</emphasis>
140–170, 
<emphasis box="[958,980,1173,1197]" italics="true" pageId="374" pageNumber="375">f1</emphasis>
45–50, 
<emphasis box="[1075,1098,1173,1197]" italics="true" pageId="374" pageNumber="375">f2</emphasis>
60–70, 
<emphasis box="[1192,1220,1173,1197]" italics="true" pageId="374" pageNumber="375">h1</emphasis>
340–380, 
<emphasis box="[1343,1371,1173,1197]" italics="true" pageId="374" pageNumber="375">h2</emphasis>
460– 540, 
<emphasis box="[210,248,1209,1233]" italics="true" pageId="374" pageNumber="375">ps1</emphasis>
12, 
<emphasis box="[304,343,1209,1233]" italics="true" pageId="374" pageNumber="375">ps2</emphasis>
25, 
<emphasis box="[397,425,1209,1233]" italics="true" pageId="374" pageNumber="375">g1</emphasis>
70–80, 
<emphasis box="[523,551,1209,1233]" italics="true" pageId="374" pageNumber="375">1a</emphasis>
100–120, 
