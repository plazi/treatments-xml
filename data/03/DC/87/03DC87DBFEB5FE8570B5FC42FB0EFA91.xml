<taxonomicName authorityName="Chirov and Kravtsova" authorityYear="1995" box="[867,1001,224,248]" class="Arachnida" family="Syringophilidae" genus="Mironovia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="314" pageNumber="315" phylum="Arthropoda" rank="species" species="phasiani">
