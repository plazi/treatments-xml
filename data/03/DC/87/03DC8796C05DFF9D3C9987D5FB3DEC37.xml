(
<figureCitation box="[159,231,1285,1310]" captionStart="FIGURES 1 – 15" captionStartId="3.[151,263,1738,1759]" captionTargetBox="[190,1394,195,1718]" captionTargetId="figure@3.[190,1395,195,1719]" captionTargetPageId="3" captionText="FIGURES 1 – 15. Atrovirensis spp., adults. 1. A. atrovirens, Nepal (after Gyulai et al. 2013); 2. A. annamita, HT, Vietnam (after Gyulai et al. 2013); 3. A. parannamita sp. n., male HT, China, Aut. Reg. Xizang, genit. prep. HHL- 2930 - 1; 4. A. parannamita sp. n., female PT, China, Aut. Reg. Xizang, genit. prep. HHL- 2906 - 2; 5. A. pedibus, HT, China, Prov. Hubei (after Gyulai et al. 2013); 6. A. sp. cf. A. parannamita, male, China, Aut. Reg. Xizang, genit. prep. HHL- 2907 - 1; 7. A. sinica, HT, China, Prov. Guangdong, genit. prep. ZFMK 1624; 8. A. owadai, Prov. Guangdong, HT, China (after Gyulai et al. 2013); 9. A. taiwani, HT, Taiwan (after Gyulai et al. 2013); 10. A. furcatus sp. n., male HT, China, Prov. Guangdong, genit. prep. HHL- 2921 - 1; 11. A. furcatus sp. n., female PT, China, Prov. Guangdong, genit. prep. HHL- 2919 - 2; 12. A. sp. cf. furcatus, male, China, Prov. Guangdong, genit. prep. HHL- 2918 - 1; 13. A. sp. cf. furcatus, female, China, Prov. Guangdong, genit. prep. HHL- 2909 - 2; 14. A. sacculatus sp. n., male HT, China, Aut. Reg. Xizang, genit. prep. HHL- 2910 - 1; 15. A. sacculatus sp. n., male PT, China, Aut. Reg. Xizang, genit. prep. HHL- 2908 - 1." httpUri="https://zenodo.org/record/267006/files/figure.png" pageId="11" pageNumber="212">Figs 6</figureCitation>
, 
<figureCitation box="[243,273,1286,1310]" captionStart="FIGURES 24 – 26" captionStartId="5.[151,263,1840,1861]" captionTargetBox="[151,1436,197,1810]" captionTargetId="figure@5.[151,1436,193,1818]" captionTargetPageId="5" captionText="FIGURES 24 – 26. Atrovirensis spp., male genitalia. 24. A. parannamita sp. n., HT, China, Aut. Reg. Xizang, genit. prep. HHL- 2930 - 1; 25. A. pedibus. HT, China, Prov. Hubei, genit. prep. PG 3086 (after Gyulai et al. 2013); 26. A. sp. cf. A. parannamita HT, China, Aut. Reg. Xizang, genit. prep. HHL- 2907 - 1." httpUri="https://zenodo.org/record/267008/files/figure.png" pageId="11" pageNumber="212">26</figureCitation>
, 
<figureCitation box="[285,314,1285,1310]" captionStart="FIGURES 36 – 44" captionStartId="9.[151,263,1840,1861]" captionTargetBox="[151,1436,193,1810]" captionTargetId="figure@9.[151,1436,193,1818]" captionTargetPageId="9" captionText="FIGURES 36 – 44. Atrovirensis spp., female genitalia. 36. A. atrovirens, Nepal, genit. prep. RL 10874; 37. A. annamita, Vietnam, PT, genit. prep. RL 10894; 38. A. parannamita sp. n., PT, China, Aut. Reg. Xizang, genit. prep. HHL- 2906 - 2; 39. A. pedibus, PT, China, Prov. Hubei, GYP 3329; 40. A. sp. cf. A. parannamita, China, Aut. Reg. Xizang, genit. prep. HHL- 2925 - 2; 41. A. owadai, PT, China, Guangdong, genit. prep. W 023 (after Gyulai et al. 2013); 42. A. owadai, PT, China, Prov. Guangdong, genit. prep. HHL- 2913 - 2; 43. A. furcatus sp. n., PT, China, Prov. Guangdong, HHL- 2919 - 2; 44. A. sp. cf. furcatus, China, Prov. Guangdong, HHL- 2909 - 2." httpUri="https://zenodo.org/record/267012/files/figure.png" pageId="11" pageNumber="212">40</figureCitation>
