<taxonomicName authorityName="Skoracki" authorityYear="2011" box="[459,562,1784,1808]" class="Arachnida" family="Syringophilidae" genus="Aulonastus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="fringillus">
