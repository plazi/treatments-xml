<collectingMunicipality box="[850,991,1726,1748]" pageId="12" pageNumber="381">Myall Creek</collectingMunicipality>
<taxonomicName authorityName="Owen" authorityYear="1838" box="[998,1126,1726,1747]" class="Mammalia" family="Diprotodontidae" genus="Diprotodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diprotodontia" pageId="12" pageNumber="381" phylum="Chordata" rank="genus">
