<taxonomicName box="[1054,1164,1525,1549]" class="Mammalia" family="Alagomyidae" genus="Alagomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
<emphasis box="[1054,1164,1525,1549]" italics="true" pageId="14" pageNumber="15">Alagomys</emphasis>
</taxonomicName>
and 
<emphasis box="[701,890,1554,1578]" italics="true" pageId="14" pageNumber="15">
<taxonomicName box="[701,886,1554,1578]" class="Mammalia" family="Alagomyidae" genus="Tribosphenomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">Tribosphenomys</taxonomicName>
