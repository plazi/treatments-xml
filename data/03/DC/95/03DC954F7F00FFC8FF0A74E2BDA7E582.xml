<emphasis bold="true" box="[199,262,531,555]" pageId="10" pageNumber="11">Host.</emphasis>
<collectingCountry box="[269,366,530,554]" name="Trinidad and Tobago" pageId="10" pageNumber="11">Tobacco</collectingCountry>
, 
<taxonomicName box="[377,591,531,555]" class="Magnoliopsida" family="Solanaceae" genus="Nicotiana" kingdom="Plantae" order="Solanales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="tabacum">
<emphasis box="[377,591,531,555]" italics="true" pageId="10" pageNumber="11">Nicotiana tabacum</emphasis>
</taxonomicName>
L. [
<taxonomicName box="[636,768,530,554]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
