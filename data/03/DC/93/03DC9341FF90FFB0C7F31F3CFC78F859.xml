<emphasis bold="true" box="[199,347,511,536]" pageId="7" pageNumber="408">
<typeStatus box="[199,258,511,536]" pageId="7" pageNumber="408">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName box="[362,564,511,536]" class="Malacostraca" family="Parapseudidae" genus="Actenos" kingdom="Animalia" order="Tanaidacea" pageId="7" pageNumber="408" phylum="Arthropoda" rank="species" species="marshalli" status="sp. nov.">
<emphasis box="[362,564,511,536]" italics="true" pageId="7" pageNumber="408">Actenos marshalli</emphasis>
</taxonomicName>
