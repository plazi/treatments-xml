<number box="[792,844,1719,1743]" pageId="32" pageNumber="483" value="11.2">11.2</number>
. Carapace 
<number box="[965,1000,1718,1743]" pageId="32" pageNumber="483" value="5.6">5.6</number>
long; 
<number box="[1072,1121,1718,1743]" pageId="32" pageNumber="483" value="4.55">4.55</number>
wide; 
