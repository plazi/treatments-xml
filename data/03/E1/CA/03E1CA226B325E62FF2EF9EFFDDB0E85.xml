high. Clypeus 
<number box="[1401,1436,1718,1743]" pageId="32" pageNumber="483" value="0.5">0.5</number>
high. Abdomen 
<number box="[333,368,1754,1779]" pageId="32" pageNumber="483" value="5.2">5.2</number>
long; 
<number box="[440,475,1755,1779]" pageId="32" pageNumber="483" value="3.7">3.7</number>
