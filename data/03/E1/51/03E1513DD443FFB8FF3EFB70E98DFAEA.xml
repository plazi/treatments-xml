<paragraph blockId="3.[151,1437,151,1403]" pageId="3" pageNumber="102">
<taxonomicName authority="Lindner" authorityName="Lindner" box="[199,559,766,791]" class="Insecta" family="Syrphidae" genus="Ischyroptera" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="102" phylum="Arthropoda" rank="species" species="annulipes">
<emphasis box="[199,458,766,791]" italics="true" pageId="3" pageNumber="102">Ischyroptera annulipes</emphasis>
Lindner
</taxonomicName>
: 
<typeStatus box="[571,678,766,791]" pageId="3" pageNumber="102">Holotype</typeStatus>
