<emphasis bold="true" box="[657,1088,812,836]" pageId="7" pageNumber="16">
<taxonomicName authorityName="Gnezdilov" authorityYear="2015" box="[657,914,812,836]" class="Insecta" family="Issidae" genus="Bruneastrum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="16" phylum="Arthropoda" rank="species" species="cardinale">
