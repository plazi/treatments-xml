<taxonomicName authority="Butler, 1875" authorityName="Butler" authorityYear="1875" box="[655,1088,972,996]" class="Insecta" family="Issidae" genus="Hemisphaerius" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="16" phylum="Arthropoda" rank="species" species="bacculinus">
