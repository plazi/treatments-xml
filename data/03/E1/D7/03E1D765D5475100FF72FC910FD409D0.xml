<taxonomicNameLabel box="[597,662,1836,1862]" pageId="9" pageNumber="510" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName authority="Menzel &amp; Vilkamaa, 2021" authorityName="Menzel &amp; Vilkamaa" authorityYear="2021" box="[719,837,1836,1862]" class="Insecta" family="Sciaridae" genus="Lycoriella" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="510" phylum="Arthropoda" rank="species" species="tundrae" status="sp. n.">
<emphasis box="[719,837,1836,1862]" italics="true" pageId="9" pageNumber="510">L. tundrae</emphasis>
</taxonomicName>
