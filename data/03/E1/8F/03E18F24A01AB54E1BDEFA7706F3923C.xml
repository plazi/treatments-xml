. 
<taxonomicName authorityName="Bachmayer" authorityYear="1950" box="[551,776,662,688]" class="Malacostraca" family="Galatheidae" genus="Galathea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="212" phylum="Arthropoda" rank="species" species="weinfurteri">
