</taxonomicName>
(
<emphasis bold="true" box="[941,953,1860,1882]" pageId="4" pageNumber="148">x</emphasis>
) and 
<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[1007,1140,1860,1881]" class="Insecta" family="Pulicidae" genus="Pulex" kingdom="Animalia" order="Siphonaptera" pageId="4" pageNumber="148" phylum="Arthropoda" rank="species" species="irritans">
