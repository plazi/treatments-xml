setae; R 
<number box="[1051,1061,696,713]" pageId="7" pageNumber="8" value="1.0">1</number>
with 
<number box="[1126,1201,684,710]" pageId="7" pageNumber="8" value="18.0" valueMax="25.0" valueMin="11.0">11–25</number>
; R 
<number box="[1233,1243,696,713]" pageId="7" pageNumber="8" value="4.0">4</number>
+ 
<number box="[1254,1263,696,713]" pageId="7" pageNumber="8" value="5.0">5</number>
with 
<number box="[264,341,727,753]" pageId="7" pageNumber="8" value="21.0" valueMax="22.0" valueMin="20.0">20–22</number>
