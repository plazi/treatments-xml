<taxonomicName authority="Gaillard, 1900" authorityName="Gaillard" authorityYear="1900" box="[203,497,1612,1629]" class="Mammalia" family="Muridae" genus="Anomalomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="17" pageNumber="324" phylum="Chordata" rank="species" species="gaudryi">
