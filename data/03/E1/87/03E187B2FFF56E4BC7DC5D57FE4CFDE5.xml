<collectingRegion box="[837,944,548,572]" country="Canada" name="Manitoba" pageId="54" pageNumber="55">Manitoba</collectingRegion>
and 
<collectingRegion box="[1004,1090,548,572]" country="Canada" name="Ontario" pageId="54" pageNumber="55">Ontario</collectingRegion>
in 
<collectingCountry box="[1131,1218,548,572]" name="Canada" pageId="54" pageNumber="55">Canada</collectingCountry>
, and 
<collectingRegion box="[1282,1338,548,572]" country="United States of America" name="Ohio" pageId="54" pageNumber="55">Ohio</collectingRegion>
in 
<collectingCountry box="[1380,1436,548,572]" name="United States of America" pageId="54" pageNumber="55">USA</collectingCountry>
(
