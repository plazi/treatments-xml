<taxonomicName authorityName="C. L. Koch" authorityYear="1839" box="[294,414,1724,1746]" class="Arachnida" family="Cosmetidae" genus="Paecilaema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="20" pageNumber="211" phylum="Arthropoda" rank="genus">Paecilaema</taxonomicName>
