<subSubSection pageId="6" pageNumber="313" type="reference_group">
<paragraph blockId="6.[111,647,851,1130]" pageId="6" pageNumber="313">
<taxonomicName baseAuthorityName="von Meyer" baseAuthorityYear="1838" box="[113,366,881,907]" class="Mammalia" family="Castoridae" genus="Trogontherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="6" pageNumber="313" phylum="Chordata" rank="species" species="minutum">
