<taxonomicName authority="(Xu et al. 2013)" authorityName="Xu" authorityYear="2013" baseAuthorityName="Xu" baseAuthorityYear="2013" box="[685,1011,1361,1388]" class="Insecta" family="Dryinidae" genus="Dryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="533" phylum="Arthropoda" rank="species" species="peterseni">
