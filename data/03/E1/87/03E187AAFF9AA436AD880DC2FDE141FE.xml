<emphasis box="[1270,1390,1325,1351]" italics="true" pageId="4" pageNumber="533">D. pyrillae</emphasis>
</taxonomicName>
and 
<taxonomicName authority="Latreille, 1804" authorityName="Ashmead" authorityYear="1904" box="[151,275,1362,1387]" class="Insecta" family="Dryinidae" genus="Dryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="533" phylum="Arthropoda" rank="species" species="stantoni">
