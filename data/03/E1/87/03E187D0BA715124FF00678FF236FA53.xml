<subSubSection pageId="5" pageNumber="530" type="description">
<paragraph blockId="5.[151,1437,511,1439]" pageId="5" pageNumber="530">
<emphasis bold="true" box="[199,345,655,682]" pageId="5" pageNumber="530">Description.</emphasis>
Tegmen 
<emphasis box="[450,462,657,682]" italics="true" pageId="5" pageNumber="530">c</emphasis>
. 
<quantity box="[477,641,655,683]" metricMagnitude="-2" metricUnit="m" metricValue="2.01" metricValueMax="2.05" metricValueMin="1.97" pageId="5" pageNumber="530" unit="mm" value="20.1" valueMax="20.5" valueMin="19.7">19.7–20.5 mm</quantity>
