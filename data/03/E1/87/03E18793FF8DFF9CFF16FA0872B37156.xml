Pupinidius 
<taxonomicName authority="Moellendorff, 1901" authorityName="Moellendorff" authorityYear="1901" box="[430,725,1578,1601]" pageId="25" pageNumber="26" rank="subSpecies" species="nanpingensis" subSpecies="nanpingensis">
<taxonomicName box="[430,574,1578,1601]" pageId="25" pageNumber="26" rank="species" species="nanpingensis">nanpingensis</taxonomicName>
<taxonomicName authority="Moellendorff, 1901" authorityName="Moellendorff" authorityYear="1901" box="[581,725,1578,1601]" pageId="25" pageNumber="26" rank="species" species="nanpingensis">nanpingensis</taxonomicName>
