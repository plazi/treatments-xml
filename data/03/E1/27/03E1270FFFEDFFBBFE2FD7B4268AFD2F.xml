<collectingCountry box="[304,406,667,691]" name="Australia" pageId="44" pageNumber="45">Australia</collectingCountry>
: 
<collectingRegion box="[416,614,667,691]" country="Australia" name="Northern Territory" pageId="44" pageNumber="45">Northern Territory</collectingRegion>
, 
<collectingRegion box="[624,753,667,691]" country="Australia" name="Queensland" pageId="44" pageNumber="45">Queensland</collectingRegion>
, 
<collectingRegion box="[763,956,667,691]" country="Australia" name="Western Australia" pageId="44" pageNumber="45">Western Australia</collectingRegion>
.
</paragraph>
