<collectingCountry box="[266,368,699,723]" name="Australia" pageId="23" pageNumber="24">Australia</collectingCountry>
: 
<collectingRegion box="[376,673,699,723]" country="Australia" name="Australian Capital Territory" pageId="23" pageNumber="24">Australian Capital Territory</collectingRegion>
, 
<collectingRegion box="[683,772,699,723]" country="Australia" name="Victoria" pageId="23" pageNumber="24">Victoria</collectingRegion>
.
</paragraph>
