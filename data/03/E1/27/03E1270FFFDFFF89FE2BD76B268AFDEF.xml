<collectingCountry box="[304,406,603,627]" name="Australia" pageId="30" pageNumber="31">Australia</collectingCountry>
: 
<collectingRegion box="[416,545,603,627]" country="Australia" name="Queensland" pageId="30" pageNumber="31">Queensland</collectingRegion>
, 
<collectingRegion box="[555,753,603,627]" country="Australia" name="Northern Territory" pageId="30" pageNumber="31">Northern Territory</collectingRegion>
, 
<collectingRegion box="[763,956,603,627]" country="Australia" name="Western Australia" pageId="30" pageNumber="31">Western Australia</collectingRegion>
