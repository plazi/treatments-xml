<taxonomicName authority="Chambers, 1872" authorityName="Chambers" authorityYear="1872" box="[1024,1338,1677,1703]" class="Insecta" family="Gelechiidae" genus="Agnippe" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
<emphasis box="[1024,1126,1677,1702]" italics="true" pageId="28" pageNumber="29">Agnippe</emphasis>
Chambers, 1872
</taxonomicName>
(
<bibRefCitation author="Lee &amp; Brown" firstAuthor="Lee" pageId="28" pageNumber="29" pagination="1 - 55" refId="ref41440" refString="Lee, S. M. &amp; Brown, R. L. (2008) Revision of Holarctic Teleiodini (Lepidoptera: Gelechiidae). Zootaxa, 1818, 1 - 55." type="journal article" year="2008">Lee &amp; Brown 2008</bibRefCitation>
), and 
<taxonomicName authority="(Christoph, 1888)" baseAuthorityName="Christoph" baseAuthorityYear="1888" box="[370,752,1714,1740]" class="Insecta" family="Gelechiidae" genus="Agnippe" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="pseudolella">
<emphasis box="[370,537,1715,1740]" italics="true" pageId="28" pageNumber="29">E. pseudolella</emphasis>
(Christoph, 1888)
</taxonomicName>
