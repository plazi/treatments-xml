<collectingCountry box="[624,713,936,962]" name="Finland" pageId="46" pageNumber="47">Finland</collectingCountry>
and 
<collectingCountry box="[774,849,936,962]" name="Latvia" pageId="46" pageNumber="47">Latvia</collectingCountry>
