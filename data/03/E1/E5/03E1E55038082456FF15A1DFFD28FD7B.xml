, 
<collectingRegion box="[501,596,722,746]" country="China" name="Guangxi" pageId="3" pageNumber="149">Guangxi</collectingRegion>
, 
<collectingRegion box="[606,736,722,746]" country="China" name="Guangdong" pageId="3" pageNumber="149">Guangdong</collectingRegion>
).
</paragraph>
