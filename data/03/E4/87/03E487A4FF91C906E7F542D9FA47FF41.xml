<emphasis bold="true" box="[198,359,1946,1972]" pageId="55" pageNumber="56">Distribution.</emphasis>
<collectingCountry box="[365,475,1946,1972]" name="Australia" pageId="55" pageNumber="56">Australia</collectingCountry>
, 
<collectingCountry box="[488,606,1946,1972]" name="Indonesia" pageId="55" pageNumber="56">Indonesia</collectingCountry>
