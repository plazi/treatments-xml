<number box="[1289,1304,1312,1338]" pageId="7" pageNumber="50" value="2.0">2</number>
; trochanter III d; femur III ad 
<number box="[363,373,1350,1376]" pageId="7" pageNumber="50" value="1.0">1</number>
and ad 
<number box="[461,476,1350,1376]" pageId="7" pageNumber="50" value="2.0">2</number>
; trochanter IV d; femur IV ad 
<number box="[832,843,1350,1376]" pageId="7" pageNumber="50" value="1.0">1</number>
, ad 
<number box="[886,901,1350,1376]" pageId="7" pageNumber="50" value="2.0">2</number>
