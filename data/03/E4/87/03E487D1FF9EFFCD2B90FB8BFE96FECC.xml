<typeStatus box="[285,443,151,176]" pageId="6" pageNumber="277">
<emphasis bold="true" box="[285,443,151,176]" pageId="6" pageNumber="277">HOLOTYPE</emphasis>
</typeStatus>
♂ (BPBM 
<number box="[581,660,151,176]" pageId="6" pageNumber="277" value="16656.0">16,656</number>
) and 
<number box="[727,741,152,176]" pageId="6" pageNumber="277" value="2.0">2</number>
♀ 
<typeStatus box="[781,945,151,176]" pageId="6" pageNumber="277">
<emphasis bold="true" box="[781,945,151,176]" pageId="6" pageNumber="277">PARATYPES</emphasis>
</typeStatus>
from 
<collectingCountry box="[1019,1296,152,176]" name="French Polynesia" pageId="6" pageNumber="277">FRENCH POLYNESIA</collectingCountry>
