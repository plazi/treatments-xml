and 
<taxonomicName authority="Kaplin, 1999" authorityName="Kaplin" authorityYear="1999" box="[381,697,222,245]" class="Insecta" family="Machilidae" genus="Allopsontus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeognatha" pageId="7" pageNumber="442" phylum="Arthropoda" rank="species" species="smelyanskii">
