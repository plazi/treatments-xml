<paragraph blockId="52.[151,1437,566,2031]" lastBlockId="55.[151,1436,151,464]" lastPageId="55" lastPageNumber="556" pageId="52" pageNumber="553">
<emphasis bold="true" box="[199,295,1934,1959]" pageId="52" pageNumber="553">Female:</emphasis>
<typeStatus box="[305,421,1935,1959]" pageId="52" pageNumber="553">Lectotype</typeStatus>
