<taxonomicName authorityName="JAQUELIN DU VAL" authorityYear="1850" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="149" phylum="Arthropoda" rank="species" species="guynemeri">
