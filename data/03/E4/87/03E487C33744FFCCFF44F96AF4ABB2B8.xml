</taxonomicName>
(
<taxonomicName box="[492,682,260,285]" class="Magnoliopsida" family="Chenopodiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="14" pageNumber="495" phylum="Tracheophyta" rank="family">CHenopodiaCeae</taxonomicName>
