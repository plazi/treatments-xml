</date>
; 
<accessionNumber box="[631,775,1043,1069]" httpUri="http://www.uniprot.org/uniprot/ZMB85340" pageId="6" pageNumber="7">
<collectionCode box="[631,695,1043,1069]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="6" pageNumber="7">ZMB</collectionCode>
