<number box="[386,438,296,320]" pageId="22" pageNumber="523" value="3.28">3.28</number>
, L 
<number box="[470,480,296,320]" pageId="22" pageNumber="523" value="1.0">1</number>
<number box="[487,536,295,320]" pageId="22" pageNumber="523" value="8.61">8.61</number>
