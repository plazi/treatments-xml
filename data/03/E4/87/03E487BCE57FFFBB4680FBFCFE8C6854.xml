<taxonomicName authority="Rosario &amp; Hunter" authorityName="Rosario &amp; Hunter" box="[151,738,1079,1105]" class="Arachnida" family="Laelapidae" genus="Myrmozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="53" phylum="Arthropoda" rank="species" species="rotundiscutum">
