Kirkaldy 
<number box="[336,390,990,1011]" pageId="6" pageNumber="7" value="1903.0">1903</number>
: 
<number box="[407,435,990,1011]" pageId="6" pageNumber="7" value="13.0">13</number>
</taxonomicName>
. Type-species, 
<taxonomicName authority="Stal" authorityName="Stal" box="[604,914,989,1012]" class="Malacostraca" family="Lysianassidae" genus="Pachynus" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="bimaculicollis">
