<number box="[1301,1311,227,251]" pageId="5" pageNumber="54" value="1.0">1</number>
<number box="[1323,1374,227,251]" pageId="5" pageNumber="54" value="1.09">1.09</number>
×, F 
<number box="[1423,1436,227,251]" pageId="5" pageNumber="54" value="2.0">2</number>
