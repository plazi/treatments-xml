<number box="[616,647,226,251]" pageId="5" pageNumber="54" value="12.0">12</number>
: 
<number box="[651,681,226,251]" pageId="5" pageNumber="54" value="13.0">13</number>
: 
<number box="[685,716,226,251]" pageId="5" pageNumber="54" value="12.0">12</number>
: 
<number box="[720,751,226,251]" pageId="5" pageNumber="54" value="12.0">12</number>
: 
<number box="[755,783,226,251]" pageId="5" pageNumber="54" value="11.0">11</number>
: 
<number box="[788,845,226,251]" pageId="5" pageNumber="54" value="11.5">11.5</number>
: 
<number box="[849,877,226,251]" pageId="5" pageNumber="54" value="11.0">11</number>
: 
<number box="[881,914,226,251]" pageId="5" pageNumber="54" value="25.0">25</number>
; pedicel 
<number box="[1024,1075,226,251]" pageId="5" pageNumber="54" value="1.53">1.53</number>
×, anellus 
<number box="[1198,1250,227,251]" pageId="5" pageNumber="54" value="0.42">0.42</number>
