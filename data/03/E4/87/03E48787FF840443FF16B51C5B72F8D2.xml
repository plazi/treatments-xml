×, F 
<number box="[420,433,1504,1528]" pageId="5" pageNumber="54" value="3.0">3</number>
<number box="[439,519,1504,1528]" pageId="5" pageNumber="54" value="1.115" valueMax="1.23" valueMin="1.0">1–1.23</number>
×, F 
<number box="[562,576,1504,1528]" pageId="5" pageNumber="54" value="4.0">4</number>
<number box="[584,663,1504,1528]" pageId="5" pageNumber="54" value="1.115" valueMax="1.23" valueMin="1.0">1–1.23</number>
×, F 
<number box="[708,721,1504,1529]" pageId="5" pageNumber="54" value="5.0">5</number>
<number box="[728,829,1504,1528]" pageId="5" pageNumber="54" value="1.065" valueMax="1.23" valueMin="0.9">0.9–1.23</number>
×, F 
<number box="[872,885,1504,1529]" pageId="5" pageNumber="54" value="6.0">6</number>
