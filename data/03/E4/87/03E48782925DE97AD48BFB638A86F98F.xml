<quantity box="[845,928,1506,1528]" metricMagnitude="3" metricUnit="m" metricValue="1.9" pageId="6" pageNumber="339" unit="m" value="1900.0">1900 m</quantity>
. Seven 
<typeStatus box="[1015,1059,1507,1528]" pageId="6" pageNumber="339">type</typeStatus>
