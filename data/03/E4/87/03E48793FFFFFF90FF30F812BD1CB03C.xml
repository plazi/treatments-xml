<collectingRegion box="[352,498,2069,2095]" country="Bolivia" name="Cochabamba" pageId="18" pageNumber="219">Cochabamba</collectingRegion>
and 
<collectingRegion box="[553,633,2069,2095]" country="Bolivia" name="La Paz" pageId="18" pageNumber="219">La Paz</collectingRegion>
