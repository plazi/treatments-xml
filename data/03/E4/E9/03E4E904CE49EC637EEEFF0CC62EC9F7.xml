<location LSID="urn:lsid:plazi:treatment:03E4E904CE49EC637EEEFF0CC62EC9F7:8E920EC9CE49EC637AF5FF41C3DCC8C9" box="[1164,1245,218,241]" name="Jumilla" pageId="38" pageNumber="39" stateProvince="Murcia">Jumilla</location>
