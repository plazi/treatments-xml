</taxonomicName>
and 
<taxonomicName box="[306,385,1411,1434]" class="Insecta" family="Crabronidae" genus="Arpactophilus" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="tayo">
