<subSubSection pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[151,1436,151,608]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[199,334,548,573]" pageId="21" pageNumber="22">Etymology.</emphasis>
