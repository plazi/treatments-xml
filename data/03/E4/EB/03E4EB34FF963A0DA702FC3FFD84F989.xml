<number box="[1206,1235,364,390]" pageId="20" pageNumber="55" value="38.0">38</number>
and 
<number box="[1294,1323,364,390]" pageId="20" pageNumber="55" value="48.0">48</number>
µm long with 
<number box="[435,450,404,430]" pageId="20" pageNumber="55" value="6.0">6</number>
