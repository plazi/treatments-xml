<taxonomicName authority="Kirk-Spriggs &amp; Wiegmann, 2013" authorityName="Kirk-Spriggs &amp; Wiegmann" authorityYear="2013" box="[891,1026,542,566]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="friedberg" status="sp. n.">
<emphasis box="[891,910,542,566]" italics="true" pageId="50" pageNumber="51">C</emphasis>
. 
<emphasis box="[923,1026,542,566]" italics="true" pageId="50" pageNumber="51">friedberg</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1032,1093,543,566]" pageId="50" pageNumber="51" rank="species">sp. n.</taxonomicNameLabel>
, 
<emphasis box="[1105,1124,542,566]" italics="true" pageId="50" pageNumber="51">C</emphasis>
. 
<emphasis box="[1137,1178,542,566]" italics="true" pageId="50" pageNumber="51">hay</emphasis>
