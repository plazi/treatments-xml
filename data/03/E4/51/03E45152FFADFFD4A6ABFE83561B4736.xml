. 
<emphasis box="[151,264,507,531]" italics="true" pageId="50" pageNumber="51">angolense</emphasis>
Tsacas
</taxonomicName>
, 
<taxonomicName authority="Tsacas" authorityName=": Tsacas" authorityYear="1977" box="[367,663,507,531]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="campsiphallum">
<emphasis box="[367,386,507,531]" italics="true" pageId="50" pageNumber="51">C</emphasis>
. 
<emphasis box="[404,575,507,531]" italics="true" pageId="50" pageNumber="51">campsiphallum</emphasis>
Tsacas
</taxonomicName>
, 
<taxonomicName authority="Tsacas" authorityName=": Tsacas" authorityYear="1977" box="[678,838,507,531]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="sao">
<emphasis box="[678,697,507,531]" italics="true" pageId="50" pageNumber="51">C</emphasis>
. 
<emphasis box="[715,754,507,530]" italics="true" pageId="50" pageNumber="51">sao</emphasis>
Tsacas
</taxonomicName>
and 
<taxonomicName authority="Curran" authorityName="Curran, L. Tsacas" authorityYear="1933" box="[901,1209,506,531]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="quinquevittatum">
