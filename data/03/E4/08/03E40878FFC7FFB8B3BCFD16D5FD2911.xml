<paragraph blockId="27.[189,399,919,945]" box="[189,399,919,945]" pageId="27" pageNumber="28">
<heading bold="true" box="[189,399,919,945]" fontSize="11" level="3" pageId="27" pageNumber="28" reason="3">
<emphasis bold="true" box="[189,399,919,945]" pageId="27" pageNumber="28">Taxonomic notes</emphasis>
