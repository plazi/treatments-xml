<taxonomicName authority="Grube, 1870" authorityName="Grube" authorityYear="1870" box="[151,641,764,790]" class="Polychaeta" family="Sabellidae" genus="Branchiomma" kingdom="Animalia" order="Sabellida" pageId="11" pageNumber="510" phylum="Annelida" rank="species" species="luctuosum">
<emphasis bold="true" box="[151,641,764,790]" pageId="11" pageNumber="510">
<emphasis bold="true" box="[151,460,764,790]" italics="true" pageId="11" pageNumber="510">Branchiomma luctuosum</emphasis>
(
<bibRefCitation author="Grube" box="[475,633,764,790]" pageId="11" pageNumber="510" refString="Grube, E. (1870) Neue Arten der Gattung Sabella. Jahres - Bericht der Schlesischen Gesellschaft fur vaterlandische Kultur, Breslau, 48, 67 - 68." type="journal article" year="1870">
Grube, 
<number box="[573,633,764,790]" pageId="11" pageNumber="510" value="1870.0">1870</number>
</bibRefCitation>
)
</emphasis>
