<taxonomicName authority="Caleb &amp; Mathai, 2015" authorityName="Caleb &amp; Mathai" authorityYear="2015" class="Arachnida" family="Salticidae" genus="Langona" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="379" phylum="Arthropoda" rank="species" species="albolinea">
<emphasis box="[1414,1430,295,320]" italics="true" pageId="5" pageNumber="379">L</emphasis>
. 
<emphasis box="[151,257,331,357]" italics="true" pageId="5" pageNumber="379">albolinea</emphasis>
<bibRefCitation author="Caleb, J. T. D. &amp; Mungkung, S. &amp; Mathai, M. T." box="[264,518,330,357]" pageId="5" pageNumber="379" pagination="1 - 18" refId="ref3333" refString="Caleb, J. T. D., Mungkung, S. &amp; Mathai, M. T. (2015) Four new species of jumping spider (Araneae: Salticidae: Aelurillinae) with the description of a new genus from South India. Peckhamia, 124.1, 1 - 18." type="book chapter" year="2015">Caleb &amp; Mathai, 2015</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Heciak &amp; Proszynski, 1983" authorityName="Heciak &amp; Proszynski" authorityYear="1983" box="[529,963,331,357]" class="Arachnida" family="Salticidae" genus="Langona" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="379" phylum="Arthropoda" rank="species" species="alfensis">
<emphasis box="[529,545,331,356]" italics="true" pageId="5" pageNumber="379">L</emphasis>
. 
<emphasis box="[559,645,331,357]" italics="true" pageId="5" pageNumber="379">alfensis</emphasis>
<bibRefCitation author="Heciak, S. &amp; Proszynski, J." box="[652,963,331,357]" pageId="5" pageNumber="379" pagination="207 - 233" refId="ref3498" refString="Heciak, S. &amp; Proszynski, J. (1983) Remarks on Langona Simon (Araneae, Salticidae). Annales Zoologici, 37, 207 - 233." type="journal article" year="1983">Hęciak &amp; Prószyński, 1983</bibRefCitation>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Caleb, Mungkung &amp; Mathai" baseAuthorityYear="2015" box="[974,1074,331,357]" class="Arachnida" family="Salticidae" genus="Langona" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="379" phylum="Arthropoda" rank="species" species="davidi">
<emphasis box="[974,990,331,356]" italics="true" pageId="5" pageNumber="379">L</emphasis>
. 
<emphasis box="[1004,1074,331,357]" italics="true" pageId="5" pageNumber="379">davidi</emphasis>
</taxonomicName>
, 
<taxonomicName authority="Song, Xie, Zhu &amp; Wu, 1997" authorityName="Song, Xie, Zhu &amp; Wu" authorityYear="1997" class="Arachnida" family="Salticidae" genus="Langona" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="379" phylum="Arthropoda" rank="species" species="hongkong">
