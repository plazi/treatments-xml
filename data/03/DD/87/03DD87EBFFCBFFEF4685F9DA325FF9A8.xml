<taxonomicName authority="Kovarik et al., 2019" authorityName="Kovarik" authorityYear="2019" box="[264,768,1515,1539]" class="Arachnida" family="Buthidae" genus="Parabuthus" kingdom="Animalia" order="Scorpiones" pageId="16" pageNumber="15" phylum="Arthropoda" rank="species" species="somalilandus">
<emphasis box="[264,539,1515,1539]" italics="true" pageId="16" pageNumber="15">Parabuthus somalilandus</emphasis>
Kovařík et al., 2019
</taxonomicName>
, 
<taxonomicName authority="(Pocock, 1896)" baseAuthorityName="Pocock" baseAuthorityYear="1896" box="[113,537,1547,1571]" class="Arachnida" family="Buthidae" genus="Hottentotta" kingdom="Animalia" order="Scorpiones" pageId="16" pageNumber="15" phylum="Arthropoda" rank="species" species="polystictus">
<emphasis box="[113,359,1547,1571]" italics="true" pageId="16" pageNumber="15">Hottentotta polystictus</emphasis>
(Pocock, 1896)
</taxonomicName>
, 
<taxonomicName box="[551,718,1547,1571]" class="Arachnida" family="Scorpionidae" genus="Pandinurus" kingdom="Animalia" order="Scorpiones" pageId="16" pageNumber="15" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis box="[551,675,1547,1571]" italics="true" pageId="16" pageNumber="15">Pandinurus</emphasis>
sp.
</taxonomicName>
, and 
<taxonomicName box="[113,299,1579,1602]" class="Arachnida" family="Hemiscorpiidae" genus="Hemiscorpius" kingdom="Animalia" order="Scorpiones" pageId="16" pageNumber="15" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis box="[113,261,1579,1602]" italics="true" pageId="16" pageNumber="15">Hemiscorpius</emphasis>
sp.
</taxonomicName>
</paragraph>
