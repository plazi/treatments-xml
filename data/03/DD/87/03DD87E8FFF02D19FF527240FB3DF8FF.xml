<taxonomicName authority="Koch, 1840" authorityName="Koch" authorityYear="1840" box="[294,691,1098,1120]" class="Arachnida" family="Analgidae" genus="Dermaleichus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="glandarinus">
