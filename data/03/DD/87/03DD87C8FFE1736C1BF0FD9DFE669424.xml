<paragraph blockId="88.[837,1468,839,1362]" pageId="88" pageNumber="365">
<materialsCitation collectingDate="1913-12-16" collectorName="Cockerell E. P. R. &amp; Rev. George Brown" location="Islands Bought of Cockerell" pageId="88" pageNumber="365" specimenCount="50" specimenCount-male="1" stateProvince="York" typeStatus="syntype">
