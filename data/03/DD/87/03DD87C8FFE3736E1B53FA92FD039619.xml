of 
<taxonomicName authority="Gould, 1854" authorityName="Gould" authorityYear="1854" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="90" pageNumber="367" phylum="Chordata" rank="species" species="longipilis">
