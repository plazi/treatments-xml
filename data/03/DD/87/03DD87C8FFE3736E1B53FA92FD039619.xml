<subSubSection pageId="90" pageNumber="367" type="materials_examined">
<paragraph blockId="90.[839,1466,1610,1663]" pageId="90" pageNumber="367">
<emphasis bold="true" box="[839,944,1610,1634]" pageId="90" pageNumber="367">
<typeStatus box="[839,944,1610,1634]" pageId="90" pageNumber="367">Holotype</typeStatus>
