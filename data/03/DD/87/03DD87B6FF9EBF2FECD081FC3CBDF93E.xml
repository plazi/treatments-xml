<taxonomicName class="Insecta" family="Reduviidae" genus="Camarochilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="americanus">
