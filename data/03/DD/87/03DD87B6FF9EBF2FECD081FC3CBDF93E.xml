<taxonomicName authorityName="Weirauch &amp; Forero &amp; Schuh" authorityYear="2020" box="[1110,1212,1532,1556]" class="Insecta" family="Reduviidae" genus="Camarochilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="medius">
