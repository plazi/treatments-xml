and 
<taxonomicName authority="Rheims &amp; Alayón, 2016" authorityName="Rheims &amp; Alayón" authorityYear="2016" box="[1155,1274,1915,1938]" class="Arachnida" family="Sparassidae" genus="Neostasina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="maroon" status="sp. nov.">
