: male, 
<collectingRegion box="[736,914,1352,1373]" country="India" name="Himachal Pradesh" pageId="1" pageNumber="212">Himachal Pradesh</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:03DD87917A6B4974FDF085EBFFA1FA70:8EAB605C7A6B4974FC278179F891FAC0" box="[923,990,1352,1373]" municipality="Material" name="Banjar" pageId="1" pageNumber="212" stateProvince="Himachal Pradesh">Banjar</location>
