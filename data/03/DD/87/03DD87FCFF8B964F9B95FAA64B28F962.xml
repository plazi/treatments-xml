</bibRefCitation>
and 
<taxonomicName authority="Burkenroad, 1936" authorityName="Burkenroad" authorityYear="1936" box="[627,990,1536,1561]" class="Malacostraca" family="Solenoceridae" genus="Hymenopenaeus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="aphoticus">
