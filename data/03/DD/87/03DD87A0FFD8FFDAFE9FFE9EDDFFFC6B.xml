<collectingCountry box="[980,1045,894,917]" name="Oman" pageId="11" pageNumber="264">Oman</collectingCountry>
and 
<collectingCountry box="[1097,1152,894,917]" name="United Arab Emirates" pageId="11" pageNumber="264">UAE</collectingCountry>
