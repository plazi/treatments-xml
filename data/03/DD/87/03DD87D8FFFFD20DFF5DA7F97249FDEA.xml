<taxonomicName authority="G. W. Muller, 1900" authorityName="G. W. Muller" authorityYear="1900" box="[851,1250,452,478]" class="Ostracoda" family="Candonidae" genus="Candona" kingdom="Animalia" order="Podocopida" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="rara">
<emphasis box="[851,1018,452,478]" italics="true" pageId="10" pageNumber="11">Candona rara</emphasis>
<collectionCodeEnum box="[1026,1049,452,478]" pageId="10" pageNumber="11">
<collectionCode box="[1026,1049,452,478]" country="Switzerland" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15706" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="10" pageNumber="11" type="Herbarium">G</collectionCode>
</collectionCodeEnum>
. 
<collectionCodeEnum box="[1052,1083,452,478]" pageId="10" pageNumber="11">
<collectionCode box="[1052,1083,452,478]" country="Austria" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15588" lsid="urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="10" pageNumber="11" type="Herbarium">W</collectionCode>
</collectionCodeEnum>
