<taxonomicName authority="Mironov" authorityName="Mironov" box="[466,614,1857,1879]" pageId="61" pageNumber="62" rank="species" species="attila" status="sp. n.">
<taxonomicName box="[466,520,1857,1879]" pageId="61" pageNumber="62" rank="species" species="attila">
