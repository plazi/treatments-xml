<emphasis bold="true" box="[151,287,1300,1322]" pageId="87" pageNumber="88">FIGURE 51.</emphasis>
<taxonomicName authority="Mironov" authorityName="Mironov &amp; Literak &amp; Sychra &amp; Capek" authorityYear="2017" box="[293,665,1300,1322]" class="Arachnida" family="Proctophyllodidae" genus="Proctophyllodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="euphoniae" status="sp. n.">
<taxonomicName authority="" authorityName="" box="[293,569,1300,1322]" class="Arachnida" family="Proctophyllodidae" genus="Proctophyllodes" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Sarcoptiformes" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="euphoniae">
<emphasis box="[293,569,1300,1322]" italics="true" pageId="87" pageNumber="88">
<taxonomicName authority="" authorityName="" box="[293,458,1300,1322]" class="Arachnida" family="Proctophyllodidae" genus="Proctophyllodes" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Sarcoptiformes" pageId="87" pageNumber="88" phylum="Arthropoda" rank="genus">Proctophyllodes</taxonomicName>
