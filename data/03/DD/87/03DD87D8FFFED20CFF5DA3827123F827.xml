<collectionCode box="[407,430,1499,1525]" country="Norway" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13093" lsid="urn:lsid:biocol.org:col:13093" name="Botanical Museum - University of Oslo" pageId="11" pageNumber="12" type="Herbarium">O</collectionCode>
