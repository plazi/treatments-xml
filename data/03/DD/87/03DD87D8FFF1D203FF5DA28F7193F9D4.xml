<taxonomicName authority="(Hartmann, 1964)" authorityName="Savatenalinton &amp; Martens" authorityYear="2009" baseAuthorityName="Hartmann" baseAuthorityYear="1964" box="[243,794,1238,1265]" class="Ostracoda" family="Cyprididae" genus="Bradleytriebella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="tuberculata">
