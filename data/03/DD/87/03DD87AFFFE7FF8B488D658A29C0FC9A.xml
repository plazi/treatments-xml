<emphasis bold="true" box="[151,209,514,539]" pageId="93" pageNumber="94">
<collectionCode box="[151,209,514,539]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="93" pageNumber="94">NEW</collectionCode>
