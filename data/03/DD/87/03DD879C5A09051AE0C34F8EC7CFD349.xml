</taxonomicName>
: habitus (
<emphasis bold="true" box="[645,668,1502,1523]" pageId="10" pageNumber="295">30</emphasis>
); forebody (
<emphasis bold="true" box="[789,812,1502,1523]" pageId="10" pageNumber="295">31</emphasis>
); antenna (
<emphasis bold="true" box="[922,945,1502,1523]" pageId="10" pageNumber="295">32</emphasis>
); labium (
<emphasis bold="true" box="[1048,1071,1502,1523]" pageId="10" pageNumber="295">33</emphasis>
); male abdomen (
