<subSubSection pageId="20" pageNumber="363" type="diagnosis">
<paragraph blockId="20.[151,1436,1864,2032]" pageId="20" pageNumber="363">
<emphasis bold="true" box="[199,314,1936,1961]" pageId="20" pageNumber="363">Diagnosis</emphasis>
