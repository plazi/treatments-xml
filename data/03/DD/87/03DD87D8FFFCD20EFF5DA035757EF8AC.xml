<collectionCode box="[377,400,1681,1707]" country="Norway" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13093" lsid="urn:lsid:biocol.org:col:13093" name="Botanical Museum - University of Oslo" pageId="9" pageNumber="10" type="Herbarium">O</collectionCode>
</collectionCodeEnum>
