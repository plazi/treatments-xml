<taxonomicName authorityName="Simon. Even" authorityYear="1893" box="[581,654,827,845]" class="Arachnida" family="Oonopidae" genus="Dysderina" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="13" phylum="Arthropoda" rank="species" species="armata">
<emphasis box="[581,654,827,845]" italics="true" pageId="13" pageNumber="13">armata</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="13" pageNumber="13">
<paragraph blockId="13.[128,654,450,1758]" pageId="13" pageNumber="13">
<keyLead pageId="13" pageNumber="13">
