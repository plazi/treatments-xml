<typeStatus box="[1238,1286,1016,1040]" pageId="86" pageNumber="87">type</typeStatus>
locality, and 
<collectingCountry box="[151,284,1051,1075]" name="Ivory Coast" pageId="86" pageNumber="87">Ivory Coast</collectingCountry>
) (
