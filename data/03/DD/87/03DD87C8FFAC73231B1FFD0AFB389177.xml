<location LSID="urn:lsid:plazi:treatment:03DD87C8FFAC73231B1FFD0AFB389177:8EAB6005FFAF732319E6F953FDAA91EC" box="[468,639,1720,1744]" name="Palmer Register" pageId="22" pageNumber="299" stateProvince="National Capital">Palmer Register</location>
