<typeStatus box="[864,912,1475,1501]" pageId="11" pageNumber="200">type</typeStatus>
locality Manki in 
<collectingRegion box="[1124,1368,1474,1500]" country="Pakistan" name="Khyber Pakhtunkhwa" pageId="11" pageNumber="200">Khyber Pakhtunkhwa</collectingRegion>
