</taxonomicName>
[“Va.”], female, TL = 5.2 mm; 2. 
<taxonomicName authority="(Champion)" authorityName="Champion" class="Insecta" family="Salpingidae" genus="Elacatis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="immaculatus">
<emphasis italics="true" pageId="4" pageNumber="305">Elacatis immaculatus</emphasis>
(Champion)
</taxonomicName>
[AZ: Pinal Co.], male, TL = 4.2 mm; 3. 
<taxonomicName authority="(Champion)" authorityName="Champion" box="[815,1138,1877,1899]" class="Insecta" family="Salpingidae" genus="Elacatis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="senecionis">
<emphasis box="[815,1009,1877,1899]" italics="true" pageId="4" pageNumber="305">Elacatis senecionis</emphasis>
(Champion)
</taxonomicName>
[TX: La Salle Co.], male, TL = 5.1 mm; 4. 
<taxonomicName authority="(LeConte)" authorityName="LeConte" box="[286,582,1908,1930]" class="Insecta" family="Salpingidae" genus="Elacatis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="umbrosus">
<emphasis box="[286,472,1908,1930]" italics="true" pageId="4" pageNumber="305">Elacatis umbrosus</emphasis>
(LeConte)
</taxonomicName>
dark form [AZ: Coconino Co.], male, TL = 5.5 mm; 5. 
<taxonomicName box="[1143,1330,1908,1930]" class="Insecta" family="Salpingidae" genus="Elacatis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="umbrosus">
