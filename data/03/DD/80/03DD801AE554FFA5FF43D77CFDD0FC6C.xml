<number box="[607,624,461,487]" pageId="4" pageNumber="5" value="4.0">4</number>
, L S- 
<number box="[689,699,461,487]" pageId="4" pageNumber="5" value="1.0">1</number>
/ 
<number box="[709,725,461,487]" pageId="4" pageNumber="5" value="2.0">2</number>
<number box="[732,787,461,487]" pageId="4" pageNumber="5" value="0.94">0.94</number>
; I-Leg- 
