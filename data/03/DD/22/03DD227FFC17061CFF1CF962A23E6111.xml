<collectingCountry box="[836,884,1020,1046]" name="Iraq" pageId="7" pageNumber="61">Iraq</collectingCountry>
, 
<collectingCountry box="[895,943,1020,1046]" name="Iran" pageId="7" pageNumber="61">Iran</collectingCountry>
