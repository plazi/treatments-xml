<collectionCode LSID="urn:lsid:biocol.org:col:15083" box="[1241,1301,509,533]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15083" name="CRAI - Universitat de Barcelona" pageId="17" pageNumber="53">BCN</collectionCode>
