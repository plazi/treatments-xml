<taxonomicName authorityName="Luo &amp; Liu &amp; Feng" authorityYear="2019" box="[501,652,1095,1119]" class="Insecta" family="Cixiidae" genus="Indolipa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="192" phylum="Arthropoda" rank="species" species="fopingensis">
