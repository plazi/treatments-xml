<paragraph blockId="12.[151,1436,1660,2009]" pageId="12" pageNumber="51">
<emphasis bold="true" box="[151,333,1660,1684]" pageId="12" pageNumber="51">Measurements.</emphasis>
Body length: 
<number box="[500,584,1660,1684]" pageId="12" pageNumber="51" value="2.1500000000000004" valueMax="3.2" valueMin="1.1">1.1–3.2</number>
(
<number box="[602,639,1660,1684]" pageId="12" pageNumber="51" value="2.1">2.1</number>
) mm, maximum width: 
<number box="[921,1019,1660,1684]" pageId="12" pageNumber="51" value="0.865" valueMax="1.2" valueMin="0.53">0.53–1.2</number>
(
<number box="[1037,1074,1660,1684]" pageId="12" pageNumber="51" value="0.7">0.7</number>
) mm (n= 
<number box="[1184,1213,1660,1684]" pageId="12" pageNumber="51" value="48.0">48</number>
) ex gall 
<taxonomicName authority="Schlechtendal" authorityName="Schlechtendal" class="Insecta" family="Cynipidae" genus="Diplolepis" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="51" phylum="Arthropoda" rank="species" species="mayri">
<emphasis italics="true" pageId="12" pageNumber="51">Diplolepis mayri</emphasis>
(Schlechtendal)
</taxonomicName>
; body length: 
<number box="[587,685,1695,1719]" pageId="12" pageNumber="51" value="2.025" valueMax="3.3" valueMin="0.75">0.75–3.3</number>
(
<number box="[707,744,1695,1719]" pageId="12" pageNumber="51" value="1.8">1.8</number>
) mm, maximum width: 
<number box="[1042,1126,1695,1719]" pageId="12" pageNumber="51" value="0.7" valueMax="0.8" valueMin="0.6">0.6–0.8</number>
(
<number box="[1148,1200,1695,1719]" pageId="12" pageNumber="51" value="0.65">0.65</number>
) mm (n= 
<number box="[1319,1332,1695,1719]" pageId="12" pageNumber="51" value="3.0">3</number>
) ex gall 
<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[151,468,1732,1756]" class="Insecta" family="Cynipidae" genus="Diplolepis" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="51" phylum="Arthropoda" rank="species" species="rosae">
<emphasis box="[151,336,1732,1756]" italics="true" pageId="12" pageNumber="51">Diplolepis rosae</emphasis>
(Linnaeus)
</taxonomicName>
; body length: 
<number box="[627,726,1732,1756]" pageId="12" pageNumber="51" value="1.475" valueMax="1.55" valueMin="1.4">1.4–1.55</number>
(
<number box="[741,792,1732,1756]" pageId="12" pageNumber="51" value="1.45">1.45</number>
) mm, maximum width: 
<quantity box="[1065,1152,1732,1756]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="12" pageNumber="51" unit="mm" value="0.6">0.6 mm</quantity>
ex gall 
<taxonomicName authority="Giraud" authorityName="Giraud" class="Insecta" family="Cynipidae" genus="Diplolepis" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="51" phylum="Arthropoda" rank="species" species="spinosissimae">
<emphasis italics="true" pageId="12" pageNumber="51">Diplolepis spinosissimae</emphasis>
(Giraud)
</taxonomicName>
(n= 
<number box="[398,413,1767,1791]" pageId="12" pageNumber="51" value="2.0">2</number>
