<taxonomicName authorityName="Turner" authorityYear="1902" box="[612,699,1702,1727]" class="Insecta" family="Noctuidae" genus="Ecpatia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="576" phylum="Arthropoda" rank="genus">
