<quantity box="[1201,1334,1664,1691]" metricMagnitude="2" metricUnit="m" metricValue="2.95" metricValueMax="3.5" metricValueMin="2.4" pageId="2" pageNumber="576" unit="m" value="295.0" valueMax="350.0" valueMin="240.0">240–350 m</quantity>
