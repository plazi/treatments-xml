<collectingDate box="[1034,1071,399,420]" pageId="2" pageNumber="153" value="1940-10">
<date box="[1034,1071,399,420]" pageId="2" pageNumber="153" value="1940-10">40x</date>
</collectingDate>
magnification. 
<location LSID="urn:lsid:plazi:treatment:03DD5963FFF7FFDEFF7AFF7FFA60EEF5:8EABBEAEFFF7FFDEFB2BFE68FACAECC6" box="[1222,1334,399,420]" country="Madagascar" municipality="Africa" name="Continental" pageId="2" pageNumber="153">Continental</location>
sub-Saharan 
<collectingMunicipality box="[261,325,429,450]" pageId="2" pageNumber="153">Africa</collectingMunicipality>
, 
<collectingCountry box="[335,450,429,450]" name="Madagascar" pageId="2" pageNumber="153">Madagascar</collectingCountry>
................................................................... 
<taxonomicName box="[1260,1373,428,450]" class="Insecta" family="Staphylinidae" genus="Acylophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="153" phylum="Arthropoda" rank="species" species="densipennis">
<emphasis box="[1260,1373,428,450]" italics="true" pageId="2" pageNumber="153">densipennis</emphasis>
</taxonomicName>
group
</keyLead>
</materialsCitation>
</paragraph>
