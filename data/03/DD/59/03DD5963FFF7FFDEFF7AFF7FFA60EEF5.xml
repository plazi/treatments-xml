<materialsCitation ID-GBIF-Occurrence="1913892195" collectingDate="1940-10" country="Madagascar" location="Continental" municipality="Africa" pageId="2" pageNumber="153" specimenCount="1">
<keyLead pageId="2" pageNumber="153">
