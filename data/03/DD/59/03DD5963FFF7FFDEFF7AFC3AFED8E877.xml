group (
<bibRefCitation box="[551,674,989,1015]" pageId="2" pageNumber="153" refId="ref17680" refString="Lott, D. A. (2010) The species of Acylophorus Nordmann (Coleoptera: Staphylinidae: Staphylininae) in continental sub- Saharan Africa. Zootaxa, 2401, 1 - 51." type="journal article">Lott 2010</bibRefCitation>
