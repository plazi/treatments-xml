<caption httpUri="https://zenodo.org/record/1444844/files/figure.png" pageId="4" pageNumber="380" startId="4.[151,250,1329,1351]" targetBox="[194,1396,258,1279]" targetPageId="4">
<paragraph blockId="4.[151,1436,1329,1412]" pageId="4" pageNumber="380">
