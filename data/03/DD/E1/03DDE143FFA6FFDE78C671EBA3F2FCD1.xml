<taxonomicName authority="Calman, 1925" authorityName="Calman" authorityYear="1925" box="[288,649,1329,1351]" class="Malacostraca" family="Benthesicymidae" genus="Gennadas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="380" phylum="Arthropoda" rank="species" species="gilchristi">
