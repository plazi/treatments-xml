x 
<number box="[741,796,1617,1641]" pageId="2" pageNumber="575" value="9.0" valueMax="11.0" valueMin="7.0">7–11</number>
</date>
µm) and small (
<number box="[985,1084,1616,1641]" pageId="2" pageNumber="575" value="112.5" valueMax="125.0" valueMin="100.0">100–125</number>
