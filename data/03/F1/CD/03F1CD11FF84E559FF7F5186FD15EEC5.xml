<collectingCountry box="[1064,1116,332,357]" name="Italy" pageId="4" pageNumber="151">Italy</collectingCountry>
, 
<collectingCountry box="[1132,1213,332,357]" name="Poland" pageId="4" pageNumber="151">Poland</collectingCountry>
