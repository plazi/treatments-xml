<collectingCountry box="[1229,1314,332,357]" name="Austria" pageId="4" pageNumber="151">Austria</collectingCountry>
, 
<collectingCountry box="[1331,1432,332,357]" name="Slovenia" pageId="4" pageNumber="151">Slovenia</collectingCountry>
, 
<collectingCountry box="[151,236,367,392]" name="Croatia" pageId="4" pageNumber="151">Croatia</collectingCountry>
, 
<collectingCountry box="[247,327,367,392]" name="Bosnia and Herzegovina" pageId="4" pageNumber="151">Bosnia</collectingCountry>
