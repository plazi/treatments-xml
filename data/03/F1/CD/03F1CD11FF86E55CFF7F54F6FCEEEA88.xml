<taxonomicName authority="" authorityName="" box="[151,277,1404,1428]" class="Clitellata" family="Lumbricidae" genus="Serbiona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="1" pageNumber="148" phylum="Annelida" rank="species" species="kosowensis">kosowensis</taxonomicName>
