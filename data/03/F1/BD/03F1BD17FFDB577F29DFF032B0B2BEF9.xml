<subSubSection pageId="4" pageNumber="183" type="etymology">
<paragraph blockId="4.[151,1436,151,501]" pageId="4" pageNumber="183">
<emphasis bold="true" box="[199,326,439,464]" pageId="4" pageNumber="183">Etymology</emphasis>
. 
<taxonomicName authorityName="Sohn &amp; Park" authorityYear="2018" box="[342,506,440,464]" class="Insecta" family="Cosmopterigidae" genus="Gisilia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="183" phylum="Arthropoda" rank="species" species="tamrae">
