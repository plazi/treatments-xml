material. 
<typeStatus box="[377,486,1988,2012]" pageId="14" pageNumber="15">Holotype</typeStatus>
</emphasis>
female (
<collectionCode box="[584,657,1988,2012]" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="14" pageNumber="15">INBio</collectionCode>
) labelled “ 
<collectingCountry box="[777,904,1988,2012]" name="Costa Rica" pageId="14" pageNumber="15">Costa Rica</collectingCountry>
, Guanacaste, Parque Nacional Diriá, Retallano, alred. Torre Control de Incendio, 
<quantity box="[543,676,151,175]" metricMagnitude="2" metricUnit="m" metricValue="6.5" metricValueMax="7.0" metricValueMin="6.0" pageId="15" pageNumber="16" unit="m" value="650.0" valueMax="700.0" valueMin="600.0">600–700 m</quantity>
, 
