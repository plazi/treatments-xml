<location LSID="urn:lsid:plazi:treatment:03F15C39D6040612FD96FCE3C186FCEA:8E87BBF4D6010613FBBFFD27C1EDFD06" box="[1100,1217,708,734]" name="Torsotope" pageId="36" pageNumber="37" stateProvince="Tete">Torsotope</location>
