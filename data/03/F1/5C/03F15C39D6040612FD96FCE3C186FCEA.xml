<paragraph blockId="37.[189,1398,602,666]" pageId="37" pageNumber="38">
SIZE. Length 
<quantity box="[337,420,603,629]" metricMagnitude="-2" metricUnit="m" metricValue="3.6" pageId="37" pageNumber="38" unit="mm" value="36.0">36 mm</quantity>
, diameter 
<quantity box="[536,626,603,629]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="37" pageNumber="38" unit="mm" value="2.2">2.2 mm</quantity>
