<quantity box="[342,471,1830,1857]" metricMagnitude="-2" metricUnit="m" metricValue="5.65" metricValueMax="5.7" metricValueMin="5.6" pageId="83" pageNumber="84" unit="mm" value="56.5" valueMax="57.0" valueMin="56.0">56–57 mm</quantity>
