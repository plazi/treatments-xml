<quantity box="[1131,1190,332,357]" metricMagnitude="-1" metricUnit="m" metricValue="6.096" pageId="6" pageNumber="557" unit="in" value="24.0">24 in</quantity>
<taxonomicName baseAuthorityName="Jensen" baseAuthorityYear="1985" box="[1197,1378,332,356]" class="Chromadorea" family="Desmodoridae" genus="Bolbonema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="6" pageNumber="557" phylum="Nematoda" rank="species" species="longisetosum">
