<paragraph blockId="7.[151,847,1501,1527]" box="[151,847,1501,1527]" pageId="7" pageNumber="372">
<heading bold="true" box="[151,847,1501,1527]" fontSize="11" level="1" pageId="7" pageNumber="372" reason="1">
