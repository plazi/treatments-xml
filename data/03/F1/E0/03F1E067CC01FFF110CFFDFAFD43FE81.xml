<paragraph blockId="8.[190,1440,258,346]" pageId="8" pageNumber="7">
<emphasis bold="true" box="[192,324,258,282]" pageId="8" pageNumber="7">Remarks.</emphasis>
<taxonomicName authorityName=", Rifkind" authorityYear="2017" box="[329,589,258,282]" class="Insecta" family="Melyridae" genus="Quadrophenia" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="mooni">
