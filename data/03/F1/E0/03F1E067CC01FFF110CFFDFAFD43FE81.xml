<taxonomicName authority="Chevrolat" authorityName="Chevrolat" box="[484,744,290,314]" class="Insecta" family="Chrysomelidae" genus="Diabrotica" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="genus">
