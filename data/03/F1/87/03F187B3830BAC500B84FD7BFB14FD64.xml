<taxonomicName box="[255,415,1985,2006]" class="Insecta" family="Chrysomelidae" genus="Hoplasoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="422" phylum="Arthropoda" rank="species" species="acuminatum">
<emphasis box="[255,415,1985,2006]" italics="true" pageId="3" pageNumber="422">H. acuminatum</emphasis>
</taxonomicName>
(female, holotype, 8.3 mm); 4— 
<taxonomicName box="[773,934,1985,2006]" class="Insecta" family="Chrysomelidae" genus="Hoplasoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="422" phylum="Arthropoda" rank="species" species="acuminatum">
