<quantity box="[1097,1262,745,768]" metricMagnitude="-3" metricUnit="m" metricValue="1.615" metricValueMax="1.71" metricValueMin="1.52" pageId="10" pageNumber="317" unit="mm" value="1.615" valueMax="1.71" valueMin="1.52">1.52–1.71 mm</quantity>
