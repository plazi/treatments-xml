<quantity box="[897,942,1674,1699]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="25" pageNumber="274" unit="in" value="2.0">2 in</quantity>
<taxonomicName box="[948,1108,1675,1699]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="274" phylum="Arthropoda" rank="species" species="longuloides">
