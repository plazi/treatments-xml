<taxonomicName authority="HagenS 1882" authorityName="Hagens" authorityYear="1882" box="[1148,1432,1315,1339]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="280" phylum="Arthropoda" rank="species" species="longulus">
