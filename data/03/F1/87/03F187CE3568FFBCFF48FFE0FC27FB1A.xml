<taxonomicName authorityName="Bluthgen" authorityYear="1924" box="[440,560,1112,1136]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="258" phylum="Arthropoda" rank="species" species="dusmeti">
