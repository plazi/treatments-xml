<taxonomicName authorityName="Proshchalykin" authorityYear="2017" baseAuthorityName="Warncke" baseAuthorityYear="1992" box="[1260,1408,1076,1100]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="258" phylum="Arthropoda" rank="species" species="nurekensis">
