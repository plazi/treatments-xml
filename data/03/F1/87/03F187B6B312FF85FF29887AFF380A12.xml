<taxonomicName authorityName="Benham" authorityYear="1892" class="Clitellata" family="Glossoscolecidae" genus="Rhinodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="4" pageNumber="452" phylum="Annelida" rank="species" species="ecuadoriensis" subGenus="Maipure">
<emphasis italics="true" pageId="4" pageNumber="452">M. (Mp.) ecuadoriensis</emphasis>
</taxonomicName>
, and 
<emphasis box="[384,635,1110,1134]" italics="true" pageId="4" pageNumber="452">
<taxonomicName baseAuthorityName="Michaelsen" baseAuthorityYear="1900" box="[384,631,1110,1134]" class="Clitellata" family="Glossoscolecidae" genus="Martiodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="4" pageNumber="452" phylum="Annelida" rank="species" species="savanicola" subGenus="Maipure">M. (Mp.) savanicola</taxonomicName>
,
</emphasis>
in 
<collectingCountry box="[688,798,1110,1135]" name="Colombia" pageId="4" pageNumber="452">Colombia</collectingCountry>
