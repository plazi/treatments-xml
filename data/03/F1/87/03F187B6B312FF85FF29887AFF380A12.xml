<taxonomicName baseAuthorityName="Cognetti" baseAuthorityYear="1904" box="[1032,1257,1111,1135]" class="Clitellata" family="Glossoscolecidae" genus="Martiodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="4" pageNumber="452" phylum="Annelida" rank="species" species="micrurus" subGenus="Maipure">
