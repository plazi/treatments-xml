, 10, 12, 14, 16–20, 
<typeStatus box="[937,1028,1933,1957]" pageId="5" pageNumber="376">holotype</typeStatus>
, 
<specimenCount box="[1040,1060,1933,1956]" pageId="5" pageNumber="376" type="female">♀</specimenCount>
; 11, 13, 15, 21, 
