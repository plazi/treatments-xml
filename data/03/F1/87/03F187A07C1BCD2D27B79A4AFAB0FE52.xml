and 
<collectorName box="[1205,1373,1789,1814]" pageId="29" pageNumber="130">K.A. Williams</collectorName>
(
<specimenCount box="[1395,1430,1790,1814]" pageId="29" pageNumber="130" type="female">2♀</specimenCount>
, 
<collectionCode box="[151,224,1825,1849]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33156" lsid="urn:lsid:biocol.org:col:33156" name="California State Collection of Arthropods" pageId="29" pageNumber="130" type="Museum">CSCA</collectionCode>
<collectionCode box="[231,312,1825,1849]" pageId="29" pageNumber="130">EMUS</collectionCode>
, 
<figureCitation box="[323,394,1824,1849]" captionStart="FIGURES 1–4" captionStartId="7.[151,264,1980,2003]" captionTargetBox="[166,1419,189,1961]" captionTargetId="figure@7.[166,1419,189,1961]" captionTargetPageId="7" captionText="FIGURES 1–4. Lomachaeta females; dorsal habitus. 1) L. argenta; 2) L. vacamuerta, LaSalle County, Texas; 3) L. vacamuerta, Kennedy County, Texas; 4) L. osita, sp. nov. Scale bar = 1 mm." httpUri="https://zenodo.org/record/2588772/files/figure.png" pageId="29" pageNumber="130">Figs 2</figureCitation>
