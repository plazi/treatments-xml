<geoCoordinate direction="south" orientation="latitude" pageId="2" pageNumber="411" precision="925" value="-1.5166667">
<emphasis box="[1408,1436,476,500]" italics="true" pageId="2" pageNumber="411">S1</emphasis>
31
</geoCoordinate>
(30–32), 
<geoCoordinate box="[292,357,512,536]" direction="south" orientation="latitude" pageId="2" pageNumber="411" precision="925" value="-2.5166667">
<emphasis box="[292,320,512,536]" italics="true" pageId="2" pageNumber="411">S2</emphasis>
31
</geoCoordinate>
(30–32), 
<geoCoordinate box="[470,535,512,536]" direction="south" orientation="latitude" pageId="2" pageNumber="411" precision="925" value="-3.5166667">
<emphasis box="[470,498,512,536]" italics="true" pageId="2" pageNumber="411">S3</emphasis>
31
</geoCoordinate>
(30–33), 
<geoCoordinate box="[649,713,512,536]" direction="south" orientation="latitude" pageId="2" pageNumber="411" precision="925" value="-4.5">
<emphasis box="[649,677,512,536]" italics="true" pageId="2" pageNumber="411">S4</emphasis>
30
</geoCoordinate>
(30–32), 
<geoCoordinate box="[827,891,512,536]" direction="south" orientation="latitude" pageId="2" pageNumber="411" precision="925" value="-5.45">
<emphasis box="[827,855,512,535]" italics="true" pageId="2" pageNumber="411">S5</emphasis>
27
</geoCoordinate>
(25–30), 
<emphasis box="[1005,1037,512,536]" italics="true" pageId="2" pageNumber="411">R1</emphasis>
24 (21–25), 
<emphasis box="[1187,1218,512,536]" italics="true" pageId="2" pageNumber="411">R2</emphasis>
24 (23–25), 
