<subSubSection box="[385,618,1554,1582]" pageId="0" pageNumber="507" type="nomenclature">
<paragraph blockId="0.[385,618,1554,1582]" box="[385,618,1554,1582]" pageId="0" pageNumber="507">
<heading bold="true" box="[385,618,1554,1582]" centered="true" fontSize="11" level="1" pageId="0" pageNumber="507" reason="0">
<emphasis bold="true" box="[385,618,1554,1582]" pageId="0" pageNumber="507">
<taxonomicName authority="Grazia &amp; Frey-da-Silva, 2003" authorityName="Grazia &amp; Frey-da-Silva" authorityYear="2003" box="[385,487,1554,1582]" class="Insecta" family="Pentatomidae" genus="Rideriana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="507" phylum="Arthropoda" rank="genus">
