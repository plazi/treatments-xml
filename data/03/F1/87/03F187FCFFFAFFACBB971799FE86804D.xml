<geoCoordinate box="[624,688,1778,1802]" direction="south" orientation="latitude" pageId="5" pageNumber="414" precision="925" value="-5.233333">
<emphasis box="[624,652,1779,1802]" italics="true" pageId="5" pageNumber="414">S5</emphasis>
14
</geoCoordinate>
(13–16), 
<emphasis box="[797,829,1778,1802]" italics="true" pageId="5" pageNumber="414">R1</emphasis>
9 (8–10), 
<emphasis box="[945,976,1778,1802]" italics="true" pageId="5" pageNumber="414">R2</emphasis>
10 (9–10), 
<emphasis box="[1107,1137,1778,1802]" italics="true" pageId="5" pageNumber="414">R3</emphasis>
