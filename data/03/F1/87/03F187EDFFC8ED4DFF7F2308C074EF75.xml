<collectingCountry box="[1164,1269,732,756]" name="Germany" pageId="5" pageNumber="546">Germany</collectingCountry>
, 
<collectingCountry box="[1280,1340,732,757]" name="Italy" pageId="5" pageNumber="546">Italia</collectingCountry>
, 
<collectingCountry box="[1351,1432,732,757]" name="Poland" pageId="5" pageNumber="546">Poland</collectingCountry>
, 
<collectingCountry box="[151,244,767,792]" name="Ukraine" pageId="5" pageNumber="546">Ukraine</collectingCountry>
, 
