<subSubSection pageId="5" pageNumber="67" type="etymology">
<paragraph blockId="5.[151,1436,1390,1450]" pageId="5" pageNumber="67">
<emphasis bold="true" box="[199,334,1390,1414]" pageId="5" pageNumber="67">Etymology.</emphasis>
