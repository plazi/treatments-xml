<paragraph blockId="9.[264,620,1114,1176]" box="[264,620,1114,1141]" pageId="9" pageNumber="10">
<heading bold="true" box="[264,620,1114,1141]" fontSize="11" level="1" pageId="9" pageNumber="10" reason="1">
<emphasis bold="true" box="[264,620,1114,1141]" pageId="9" pageNumber="10">
<taxonomicName box="[264,519,1115,1141]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="harteni" status="sp. nov.">
<emphasis bold="true" box="[264,519,1115,1141]" italics="true" pageId="9" pageNumber="10">Dohrniphora harteni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[526,620,1114,1140]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="9.[264,620,1114,1176]" box="[264,367,1150,1176]" pageId="9" pageNumber="10">
(Fig. 
<number box="[327,357,1150,1176]" pageId="9" pageNumber="10" value="12.0">12</number>
