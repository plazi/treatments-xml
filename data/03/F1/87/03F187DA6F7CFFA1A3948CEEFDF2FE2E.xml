<taxonomicName authorityName="Prashad" authorityYear="1932" box="[1203,1312,188,212]" class="Bivalvia" family="Limidae" genus="Lima" kingdom="Animalia" order="Limida" pageId="45" pageNumber="46" phylum="Mollusca" rank="species" species="tomlini">
