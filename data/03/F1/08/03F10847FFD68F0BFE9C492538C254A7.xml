<tr box="[264,1322,964,990]" gridrow="2" pageId="8" pageNumber="67" rowspan-1="1" rowspan-2="1" rowspan-3="1" rowspan-4="1" rowspan-5="1" rowspan-6="1" rowspan-7="1" rowspan-8="1" rowspan-9="1">
<th box="[264,499,964,990]" gridcol="0" gridrow="2" pageId="8" pageNumber="67">Southeast of Brazil.</th>
