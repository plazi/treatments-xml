<materialsCitation collectingDate="2004-08-01" collectorName="Q. Z. Song" country="China" elevation="1899" latitude="26.466667" location="Leigongshan National Natural Reserve" longLatPrecision="1241" longitude="108.28333" municipality="Leishan" pageId="5" pageNumber="21" specimenCount="1" specimenCount-male="1" stateProvince="Guizhou" typeStatus="holotype">
