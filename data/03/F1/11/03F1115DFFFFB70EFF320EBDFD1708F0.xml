<subSubSection pageId="6" pageNumber="22" type="etymology">
<paragraph blockId="6.[151,1436,151,758]" pageId="6" pageNumber="22">
<emphasis bold="true" box="[197,341,229,255]" pageId="6" pageNumber="22">Etymology.</emphasis>
