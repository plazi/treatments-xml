, 
<collectingCountry box="[919,989,1148,1173]" name="China" pageId="17" pageNumber="186">China</collectingCountry>
, 
<collectingCountry box="[1006,1091,1148,1173]" name="Taiwan" pageId="17" pageNumber="186">Taiwan</collectingCountry>
, 
<collectingCountry box="[1108,1206,1148,1173]" name="Vietnam" pageId="17" pageNumber="186">Vietnam</collectingCountry>
, 
<collectingCountry box="[1223,1265,1148,1173]" name="Fiji" pageId="17" pageNumber="186">Fiji</collectingCountry>
