<collectingRegion box="[1093,1210,996,1021]" country="Colombia" name="Antioquia" pageId="19" pageNumber="318">Antioquia</collectingRegion>
]) 
<date box="[1238,1352,996,1021]" pageId="19" pageNumber="318" value="1959-01-29">29-I-1959</date>
” / “C. Carmona Veza” / “ 
<typeStatus box="[360,510,1033,1057]" pageId="19" pageNumber="318">HOLOTYPE</typeStatus>
<specimenCount box="[518,539,1033,1056]" pageId="19" pageNumber="318" type="male">♂</specimenCount>
