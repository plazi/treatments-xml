<paragraph blockId="3.[151,1437,727,1616]" box="[199,1207,1412,1437]" pageId="3" pageNumber="386">
<emphasis bold="true" box="[199,373,1412,1437]" pageId="3" pageNumber="386">
<typeStatus box="[199,258,1412,1437]" pageId="3" pageNumber="386">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[380,489,1412,1437]" pageId="3" pageNumber="386">Holotype</typeStatus>
: ♂, Taizhong, 
<collectingCountry box="[651,736,1412,1437]" name="Taiwan" pageId="3" pageNumber="386">Taiwan</collectingCountry>
, 
<date box="[747,929,1412,1437]" pageId="3" pageNumber="386" value="2006-08-16">
<number box="[747,775,1412,1437]" pageId="3" pageNumber="386" value="16.0">16</number>
August 
<number box="[870,929,1412,1437]" pageId="3" pageNumber="386" value="2006.0">2006</number>
