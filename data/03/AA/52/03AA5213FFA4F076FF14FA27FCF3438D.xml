<taxonomicName baseAuthorityName="Croizet &amp; Jobert" baseAuthorityYear="1828" box="[793,1060,406,432]" class="Mammalia" family="Suidae" genus="Dasychoerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Artiodactyla" pageId="5" pageNumber="660" phylum="Chordata" rank="species" species="arvernensis">
