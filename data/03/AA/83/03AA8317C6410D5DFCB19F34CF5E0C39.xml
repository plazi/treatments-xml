(
<bibRefCitation author="SCHILDER F. A." box="[439,562,1657,1674]" pageId="10" pageNumber="755" pagination="98 - 122" refId="ref25561" refString="SCHILDER F. A. 1922. - Contributions to the knowledge of the genera Cypraea and Trivia. Proceedings of the Malacological Society of London 15: 98 - 122." type="journal article" year="1922">Schilder, 1922</bibRefCitation>
)
</taxonomicName>
, MNHN (stn DC73), apertural view (L 4.5 mm); 
<emphasis bold="true" box="[961,976,1657,1674]" pageId="10" pageNumber="755">Q</emphasis>
, same, right view; 
<emphasis bold="true" box="[1134,1147,1657,1674]" pageId="10" pageNumber="755">R</emphasis>
, same, dorsal view; 
<emphasis bold="true" box="[218,230,1680,1697]" pageId="10" pageNumber="755">S</emphasis>
