<emphasis bold="true" box="[964,978,1635,1652]" pageId="10" pageNumber="755">N</emphasis>
, same, right view; 
<emphasis bold="true" box="[1134,1149,1635,1652]" pageId="10" pageNumber="755">O</emphasis>
, same, apertural view; 
<emphasis bold="true" box="[242,254,1657,1674]" pageId="10" pageNumber="755">P</emphasis>
, 
<taxonomicName authority="(Schilder, 1922)" baseAuthorityName="Schilder" baseAuthorityYear="1922" box="[265,567,1657,1674]" class="Gastropoda" family="Triviidae" genus="Cleotrivia" kingdom="Animalia" order="Littorinimorpha" pageId="10" pageNumber="755" phylum="Mollusca" rank="species" species="antillarum">
