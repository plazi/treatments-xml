</taxonomicName>
<taxonomicNameLabel box="[1039,1117,1600,1621]" pageId="9" pageNumber="95" rank="species">sp. nov.</taxonomicNameLabel>
, slide No. MNHU-NK073; (D) 
<taxonomicName authority="Du &amp; Wang &amp; Li, 2013" authorityName="Du &amp; Wang &amp; Li" authorityYear="2013" box="[473,573,1629,1651]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="95" phylum="Arthropoda" rank="species" species="latiloba" status="sp. nov.">
<emphasis box="[473,573,1629,1651]" italics="true" pageId="9" pageNumber="95">P. latiloba</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[578,656,1630,1651]" pageId="9" pageNumber="95" rank="species">sp. nov.</taxonomicNameLabel>
, slide No. MNHU-NK065; (E) 
<taxonomicName authorityName="Du &amp; Wang &amp; Li" authorityYear="2013" box="[974,1073,1629,1651]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="95" phylum="Arthropoda" rank="species" species="latiloba">
