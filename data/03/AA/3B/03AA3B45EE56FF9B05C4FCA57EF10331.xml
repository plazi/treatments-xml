<subSubSection lastPageId="12" lastPageNumber="43" pageId="9" pageNumber="40" type="etymology">
<paragraph blockId="9.[151,1437,920,1952]" pageId="9" pageNumber="40">
<emphasis bold="true" box="[198,339,1887,1913]" pageId="9" pageNumber="40">Etymology.</emphasis>
