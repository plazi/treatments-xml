<number box="[1144,1159,960,986]" pageId="9" pageNumber="40" value="3.0">3</number>
(
<number box="[1177,1226,960,986]" pageId="9" pageNumber="40" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual seta (
<emphasis box="[306,321,999,1024]" italics="true" pageId="9" pageNumber="40">d</emphasis>
