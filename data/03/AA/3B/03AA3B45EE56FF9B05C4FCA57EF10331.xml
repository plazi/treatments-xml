<number box="[408,438,960,986]" pageId="9" pageNumber="40" value="50.0">50</number>
(
<number box="[455,536,960,986]" pageId="9" pageNumber="40" value="50.0" valueMax="52.0" valueMin="48.0">48–52</number>
