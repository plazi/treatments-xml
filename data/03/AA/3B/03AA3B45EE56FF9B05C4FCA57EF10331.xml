<number box="[340,369,998,1024]" pageId="9" pageNumber="40" value="11.0">11</number>
(
<number box="[387,466,998,1024]" pageId="9" pageNumber="40" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
), cheliceral stylets 
<number box="[703,733,998,1024]" pageId="9" pageNumber="40" value="52.0">52</number>
(
<number box="[750,833,998,1024]" pageId="9" pageNumber="40" value="52.5" valueMax="55.0" valueMin="50.0">50–55</number>
). 
<emphasis bold="true" box="[854,1066,998,1024]" pageId="9" pageNumber="40">Prodorsal shield</emphasis>
<number box="[1075,1105,998,1024]" pageId="9" pageNumber="40" value="31.0">31</number>
(
<number box="[1122,1203,998,1024]" pageId="9" pageNumber="40" value="30.5" valueMax="31.0" valueMin="30.0">30–31</number>
