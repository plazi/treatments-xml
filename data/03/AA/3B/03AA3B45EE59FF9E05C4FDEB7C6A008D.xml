<emphasis bold="true" box="[740,751,959,985]" pageId="6" pageNumber="37">I</emphasis>
(
<emphasis box="[772,804,960,985]" italics="true" pageId="6" pageNumber="37">
<number box="[772,786,960,985]" pageId="6" pageNumber="37" value="1.0">1</number>
