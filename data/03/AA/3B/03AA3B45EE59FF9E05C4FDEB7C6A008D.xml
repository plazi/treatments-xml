<number box="[743,788,728,754]" pageId="6" pageNumber="37" value="345.0">345</number>
(
<number box="[805,918,728,754]" pageId="6" pageNumber="37" value="345.0" valueMax="350.0" valueMin="340.0">340–350</number>
