<number box="[940,970,728,754]" pageId="6" pageNumber="37" value="80.0">80</number>
(
<number box="[987,1066,728,754]" pageId="6" pageNumber="37" value="82.5" valueMax="85.0" valueMin="80.0">80–85</number>
