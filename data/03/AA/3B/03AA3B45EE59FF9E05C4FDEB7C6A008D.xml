<number box="[151,180,921,947]" pageId="6" pageNumber="37" value="35.0">35</number>
(
<number box="[196,272,921,947]" pageId="6" pageNumber="37" value="36.0" valueMax="37.0" valueMin="35.0">35–37</number>
) projecting forward. 
<emphasis bold="true" box="[520,673,920,946]" pageId="6" pageNumber="37">Coxal plates</emphasis>
smooth; anterolateral setae on coxisternum 
<emphasis bold="true" box="[1189,1201,920,946]" pageId="6" pageNumber="37">I</emphasis>
(
<emphasis box="[1217,1246,921,946]" italics="true" pageId="6" pageNumber="37">
<number box="[1217,1230,921,946]" pageId="6" pageNumber="37" value="1.0">1</number>
b
