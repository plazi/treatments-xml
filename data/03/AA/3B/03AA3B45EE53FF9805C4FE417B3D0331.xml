<number box="[1072,1101,728,754]" pageId="12" pageNumber="43" value="10.0">10</number>
(
<number box="[1119,1181,728,754]" pageId="12" pageNumber="43" value="9.0" valueMax="13.0" valueMin="5.0">5–13</number>
