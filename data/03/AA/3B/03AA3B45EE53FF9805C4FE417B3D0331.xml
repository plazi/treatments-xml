<number box="[940,970,573,599]" pageId="12" pageNumber="43" value="71.0">71</number>
(
<number box="[987,1066,573,599]" pageId="12" pageNumber="43" value="70.0" valueMax="72.0" valueMin="68.0">68–72</number>
