<number box="[408,438,612,638]" pageId="12" pageNumber="43" value="70.0">70</number>
(
<number box="[456,536,612,638]" pageId="12" pageNumber="43" value="71.5" valueMax="73.0" valueMin="70.0">70–73</number>
