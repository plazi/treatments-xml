<number box="[1158,1188,573,599]" pageId="12" pageNumber="43" value="70.0">70</number>
(
<number box="[1206,1285,573,599]" pageId="12" pageNumber="43" value="70.5" valueMax="72.0" valueMin="69.0">69–72</number>
