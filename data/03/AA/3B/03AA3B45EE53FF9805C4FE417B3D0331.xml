<number box="[743,788,573,599]" pageId="12" pageNumber="43" value="288.0">288</number>
(
<number box="[805,918,573,599]" pageId="12" pageNumber="43" value="265.0" valueMax="290.0" valueMin="240.0">240–290</number>
