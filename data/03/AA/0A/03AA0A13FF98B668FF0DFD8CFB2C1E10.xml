<quantity box="[400,547,937,964]" metricMagnitude="-1" metricUnit="m" metricValue="2.4" metricValueMax="2.5" metricValueMin="2.3" pageId="14" pageNumber="415" unit="mm" value="240.0" valueMax="250.0" valueMin="230.0">230–250 µm</quantity>
