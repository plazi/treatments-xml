</taxonomicName>
, ravines with 
<taxonomicName box="[870,969,1366,1390]" class="Magnoliopsida" family="Platanaceae" genus="Platanus" kingdom="Plantae" order="Proteales" pageId="18" pageNumber="297" phylum="Tracheophyta" rank="genus">
<emphasis box="[870,969,1366,1390]" italics="true" pageId="18" pageNumber="297">Platanus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1026,1090,1367,1390]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="297" phylum="Tracheophyta" rank="genus">
<emphasis box="[1026,1090,1367,1390]" italics="true" pageId="18" pageNumber="297">Pinus</emphasis>
</taxonomicName>
, mixed forests with 
<taxonomicName box="[1324,1388,1367,1390]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="297" phylum="Tracheophyta" rank="genus">
<emphasis box="[1324,1388,1367,1390]" italics="true" pageId="18" pageNumber="297">Pinus</emphasis>
</taxonomicName>
