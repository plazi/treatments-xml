<typeStatus box="[727,777,1481,1507]" pageId="24" pageNumber="25">type</typeStatus>
series from 
<collectingCountry box="[927,990,1481,1507]" name="United States of America" pageId="24" pageNumber="25">USA</collectingCountry>
