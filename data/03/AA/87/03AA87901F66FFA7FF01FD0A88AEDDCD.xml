<emphasis bold="true" box="[151,577,654,680]" pageId="2" pageNumber="170">
<taxonomicName box="[151,434,654,680]" class="Insecta" family="Ptiloneuridae" genus="Brasineura" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="170" phylum="Arthropoda" rank="species" species="diamantina" status="n. sp.">
<emphasis bold="true" box="[151,434,654,680]" italics="true" pageId="2" pageNumber="170">Brasineura diamantina</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[441,507,654,680]" pageId="2" pageNumber="170" rank="species">n. sp.</taxonomicNameLabel>
