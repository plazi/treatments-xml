, 
<collectingCountry box="[1172,1234,1532,1556]" name="Syria" pageId="9" pageNumber="288">Syria</collectingCountry>
, southern 
<collectingCountry box="[1356,1436,1532,1556]" name="Turkey" pageId="9" pageNumber="288">Turkey</collectingCountry>
