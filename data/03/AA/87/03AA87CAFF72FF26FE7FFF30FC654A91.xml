<subSubSection pageId="153" pageNumber="152" type="description">
<paragraph blockId="153.[113,666,276,665]" box="[113,377,550,571]" pageId="153" pageNumber="152">
<emphasis bold="true" box="[113,291,550,570]" pageId="153" pageNumber="152">Species group:</emphasis>
