<quantity box="[241,400,1099,1122]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" metricValueMax="4.6" metricValueMin="3.6" pageId="28" pageNumber="773" unit="mm" value="4.1" valueMax="4.6" valueMin="3.6">3.60–4.60 mm</quantity>
