)................................................ 
<taxonomicName baseAuthorityName="Newman" baseAuthorityYear="1838" box="[1274,1434,796,818]" class="Insecta" family="Capniidae" genus="Zwicknia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="15" pageNumber="216" phylum="Arthropoda" rank="species" species="bifrons">
