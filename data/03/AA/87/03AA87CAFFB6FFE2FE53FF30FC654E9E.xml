<taxonomicName authorityName="Dana" authorityYear="1846" box="[880,999,1018,1039]" class="Anthozoa" family="Oculinidae" genus="Madrepora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="93" pageNumber="92" phylum="Cnidaria" rank="species" species="hyacinthus">
