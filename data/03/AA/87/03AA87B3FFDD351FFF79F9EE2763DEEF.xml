<paragraph blockId="27.[151,604,1590,1616]" box="[151,604,1590,1616]" pageId="27" pageNumber="328">
<heading bold="true" box="[151,604,1590,1616]" fontSize="11" level="1" pageId="27" pageNumber="328" reason="1">
<taxonomicName authority="Butler, 1896" authorityName="Butler" authorityYear="1896" box="[151,604,1590,1616]" class="Insecta" family="Hesperiidae" genus="Chondrolepis" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="328" phylum="Arthropoda" rank="species" species="telisignata">
<emphasis bold="true" box="[151,604,1590,1616]" pageId="27" pageNumber="328">
<emphasis bold="true" box="[151,446,1590,1616]" italics="true" pageId="27" pageNumber="328">Chondrolepis telisignata</emphasis>
<bibRefCitation author="Butler" box="[453,604,1590,1616]" pageId="27" pageNumber="328" refString="Butler, A. G. (1896) On a collection of butterflies obtained by Mr. Richard Crawshay in Nyasa-land, between the months of January and April 1895. Proceedings of the Zoological Society of London, 1896, 108 - 136, plate VI. Available from: http: / / www. biodiversitylibrary. org / pdf 2 / 003294700074271. pdf (accessed 9 September 2014)" type="proceedings paper" year="1896">
Butler, 
<number box="[545,604,1590,1616]" pageId="27" pageNumber="328" value="1896.0">1896</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
