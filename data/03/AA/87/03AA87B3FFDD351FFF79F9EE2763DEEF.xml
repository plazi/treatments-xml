<taxonomicName box="[923,1092,332,357]" class="Liliopsida" family="Poaceae" genus="Setaria" kingdom="Plantae" order="Poales" pageId="28" pageNumber="329" phylum="Tracheophyta" rank="species" species="sulcata">
<emphasis box="[923,1092,332,357]" italics="true" pageId="28" pageNumber="329">Setaria sulcata</emphasis>
</taxonomicName>
(= 
<taxonomicName box="[1130,1246,333,356]" class="Liliopsida" family="Poaceae" genus="Setaria" kingdom="Plantae" order="Poales" pageId="28" pageNumber="329" phylum="Tracheophyta" rank="species" species="poiretiana">
<emphasis box="[1130,1246,333,356]" italics="true" pageId="28" pageNumber="329">poiretiana</emphasis>
</taxonomicName>
) (Tigoni Woods, 
<number box="[151,180,367,392]" pageId="28" pageNumber="329" value="87.0">87</number>
/ 
<number box="[189,202,367,392]" pageId="28" pageNumber="329" value="5.0">5</number>
) and 
<taxonomicName box="[265,424,367,392]" class="Liliopsida" family="Poaceae" genus="Setaria" kingdom="Plantae" order="Poales" pageId="28" pageNumber="329" phylum="Tracheophyta" rank="species" species="megaphylla">
<emphasis box="[265,424,367,392]" italics="true" pageId="28" pageNumber="329">S. megaphylla</emphasis>
</taxonomicName>
(= 
<taxonomicName box="[463,561,367,392]" class="Liliopsida" family="Poaceae" genus="Setaria" kingdom="Plantae" order="Poales" pageId="28" pageNumber="329" phylum="Tracheophyta" rank="species" species="plicatilis">
<emphasis box="[463,561,367,392]" italics="true" pageId="28" pageNumber="329">plicatilis</emphasis>
</taxonomicName>
