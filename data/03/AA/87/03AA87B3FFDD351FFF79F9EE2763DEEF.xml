or 
<taxonomicName box="[476,655,439,464]" class="Liliopsida" family="Poaceae" genus="Ehrharta" kingdom="Plantae" order="Poales" pageId="28" pageNumber="329" phylum="Tracheophyta" rank="species" species="erecta">
<emphasis box="[476,655,439,464]" italics="true" pageId="28" pageNumber="329">Ehrharta erecta</emphasis>
</taxonomicName>
<taxonomicName box="[662,826,439,464]" pageId="28" pageNumber="329" rank="variety" variety="abyssinica">
var. 
<emphasis box="[708,826,439,464]" italics="true" pageId="28" pageNumber="329">abyssinica</emphasis>
