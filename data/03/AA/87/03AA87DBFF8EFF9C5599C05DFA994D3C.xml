<collectingCountry box="[359,426,1276,1302]" name="China" pageId="12" pageNumber="199">China</collectingCountry>
(
<collectingRegion box="[441,517,1276,1302]" country="China" name="Fujian" pageId="12" pageNumber="199">Fujian</collectingRegion>
, 
<collectingRegion box="[528,662,1276,1302]" country="China" name="Guangdong" pageId="12" pageNumber="199">Guangdong</collectingRegion>
, 
<collectingRegion box="[673,772,1276,1302]" country="China" name="Guangxi" pageId="12" pageNumber="199">Guangxi</collectingRegion>
, 
<collectingCountry box="[783,917,1277,1303]" name="Hong Kong" pageId="12" pageNumber="199">Hong Kong</collectingCountry>
) and 
<collectingCountry box="[978,1073,1276,1302]" name="Vietnam" pageId="12" pageNumber="199">Vietnam</collectingCountry>
(Ba Vi, Tay Thien) (
<figureCitation box="[1304,1388,1276,1302]" captionStart="FIGURE 13" captionStartId="21.[151,250,1650,1674]" captionTargetBox="[151,1436,633,1627]" captionTargetId="figure-250@21.[151,1436,633,1627]" captionTargetPageId="21" captionText="FIGURE 13. Distribution map of Drepanosticta carmichaeli group. D. vietnamica (yellow circle); D. annandalei (red circle); D. brownelli (white circle); D. carmichaeli (red square); D. emtrai (blue square); D. hongkongensis (pink square); D. jurzitzai (red triangle); D. wildermuthi sp. nov. (yellow triangle); D. sumatrana (pink triangle); D. tenella (red diamond)." figureDoi="http://doi.org/10.5281/zenodo.5677863" httpUri="https://zenodo.org/record/5677863/files/figure.png" pageId="12" pageNumber="199">Fig. 13</figureCitation>
).
</paragraph>
