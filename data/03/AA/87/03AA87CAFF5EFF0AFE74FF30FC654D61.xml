<taxonomicName baseAuthorityName="Dana" baseAuthorityYear="1846" box="[880,953,816,837]" class="Anthozoa" family="Acroporidae" genus="Acropora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="181" pageNumber="180" phylum="Cnidaria" rank="species" species="nasuta">
