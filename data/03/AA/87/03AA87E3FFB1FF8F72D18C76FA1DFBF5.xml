<taxonomicName box="[839,903,333,356]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
<emphasis box="[839,903,333,356]" italics="true" pageId="3" pageNumber="282">Pinus</emphasis>
</taxonomicName>
forest, near ponds with 
<taxonomicName box="[1172,1297,332,356]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
<emphasis box="[1172,1297,332,356]" italics="true" pageId="3" pageNumber="282">Eucalyptus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1351,1429,332,356]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
