<emphasis box="[980,1259,296,320]" italics="true" pageId="3" pageNumber="282">Sarcopoterium spinosum</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1274,1388,296,319]" class="Magnoliopsida" family="Fabaceae" genus="Ceratonia" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
<emphasis box="[1274,1388,296,319]" italics="true" pageId="3" pageNumber="282">Ceratonia</emphasis>
</taxonomicName>
and 
<taxonomicName box="[151,243,333,356]" class="Magnoliopsida" family="Anacardiaceae" genus="Pistacia" kingdom="Plantae" order="Sapindales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
<emphasis box="[151,243,333,356]" italics="true" pageId="3" pageNumber="282">Pistacia</emphasis>
</taxonomicName>
, mixed forests with 
<taxonomicName box="[471,535,333,356]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
<emphasis box="[471,535,333,356]" italics="true" pageId="3" pageNumber="282">Pinus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[549,643,333,356]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
<emphasis box="[549,643,333,356]" italics="true" pageId="3" pageNumber="282">Quercus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[697,749,332,356]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="3" pageNumber="282" phylum="Tracheophyta" rank="genus">
