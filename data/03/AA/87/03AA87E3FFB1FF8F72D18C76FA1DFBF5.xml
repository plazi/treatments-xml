<paragraph blockId="3.[151,1436,151,1041]" pageId="3" pageNumber="282">
<emphasis bold="true" box="[201,453,151,176]" pageId="3" pageNumber="282">General distribution.</emphasis>
Europe: 
<collectingCountry box="[555,646,151,175]" name="Albania" pageId="3" pageNumber="282">Albania</collectingCountry>
, 
<collectingCountry box="[658,936,151,175]" name="Bosnia and Herzegovina" pageId="3" pageNumber="282">Bosnia and Herzegovina</collectingCountry>
, 
<collectingCountry box="[947,1046,151,175]" name="Bulgaria" pageId="3" pageNumber="282">Bulgaria</collectingCountry>
, 
<collectingCountry box="[1058,1144,151,175]" name="Croatia" pageId="3" pageNumber="282">Croatia</collectingCountry>
, 
<collectingCountry box="[1154,1233,151,175]" name="Greece" pageId="3" pageNumber="282">Greece</collectingCountry>
(mainland, Crete, Cyclades, 
<collectingCountry box="[272,412,188,212]" name="Greece" pageId="3" pageNumber="282">Dodecanese</collectingCountry>
), 
<collectingCountry box="[435,574,189,213]" name="Serbia" pageId="3" pageNumber="282">Montenegro</collectingCountry>
, 
<collectingCountry box="[590,691,188,212]" name="Romania" pageId="3" pageNumber="282">Romania</collectingCountry>
(?), 
<collectingCountry box="[751,852,188,212]" name="Slovenia" pageId="3" pageNumber="282">Slovenia</collectingCountry>
; West Asia: 
<collectingCountry box="[1005,1089,188,212]" name="Cyprus" pageId="3" pageNumber="282">Cyprus</collectingCountry>
, Near and Middle East (
<collectingCountry box="[1381,1436,188,212]" name="Turks and Caicos Islands" pageId="3" pageNumber="282">Turk</collectingCountry>
<number box="[151,210,223,247]" pageId="3" pageNumber="282" value="1952.0">1952</number>
; Zapparoli 
<number box="[337,466,223,247]" pageId="3" pageNumber="282" value="1991.1995">1991, 1995</number>
, 
<number box="[477,536,223,247]" pageId="3" pageNumber="282" value="2002.0">2002</number>
; Kos 
<number box="[600,659,223,247]" pageId="3" pageNumber="282" value="1992.0">1992</number>
; Negrea &amp; Matic 
<number box="[860,919,223,247]" pageId="3" pageNumber="282" value="1996.0">1996</number>
; Stoev 
<number box="[1001,1060,223,247]" pageId="3" pageNumber="282" value="1997.0">1997</number>
