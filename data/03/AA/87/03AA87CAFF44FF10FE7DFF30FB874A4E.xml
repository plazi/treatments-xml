<subSubSection pageId="175" pageNumber="174" type="description">
<paragraph blockId="175.[113,666,276,622]" box="[113,377,449,470]" pageId="175" pageNumber="174">
<emphasis bold="true" box="[113,291,449,469]" pageId="175" pageNumber="174">Species group:</emphasis>
