<subSubSection pageId="185" pageNumber="184" type="description">
<paragraph blockId="185.[113,666,276,618]" box="[113,376,474,495]" pageId="185" pageNumber="184">
<emphasis bold="true" box="[113,291,474,494]" pageId="185" pageNumber="184">Species group:</emphasis>
