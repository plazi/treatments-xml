and 
<taxonomicName authority="(Fabricius) (Roepke, 1916)" authorityName="(Fabricius) (Roepke" authorityYear="1916" class="Insecta" family="Flatidae" genus="Lawana" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="candida">
