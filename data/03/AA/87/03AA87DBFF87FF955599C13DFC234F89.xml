and 
<collectingRegion box="[628,724,1929,1955]" country="China" name="Guangxi" pageId="5" pageNumber="192">Guangxi</collectingRegion>
Provinces) (
<figureCitation box="[866,950,1929,1955]" captionStart="FIGURE 13" captionStartId="21.[151,250,1650,1674]" captionTargetBox="[151,1436,633,1627]" captionTargetId="figure-250@21.[151,1436,633,1627]" captionTargetPageId="21" captionText="FIGURE 13. Distribution map of Drepanosticta carmichaeli group. D. vietnamica (yellow circle); D. annandalei (red circle); D. brownelli (white circle); D. carmichaeli (red square); D. emtrai (blue square); D. hongkongensis (pink square); D. jurzitzai (red triangle); D. wildermuthi sp. nov. (yellow triangle); D. sumatrana (pink triangle); D. tenella (red diamond)." figureDoi="http://doi.org/10.5281/zenodo.5677863" httpUri="https://zenodo.org/record/5677863/files/figure.png" pageId="5" pageNumber="192">Fig. 13</figureCitation>
).
