<number box="[623,744,754,780]" pageId="3" pageNumber="60" value="0.76" valueMax="0.77" valueMin="0.75">0.75–0.77</number>
, total length 
<number box="[900,1018,754,780]" pageId="3" pageNumber="60" value="1.2149999999999999" valueMax="1.23" valueMin="1.2">1.20–1.23</number>
(n = 
<number box="[1086,1103,754,780]" pageId="3" pageNumber="60" value="2.0">2</number>
). Female carapace length is 
<number box="[151,271,794,820]" pageId="3" pageNumber="60" value="0.76" valueMax="0.8" valueMin="0.72">0.72–0.80</number>
, total length 
<number box="[423,562,794,820]" pageId="3" pageNumber="60" value="1.225" valueMax="1.3" valueMin="1.15">1.15 to 1.30</number>
(n = 
<number box="[625,642,794,820]" pageId="3" pageNumber="60" value="2.0">2</number>
).
</paragraph>
