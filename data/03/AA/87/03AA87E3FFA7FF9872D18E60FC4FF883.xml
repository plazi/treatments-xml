<paragraph blockId="20.[151,1436,1313,1913]" pageId="20" pageNumber="299">
<emphasis bold="true" box="[201,451,1421,1446]" pageId="20" pageNumber="299">General distribution.</emphasis>
Europe: 
<collectingCountry box="[562,653,1421,1445]" name="Albania" pageId="20" pageNumber="299">Albania</collectingCountry>
, 
<collectingCountry box="[671,770,1421,1445]" name="Bulgaria" pageId="20" pageNumber="299">Bulgaria</collectingCountry>
, 
<collectingCountry box="[788,867,1421,1445]" name="Greece" pageId="20" pageNumber="299">Greece</collectingCountry>
(mainland, Crete, Cyclades, 
<collectingCountry box="[1217,1357,1421,1445]" name="Greece" pageId="20" pageNumber="299">Dodecanese</collectingCountry>
), FYR 
<collectingCountry box="[151,276,1457,1481]" name="Macedonia" pageId="20" pageNumber="299">Macedonia</collectingCountry>
; North Africa: 
<collectingCountry box="[444,513,1458,1482]" name="Egypt" pageId="20" pageNumber="299">Egypt</collectingCountry>
, 
<collectingCountry box="[525,590,1457,1481]" name="Libya" pageId="20" pageNumber="299">Libya</collectingCountry>
(Cyrenaica, Tripolitania), 
<collectingCountry box="[887,974,1457,1481]" name="Tunisia" pageId="20" pageNumber="299">Tunisia</collectingCountry>
; West Asia: Caucasus, 
<collectingCountry box="[1231,1315,1457,1481]" name="Cyprus" pageId="20" pageNumber="299">Cyprus</collectingCountry>
, 
<collectingCountry box="[1325,1373,1458,1481]" name="Iran" pageId="20" pageNumber="299">Iran</collectingCountry>
, 
<collectingCountry box="[1384,1432,1458,1481]" name="Iraq" pageId="20" pageNumber="299">Iraq</collectingCountry>
, 
<collectingCountry box="[151,216,1493,1517]" name="Israel" pageId="20" pageNumber="299">Israel</collectingCountry>
, 
<collectingCountry box="[233,310,1493,1517]" name="Jordan" pageId="20" pageNumber="299">Jordan</collectingCountry>
, 
<collectingCountry box="[328,432,1493,1517]" name="Palestine" pageId="20" pageNumber="299">Palestine</collectingCountry>
, 
<collectingCountry box="[449,605,1493,1517]" name="Saudi Arabia" pageId="20" pageNumber="299">Saudi Arabia</collectingCountry>
, 
<collectingCountry box="[622,684,1493,1517]" name="Syria" pageId="20" pageNumber="299">Syria</collectingCountry>
, 
<collectingCountry box="[702,786,1493,1517]" name="Turkey" pageId="20" pageNumber="299">Turkey</collectingCountry>
; Central Asia: Turkestan, 
<collectingCountry box="[1102,1228,1493,1517]" name="Uzbekistan" pageId="20" pageNumber="299">Uzbekistan</collectingCountry>
(Zapparoli 
<number box="[1372,1431,1493,1517]" pageId="20" pageNumber="299" value="1991.0">1991</number>
; Simaiakis 
<emphasis box="[268,317,1529,1553]" italics="true" pageId="20" pageNumber="299">et al</emphasis>
. 
<number box="[331,392,1529,1553]" pageId="20" pageNumber="299" value="2005.0">2005</number>
