<taxonomicName baseAuthorityName="Dana" baseAuthorityYear="1846" box="[880,969,588,609]" class="Anthozoa" family="Acroporidae" genus="Acropora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="67" pageNumber="66" phylum="Cnidaria" rank="species" species="echinata">
