<collectingCountry box="[1153,1279,223,248]" name="Azerbaijan" pageId="19" pageNumber="170">Azerbaijan</collectingCountry>
, 
<collectingCountry box="[1298,1432,223,248]" name="Kazakhstan" pageId="19" pageNumber="170">Kazakhstan</collectingCountry>
, 
<collectingCountry box="[151,252,260,285]" name="Armenia" pageId="19" pageNumber="170">Armenia</collectingCountry>
, 
<collectingCountry box="[265,346,260,285]" name="Turkey" pageId="19" pageNumber="170">Turkey</collectingCountry>
, 
<collectingCountry box="[360,444,260,284]" name="Cyprus" pageId="19" pageNumber="170">Cyprus</collectingCountry>
, 
<collectingCountry box="[457,550,260,285]" name="Georgia" pageId="19" pageNumber="170">Georgia</collectingCountry>
, 
<collectingCountry box="[563,611,261,284]" name="Iraq" pageId="19" pageNumber="170">Iraq</collectingCountry>
, 
<collectingCountry box="[625,690,260,285]" name="Israel" pageId="19" pageNumber="170">Israel</collectingCountry>
, 
<collectingCountry box="[704,779,260,285]" name="Russia" pageId="19" pageNumber="170">Russia</collectingCountry>
(West Siberia), 
<collectingCountry box="[967,1094,260,285]" name="Tajikistan" pageId="19" pageNumber="170">Tadjikistan</collectingCountry>
