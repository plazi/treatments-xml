<emphasis box="[446,658,1731,1753]" italics="true" pageId="7" pageNumber="226">Tetramorium pisarskii</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[663,714,1731,1753]" pageId="7" pageNumber="226" rank="species">sp. n.</taxonomicNameLabel>
, worker, holotype: 
<emphasis box="[896,908,1732,1753]" italics="true" pageId="7" pageNumber="226">a</emphasis>
<emphasis bold="true" box="[912,934,1731,1752]" pageId="7" pageNumber="226">—</emphasis>
body in profile; 
<emphasis box="[1087,1099,1731,1752]" italics="true" pageId="7" pageNumber="226">b</emphasis>
<emphasis bold="true" box="[1103,1125,1731,1752]" pageId="7" pageNumber="226">—</emphasis>
mesosoma and waist, dorsal view; 
