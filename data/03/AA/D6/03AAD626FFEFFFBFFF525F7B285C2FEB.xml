<caption httpUri="https://zenodo.org/record/306176/files/figure.png" pageId="11" pageNumber="506" targetBox="[151,1436,193,1866]" targetPageId="11">
<paragraph blockId="11.[151,1435,1887,1940]" pageId="11" pageNumber="506">
