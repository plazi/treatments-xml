<number box="[800,846,367,392]" pageId="5" pageNumber="298" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
; 
<number box="[861,875,368,392]" pageId="5" pageNumber="298" value="2.0">2</number>
