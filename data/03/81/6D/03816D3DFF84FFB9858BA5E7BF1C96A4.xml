<quantity box="[1143,1306,1820,1845]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" metricValueMax="7.5" metricValueMin="5.5" pageId="82" pageNumber="83" unit="mm" value="0.65" valueMax="0.75" valueMin="0.55">0.55–0.75 mm</quantity>
