<quantity box="[871,1035,1820,1845]" metricMagnitude="-3" metricUnit="m" metricValue="1.49" metricValueMax="1.56" metricValueMin="1.42" pageId="82" pageNumber="83" unit="mm" value="1.49" valueMax="1.56" valueMin="1.42">1.42–1.56 mm</quantity>
