<emphasis bold="true" box="[151,1125,1806,1832]" pageId="4" pageNumber="5">
<taxonomicName authority="Mahmood, Quicke &amp; Papp" authorityName="Mahmood, Quicke &amp; Papp" box="[151,759,1806,1832]" class="Insecta" family="Braconidae" genus="Doggerella" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="cohaerens" status="sp. nov.">
<emphasis bold="true" box="[151,416,1806,1832]" italics="true" pageId="4" pageNumber="5">Doggerella cohaerens</emphasis>
Mahmood, Quicke &amp; Papp
</taxonomicName>
<taxonomicNameLabel box="[766,860,1806,1832]" pageId="4" pageNumber="5" rank="species">sp. nov.</taxonomicNameLabel>
(
<figureCitation box="[876,951,1806,1832]" captionStart="FIGURE 3" captionStartId="16.[151,250,1863,1886]" captionTargetBox="[151,1436,195,1839]" captionTargetId="figure@16.[151,1436,195,1839]" captionTargetPageId="16" captionText="FIGURE 3. Doggerella azhari sp. n.; a, head, dorsal aspect; b, metasoma from tergite 2 dorsolateral aspect. Doggerella cohaerens sp. n.; c, head, dorsal aspect; d, mesosoma dorsal aspect; e, propodeum dorsal aspect; f, fore wing, distal part." httpUri="https://zenodo.org/record/277973/files/figure.png" pageId="4" pageNumber="5">
Figs 
<number box="[937,951,1806,1832]" pageId="4" pageNumber="5" value="3.0">3</number>
</figureCitation>
c–f, 
<number box="[1003,1018,1806,1832]" pageId="4" pageNumber="5" value="4.0">4</number>
a, 
<number box="[1047,1075,1806,1832]" pageId="4" pageNumber="5" value="14.0">14</number>
a–f)
</emphasis>
</heading>
</paragraph>
