<collectingCountry box="[993,1078,295,319]" name="Taiwan" pageId="3" pageNumber="493">Taiwan</collectingCountry>
to Western 
<collectingCountry box="[1220,1323,295,319]" name="Australia" pageId="3" pageNumber="493">Australia</collectingCountry>
and 
<collectingCountry name="New Zealand" pageId="3" pageNumber="493">New Zealand</collectingCountry>
], 
<taxonomicName authority="Benedict, 1902" authorityName="Benedict" authorityYear="1902" box="[270,557,332,356]" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="493" phylum="Arthropoda" rank="species" species="mina">
