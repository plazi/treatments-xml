and 
<emphasis box="[1312,1339,1452,1475]" italics="true" pageId="25" pageNumber="981">an</emphasis>
<subScript attach="left" box="[1338,1347,1460,1477]" fontSize="7" pageId="25" pageNumber="981">3</subScript>
