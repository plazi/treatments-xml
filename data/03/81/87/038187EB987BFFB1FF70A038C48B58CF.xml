<collectionCode LSID="urn:lsid:biocol.org:col:32981" box="[1126,1200,1901,1925]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="5" pageNumber="284">ANIC</collectionCode>
