<paragraph blockId="2.[151,368,908,967]" box="[151,368,908,934]" pageId="2" pageNumber="379">
<heading bold="true" box="[151,368,908,934]" fontSize="11" level="1" pageId="2" pageNumber="379" reason="1">
<emphasis bold="true" box="[151,368,908,934]" pageId="2" pageNumber="379">
<taxonomicName authorityName="Hippa &amp; Saigusa" authorityYear="2016" box="[151,325,908,934]" class="Insecta" family="Mycetophilidae" genus="Manota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="379" phylum="Arthropoda" rank="species" species="incilis">
<emphasis bold="true" box="[151,325,908,934]" italics="true" pageId="2" pageNumber="379">Manota incilis</emphasis>
