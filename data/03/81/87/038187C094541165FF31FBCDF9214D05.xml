<paragraph blockId="6.[151,861,1114,1136]" box="[151,861,1114,1136]" pageId="6" pageNumber="140">
<typeStatus box="[151,201,1115,1136]" pageId="6" pageNumber="140">Type</typeStatus>
species: 
<taxonomicName authority="Arle, 1959" authorityName="Arle" authorityYear="1959" box="[294,622,1114,1136]" class="Entognatha" family="Neanuridae" genus="Neanura" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="140" phylum="Arthropoda" rank="species" species="brasiliensis">
<emphasis box="[294,506,1114,1136]" italics="true" pageId="6" pageNumber="140">Neanura brasiliensis</emphasis>
<bibRefCitation author="Arle" box="[512,622,1114,1136]" pageId="6" pageNumber="140" refString="Arle, R. (1959) Collembola Arthropleona do Brasil oriental e central. Arquivos do Museu Nacional, 49, 155 - 211." type="journal article" year="1959">
Arlé, 
<number box="[569,622,1114,1136]" pageId="6" pageNumber="140" value="1959.0">1959</number>
</bibRefCitation>
</taxonomicName>
, by present designation
</paragraph>
