, 
<date box="[981,1131,1698,1722]" pageId="4" pageNumber="5" value="1929-09-08" valueMax="1929-09-30" valueMin="1929-09-08">
<number box="[981,1040,1698,1722]" pageId="4" pageNumber="5" value="19.0" valueMax="30.0" valueMin="8.0">8–30</number>
