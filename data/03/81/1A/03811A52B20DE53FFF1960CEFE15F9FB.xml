<taxonomicName authority="Curvatapenis" authorityName="Curvatapenis" box="[714,1069,1556,1580]" class="Insecta" family="Tenthredinidae" genus="Neocolochelyna" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="12" pageNumber="444" phylum="Arthropoda" rank="genus">
