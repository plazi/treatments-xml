<paragraph blockId="63.[140,1095,160,1085]" pageId="63" pageNumber="1949">
<emphasis box="[179,295,964,988]" italics="true" pageId="63" pageNumber="1949">Comments</emphasis>
. Adult 
<taxonomicName authority="Mifsud &amp; Burckhardt, 2002" authorityName="Mifsud &amp; Burckhardt" authorityYear="2002" box="[389,549,964,988]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="63" pageNumber="1949" phylum="Arthropoda" rank="species" species="boehmeriae" status="sp. n.">
<emphasis box="[389,549,964,988]" italics="true" pageId="63" pageNumber="1949">P. boehmeriae</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[559,622,965,988]" pageId="63" pageNumber="1949" rank="species">sp. n.</taxonomicNameLabel>
