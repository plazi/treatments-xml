<taxonomicName authority="Uichanco" authorityName="Uichanco" authorityYear="1921" box="[736,1030,1324,1348]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="74" pageNumber="1960" phylum="Arthropoda" rank="species" species="maculipennis">
