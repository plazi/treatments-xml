<taxonomicName authority="Lindl." authorityName="Lindl." box="[607,921,160,184]" class="Magnoliopsida" family="Malvaceae" genus="Sterculia" kingdom="Plantae" order="Malvales" pageId="64" pageNumber="1950" phylum="Tracheophyta" rank="species" species="tragacantha">
<emphasis box="[607,846,160,184]" italics="true" pageId="64" pageNumber="1950">Sterculia tragacantha</emphasis>
Lindl.
</taxonomicName>
(
<taxonomicName box="[935,1083,160,184]" family="Sterculiaceae" pageId="64" pageNumber="1950" rank="family">Sterculiaceae</taxonomicName>
