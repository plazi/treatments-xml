<taxonomicName baseAuthorityName="Crawford" baseAuthorityYear="1917" box="[490,626,590,614]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="89" pageNumber="1975" phylum="Arthropoda" rank="species" species="brevicephala">
