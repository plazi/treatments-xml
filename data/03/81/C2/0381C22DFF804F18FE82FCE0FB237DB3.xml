<taxonomicName authority="Wasmann, 1896" authorityName="Wasmann" authorityYear="1896" box="[883,1255,414,437]" class="Insecta" family="Tenebrionidae" genus="Xenotermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="346" phylum="Arthropoda" rank="species" species="feai">
