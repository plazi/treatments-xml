<quantity box="[506,665,1194,1219]" metricMagnitude="3" metricUnit="m" metricValue="2.096" metricValueMax="2.578" metricValueMin="1.614" pageId="22" pageNumber="363" unit="m" value="2096.0" valueMax="2578.0" valueMin="1614.0">1614–2578 m</quantity>
