<subSubSection pageId="null" pageNumber="417" type="vernacular_name">
<paragraph pageId="null" pageNumber="417">
<normalizedToken originalValue="Weiße" pageId="null" pageNumber="417">Weisse</normalizedToken>
Schnabelbinse
</paragraph>
