, 
<taxonomicName authorityName="Lambe" authorityYear="1914" box="[1489,1721,1759,1798]" class="Reptilia" family="Tyrannosauridae" genus="Gorgosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="4" pageNumber="327" phylum="Chordata" rank="genus">Gorgosaurus</taxonomicName>
