<typeStatus box="[303,405,252,276]" pageId="22" pageNumber="23">holotype</typeStatus>
: 
<collectingCountry box="[417,488,252,276]" name="Brazil" pageId="22" pageNumber="23">Brazil</collectingCountry>
, Pará, Ourém (
<taxonomicName box="[663,789,252,276]" class="Arachnida" family="Cranaidae" genus="Phareicranaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="22" pageNumber="33" phylum="Arthropoda" rank="species" species="patauateua" status="sp. n.">Patauateua</taxonomicName>
, fazenda Gavião), 
<date box="[1005,1126,252,276]" pageId="22" pageNumber="23" value="1990-10-29">
<number box="[1005,1035,252,276]" pageId="22" pageNumber="23" value="29.0">29</number>
.X. 
<number box="[1065,1126,252,276]" pageId="22" pageNumber="23" value="1990.0">1990</number>
</date>
, P. Tadeu 
<emphasis box="[1245,1286,252,276]" italics="true" pageId="22" pageNumber="23">leg.</emphasis>
(MPEG- 
<number box="[1395,1424,252,276]" pageId="22" pageNumber="23" value="40.0">40</number>
). 
<typeStatus box="[151,263,288,312]" pageId="22" pageNumber="23">Paratypes</typeStatus>
