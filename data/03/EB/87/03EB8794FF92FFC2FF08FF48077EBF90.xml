<number box="[538,666,799,823]" pageId="24" pageNumber="25" value="13.53" valueMax="18.9" valueMin="8.16">8, 16–18, 9</number>
, 
<number box="[677,708,799,823]" pageId="24" pageNumber="25" value="10.0">10</number>
.
