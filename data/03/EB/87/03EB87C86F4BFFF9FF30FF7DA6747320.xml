<taxonomicName authority="(Klie, 1930)" authorityName="Karanovic" authorityYear="2011" baseAuthorityName="Klie" baseAuthorityYear="1930" box="[491,900,1278,1302]" class="Ostracoda" family="Candonidae" genus="Keysercypria" kingdom="Animalia" order="Podocopida" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="longiseta">
