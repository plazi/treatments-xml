<collectingCountry box="[750,870,484,510]" name="Venezuela" pageId="11" pageNumber="12">Venezuela</collectingCountry>
(
<bibRefCitation author="Opresko" box="[891,1074,484,510]" pageId="11" pageNumber="31" refString="Opresko, D. M. (1972) Redescriptions and reevaluation of the Antipatharians described by L. F. de Pourtales. Biological Results of the University of Miami Deep - Sea Expedition, 97, 950 - 1015." type="journal article" year="1972">
Opresko, 
<number box="[1011,1074,484,510]" pageId="11" pageNumber="12" value="1972.0">1972</number>
</bibRefCitation>
); Caribbean (
<bibRefCitation author="Brook" pageId="11" pageNumber="30" refString="Brook, G. (1889) Report on the Antipatharia. Report on the scientific results of the Voyage Challenger. Zoology, 32, 5 - 222." type="journal article" year="1889">
Brook, 
<number box="[264,326,524,550]" pageId="11" pageNumber="12" value="1889.0">1889</number>
</bibRefCitation>
; 
<bibRefCitation author="Opresko" box="[343,525,524,550]" pageId="11" pageNumber="31" refString="Opresko, D. M. (1972) Redescriptions and reevaluation of the Antipatharians described by L. F. de Pourtales. Biological Results of the University of Miami Deep - Sea Expedition, 97, 950 - 1015." type="journal article" year="1972">
Opresko, 
<number box="[462,525,524,550]" pageId="11" pageNumber="12" value="1972.0">1972</number>
</bibRefCitation>
); Boca de Navios, NW 
<collectingCountry box="[827,927,524,550]" name="Trinidad and Tobago" pageId="11" pageNumber="12">Trinidad</collectingCountry>
(
<bibRefCitation author="Warner" box="[947,1115,524,550]" pageId="11" pageNumber="31" refString="Warner, G. F. (1981) Species descriptions and ecological observations of black corals (Antipatharia) from Trinidad. Bulletin of Marine Science, 31 (1), 147 - 163." type="journal article" year="1981">
Warner, 
<number box="[1052,1115,524,550]" pageId="11" pageNumber="12" value="1981.0">1981</number>
</bibRefCitation>
); 
<collectingCountry box="[1141,1217,524,550]" name="Brazil" pageId="11" pageNumber="12">Brazil</collectingCountry>
: oceanic seamount off eastern 
<collectingCountry box="[515,586,564,590]" name="Brazil" pageId="11" pageNumber="12">Brazil</collectingCountry>
(about 
<number box="[676,705,564,590]" pageId="11" pageNumber="12" value="19.0">19</number>
<emphasis box="[705,717,564,589]" italics="true" pageId="11" pageNumber="12">°</emphasis>
S—Fig. 
<number box="[823,837,564,590]" pageId="11" pageNumber="12" value="1.0">1</number>
).
</paragraph>
