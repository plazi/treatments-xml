<keyLead pageId="29" pageNumber="30">
<number box="[264,282,284,310]" pageId="29" pageNumber="30" value="4.0">4</number>
. Secondary and tertiary pinnules long: secondaries maximum length 
<quantity box="[1112,1240,284,310]" metricMagnitude="-2" metricUnit="m" metricValue="3.3" metricValueMax="4.7" metricValueMin="1.9" pageId="29" pageNumber="30" unit="mm" value="33.0" valueMax="47.0" valueMin="19.0">19–47 mm</quantity>
