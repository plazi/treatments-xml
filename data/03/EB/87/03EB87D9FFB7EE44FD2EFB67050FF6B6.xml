<subSubSection box="[343,1472,2338,2378]" pageId="5" pageNumber="54" type="etymology">
<paragraph blockId="5.[248,1628,1926,2471]" box="[343,1472,2338,2378]" pageId="5" pageNumber="54">
<emphasis box="[343,525,2342,2377]" italics="true" pageId="5" pageNumber="54">Etymology</emphasis>
