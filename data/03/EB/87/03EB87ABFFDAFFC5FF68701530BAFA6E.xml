(mean = 65± 
<quantity box="[295,354,260,285]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" pageId="7" unit="mm" value="3.0">3 µm</quantity>
), anterior aperture width 
<quantity box="[637,751,260,285]" metricMagnitude="-2" metricUnit="m" metricValue="4.65" metricValueMax="5.3" metricValueMin="4.0" pageId="7" unit="mm" value="46.5" valueMax="53.0" valueMin="40.0">40–53 µm</quantity>
(mean = 45± 
<quantity box="[901,961,260,285]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="7" unit="mm" value="5.0">5 µm</quantity>
), toe length 
<quantity box="[1099,1212,260,285]" metricMagnitude="-2" metricUnit="m" metricValue="3.05" metricValueMax="3.2" metricValueMin="2.9" pageId="7" unit="mm" value="30.5" valueMax="32.0" valueMin="29.0">29–32 µm</quantity>
(mean = 30± 
<quantity box="[1366,1423,260,284]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="7" unit="mm" value="1.0">1 µm</quantity>
); trophi (N = 5): right ramus length 
<quantity box="[542,684,296,320]" metricMagnitude="-2" metricUnit="m" metricValue="1.015" metricValueMax="1.14" metricValueMin="0.89" pageId="7" unit="mm" value="10.15" valueMax="11.4" valueMin="8.9">8.9–11.4 µm</quantity>
(mean = 10.0± 
<quantity box="[860,941,296,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="7" unit="mm" value="1.1">1.1 µm</quantity>
