<taxonomicName authority="Faizi &amp; Nozari &amp; Ghobari &amp; Kalashian, 2020" authorityName="Faizi &amp; Nozari &amp; Ghobari &amp; Kalashian" authorityYear="2020" box="[541,976,1983,2007]" class="Insecta" family="Cerambycidae" genus="Dorcadion" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="597" phylum="Arthropoda" rank="species" species="darakiensis" status="sp. nov." subGenus="Cribridorcadion">
<emphasis box="[541,650,1983,2007]" italics="true" pageId="3" pageNumber="597">Dorcadion</emphasis>
(
<emphasis box="[671,838,1983,2007]" italics="true" pageId="3" pageNumber="597">Cribridorcadion</emphasis>
) 
<emphasis box="[859,976,1983,2007]" italics="true" pageId="3" pageNumber="597">darakiensis</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[994,1081,1983,2007]" pageId="3" pageNumber="597">
<taxonomicNameLabel box="[994,1081,1983,2007]" pageId="3" pageNumber="597" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
