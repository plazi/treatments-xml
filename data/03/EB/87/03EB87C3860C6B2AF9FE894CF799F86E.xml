<taxonomicName authority="(Yoo)" baseAuthorityName="Yoo" baseAuthorityYear="1988" class="Gastropoda" family="Streptacididae" genus="Streptacis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="30" pageNumber="93" phylum="Mollusca" rank="species" species="elegantissima">
