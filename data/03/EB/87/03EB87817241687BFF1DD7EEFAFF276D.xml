</subSubSection>
<subSubSection pageId="1" pageNumber="348" type="etymology">
<paragraph blockId="1.[813,1452,1027,1083]" pageId="1" pageNumber="348">
<emphasis bold="true" box="[813,960,1027,1051]" italics="true" pageId="1" pageNumber="348">Etymology. –</emphasis>
