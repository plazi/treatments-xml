<subSubSection pageId="5" pageNumber="56" type="etymology">
<paragraph blockId="5.[264,1322,1247,1353]" box="[264,389,1247,1272]" pageId="5" pageNumber="56">
<emphasis box="[264,389,1247,1272]" italics="true" pageId="5" pageNumber="56">Etymology</emphasis>
</paragraph>
<paragraph blockId="5.[264,1322,1247,1353]" pageId="5" pageNumber="56">
