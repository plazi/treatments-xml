<taxonomicName authorityName="Shavrin &amp; Smetana" authorityYear="2018" box="[1125,1252,1972,1994]" class="Insecta" family="Staphylinidae" genus="Anthobium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="species" species="besucheti">
