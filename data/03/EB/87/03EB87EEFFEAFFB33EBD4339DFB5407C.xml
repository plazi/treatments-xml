<paragraph blockId="13.[151,1437,151,1544]" pageId="13" pageNumber="72">
<emphasis bold="true" box="[199,315,1052,1076]" pageId="13" pageNumber="72">Remarks.</emphasis>
<taxonomicName box="[323,777,1052,1076]" class="Clitellata" family="Glossoscolecidae" genus="Glossoscolex" kingdom="Animalia" order="Haplotaxida" pageId="13" pageNumber="72" phylum="Annelida" rank="species" species="primaensis" subGenus="Glossoscolex">
