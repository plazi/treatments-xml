<collectingRegion box="[660,741,1402,1427]" country="United States of America" name="Florida" pageId="1" pageNumber="534">Florida</collectingRegion>
to 
<collectingCountry box="[792,860,1402,1427]" name="Brazil" pageId="1" pageNumber="534">Brazil</collectingCountry>
