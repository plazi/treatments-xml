<emphasis box="[394,643,376,401]" italics="true" pageId="7" pageNumber="58">Perciana marmorea</emphasis>
Walker, 
<number box="[758,822,376,402]" pageId="7" pageNumber="58" value="1865.0">1865</number>
</taxonomicName>
) and 
<taxonomicName authority="Hampson, 1893" authorityName="Hampson" authorityYear="1893" box="[893,1264,376,402]" class="Insecta" family="Noctuidae" genus="Mecistoptera" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="58" phylum="Arthropoda" rank="genus">
