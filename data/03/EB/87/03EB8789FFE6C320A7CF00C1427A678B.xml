<taxonomicName authority="Warren, 1913" authorityName="Warren" authorityYear="1913" box="[638,947,301,327]" class="Insecta" family="Noctuidae" genus="Lophomilia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="58" phylum="Arthropoda" rank="genus">
