<number box="[887,947,301,327]" pageId="7" pageNumber="58" value="1913.0">1913</number>
</taxonomicName>
(
<typeStatus box="[964,1018,301,327]" pageId="7" pageNumber="58">type</typeStatus>
species 
<taxonomicName authority="Butler, 1879" authorityName="Butler" authorityYear="1879" class="Insecta" family="Noctuidae" genus="Egnasia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="58" phylum="Arthropoda" rank="species" species="polybapta">
