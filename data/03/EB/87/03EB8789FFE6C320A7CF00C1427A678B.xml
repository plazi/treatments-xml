(
<typeStatus box="[587,640,338,364]" pageId="7" pageNumber="58">type</typeStatus>
species 
<taxonomicName authority="Bremer, 1864" authorityName="Bremer" authorityYear="1864" box="[742,1207,338,364]" class="Insecta" family="Noctuidae" genus="Herminia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="58" phylum="Arthropoda" rank="species" species="albomaculalis">
<emphasis box="[742,1032,338,363]" italics="true" pageId="7" pageNumber="58">Herminia albomaculalis</emphasis>
Bremer, 
<number box="[1144,1207,338,364]" pageId="7" pageNumber="58" value="1864.0">1864</number>
