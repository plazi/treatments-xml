<number box="[898,914,661,685]" pageId="6" pageNumber="527" value="2.0">2</number>
)– 
<number box="[940,954,661,685]" pageId="6" pageNumber="527" value="5.0">5</number>
(
<number box="[963,973,661,685]" pageId="6" pageNumber="527" value="1.0">1</number>
