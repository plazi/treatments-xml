<collectingCountry box="[666,776,1050,1075]" name="Sri Lanka" pageId="24" pageNumber="25">Sri Lanka</collectingCountry>
(
<bibRefCitation author="Yonow" box="[791,939,1051,1075]" pageId="24" pageNumber="25" refString="Yonow, N. (1984) Doridacean nudibranchs from Sri Lanka, with descriptions of four new species. The Veliger, 26, 214 - 228." type="journal article" year="1984">Yonow 1984</bibRefCitation>
), 
<collectingCountry box="[959,1063,1050,1075]" name="Tanzania" pageId="24" pageNumber="25">Tanzania</collectingCountry>
, 
<collectingCountry box="[1074,1169,1050,1075]" name="Réunion" pageId="24" pageNumber="25">Réunion</collectingCountry>
Island (
<bibRefCitation author="Rudman" box="[1259,1423,1050,1075]" pageId="24" pageNumber="25" refString="Rudman, W. B. (1987) The Chromodorididae (Opisthobranchia: Mollusca) of the Indo-West Pacific: Chrornodods epicuria, C. aureopurpurea, C. anndata, C. coi and Risbecia tryoni colour groups. Zoological Journal of the Linnean Society, 90, 305 - 407. https: // doi. org / 10.1111 / j. 1096 - 3642.1987. tb 01357. x" type="journal article" year="1987">Rudman 1987</bibRefCitation>
), 
<collectingCountry box="[151,288,1086,1111]" name="Madagascar" pageId="24" pageNumber="25">Madagascar</collectingCountry>
, 
<collectingCountry box="[299,443,1086,1111]" name="South Africa" pageId="24" pageNumber="25">
<collectingRegion box="[299,364,1086,1111]" country="Tanzania" name="Kusini Unguja" pageId="24" pageNumber="25">South</collectingRegion>
Africa
</collectingCountry>
(Gosliner 
<emphasis box="[563,612,1086,1111]" italics="true" pageId="24" pageNumber="25">et al</emphasis>
. 2008) and 
<collectingCountry box="[745,896,1086,1111]" name="Mozambique" pageId="24" pageNumber="25">Mozambique</collectingCountry>
.
</paragraph>
