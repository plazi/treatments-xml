, 
<taxonomicName authority="Perrier, R, 1898" authorityName="Perrier, R" authorityYear="1898" class="Holothuroidea" family="Phyllophoridae" genus="Thyone" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dendrochirotida" pageId="4" pageNumber="537" phylum="Echinodermata" rank="species" species="gadeana">
