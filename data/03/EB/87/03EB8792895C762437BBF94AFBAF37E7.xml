<taxonomicName authority="Cherbonnier, 1988" authorityName="Cherbonnier" authorityYear="1988" box="[450,771,1823,1848]" class="Holothuroidea" family="Phyllophoridae" genus="Thyone" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dendrochirotida" pageId="4" pageNumber="537" phylum="Echinodermata" rank="species" species="vadosa">
