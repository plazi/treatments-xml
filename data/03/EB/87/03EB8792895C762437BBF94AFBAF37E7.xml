</taxonomicName>
, 
<taxonomicName authority="Koehler &amp; Vaney, 1908" authorityName="Koehler &amp; Vaney" authorityYear="1908" box="[452,822,1715,1740]" class="Holothuroidea" family="Phyllophoridae" genus="Thyone" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dendrochirotida" pageId="4" pageNumber="537" phylum="Echinodermata" rank="species" species="dura">
