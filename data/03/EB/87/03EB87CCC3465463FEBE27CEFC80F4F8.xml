<heading bold="true" box="[264,760,1278,1304]" fontSize="11" level="1" pageId="1" pageNumber="40" reason="1">
<emphasis bold="true" box="[264,760,1278,1304]" pageId="1" pageNumber="40">
<taxonomicName authority="Miller" authorityName="Miller" box="[264,603,1278,1304]" class="Insecta" family="Dytiscidae" genus="Desmopachria" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="pilosa" status="new species">
