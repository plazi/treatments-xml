<collectingRegion box="[666,773,1612,1637]" country="Laos" name="Viangchan" pageId="38" pageNumber="239">Vientiane</collectingRegion>
Province, Vang Vieng, W. of Nam Song (
<geoCoordinate box="[1261,1431,1612,1637]" direction="north" orientation="latitude" pageId="38" pageNumber="239" precision="1" value="18.929682">N18°55'46.86&quot;</geoCoordinate>
, 
<geoCoordinate box="[151,332,1647,1672]" direction="east" orientation="longitude" pageId="38" pageNumber="239" precision="1" value="102.34912">102°20'56.82&quot;E</geoCoordinate>
), 
<quantity box="[352,425,1648,1672]" metricMagnitude="2" metricUnit="m" metricValue="3.2399999999999998" pageId="38" pageNumber="239" unit="m" value="324.0">324 m</quantity>
