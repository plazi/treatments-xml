<taxonomicName authority="Wesener, 2019" authorityName="Wesener" authorityYear="2019" box="[292,537,1947,1969]" class="Diplopoda" family="Zephronidae" genus="Sphaerobelum" kingdom="Animalia" order="Sphaerotheriida" pageId="20" pageNumber="221" phylum="Arthropoda" rank="species" species="spinatum" status="n. sp.">
<emphasis box="[292,537,1947,1969]" italics="true" pageId="20" pageNumber="221">Sphaerobelum spinatum</emphasis>
</taxonomicName>
<emphasis bold="true" box="[542,599,1947,1969]" pageId="20" pageNumber="221">
<taxonomicNameLabel box="[542,599,1947,1969]" pageId="20" pageNumber="221" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, holotype (ZMUC), SEM, 
<emphasis bold="true" box="[872,890,1947,1969]" pageId="20" pageNumber="221">A</emphasis>
: left antenna, lateral view; 
<emphasis bold="true" box="[1165,1181,1947,1969]" pageId="20" pageNumber="221">B</emphasis>
: apical disc with sensory cones; 
<emphasis bold="true" box="[224,251,1977,1999]" pageId="20" pageNumber="221">C:</emphasis>
gnathochilarium, ventral view. Abbreviations: 
<emphasis bold="true" box="[742,769,1977,1999]" pageId="20" pageNumber="221">cP</emphasis>
= central pads; 
<emphasis bold="true" box="[941,955,1977,1999]" pageId="20" pageNumber="221">d</emphasis>
= apical disc; 
<emphasis bold="true" box="[1113,1147,1977,1999]" pageId="20" pageNumber="221">LL</emphasis>
= lamellae linguales; 
<emphasis bold="true" box="[1380,1412,1977,1999]" pageId="20" pageNumber="221">LP</emphasis>
= lateral palpus; 
<emphasis bold="true" box="[300,324,2008,2030]" pageId="20" pageNumber="221">sb</emphasis>
= sensilla basiconica; 
<emphasis bold="true" box="[553,574,2008,2030]" pageId="20" pageNumber="221">sc</emphasis>
= sensory cones; 
<emphasis bold="true" box="[756,776,2008,2030]" pageId="20" pageNumber="221">St</emphasis>
