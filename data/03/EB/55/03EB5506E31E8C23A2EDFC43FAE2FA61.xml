<subSubSection lastPageId="23" lastPageNumber="224" pageId="21" pageNumber="222" type="description">
<paragraph blockId="21.[151,1437,151,357]" box="[199,786,151,176]" pageId="21" pageNumber="222">
<emphasis bold="true" box="[199,345,151,176]" pageId="21" pageNumber="222">Description.</emphasis>
Based on 
<typeStatus box="[462,560,151,176]" pageId="21" pageNumber="222">holotype</typeStatus>
and both 
<typeStatus box="[671,782,152,176]" pageId="21" pageNumber="222">paratypes</typeStatus>
