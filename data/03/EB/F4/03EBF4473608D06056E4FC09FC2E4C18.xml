, 
<taxonomicName authority="(Girault, 1913 b)" baseAuthorityName="Girault" baseAuthorityYear="1913" class="Insecta" family="Eulophidae" genus="Epomphaloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="flavus">
