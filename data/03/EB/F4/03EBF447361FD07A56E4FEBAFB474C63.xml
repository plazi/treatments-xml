<subSubSection pageId="24" pageNumber="25" type="etymology">
<paragraph blockId="24.[151,1437,150,1834]" pageId="24" pageNumber="25">
<emphasis bold="true" box="[199,334,1447,1473]" pageId="24" pageNumber="25">Etymology.</emphasis>
