<paragraph blockId="12.[151,1127,219,532]" pageId="12" pageNumber="623">
<taxonomicName authorityName="Stal" authorityYear="1870" box="[181,330,349,372]" class="Insecta" family="Largidae" genus="Iphita" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="623" phylum="Arthropoda" rank="species" species="limbata">
<emphasis box="[181,330,349,372]" italics="true" pageId="12" pageNumber="623">Iphita limbata</emphasis>
</taxonomicName>
differs from 
<taxonomicName authority="Stehlík &amp; Jindra, 2008" authorityName="Stehlík &amp; Jindra" authorityYear="2008" box="[463,543,349,372]" class="Insecta" family="Largidae" genus="Iphita" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="623" phylum="Arthropoda" rank="species" species="heissi" status="sp. nov.">
