<emphasis box="[158,298,995,1013]" italics="true" pageId="33" pageNumber="644">E. sulawesiensis</emphasis>
Stehlík &amp; Jindra, 2006
</taxonomicName>
; 47 – 
<taxonomicName authority="Stehlík &amp; Jindra, 2008" authorityName="Stehlík &amp; Jindra" authorityYear="2008" box="[556,683,995,1013]" class="Insecta" family="Curculionidae" genus="Ectatops" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="644" phylum="Arthropoda" rank="species" species="schoenitzeri" status="sp. nov.">
<emphasis box="[556,683,995,1013]" italics="true" pageId="33" pageNumber="644">E. schoenitzeri</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[689,757,995,1014]" pageId="33" pageNumber="644" rank="species">sp. nov.</taxonomicNameLabel>
48-50 – parameres: 48 – 
<taxonomicName authority="Stehlík &amp; Jindra, 2008" authorityName="Stehlík &amp; Jindra" authorityYear="2008" box="[978,1058,995,1013]" class="Insecta" family="Curculionidae" genus="Ectatops" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="644" phylum="Arthropoda" rank="species" species="riedeli" status="sp. nov.">
<emphasis box="[978,1058,995,1013]" italics="true" pageId="33" pageNumber="644">E. riedeli</emphasis>
</taxonomicName>
<taxonomicNameLabel pageId="33" pageNumber="644" rank="species">sp. nov.</taxonomicNameLabel>
