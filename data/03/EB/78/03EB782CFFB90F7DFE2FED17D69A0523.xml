(
<typeStatus box="[426,504,1582,1601]" pageId="29" pageNumber="640">holotype</typeStatus>
, J, 
<quantity box="[537,616,1582,1601]" metricMagnitude="-3" metricUnit="m" metricValue="7.4" pageId="29" pageNumber="640" unit="mm" value="7.4">7.40 mm</quantity>
); 42 – 
<taxonomicName authority="Stehlík &amp; Jindra, 2008" authorityName="Stehlík &amp; Jindra" authorityYear="2008" box="[671,806,1583,1601]" class="Insecta" family="Curculionidae" genus="Ectatops" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="640" phylum="Arthropoda" rank="species" species="riedeli" status="sp. nov.">
<emphasis box="[671,806,1583,1601]" italics="true" pageId="29" pageNumber="640">Ectatops riedeli</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[810,876,1583,1602]" pageId="29" pageNumber="640" rank="species">sp. nov.</taxonomicNameLabel>
(
