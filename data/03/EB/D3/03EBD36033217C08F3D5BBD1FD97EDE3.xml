from 
<collectingCountry box="[658,781,494,519]" name="Zimbabwe" pageId="13" pageNumber="164">Zimbabwe</collectingCountry>
, 
<collectingCountry box="[791,939,494,519]" name="Mozambique" pageId="13" pageNumber="164">Mozambique</collectingCountry>
(
<collectionCode box="[953,1042,495,519]" pageId="13" pageNumber="164">BMNH</collectionCode>
, 
<collectionCode box="[1052,1136,495,518]" country="Zimbabwe" httpUri="http://grbio.org/cool/yne7-xrqc" name="Natural History Museum of Zimbabwe" pageId="13" pageNumber="164">NMBZ</collectionCode>
