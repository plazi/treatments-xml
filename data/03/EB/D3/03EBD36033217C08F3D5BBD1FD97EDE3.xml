<taxonomicName authorityName="Navas" authorityYear="1912" box="[900,1003,602,627]" class="Insecta" family="Psychopsidae" genus="Cabralis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="13" pageNumber="164" phylum="Arthropoda" rank="species" species="gloriosus">gloriosus</taxonomicName>
