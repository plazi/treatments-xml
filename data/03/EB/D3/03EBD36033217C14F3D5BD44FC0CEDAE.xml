<taxonomicName authorityName="Bakkes &amp; Sole &amp; Mansell" authorityYear="2017" box="[425,515,1804,1829]" class="Insecta" family="Psychopsidae" genus="Cabralis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="13" pageNumber="164" phylum="Arthropoda" rank="species" species="pallidus">
