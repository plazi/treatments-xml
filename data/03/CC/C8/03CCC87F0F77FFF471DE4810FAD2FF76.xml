and 
<taxonomicName box="[151,236,1696,1719]" class="Insecta" family="Tricorythidae" genus="Tricorythus" kingdom="Animalia" order="Ephemeroptera" pageId="8" pageNumber="281" phylum="Arthropoda" rank="species" species="tener" status="sp.n.">
