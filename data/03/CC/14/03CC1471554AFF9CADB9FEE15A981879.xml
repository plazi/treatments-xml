<collectingCountry box="[1230,1277,826,851]" name="United States of America" pageId="2" pageNumber="3">USA</collectingCountry>
(
<collectingRegion box="[1290,1393,826,851]" country="United States of America" name="Alabama" pageId="2" pageNumber="3">Alabama</collectingRegion>
, 
<collectingRegion box="[1403,1491,826,851]" country="United States of America" name="Arizona" pageId="2" pageNumber="3">Arizona</collectingRegion>
