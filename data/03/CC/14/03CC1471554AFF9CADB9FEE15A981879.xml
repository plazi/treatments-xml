<collectingRegion box="[998,1077,858,883]" country="United States of America" name="Florida" pageId="2" pageNumber="3">Florida</collectingRegion>
, 
<collectingRegion box="[1096,1186,858,883]" country="United States of America" name="Georgia" pageId="2" pageNumber="3">Georgia</collectingRegion>
