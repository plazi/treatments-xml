<quantity box="[725,813,1864,1890]" metricMagnitude="2" metricUnit="m" metricValue="7.0104000000000015" pageId="6" pageNumber="37" unit="ft" value="2300.0">2300 ft</quantity>
. Champion’. NHM 
<typeStatus box="[1046,1096,1864,1890]" pageId="6" pageNumber="37">type</typeStatus>
Hym. 
<number box="[1177,1192,1864,1890]" pageId="6" pageNumber="37" value="3.0">3</number>
c 
