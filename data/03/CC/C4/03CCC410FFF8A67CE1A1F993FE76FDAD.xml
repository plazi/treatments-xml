</taxonomicName>
<emphasis bold="true" box="[730,815,1423,1447]" pageId="9" pageNumber="10">
<taxonomicNameLabel box="[730,815,1423,1447]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName box="[867,1060,1424,1447]" class="Insecta" family="Ephydridae" genus="Nostima" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="negramaculata" status="sp. nov.">
<emphasis box="[867,1060,1424,1447]" italics="true" pageId="9" pageNumber="10">N. negramaculata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1066,1151,1423,1447]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
