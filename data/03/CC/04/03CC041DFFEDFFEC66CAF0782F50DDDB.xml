<taxonomicName authorityName=": Bergroth" authorityYear="1908" box="[743,880,850,873]" class="Insecta" family="Pentatomidae" genus="Menestheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="278" phylum="Arthropoda" rank="species" species="cuneatus">
