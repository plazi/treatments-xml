<paragraph blockId="5.[151,777,255,556]" box="[151,710,532,556]" pageId="5" pageNumber="277">
<taxonomicName authority="(Butler)" baseAuthorityName="Butler" baseAuthorityYear="1870" box="[151,364,532,556]" class="Insecta" family="Castniidae" genus="Telchin" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="277" phylum="Arthropoda" rank="species" species="diva">
