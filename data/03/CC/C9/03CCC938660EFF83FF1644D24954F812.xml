(
<taxonomicName authority="Viryi Boisduval" authorityName="Viryi Boisduval" box="[828,1074,1654,1678]" class="Insecta" family="Castniidae" genus="Castnia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="276" phylum="Arthropoda" rank="genus">
