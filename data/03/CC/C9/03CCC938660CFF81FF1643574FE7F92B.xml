<paragraph blockId="6.[151,1437,722,1145]" pageId="6" pageNumber="278">
<taxonomicName authority="Westwood" authorityName="Westwood" authorityYear="1877" box="[151,445,784,808]" class="Insecta" family="Castniidae" genus="Castnia" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="278" phylum="Arthropoda" rank="species" species="clitarcha">
