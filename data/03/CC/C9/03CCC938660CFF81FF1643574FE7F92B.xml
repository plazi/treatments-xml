<typeStatus box="[1007,1051,784,808]" pageId="6" pageNumber="278">type</typeStatus>
locality as 
<collectingCountry box="[1170,1253,784,807]" name="Panama" pageId="6" pageNumber="278">Panama</collectingCountry>
