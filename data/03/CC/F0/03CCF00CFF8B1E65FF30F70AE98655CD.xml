<subSubSection pageId="6" pageNumber="60" type="nomenclature">
<paragraph blockId="6.[151,506,1047,1108]" box="[151,506,1047,1073]" pageId="6" pageNumber="60">
<heading bold="true" box="[151,506,1047,1073]" fontSize="11" level="1" pageId="6" pageNumber="60" reason="1">
<emphasis bold="true" box="[151,506,1047,1073]" pageId="6" pageNumber="60">
<taxonomicName box="[151,347,1047,1073]" class="Insecta" family="Psychomyiidae" genus="Paduniella" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="60" phylum="Arthropoda" rank="species" species="sona" status="new species">
