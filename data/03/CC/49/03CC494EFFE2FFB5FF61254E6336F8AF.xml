<paragraph blockId="17.[151,1436,151,255]" pageId="17" pageNumber="615">
<emphasis bold="true" box="[198,272,151,177]" pageId="17" pageNumber="615">Head.</emphasis>
<emphasis box="[280,344,152,177]" italics="true" pageId="17" pageNumber="615">Head</emphasis>
