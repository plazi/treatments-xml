<taxonomicName box="[963,1069,1071,1097]" class="Insecta" family="Vespidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="87" phylum="Arthropoda" rank="family">Vespidae</taxonomicName>
(
<taxonomicName authority="(Saussure, 1852))" baseAuthorityName="Saussure" baseAuthorityYear="1852" class="Insecta" family="Tachinidae" genus="Delta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="87" phylum="Arthropoda" rank="species" species="latreillei">
<emphasis box="[1089,1266,1071,1097]" italics="true" pageId="12" pageNumber="87">Delta latreillei</emphasis>
(Saussure, 1852))
</taxonomicName>
and 
<taxonomicName authorityName="Latreille" authorityYear="1802" box="[324,445,1109,1135]" class="Insecta" family="Sphecidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="87" phylum="Arthropoda" rank="family">Sphecidae</taxonomicName>
