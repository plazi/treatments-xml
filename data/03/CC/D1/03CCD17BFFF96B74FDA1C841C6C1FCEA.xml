<collectingCountry box="[311,446,462,488]" name="Philippines" pageId="9" pageNumber="84">Philippines</collectingCountry>
, 
<collectingCountry box="[458,575,462,488]" name="Sri Lanka" pageId="9" pageNumber="84">Sri Lanka</collectingCountry>
