<subSubSection pageId="8" pageNumber="61" type="description">
<paragraph blockId="8.[151,1436,1466,2012]" pageId="8" pageNumber="61">
<emphasis bold="true" box="[151,231,1466,1492]" pageId="8" pageNumber="61">
<typeStatus box="[151,226,1466,1492]" pageId="8" pageNumber="61">Types</typeStatus>
:
</emphasis>
male 
<typeStatus box="[312,414,1466,1492]" pageId="8" pageNumber="61">holotype</typeStatus>
and female 
<typeStatus box="[569,670,1466,1492]" pageId="8" pageNumber="61">paratype</typeStatus>
(
<collectionCode box="[688,781,1466,1492]" httpUri="http://grbio.org/cool/zxpe-c0as" name="Virginia Museum of Natural History" pageId="8" pageNumber="61">VMNH</collectionCode>
) from Matts Black Cave, 
<quantity box="[1104,1193,1466,1492]" metricMagnitude="3" metricUnit="m" metricValue="4.828032" pageId="8" pageNumber="61" unit="mi" value="3.0">3 miles</quantity>
north of Frankford, 
<emphasis box="[151,323,1507,1532]" italics="true" pageId="8" pageNumber="61">Greenbrier Co</emphasis>
., 
<collectionCode box="[345,425,1506,1532]" pageId="8" pageNumber="61">WEST</collectionCode>
VIRGINIA, collected 
<date box="[693,850,1506,1532]" pageId="8" pageNumber="61" value="2002-06-12">
<number box="[693,722,1506,1532]" pageId="8" pageNumber="61" value="12.0">12</number>
June 
<number box="[791,850,1506,1532]" pageId="8" pageNumber="61" value="2002.0">2002</number>
</date>
