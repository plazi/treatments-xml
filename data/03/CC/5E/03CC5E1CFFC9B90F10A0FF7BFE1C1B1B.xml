<collectionCode box="[1356,1436,264,290]" pageId="10" pageNumber="63">WEST</collectionCode>
VIRGINIA, collected 
<date box="[412,640,304,330]" pageId="10" pageNumber="63" value="2000-09-23">
<number box="[412,441,304,330]" pageId="10" pageNumber="63" value="23.0">23</number>
September 
<number box="[581,640,304,330]" pageId="10" pageNumber="63" value="2000.0">2000</number>
</date>
