<specimenCount box="[910,930,626,651]" pageId="9" pageNumber="188" type="male">♂</specimenCount>
6.5–8.0; fore femora 
<specimenCount box="[1175,1194,626,651]" pageId="9" pageNumber="188" type="male">♂</specimenCount>
7.5–9.0; hind femora 
<specimenCount box="[151,170,662,687]" pageId="9" pageNumber="188" type="male">♂</specimenCount>
