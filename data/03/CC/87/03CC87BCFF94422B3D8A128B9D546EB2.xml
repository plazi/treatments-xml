<quantity box="[1217,1363,1616,1641]" metricMagnitude="-1" metricUnit="m" metricValue="4.12" metricValueMax="4.34" metricValueMin="3.9" pageId="7" pageNumber="308" unit="mm" value="412.0" valueMax="434.0" valueMin="390.0">390–434 µm</quantity>
