<emphasis bold="true" box="[151,278,1114,1140]" pageId="14" pageNumber="15">Family:—</emphasis>
<taxonomicName box="[278,441,1114,1140]" class="Jungermanniopsida" family="Lejeuneaceae" kingdom="Plantae" order="Porellales" pageId="14" pageNumber="15" phylum="Bryophyta" rank="family">Lejeuneaceae</taxonomicName>
