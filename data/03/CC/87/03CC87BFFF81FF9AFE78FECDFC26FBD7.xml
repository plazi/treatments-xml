<subSubSection pageId="77" pageNumber="78" type="discussion">
<paragraph blockId="77.[189,303,752,778]" box="[189,303,752,778]" pageId="77" pageNumber="78">
<heading bold="true" box="[189,303,752,778]" fontSize="11" level="3" pageId="77" pageNumber="78" reason="3">
