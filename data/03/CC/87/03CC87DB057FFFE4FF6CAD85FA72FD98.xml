<geoCoordinate box="[744,851,258,285]" degrees="93, 05" direction="north" orientation="latitude" pageId="8" pageNumber="360" precision="555" value="93.05">93, 05°N</geoCoordinate>
, 
<geoCoordinate box="[863,937,259,285]" degrees="120" direction="east" orientation="longitude" pageId="8" pageNumber="360" precision="55555" value="120.0">120°E</geoCoordinate>
