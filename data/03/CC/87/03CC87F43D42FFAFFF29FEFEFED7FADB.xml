<number box="[396,406,1971,1997]" pageId="6" pageNumber="7" value="1.0">1</number>
-St 
<number box="[442,456,1971,1997]" pageId="6" pageNumber="7" value="3.0">3</number>
<number box="[463,494,1971,1997]" pageId="6" pageNumber="7" value="66.0">66</number>
(
