), St 
<number box="[624,638,1971,1997]" pageId="6" pageNumber="7" value="2.0">2</number>
-St 
<number box="[671,685,1971,1997]" pageId="6" pageNumber="7" value="2.0">2</number>
<number box="[692,723,1971,1997]" pageId="6" pageNumber="7" value="70.0">70</number>
(
<number box="[732,809,1971,1997]" pageId="6" pageNumber="7" value="68.5" valueMax="70.0" valueMin="67.0">67–70</number>
). Genital shield smooth; distance between St 
<number box="[1332,1345,1971,1997]" pageId="6" pageNumber="7" value="5.0">5</number>
-St 
<number box="[1379,1392,1971,1997]" pageId="6" pageNumber="7" value="5.0">5</number>
<number box="[1399,1432,1971,1997]" pageId="6" pageNumber="7" value="67.0">67</number>
