<number box="[773,817,2011,2037]" pageId="6" pageNumber="7" value="115.0">115</number>
(
<number box="[826,915,2011,2037]" pageId="6" pageNumber="7" value="115.0" valueMax="131.0" valueMin="99.0">99–131</number>
) long, 
<number box="[999,1029,2011,2037]" pageId="6" pageNumber="7" value="52.0">52</number>
