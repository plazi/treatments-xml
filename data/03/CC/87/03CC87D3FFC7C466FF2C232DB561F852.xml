<emphasis bold="true" box="[151,342,1666,1690]" pageId="7" pageNumber="23">FIGURES 15,16.</emphasis>
<taxonomicName box="[349,580,1667,1690]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="23" phylum="Arthropoda" rank="species" species="sarasini">
<emphasis box="[349,580,1667,1690]" italics="true" pageId="7" pageNumber="23">Lordomyrma sarasini</emphasis>
</taxonomicName>
, New 
<taxonomicName box="[650,773,1667,1690]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="23" phylum="Arthropoda" rank="species" species="caledonica">Caledonica</taxonomicName>
, HW 1.01mm, WL 1.42mm. 
<emphasis bold="true" box="[151,342,1701,1725]" pageId="7" pageNumber="23">FIGURES 17,18.</emphasis>
<taxonomicName box="[349,549,1702,1725]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="23" phylum="Arthropoda" rank="species" species="rouxi">
<emphasis box="[349,549,1702,1725]" italics="true" pageId="7" pageNumber="23">Lordomyrma rouxi</emphasis>
</taxonomicName>
, New 
<taxonomicName box="[620,743,1701,1724]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="23" phylum="Arthropoda" rank="species" species="caledonica">Caledonica</taxonomicName>
, HW 1.68mm, WL 2.01mm.
