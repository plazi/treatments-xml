<number box="[412,422,1205,1231]" pageId="8" pageNumber="9" value="1.0">1</number>
-St 
<number box="[457,471,1205,1231]" pageId="8" pageNumber="9" value="3.0">3</number>
<number box="[482,514,1205,1231]" pageId="8" pageNumber="9" value="61.0">61</number>
