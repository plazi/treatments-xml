<paragraph blockId="33.[188,1399,1334,1396]" pageId="33" pageNumber="34">
and 
<taxonomicName authorityName="Stoyanow" authorityYear="1910" box="[237,319,1334,1360]" class="Cephalopoda" family="Dzhulfitidae" genus="Paratirolites" kingdom="Animalia" order="Ammonoidea" pageId="33" pageNumber="34" phylum="Mollusca" rank="species" species="kittli">
