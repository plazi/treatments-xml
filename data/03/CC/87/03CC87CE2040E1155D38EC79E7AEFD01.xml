in 
<collectingRegion box="[269,356,672,693]" country="China" name="Zhejiang" pageId="6">Zhejiang</collectingRegion>
</materialsCitation>
, 
<materialsCitation location="Nanling Nature Reserve" pageId="6" specimenCount="1" stateProvince="Guangdong">
southwards to the 
