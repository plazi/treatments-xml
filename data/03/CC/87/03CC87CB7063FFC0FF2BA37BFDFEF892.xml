to 
<collectingMunicipality box="[846,1024,1407,1432]" pageId="14" pageNumber="307">central Ecuador</collectingMunicipality>
, ~ 
<quantity box="[1049,1188,1407,1432]" metricMagnitude="3" metricUnit="m" metricValue="1.0" metricValueMax="1.5" metricValueMin="0.5" pageId="14" pageNumber="307" unit="m" value="1000.0" valueMax="1500.0" valueMin="500.0">
