<emphasis bold="true" box="[199,315,583,608]" pageId="54" pageNumber="55">Remarks.</emphasis>
<taxonomicName box="[322,602,583,608]" class="Insecta" family="Pseudococcidae" genus="Formicococcus" kingdom="Animalia" order="Hemiptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="njalensis">
