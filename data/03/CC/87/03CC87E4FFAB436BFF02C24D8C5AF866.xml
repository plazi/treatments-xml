<collectionCode LSID="urn:lsid:biocol.org:col:1013" box="[673,754,2014,2038]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:1013" name="Essig Museum of Entomology" pageId="48" pageNumber="49">EMEC</collectionCode>
