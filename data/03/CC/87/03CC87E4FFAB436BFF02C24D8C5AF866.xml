<collectionCode LSID="urn:lsid:biocol.org:col:33156" box="[585,662,2014,2038]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33156" name="California State Collection of Arthropods" pageId="48" pageNumber="49">CSCA</collectionCode>
