<taxonomicName authority="Newstead" authorityName="Newstead" class="Insecta" family="Pseudococcidae" genus="Heterococcus" kingdom="Animalia" lastPageId="59" lastPageNumber="60" order="Hemiptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="pulverarius">
