<taxonomicName authority="(Chaudoir, 1867)" authorityName="Chaudoir" authorityYear="1867" baseAuthorityName="Chaudoir" baseAuthorityYear="1867" box="[282,746,1879,1901]" class="Insecta" family="Carabidae" genus="Craspedophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="247" phylum="Arthropoda" rank="species" species="hexagonus">
