<emphasis bold="true" box="[151,350,1644,1668]" pageId="11" pageNumber="12">FIGURES 25–28.</emphasis>
<taxonomicName box="[358,609,1645,1668]" class="Insecta" family="Rhagionidae" genus="Chrysopilus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="plaumanni" status="sp. n.">
<emphasis box="[358,609,1645,1668]" italics="true" pageId="11" pageNumber="12">Chrysopilus plaumanni</emphasis>
</taxonomicName>
<emphasis bold="true" box="[617,678,1644,1668]" pageId="11" pageNumber="12">
<taxonomicNameLabel box="[617,678,1644,1668]" pageId="11" pageNumber="12" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
