<subSubSection pageId="12" pageNumber="35" type="discussion">
<paragraph blockId="12.[835,1474,544,760]" pageId="12" pageNumber="35">
<emphasis box="[835,940,545,569]" italics="true" pageId="12" pageNumber="35">
<emphasis bold="true" box="[835,933,545,569]" italics="true" pageId="12" pageNumber="35">Remarks</emphasis>
.
</emphasis>
<emphasis bold="true" box="[945,958,544,568]" pageId="12" pageNumber="35">–</emphasis>
