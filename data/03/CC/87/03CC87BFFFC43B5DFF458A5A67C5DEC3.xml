<number box="[151,164,318,339]" pageId="2" pageNumber="590" value="9.0">9</number>
. 
<taxonomicName box="[225,333,318,339]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="2" pageNumber="596" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
black. Metasomal segment 
<number box="[616,628,318,339]" pageId="2" pageNumber="590" value="2.0">2</number>
