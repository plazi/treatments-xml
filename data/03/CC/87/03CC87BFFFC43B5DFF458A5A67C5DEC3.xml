<number box="[1155,1180,347,368]" pageId="2" pageNumber="590" value="41.0">41</number>
. 
<taxonomicName authority="Tsuneki" authorityName="Tsuneki" box="[1191,1429,347,368]" class="Insecta" family="Mutillidae" genus="Taiwanomyrme" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="590" phylum="Arthropoda" rank="genus">
