<number box="[670,684,1871,1897]" pageId="37" pageNumber="38" value="2.0">2</number>
<number box="[697,726,1871,1897]" pageId="37" pageNumber="38" value="71.0">71</number>
(
<number box="[735,815,1871,1897]" pageId="37" pageNumber="38" value="72.0" valueMax="74.0" valueMin="70.0">70–74</number>
). Genital shield smooth; distance between St 
<number box="[1375,1388,1871,1897]" pageId="37" pageNumber="38" value="5.0">5</number>
-St 
<number box="[1422,1435,1871,1897]" pageId="37" pageNumber="38" value="5.0">5</number>
<number box="[151,181,1911,1937]" pageId="37" pageNumber="38" value="71.0">71</number>
