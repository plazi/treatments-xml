<number box="[1326,1340,1946,1972]" pageId="29" pageNumber="30" value="4.0">4</number>
and Z 
<number box="[1423,1436,1946,1972]" pageId="29" pageNumber="30" value="5.0">5</number>
smooth; ratios s 
<number box="[345,363,1986,2012]" pageId="29" pageNumber="30" value="4.0">4</number>
:Z 
<number box="[389,400,1986,2012]" pageId="29" pageNumber="30" value="1.0">1</number>
