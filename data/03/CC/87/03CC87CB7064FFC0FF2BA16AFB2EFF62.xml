<paragraph blockId="9.[151,1437,1902,2035]" pageId="9" pageNumber="302">
<emphasis bold="true" box="[199,345,1938,1963]" pageId="9" pageNumber="302">Description.</emphasis>
Adult size ~ 
<quantity box="[490,633,1938,1963]" metricMagnitude="-1" metricUnit="m" metricValue="3.85" metricValueMax="4.1" metricValueMin="3.6" pageId="9" pageNumber="302" unit="mm" value="385.0" valueMax="410.0" valueMin="360.0">360–410mm</quantity>
SVL and ~ 
<quantity box="[764,908,1938,1963]" metricMagnitude="-1" metricUnit="m" metricValue="2.4" metricValueMax="2.5" metricValueMin="2.3" pageId="9" pageNumber="302" unit="mm" value="240.0" valueMax="250.0" valueMin="230.0">230–250mm</quantity>
