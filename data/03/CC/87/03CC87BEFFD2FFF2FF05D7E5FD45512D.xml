<specimenCount box="[780,799,1962,1986]" pageId="6" pageNumber="441" type="female">♀</specimenCount>
); forewing length: 
<quantity box="[1015,1136,1962,1986]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" metricValueMax="1.9" metricValueMin="1.7" pageId="6" pageNumber="441" unit="mm" value="18.0" valueMax="19.0" valueMin="17.0">17–19 mm</quantity>
(Ƌ), 
<quantity box="[1196,1274,1962,1986]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" pageId="6" pageNumber="441" unit="mm" value="18.0">18 mm</quantity>
(
<specimenCount box="[1292,1311,1962,1986]" pageId="6" pageNumber="441" type="female">♀</specimenCount>
); forewing width: 
<quantity box="[229,350,1998,2022]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" metricValueMax="1.4" metricValueMin="1.2" pageId="6" pageNumber="441" unit="mm" value="13.0" valueMax="14.0" valueMin="12.0">12–14 mm</quantity>
(Ƌ), 
<quantity box="[411,490,1998,2022]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" pageId="6" pageNumber="441" unit="mm" value="12.0">12 mm</quantity>
(
