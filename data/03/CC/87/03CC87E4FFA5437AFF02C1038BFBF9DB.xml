<taxonomicName authority="Thurber" authorityName="Thurber" box="[511,825,1298,1323]" class="Liliopsida" family="Poaceae" genus="Pleuraphis" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="rigida">
<emphasis box="[511,709,1298,1323]" italics="true" pageId="33" pageNumber="34">Pleuraphis rigida</emphasis>
(Thurber)
</taxonomicName>
(
<taxonomicName box="[840,935,1299,1323]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
