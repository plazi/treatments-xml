<taxonomicName box="[232,355,903,928]" class="Insecta" family="Hemiptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
