<collectingCountry box="[296,368,712,738]" name="United States Pacific Island Wildlife Refuges" pageId="24" pageNumber="25">Baker</collectingCountry>
, 
<number box="[379,440,712,738]" pageId="24" pageNumber="25" value="1962.0">1962</number>
</bibRefCitation>
</taxonomicName>
