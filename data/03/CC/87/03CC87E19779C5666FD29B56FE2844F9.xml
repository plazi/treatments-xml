<taxonomicName authority="soror" box="[1158,1320,1843,1866]" class="Insecta" family="Buprestidae" genus="Melobasis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="subSpecies" species="soror" subSpecies="soror">
