<taxonomicName authority="soror" box="[451,613,1771,1794]" class="Insecta" family="Buprestidae" genus="Melobasis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="subSpecies" species="soror" subSpecies="soror">
