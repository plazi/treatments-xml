<number box="[873,886,392,418]" pageId="4" pageNumber="5" value="5.0">5</number>
-St 
<number box="[921,934,392,418]" pageId="4" pageNumber="5" value="5.0">5</number>
[
<number box="[949,985,392,418]" pageId="4" pageNumber="5" value="74.0">74</number>
