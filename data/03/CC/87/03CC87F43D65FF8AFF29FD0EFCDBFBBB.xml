<number box="[520,550,472,498]" pageId="34" pageNumber="35" value="29.0">29</number>
(
<number box="[558,634,472,498]" pageId="34" pageNumber="35" value="29.0" valueMax="30.0" valueMin="28.0">28–30</number>
