<number box="[1142,1175,826,852]" pageId="33" pageNumber="34" value="34.0">34</number>
; setae z 
<number box="[1265,1280,826,852]" pageId="33" pageNumber="34" value="2.0">2</number>
and z 
<number box="[1348,1363,826,852]" pageId="33" pageNumber="34" value="4.0">4</number>
