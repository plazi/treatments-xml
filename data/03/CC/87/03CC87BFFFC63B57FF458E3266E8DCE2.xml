<taxonomicName authority="Chen, 1957" authorityName="Chen" authorityYear="1957" box="[886,1185,764,789]" class="Insecta" family="Mutillidae" genus="Smicromyrme" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="596" phylum="Arthropoda" rank="species" species="impressus">
