<taxonomicName box="[588,842,429,454]" class="Magnoliopsida" family="Malvaceae" genus="Dombeya" kingdom="Plantae" order="Malvales" pageId="8" pageNumber="29" phylum="Tracheophyta" rank="species" species="emarginata">
<emphasis box="[588,842,429,454]" italics="true" pageId="8" pageNumber="29">Dombeya emarginata</emphasis>
</taxonomicName>
(
<taxonomicName box="[857,987,429,455]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="8" pageNumber="29" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
) (
<bibRefCitation author="Meyrick" box="[1010,1182,429,455]" pageId="8" pageNumber="29" refString="Meyrick, E. (1936 - 1937) Exotic Microlepidoptera 5 (1 - 2). Marlborough. pp. 1 - 64." type="journal article" year="1936">
Meyrick 
<number box="[1119,1182,429,455]" pageId="8" pageNumber="29" value="1936.0">1936</number>
</bibRefCitation>
).
</paragraph>
</subSubSection>
