<collectingCountry box="[621,673,615,637]" name="India" pageId="14" pageNumber="1563">India</collectingCountry>
and 
<collectingCountry box="[723,824,616,637]" name="Myanmar" pageId="14" pageNumber="1563">Myanmar</collectingCountry>
