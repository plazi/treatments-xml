<collectingCountry box="[946,1039,621,643]" name="Malaysia" pageId="17" pageNumber="1566">Malaysia</collectingCountry>
and 
<collectingCountry box="[178,230,651,672]" name="Laos" pageId="17" pageNumber="1566">Laos</collectingCountry>
