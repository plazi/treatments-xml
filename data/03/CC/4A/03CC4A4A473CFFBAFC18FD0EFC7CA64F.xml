</taxonomicName>
, and 
<taxonomicName authorityName="Thomson" authorityYear="1870" box="[1141,1288,892,915]" class="Insecta" family="Mutillidae" genus="Smicromyrme" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="480" phylum="Arthropoda" rank="genus" sensu="stricto">
