<number box="[423,451,1894,1918]" pageId="33" pageNumber="34" value="92.0">92</number>
(
<number box="[469,555,1893,1918]" pageId="33" pageNumber="34" value="85.0" valueMax="110.0" valueMin="60.0">60–110</number>
) × 
