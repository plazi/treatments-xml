<subSubSection box="[151,934,726,751]" pageId="32" pageNumber="33" type="materials_examined">
<paragraph blockId="32.[151,1437,726,2011]" box="[151,934,726,751]" pageId="32" pageNumber="33">
<emphasis bold="true" box="[151,306,726,751]" pageId="32" pageNumber="33">
<typeStatus box="[151,210,726,751]" pageId="32" pageNumber="33">Type</typeStatus>
species.
</emphasis>
<taxonomicName box="[313,527,726,751]" class="Demospongiae" family="Latrunculiidae" genus="Latrunclava" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="imago">
