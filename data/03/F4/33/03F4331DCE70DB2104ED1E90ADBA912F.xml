(
<number box="[487,534,872,898]" pageId="8" pageNumber="63" value="250.0">250</number>
) long, 
<number box="[625,728,872,898]" pageId="8" pageNumber="63" value="215.0" valueMax="250.0" valueMin="180.0">180–250</number>
(
<number box="[747,794,872,898]" pageId="8" pageNumber="63" value="220.0">220</number>
) wide. Interlabia 
<number box="[1013,1101,872,898]" pageId="8" pageNumber="63" value="113.5" valueMax="130.0" valueMin="97.0">97–130</number>
(
<number box="[1121,1167,872,898]" pageId="8" pageNumber="63" value="120.0">120</number>
) long, 
<number box="[1259,1361,872,898]" pageId="8" pageNumber="63" value="160.0" valueMax="190.0" valueMin="130.0">130–190</number>
(
<number box="[1381,1427,872,898]" pageId="8" pageNumber="63" value="170.0">170</number>
) wide. Oesophagus 
<number box="[376,494,912,938]" pageId="8" pageNumber="63" value="4.42" valueMax="5.83" valueMin="3.01">3.01–5.83</number>
(
<number box="[511,565,912,938]" pageId="8" pageNumber="63" value="4.31">4.31</number>
