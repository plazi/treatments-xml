(
<number box="[1381,1428,952,978]" pageId="8" pageNumber="63" value="290.0">290</number>
) long by 
<number box="[247,350,992,1018]" pageId="8" pageNumber="63" value="290.0" valueMax="390.0" valueMin="190.0">190–390</number>
(
<number box="[365,412,992,1018]" pageId="8" pageNumber="63" value="290.0">290</number>
) wide. Ventricular appendix 
<number box="[752,870,992,1018]" pageId="8" pageNumber="63" value="1.36" valueMax="1.8" valueMin="0.92">0.92–1.80</number>
(
<number box="[885,939,992,1018]" pageId="8" pageNumber="63" value="1.36">1.36</number>
) mm long, 
<number box="[1075,1177,992,1018]" pageId="8" pageNumber="63" value="140.0" valueMax="170.0" valueMin="110.0">110–170</number>
(
<number box="[1193,1239,992,1018]" pageId="8" pageNumber="63" value="140.0">140</number>
) wide. Intestinal caecum 
<number box="[249,366,1032,1058]" pageId="8" pageNumber="63" value="1.96" valueMax="2.43" valueMin="1.49">1.49–2.43</number>
(
<number box="[383,436,1032,1058]" pageId="8" pageNumber="63" value="1.88">1.88</number>
) mm long, 
<number box="[576,694,1032,1058]" pageId="8" pageNumber="63" value="45.05" valueMax="48.4" valueMin="41.7">41.7–48.4</number>
(
<number box="[711,765,1032,1058]" pageId="8" pageNumber="63" value="44.3">44.3</number>
) % of oesophagus length; 
<number box="[1083,1185,1032,1058]" pageId="8" pageNumber="63" value="155.0" valueMax="180.0" valueMin="130.0">130–180</number>
(
