<number box="[684,772,1072,1098]" pageId="8" pageNumber="63" value="0.7" valueMax="0.8" valueMin="0.6">0.6–0.8</number>
(
<number box="[795,809,1072,1098]" pageId="8" pageNumber="63" value="1.0">1</number>
: 
<number box="[824,865,1072,1098]" pageId="8" pageNumber="63" value="0.7">0.7</number>
). Tail length 
<number box="[1024,1127,1072,1098]" pageId="8" pageNumber="63" value="410.0" valueMax="470.0" valueMin="350.0">350–470</number>
(
<number box="[1145,1195,1072,1098]" pageId="8" pageNumber="63" value="420.0">420</number>
); tail tip 
<number box="[1304,1378,1072,1098]" pageId="8" pageNumber="63" value="63.5" valueMax="78.0" valueMin="49.0">49–78</number>
