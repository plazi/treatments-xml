(
<number box="[338,385,952,978]" pageId="8" pageNumber="63" value="950.0">950</number>
) and excretory pore 
<number box="[626,743,952,978]" pageId="8" pageNumber="63" value="1045.0" valueMax="1120.0" valueMin="970.0">970–1120</number>
(
