<number box="[624,747,1497,1523]" pageId="12" pageNumber="13" value="0.33999999999999997" valueMax="0.38" valueMin="0.3">0.30–0.38</number>
, 
<quantity box="[759,868,1497,1523]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="12" pageNumber="13" unit="mm" value="0.34">0.34 mm</quantity>
