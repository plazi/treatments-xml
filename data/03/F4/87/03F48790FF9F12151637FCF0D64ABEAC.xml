, 
<quantity box="[1199,1308,1337,1363]" metricMagnitude="-5" metricUnit="m" metricValue="9.0" pageId="12" pageNumber="13" unit="mm" value="0.09">0.09 mm</quantity>
), length/width 
<number box="[420,459,1377,1403]" pageId="12" pageNumber="13" value="0.7">0.7</number>
, (
<number box="[481,520,1377,1403]" pageId="12" pageNumber="13" value="0.7">0.7</number>
