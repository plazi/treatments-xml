<paragraph blockId="17.[813,1420,1895,2091]" pageId="17" pageNumber="668">
<emphasis bold="true" box="[813,1101,1895,1920]" pageId="17" pageNumber="668">Distribution and ecology.</emphasis>
<collectingCountry box="[1107,1215,1896,1920]" name="Colombia" pageId="17" pageNumber="668">Colombia</collectingCountry>
, 
<collectingCountry box="[1225,1346,1896,1920]" name="Costa Rica" pageId="17" pageNumber="668">Costa Rica</collectingCountry>
, 
<collectingCountry name="Ecuador" pageId="17" pageNumber="668">Ecuador</collectingCountry>
, 
<collectingCountry box="[861,969,1930,1954]" name="Honduras" pageId="17" pageNumber="668">Honduras</collectingCountry>
