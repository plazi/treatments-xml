<collectingCountry box="[980,1095,1930,1954]" name="Nicaragua" pageId="17" pageNumber="668">Nicaragua</collectingCountry>
, and 
<collectingCountry box="[1152,1241,1930,1953]" name="Panama" pageId="17" pageNumber="668">Panama</collectingCountry>
