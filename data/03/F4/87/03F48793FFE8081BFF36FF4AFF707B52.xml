<quantity box="[1231,1362,1199,1226]" metricMagnitude="2" metricUnit="m" metricValue="5.475" metricValueMax="6.45" metricValueMin="4.5" pageId="9" pageNumber="360" unit="m" value="547.5" valueMax="645.0" valueMin="450.0">
