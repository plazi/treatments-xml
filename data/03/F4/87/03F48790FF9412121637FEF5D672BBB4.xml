<subSubSection box="[264,686,283,310]" pageId="7" pageNumber="8" type="nomenclature">
<paragraph blockId="7.[264,686,283,310]" box="[264,686,283,310]" pageId="7" pageNumber="8">
<heading bold="true" box="[264,686,283,310]" fontSize="11" level="1" pageId="7" pageNumber="8" reason="1">
<emphasis bold="true" box="[264,686,283,310]" pageId="7" pageNumber="8">
<taxonomicName authority="Webb" authorityName="Webb" box="[264,558,283,310]" class="Insecta" family="Therevidae" genus="Nigranitida" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="gibba" status="spec. nov.">
<emphasis bold="true" box="[264,478,284,310]" italics="true" pageId="7" pageNumber="8">Nigranitida gibba</emphasis>
Webb
</taxonomicName>
<taxonomicNameLabel box="[566,686,283,309]" pageId="7" pageNumber="8" rank="species">spec. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
