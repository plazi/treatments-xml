and 
<taxonomicName authority="(Kieffer, 1911)" baseAuthorityName="Kieffer" baseAuthorityYear="1911" box="[438,770,943,969]" class="Insecta" family="Chironomidae" genus="Eukiefferiella" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="235" phylum="Arthropoda" rank="species" species="brevicalcar">
