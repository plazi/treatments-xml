<taxonomicName authorityName="Darevsky &amp; Szczerbak" authorityYear="1997" box="[632,750,1484,1510]" class="Reptilia" family="Gekkonidae" genus="Gonydactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="157" phylum="Chordata" rank="species" species="paradoxus">
