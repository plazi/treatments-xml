; 
<emphasis box="[591,775,1695,1719]" italics="true" pageId="14" pageNumber="665">
<collectionCode box="[591,613,1696,1719]" country="Norway" lsid="urn:lsid:biocol.org:col:13093" name="Botanical Museum - University of Oslo" pageId="14" pageNumber="665" type="Herbarium">O</collectionCode>
