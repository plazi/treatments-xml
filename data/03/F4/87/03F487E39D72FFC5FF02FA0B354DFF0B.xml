<quantity box="[454,575,1593,1617]" metricMagnitude="2" metricUnit="m" metricValue="6.55" metricValueMax="7.6" metricValueMin="5.5" pageId="14" pageNumber="665" unit="m" value="655.0" valueMax="760.0" valueMin="550.0">550–760 m</quantity>
