</taxonomicName>
аnd 
<taxonomicName authority="Eschscholtz, 1829" authorityName="Eschscholtz" authorityYear="1829" box="[818,998,1757,1781]" class="Insecta" family="Tenebrionidae" genus="Prosodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="457" phylum="Arthropoda" rank="species" species="chorassanica">
<emphasis box="[818,998,1757,1781]" italics="true" pageId="6" pageNumber="457">P. chorassanica</emphasis>
</taxonomicName>
