<taxonomicName authority="Loew" authorityName="Loew" class="Insecta" family="Pipunculidae" genus="Tomosvaryella" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="495" phylum="Arthropoda" rank="species" species="subvirescens">
