<collectingCountry box="[585,693,1080,1104]" name="Colombia" pageId="34" pageNumber="685">Colombia</collectingCountry>
, 
<collectingCountry name="Costa Rica" pageId="34" pageNumber="685">Costa Rica</collectingCountry>
