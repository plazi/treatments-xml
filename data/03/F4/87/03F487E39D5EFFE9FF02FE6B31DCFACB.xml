<specimenCount box="[350,427,1215,1239]" pageId="34" pageNumber="685" type="generic">
<quantity box="[350,423,1215,1239]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="34" pageNumber="685" unit="m" value="800.0">800 m</quantity>
