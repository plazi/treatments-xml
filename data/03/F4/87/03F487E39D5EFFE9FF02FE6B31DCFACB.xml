<collectingCountry box="[235,366,1113,1137]" name="El Salvador" pageId="34" pageNumber="685">El Salvador</collectingCountry>
, 
<collectingCountry box="[380,501,1113,1137]" name="Guatemala" pageId="34" pageNumber="685">Guatemala</collectingCountry>
, 
<collectingCountry box="[515,623,1113,1137]" name="Honduras" pageId="34" pageNumber="685">Honduras</collectingCountry>
