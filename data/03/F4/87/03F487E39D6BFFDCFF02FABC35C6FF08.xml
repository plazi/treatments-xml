, 
<collectingCountry box="[546,667,1910,1934]" name="Guatemala" pageId="23" pageNumber="674">Guatemala</collectingCountry>
, 
<collectingCountry box="[684,771,1910,1934]" name="Guyana" pageId="23" pageNumber="674">Guyana</collectingCountry>
