<collectingCountry box="[168,276,1910,1934]" name="Colombia" pageId="23" pageNumber="674">Colombia</collectingCountry>
, 
<collectingCountry box="[293,420,1910,1934]" name="Costa Rica" pageId="23" pageNumber="674">Costa Rica</collectingCountry>
