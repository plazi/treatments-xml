<number box="[858,905,1644,1670]" pageId="11" pageNumber="12" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
. 
<taxonomicName box="[916,1041,1644,1669]" class="Magnoliopsida" family="Combretaceae" genus="Terminalia" kingdom="Plantae" order="Myrtales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="genus">
<emphasis box="[916,1041,1644,1669]" italics="true" pageId="11" pageNumber="12">Terminalia</emphasis>
