<collectionCode LSID="urn:lsid:biocol.org:col:34700" box="[578,635,364,390]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="12" pageNumber="13">CAS</collectionCode>
(
<typeStatus box="[651,711,364,390]" pageId="12" pageNumber="13">Type</typeStatus>
