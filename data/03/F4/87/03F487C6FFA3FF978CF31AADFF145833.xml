<quantity box="[922,1025,548,572]" metricMagnitude="-1" metricUnit="m" metricValue="5.461" metricValueMax="5.588" metricValueMin="5.334" pageId="4" pageNumber="64" unit="in" value="21.5" valueMax="22.0" valueMin="21.0">21–22 in</quantity>
number in 
<taxonomicName box="[1167,1331,548,572]" class="Adenophorea" family="Cyatholaimidae" genus="Paracanthonchus" kingdom="Animalia" order="Desmodorida" pageId="4" pageNumber="64" phylum="Nematoda" rank="species" species="multitubifer">
