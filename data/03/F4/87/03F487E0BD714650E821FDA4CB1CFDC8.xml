<emphasis box="[113,200,573,596]" italics="true" pageId="21" pageNumber="22">Populus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[253,306,573,596]" class="Magnoliopsida" family="Salicaceae" genus="Salix" kingdom="Plantae" order="Malpighiales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="genus">
<emphasis box="[253,306,573,596]" italics="true" pageId="21" pageNumber="22">Salix</emphasis>
