<geoCoordinate box="[829,968,306,332]" degrees="21" direction="north" minutes="42" orientation="latitude" pageId="11" pageNumber="654" precision="15" seconds="10" value="21.702778">21°42'10&quot;N</geoCoordinate>
<geoCoordinate box="[975,1124,306,332]" degrees="158" direction="west" minutes="0" orientation="longitude" pageId="11" pageNumber="654" precision="15" seconds="40" value="-158.01111">158°0'40&quot;W</geoCoordinate>
