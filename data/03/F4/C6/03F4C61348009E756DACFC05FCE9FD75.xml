<quantity box="[971,1033,1843,1860]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="11" pageNumber="222" unit="mm" value="3.5">3,5 mm</quantity>
(56-57), 
<quantity box="[1107,1154,1843,1860]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="11" pageNumber="222" unit="mm" value="4.0">4 mm</quantity>
