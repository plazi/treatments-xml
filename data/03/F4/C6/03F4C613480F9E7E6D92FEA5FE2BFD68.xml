<location LSID="urn:lsid:plazi:treatment:03F4C613480F9E7E6D92FEA5FE2BFD68:8E8221DE480F9E7C6CF6FB9FFD88FBC1" box="[406,549,1046,1069]" country="Liberia" county="Grand Gedah County" name="Balloon creek" pageId="5" pageNumber="216" stateProvince="Margibi">Balloon creek</location>
, 
<collectingCounty box="[559,783,1046,1069]" pageId="5" pageNumber="216">Grand Gedah County</collectingCounty>
