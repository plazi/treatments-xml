<emphasis box="[1190,1327,295,320]" italics="true" pageId="7" pageNumber="283">
<taxonomicName authorityName="Sasa" authorityYear="1961" box="[1190,1323,295,320]" class="Arachnida" family="Pygmephoridae" genus="Bakerdania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="283" phylum="Arthropoda" rank="genus">Bakerdania</taxonomicName>
,
