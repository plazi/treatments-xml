<paragraph blockId="19.[151,1436,151,418]" box="[197,1117,151,178]" pageId="19" pageNumber="20">
<emphasis bold="true" box="[197,328,151,177]" pageId="19" pageNumber="20">
<typeStatus box="[197,258,151,177]" pageId="19" pageNumber="20">Type</typeStatus>
data.
</emphasis>
<collectingCountry box="[336,455,152,178]" name="Mexico" pageId="19" pageNumber="20">MEXICO</collectingCountry>
, Aguas Calientes, ex 
<taxonomicName box="[707,810,152,177]" class="Magnoliopsida" family="Fabaceae" genus="Prosopis" kingdom="Animalia" order="Fabales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="genus">
