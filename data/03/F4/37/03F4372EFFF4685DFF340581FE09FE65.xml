<typeStatus box="[396,455,368,392]" pageId="19" pageNumber="420">types</typeStatus>
of 
<taxonomicName authority="" authorityName="" box="[492,676,367,392]" class="Gastropoda" family="Melanodrymiidae" genus="Paskentana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeogastropoda" pageId="19" pageNumber="420" phylum="Mollusca" rank="species" species="paskentaensis">
