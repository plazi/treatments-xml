<paragraph blockId="12.[151,1413,448,501]" pageId="12" pageNumber="560">
<typeStatus box="[151,200,449,470]" pageId="12" pageNumber="560">Type</typeStatus>
species: 
<taxonomicName authority="Martin, 1999: 70" authorityName="Martin" authorityPageNumber="70" authorityYear="1999" box="[294,705,448,470]" class="Insecta" family="Aleyrodidae" genus="Dialeurolonga" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="560" phylum="Arthropoda" rank="species" species="swainei">
<emphasis box="[294,526,448,470]" italics="true" pageId="12" pageNumber="560">Dialeurolonga swainei</emphasis>
Martin, 
<number box="[613,667,448,469]" pageId="12" pageNumber="560" value="1999.0">1999</number>
: 
<number box="[677,705,448,469]" pageId="12" pageNumber="560" value="70.0">70</number>
</taxonomicName>
. 
<typeStatus box="[715,809,448,470]" pageId="12" pageNumber="560">Holotype</typeStatus>
puparium. 
<collectingCountry box="[926,1023,448,470]" name="Australia" pageId="12" pageNumber="560">Australia</collectingCountry>
: Queenland, Broadbeach, viii-ix. 
<number box="[1357,1409,448,470]" pageId="12" pageNumber="560" value="1970.0">1970</number>
, G. Swaine, on 
<taxonomicName box="[345,401,480,501]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="12" pageNumber="560" phylum="Mollusca" rank="genus">
