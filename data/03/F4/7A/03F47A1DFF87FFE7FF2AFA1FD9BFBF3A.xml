<subSubSection box="[199,603,1052,1077]" pageId="6" pageNumber="293" type="distribution">
<paragraph blockId="6.[151,1437,151,1653]" box="[199,603,1052,1077]" pageId="6" pageNumber="293">
<emphasis bold="true" box="[199,352,1052,1077]" pageId="6" pageNumber="293">Distribution.</emphasis>
South-central 
<collectingCountry box="[517,599,1052,1077]" name="Turkey" pageId="6" pageNumber="293">Turkey</collectingCountry>
