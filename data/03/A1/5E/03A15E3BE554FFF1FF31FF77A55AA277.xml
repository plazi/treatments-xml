<taxonomicName authority="Griffini" authorityName="Griffini" authorityYear="1893" box="[1190,1432,258,283]" class="Insecta" family="Stenopelmatidae" genus="Stenopelmatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="407" phylum="Arthropoda" rank="species" species="calcaratus">
<emphasis box="[1190,1337,259,283]" italics="true" pageId="6" pageNumber="407">S. calcaratus</emphasis>
