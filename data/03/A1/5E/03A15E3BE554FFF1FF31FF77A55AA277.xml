<taxonomicName authority="Burmeister" authorityName="Burmeister" box="[638,856,294,319]" class="Insecta" family="Stenopelmatidae" genus="Stenopelmatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="407" phylum="Arthropoda" rank="species" species="talpa">
