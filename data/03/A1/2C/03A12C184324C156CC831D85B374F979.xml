<collectionCode LSID="urn:lsid:biocol.org:col:34797" box="[552,627,1634,1660]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="2" pageNumber="3">INHS</collectionCode>
