<paragraph blockId="10.[264,1323,283,470]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[312,412,323,349]" pageId="10" pageNumber="11">Thorax.</emphasis>
Acrostichals 
<number box="[571,648,324,350]" pageId="10" pageNumber="11" value="13.0" valueMax="16.0" valueMin="10.0">10–16</number>
; dorsocentrals 
<number box="[822,884,324,350]" pageId="10" pageNumber="11" value="8.5" valueMax="10.0" valueMin="7.0">7–10</number>
; prealars 
<number box="[995,1010,324,350]" pageId="10" pageNumber="11" value="1.0">1</number>
