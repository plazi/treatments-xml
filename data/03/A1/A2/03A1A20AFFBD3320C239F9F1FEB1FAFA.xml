<number box="[1197,1274,1826,1852]" pageId="9" pageNumber="10" value="36.0" valueMax="40.0" valueMin="32.0">32–40</number>
; 
<number box="[1285,1317,1826,1852]" pageId="9" pageNumber="10" value="36.0">36</number>
; 
<number box="[264,297,1866,1892]" pageId="9" pageNumber="10" value="96.0">96</number>
