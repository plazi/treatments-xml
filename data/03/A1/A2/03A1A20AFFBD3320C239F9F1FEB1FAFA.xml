<paragraph blockId="9.[264,1323,1626,1892]" pageId="9" pageNumber="10">
Total length 
<quantity box="[466,643,1666,1692]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" metricValueMax="2.4" metricValueMin="2.2" pageId="9" pageNumber="10" unit="mm" value="2.3" valueMax="2.4" valueMin="2.2">2.20–2.40 mm</quantity>
