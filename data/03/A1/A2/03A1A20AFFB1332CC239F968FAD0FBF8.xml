<number box="[369,402,1845,1871]" pageId="5" pageNumber="6" value="40.0">40</number>
; 
<number box="[413,445,1845,1871]" pageId="5" pageNumber="6" value="36.0">36</number>
; 
<number box="[458,491,1845,1871]" pageId="5" pageNumber="6" value="96.0">96</number>
; 
<number box="[503,549,1845,1871]" pageId="5" pageNumber="6" value="110.0">110</number>
