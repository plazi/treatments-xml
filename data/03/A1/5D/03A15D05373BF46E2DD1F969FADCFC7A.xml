<number box="[561,599,626,652]" pageId="18" pageNumber="19" value="2.6">2.6</number>
; POL/OOL/POO = 
<number box="[834,873,626,652]" pageId="18" pageNumber="19" value="7.4">7.4</number>
/ 
<number box="[882,917,626,652]" pageId="18" pageNumber="19" value="1.8">1.8</number>
/1.0; WH/WT = 
<number box="[1109,1149,626,652]" pageId="18" pageNumber="19" value="1.2">1.2</number>
; LW/LM/HW = 
<number box="[1346,1381,626,652]" pageId="18" pageNumber="19" value="1.8">1.8</number>
/1.0/ 
<number box="[151,191,663,689]" pageId="18" pageNumber="19" value="1.4">1.4</number>
; PM/ST = 
<number box="[319,359,663,689]" pageId="18" pageNumber="19" value="0.8">0.8</number>
; MM/LG = 
<number box="[501,541,663,689]" pageId="18" pageNumber="19" value="1.4">1.4</number>
.
</paragraph>
<paragraph blockId="18.[151,1437,152,908]" box="[196,417,700,726]" pageId="18" pageNumber="19">MALE. Unknown.</paragraph>
