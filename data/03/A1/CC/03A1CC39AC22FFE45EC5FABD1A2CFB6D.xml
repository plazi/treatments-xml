<subSubSection lastPageId="19" lastPageNumber="20" pageId="17" pageNumber="18" type="description">
<paragraph blockId="17.[151,1437,1508,2039]" box="[199,746,1796,1823]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[199,373,1796,1822]" pageId="17" pageNumber="18">Redescription.</emphasis>
Body length: 
<quantity box="[532,639,1796,1823]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="17" pageNumber="18" unit="mm" value="9.0" valueMax="10.0" valueMin="8.0">8–10 mm</quantity>
(n 
<emphasis box="[676,695,1797,1822]" italics="true" pageId="17" pageNumber="18">=</emphasis>
