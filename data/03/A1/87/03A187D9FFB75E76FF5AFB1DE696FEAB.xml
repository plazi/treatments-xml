<taxonomicName authority="Danzig" authorityName="Danzig" box="[151,419,1198,1224]" class="Insecta" family="Aleyrodidae" genus="Dialeurolobus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="179" phylum="Arthropoda" rank="genus">
