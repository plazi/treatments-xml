<specimenCount box="[976,1090,1218,1244]" pageId="4" pageNumber="53" type="female">
<number box="[976,990,1218,1244]" pageId="4" pageNumber="53" value="3.0">3</number>
females
</specimenCount>
(
<collectionCode box="[1107,1200,1218,1244]" httpUri="http://grbio.org/cool/15kb-kde6" name="Museum of New Zealand Te Papa Tongarewa - Entomology" pageId="4" pageNumber="53">MONZ</collectionCode>
), South Island, near Discovery Stream, Hawdon Hut, 
<date box="[541,690,1258,1284]" pageId="4" pageNumber="53" value="1978-09-02">
<number box="[541,556,1258,1284]" pageId="4" pageNumber="53" value="2.0">2</number>
Sept. 
<number box="[629,690,1258,1284]" pageId="4" pageNumber="53" value="1978.0">1978</number>
