<quantity box="[999,1153,1287,1313]" metricMagnitude="-3" metricUnit="m" metricValue="2.55" metricValueMax="2.7" metricValueMin="2.4" pageId="2" pageNumber="63" unit="mm" value="2.55" valueMax="2.7" valueMin="2.4">2.4–2.7 mm</quantity>
