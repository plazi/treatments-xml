<quantity box="[1050,1132,655,679]" metricMagnitude="-2" metricUnit="m" metricValue="2.2" pageId="1" pageNumber="94" unit="mm" value="22.0">22 mm</quantity>
; ♀ 
<quantity box="[1173,1297,654,679]" metricMagnitude="-2" metricUnit="m" metricValue="2.4" metricValueMax="2.5" metricValueMin="2.3" pageId="1" pageNumber="94" unit="mm" value="24.0" valueMax="25.0" valueMin="23.0">23–25 mm</quantity>
. 
<collectingCountry box="[1308,1364,654,679]" name="India" pageId="1" pageNumber="94">Hind</collectingCountry>
wing: ♂ 
<quantity box="[179,261,691,715]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="1" pageNumber="94" unit="mm" value="20.0">20 mm</quantity>
; ♀ 
<quantity box="[300,424,691,715]" metricMagnitude="-2" metricUnit="m" metricValue="2.25" metricValueMax="2.3" metricValueMin="2.2" pageId="1" pageNumber="94" unit="mm" value="22.5" valueMax="23.0" valueMin="22.0">22–23 mm</quantity>
