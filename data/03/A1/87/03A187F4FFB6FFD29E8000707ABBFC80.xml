<caption httpUri="https://zenodo.org/record/199994/files/figure.png" pageId="21" pageNumber="22" targetBox="[181,1411,219,1657]" targetPageId="21">
<paragraph blockId="21.[151,1436,1658,1746]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[151,298,1658,1682]" pageId="21" pageNumber="22">FIGURE 12.</emphasis>
<taxonomicName box="[308,526,1659,1682]" class="Gastropoda" family="Enidae" genus="Pupopsis" kingdom="Animalia" order="Stylommatophora" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="yengiawat" status="sp. nov.">
<emphasis box="[308,526,1659,1682]" italics="true" pageId="21" pageNumber="22">Pupopsis yengiawat</emphasis>
