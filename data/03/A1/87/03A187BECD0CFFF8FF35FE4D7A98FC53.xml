<typeStatus box="[947,1006,771,795]" pageId="8" pageNumber="253">types</typeStatus>
of 
<taxonomicName authorityName=": Jeekel" authorityYear="1963" box="[1045,1214,770,795]" class="Diplopoda" family="Platyrhacidae" genus="Platyrhacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="253" phylum="Arthropoda" rank="species" species="broelemanni">
<emphasis box="[1045,1214,770,795]" italics="true" pageId="8" pageNumber="253">P. broelemanni</emphasis>
</taxonomicName>
. 
<location LSID="urn:lsid:plazi:treatment:03A187BECD0CFFF8FF35FE4D7A98FC53:8ED76073CD0CFFF8FB6CFC9A7FBBFCE3" box="[1230,1274,770,795]" name="The" pageId="8" pageNumber="253">The</location>
other, cat. no
</materialsCitation>
