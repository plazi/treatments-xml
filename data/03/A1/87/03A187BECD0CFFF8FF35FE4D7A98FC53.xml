<materialsCitation ID-GBIF-Occurrence="1915274036" collectionCode="ZMB" collectorName="System der Polydesmiden" country="Ecuador" location="Richard Haensch" municipality="Attems" pageId="8" pageNumber="253" specimenCode="ZMB 5173" specimenCount="1" typeStatus="holotype">
