, holotype, ♂; 18, 
<taxonomicName authorityName=", Park &amp; Karsholt" authorityYear="1999" box="[1024,1176,1342,1366]" class="Insecta" family="Gelechiidae" genus="Psoricoptera" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="260" phylum="Arthropoda" rank="species" species="latignathosa">
<emphasis box="[1024,1176,1342,1366]" italics="true" pageId="7" pageNumber="260">P. latignathosa</emphasis>
</taxonomicName>
, ♀; 19–20, 
<taxonomicName authorityName="Zeller" authorityYear="1839" box="[1299,1430,1342,1366]" class="Insecta" family="Gelechiidae" genus="Gelechia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="260" phylum="Arthropoda" rank="species" species="gibbosella">
<emphasis box="[1299,1430,1342,1366]" italics="true" pageId="7" pageNumber="260">P. gibbosella</emphasis>
</taxonomicName>
, ♂; 21–22, 
<taxonomicName authority="Zheng &amp; Li, 2021" authorityName="Zheng &amp; Li" authorityYear="2021" box="[260,419,1378,1402]" class="Insecta" family="Gelechiidae" genus="Psoricoptera" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="260" phylum="Arthropoda" rank="species" species="minutignatha" status="sp. nov.">
