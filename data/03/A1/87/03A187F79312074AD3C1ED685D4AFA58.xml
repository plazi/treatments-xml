, 
<taxonomicName authority="Forel. Several" authorityName="Forel. Several" box="[540,1006,1442,1467]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subSpecies" species="sculpturata" subSpecies="zambeziana">
