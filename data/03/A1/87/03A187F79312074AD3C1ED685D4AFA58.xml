, 
<taxonomicName authority="Forel" authorityName="Forel" authorityYear="1894" box="[283,635,1406,1431]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subSpecies" species="sculpturata" subSpecies="berthoudi">
