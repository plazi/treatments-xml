, 
<taxonomicName authority="Forel" authorityName="Forel" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subSpecies" species="sculpturata" subSpecies="welgelengensis">
