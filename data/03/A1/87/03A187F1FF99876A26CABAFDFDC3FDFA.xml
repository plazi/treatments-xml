<specimenCount box="[1194,1243,398,422]" pageId="21" pageNumber="472" type="male">32♂</specimenCount>
; 
<specimenCount box="[1254,1304,398,422]" pageId="21" pageNumber="472" type="female">30♀</specimenCount>
