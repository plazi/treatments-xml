<location LSID="urn:lsid:plazi:treatment:03A187BEFFB0FF97FF51CD0DFF46C83E:8ED76073FFB6FF97FBB6CA55FB49C932" box="[1136,1232,1087,1112]" country="Egypt" county="Athias-Henriot" municipality="In" name="Halliday" pageId="21" pageNumber="500">Halliday</location>
