18, 
<emphasis box="[750,802,1578,1602]" italics="true" pageId="19" pageNumber="498">UR1</emphasis>
17, 
<emphasis box="[851,902,1578,1602]" italics="true" pageId="19" pageNumber="498">UR4</emphasis>
16, 
