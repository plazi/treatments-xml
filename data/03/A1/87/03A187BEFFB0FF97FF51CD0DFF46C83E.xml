<collectingRegion box="[1348,1432,871,896]" country="United States of America" name="Florida" pageId="21" pageNumber="500">Florida</collectingRegion>
, 
<collectingCountry box="[151,211,908,932]" name="United States of America" pageId="21" pageNumber="500">USA</collectingCountry>
