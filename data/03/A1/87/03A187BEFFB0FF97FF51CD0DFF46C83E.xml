<location LSID="urn:lsid:plazi:treatment:03A187BEFFB0FF97FF51CD0DFF46C83E:8ED76073FFB6FF97FD68CDC5FD43CEA2" box="[686,730,943,968]" country="Egypt" county="Athias-Henriot" municipality="In" name="The" pageId="21" pageNumber="500">The</location>
