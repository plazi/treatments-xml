<quantity box="[270,348,278,303]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" metricValueMax="7.0" metricValueMin="2.0" pageId="4" pageNumber="147" unit="cm" value="4.5" valueMax="7.0" valueMin="2.0">2–7 cm</quantity>
