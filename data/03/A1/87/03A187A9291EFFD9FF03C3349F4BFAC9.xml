</taxonomicName>
with frons not demarcated from vertex; BL 
<quantity box="[990,1093,1740,1765]" metricMagnitude="-4" metricUnit="m" metricValue="6.4" pageId="39" pageNumber="40" unit="mm" value="0.64">0.64 mm</quantity>
