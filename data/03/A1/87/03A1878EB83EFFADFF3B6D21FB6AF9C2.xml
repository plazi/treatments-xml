<paragraph blockId="1.[151,732,1615,1676]" box="[151,267,1651,1676]" pageId="1" pageNumber="127">
(
<figureCitation box="[159,258,1651,1676]" captionStart-0="FIGURES 1 – 5" captionStart-1="FIGURE 6 – 9" captionStartId-0="2.[151,269,1900,1923]" captionStartId-1="3.[151,255,1329,1352]" captionTargetBox-0="[160,1436,999,1898]" captionTargetBox-1="[163,1431,234,1236]" captionTargetId-0="figure@2.[156,1441,992,1900]" captionTargetId-1="figure@3.[163,1448,234,1319]" captionTargetPageId-0="2" captionTargetPageId-1="3" captionText-0="FIGURES 1 – 5. Acidoxantha galibeedu sp. nov., Holotype ♀. 1, head (lateral view); 2, thorax (dorsal view); 3, thorax (lateral view) and legs; 4, wing; 5, abdomen; 5 a, ovipositor (lateral view)." captionText-1="FIGURE 6 – 9. Acidoxantha galibeedu sp. nov., Paratype ♂. 6, habitus (lateral); 7, epandrium and surstyli (posterior view); 8, epandrium and surstyli (lateral view); 9, glans of phallus." httpUri-0="https://zenodo.org/record/228515/files/figure.png" httpUri-1="https://zenodo.org/record/228516/files/figure.png" pageId="1" pageNumber="127">
Figs 
<number box="[215,258,1652,1676]" pageId="1" pageNumber="127" value="5.0" valueMax="9.0" valueMin="1.0">1–9</number>
</figureCitation>
)
