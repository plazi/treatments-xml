</bibRefCitation>
</taxonomicName>
in 
<taxonomicName authority="Stephens, 1815" authorityName="Stephens" authorityYear="1815" box="[659,992,541,567]" class="Aves" family="Cuculidae" genus="Centropus" kingdom="Animalia" order="Cuculiformes" pageId="7" pageNumber="57" phylum="Chordata" rank="species" species="sinensis">
<emphasis box="[659,787,542,567]" italics="true" pageId="7" pageNumber="57">C. sinensis</emphasis>
(Stephens, 
<number box="[924,984,541,567]" pageId="7" pageNumber="57" value="1815.0">1815</number>
)
</taxonomicName>
from 
<collectingCountry box="[1065,1129,541,567]" name="India" pageId="7" pageNumber="57">India</collectingCountry>
; 
<taxonomicName authority="Williams, 1929" authorityName="Williams" authorityYear="1929" class="Secernentea" family="Habronematidae" genus="Torquatoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="7" pageNumber="57" phylum="Nematoda" rank="species" species="crotophaga">
<emphasis box="[1141,1305,542,567]" italics="true" pageId="7" pageNumber="57">T. crotophaga</emphasis>
(
<bibRefCitation author="Williams" pageId="7" pageNumber="58" refString="Williams, O. L. (1929) Revision of the nematode genus Torquatalla with a description of Torquatalla crotophaga sp. nov. University of California Publications in Zoology, 33, 169 - 178." type="journal article" year="1929">
Williams, 
<number box="[151,212,580,606]" pageId="7" pageNumber="57" value="1929.0">1929</number>
</bibRefCitation>
)
</taxonomicName>
in 
<emphasis box="[260,450,580,605]" italics="true" pageId="7" pageNumber="57">
<taxonomicName box="[260,403,580,605]" class="Secernentea" family="Habronematidae" genus="Torquatoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="7" pageNumber="58" phylum="Nematoda" rank="species" species="crotophaga" status="sp. nov.">Crotophaga</taxonomicName>
ani
</emphasis>
(Linnaeus, 
<number box="[594,655,580,606]" pageId="7" pageNumber="57" value="1758.0">1758</number>
) from Central 
<collectingCountry box="[834,937,580,606]" name="United States of America" pageId="7" pageNumber="57">America</collectingCountry>
(
<collectingCountry box="[954,1055,580,606]" name="Panama" pageId="7" pageNumber="57">Panama</collectingCountry>
), 
<taxonomicName box="[1076,1246,580,605]" class="Aves" family="Cuculidae" genus="Centropus" kingdom="Animalia" order="Cuculiformes" pageId="7" pageNumber="57" phylum="Chordata" rank="species" species="sulcirostris">
<emphasis box="[1076,1246,580,605]" italics="true" pageId="7" pageNumber="57">C. sulcirostris</emphasis>
</taxonomicName>
and 
<taxonomicName class="Aves" family="Cuculidae" genus="Geococcyx" kingdom="Animalia" order="Cuculiformes" pageId="7" pageNumber="57" phylum="Chordata" rank="species" species="californianus">
<emphasis italics="true" pageId="7" pageNumber="57">Geococcyx californianus</emphasis>
</taxonomicName>
from South Texas (
<collectingCountry box="[544,608,618,644]" name="United States of America" pageId="7" pageNumber="57">USA</collectingCountry>
), and 
<emphasis box="[681,823,619,644]" italics="true" pageId="7" pageNumber="57">Guira guira</emphasis>
from 
<collectingCountry box="[897,973,618,644]" name="Brazil" pageId="7" pageNumber="57">Brazil</collectingCountry>
; 
<taxonomicName authority="Rasheed, 1960" authorityName="Rasheed" authorityYear="1960" box="[986,1290,618,644]" class="Secernentea" family="Habronematidae" genus="Torquatoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="7" pageNumber="57" phylum="Nematoda" rank="species" species="singhi">
<emphasis box="[986,1087,619,644]" italics="true" pageId="7" pageNumber="57">T. singhi</emphasis>
(Rasheed, 
<number box="[1221,1282,618,644]" pageId="7" pageNumber="57" value="1960.0">1960</number>
)
</taxonomicName>
in 
<taxonomicName authority="Vieillot, 1817" authorityName="Vieillot" authorityYear="1817" class="Aves" family="Dicruridae" genus="Dicrurus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="57" phylum="Chordata" rank="species" species="macrocercus">
<emphasis italics="true" pageId="7" pageNumber="57">Dicrurus macrocercus</emphasis>
Vieillot, 
<number box="[420,481,657,683]" pageId="7" pageNumber="57" value="1817.0">1817</number>
</taxonomicName>
from 
<collectingCountry box="[555,617,657,683]" name="India" pageId="7" pageNumber="57">India</collectingCountry>
and 
<taxonomicName authority="Zhang and Brooks, 2004" authorityName="Zhang and Brooks" authorityYear="2004" box="[678,1106,657,683]" class="Secernentea" family="Habronematidae" genus="Torquatoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="7" pageNumber="57" phylum="Nematoda" rank="species" species="trogoni">
<emphasis box="[678,797,657,682]" italics="true" pageId="7" pageNumber="57">T. trogoni</emphasis>
Zhang and Brooks, 
<quantity box="[1046,1106,657,683]" metricMagnitude="1" metricUnit="m" metricValue="5.090159999999999" pageId="7" pageNumber="57" unit="in" value="2004.0">2004</quantity>
</taxonomicName>
in 
<taxonomicName authority="Gould, 1838" authorityName="Gould" authorityYear="1838" class="Aves" family="Trogonidae" genus="Trogon" kingdom="Animalia" order="Trogoniformes" pageId="7" pageNumber="57" phylum="Chordata" rank="species" species="massena">
<emphasis box="[1147,1345,657,682]" italics="true" pageId="7" pageNumber="57">Trogon massena</emphasis>
Gould, 
<number box="[151,210,696,722]" pageId="7" pageNumber="57" value="1838.0">1838</number>
