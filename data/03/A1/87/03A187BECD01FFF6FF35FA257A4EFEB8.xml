<taxonomicName authorityName="" authorityYear="1847" box="[151,300,188,213]" class="Diplopoda" family="Platyrhacidae" genus="Platyrhachus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="6" pageNumber="251" phylum="Arthropoda" rank="genus">
