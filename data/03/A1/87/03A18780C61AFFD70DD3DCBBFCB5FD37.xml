<taxonomicName authorityName="KITS &amp; MARSHALL" authorityYear="2013" box="[151,306,1874,1896]" class="Insecta" family="Sphaeroceridae" genus="Antrops" kingdom="Animalia" order="Diptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="quadrilobus">
<emphasis box="[151,306,1874,1896]" italics="true" pageId="83" pageNumber="84">A. quadrilobus</emphasis>
</taxonomicName>
, sternite 5; (163) 
<taxonomicName authorityName="KITS &amp; MARSHALL" authorityYear="2013" box="[505,679,1874,1896]" class="Insecta" family="Sphaeroceridae" genus="Antrops" kingdom="Animalia" order="Diptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="sierrazulensis">
<emphasis box="[505,679,1874,1896]" italics="true" pageId="83" pageNumber="84">A. sierrazulensis</emphasis>
