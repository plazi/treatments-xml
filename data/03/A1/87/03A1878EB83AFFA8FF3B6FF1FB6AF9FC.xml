<paragraph blockId="5.[151,674,1183,1245]" box="[151,295,1220,1245]" pageId="5" pageNumber="131">
(
<figureCitation box="[159,287,1220,1245]" captionStart-0="FIGURE 12" captionStart-1="FIGURES 13 – 17" captionStart-2="FIGURES 18 – 20" captionStartId-0="5.[151,255,1087,1110]" captionStartId-1="6.[151,269,1299,1322]" captionStartId-2="7.[151,269,1070,1093]" captionTargetBox-0="[170,1388,195,1085]" captionTargetBox-1="[162,1436,421,1267]" captionTargetBox-2="[188,1372,222,1011]" captionTargetId-0="figure@5.[144,1452,195,1120]" captionTargetId-1="figure@6.[151,1436,421,1329]" captionTargetId-2="figure@7.[151,1436,188,1096]" captionTargetPageId-0="5" captionTargetPageId-1="6" captionTargetPageId-2="7" captionText-0="FIGURE 12. Philophylla lachung sp. nov., Holotype ♂ (Habitus, dorsal view)." captionText-1="FIGURES 13 – 17. Philophylla lachung sp. nov. 13, head; 14, thorax (dorsal view); 15, thorax (lateral view); 16, wing; 17, abdomen." captionText-2="FIGURES 18 – 20. Philophylla lachung sp. nov. 18, epandrium and surstyli (posterior view); 19, epandrium and surstyli (lateral view); 20, glans of phallus." httpUri-0="https://zenodo.org/record/228518/files/figure.png" httpUri-1="https://zenodo.org/record/228519/files/figure.png" httpUri-2="https://zenodo.org/record/228520/files/figure.png" pageId="5" pageNumber="131">
Figs 
<number box="[215,287,1220,1244]" pageId="5" pageNumber="131" value="16.0" valueMax="20.0" valueMin="12.0">12–20</number>
