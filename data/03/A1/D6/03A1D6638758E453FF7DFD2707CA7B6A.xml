<taxonomicName authority="Pancher ex Brongn." authorityName="Brongn." box="[426,965,513,539]" class="Liliopsida" family="Arecaceae" genus="Cyphokentia" kingdom="Plantae" order="Arecales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="species" species="macrostachys">
<emphasis box="[426,729,513,539]" italics="true" pageId="62" pageNumber="63">Cyphokentia macrostachys</emphasis>
Pancher ex Brongn.
</taxonomicName>
(
<taxonomicName box="[981,1101,513,539]" class="Liliopsida" family="Arecaceae" kingdom="Plantae" order="Arecales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="family">Arecaceae</taxonomicName>
) and the other specimen was collected on 
<taxonomicName authorityName="Vieillard" authorityYear="1873" box="[294,411,549,575]" class="Liliopsida" family="Arecaceae" genus="Basselinia" kingdom="Plantae" order="Arecales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="genus">
<emphasis box="[294,411,549,575]" italics="true" pageId="62" pageNumber="63">Basselinia</emphasis>
</taxonomicName>
