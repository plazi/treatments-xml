<number box="[1140,1156,1420,1443]" pageId="30" pageNumber="31" value="7.0">7</number>
, uniserial. Clypeus with 
<number box="[151,220,1455,1479]" pageId="30" pageNumber="31" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
