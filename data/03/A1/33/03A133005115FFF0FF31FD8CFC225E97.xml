<number box="[1196,1239,1383,1407]" pageId="30" pageNumber="31" value="3.5" valueMax="5.0" valueMin="2.0">2–5</number>
lengths: 
<number pageId="30" pageNumber="31" value="14564.0" valueMax="29102.0" valueMin="26.0">26– 29, 102</number>
– 
<number box="[210,310,1419,1443]" pageId="30" pageNumber="31" value="103100.0">103, 100</number>
– 
<number box="[325,422,1419,1443]" pageId="30" pageNumber="31" value="101177.0">101, 177</number>
– 
<number box="[437,479,1419,1443]" pageId="30" pageNumber="31" value="201.0">201</number>
