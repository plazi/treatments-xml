<number box="[498,515,1817,1841]" pageId="17" pageNumber="18" value="4.0">4</number>
; Pa 
<number box="[562,577,1817,1841]" pageId="17" pageNumber="18" value="1.0">1</number>
; Scts 
<number box="[645,662,1817,1841]" pageId="17" pageNumber="18" value="4.0">4</number>
. Halteres with 
<number box="[830,844,1817,1841]" pageId="17" pageNumber="18" value="4.0">4</number>
