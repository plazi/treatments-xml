<quantity box="[857,1023,1638,1662]" metricMagnitude="-3" metricUnit="m" metricValue="1.22" metricValueMax="1.24" metricValueMin="1.2" pageId="17" pageNumber="18" unit="mm" value="1.22" valueMax="1.24" valueMin="1.2">1.20–1.24 mm</quantity>
