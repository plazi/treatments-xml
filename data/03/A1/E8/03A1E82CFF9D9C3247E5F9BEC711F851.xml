, ALE 
<number box="[1380,1432,1003,1027]" pageId="8" pageNumber="101" value="0.23">0.23</number>
, PME 
<number box="[215,267,1039,1063]" pageId="8" pageNumber="101" value="0.17">0.17</number>
, PLE 
<number box="[335,373,1039,1063]" pageId="8" pageNumber="101" value="0.2">0.2</number>
