<emphasis bold="true" box="[136,288,317,341]" pageId="1" pageNumber="57">
<typeStatus box="[136,194,317,341]" pageId="1" pageNumber="57">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Tanasevitch, 2020" authorityName="Tanasevitch" authorityYear="2020" box="[296,505,317,341]" class="Arachnida" family="Linyphiidae" genus="Asiafroneta" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="57" phylum="Arthropoda" rank="species" species="pallida" status="new species">
