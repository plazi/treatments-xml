<taxonomicName authority="(Say, 1825)" baseAuthorityName="Say" baseAuthorityYear="1825" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" kingdom="Animalia" order="Stylommatophora" pageId="25" pageNumber="82" phylum="Mollusca" rank="species" species="multilineatus" subGenus="Mesembrinus">
