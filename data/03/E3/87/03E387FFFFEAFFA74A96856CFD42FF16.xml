<subSubSection pageId="4" pageNumber="333" type="reference_group">
<paragraph blockId="4.[151,1436,1880,2013]" pageId="4" pageNumber="333">
[type = 
<taxonomicName authority="Kittlitz, 1833" authorityName="Kittlitz" authorityYear="1833" box="[726,1091,1880,1905]" class="Aves" family="Fringillidae" genus="Fringilla" kingdom="Animalia" order="Passeriformes" pageId="4" pageNumber="333" phylum="Chordata" rank="species" species="alaudina">
