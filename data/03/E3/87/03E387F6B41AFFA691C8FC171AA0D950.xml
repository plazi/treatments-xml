); middle segment length 
<number box="[233,337,1642,1668]" pageId="12" pageNumber="13" value="116.0" valueMax="127.0" valueMin="105.0">105–127</number>
(
<number box="[354,399,1642,1668]" pageId="12" pageNumber="13" value="112.0">112</number>
± 
<number box="[432,449,1642,1668]" pageId="12" pageNumber="13" value="9.0">9</number>
); terminal claw length 
<number box="[727,801,1642,1668]" pageId="12" pageNumber="13" value="28.5" valueMax="33.0" valueMin="24.0">24–33</number>
