, width 
<number box="[1132,1170,1714,1739]" pageId="7" pageNumber="439" value="1.5">1.5</number>
; mesosomal length 3.0, width 
<number box="[223,261,1750,1775]" pageId="7" pageNumber="439" value="1.6">1.6</number>
; fore wing length 
<number box="[469,504,1750,1775]" pageId="7" pageNumber="439" value="5.2">5.2</number>
(as preserved), width 
<quantity box="[758,848,1751,1775]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="7" pageNumber="439" unit="mm" value="2.7">2.7 mm</quantity>
; hind wing length 
<number box="[1060,1098,1751,1775]" pageId="7" pageNumber="439" value="3.9">3.9</number>
, width 
<number box="[1182,1220,1751,1775]" pageId="7" pageNumber="439" value="1.2">1.2</number>
; hind femur length 
<number box="[151,188,1786,1811]" pageId="7" pageNumber="439" value="1.5">1.5</number>
, hind tibia length 
<number box="[396,434,1786,1811]" pageId="7" pageNumber="439" value="2.6">2.6</number>
; first metasomal segment length 
<number box="[811,848,1787,1811]" pageId="7" pageNumber="439" value="1.1">1.1</number>
, width 
<number box="[934,972,1787,1811]" pageId="7" pageNumber="439" value="0.7">0.7</number>
; metasomal length 
<number box="[1193,1231,1786,1811]" pageId="7" pageNumber="439" value="5.1">5.1</number>
; ovipositor length 
<number box="[151,186,1823,1847]" pageId="7" pageNumber="439" value="3.7">3.7</number>
(as preserved).
</paragraph>
