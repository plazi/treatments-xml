genus:
</emphasis>
<taxonomicName authority="Vicente &amp; Mauries, 1980" authorityName=", Vicente &amp; Mauries" authorityYear="1980" box="[287,702,651,675]" class="Diplopoda" family="Vandeleumatidae" genus="Guipuzcosoma" kingdom="Animalia" order="Chordeumatida" pageId="4" pageNumber="146" phylum="Arthropoda" rank="genus">
