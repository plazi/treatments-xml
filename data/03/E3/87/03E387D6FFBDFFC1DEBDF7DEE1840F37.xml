, 
<taxonomicName authority="(Sowerby I, 1833)" baseAuthorityName="Sowerby I" baseAuthorityYear="1833" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="74" phylum="Mollusca" rank="species" species="strigatus" subGenus="Drymaeus">
<emphasis box="[1326,1436,1318,1341]" italics="true" pageId="17" pageNumber="74">Drymaeus</emphasis>
(
<emphasis box="[840,950,1350,1373]" italics="true" pageId="17" pageNumber="74">Drymaeus</emphasis>
) 
<emphasis box="[966,1059,1350,1373]" italics="true" pageId="17" pageNumber="74">strigatus</emphasis>
(Sowerby I, 1833)
</taxonomicName>
.
</paragraph>
