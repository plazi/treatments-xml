<taxonomicName authority="Beddard, 1886" authorityName="Beddard" authorityYear="1886" box="[299,694,1434,1460]" class="Clitellata" family="Moniligastridae" genus="Moniligaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="barwelli">
