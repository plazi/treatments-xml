<taxonomicName authority="Ganychorus" authorityName="Ganychorus" box="[927,1179,2011,2037]" class="Insecta" family="Braconidae" genus="Blacus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="41" phylum="Arthropoda" rank="genus">
<emphasis box="[927,1007,2011,2036]" italics="true" pageId="4" pageNumber="41">Blacus</emphasis>
(
<emphasis box="[1025,1169,2011,2036]" italics="true" pageId="4" pageNumber="41">Ganychorus</emphasis>
)
</taxonomicName>
in 
<bibRefCitation author="Achterberg" lastPageId="5" lastPageNumber="42" pageId="4" pageNumber="41" refString="Achterberg, C. van (1988) Revision of the subfamily Blacinae Foerster (Hymenoptera: Braconidae). Zoologische Verhandelingen, 249, 324 pp." type="book" year="1988">
Achterberg (
<number box="[1368,1432,2011,2037]" pageId="4" pageNumber="41" value="1988.0">1988</number>
: 
<number box="[151,230,152,178]" pageId="5" pageNumber="42" value="91.0" valueMax="92.0" valueMin="90.0">90–92</number>
)
