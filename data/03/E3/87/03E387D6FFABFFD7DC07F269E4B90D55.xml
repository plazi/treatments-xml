, 
<taxonomicName authority="(Reeve, 1849)" baseAuthorityName="Reeve" baseAuthorityYear="1849" class="Gastropoda" family="Bulimulidae" genus="Bostryx" kingdom="Animalia" order="Stylommatophora" pageId="7" pageNumber="64" phylum="Mollusca" rank="species" species="nigropileatus">
