<collectingCountry box="[266,334,1495,1518]" name="China" pageId="5" pageNumber="178">China</collectingCountry>
: 
<collectingRegion box="[344,436,1495,1518]" country="China" name="Guangxi" pageId="5" pageNumber="178">Guangxi</collectingRegion>
(
<figureCitation box="[450,529,1495,1518]" captionStart="Fig" captionStartId="7.[113,144,736,755]" captionTargetBox="[179,1018,219,727]" captionTargetId="figure-329@7.[179,1018,219,725]" captionTargetPageId="7" captionText="Fig. 13. Distribution of Gonioctena (B.) mauroi species-group." figureDoi="http://doi.org/10.5281/zenodo.5316162" httpUri="https://zenodo.org/record/5316162/files/figure.png" pageId="5" pageNumber="178">Fig. 13</figureCitation>
