<subSubSection pageId="3" pageNumber="58" type="etymology">
<paragraph blockId="3.[121,759,1705,1949]" pageId="3" pageNumber="58">
<taxonomicName authorityName="Hwang &amp; Kim &amp; Kang &amp; Ahn" authorityYear="2017" box="[121,401,1786,1810]" class="Flavobacteriia" family="Flavobacteriaceae" genus="Flavobacterium" kingdom="Bacteria" order="Flavobacteriales" pageId="3" pageNumber="58" phylum="Bacteroidetes" rank="species" species="eburneum">
