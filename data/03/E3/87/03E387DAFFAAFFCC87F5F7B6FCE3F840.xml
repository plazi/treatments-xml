<paragraph blockId="4.[418,1813,1253,1578]" box="[418,1128,1253,1294]" pageId="4" pageNumber="60">
<emphasis bold="true" box="[418,890,1253,1294]" pageId="4" pageNumber="60">
<taxonomicName authorityName="Blair" box="[418,883,1253,1294]" class="Insecta" family="Ptinidae" genus="Dorcatomiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="60" phylum="Arthropoda" rank="species" species="guamensis" status="new species">Dorcatomiella guamensis</taxonomicName>
,
