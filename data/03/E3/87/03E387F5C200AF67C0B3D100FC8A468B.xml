<taxonomicName authority="sensu HEINRICH (1962)" authorityName="sensu HEINRICH" authorityYear="1962" box="[360,740,857,879]" class="Insecta" family="Ichneumonidae" genus="Asthenolabus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="524" phylum="Arthropoda" rank="genus">
<emphasis box="[360,496,857,879]" italics="true" pageId="3" pageNumber="524">Asthenolabus</emphasis>
