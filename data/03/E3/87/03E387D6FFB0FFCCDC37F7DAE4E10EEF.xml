<taxonomicName authority="(Albers, 1854)" baseAuthorityName="Albers" baseAuthorityYear="1854" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" kingdom="Animalia" order="Stylommatophora" pageId="28" pageNumber="85" phylum="Mollusca" rank="species" species="vestalis">
