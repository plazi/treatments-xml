<taxonomicName authorityName="Loof and Zullini" authorityYear="2000" box="[144,255,1596,1620]" class="Adenophorea" family="Qudsianematidae" genus="Parapalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="13" pageNumber="1266" phylum="Nematoda" rank="genus">
