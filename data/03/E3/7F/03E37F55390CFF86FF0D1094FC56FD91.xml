<paragraph blockId="11.[151,564,222,245]" box="[151,564,222,245]" pageId="11" pageNumber="250">
<taxonomicName authority="Girault, 1928: 2" authorityName="Girault" authorityPageNumber="2" authorityYear="1928" box="[151,564,222,245]" class="Insecta" family="Phlaeothripidae" genus="Phaulothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="11" pageNumber="250" phylum="Arthropoda" rank="species" species="longitubus">
<emphasis box="[151,396,223,245]" italics="true" pageId="11" pageNumber="250">Phaulothrips longitubus</emphasis>
Girault, 
<number box="[487,541,223,244]" pageId="11" pageNumber="250" value="1928.0">1928</number>
: 
<number box="[551,564,223,244]" pageId="11" pageNumber="250" value="2.0">2</number>
</taxonomicName>
