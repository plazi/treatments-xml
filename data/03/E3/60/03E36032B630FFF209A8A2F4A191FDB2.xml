<paragraph blockId="21.[151,1437,152,611]" box="[199,607,152,179]" pageId="21" pageNumber="490">
<emphasis bold="true" box="[199,353,152,179]" pageId="21" pageNumber="490">Distribution.</emphasis>
<collectingCountry box="[359,426,152,178]" name="China" pageId="21" pageNumber="490">China</collectingCountry>
(
<collectingRegion box="[441,516,152,179]" country="China" name="Anhui" pageId="21" pageNumber="490">Anhui</collectingRegion>
), 
<collectingCountry box="[536,603,153,179]" name="Japan" pageId="21" pageNumber="490">Japan</collectingCountry>
.
</paragraph>
