<subSubSection lastPageId="19" lastPageNumber="20" pageId="18" pageNumber="19" type="diagnosis">
<paragraph blockId="18.[151,1436,1897,1962]" pageId="18" pageNumber="19">
<emphasis bold="true" box="[198,319,1897,1923]" pageId="18" pageNumber="19">Diagnosis</emphasis>
