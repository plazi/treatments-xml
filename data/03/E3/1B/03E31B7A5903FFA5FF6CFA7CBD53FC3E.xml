s 
<taxonomicName authority="(LePeletier &amp; Serville, 1828)" baseAuthorityName="LePeletier &amp; Serville" baseAuthorityYear="1828" box="[274,731,1449,1476]" class="Insecta" family="Dynastidae" genus="Bothynus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="102" phylum="Arthropoda" rank="species" species="entellus">
