(
<taxonomicName authority="Aldrich, 1905" authorityName="Aldrich" authorityYear="1905" box="[849,1039,1413,1440]" class="Insecta" family="Dolichopodidae" kingdom="Animalia" order="Diptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="subFamily" subFamily="Xanthochlorinae">Xanthochlorinae</taxonomicName>
