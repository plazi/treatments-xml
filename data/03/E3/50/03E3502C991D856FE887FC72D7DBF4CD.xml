<taxonomicName authority="Loew, 1857" authorityName="Loew" authorityYear="1857" box="[193,649,909,935]" class="Insecta" family="Dolichopodidae" genus="Campsicnemus" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="marginatus">
