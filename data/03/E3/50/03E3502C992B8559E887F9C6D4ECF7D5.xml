(
<taxonomicName authorityName="Latreille" authorityYear="1809" box="[581,763,1593,1620]" class="Insecta" family="Dolichopodidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="subFamily" subFamily="Dolichopodinae">Dolichopodinae</taxonomicName>
