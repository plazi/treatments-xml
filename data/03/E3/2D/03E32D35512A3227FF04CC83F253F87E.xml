<number box="[516,587,866,890]" pageId="37" pageNumber="38" value="29.5" valueMax="30.0" valueMin="29.0">29–30</number>
setae. Tentorium 
<number box="[788,830,866,890]" pageId="37" pageNumber="38" value="189.0">189</number>
(
<number box="[847,859,866,890]" pageId="37" pageNumber="38" value="1.0">1</number>
