<number box="[653,697,830,854]" pageId="37" pageNumber="38" value="112.0">112</number>
; 
<number box="[709,754,830,854]" pageId="37" pageNumber="38" value="102.0">102</number>
; 
<number box="[765,810,830,854]" pageId="37" pageNumber="38" value="123.0">123</number>
; 
<number box="[822,867,829,854]" pageId="37" pageNumber="38" value="126.0">126</number>
; 
<number box="[879,924,830,854]" pageId="37" pageNumber="38" value="143.0">143</number>
. Longest seta 
<number box="[1083,1180,830,854]" pageId="37" pageNumber="38" value="418.0" valueMax="439.0" valueMin="397.0">397–439</number>
µm long. AR (
<number box="[1351,1363,830,854]" pageId="37" pageNumber="38" value="1.0">1</number>
