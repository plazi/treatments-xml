<paragraph blockId="40.[151,1436,1568,2025]" pageId="40" pageNumber="41">
Total length 
<quantity box="[341,444,1892,1917]" metricMagnitude="-3" metricUnit="m" metricValue="5.01" pageId="40" pageNumber="41" unit="mm" value="5.01">5.01 mm</quantity>
