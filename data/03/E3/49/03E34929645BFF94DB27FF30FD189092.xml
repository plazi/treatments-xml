<quantity box="[1216,1339,472,496]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.3" metricValueMin="1.8" pageId="11" pageNumber="626" unit="mm" value="2.05" valueMax="2.3" valueMin="1.8">1.8–2.3 mm</quantity>
