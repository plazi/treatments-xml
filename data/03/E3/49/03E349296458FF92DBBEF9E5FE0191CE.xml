<quantity box="[150,287,1741,1764]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" metricValueMax="2.4" metricValueMin="1.9" pageId="9" pageNumber="624" unit="mm" value="2.15" valueMax="2.4" valueMin="1.9">1.9–2.4 mm</quantity>
