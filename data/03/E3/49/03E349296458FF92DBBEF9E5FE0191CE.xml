<quantity box="[363,527,1741,1764]" metricMagnitude="-4" metricUnit="m" metricValue="8.75" metricValueMax="9.3" metricValueMin="8.2" pageId="9" pageNumber="624" unit="mm" value="0.875" valueMax="0.93" valueMin="0.82">0.82–0.93 mm</quantity>
, PL: 
<quantity box="[591,754,1741,1764]" metricMagnitude="-4" metricUnit="m" metricValue="6.799999999999999" metricValueMax="7.1" metricValueMin="6.5" pageId="9" pageNumber="624" unit="mm" value="0.6799999999999999" valueMax="0.71" valueMin="0.65">0.65–0.71 mm</quantity>
, PW: 
<quantity box="[205,364,1773,1796]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="7.9" metricValueMin="7.1" pageId="9" pageNumber="624" unit="mm" value="0.75" valueMax="0.79" valueMin="0.71">0.71–0.79 mm</quantity>
, EL: 
<quantity box="[425,584,1773,1796]" metricMagnitude="-4" metricUnit="m" metricValue="7.6" metricValueMax="8.1" metricValueMin="7.1" pageId="9" pageNumber="624" unit="mm" value="0.76" valueMax="0.81" valueMin="0.71">0.71–0.81 mm</quantity>
, EW: 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="9.299999999999999" metricValueMax="9.8" metricValueMin="8.8" pageId="9" pageNumber="624" unit="mm" value="0.9299999999999999" valueMax="0.98" valueMin="0.88">0.88–0.98 mm</quantity>
, SL: 
<quantity box="[248,405,1805,1828]" metricMagnitude="-4" metricUnit="m" metricValue="5.949999999999999" metricValueMax="6.3" metricValueMin="5.6" pageId="9" pageNumber="624" unit="mm" value="0.595" valueMax="0.63" valueMin="0.56">0.56–0.63 mm</quantity>
