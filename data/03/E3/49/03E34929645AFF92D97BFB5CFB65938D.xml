<quantity box="[592,755,1926,1949]" metricMagnitude="-4" metricUnit="m" metricValue="6.45" metricValueMax="6.8" metricValueMin="6.1" pageId="10" pageNumber="625" unit="mm" value="0.645" valueMax="0.68" valueMin="0.61">0.61–0.68 mm</quantity>
, PW: 
<quantity box="[206,365,1958,1981]" metricMagnitude="-4" metricUnit="m" metricValue="6.8999999999999995" metricValueMax="7.3" metricValueMin="6.5" pageId="10" pageNumber="625" unit="mm" value="0.69" valueMax="0.73" valueMin="0.65">0.65–0.73 mm</quantity>
, EL: 
<quantity box="[425,584,1958,1981]" metricMagnitude="-4" metricUnit="m" metricValue="8.25" metricValueMax="8.6" metricValueMin="7.9" pageId="10" pageNumber="625" unit="mm" value="0.825" valueMax="0.86" valueMin="0.79">0.79–0.86 mm</quantity>
, EW: 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="8.9" metricValueMax="9.3" metricValueMin="8.5" pageId="10" pageNumber="625" unit="mm" value="0.89" valueMax="0.93" valueMin="0.85">0.85–0.93 mm</quantity>
, SL: 
<quantity box="[249,406,1990,2013]" metricMagnitude="-4" metricUnit="m" metricValue="7.1" metricValueMax="7.6" metricValueMin="6.6" pageId="10" pageNumber="625" unit="mm" value="0.71" valueMax="0.76" valueMin="0.66">0.66–0.76 mm</quantity>
