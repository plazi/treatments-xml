</taxonomicName>
<emphasis bold="true" box="[264,355,1148,1174]" pageId="10" pageNumber="141">
<taxonomicNameLabel box="[264,355,1148,1174]" pageId="10" pageNumber="141" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, and 
<taxonomicName authority="Xu &amp; Yu &amp; Liu &amp; Li, 2022" authorityName="Xu &amp; Yu &amp; Liu &amp; Li" authorityYear="2022" box="[418,555,1148,1174]" class="Arachnida" family="Liphistiidae" genus="Luthela" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="141" phylum="Arthropoda" rank="species" species="yuncheng" status="sp. nov.">
