<collectingRegion box="[1401,1474,861,885]" country="India" name="Kerala" pageId="2" pageNumber="311">Kerala</collectingRegion>
and 
<collectingRegion box="[882,1014,893,917]" country="India" name="Tamil Nadu" pageId="2" pageNumber="311">Tamil Nadu</collectingRegion>
) (
<figureCitation box="[1038,1121,893,917]" captionStart="Figs" captionStartId="9.[113,157,2093,2115]" captionTargetBox="[141,1427,190,2077]" captionTargetPageId="9" captionText="Figs. 40–41. Distribution of the species of the Laccophilus javanicus group" figureDoi="http://doi.org/10.5281/zenodo.5385208" httpUri="https://zenodo.org/record/5385208/files/figure.png" pageId="2" pageNumber="311">Fig. 40</figureCitation>
