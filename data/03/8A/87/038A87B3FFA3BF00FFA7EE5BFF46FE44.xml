<emphasis bold="true" box="[1022,1043,802,825]" pageId="4" pageNumber="729">*</emphasis>
, 
<quantity box="[1059,1166,802,825]" metricMagnitude="-2" metricUnit="m" metricValue="1.7" metricValueMax="1.8" metricValueMin="1.6" pageId="4" pageNumber="729" unit="mm" value="17.0" valueMax="18.0" valueMin="16.0">16-18 mm</quantity>
; 
<emphasis bold="true" box="[1181,1202,802,825]" pageId="4" pageNumber="729">+</emphasis>
, 15.5-17.5. Pronotum length: 
<emphasis bold="true" box="[979,1000,827,850]" pageId="4" pageNumber="729">*</emphasis>
, 
<quantity box="[1015,1117,827,850]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" metricValueMax="5.2" metricValueMin="5.0" pageId="4" pageNumber="729" unit="mm" value="5.1" valueMax="5.2" valueMin="5.0">5-5.2 mm</quantity>
; 
<emphasis bold="true" box="[1132,1153,827,850]" pageId="4" pageNumber="729">+</emphasis>
, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="4.65" metricValueMax="5.1" metricValueMin="4.2" pageId="4" pageNumber="729" unit="mm" value="4.65" valueMax="5.1" valueMin="4.2">4.2- 5.1 mm</quantity>
