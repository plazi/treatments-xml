25° 
<number box="[988,1019,749,773]" pageId="4" pageNumber="61" value="22.0">22</number>
' N
</geoCoordinate>
, 
<geoCoordinate box="[1064,1163,749,773]" direction="east" orientation="longitude" pageId="4" pageNumber="61" precision="925" value="49.566666">
<number box="[1064,1094,749,773]" pageId="4" pageNumber="61" value="49.0">49</number>
° 
