leg with 
<number box="[868,881,1978,2001]" pageId="0" pageNumber="57" value="3.0">3</number>
setae on 
<number box="[981,990,1978,2001]" pageId="0" pageNumber="57" value="1.0">1</number>
.subcoxa, 
<number box="[1101,1114,1978,2001]" pageId="0" pageNumber="57" value="2.0">2</number>
on 
<number box="[1153,1166,1978,2001]" pageId="0" pageNumber="57" value="2.0">2</number>
.subcoxa, 
<number box="[1274,1287,1978,2001]" pageId="0" pageNumber="57" value="8.0">8</number>
on coxa, 
<number box="[1390,1403,1978,2001]" pageId="0" pageNumber="57" value="6.0">6</number>
on trochanter, 
<number box="[271,297,2012,2035]" pageId="0" pageNumber="57" value="11.0">11</number>
on femur, 
<number box="[413,440,2012,2035]" pageId="0" pageNumber="57" value="18.0">18</number>
on tibiotarsus (
<figureCitation box="[605,684,2012,2035]" captionStart="FIGURES 5 – 14" captionStartId="2.[151,269,1847,1871]" captionTargetBox="[186,1343,192,1846]" captionTargetId="figure@2.[151,1436,94,1882]" captionTargetPageId="2" captionText="FIGURES 5 – 14. Pseudachorutes cheni sp. nov.: 5, mandible; 6, maxilla; 7, Ant. IV, ventral view; 8, postantennal organ and eyes; 9, labrum; 10, female genital plate; 11, labium; 12, abdomen, ventral view; 13, hind leg; 14, unguis III, dorsal view." httpUri="https://zenodo.org/record/181805/files/figure.png" pageId="0" pageNumber="57">
