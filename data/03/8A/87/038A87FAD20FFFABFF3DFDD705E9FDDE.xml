<heading bold="true" box="[151,390,546,573]" fontSize="11" level="1" pageId="1" pageNumber="272" reason="1">
<emphasis bold="true" box="[151,390,546,573]" pageId="1" pageNumber="272">
<taxonomicName authority="Rédei &amp; Tsai, 2021" authorityName="Rédei &amp; Tsai" authorityYear="2021" box="[151,265,546,573]" class="Insecta" family="Pentatomidae" genus="Zhengica" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="272" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,265,546,573]" italics="true" pageId="1" pageNumber="272">Zhengica</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[280,390,547,573]" pageId="1" pageNumber="272" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
