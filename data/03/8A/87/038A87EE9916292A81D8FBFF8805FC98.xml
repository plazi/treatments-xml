<taxonomicName baseAuthorityName="Blainville" baseAuthorityYear="1834" box="[114,310,687,714]" class="Calcarea" family="Sycettidae" genus="Sycon" kingdom="Animalia" order="Leucosolenida" pageId="10" pageNumber="197" phylum="Porifera" rank="species" species="gelatinosum">
<emphasis box="[114,310,687,714]" italics="true" pageId="10" pageNumber="197">Sycon gelatinosum</emphasis>
