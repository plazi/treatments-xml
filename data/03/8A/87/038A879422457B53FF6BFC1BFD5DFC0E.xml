<caption httpUri="https://zenodo.org/record/192039/files/figure.png" pageId="4" pageNumber="5" targetBox="[210,1399,192,1645]" targetPageId="4">
<paragraph blockId="4.[151,1436,1688,1780]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[151,349,1688,1712]" pageId="4" pageNumber="5">FIGURES 12–16.</emphasis>
<taxonomicName box="[355,752,1688,1711]" class="Insecta" family="Lauxaniidae" genus="Homoneura" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="apicomata" status="sp. nov." subGenus="Homoneura">
<emphasis box="[355,482,1688,1711]" italics="true" pageId="4" pageNumber="5">Homoneura</emphasis>
(
<emphasis box="[497,624,1688,1711]" italics="true" pageId="4" pageNumber="5">Homoneura</emphasis>
