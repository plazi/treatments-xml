, N: 
<number box="[1132,1163,296,320]" pageId="8" pageNumber="490" value="40.0">40</number>
<number box="[1176,1204,295,320]" pageId="8" pageNumber="490" value="45.0">45</number>
<number box="[1217,1262,296,320]" pageId="8" pageNumber="490" value="792.0">792</number>
, E: 
<number box="[1303,1333,296,320]" pageId="8" pageNumber="490" value="41.0">41</number>
<number box="[1346,1374,296,320]" pageId="8" pageNumber="490" value="41.0">41</number>
131, 
<date box="[151,290,332,356]" pageId="8" pageNumber="490" value="2013-07-01">01.VII.2013</date>
