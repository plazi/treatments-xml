<subSubSection lastPageId="26" lastPageNumber="27" pageId="24" pageNumber="25" type="reference_group">
<paragraph blockId="24.[298,1316,1250,1271]" lastBlockId="26.[151,1436,151,258]" lastPageId="26" lastPageNumber="27" pageId="24" pageNumber="25">
<taxonomicName authority="Pieper, 1884" authorityName="Pieper" authorityYear="1884" box="[298,576,1250,1271]" class="Hydrozoa" family="Hebellidae" genus="Scandia" kingdom="Animalia" order="Leptothecata" pageId="24" pageNumber="25" phylum="Cnidaria" rank="species" species="gigas">
<emphasis box="[298,433,1250,1271]" italics="true" pageId="24" pageNumber="25">Scandia gigas</emphasis>
(
<bibRefCitation author="Pieper" box="[446,569,1250,1271]" pageId="24" pageNumber="52" refString="Pieper, F. W. (1884) Erganzungen zu &quot; Heller's Zoophyten etc. des Adriatischen Meeres &quot;. Zoologischer Anzeiger, 7, 148 - 152, 164 - 169, 185 - 188, 216 - 221." type="journal article" year="1884">
Pieper, 
<number box="[520,569,1250,1271]" pageId="24" pageNumber="25" value="1884.0">1884</number>
</bibRefCitation>
