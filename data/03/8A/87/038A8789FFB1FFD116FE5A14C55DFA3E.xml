<taxonomicName authority="Xu &amp; Yu &amp; Liu &amp; Li, 2022" authorityName="Xu &amp; Yu &amp; Liu &amp; Li" authorityYear="2022" box="[382,497,223,249]" class="Arachnida" family="Liphistiidae" genus="Luthela" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="152" phylum="Arthropoda" rank="species" species="badong" status="sp. nov.">
<emphasis box="[382,497,223,249]" italics="true" pageId="21" pageNumber="152">L. badong</emphasis>
</taxonomicName>
<emphasis bold="true" box="[505,595,223,249]" pageId="21" pageNumber="152">
<taxonomicNameLabel box="[505,595,223,249]" pageId="21" pageNumber="152" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, and 
<taxonomicName authority="Xu &amp; Yu &amp; Liu &amp; Li, 2022" authorityName="Xu &amp; Yu &amp; Liu &amp; Li" authorityYear="2022" box="[659,765,223,248]" class="Arachnida" family="Liphistiidae" genus="Luthela" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="152" phylum="Arthropoda" rank="species" species="yiyuan" status="sp. nov.">
