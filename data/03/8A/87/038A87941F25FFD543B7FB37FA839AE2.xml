<location LSID="urn:lsid:plazi:treatment:038A87941F25FFD543B7FB37FA839AE2:8EFC60591F25FFD547FEFAEFFADC9A3A" box="[1246,1382,1334,1359]" name="Veny Valley" pageId="5" pageNumber="322" stateProvince="Val d'Aosta">Veny Valley</location>
(
<collectingRegion country="Italy" name="Valle d'Aosta" pageId="5" pageNumber="322">Val dâ€™Aosta</collectingRegion>
), 
<collectingDate box="[263,508,1370,1395]" isEnumeration="true" pageId="5" pageNumber="322">
<date box="[263,379,1370,1395]" pageId="5" pageNumber="322" value="1995-09-11">11IX1995</date>
, 
<date box="[391,508,1371,1395]" pageId="5" pageNumber="322" value="1997-09-12">12IX1997</date>
</collectingDate>
</materialsCitation>
