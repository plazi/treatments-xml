<paragraph blockId="22.[151,1436,1437,2004]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[151,339,1437,1463]" pageId="22" pageNumber="23">
<typeStatus box="[151,214,1437,1463]" pageId="22" pageNumber="23">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[348,464,1437,1463]" pageId="22" pageNumber="23">Holotype</typeStatus>
, male, 
<collectingCountry box="[551,721,1437,1463]" name="Turkmenistan" pageId="22" pageNumber="23">Turkmenistan</collectingCountry>
, 
<quantity box="[734,811,1437,1463]" metricMagnitude="4" metricUnit="m" metricValue="1.8" pageId="22" pageNumber="23" unit="km" value="18.0">18 km</quantity>
from Ashkhabad, Bagyr (Novaya Nisa), soil under wormwood and spurge, 
<date box="[435,561,1476,1502]" pageId="22" pageNumber="23" value="1990-04-20">
<number box="[435,467,1476,1502]" pageId="22" pageNumber="23" value="20.0">20</number>
