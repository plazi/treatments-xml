<quantity metricMagnitude="-3" metricUnit="m" metricValue="4.85" metricValueMax="5.2" metricValueMin="4.5" pageId="6" pageNumber="73" unit="mm" value="4.85" valueMax="5.2" valueMin="4.5">4.5–5.2 mm</quantity>
