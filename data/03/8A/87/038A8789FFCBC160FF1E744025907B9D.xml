<bibRefCitation author="Fraser" pageId="35" pageNumber="50" refString="Fraser, C. M. (1947) Hydroids of the 1939 Allan Hancock Caribbean Sea Expedition. Allan Hancock Atlantic Expedition, 4, 1 - 24." type="journal article" year="1947">
Fraser 
<number box="[1271,1332,352,378]" pageId="35" pageNumber="36" value="1947.0">1947</number>
, present study
</bibRefCitation>
), 
<collectingCountry box="[239,311,392,418]" name="Brazil" pageId="35" pageNumber="36">Brazil</collectingCountry>
(
<bibRefCitation author="Migotto" box="[326,632,392,418]" pageId="35" pageNumber="51" refString="Migotto, A. E. &amp; Vervoort, W. (1998) Redescription of Sertularia notabilis Fraser, 1947 (Sertulariidae, Hydrozoa). Zoologische Meddedelingen, Leiden, 72 (7), 89 - 100." type="journal article" year="1998">
Migotto &amp; Vervoort 
<number box="[569,632,392,418]" pageId="35" pageNumber="36" value="1998.0">1998</number>
</bibRefCitation>
