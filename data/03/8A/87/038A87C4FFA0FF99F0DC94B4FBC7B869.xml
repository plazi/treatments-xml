, 
<collectingRegion box="[1155,1330,733,757]" country="India" name="Andhra Pradesh" pageId="5" pageNumber="314">Andhra Pradesh</collectingRegion>
, 
<collectingRegion box="[1343,1448,733,757]" country="India" name="Nagaland" pageId="5" pageNumber="314">Nagaland</collectingRegion>
