j 
<number box="[769,781,152,176]" pageId="3" pageNumber="60" value="4.0">4</number>
</emphasis>
, 
<emphasis box="[794,816,152,175]" italics="true" pageId="3" pageNumber="60">
j 
<number box="[804,816,152,175]" pageId="3" pageNumber="60" value="5.0">5</number>
