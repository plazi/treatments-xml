and 
<number box="[151,162,1127,1153]" pageId="7" pageNumber="30" value="1.0">1</number>
: 
<number box="[166,182,1127,1153]" pageId="7" pageNumber="30" value="5.0">5</number>
+ 
<number box="[202,220,1127,1153]" pageId="7" pageNumber="30" value="4.0">4</number>
/ 
<number box="[232,243,1127,1153]" pageId="7" pageNumber="30" value="1.0">1</number>
