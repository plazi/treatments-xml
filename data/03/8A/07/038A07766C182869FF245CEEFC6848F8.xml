<subSubSection box="[151,968,222,247]" pageId="18" pageNumber="469" type="materials_examined">
<paragraph blockId="18.[151,1436,222,391]" box="[151,968,222,247]" pageId="18" pageNumber="469">
<emphasis bold="true" box="[151,299,222,247]" pageId="18" pageNumber="469">Type species</emphasis>
. 
<taxonomicName authority="Thomson, 1921" authorityName="Thomson" authorityYear="1921" box="[313,703,222,247]" class="Anthozoa" family="Alcyoniidae" genus="Alcyonium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="18" pageNumber="469" phylum="Cnidaria" rank="species" species="wilsoni">
