<emphasis box="[627,708,596,621]" italics="true" pageId="241" pageNumber="242">Mymar</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Waterhouse" authorityYear="1915" box="[724,869,595,621]" class="Insecta" family="Mymaridae" genus="Palaeoneura" kingdom="Animalia" order="Hymenoptera" pageId="241" pageNumber="242" phylum="Arthropoda" rank="genus">
<emphasis box="[724,869,595,621]" italics="true" pageId="241" pageNumber="242">Palaeoneura</emphasis>
