<taxonomicName authorityName="Curtis Keys" authorityYear="1829" box="[265,346,632,657]" class="Insecta" family="Mymaridae" genus="Mymar" kingdom="Animalia" order="Hymenoptera" pageId="241" pageNumber="242" phylum="Arthropoda" rank="genus">
<emphasis box="[265,346,632,657]" italics="true" pageId="241" pageNumber="242">Mymar</emphasis>
