<bibRefCitation author="PIC M." box="[231,343,444,468]" pageId="35" pageNumber="662" pagination="1 - 36" refId="ref24627" refString="PIC M. 1932: Nouveautes diverses. Melanges Exotico-Entomologiques 60: 1 - 36." type="journal article" year="1932">PIC (1932)</bibRefCitation>
described 
<taxonomicName authorityName="Pic" authorityYear="1932" box="[459,666,445,468]" class="Insecta" family="Anthicidae" genus="Notoxus" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="662" phylum="Arthropoda" rank="species" species="suturalifer">
