<taxonomicName authorityName="Bonadona" authorityYear="1986" class="Insecta" family="Anthicidae" genus="Notoxus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="663" phylum="Arthropoda" rank="species" species="brinckianus">
<emphasis italics="true" pageId="36" pageNumber="663">N. brinckianus</emphasis>
</taxonomicName>
