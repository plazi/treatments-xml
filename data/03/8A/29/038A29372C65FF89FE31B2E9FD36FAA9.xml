<typeStatus box="[634,727,1160,1184]" pageId="36" pageNumber="663">holotype</typeStatus>
(male) and 
<specimenCount box="[849,982,1161,1185]" pageId="36" pageNumber="663" type="generic" typeStatus="paratypes">19 paratypes</specimenCount>
