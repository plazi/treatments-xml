<collectingRegion box="[298,455,1423,1445]" country="Brazil" name="Espirito Santo" pageId="6" pageNumber="7">Espirito Santo</collectingRegion>
state, 
<collectingCountry box="[530,595,1423,1445]" name="Brazil" pageId="6" pageNumber="7">Brazil</collectingCountry>
