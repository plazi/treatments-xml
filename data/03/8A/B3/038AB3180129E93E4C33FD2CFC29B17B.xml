SPECIMENS EXAMINED: 
<materialsCitation ID-GBIF-Occurrence="3421079351" collectionCode="MZB, NMV" location="Mt. Bawakaraeng" pageId="49" pageNumber="49" specimenCode="MZB 40991, 41027, 43003, NMV Z56801, NMV Z57200, NMV Z57223" specimenCount="1">
<location LSID="urn:lsid:plazi:treatment:038AB3180129E93E4C33FD2CFC29B17B:8EFC54D50128E93E4CDFFAA7FB26B139" box="[1004,1211,1317,1341]" name="Mt. Bawakaraeng" pageId="49" pageNumber="49">Mt. Bawakaraeng</location>
(
<specimenCode box="[691,823,1350,1374]" pageId="49" pageNumber="49">MZB 40991</specimenCode>
, 
<specimenCode box="[833,900,1350,1374]" pageId="49" pageNumber="49">41027</specimenCode>
, 
