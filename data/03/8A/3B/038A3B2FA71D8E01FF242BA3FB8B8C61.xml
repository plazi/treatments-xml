<number box="[340,399,1825,1849]" pageId="5" pageNumber="506" value="1972.0">1972</number>
</bibRefCitation>
, 
<bibRefCitation author="Cardale" box="[410,563,1824,1849]" pageId="5" pageNumber="506" refString="Cardale, J. C. (1985) Vespoidea and Sphecoidea. Zoological Catalog of Australia, 2, 150 - 303." type="journal article" year="1985">
Cardale 
<number box="[504,563,1824,1849]" pageId="5" pageNumber="506" value="1985.0">1985</number>
</bibRefCitation>
