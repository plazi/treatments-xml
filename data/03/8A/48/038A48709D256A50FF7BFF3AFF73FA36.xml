<number box="[1041,1055,1296,1319]" pageId="4" pageNumber="593" value="4.0">4</number>
and S 
<number box="[1122,1165,1296,1319]" pageId="4" pageNumber="593" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
); S 
<number box="[1200,1213,1296,1319]" pageId="4" pageNumber="593" value="8.0">8</number>
