<number box="[629,643,955,979]" pageId="31" pageNumber="32" value="3.0">3</number>
with 
<number box="[706,748,955,979]" pageId="31" pageNumber="32" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
