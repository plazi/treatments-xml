in preparation, 
<collectingCountry box="[729,842,1123,1147]" name="Mexico" pageId="20" pageNumber="21">MEXICO</collectingCountry>
, Veracruz, Volcan San Martin, 
<quantity box="[1199,1287,1122,1147]" metricMagnitude="3" metricUnit="m" metricValue="1.067" pageId="20" pageNumber="21" unit="m" value="1067.0">1067 m</quantity>
, 
<date box="[1301,1432,1122,1147]" pageId="20" pageNumber="21" value="1953-07-14">
<number box="[1301,1330,1122,1147]" pageId="20" pageNumber="21" value="14.0">14</number>
.vii. 
