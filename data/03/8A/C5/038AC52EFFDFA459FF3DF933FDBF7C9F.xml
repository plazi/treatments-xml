<collectingCountry box="[818,931,1899,1923]" name="Mexico" pageId="31" pageNumber="32">MEXICO</collectingCountry>
, Yucatan, Chichen Itza, Berlese sample, leaf mould from moist woods and dry cenote, 
<date box="[653,775,1934,1959]" pageId="31" pageNumber="32" value="1951-06-25">
<number box="[653,684,1934,1959]" pageId="31" pageNumber="32" value="25.0">25</number>
.vi. 
<number box="[715,775,1934,1959]" pageId="31" pageNumber="32" value="1951.0">1951</number>
</date>
, L. J. Stannard, coll., acc. no. 
<number box="[1146,1219,1934,1959]" pageId="31" pageNumber="32" value="49763.0">49763</number>
, 
<collectionCode LSID="urn:lsid:biocol.org:col:34797" box="[1235,1300,1935,1959]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="31" pageNumber="32">INHS</collectionCode>
<number box="[1312,1406,1934,1959]" pageId="31" pageNumber="32" value="579835.0">579,835</number>
; 
<number box="[1422,1436,1935,1959]" pageId="31" pageNumber="32" value="3.0">3</number>
<typeStatus box="[151,258,1971,1995]" pageId="31" pageNumber="32">paratypes</typeStatus>
in preparation and 
<quantity box="[502,565,1970,1995]" metricMagnitude="0" metricUnit="m" metricValue="1.8542" pageId="31" pageNumber="32" unit="in" value="73.0">73 in</quantity>
