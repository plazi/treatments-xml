<subSubSection lastPageId="11" lastPageNumber="302" pageId="10" pageNumber="301" type="description">
<paragraph blockId="10.[151,806,258,342]" box="[151,601,258,280]" pageId="10" pageNumber="301">
<taxonomicName authority="Van Name, 1931" authorityName="Van Name" authorityYear="1931" box="[151,537,258,280]" class="Ascidiacea" family="Styelidae" genus="Stolonica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stolidobranchia" pageId="10" pageNumber="301" phylum="Chordata" rank="species" species="zorritensis">
