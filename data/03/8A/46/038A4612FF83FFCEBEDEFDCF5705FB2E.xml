<taxonomicName authority=": Van Name 1945" authorityName=": Van Name" authorityYear="1945" box="[261,496,757,779]" class="Ascidiacea" family="Styelidae" genus="Symplegma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stolidobranchia" pageId="11" pageNumber="302" phylum="Chordata" rank="species" species="viride">
