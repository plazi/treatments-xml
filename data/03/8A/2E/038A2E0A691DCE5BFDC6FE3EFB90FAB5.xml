<taxonomicName authorityName="Andrássy" authorityYear="2012" box="[278,368,1373,1397]" class="Adenophorea" family="Actinolaimidae" genus="Egtitus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="31" pageNumber="482" phylum="Nematoda" rank="species" species="biformis">
