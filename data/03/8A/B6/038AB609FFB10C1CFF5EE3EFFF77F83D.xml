<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[1210,1296,1873,1896]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="9" pageNumber="41">MNHN</collectionCode>
