<taxonomicName authorityName="Berlese" authorityYear="1904" box="[563,736,1854,1880]" class="Arachnida" family="Parholaspididae" genus="Gamasholaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="427" phylum="Arthropoda" rank="genus">
