<subSubSection pageId="2" pageNumber="188" type="description">
<paragraph blockId="2.[151,1055,151,247]" box="[151,1055,222,247]" pageId="2" pageNumber="188">
http://lsid.speciesfile.org/urn:lsid: 
<taxonomicName box="[522,650,222,247]" class="Insecta" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="188" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
