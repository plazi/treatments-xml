<taxonomicName box="[779,906,290,316]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="13" pageNumber="536" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
; rupestrian grassland, road Andarai-Mucugê, Chapada Diamantina, 
<geoCoordinate box="[401,533,326,353]" degrees="12" direction="south" minutes="58" orientation="latitude" pageId="13" pageNumber="536" precision="15" seconds="14" value="-12.970555">12º58’14”S</geoCoordinate>
, 
<geoCoordinate box="[545,685,326,352]" degrees="41" direction="west" minutes="20" orientation="longitude" pageId="13" pageNumber="536" precision="15" seconds="04" value="-41.334442">41º20’04”W</geoCoordinate>
, 
<quantity box="[697,781,326,352]" metricMagnitude="3" metricUnit="m" metricValue="1.103" pageId="13" pageNumber="536" unit="m" value="1103.0">1103 m</quantity>
asl; 
<date box="[834,1021,326,352]" pageId="13" pageNumber="536" value="2011-01-10">10 January 2011</date>
.
</paragraph>
