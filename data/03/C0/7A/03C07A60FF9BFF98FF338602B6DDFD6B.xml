<specimenCount box="[594,684,1044,1070]" pageId="2" pageNumber="525" type="male">3 males</specimenCount>
on 
<taxonomicName box="[736,975,1044,1070]" class="Liliopsida" family="Poaceae" genus="Paspalum" kingdom="Plantae" order="Poales" pageId="2" pageNumber="525" phylum="Tracheophyta" rank="species" species="carinatum">
<emphasis box="[736,975,1044,1070]" italics="true" pageId="2" pageNumber="525">Paspalum carinatum</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[995,1093,1044,1070]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="2" pageNumber="525" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
); 
<specimenCount box="[1117,1245,1044,1071]" pageId="2" pageNumber="525" type="female">one female</specimenCount>
on 
<taxonomicName class="Liliopsida" family="Poaceae" genus="Andropogon" kingdom="Plantae" order="Poales" pageId="2" pageNumber="525" phylum="Tracheophyta" rank="species" species="leucostachyus">
<emphasis italics="true" pageId="2" pageNumber="525">Andropogon leucostachyus</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[324,422,1080,1106]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="2" pageNumber="525" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
); pedestrian grasslands, Mucugê, Chapada Diamantina, 
<geoCoordinate box="[1054,1186,1080,1107]" degrees="12" direction="south" minutes="53" orientation="latitude" pageId="2" pageNumber="525" precision="15" seconds="49" value="-12.896944">12º53’49”S</geoCoordinate>
, 
<geoCoordinate box="[1199,1339,1080,1107]" degrees="41" direction="west" minutes="18" orientation="longitude" pageId="2" pageNumber="525" precision="15" seconds="58" value="-41.31611">41º18’58”W</geoCoordinate>
, 
<quantity box="[1352,1436,1080,1107]" metricMagnitude="3" metricUnit="m" metricValue="1.125" pageId="2" pageNumber="525" unit="m" value="1125.0">1125 m</quantity>
asl; 
<date box="[197,386,1116,1143]" pageId="2" pageNumber="525" value="2012-01-19">19 January 2012</date>
. One female on 
<taxonomicName box="[567,803,1116,1142]" class="Liliopsida" family="Cyperaceae" genus="Rhynchospora" kingdom="Plantae" order="Poales" pageId="2" pageNumber="525" phylum="Tracheophyta" rank="species" species="tenuis">
<emphasis box="[567,803,1116,1142]" italics="true" pageId="2" pageNumber="525">Rhynchospora tenuis</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[817,954,1116,1142]" class="Liliopsida" family="Cyperaceae" kingdom="Plantae" order="Poales" pageId="2" pageNumber="525" phylum="Tracheophyta" rank="family">Cyperaceae</taxonomicName>
); pedestrian grasslands, crossroad MG-010 with track Morro do Pilar, Morro do Pilar, Serra do Cipó, 
<geoCoordinate box="[799,931,1152,1178]" degrees="19" direction="south" minutes="13" orientation="latitude" pageId="2" pageNumber="525" precision="15" seconds="12" value="-19.220001">19º13’12”S</geoCoordinate>
, 
<geoCoordinate box="[943,1084,1152,1178]" degrees="43" direction="west" minutes="29" orientation="longitude" pageId="2" pageNumber="525" precision="15" seconds="04" value="-43.484444">43º29’04”W</geoCoordinate>
, 
<quantity box="[1095,1180,1152,1179]" metricMagnitude="3" metricUnit="m" metricValue="1.29" pageId="2" pageNumber="525" unit="m" value="1290.0">1290 m</quantity>
