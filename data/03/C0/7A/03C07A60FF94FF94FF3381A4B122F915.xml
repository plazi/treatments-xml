16 (15_17), 
<emphasis box="[1240,1262,1888,1914]" italics="true" pageId="13" pageNumber="536">j6</emphasis>
21 (19–22), 
<emphasis box="[1410,1436,1889,1915]" italics="true" pageId="13" pageNumber="536">J2</emphasis>
22 (20–23), 
<emphasis box="[288,315,1925,1951]" italics="true" pageId="13" pageNumber="536">J5</emphasis>
8 (7–9), 
<emphasis box="[417,441,1925,1951]" italics="true" pageId="13" pageNumber="536">z2</emphasis>
26 (24–27), 
<emphasis box="[585,610,1925,1951]" italics="true" pageId="13" pageNumber="536">z4</emphasis>
27 (25–29), 
<emphasis box="[754,779,1925,1951]" italics="true" pageId="13" pageNumber="536">z5</emphasis>
