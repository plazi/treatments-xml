and 
<taxonomicName box="[194,310,1714,1736]" class="Amphibia" family="Microhylidae" genus="Cophixalus" kingdom="Animalia" order="Anura" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="amabilis">
<emphasis box="[194,310,1714,1736]" italics="true" pageId="4" pageNumber="5">C. amabilis</emphasis>
</taxonomicName>
(filled triangle), and an additional locality for 
<taxonomicName box="[780,935,1714,1736]" class="Amphibia" family="Microhylidae" genus="Cophixalus" kingdom="Animalia" order="Anura" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="albolineatus" status="sp. nov.">
<emphasis box="[780,935,1714,1736]" italics="true" pageId="4" pageNumber="5">C. albolineatus</emphasis>
