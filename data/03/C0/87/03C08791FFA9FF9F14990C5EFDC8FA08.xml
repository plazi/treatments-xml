<subSubSection pageId="14" pageNumber="515" type="materials_examined">
<paragraph blockId="14.[151,1437,754,1463]" pageId="14" pageNumber="515">
<materialsCitation ID-GBIF-Occurrence="1416605856" country="Slovenia" county="Current" location="Museum" municipality="Invertebrata" pageId="14" pageNumber="515" specimenCount="2" stateProvince="Ljubljana" typeStatus="holotype">
