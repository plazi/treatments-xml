<taxonomicName box="[856,1067,364,389]" class="Pinopsida" family="Pinaceae" genus="Tsuga" kingdom="Plantae" order="Pinales" pageId="13" pageNumber="58" phylum="Tracheophyta" rank="species" species="canadensis">
<emphasis box="[856,1067,364,389]" italics="true" pageId="13" pageNumber="58">Tsuga canadensis</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[1141,1305,364,389]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="13" pageNumber="58" phylum="Tracheophyta" rank="species" species="strobus">
<emphasis box="[1141,1305,364,389]" italics="true" pageId="13" pageNumber="58">Pinus strobus</emphasis>
