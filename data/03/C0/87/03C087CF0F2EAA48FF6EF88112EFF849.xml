<taxonomicName authorityName="Alpheraky" authorityYear="1887" box="[789,887,152,176]" class="Insecta" family="Noctuidae" genus="Miselia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="cortex">
