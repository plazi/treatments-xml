. 
<emphasis box="[817,887,153,176]" italics="true" pageId="7" pageNumber="62">cortex</emphasis>
</taxonomicName>
(
<figureCitation box="[903,1011,151,175]" captionStart="FIGURES 1–8" captionStartId="2.[151,263,1847,1870]" captionTargetBox="[162,1436,196,1823]" captionTargetId="figure-100@2.[151,1436,195,1823]" captionTargetPageId="2" captionText="FIGURES 1–8. Atrachea and Parvispinia spp., adults. 1. A. nitens, male, Japan (ZFMK); 2. A. jankowskyi male, Russia Far East, Amur (ZFMK); 3. P. parvispina, lectotype, male, Russia, Siberia (ZISP); 4. P. parvispina, female, Russia, Siberia, Irkutsk (ZMHU); 5. P. cortex, male Kazakhstan (ZISP); 6. P. cortex, lectotype, female, Kyrgyzstan (ZISP); 7. P. caelestina, holotype, male, China, Quinghai (PG); 8. P. caelestina, female, China, Sichuan (AS)." pageId="7" pageNumber="62">Figs. 5, 6</figureCitation>
, 
<figureCitation box="[1024,1054,151,175]" captionStart="FIGURES 17–20" captionStartId="6.[151,263,1841,1864]" captionTargetBox="[151,1427,195,1813]" captionTargetId="figure-62@6.[151,1436,193,1821]" captionTargetPageId="6" captionText="FIGURES 17–20. Parvispinia spp., male genitalia. 17. P.parvispina, Russia, Transbaikalia, 7268LR (HNHM); 18. P. cortex, Kazakhstan, BJ1950 (AS); 19. P. caelestina, holotype, China, Quinghai, PG1247 (PG); 20. P. barkama, holotype, China, Sichuan, BJ1840 (AS)." pageId="7" pageNumber="62">18</figureCitation>
, 
<figureCitation box="[1066,1095,151,175]" captionStart="FIGURES 21–26" captionStartId="8.[151,263,1865,1888]" captionTargetBox="[165,1433,221,1838]" captionTargetId="figure-78@8.[151,1436,217,1845]" captionTargetPageId="8" captionText="FIGURES 21–26. Parvispinia and Atrachea spp., male (21, 22) and female genitalia (23–26). 21. P. geminus, holotype, China, Sichuan, BJ1945 (AS); 22. P. parilis, holotype, China, Sichuan, BJ1943 (AS); 23. Atrachea nitens, Russia Far East, South Kuriles; 24. P. parvispina, Kazakhstan; 25. Ditto, ovipositor enlarged; 16. P. cortex, Kazakhstan, AM (ZISP)." pageId="7" pageNumber="62">26</figureCitation>
) and 
<taxonomicName authorityName="Tschetverikov" authorityYear="1904" box="[1159,1307,152,176]" class="Insecta" family="Noctuidae" genus="Ammoconia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="parvispina">
<emphasis box="[1159,1173,153,176]" italics="true" pageId="7" pageNumber="62">P</emphasis>
. 
<emphasis box="[1187,1307,152,175]" italics="true" pageId="7" pageNumber="62">parvispina</emphasis>
</taxonomicName>
(
<figureCitation box="[1323,1431,151,175]" captionStart="FIGURES 1–8" captionStartId="2.[151,263,1847,1870]" captionTargetBox="[162,1436,196,1823]" captionTargetId="figure-100@2.[151,1436,195,1823]" captionTargetPageId="2" captionText="FIGURES 1–8. Atrachea and Parvispinia spp., adults. 1. A. nitens, male, Japan (ZFMK); 2. A. jankowskyi male, Russia Far East, Amur (ZFMK); 3. P. parvispina, lectotype, male, Russia, Siberia (ZISP); 4. P. parvispina, female, Russia, Siberia, Irkutsk (ZMHU); 5. P. cortex, male Kazakhstan (ZISP); 6. P. cortex, lectotype, female, Kyrgyzstan (ZISP); 7. P. caelestina, holotype, male, China, Quinghai (PG); 8. P. caelestina, female, China, Sichuan (AS)." pageId="7" pageNumber="62">Figs. 3, 4</figureCitation>
, 
<figureCitation box="[151,181,188,212]" captionStart="FIGURES 17–20" captionStartId="6.[151,263,1841,1864]" captionTargetBox="[151,1427,195,1813]" captionTargetId="figure-62@6.[151,1436,193,1821]" captionTargetPageId="6" captionText="FIGURES 17–20. Parvispinia spp., male genitalia. 17. P.parvispina, Russia, Transbaikalia, 7268LR (HNHM); 18. P. cortex, Kazakhstan, BJ1950 (AS); 19. P. caelestina, holotype, China, Quinghai, PG1247 (PG); 20. P. barkama, holotype, China, Sichuan, BJ1840 (AS)." pageId="7" pageNumber="62">17</figureCitation>
, 
<figureCitation box="[192,222,188,212]" captionStart="FIGURES 21–26" captionStartId="8.[151,263,1865,1888]" captionTargetBox="[165,1433,221,1838]" captionTargetId="figure-78@8.[151,1436,217,1845]" captionTargetPageId="8" captionText="FIGURES 21–26. Parvispinia and Atrachea spp., male (21, 22) and female genitalia (23–26). 21. P. geminus, holotype, China, Sichuan, BJ1945 (AS); 22. P. parilis, holotype, China, Sichuan, BJ1943 (AS); 23. Atrachea nitens, Russia Far East, South Kuriles; 24. P. parvispina, Kazakhstan; 25. Ditto, ovipositor enlarged; 16. P. cortex, Kazakhstan, AM (ZISP)." pageId="7" pageNumber="62">24</figureCitation>
