2. 
<taxonomicName authorityName="De Vis" authorityYear="1894" box="[180,336,1556,1582]" class="Aves" family="Neosittidae" genus="Eulacestoma" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="508" phylum="Chordata" rank="genus">
