(
<taxonomicName authority="Leptodirini" authorityName="Leptodirini" box="[611,887,1973,1998]" class="Insecta" family="Cholevidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="516" phylum="Arthropoda" rank="family">Cholevidae, Leptodirini</taxonomicName>
