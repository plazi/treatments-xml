<collectingCountry box="[441,525,746,769]" name="Bolivia" pageId="21" pageNumber="688">Bolivia</collectingCountry>
, 
<collectingCountry box="[540,607,746,769]" name="Brazil" pageId="21" pageNumber="688">Brazil</collectingCountry>
