(
<emphasis bold="true" box="[363,516,1670,1693]" italics="true" pageId="35" pageNumber="702">Pseudechoma</emphasis>
) 
<emphasis bold="true" box="[531,691,1670,1693]" italics="true" pageId="35" pageNumber="702">marginipennis</emphasis>
</emphasis>
</taxonomicName>
</paragraph>
</subSubSection>
