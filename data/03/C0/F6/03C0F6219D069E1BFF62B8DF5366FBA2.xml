and 
<collectingCountry box="[442,505,792,815]" name="Brazil" pageId="11" pageNumber="678">Brazil</collectingCountry>
(
<collectingRegion box="[515,589,792,815]" country="Brazil" name="Parana" pageId="11" pageNumber="678">Paraná</collectingRegion>
