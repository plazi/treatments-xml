<number box="[448,456,2000,2014]" pageId="5" value="2.0">2</number>
<number box="[462,492,1985,2009]" pageId="5" value="21.0">21</number>
, 
<emphasis box="[504,528,1986,2009]" italics="true" pageId="5">ps</emphasis>
