<paragraph blockId="4.[151,1436,439,572]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[151,483,439,464]" pageId="4" pageNumber="5">
<number box="[151,166,439,464]" pageId="4" pageNumber="5" value="2.0">2</number>
. 
<taxonomicName box="[180,305,439,464]" class="Insecta" family="Cantharidae" genus="Taiwanocantharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="malaisei">
<emphasis bold="true" box="[180,305,439,464]" italics="true" pageId="4" pageNumber="5">T. malaisei</emphasis>
</taxonomicName>
species-group.
</emphasis>
