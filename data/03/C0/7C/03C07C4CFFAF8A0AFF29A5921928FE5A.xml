<heading bold="true" box="[151,426,1231,1257]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<emphasis bold="true" box="[151,426,1231,1257]" pageId="4" pageNumber="5">
<taxonomicName box="[151,310,1231,1257]" class="Insecta" family="Cantharidae" genus="Taocantharis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,310,1231,1257]" italics="true" pageId="4" pageNumber="5">Taocantharis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[316,426,1231,1257]" pageId="4" pageNumber="5" rank="genus">gen. nov.</taxonomicNameLabel>
