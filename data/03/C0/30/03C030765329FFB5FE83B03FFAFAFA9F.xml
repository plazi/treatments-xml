<quantity box="[498,623,753,779]" metricMagnitude="-3" metricUnit="m" metricValue="3.25" metricValueMax="3.5" metricValueMin="3.0" pageId="10" pageNumber="11" unit="mm" value="3.25" valueMax="3.5" valueMin="3.0">3–3.5 mm</quantity>
, taken from the green sea urchin 
<taxonomicName class="Echinoidea" family="Strongylocentrotidae" genus="Strongylocentrotus" kingdom="Animalia" order="Camarodonta" pageId="10" pageNumber="11" phylum="Echinodermata" rank="species" species="droebachiensis">
<emphasis italics="true" pageId="10" pageNumber="11">Strongylocentrotus droebachiensis</emphasis>
</taxonomicName>
