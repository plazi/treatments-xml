Figs. 
<number box="[641,673,1602,1628]" pageId="1" pageNumber="2" value="24.39">24</number>
</figureCitation>
, 
<figureCitation box="[686,718,1602,1628]" captionStart="FIGURES 33 – 39" captionStartId="17.[151,269,1781,1805]" captionTargetBox="[151,1436,197,1756]" captionTargetId="figure@17.[151,1436,197,1757]" captionTargetPageId="17" captionText="FIGURES 33 – 39. Lophomilia spp. and Hepatica anceps, adults and male genitalia. 33. L. striatipurpurea, imago (after Holloway 1976); 34. L. striatipurpurea, male genitalia (after Holloway 1976); 35. L. albicosta, China, Tapaishan, prep. 2093 VK ZFMK; 36. L. kobesi, holotype, prep. 6540 GB, Indonesia, Sumatra (ZSM); 37. L. violescens, holotype, Nepal (after Yoshimoto 1993); 38. L. albistria, prep. 6526 GB, Nepal, Kathmandu, Pulchouki Mt. (ZSM); 39. Hepatica anceps, Russia, Primorye terr. prep. AM ZISP." httpUri="https://zenodo.org/record/274664/files/figure.png" pageId="1" pageNumber="2">39</figureCitation>
)] and 
<taxonomicName authority="Moore, 1882" authorityName="Moore" authorityYear="1882" box="[793,1051,1602,1628]" class="Insecta" family="Noctuidae" genus="Coarica" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis box="[793,889,1603,1628]" italics="true" pageId="1" pageNumber="2">Coarica</emphasis>
Moore, 
<number box="[992,1051,1602,1628]" pageId="1" pageNumber="2" value="1882.0">1882</number>
</taxonomicName>
[Type-species: 
<taxonomicName authority="Moore, 1882" authorityName="Moore" authorityYear="1882" class="Insecta" family="Noctuidae" genus="Coarica" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="fasciata">
<emphasis box="[1238,1436,1603,1628]" italics="true" pageId="1" pageNumber="2">Coarica fasciata</emphasis>
