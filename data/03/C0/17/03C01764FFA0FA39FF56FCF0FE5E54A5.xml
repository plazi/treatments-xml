<taxonomicName authority="Staudinger, 1892" authorityName="Staudinger" authorityYear="1892" box="[946,1259,1564,1590]" class="Insecta" family="Noctuidae" genus="Hepatica" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis box="[946,1054,1564,1589]" italics="true" pageId="1" pageNumber="2">Hepatica</emphasis>
Staudinger, 
<number box="[1200,1259,1564,1590]" pageId="1" pageNumber="2" value="1892.0">1892</number>
</taxonomicName>
[Type-species: 
<taxonomicName authority="Staudinger, 1892" authorityName="Staudinger" authorityYear="1892" box="[151,557,1602,1628]" class="Insecta" family="Noctuidae" genus="Hepatica" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="anceps">
<emphasis box="[151,349,1603,1628]" italics="true" pageId="1" pageNumber="2">Hepatica anceps</emphasis>
