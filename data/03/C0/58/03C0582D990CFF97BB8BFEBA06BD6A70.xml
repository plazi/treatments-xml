<emphasis bold="true" box="[151,349,1731,1755]" pageId="32" pageNumber="33">FIGURES 80–87.</emphasis>
<taxonomicName authority="Andersen &amp; Saether &amp; Mendes, 2010" authorityName="Andersen &amp; Saether &amp; Mendes" authorityYear="2010" box="[356,658,1731,1754]" class="Insecta" family="Chironomidae" genus="Pseudosmittia" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="cambuciensis" status="sp. n.">
<emphasis box="[356,658,1731,1754]" italics="true" pageId="32" pageNumber="33">Pseudosmittia cambuciensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[670,737,1731,1755]" pageId="32" pageNumber="33">
<taxonomicNameLabel box="[670,731,1731,1755]" pageId="32" pageNumber="33" rank="species">sp. n.</taxonomicNameLabel>
,
</emphasis>
male. 
<emphasis bold="true" box="[809,836,1731,1755]" pageId="32" pageNumber="33">80</emphasis>
—tentorium, stipes, and cibarial pump; 
<emphasis bold="true" box="[1260,1287,1731,1755]" pageId="32" pageNumber="33">81</emphasis>
—palp; 
<emphasis bold="true" box="[1373,1400,1731,1755]" pageId="32" pageNumber="33">82</emphasis>
— thorax; 
<emphasis bold="true" box="[233,260,1767,1791]" pageId="32" pageNumber="33">83</emphasis>
