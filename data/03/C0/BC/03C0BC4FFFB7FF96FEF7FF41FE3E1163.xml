(
<figureCitation box="[503,659,176,201]" captionStart="Figure 3" captionStartId="7.[160,225,1675,1698]" captionTargetBox="[172,1153,464,1644]" captionTargetId="figure@7.[163,1153,464,1644]" captionTargetPageId="7" captionText="Figure 3. (A–C) Thoradonta spiculobaoides sp. nov.: (A, B) lateral and dorsal views of male body; (C) oblique view of female body (scale bar 1 mm)." figureDoi="http://doi.org/10.5281/zenodo.3990469" httpUri="https://zenodo.org/record/3990469/files/figure.png" pageId="7" pageNumber="840">Figures 3A–C</figureCitation>
, 
<figureCitation box="[674,734,176,201]" captionStart="Figure 4" captionStartId="8.[160,225,813,836]" captionTargetBox="[162,1152,142,783]" captionTargetId="figure@8.[162,1153,142,783]" captionTargetPageId="8" captionText="Figure 4. (A–F) Thoradonta spiculobaoides sp. nov.: (A) dorsal view of male head and anterior pronotum; (B) frontal view of male head; (C) lateral view of male head; (D) oblique-dorsal view of male subgenital plate; (E) lateral view of female ovipositor; (F) ventral view of female subgenital plate (scale bar 1 mm)." figureDoi="http://doi.org/10.5281/zenodo.3990471" httpUri="https://zenodo.org/record/3990471/files/figure.png" pageId="7" pageNumber="840">4A–F</figureCitation>
, 
<figureCitation box="[748,813,176,201]" captionStart="Figure 5" captionStartId="8.[160,225,1508,1531]" captionTargetBox="[163,1153,963,1476]" captionTargetId="figure@8.[163,1153,963,1476]" captionTargetPageId="8" captionText="Figure 5. (A–D) Thoradonta spiculobaoides sp. nov. male: (A) lateral view of fore femur; (B) lateral view of hind femur; (C) lateral view of tegmen and mid femur; (D) lateral view of hind tibia (scale bar 1 mm)." figureDoi="http://doi.org/10.5281/zenodo.3990473" httpUri="https://zenodo.org/record/3990473/files/figure.png" pageId="7" pageNumber="840">5A–D</figureCitation>
