<number box="[591,605,1119,1145]" pageId="1" pageNumber="2" value="2.0">2</number>
and 
<number box="[663,692,1119,1145]" pageId="1" pageNumber="2" value="74.0">74</number>
(
<number box="[709,780,1119,1145]" pageId="1" pageNumber="2" value="74.0" valueMax="78.0" valueMin="70.0">70­78</number>
