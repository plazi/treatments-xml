<subSubSection pageId="43" pageNumber="44" type="distribution">
<paragraph blockId="43.[151,1437,546,1619]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[199,536,1554,1581]" pageId="43" pageNumber="44">Distribution and phenology.</emphasis>
