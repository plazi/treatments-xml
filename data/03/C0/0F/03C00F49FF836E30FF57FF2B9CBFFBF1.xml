<taxonomicName authorityName="Jaschhof &amp; Jaschhof" authorityYear="2020" box="[1039,1200,1088,1113]" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="warraensis">
<emphasis box="[1039,1200,1088,1113]" italics="true" pageId="8" pageNumber="9">W. warraensis</emphasis>
</taxonomicName>
, the first 
<taxonomicName authorityName="Rondani" authorityYear="1860" box="[1315,1436,1088,1113]" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
