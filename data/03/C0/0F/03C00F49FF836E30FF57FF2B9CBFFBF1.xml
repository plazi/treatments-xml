<heading bold="true" box="[151,467,151,178]" fontSize="11" level="2" pageId="8" pageNumber="9" reason="4">
<emphasis bold="true" box="[151,467,151,178]" pageId="8" pageNumber="9">
<taxonomicName authorityName="Mohrig &amp; Mamaev" authorityYear="1970" box="[151,386,151,178]" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="discreta">
<emphasis bold="true" box="[151,386,151,178]" italics="true" pageId="8" pageNumber="9">Winnertzia discreta</emphasis>
