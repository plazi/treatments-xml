<taxonomicName authority="Felt" authorityName="Felt" authorityYear="1908" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="rubida">
