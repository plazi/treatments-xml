<taxonomicName baseAuthorityName="Jaschhof &amp; Jaschhof" baseAuthorityYear="2013" box="[935,1056,475,500]" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="genus">
<emphasis box="[935,1056,475,500]" italics="true" pageId="50" pageNumber="51">Winnertzia</emphasis>
