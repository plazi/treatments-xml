and 
<taxonomicName authorityName="Jaschhof &amp; Jaschhof" authorityYear="2020" box="[982,1153,1907,1931]" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="ruliki">
