<emphasis bold="true" box="[189,250,889,915]" pageId="32" pageNumber="33">
<typeStatus box="[189,250,889,915]" pageId="32" pageNumber="33">Type</typeStatus>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="32.[189,1398,940,1005]" pageId="32" pageNumber="33">
<collectingRegion box="[189,334,940,966]" country="Indonesia" name="Sumatera Utara" pageId="32" pageNumber="33">SUMATRA</collectingRegion>
