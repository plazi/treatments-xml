(KU721885) and 
<taxonomicName baseAuthorityName="Carl" baseAuthorityYear="1902" class="Diplopoda" family="Paradoxosomatidae" genus="Pogonosternum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Pogonosternum nigrovirgatum" order="Polydesmida" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="nigrovirgatum">
