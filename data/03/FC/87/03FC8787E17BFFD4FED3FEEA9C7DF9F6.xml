(Philippi, 
<number box="[984,1046,663,689]" pageId="16" pageNumber="17" value="1887.0">1887</number>
)
</taxonomicName>
, Hawaii, 
<collectingCountry box="[1166,1225,663,689]" name="United States of America" pageId="16" pageNumber="17">USA</collectingCountry>
