<taxonomicName authority="Heller, 1923" authorityName="Heller" authorityYear="1923" box="[298,664,832,856]" class="Insecta" family="Cerambycidae" genus="Distenia" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="466" phylum="Arthropoda" rank="species" species="heterotarsalis">
