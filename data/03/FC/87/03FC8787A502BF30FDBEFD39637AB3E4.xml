</taxonomicName>
(
<taxonomicName box="[563,740,305,332]" class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
) and grasses (
<taxonomicName box="[914,1017,305,331]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
