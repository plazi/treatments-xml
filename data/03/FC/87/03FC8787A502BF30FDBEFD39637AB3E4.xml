<taxonomicName baseAuthorityName="Fabricius" baseAuthorityYear="1782" box="[478,630,376,402]" class="Insecta" family="Formicidae" genus="Lasius" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="flavus">
