<quantity box="[1339,1406,583,608]" metricMagnitude="0" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="11" pageNumber="119" unit="m" value="4.0" valueMax="5.0" valueMin="3.0">3-5 m</quantity>
