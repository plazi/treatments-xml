<taxonomicName authority="Shaw, 1792" authorityName="Shaw" authorityYear="1792" box="[413,876,1737,1763]" class="Actinopterygii" family="Istiophoridae" genus="Istiophorus" kingdom="Animalia" order="Perciformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="platypterus">
<emphasis box="[413,695,1737,1762]" italics="true" pageId="13" pageNumber="14">Istiophorus platypterus</emphasis>
(Shaw, 
<number box="[808,868,1737,1763]" pageId="13" pageNumber="14" value="1792.0">1792</number>
)
</taxonomicName>
(
<taxonomicName box="[900,1062,1737,1763]" class="Actinopterygii" family="Istiophoridae" kingdom="Animalia" order="Perciformes" pageId="13" pageNumber="14" phylum="Chordata" rank="family">Istiophoridae</taxonomicName>
