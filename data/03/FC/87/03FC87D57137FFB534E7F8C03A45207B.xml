</tr>
<tr box="[159,1265,1187,1209]" gridrow="2" pageId="17" pageNumber="318">
<td box="[159,245,1187,1209]" gridcol="0" gridrow="2" pageId="17" pageNumber="318">leg 1</td>
<td box="[480,617,1187,1209]" gridcol="1" gridrow="2" pageId="17" pageNumber="318">1-0</td>
<td box="[801,933,1187,1209]" gridcol="2" gridrow="2" pageId="17" pageNumber="318">0-1;0-1;1,2,2</td>
<td box="[1122,1265,1187,1209]" gridcol="3" gridrow="2" pageId="17" pageNumber="318">I-0;0-1;I,2,2</td>
</tr>
<tr box="[159,1265,1228,1250]" gridrow="3" pageId="17" pageNumber="318">
<td box="[159,245,1228,1250]" gridcol="0" gridrow="3" pageId="17" pageNumber="318">legs 2–4</td>
<td box="[480,617,1228,1250]" gridcol="1" gridrow="3" pageId="17" pageNumber="318">1-0</td>
