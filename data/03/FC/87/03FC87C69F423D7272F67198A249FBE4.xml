<subSubSection box="[199,1135,151,176]" pageId="10" pageNumber="11" type="etymology">
<paragraph blockId="10.[151,1436,151,1077]" box="[199,1135,151,176]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[199,334,151,176]" pageId="10" pageNumber="11">Etymology.</emphasis>
