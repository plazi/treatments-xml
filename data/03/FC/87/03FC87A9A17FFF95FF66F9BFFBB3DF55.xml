<subSubSection lastPageId="10" lastPageNumber="539" pageId="9" pageNumber="538" type="diagnosis">
<paragraph blockId="9.[151,1437,151,717]" pageId="9" pageNumber="538">
<emphasis bold="true" box="[199,321,655,680]" pageId="9" pageNumber="538">Diagnosis.</emphasis>
