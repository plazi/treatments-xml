<collectingCountry box="[568,658,1276,1299]" name="Suriname" pageId="4" pageNumber="51">Surinam</collectingCountry>
(
<figureCitation box="[673,750,1276,1299]" captionStart="FIGURE 21" captionStartId="20.[151,254,1504,1528]" captionTargetBox="[151,1436,194,1441]" captionTargetId="figure@20.[151,1436,194,1480]" captionTargetPageId="20" captionText="FIGURE 21. Distribution of Mesoleptobasis." httpUri="https://zenodo.org/record/188667/files/figure.png" pageId="4" pageNumber="51">
Fig. 
<number box="[723,750,1276,1299]" pageId="4" pageNumber="51" value="21.0">21</number>
</figureCitation>
) ............................................................. 
