<collectingCountry box="[765,814,956,979]" name="Peru" pageId="4" pageNumber="51">Peru</collectingCountry>
(
<figureCitation box="[829,906,956,979]" captionStart="FIGURE 21" captionStartId="20.[151,254,1504,1528]" captionTargetBox="[151,1436,194,1441]" captionTargetId="figure@20.[151,1436,194,1480]" captionTargetPageId="20" captionText="FIGURE 21. Distribution of Mesoleptobasis." httpUri="https://zenodo.org/record/188667/files/figure.png" pageId="4" pageNumber="51">
