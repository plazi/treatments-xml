<emphasis bold="true" box="[151,297,1261,1288]" pageId="25" pageNumber="26">Description.</emphasis>
<typeStatus box="[304,412,1262,1288]" pageId="25" pageNumber="26">Holotype</typeStatus>
. Male. Length 
<quantity box="[580,669,1262,1289]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="25" pageNumber="26" unit="mm" value="3.2">3.2 mm</quantity>
. LFW 
<quantity box="[745,834,1262,1289]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="25" pageNumber="26" unit="mm" value="3.4">3.4 mm</quantity>
