<collectingCountry box="[518,603,1045,1070]" name="Bolivia" pageId="8" pageNumber="251">Bolivia</collectingCountry>
, 
<collectingCountry box="[614,719,1046,1070]" name="Paraguay" pageId="8" pageNumber="251">Paraguay</collectingCountry>
, 
