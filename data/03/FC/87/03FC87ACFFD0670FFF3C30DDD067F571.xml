° S
</geoCoordinate>
, 
<geoCoordinate box="[577,745,802,826]" direction="east" orientation="longitude" pageId="4" pageNumber="5" precision="1" value="133.8947">
<number box="[577,713,802,826]" pageId="4" pageNumber="5" value="133.894701">133.894701</number>
° E
</geoCoordinate>
) and Gita (- 
<geoCoordinate box="[877,1020,802,826]" direction="south" orientation="latitude" pageId="4" pageNumber="5" precision="1" value="-1.393074">
<number box="[877,988,802,826]" pageId="4" pageNumber="5" value="1.393074">1.393074</number>
° S
</geoCoordinate>
, 
<geoCoordinate box="[1032,1203,802,826]" direction="east" orientation="longitude" pageId="4" pageNumber="5" precision="1" value="133.95787">
