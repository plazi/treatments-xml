/ 
<number box="[1008,1056,296,320]" pageId="14" pageNumber="15" value="0.81">0.81</number>
/ 
<number box="[1065,1115,296,320]" pageId="14" pageNumber="15" value="0.94">0.94</number>
; metatarsi + tarsi 1.00/ 
<number box="[1378,1427,295,320]" pageId="14" pageNumber="15" value="0.96">0.96</number>
/ 
<number box="[151,203,332,357]" pageId="14" pageNumber="15" value="0.94">0.94</number>
/ 
<number box="[212,260,332,357]" pageId="14" pageNumber="15" value="1.05">1.05</number>
