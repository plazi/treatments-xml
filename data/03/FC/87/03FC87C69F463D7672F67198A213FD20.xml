<number box="[334,384,332,357]" pageId="14" pageNumber="15" value="2.92">2.92</number>
/ 
<number box="[393,442,332,357]" pageId="14" pageNumber="15" value="2.67">2.67</number>
/ 
<number box="[450,500,332,357]" pageId="14" pageNumber="15" value="2.5">2.50</number>
/ 
<number box="[509,557,332,357]" pageId="14" pageNumber="15" value="2.91">2.91</number>
. Eye diameter and interdistance: AME 
<number box="[1014,1066,332,356]" pageId="14" pageNumber="15" value="0.1">0.10</number>
, ALE 
<number box="[1144,1196,332,356]" pageId="14" pageNumber="15" value="0.07">0.07</number>
