<quantity box="[1094,1204,1782,1807]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" pageId="153" pageNumber="154" unit="mm" value="13.5">13.5 mm</quantity>
), BMNH 1993.876; 7709#44, 1250– 
<quantity box="[333,420,1818,1843]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="153" pageNumber="154" unit="m" value="1500.0">1500 m</quantity>
