<taxonomicName authority="Bjornberg" authorityName="Bjornberg" box="[294,646,1673,1695]" class="Hexanauplia" family="Megacalanidae" genus="Bathycalanus" higherTaxonomySource="CoL" kingdom="Animalia" order="Calanoida" pageId="124" pageNumber="125" phylum="Arthropoda" rank="species" species="unicornis">
