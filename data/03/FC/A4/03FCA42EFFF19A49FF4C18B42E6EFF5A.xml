<taxonomicName authority="Ashmead" authorityName="Ashmead" box="[662,889,1941,1964]" class="Insecta" family="Xyelidae" genus="Megaxyela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="99" phylum="Arthropoda" rank="genus">
