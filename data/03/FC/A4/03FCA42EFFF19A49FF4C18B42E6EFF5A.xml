<paragraph blockId="5.[151,1436,1282,2033]" pageId="5" pageNumber="99">
<taxonomicName box="[199,313,1282,1305]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="5" pageNumber="99" phylum="Arthropoda" rank="genus">
