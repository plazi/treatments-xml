<number box="[661,713,770,794]" pageId="14" pageNumber="453" value="0.29">0.29</number>
) = 
<number box="[751,803,770,794]" pageId="14" pageNumber="453" value="1.38">1.38</number>
. 
<number box="[814,824,770,794]" pageId="14" pageNumber="453" value="1.0">1</number>
RS length = 
<quantity box="[970,1074,770,794]" metricMagnitude="-5" metricUnit="m" metricValue="9.0" pageId="14" pageNumber="453" unit="mm" value="0.09">0.09 mm</quantity>
; 
