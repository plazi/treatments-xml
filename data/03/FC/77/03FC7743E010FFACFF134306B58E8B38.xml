; inter tentorial pit distance = 
<quantity box="[1229,1332,1920,1944]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="12" pageNumber="451" unit="mm" value="0.3">0.30 mm</quantity>
; width of face at dorsal clypeal edge = 
<quantity box="[488,593,1956,1981]" metricMagnitude="-4" metricUnit="m" metricValue="5.4" pageId="12" pageNumber="451" unit="mm" value="0.54">0.54 mm</quantity>
; clypeus height = 
<quantity box="[804,908,1957,1981]" metricMagnitude="-4" metricUnit="m" metricValue="1.1" pageId="12" pageNumber="451" unit="mm" value="0.11">0.11 mm</quantity>
