<collectingCountry box="[189,259,1896,1922]" name="China" pageId="29" pageNumber="30">China</collectingCountry>
(
<collectingRegion box="[275,374,1896,1922]" country="China" name="Qinghai" pageId="29" pageNumber="30">Qinghai</collectingRegion>
