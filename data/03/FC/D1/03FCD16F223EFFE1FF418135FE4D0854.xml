</taxonomicName>
— 
<collectingCountry box="[1055,1110,1196,1221]" name="Peru" pageId="11" pageNumber="570">Peru</collectingCountry>
; 
<taxonomicName authority="(Brolemann, 1902)" authorityName="Brolemann" authorityYear="1902" baseAuthorityName="Brolemann" baseAuthorityYear="1902" class="Chilopoda" family="Cryptopidae" genus="Cryptops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scolopendromorpha" pageId="11" pageNumber="570" phylum="Arthropoda" rank="species" species="iheringi" subGenus="Trigonocryptops">
<emphasis box="[1120,1146,1197,1220]" italics="true" pageId="11" pageNumber="570">C.</emphasis>
(
<emphasis box="[1163,1182,1197,1220]" italics="true" pageId="11" pageNumber="570">T.</emphasis>
) 
<emphasis box="[1199,1288,1196,1221]" italics="true" pageId="11" pageNumber="570">iheringi</emphasis>
(Brölemann, 1902)
</taxonomicName>
, 
<taxonomicName authority="Ribaut, 1923" authorityName="Ribaut" authorityYear="1923" box="[230,654,1231,1256]" class="Chilopoda" family="Cryptopidae" genus="Cryptops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scolopendromorpha" pageId="11" pageNumber="570" phylum="Arthropoda" rank="subSpecies" species="sarasini" subGenus="Trigonocryptops" subSpecies="furcatus">
