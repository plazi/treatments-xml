<paragraph blockId="6.[151,496,1858,1883]" box="[151,496,1858,1883]" pageId="6" pageNumber="505">
<heading bold="true" box="[151,496,1858,1883]" fontSize="10" level="2" pageId="6" pageNumber="505" reason="2">
