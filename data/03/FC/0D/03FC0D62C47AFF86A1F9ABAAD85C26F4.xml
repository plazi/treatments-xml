<taxonomicName authority="(Signoret, 1875)" baseAuthorityName="Signoret" baseAuthorityYear="1875" box="[151,556,151,176]" class="Insecta" family="Pseudococcidae" genus="Planococcus" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="516" phylum="Arthropoda" rank="species" species="ficus">
