<taxonomicName authorityName="Penz" authorityYear="2021" baseAuthorityName="Zikan" baseAuthorityYear="1942" box="[757,864,1922,1945]" class="Insecta" family="Nymphalidae" genus="Cithaerias" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="304" phylum="Arthropoda" rank="species" species="azurina">
