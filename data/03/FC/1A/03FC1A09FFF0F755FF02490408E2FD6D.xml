, 
<collectingCountry box="[516,603,1647,1671]" name="Bolivia" pageId="49" pageNumber="48">Bolivia</collectingCountry>
and 
<collectingCountry box="[661,780,1648,1672]" name="Paraguay" pageId="49" pageNumber="48">Paraguay</collectingCountry>
(
<bibRefCitation author="Ratcliffe, B. C." box="[793,967,1647,1671]" pageId="49" pageNumber="48" pagination="93 - 204" refId="ref27306" refString="Ratcliffe, B. C. 1976. A revision of the genus Strategus (Coleoptera: Scarabaeidae). Bulletin of the University of Nebraska State Museum 10: 93 - 204." type="journal article" year="1976">Ratcliffe 1976</bibRefCitation>
).
</paragraph>
