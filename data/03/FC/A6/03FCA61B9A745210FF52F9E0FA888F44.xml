<collectingRegion box="[992,1190,1828,1854]" country="Turkey" name="Antalya" pageId="4" pageNumber="296">Antalya Province</collectingRegion>
, 
<collectingCountry box="[1201,1283,1828,1854]" name="Turkey" pageId="4" pageNumber="296">Turkey</collectingCountry>
.
