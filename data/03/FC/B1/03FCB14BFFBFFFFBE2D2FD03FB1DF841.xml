</number>
<emphasis italics="true" pageId="5" pageNumber="6">, an</emphasis>
<number box="[179,187,1797,1810]" pageId="5" pageNumber="6" value="1.0">
<emphasis box="[179,187,1797,1810]" italics="true" pageId="5" pageNumber="6">1</emphasis>
</number>
