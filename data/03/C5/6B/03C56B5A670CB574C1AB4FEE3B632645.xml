<taxonomicName baseAuthorityName="Mazur" baseAuthorityYear="2002" box="[1173,1331,1716,1741]" class="Magnoliopsida" family="Tamaricaceae" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Caryophyllales" pageId="345" pageNumber="346" phylum="Tracheophyta" rank="family">Tamaricaceae</taxonomicName>
