</taxonomicName>
, 
<taxonomicName box="[1177,1264,794,817]" class="Magnoliopsida" family="Fabaceae" genus="Genista" kingdom="Plantae" order="Fabales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis box="[1177,1264,794,817]" italics="true" pageId="23" pageNumber="24">Genista</emphasis>
</taxonomicName>
, 
<taxonomicName authority="(Frieser 1981)" baseAuthorityName="Frieser" baseAuthorityYear="1981" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis box="[1278,1342,794,817]" italics="true" pageId="23" pageNumber="24">Pinus</emphasis>
(
<bibRefCitation author="Frieser, R." pageId="23" pageNumber="24" pagination="33 - 107" refId="ref389501" refString="Frieser, R. (1981) Die Anthribiden der Westpalaearktis einschlisslich der Arten der UdSSR (Coleoptera, Anthribidae). Mitteilungen der munchner entomologische Gesellschaft, 71, 33 - 107." type="journal article" year="1981">Frieser 1981</bibRefCitation>
)
</taxonomicName>
