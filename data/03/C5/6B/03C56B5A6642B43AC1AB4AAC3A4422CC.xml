<subSubSection pageId="23" pageNumber="24" type="biology_ecology">
<paragraph blockId="23.[151,1436,613,926]" pageId="23" pageNumber="24">
<emphasis bold="true" box="[199,295,793,818]" pageId="23" pageNumber="24">Biology.</emphasis>
