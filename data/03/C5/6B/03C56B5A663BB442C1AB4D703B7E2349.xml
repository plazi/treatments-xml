(
<taxonomicName authorityName="R.Brown" authorityYear="1833" box="[1254,1429,439,464]" class="Magnoliopsida" family="Limnanthaceae" kingdom="Plantae" order="Brassicales" pageId="111" pageNumber="112" phylum="Tracheophyta" rank="family">Limnanthaceae</taxonomicName>
) (
<bibRefCitation author="Dieckmann, L." box="[158,370,476,501]" pageId="111" pageNumber="112" pagination="3 - 128" refId="ref387872" refString="Dieckmann, L. (1972 b) Beitrage zur Insektenfauna der DDR, Coleoptera-Curculionidae, Ceutorhynchinae. Beitrage zur Entomologie, 22, 3 - 128. https: // doi. org / 10.21248 / contrib. entomol. 22.1 - 2.3 - 128" type="journal article" year="1972">Dieckmann 1972b</bibRefCitation>
; 
<bibRefCitation author="Kryzhanovskaya, T. V." box="[381,636,476,501]" pageId="111" pageNumber="112" pagination="304 - 309" refId="ref396047" refString="Kryzhanovskaya, T. V. (1977) K izucheniiu troficheskikh sviazei dolgonosikov-skrytnokhobotnikov roda Ceuthorrhynchus Germ. (Coleoptera, Curculionidae) levoberezhnoi Ukrainy i Kryma. Entomologicheskoe Obozrenie, 56 (2), 304 - 309. [in Russian]" type="journal article" year="1977">Kryzhanovskaya 1977</bibRefCitation>
