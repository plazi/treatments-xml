</taxonomicName>
(
<taxonomicName authorityName="E.P.Ventenat" authorityYear="1799" box="[158,315,367,392]" class="Liliopsida" family="Alismataceae" kingdom="Plantae" order="Alismatales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="family">Alismataceae</taxonomicName>
), 
<taxonomicName box="[334,461,367,392]" class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Polygonales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="genus">
<emphasis box="[334,461,367,392]" italics="true" pageId="87" pageNumber="88">Polygonum</emphasis>
</taxonomicName>
and 
<taxonomicName box="[516,592,369,392]" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Polygonales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="genus">
<emphasis box="[516,592,369,392]" italics="true" pageId="87" pageNumber="88">Rumex</emphasis>
</taxonomicName>
(
<taxonomicName box="[607,766,367,392]" class="Magnoliopsida" family="Polygonaceae" kingdom="Plantae" order="Polygonales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="family">Polygonaceae</taxonomicName>
