<paragraph blockId="369.[151,1436,1290,1819]" pageId="369" pageNumber="370">
<emphasis bold="true" box="[199,273,1686,1711]" pageId="369" pageNumber="370">Notes.</emphasis>
<taxonomicName authorityName="Gebler" authorityYear="1833" box="[284,456,1686,1711]" class="Insecta" family="Curculionidae" genus="Cleonus" kingdom="Animalia" order="Coleoptera" pageId="369" pageNumber="370" phylum="Arthropoda" rank="species" species="adumbratus">
