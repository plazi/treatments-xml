<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[151,348,223,248]" class="Magnoliopsida" family="Scrophulariaceae" kingdom="Plantae" order="Lamiales" pageId="194" pageNumber="195" phylum="Tracheophyta" rank="family">Scrophulariaceae</taxonomicName>
: 
<taxonomicName baseAuthorityName="Gumovsky" baseAuthorityYear="1997" box="[360,576,223,248]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="194" pageNumber="195" phylum="Tracheophyta" rank="species" species="thapsus">
<emphasis box="[360,576,223,248]" italics="true" pageId="194" pageNumber="195">Verbascum thapsus</emphasis>
</taxonomicName>
(
<bibRefCitation author="Wingelmuller, A." box="[592,816,223,248]" pageId="194" pageNumber="195" pagination="143 - 221" refId="ref414037" refString="Wingelmuller, A. (1937) Monographie der palaarktischen Arten der Tribus Cionini. Koleopterologische Rundschau, 23 (4 - 5), 143 - 221." type="journal article" year="1937">Wingelmüller 1937</bibRefCitation>
; 
<bibRefCitation author="Smreczynski, S." box="[829,1041,223,248]" pageId="194" pageNumber="195" refId="ref409378" refString="Smreczynski, S. (1976) Ryjkowce-Curculionidae. Podrodzina Curculioninae. Plemiona: Nanophyini, Mecinini, Cionini, Anoplini, Rhynchaenini i uzupelnienia do zeszytow 98 a-e (z 268 rysunkami). Klucze do oznaczania owadow Polski. Cz. 19, z. 98 f. Panstwowe Wydawnyctwo Naukowe, Warszawa, 115 pp." type="book" year="1976">Smreczyński 1976</bibRefCitation>
; Gumovsky 1997); 
<taxonomicName box="[1266,1429,223,248]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="194" pageNumber="195" phylum="Tracheophyta" rank="species" species="densiflorum">
<emphasis box="[1266,1429,223,248]" italics="true" pageId="194" pageNumber="195">V. densiflorum</emphasis>
</taxonomicName>
