<taxonomicName authority="(Dedyukhin 2012)" baseAuthorityName="Dedyukhin" baseAuthorityYear="2012" box="[151,476,260,285]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="194" pageNumber="195" phylum="Tracheophyta" rank="species" species="nigrum">
