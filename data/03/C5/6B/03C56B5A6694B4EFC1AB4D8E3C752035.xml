<taxonomicName authority="longicollis" class="Insecta" family="Curculionidae" genus="Cionus" kingdom="Animalia" order="Coleoptera" pageId="194" pageNumber="195" phylum="Arthropoda" rank="subSpecies" species="longicollis" subSpecies="longicollis">
