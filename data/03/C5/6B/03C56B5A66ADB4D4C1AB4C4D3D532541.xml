<subSubSection pageId="249" pageNumber="250" type="biology_ecology">
<paragraph blockId="249.[151,1437,151,1040]" pageId="249" pageNumber="250">
<emphasis bold="true" box="[199,295,943,968]" pageId="249" pageNumber="250">Biology.</emphasis>
