<taxonomicName authority="(Hoffmann 1958)" baseAuthorityName="Hoffmann" baseAuthorityYear="1958" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="210" pageNumber="211" phylum="Chordata" rank="species" species="cinerea">
