<collectingCountry box="[1210,1277,763,787]" name="Libya" pageId="12" pageNumber="494">Libya</collectingCountry>
, 
<collectingCountry box="[1289,1366,763,787]" name="Turkey" pageId="12" pageNumber="494">Turkey</collectingCountry>
, and 
<collectingCountry box="[1430,1489,763,787]" name="Israel" pageId="12" pageNumber="494">Israel</collectingCountry>
(
<bibRefCitation author="Castagnoli M. &amp; Pegazzano F." box="[499,826,798,823]" pageId="12" pageNumber="494" pagination="281 - 297" refId="ref16019" refString="Castagnoli M., Pegazzano F. 1979. Rassegna degli Acari Tenuipalpidi dell'olivo con descrizione di Hystripalpus rotai sp. nov. Redia, 62: 281 - 297." type="journal article" year="1979">Castagnoli &amp; Pegazzano, 1979</bibRefCitation>
