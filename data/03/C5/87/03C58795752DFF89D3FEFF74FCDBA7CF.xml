<collectingCountry box="[1001,1079,763,787]" name="Greece" pageId="12" pageNumber="494">Greece</collectingCountry>
, 
<collectingCountry box="[1091,1136,764,787]" name="Iran" pageId="12" pageNumber="494">Iran</collectingCountry>
, 
<collectingCountry box="[1148,1198,763,787]" name="Italy" pageId="12" pageNumber="494">Italy</collectingCountry>
