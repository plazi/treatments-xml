<taxonomicName box="[884,1004,1325,1347]" class="Insecta" family="Chrysomelidae" genus="Ceropachys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="334" phylum="Arthropoda" rank="genus">
