: 
<taxonomicName box="[1270,1387,1325,1347]" class="Insecta" family="Zopheridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="334" phylum="Arthropoda" rank="family">Colydiidae</taxonomicName>
].
