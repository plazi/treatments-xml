<emphasis box="[558,651,1044,1070]" italics="true" pageId="5" pageNumber="4">
<taxonomicName authorityName="Opitz" authorityYear="2022" box="[558,646,1044,1070]" class="Insecta" family="Cleridae" genus="Onychotillus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="lineatus">lineatus</taxonomicName>
