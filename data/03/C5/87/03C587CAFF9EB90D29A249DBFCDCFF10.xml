<taxonomicName authority="Fabricius, 1804" authorityName="Fabricius" authorityYear="1804" box="[294,622,1805,1827]" class="Insecta" family="Ichneumonidae" genus="Ophion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="166" phylum="Arthropoda" rank="species" species="nutritor">
