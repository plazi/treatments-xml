<collectingCountry box="[151,206,620,645]" name="Italy" pageId="50" pageNumber="51">Italy</collectingCountry>
</materialsCitation>
; 
<materialsCitation ID-GBIF-Occurrence="1324948206" collectorName="Di Geronimo &amp; La Perna" country="Denmark" location="southern Italy" pageId="50" pageNumber="51" specimenCount="1">
