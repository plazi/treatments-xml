<taxonomicName authorityName="Spangler &amp; Steiner" authorityYear="2005" box="[900,1027,1361,1380]" class="Insecta" family="Meruidae" genus="Meru" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="346" phylum="Arthropoda" rank="species" species="phyllisae">
