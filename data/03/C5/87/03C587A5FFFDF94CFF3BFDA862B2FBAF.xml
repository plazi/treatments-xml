<taxonomicName authority="Monterosato, 1880" authorityName="Monterosato" authorityYear="1880" box="[151,583,752,774]" class="Gastropoda" family="Skeneidae" genus="Akritogyra" higherTaxonomySource="CoL" kingdom="Animalia" order="Seguenziida" pageId="57" pageNumber="58" phylum="Mollusca" rank="species" species="conspicua">
