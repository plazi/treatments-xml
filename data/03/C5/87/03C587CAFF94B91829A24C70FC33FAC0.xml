<taxonomicName authority="Gravenhorst, 1829" authorityName="Gravenhorst" authorityYear="1829" box="[294,639,941,963]" class="Insecta" family="Ichneumonidae" genus="Porizon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="boops">
