<subSubSection pageId="25" pageNumber="226" type="etymology">
<paragraph blockId="25.[151,1436,151,501]" pageId="25" pageNumber="226">
<emphasis bold="true" box="[199,326,439,464]" pageId="25" pageNumber="226">Etymology</emphasis>
