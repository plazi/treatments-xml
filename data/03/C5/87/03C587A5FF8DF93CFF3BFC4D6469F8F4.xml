<subSubSection pageId="73" pageNumber="74" type="distribution">
<paragraph blockId="73.[151,1437,1359,1853]" pageId="73" pageNumber="74">
<emphasis bold="true" box="[199,489,1684,1709]" pageId="73" pageNumber="74">Distribution and habitat</emphasis>
. 
<taxonomicName authority="Semper, 1865" authorityName="Semper" authorityYear="1865" box="[503,772,1684,1709]" class="Gastropoda" family="Mathildidae" genus="Mathilda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="73" pageNumber="74" phylum="Mollusca" rank="species" species="cochlaeformis">
