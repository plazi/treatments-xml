<number box="[1331,1377,188,212]" pageId="7" pageNumber="42" value="150.0">150</number>
. Pronotum, length 
<number box="[314,359,223,247]" pageId="7" pageNumber="42" value="125.0">125</number>
; posteroangular major seta 
<number box="[670,701,223,247]" pageId="7" pageNumber="42" value="15.0">15</number>
. Forewing length 
<number box="[905,951,223,247]" pageId="7" pageNumber="42" value="450.0">450</number>
. Antennal segments III–VIII length 
<number box="[1359,1431,223,247]" pageId="7" pageNumber="42" value="30.3">30, 30</number>
, 
<number box="[151,223,260,284]" pageId="7" pageNumber="42" value="28.38">28, 38</number>
, 
<number box="[235,294,260,284]" pageId="7" pageNumber="42" value="7.1">7, 10</number>
.
</paragraph>
