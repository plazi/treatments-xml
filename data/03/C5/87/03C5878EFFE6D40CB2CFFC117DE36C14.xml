<taxonomicName box="[290,428,1777,1800]" class="Clitellata" family="Lumbriculidae" genus="Rhynchelmis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lumbriculida" pageId="5" pageNumber="6" phylum="Annelida" rank="species" species="tetratheca">
