<taxonomicName authorityName="Fowler" authorityYear="1934" box="[473,654,1371,1395]" class="Actinopterygii" family="Congridae" genus="Bathyuroconger" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="15" pageNumber="162" phylum="Chordata" rank="genus">
