<subSubSection pageId="48" pageNumber="49" type="distribution">
<paragraph blockId="48.[151,1437,1007,1536]" pageId="48" pageNumber="49">
<emphasis bold="true" box="[199,497,1223,1248]" pageId="48" pageNumber="49">Distribution and habitat</emphasis>
. 
<taxonomicName box="[515,666,1223,1248]" class="Scaphopoda" family="Dentaliidae" genus="Antalis" kingdom="Animalia" order="Dentaliida" pageId="48" pageNumber="49" phylum="Mollusca" rank="species" species="agilis">
<emphasis box="[515,666,1223,1248]" italics="true" pageId="48" pageNumber="49">Antalis agilis</emphasis>
</taxonomicName>
