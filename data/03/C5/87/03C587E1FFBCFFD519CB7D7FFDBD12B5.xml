<subSubSection lastPageId="20" lastPageNumber="429" pageId="19" pageNumber="428" type="materials_examined">
<paragraph blockId="19.[151,1437,1684,2033]" box="[151,330,1684,1709]" pageId="19" pageNumber="428">
