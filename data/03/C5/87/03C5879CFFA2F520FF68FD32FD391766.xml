<subSubSection box="[192,520,852,879]" pageId="7" pageNumber="6" type="diagnosis">
<paragraph blockId="7.[192,520,852,879]" box="[192,520,852,879]" pageId="7" pageNumber="6">
<emphasis bold="true" box="[192,316,852,878]" pageId="7" pageNumber="6">Diagnosis.</emphasis>
