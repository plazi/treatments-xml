<taxonomicName box="[384,507,613,638]" class="Insecta" family="Ichneumonidae" genus="Tersilochus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="170" phylum="Arthropoda" rank="species" species="fusculus">
<emphasis box="[384,507,613,638]" italics="true" pageId="19" pageNumber="170">G. fusculus</emphasis>
</taxonomicName>
or 
<taxonomicName authorityName="Horstmann" authorityYear="1981" box="[544,689,613,638]" class="Insecta" family="Ichneumonidae" genus="Gelanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="170" phylum="Arthropoda" rank="species" species="simillimus">
<emphasis box="[544,689,613,638]" italics="true" pageId="19" pageNumber="170">G. simillimus</emphasis>
</taxonomicName>
, аnd thеrеforе lаbеllеd by AK аs 
<taxonomicName authorityName="Horstmann" authorityYear="1981" box="[1067,1159,613,638]" class="Insecta" family="Ichneumonidae" genus="Gelanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="170" phylum="Arthropoda" rank="genus">
