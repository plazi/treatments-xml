<accessionNumber box="[329,475,1290,1316]" httpUri="http://www.uniprot.org/uniprot/ZMB21348" pageId="6" pageNumber="35">
<collectionCode box="[329,393,1290,1316]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="6" pageNumber="35">ZMB</collectionCode>
<number box="[401,475,1290,1316]" pageId="6" pageNumber="35" value="21348.0">21348</number>
</accessionNumber>
(B- 
<number box="[522,601,1290,1316]" pageId="6" pageNumber="35" value="10012.0">10012</number>
), unsexed, collected by Jelski on &quot; 
<number box="[1012,1104,1290,1316]" pageId="6" pageNumber="35" value="1871.7">1871. 7</number>
Czerwca&quot; (field-label) [= 
<date pageId="6" pageNumber="35" value="1871-06-07">
<number box="[1421,1436,1290,1316]" pageId="6" pageNumber="35" value="7.0">7</number>
June 
<number box="[212,273,1329,1355]" pageId="6" pageNumber="35" value="1871.0">1871</number>
</date>
] at &quot;Maraynioc&quot; (field-label) [= Maraynioc, 
<collectingCountry box="[796,856,1329,1355]" name="Peru" pageId="6" pageNumber="35">Peru</collectingCountry>
].
</paragraph>
