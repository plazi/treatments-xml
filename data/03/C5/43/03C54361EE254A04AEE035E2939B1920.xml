species:
</emphasis>
<taxonomicName authority="Zheng, 1992" authorityName="Zheng" authorityYear="1992" box="[316,706,677,703]" class="Insecta" family="Tetrigidae" genus="Gibbotettix" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="417" phylum="Arthropoda" rank="species" species="emeiensis">
