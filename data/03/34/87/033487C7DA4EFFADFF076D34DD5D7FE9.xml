<date box="[423,552,552,578]" pageId="2" pageNumber="49" value="1997-08-06">
<number box="[423,438,552,578]" pageId="2" pageNumber="49" value="6.0">6</number>
.viii. 
<number box="[489,552,552,578]" pageId="2" pageNumber="49" value="1997.0">1997</number>
</date>
, collection number 
<number box="[792,853,552,578]" pageId="2" pageNumber="49" value="9397.0">9397</number>
, coll. Ming Wu; Three females and three males, 
<collectingCountry box="[151,225,592,618]" name="China" pageId="2" pageNumber="49">China</collectingCountry>
: Tibet, Chayu County, Jigong town, 
<date box="[653,789,592,618]" pageId="2" pageNumber="49" value="1997-07-18">
<number box="[653,682,592,618]" pageId="2" pageNumber="49" value="18.0">18</number>
.vii. 
<number box="[725,789,592,618]" pageId="2" pageNumber="49" value="1997.0">1997</number>
</date>
, collection number 
<number box="[1021,1082,592,618]" pageId="2" pageNumber="49" value="9395.0">9395</number>
