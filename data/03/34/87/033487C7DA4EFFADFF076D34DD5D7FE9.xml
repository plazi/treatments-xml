<subSubSection pageId="2" pageNumber="49" type="materials_examined">
<paragraph blockId="2.[151,1437,511,1220]" pageId="2" pageNumber="49">
<emphasis bold="true" box="[151,396,511,537]" pageId="2" pageNumber="49">Material examined.</emphasis>
