<taxonomicName authorityName="Ota" authorityYear="2012" box="[738,874,2012,2038]" class="Malacostraca" family="Gnathiidae" genus="Gnathia" kingdom="Animalia" order="Isopoda" pageId="30" pageNumber="61" phylum="Arthropoda" rank="species" species="excavata">
<emphasis box="[738,874,2012,2038]" italics="true" pageId="30" pageNumber="61">G. excavata</emphasis>
</taxonomicName>
; slightly bifid in 
<taxonomicName authorityName="Svavarsson &amp; Bruce" authorityYear="2019" box="[1071,1306,2011,2037]" class="Malacostraca" family="Gnathiidae" genus="Gnathia" kingdom="Animalia" order="Isopoda" pageId="30" pageNumber="61" phylum="Arthropoda" rank="species" species="glaucostega">
<emphasis box="[1071,1306,2011,2037]" italics="true" pageId="30" pageNumber="61">Gnathia glaucostega</emphasis>
</taxonomicName>
), in having
