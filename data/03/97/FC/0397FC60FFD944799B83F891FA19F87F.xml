<taxonomicName authorityName="Ota" authorityYear="2012" box="[1258,1391,1976,2002]" class="Malacostraca" family="Gnathiidae" genus="Gnathia" kingdom="Animalia" order="Isopoda" pageId="30" pageNumber="61" phylum="Arthropoda" rank="species" species="excavata">
