<number box="[451,502,226,252]" pageId="32" pageNumber="33" value="0.4">0.40</number>
long, 
<number box="[575,626,226,252]" pageId="32" pageNumber="33" value="0.36">0.36</number>
wide. Leg measurements: I 
<number box="[954,1006,226,252]" pageId="32" pageNumber="33" value="4.8">4.80</number>
(
<number box="[1021,1076,226,252]" pageId="32" pageNumber="33" value="1.5">1.50</number>
