); II 
<number box="[1385,1436,226,252]" pageId="32" pageNumber="33" value="3.9">3.90</number>
(
<number box="[160,215,264,290]" pageId="32" pageNumber="33" value="1.25">1.25</number>
, 
<number box="[226,280,264,290]" pageId="32" pageNumber="33" value="0.25">0.25</number>
, 
<number box="[292,346,264,290]" pageId="32" pageNumber="33" value="1.2">1.20</number>
, 
<number box="[358,412,264,290]" pageId="32" pageNumber="33" value="0.7">0.70</number>
, 
<number box="[424,480,264,290]" pageId="32" pageNumber="33" value="0.5">0.50</number>
); III 
<number box="[537,588,264,290]" pageId="32" pageNumber="33" value="2.67">2.67</number>
(
<number box="[604,659,264,290]" pageId="32" pageNumber="33" value="0.85">0.85</number>
, 
<number box="[671,725,264,290]" pageId="32" pageNumber="33" value="0.2">0.20</number>
, 
<number box="[737,791,264,290]" pageId="32" pageNumber="33" value="0.75">0.75</number>
, 
<number box="[803,857,264,290]" pageId="32" pageNumber="33" value="0.5">0.50</number>
, 
<number box="[868,924,264,290]" pageId="32" pageNumber="33" value="0.37">0.37</number>
