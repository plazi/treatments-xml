<quantity box="[685,834,1656,1677]" metricMagnitude="3" metricUnit="m" metricValue="1.565" metricValueMax="2.13" metricValueMin="1.0" pageId="43" pageNumber="44" unit="m" value="1565.0" valueMax="2130.0" valueMin="1000.0">1,000–2,130 m</quantity>
