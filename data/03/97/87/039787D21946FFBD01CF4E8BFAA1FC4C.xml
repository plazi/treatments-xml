— 
<number box="[335,496,332,357]" pageId="14" pageNumber="415" value="122.675" valueMax="135.35" valueMin="110.0">110–135, 350</number>
– 
<number box="[511,738,332,357]" pageId="14" pageNumber="415" value="485.555" valueMax="560.64" valueMin="410.47">410, 470–560, 640</number>
– 
<number box="[754,858,332,357]" pageId="14" pageNumber="415" value="775.535">775, 535</number>
– 
