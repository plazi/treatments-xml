<collectingCountry box="[761,846,904,930]" name="Bolivia" pageId="72" pageNumber="73">Bolivia</collectingCountry>
, 
<collectingCountry box="[858,929,904,930]" name="Brazil" pageId="72" pageNumber="73">Brazil</collectingCountry>
, 
<collectingCountry box="[941,1054,904,930]" name="Colombia" pageId="72" pageNumber="73">Colombia</collectingCountry>
, 
<collectingCountry box="[1065,1190,904,930]" name="Costa Rica" pageId="72" pageNumber="73">Costa Rica</collectingCountry>
, 
<collectingCountry box="[1202,1297,904,930]" name="Ecuador" pageId="72" pageNumber="73">Ecuador</collectingCountry>
, 
<collectingCountry box="[1308,1432,904,930]" name="Guatemala" pageId="72" pageNumber="73">Guatemala</collectingCountry>
, 
<collectingCountry box="[151,241,940,966]" name="Guyana" pageId="72" pageNumber="73">Guyana</collectingCountry>
, 
<collectingCountry box="[251,363,940,966]" name="Honduras" pageId="72" pageNumber="73">Honduras</collectingCountry>
, 
<collectingCountry box="[373,461,940,966]" name="Mexico" pageId="72" pageNumber="73">Mexico</collectingCountry>
, 
<collectingCountry box="[472,564,940,966]" name="Panama" pageId="72" pageNumber="73">Panama</collectingCountry>
, and 
<collectingCountry box="[620,671,940,966]" name="Peru" pageId="72" pageNumber="73">Peru</collectingCountry>
(
<bibRefCitation author="Metcalf, Z. P." box="[685,852,939,966]" pageId="72" pageNumber="73" pagination="1 - 492" refId="ref88883" refString="Metcalf, Z. P. (1963 c) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i-vi, 1 - 492." type="journal article" year="1963">Metcalf 1963c</bibRefCitation>
; 
<bibRefCitation author="Sanborn, A. F." box="[862,1020,940,966]" pageId="72" pageNumber="73" refId="ref90614" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp. https: // doi. org / 10.1016 / B 978 - 0 - 12 - 416647 - 9.00001 - 2" type="book" year="2013">Sanborn 2013</bibRefCitation>
; 
<bibRefCitation author="Sanborn, A. F." box="[1031,1103,940,966]" pageId="72" pageNumber="73" pagination="246 - 252" refId="ref90676" refString="Sanborn, A. F. (2014 a) Checklist of the cicadas (Insecta: Hemiptera: Cicadidae) of Costa Rica including new records for fourteen species. Check List, 10, 246 - 252. https: // doi. org / 10.15560 / 10.2.24 6" type="journal article" year="2014">2014a</bibRefCitation>
; 
<bibRefCitation author="Sanborn, A. F." box="[1113,1186,940,966]" pageId="72" pageNumber="73" pagination="1 - 69" refId="ref91261" refString="Sanborn, A. F. (2018 c) The cicadas (Hemiptera: Cicadidae) of Panama including the description of six new species, three new combinations, one new synonymy, and nine new records. Zootaxa, 4493 (1), 1 - 69. https: // doi. org / 10.11646 / zootaxa. 4493.1. 1" type="journal article" year="2018">2018c</bibRefCitation>
). The species has been recorded from Mapiri and Yungas (
<bibRefCitation author="Jacobi, A." box="[544,684,976,1002]" pageId="72" pageNumber="73" pagination="1 - 28" refId="ref87025" refString="Jacobi, A. (1907) Homoptera Andina. Die Zikaden des kordillerengebietes von Sudamerika nach systematik und Verbreitung. I. Cicadidae. Abhandlungen und Berichte des Koniglichen Zoologischen und Anthropologisch-Enthnographischen Museums zu Dresden, 11, 1 - 28." type="journal article" year="1907">Jacobi 1907</bibRefCitation>
).
</paragraph>
