<subSubSection box="[199,1063,691,717]" pageId="44" pageNumber="45" type="etymology">
<paragraph blockId="44.[151,1437,618,862]" box="[199,1063,691,717]" pageId="44" pageNumber="45">
<emphasis bold="true" box="[199,334,691,717]" pageId="44" pageNumber="45">Etymology.</emphasis>
