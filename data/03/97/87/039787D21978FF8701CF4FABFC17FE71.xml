<taxonomicName box="[1257,1428,224,248]" class="Bivalvia" family="Unionidae" genus="Sinanodonta" kingdom="Animalia" order="Unionoida" pageId="52" pageNumber="453" phylum="Mollusca" rank="species" species="primorjensis">
<emphasis box="[1257,1428,224,248]" italics="true" pageId="52" pageNumber="453">S. primorjensis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[151,254,261,284]" class="Bivalvia" family="Unionidae" genus="Cristaria" kingdom="Animalia" order="Unionoida" pageId="52" pageNumber="453" phylum="Mollusca" rank="genus">
<emphasis box="[151,254,261,284]" italics="true" pageId="52" pageNumber="453">Cristaria</emphasis>
</taxonomicName>
