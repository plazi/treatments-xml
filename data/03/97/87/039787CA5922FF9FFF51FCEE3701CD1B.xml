<taxonomicNameLabel box="[1015,1079,1665,1691]" pageId="79" pageNumber="80" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authorityName="Sanborn" authorityYear="2019" baseAuthorityName="Boulard" baseAuthorityYear="1986" box="[1094,1258,1665,1691]" class="Insecta" family="Cicadidae" genus="Herrera" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="criqualicae" status="n. comb.">
