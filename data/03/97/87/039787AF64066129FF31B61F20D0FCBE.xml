<taxonomicName class="Adenophorea" family="Mononchidae" genus="Paramononchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="4" pageNumber="55" phylum="Nematoda" rank="species" species="arcticus">
