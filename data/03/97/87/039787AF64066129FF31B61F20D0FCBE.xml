<quantity box="[521,686,727,752]" metricMagnitude="-3" metricUnit="m" metricValue="3.4299999999999997" metricValueMax="3.78" metricValueMin="3.08" pageId="4" pageNumber="55" unit="mm" value="3.4299999999999997" valueMax="3.78" valueMin="3.08">3.08–3.78 mm</quantity>
<emphasis box="[694,717,729,752]" italics="true" pageId="4" pageNumber="55">vs</emphasis>
L = 
<number box="[777,811,727,751]" pageId="4" pageNumber="55" value="3.5">3.5</number>
–4.0 mm in 
<taxonomicName box="[952,1070,729,752]" class="Adenophorea" family="Mononchidae" genus="Paramononchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="4" pageNumber="55" phylum="Nematoda" rank="species" species="arcticus">
<emphasis box="[952,1070,729,752]" italics="true" pageId="4" pageNumber="55">P. arcticus</emphasis>
</taxonomicName>
), longer buccal cavity (
<number box="[1342,1413,727,751]" pageId="4" pageNumber="55" value="66.0" valueMax="68.0" valueMin="64.0">
64– 
<date box="[1385,1413,727,751]" pageId="4" pageNumber="55" value="1968-10-30">68</date>
</number>
x 
<number box="[151,221,764,788]" pageId="4" pageNumber="55" value="31.0" valueMax="32.0" valueMin="30.0">30–32</number>
µm 
<emphasis box="[276,299,765,788]" italics="true" pageId="4" pageNumber="55">vs</emphasis>
<number box="[307,377,764,788]" pageId="4" pageNumber="55" value="46.0" valueMax="48.0" valueMin="44.0">
44– 
<date box="[350,377,764,788]" pageId="4" pageNumber="55" value="1948-10-19" valueMax="1948-10-21" valueMin="1948-10-19">48</date>
</number>
x 
<number box="[408,478,764,788]" pageId="4" pageNumber="55" value="20.0" valueMax="21.0" valueMin="19.0">19–21</number>
µm in 
<emphasis box="[563,687,764,788]" italics="true" pageId="4" pageNumber="55">P. a rc t i c u s)</emphasis>
, larger eggs (
<number box="[846,945,764,788]" pageId="4" pageNumber="55" value="147.5" valueMax="155.0" valueMin="140.0">140–155</number>
<date box="[954,1004,764,788]" pageId="4" pageNumber="55" value="1980-10">
x 
<number box="[976,1004,764,788]" pageId="4" pageNumber="55" value="83.5" valueMax="87.0" valueMin="80.0">80</number>
</date>
–87 µm 
<emphasis box="[1101,1124,765,788]" italics="true" pageId="4" pageNumber="55">vs</emphasis>
<number box="[1133,1229,764,788]" pageId="4" pageNumber="55" value="107.5" valueMax="110.0" valueMin="105.0">105–110</number>
<date box="[1238,1288,764,788]" pageId="4" pageNumber="55" value="1960-10">
x 
<number box="[1260,1288,764,788]" pageId="4" pageNumber="55" value="70.0" valueMax="80.0" valueMin="60.0">60</number>
