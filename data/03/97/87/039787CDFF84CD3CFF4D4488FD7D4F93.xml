<quantity box="[895,1038,972,998]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" metricValueMax="1.2" metricValueMin="1.1" pageId="11" pageNumber="12" unit="mm" value="1.15" valueMax="1.2" valueMin="1.1">1.1–1.2 mm</quantity>
