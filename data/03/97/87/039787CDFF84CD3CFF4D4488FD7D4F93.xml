<quantity box="[1113,1256,972,998]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.2" metricValueMin="1.9" pageId="11" pageNumber="12" unit="mm" value="2.05" valueMax="2.2" valueMin="1.9">1.9–2.2 mm</quantity>
