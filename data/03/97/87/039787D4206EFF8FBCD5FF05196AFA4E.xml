<paragraph blockId="14.[151,1436,258,1506]" pageId="14" pageNumber="15">
<emphasis bold="true" box="[201,513,1086,1111]" pageId="14" pageNumber="15">
<emphasis box="[201,386,1086,1111]" inLineHeading="true" pageId="14" pageNumber="15" reason="1">Type material.</emphasis>
<typeStatus box="[404,513,1087,1111]" pageId="14" pageNumber="15">Holotype</typeStatus>
</emphasis>
, female (slide number NJAUAcariEri421, marked 
<typeStatus box="[1154,1263,1086,1110]" pageId="14" pageNumber="15">Holotype</typeStatus>
), from 
<taxonomicName authority="Sarg." authorityName="Sarg." class="Magnoliopsida" family="Juglandaceae" genus="Carya" kingdom="Plantae" order="Fagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="cathayensis">
<emphasis italics="true" pageId="14" pageNumber="15">Carya cathayensis</emphasis>
Sarg.
</taxonomicName>
(
<taxonomicName box="[375,530,1122,1146]" class="Magnoliopsida" family="Juglandaceae" kingdom="Plantae" order="Fagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="family">Juglandaceae</taxonomicName>
), Cuihua Mountain, Xi' an City, 
<collectingRegion box="[923,1130,1122,1146]" country="China" name="Shaanxi" pageId="14" pageNumber="15">Shaanxi Province</collectingRegion>
, P. 
<collectionCode box="[1169,1190,1123,1146]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="14" pageNumber="15">R</collectionCode>
. 
<collectingCountry box="[1207,1276,1122,1146]" name="China" pageId="14" pageNumber="15">China</collectingCountry>
