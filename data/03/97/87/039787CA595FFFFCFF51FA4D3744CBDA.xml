, 
<collectingCountry box="[1347,1432,1823,1849]" name="Bolivia" pageId="51" pageNumber="52">Bolivia</collectingCountry>
, 
<collectingCountry box="[151,222,1859,1885]" name="Brazil" pageId="51" pageNumber="52">Brazil</collectingCountry>
, 
<collectingCountry box="[234,347,1859,1885]" name="Colombia" pageId="51" pageNumber="52">Colombia</collectingCountry>
, 
<collectingCountry box="[360,455,1859,1885]" name="Ecuador" pageId="51" pageNumber="52">Ecuador</collectingCountry>
, 
<collectingCountry box="[467,636,1859,1885]" name="French Guiana" pageId="51" pageNumber="52">French Guiana</collectingCountry>
, 
<collectingCountry box="[648,740,1860,1886]" name="Panama" pageId="51" pageNumber="52">Panama</collectingCountry>
, 
<collectingCountry box="[751,805,1860,1886]" name="Peru" pageId="51" pageNumber="52">Peru</collectingCountry>
, 
<collectingCountry box="[817,927,1859,1885]" name="Suriname" pageId="51" pageNumber="52">Suriname</collectingCountry>
, 
<collectingCountry box="[939,1157,1859,1886]" name="Trinidad and Tobago" pageId="51" pageNumber="52">Trinidad &amp; Tobago</collectingCountry>
