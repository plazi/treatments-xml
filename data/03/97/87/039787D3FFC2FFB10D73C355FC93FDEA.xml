<number box="[701,820,1584,1610]" pageId="3" pageNumber="58" value="0.52" valueMax="0.54" valueMin="0.5">0.50–0.54</number>
, 
<number box="[833,884,1584,1610]" pageId="3" pageNumber="58" value="0.52">0.52</number>
