<collectingCountry box="[151,222,189,215]" name="Brazil" pageId="45" pageNumber="46">Brazil</collectingCountry>
, 
<collectingCountry box="[235,348,189,215]" name="Colombia" pageId="45" pageNumber="46">Colombia</collectingCountry>
