<number box="[816,880,523,544]" pageId="17" pageNumber="56" value="22.0" valueMax="24.0" valueMin="20.0">20–24</number>
; supratemporal canal typically with 
<number box="[1234,1246,523,544]" pageId="17" pageNumber="56" value="2.0">2</number>
