<keyLead pageId="17" pageNumber="56">
<number box="[151,163,523,544]" pageId="17" pageNumber="56" value="3.0">3</number>
a. Dorsal-fin spines 
<number box="[398,446,523,544]" pageId="17" pageNumber="56" value="8.0" valueMax="10.0" valueMin="6.0">6–10</number>
(rarely 
<number box="[524,538,523,544]" pageId="17" pageNumber="56" value="6.0">6</number>
