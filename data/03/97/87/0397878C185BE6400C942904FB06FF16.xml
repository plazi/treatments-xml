<emphasis bold="true" box="[151,986,1606,1632]" pageId="12" pageNumber="51">
<taxonomicName authority="Smith-Vaniz" authorityName="Smith-Vaniz" box="[151,751,1606,1632]" class="Actinopterygii" family="Blenniidae" genus="Meiacanthus" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="51" phylum="Chordata" rank="species" species="abditus" subGenus="Meiacanthus">
<emphasis bold="true" box="[151,309,1606,1632]" italics="true" pageId="12" pageNumber="51">Meiacanthus</emphasis>
(
<emphasis bold="true" box="[326,484,1606,1632]" italics="true" pageId="12" pageNumber="51">Meiacanthus</emphasis>
) 
<emphasis bold="true" box="[501,589,1606,1632]" italics="true" pageId="12" pageNumber="51">abditus</emphasis>
Smith-Vaniz
</taxonomicName>
and 
<taxonomicName box="[813,979,1606,1632]" class="Actinopterygii" family="Blenniidae" genus="Meiacanthus" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="51" phylum="Chordata" rank="species" species="abditus">
<emphasis bold="true" box="[813,846,1606,1632]" italics="true" pageId="12" pageNumber="51">M.</emphasis>
