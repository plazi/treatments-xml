<taxonomicName authorityName="Assing" authorityYear="1999" box="[957,1084,841,862]" class="Insecta" family="Staphylinidae" genus="Oligota" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="620" phylum="Arthropoda" rank="species" species="stefaniae">
<emphasis box="[957,1084,841,862]" italics="true" pageId="33" pageNumber="620">O. stefaniae</emphasis>
</taxonomicName>
ASSING und 
<taxonomicName box="[305,384,870,891]" class="Insecta" family="Staphylinidae" genus="Oligota" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="620" phylum="Arthropoda" rank="species" species="hirta">
