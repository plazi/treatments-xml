<paragraph blockId="16.[151,1437,1839,2009]" pageId="16" pageNumber="531">
<emphasis bold="true" box="[151,267,1948,1973]" pageId="16" pageNumber="531">Remarks.</emphasis>
<taxonomicName box="[273,521,1948,1973]" class="Actinopterygii" family="Siluridae" genus="Clarias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Siluriformes" pageId="16" pageNumber="531" phylum="Chordata" rank="species" species="batrachus">
