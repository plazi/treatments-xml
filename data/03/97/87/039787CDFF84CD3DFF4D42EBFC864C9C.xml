; HW: 
<quantity box="[1158,1304,1201,1227]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" metricValueMax="1.8" metricValueMin="1.6" pageId="10" pageNumber="11" unit="mm" value="1.7" valueMax="1.8" valueMin="1.6">1.6–1.8 mm</quantity>
