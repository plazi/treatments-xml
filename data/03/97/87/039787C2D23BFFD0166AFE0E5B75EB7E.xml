<emphasis box="[879,891,1556,1579]" italics="true" pageId="6" pageNumber="7">c</emphasis>
, cercus; 
<emphasis box="[991,1004,1556,1579]" italics="true" pageId="6" pageNumber="7">d</emphasis>
, distiphallus; 
<emphasis box="[1159,1186,1556,1579]" italics="true" pageId="6" pageNumber="7">da</emphasis>
, dorsal apodeme of parameral sheath; 
<emphasis box="[564,576,1591,1614]" italics="true" pageId="6" pageNumber="7">e</emphasis>
, epandrium; 
<emphasis box="[722,747,1591,1614]" italics="true" pageId="6" pageNumber="7">ea</emphasis>
, ejaculatory apodeme; 
<emphasis box="[1004,1017,1591,1614]" italics="true" pageId="6" pageNumber="7">g</emphasis>
