<collectingCountry box="[1221,1335,1181,1207]" name="Argentina" pageId="52" pageNumber="53">Argentina</collectingCountry>
, 
<collectingCountry box="[1347,1432,1181,1207]" name="Bolivia" pageId="52" pageNumber="53">Bolivia</collectingCountry>
, 
<collectingCountry box="[151,222,1217,1243]" name="Brazil" pageId="52" pageNumber="53">Brazil</collectingCountry>
