, 
<collectionCode box="[969,1080,1322,1348]" pageId="16" pageNumber="355">MSNUF</collectionCode>
); *Capo Caccia, 1983 (
<collectionCode box="[1377,1431,1322,1348]" pageId="16" pageNumber="355">CLI</collectionCode>
, 
<collectionCode box="[151,261,1361,1387]" pageId="16" pageNumber="355">MCSNG</collectionCode>
); 
<collectingCountry box="[280,337,1361,1387]" name="Iceland" pageId="16" pageNumber="355">Isola</collectingCountry>
Piana di Capo Caccia, 1987 (
<collectionCode box="[684,806,1361,1387]" pageId="16" pageNumber="355">CNBFVR</collectionCode>
, 
<collectionCode box="[818,928,1361,1387]" pageId="16" pageNumber="355">MCSNG</collectionCode>
