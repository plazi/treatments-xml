— 
<collectingCountry box="[375,445,1886,1912]" name="China" pageId="14" pageNumber="89">China</collectingCountry>
: 
<collectingRegion box="[455,649,1886,1913]" country="China" name="Yunnan" pageId="14" pageNumber="89">Yunnan Province</collectingRegion>
(Funing, Wenshan); northern 
<collectingCountry box="[990,1089,1886,1912]" name="Vietnam" pageId="14" pageNumber="89">Vietnam</collectingCountry>
