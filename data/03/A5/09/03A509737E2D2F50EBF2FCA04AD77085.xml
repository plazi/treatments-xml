<taxonomicName box="[309,449,1950,1974]" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="513" phylum="Arthropoda" rank="species" species="capeyork" status="sp. nov.">
<emphasis box="[309,449,1950,1974]" italics="true" pageId="24" pageNumber="513">M. capeyork</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[456,543,1951,1974]" pageId="24" pageNumber="513" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName box="[597,715,1950,1974]" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="513" phylum="Arthropoda" rank="species" species="hertogi" status="sp. nov.">
<emphasis box="[597,715,1950,1974]" italics="true" pageId="24" pageNumber="513">M. hertogi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[722,809,1951,1974]" pageId="24" pageNumber="513" rank="species">sp. nov.</taxonomicNameLabel>
</paragraph>
</subSubSection>
