<collectionCode box="[1300,1341,833,860]" country="Australia" name="The CB Rhizobium Collection" pageId="50" pageNumber="51">CB</collectionCode>
35a), 
<geoCoordinate box="[189,335,868,895]" degrees="25" direction="south" minutes="40" orientation="latitude" pageId="50" pageNumber="51" precision="15" seconds="52" value="-25.68111">25°40’52” S</geoCoordinate>
