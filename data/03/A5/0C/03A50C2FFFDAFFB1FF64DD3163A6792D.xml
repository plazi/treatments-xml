<number box="[1166,1212,869,894]" pageId="25" pageNumber="377" value="205.0">205</number>
]) for the 
<taxonomicName box="[1320,1433,869,894]" class="Reptilia" family="Viperidae" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="377" phylum="Chordata" rank="family">Viperidae</taxonomicName>
