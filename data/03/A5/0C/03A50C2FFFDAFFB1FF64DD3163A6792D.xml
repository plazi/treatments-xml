<paragraph blockId="25.[151,1437,834,930]" pageId="25" pageNumber="377">
(
<number box="[159,174,870,894]" pageId="25" pageNumber="377" value="7.0">7</number>
genera, 
<number box="[273,301,870,894]" pageId="25" pageNumber="377" value="13.0">13</number>
