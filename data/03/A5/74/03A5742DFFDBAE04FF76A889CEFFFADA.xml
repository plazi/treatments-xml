<subSubSection box="[151,633,546,571]" pageId="14" pageNumber="215" type="etymology">
<paragraph blockId="14.[151,1436,546,1291]" box="[151,633,546,571]" pageId="14" pageNumber="215">
<emphasis bold="true" box="[151,286,546,571]" pageId="14" pageNumber="215">Etymology.</emphasis>
<taxonomicName box="[292,393,546,571]" class="Insecta" family="Aphrophoridae" genus="Microsargane" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="martialis">
