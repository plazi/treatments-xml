<subSubSection box="[151,1005,1726,1751]" pageId="13" pageNumber="214" type="etymology">
<paragraph blockId="13.[151,1437,1726,2003]" box="[151,1005,1726,1751]" pageId="13" pageNumber="214">
<emphasis bold="true" box="[151,286,1726,1751]" pageId="13" pageNumber="214">Etymology.</emphasis>
<emphasis box="[292,365,1727,1750]" italics="true" pageId="13" pageNumber="214">micro-</emphasis>
small, 
<emphasis box="[448,527,1726,1751]" italics="true" pageId="13" pageNumber="214">rhaphe</emphasis>
