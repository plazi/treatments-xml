); Makokou, 
<date box="[406,521,768,794]" pageId="22" pageNumber="23" value="1963-01-16">
<number box="[406,437,768,794]" pageId="22" pageNumber="23" value="16.0">16</number>
.I.1963
</date>
, 1 ex (
<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[604,701,768,794]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="22" pageNumber="23">MNHN</collectionCode>
). 
<emphasis bold="true" box="[721,812,767,793]" pageId="22" pageNumber="23">
<collectingCountry box="[721,807,767,793]" name="Kenya" pageId="22" pageNumber="23">Kenya</collectingCountry>
:
</emphasis>
Nakuro, Rift Valley, 
<date box="[1070,1180,768,794]" pageId="22" pageNumber="23" value="1903-12">
XII. 
<number box="[1119,1180,768,794]" pageId="22" pageNumber="23" value="1903.0">1903</number>
</date>
, Ch. Alluaud, 
<number box="[1355,1384,768,794]" pageId="22" pageNumber="23" value="10.0">10</number>
exx (
<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[159,256,808,834]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="22" pageNumber="23">MNHN</collectionCode>
