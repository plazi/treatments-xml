<subSubSection pageId="47" pageNumber="48" type="etymology">
<paragraph blockId="47.[151,1436,152,578]" pageId="47" pageNumber="48">
<emphasis bold="true" box="[198,331,471,497]" pageId="47" pageNumber="48">Etymology</emphasis>
