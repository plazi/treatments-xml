<taxonomicName box="[1192,1351,472,497]" class="Insecta" family="Staphylinidae" genus="Ischnopoderona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="madecassa">
<emphasis box="[1192,1351,472,497]" italics="true" pageId="47" pageNumber="48">I. madecassa</emphasis>
</taxonomicName>
and 
<taxonomicName class="Insecta" family="Staphylinidae" genus="Ischnopoderona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="namibensis">
<emphasis italics="true" pageId="47" pageNumber="48">I. namibensis</emphasis>
</taxonomicName>
.
</paragraph>
</subSubSection>
