<quantity box="[853,1003,1018,1044]" metricMagnitude="2" metricUnit="m" metricValue="9.65" metricValueMax="10.0" metricValueMin="9.3" pageId="35" pageNumber="36" unit="m" value="965.0" valueMax="1000.0" valueMin="930.0">930–1000 m</quantity>
