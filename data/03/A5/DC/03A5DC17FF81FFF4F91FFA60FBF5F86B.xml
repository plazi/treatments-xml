<collectingCountry box="[1082,1421,1824,1848]" name="Democratic Republic of the Congo" pageId="1" pageNumber="198">Democratic Republic of Congo</collectingCountry>
and 
<collectingCountry box="[864,931,1856,1880]" name="Kenya" pageId="1" pageNumber="198">Kenya</collectingCountry>
(
<bibRefCitation author="ZIELKE, E." box="[947,1098,1856,1880]" pageId="1" pageNumber="198" pagination="1 - 199" refId="ref5276" refString="ZIELKE, E. 1971 a: Revision der Muscinae der athiopischen Region. - Series Entomologica 7: 1 - 199." type="journal article" year="1971">ZIELKE 1971a</bibRefCitation>
