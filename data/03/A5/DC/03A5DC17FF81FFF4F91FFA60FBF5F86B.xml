<collectingCountry box="[1284,1421,1856,1880]" name="South Africa" pageId="1" pageNumber="198">South Africa</collectingCountry>
and 
<collectingCountry box="[866,962,1888,1912]" name="Tanzania" pageId="1" pageNumber="198">Tanzania</collectingCountry>
(
<bibRefCitation author="PONT, A. C." box="[977,1105,1888,1912]" pageId="1" pageNumber="198" pagination="721 - 761" refId="ref5225" refString="PONT, A. C. 1980: Family Muscidae. - In: CROSSKEY, R. W. (ed.) 1980: Catalogue of the Diptera of the Afrotropical Region. - British Museum (Natural History), London, UK: 721 - 761." type="book chapter" year="1980">PONT 1980</bibRefCitation>
