<taxonomicName box="[312,450,223,248]" class="Insecta" family="Cerambycidae" genus="Pseudosparna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="389" phylum="Arthropoda" rank="species" species="pichincha">
<emphasis box="[312,450,223,248]" italics="true" pageId="4" pageNumber="389">P. pichincha</emphasis>
</taxonomicName>
, respectively from 
<collectingCountry box="[666,734,223,248]" name="Brazil" pageId="4" pageNumber="389">Brazil</collectingCountry>
