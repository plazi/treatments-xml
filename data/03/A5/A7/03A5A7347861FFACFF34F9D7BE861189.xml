<taxonomicNameLabel box="[546,606,1576,1599]" pageId="1" pageNumber="597" rank="species">
<emphasis bold="true" box="[546,606,1576,1599]" pageId="1" pageNumber="597">sp. n.</emphasis>
</taxonomicNameLabel>
(1–5) and 
<taxonomicName box="[713,834,1578,1599]" class="Arachnida" family="Lycosidae" genus="Acantholycosa" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="597" phylum="Arthropoda" rank="species" species="azarkinae">
<emphasis box="[713,834,1578,1599]" italics="true" pageId="1" pageNumber="597">A. azarkinae</emphasis>
