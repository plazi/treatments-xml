) forebody; (
<emphasis bold="true" box="[864,887,1438,1459]" pageId="19" pageNumber="504">55</emphasis>
) antenna; (
<emphasis bold="true" box="[1000,1023,1437,1458]" pageId="19" pageNumber="504">56</emphasis>
