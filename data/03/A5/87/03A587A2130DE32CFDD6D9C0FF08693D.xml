<taxonomicName baseAuthorityName="Terayama" baseAuthorityYear="1999" box="[957,1126,693,719]" class="Insecta" family="Bethylidae" genus="Propristocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="463" phylum="Arthropoda" rank="species" species="okinawensis">
