Rica
</collectingCountry>
, 
<collectingCountry box="[934,1046,1207,1232]" name="Honduras" pageId="54" pageNumber="55">Honduras</collectingCountry>
, 
<collectingRegion box="[1060,1148,1207,1232]" country="Mexico" name="Mexico" pageId="54" pageNumber="55">
<collectingCountry box="[1060,1148,1207,1232]" name="Mexico" pageId="54" pageNumber="55">Mexico</collectingCountry>
</collectingRegion>
, 
<collectingCountry box="[1163,1281,1207,1232]" name="Nicaragua" pageId="54" pageNumber="55">Nicaragua</collectingCountry>
, and 
<collectingRegion box="[1347,1436,1208,1231]" country="Panama" name="Panama" pageId="54" pageNumber="55">
<collectingCountry box="[1347,1436,1208,1231]" name="Panama" pageId="54" pageNumber="55">Panama</collectingCountry>
</collectingRegion>
(
<bibRefCitation box="[158,326,1244,1269]" pageId="54" pageNumber="55" refId="ref62351" refString="Metcalf, Z. P. (1963 c) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i - vi + 1 - 492." type="journal article">Metcalf 1963c</bibRefCitation>
