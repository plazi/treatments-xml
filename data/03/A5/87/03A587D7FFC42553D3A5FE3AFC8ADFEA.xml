und 
<taxonomicName authority="(SIEDER, 1953) (Klasse II-III)" authorityName="Klasse II-III" baseAuthorityName="SIEDER" baseAuthorityYear="1953" class="Insecta" family="Psychidae" genus="Dahlica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="1918" phylum="Arthropoda" rank="species" species="klimeschi">
