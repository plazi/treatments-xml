<emphasis box="[390,566,1523,1549]" italics="true" pageId="58" pageNumber="59">O. d. spodiurus</emphasis>
(Hershkovitz 1940)), 
<taxonomicName authority="(Waterhouse 1837)" baseAuthorityName="Waterhouse" baseAuthorityYear="1837" box="[820,1197,1523,1549]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="flavescens">
<emphasis box="[820,971,1523,1549]" italics="true" pageId="58" pageNumber="59">O. flavescens</emphasis>
(Waterhouse 1837)
</taxonomicName>
, 
<taxonomicName authority="(Massoia 1973)" baseAuthorityName="Massoia" baseAuthorityYear="1973" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="fornesi">
<emphasis box="[1211,1325,1523,1549]" italics="true" pageId="58" pageNumber="59">O. fornesi</emphasis>
(Massoia 1973)
</taxonomicName>
, 
<taxonomicName authority="(Saussure 1860)" baseAuthorityName="Saussure" baseAuthorityYear="1860" box="[233,580,1559,1586]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="fulvescens">
