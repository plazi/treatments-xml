, 
<taxonomicName authority="(Osgood 1912)" baseAuthorityName="Osgood" baseAuthorityYear="1912" box="[594,916,1559,1585]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="griseolus">
<emphasis box="[594,733,1559,1585]" italics="true" pageId="58" pageNumber="59">O. griseolus</emphasis>
(Osgood 1912)
</taxonomicName>
, 
<taxonomicName authority="(Bennett 1832)" baseAuthorityName="Bennett" baseAuthorityYear="1832" box="[930,1308,1559,1585]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="longicaudatus">
<emphasis box="[930,1125,1559,1585]" italics="true" pageId="58" pageNumber="59">O. longicaudatus</emphasis>
