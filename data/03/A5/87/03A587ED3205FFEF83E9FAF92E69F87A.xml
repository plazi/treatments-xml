<taxonomicName authority="(Osgood 1914)" baseAuthorityName="Osgood" baseAuthorityYear="1914" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="andinus">
<emphasis box="[1312,1437,1415,1441]" italics="true" pageId="58" pageNumber="59">O. andinus</emphasis>
(Osgood 1914)
</taxonomicName>
, 
<taxonomicName authority="(Thomas 1913)" baseAuthorityName="Thomas" baseAuthorityYear="1913" box="[335,642,1451,1477]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="arenalis">
<emphasis box="[335,461,1451,1477]" italics="true" pageId="58" pageNumber="59">O. arenalis</emphasis>
(Thomas 1913)
</taxonomicName>
, 
<taxonomicName authority="(Massoia 1998)" baseAuthorityName="Massoia" baseAuthorityYear="1998" box="[655,965,1451,1477]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="brendae">
<emphasis box="[655,780,1451,1477]" italics="true" pageId="58" pageNumber="59">O. brendae</emphasis>
(Massoia 1998)
</taxonomicName>
, 
<taxonomicName authority="(Myers &amp; Carleton 1981)" baseAuthorityName="Myers &amp; Carleton" baseAuthorityYear="1981" box="[977,1431,1451,1477]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="chacoensis">
<emphasis box="[977,1135,1451,1477]" italics="true" pageId="58" pageNumber="59">O. chacoensis</emphasis>
(
<bibRefCitation author="Myers, P. &amp; Carleton, M. D." box="[1150,1423,1451,1477]" pageId="58" pageNumber="59" pagination="1 - 41" refId="ref70017" refString="Myers, P. &amp; Carleton, M. D. (1981) The species of Oryzomys (Oligoryzomys) in Paraguay and the identity of Azara's &quot; Rat sixieme ou Rat a tarse noir &quot;. Miscellaneous publications / University of Michigan, Museum of Zoology, 161, 1 - 41." type="journal article" year="1981">Myers &amp; Carleton 1981</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Allen 1893)" baseAuthorityName="Allen" baseAuthorityYear="1893" box="[151,487,1487,1513]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="costaricensis">
<emphasis box="[151,332,1487,1513]" italics="true" pageId="58" pageNumber="59">O. costaricensis</emphasis>
(Allen 1893)
</taxonomicName>
, 
<taxonomicName authority="(Allen &amp; Chapman 1897)" baseAuthorityName="Allen &amp; Chapman" baseAuthorityYear="1897" box="[500,935,1487,1513]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="delicatus">
<emphasis box="[500,636,1487,1513]" italics="true" pageId="58" pageNumber="59">O. delicatus</emphasis>
(Allen &amp; Chapman 1897)
</taxonomicName>
, 
<taxonomicName authority="(Tschudi 1844)" baseAuthorityName="Tschudi" baseAuthorityYear="1844" box="[947,1276,1487,1513]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="destructor">
<emphasis box="[947,1097,1487,1513]" italics="true" pageId="58" pageNumber="59">O. destructor</emphasis>
(Tschudi 1844)
</taxonomicName>
(that includes 
<emphasis box="[151,332,1523,1549]" italics="true" pageId="58" pageNumber="59">
O. d. 
<taxonomicName baseAuthorityName="Tschudi" baseAuthorityYear="1844" box="[217,332,1523,1549]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="destructor">destructor</taxonomicName>
