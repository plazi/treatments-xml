<taxonomicName authority="Distant, 1881" authorityName="Distant" authorityYear="1881" box="[354,686,1948,1970]" class="Insecta" family="Pentatomidae" genus="Ascra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="457" phylum="Arthropoda" rank="species" species="championi">
