<subSubSection pageId="4" pageNumber="245" type="etymology">
<paragraph blockId="4.[151,1436,151,392]" box="[199,1183,332,357]" pageId="4" pageNumber="245">
<emphasis bold="true" box="[199,337,332,357]" pageId="4" pageNumber="245">Etymology:</emphasis>
