<collectingCountry box="[212,317,1402,1427]" name="Bahamas" pageId="35" pageNumber="36">Bahamas</collectingCountry>
, 
<collectingRegion box="[328,402,1402,1427]" country="Belize" name="Belize" pageId="35" pageNumber="36">
<collectingCountry box="[328,402,1402,1427]" name="Belize" pageId="35" pageNumber="36">Belize</collectingCountry>
