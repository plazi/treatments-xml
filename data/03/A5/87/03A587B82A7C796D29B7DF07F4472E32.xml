<taxonomicName authorityName="Distant" authorityYear="1905" box="[1217,1326,1294,1319]" class="Insecta" family="Cicadidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="tribe" tribe="Fidicinini">Fidicinini</taxonomicName>
