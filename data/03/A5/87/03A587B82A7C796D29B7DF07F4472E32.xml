, 
<collectingCountry box="[1339,1432,1402,1427]" name="Jamaica" pageId="35" pageNumber="36">Jamaica</collectingCountry>
, 
<collectingRegion box="[151,239,1438,1463]" country="Mexico" name="Mexico" pageId="35" pageNumber="36">
