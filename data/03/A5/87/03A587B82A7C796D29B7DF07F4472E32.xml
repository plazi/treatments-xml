<taxonomicName authorityName="Handlirsch" authorityYear="1925" box="[151,347,1330,1355]" class="Insecta" family="Cicadidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="tribe" tribe="Cryptotympanini">Cryptotympanini</taxonomicName>
