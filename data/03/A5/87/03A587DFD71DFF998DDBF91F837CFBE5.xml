<heading bold="true" box="[151,404,1686,1712]" fontSize="11" level="1" pageId="2" pageNumber="447" reason="1">
<emphasis bold="true" box="[151,404,1686,1712]" pageId="2" pageNumber="447">
Species group 
<taxonomicName authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" box="[331,404,1686,1712]" class="Insecta" family="Pentatomidae" genus="Pentatoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="447" phylum="Arthropoda" rank="species" species="bifida">bifida</taxonomicName>
</emphasis>
