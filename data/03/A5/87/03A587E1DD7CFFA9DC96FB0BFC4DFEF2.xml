<paragraph blockId="17.[177,1084,1162,1248]" box="[177,313,1162,1189]" pageId="17" pageNumber="502">
<specimenCount box="[177,198,1166,1189]" pageId="17" pageNumber="502" type="female">♀</specimenCount>
