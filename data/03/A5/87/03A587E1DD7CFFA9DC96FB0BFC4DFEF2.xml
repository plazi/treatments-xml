<paragraph blockId="17.[177,1084,1097,1148]" pageId="17" pageNumber="502">
Ƌ: tergite 
<collectionCode box="[283,329,1099,1120]" pageId="17" pageNumber="502">VIII</collectionCode>
, sternite 
<collectionCode box="[420,466,1099,1120]" pageId="17" pageNumber="502">VIII</collectionCode>
