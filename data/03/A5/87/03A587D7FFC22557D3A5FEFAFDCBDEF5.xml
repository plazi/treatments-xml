als 
<taxonomicName baseAuthorityName="BRUAND" baseAuthorityYear="1853" box="[884,1007,616,638]" class="Insecta" family="Psychidae" genus="Proutia" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="1923" phylum="Arthropoda" rank="species" species="comitella">
