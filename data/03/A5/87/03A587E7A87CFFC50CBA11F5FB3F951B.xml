<elevation box="[1024,1091,1341,1367]" metricMagnitude="2" metricUnit="m" metricValue="6.2" pageId="31" pageNumber="32" unit="m" value="620.0">620m</elevation>
</quantity>
, 
<date box="[1102,1260,1341,1367]" pageId="31" pageNumber="32" value="1987-06-01" valueMax="1987-06-14" valueMin="1987-06-01">
<collectingDate box="[1102,1260,1341,1367]" pageId="31" pageNumber="32" value="1987-06-01" valueMax="1987-06-14" valueMin="1987-06-01">1–14.VI.1987</collectingDate>
</date>
, 
<collectorName box="[1272,1384,1341,1367]" pageId="31" pageNumber="32">S&amp;J Peck</collectorName>
, 
