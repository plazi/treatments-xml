<emphasis box="[972,1121,1402,1425]" italics="true" pageId="1" pageNumber="88">Gepus invisus</emphasis>
</taxonomicName>
(
<specimenCount box="[1138,1258,1402,1425]" pageId="1" pageNumber="88" type="generic">
<number box="[1138,1150,1402,1425]" pageId="1" pageNumber="88" value="1.0">1</number>
specimen
</specimenCount>
) and 
<taxonomicName box="[1316,1435,1401,1425]" class="Insecta" family="Myrmeleontidae" genus="Solter" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="88" phylum="Arthropoda" rank="species" species="liber">
<emphasis box="[1316,1435,1401,1425]" italics="true" pageId="1" pageNumber="88">Solter liber</emphasis>
</taxonomicName>
(
<specimenCount box="[160,297,1434,1457]" pageId="1" pageNumber="88" type="generic">
<number box="[160,173,1434,1457]" pageId="1" pageNumber="88" value="3.0">3</number>
