<collectingCountry box="[762,847,1578,1603]" name="Bolivia" pageId="41" pageNumber="42">Bolivia</collectingCountry>
, 
<collectingCountry box="[859,930,1578,1603]" name="Brazil" pageId="41" pageNumber="42">Brazil</collectingCountry>
, 
<collectingCountry box="[941,1054,1578,1603]" name="Colombia" pageId="41" pageNumber="42">Colombia</collectingCountry>
, 
<collectingCountry box="[1065,1190,1578,1603]" name="Costa Rica" pageId="41" pageNumber="42">
<collectingRegion box="[1065,1129,1579,1603]" country="New Zealand" name="West Coast" pageId="41" pageNumber="42">Costa</collectingRegion>
Rica
</collectingCountry>
, 
<collectingCountry box="[1201,1296,1578,1603]" name="Ecuador" pageId="41" pageNumber="42">Ecuador</collectingCountry>
, 
<collectingRegion box="[1307,1432,1578,1603]" country="Guatemala" name="Guatemala" pageId="41" pageNumber="42">
<collectingCountry box="[1307,1432,1578,1603]" name="Guatemala" pageId="41" pageNumber="42">Guatemala</collectingCountry>
</collectingRegion>
, 
<collectingCountry box="[151,241,1615,1639]" name="Guyana" pageId="41" pageNumber="42">Guyana</collectingCountry>
, 
<collectingCountry box="[258,370,1614,1639]" name="Honduras" pageId="41" pageNumber="42">Honduras</collectingCountry>
, 
<collectingRegion box="[386,474,1614,1639]" country="Mexico" name="Mexico" pageId="41" pageNumber="42">
<collectingCountry box="[386,474,1614,1639]" name="Mexico" pageId="41" pageNumber="42">Mexico</collectingCountry>
</collectingRegion>
, 
<collectingRegion box="[492,584,1615,1638]" country="Panama" name="Panama" pageId="41" pageNumber="42">
<collectingCountry box="[492,584,1615,1638]" name="Panama" pageId="41" pageNumber="42">Panama</collectingCountry>
</collectingRegion>
, and 
<collectingCountry box="[653,704,1615,1638]" name="Peru" pageId="41" pageNumber="42">Peru</collectingCountry>
(
<bibRefCitation box="[725,899,1614,1639]" pageId="41" pageNumber="42" refId="ref62351" refString="Metcalf, Z. P. (1963 c) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i - vi + 1 - 492." type="journal article">Metcalf 1963c</bibRefCitation>
; 
<bibRefCitation box="[915,1081,1614,1639]" pageId="41" pageNumber="42" refId="ref63516" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book">Sanborn 2013</bibRefCitation>
