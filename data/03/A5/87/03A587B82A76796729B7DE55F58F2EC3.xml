<collectingRegion box="[449,542,1650,1675]" country="Panama" name="Chiriqui" pageId="41" pageNumber="42">Chiriqui</collectingRegion>
in 
<collectingRegion box="[588,677,1651,1674]" country="Panama" name="Panama" pageId="41" pageNumber="42">
<collectingCountry box="[588,677,1651,1674]" name="Panama" pageId="41" pageNumber="42">Panama</collectingCountry>
</collectingRegion>
(
<bibRefCitation box="[696,850,1650,1675]" pageId="41" pageNumber="42" refId="ref59993" refString="Distant, W. L. (1883) Rhynchota: Homoptera. Biologia Centrali-Americana; contributions to the knowledge of the fauna and flora of Mexico and Central America, Part 15 (1), 17 - 24." type="journal article">Distant 1883</bibRefCitation>
