<emphasis box="[1120,1128,379,392]" italics="true" pageId="1" pageNumber="24">2</emphasis>
</number>
and 
<emphasis box="[1185,1199,364,388]" italics="true" pageId="1" pageNumber="24">h</emphasis>
<number box="[1198,1206,379,392]" pageId="1" pageNumber="24" value="2.0">
<emphasis box="[1198,1206,379,392]" italics="true" pageId="1" pageNumber="24">2</emphasis>
</number>
; seta 
<emphasis box="[1274,1314,364,388]" italics="true" pageId="1" pageNumber="24">pdx</emphasis>
