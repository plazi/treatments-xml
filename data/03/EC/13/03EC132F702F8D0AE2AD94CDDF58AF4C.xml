<number box="[1059,1073,474,498]" pageId="1" pageNumber="24" value="2.0">2</number>
φ) - 
<number box="[1131,1145,474,498]" pageId="1" pageNumber="24" value="8.0">8</number>
or 
<number box="[1184,1201,474,498]" pageId="1" pageNumber="24" value="9.0">9</number>
(
<number box="[1211,1222,474,498]" pageId="1" pageNumber="24" value="1.0">1</number>
φ) setae; tibia I–IV with 
<number box="[209,226,511,535]" pageId="1" pageNumber="24" value="9.0">9</number>
(
<number box="[235,246,511,535]" pageId="1" pageNumber="24" value="1.0">1</number>
