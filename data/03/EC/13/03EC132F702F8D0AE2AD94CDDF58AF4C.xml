</date>
(
<number box="[716,727,511,535]" pageId="1" pageNumber="24" value="1.0">1</number>
ω) or 
<number box="[796,828,511,535]" pageId="1" pageNumber="24" value="10.0">10</number>
(
<number box="[836,846,511,535]" pageId="1" pageNumber="24" value="1.0">1</number>
ω) - 
<number box="[901,933,511,535]" pageId="1" pageNumber="24" value="10.0">10</number>
(
