<taxonomicName authorityName="Binaghi" authorityYear="1948" box="[604,753,222,246]" class="Insecta" family="Staphylinidae" genus="Pseudoeudesis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="218" phylum="Arthropoda" rank="genus">
