<subSubSection pageId="13" pageNumber="218" type="reference_group">
<paragraph blockId="13.[151,1437,221,307]" pageId="13" pageNumber="218">
<treatmentCitationGroup infered="true" pageId="13" pageNumber="218">
<taxonomicName authority="Franz, 1963: 16" authorityName="Franz" authorityPageNumber="16" authorityYear="1963" box="[151,434,221,246]" class="Insecta" family="Staphylinidae" genus="Afroeudesis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="218" phylum="Arthropoda" rank="genus">
<emphasis box="[151,270,222,246]" italics="true" pageId="13" pageNumber="218">Afroeudesis</emphasis>
<treatmentCitation author="Franz, H." box="[276,434,221,246]" page="16" pageId="13" pageNumber="218" year="1963">
<bibRefCitation author="Franz, H." box="[276,434,221,246]" pageId="13" pageNumber="218" pagination="1 - 93" refId="ref7193" refString="Franz, H. (1963) Scydmaeniden (Coleoptera) aus Ostafrika, dem Kongo und der Tschadrepublik (vorwiegend aus dem im Sommer 1962 vom Verfasser selbst gesammelten Ausbeuten). Stuttgarter Beitrage zur Naturkunde, 120, 1 - 93." type="journal article" year="1963">Franz, 1963: 16</bibRefCitation>
</treatmentCitation>
</taxonomicName>
