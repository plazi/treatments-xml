<taxonomicName authorityName="Franz" authorityYear="1980" box="[1010,1152,1305,1331]" class="Insecta" family="Staphylinidae" genus="Pseudoraphes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="214" phylum="Arthropoda" rank="species" species="huallagae">
