<taxonomicName authorityName="Franz" authorityYear="1980" box="[335,434,1341,1367]" class="Insecta" family="Staphylinidae" genus="Pseudoraphes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="214" phylum="Arthropoda" rank="species" species="minor">
