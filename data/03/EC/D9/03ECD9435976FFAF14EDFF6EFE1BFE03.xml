; metatarsus v p 
<number box="[855,865,1702,1726]" pageId="19" pageNumber="20" value="1.0">1</number>
ap, p 
<date box="[931,959,1702,1726]" pageId="19" pageNumber="20">
v 
<number box="[948,959,1702,1726]" pageId="19" pageNumber="20" value="1.0">1</number>
</date>
-0. 
<emphasis bold="true" box="[998,1020,1702,1726]" pageId="19" pageNumber="20">II</emphasis>
, tibia 
<date box="[1093,1129,1702,1726]" pageId="19" pageNumber="20">
v 
<number box="[1115,1129,1702,1726]" pageId="19" pageNumber="20" value="2.0">2</number>
</date>
ap, p 0- 
<number box="[1219,1255,1702,1726]" pageId="19" pageNumber="20" value="0.5" valueMax="1.0" valueMin="0.0">0-1</number>
-0; metatarsus v p 
<number box="[167,177,1738,1762]" pageId="19" pageNumber="20" value="1.0">1</number>
ap. 
<emphasis bold="true" box="[220,253,1739,1763]" pageId="19" pageNumber="20">III</emphasis>
