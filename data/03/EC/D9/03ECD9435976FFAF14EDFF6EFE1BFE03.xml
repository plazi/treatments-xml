<paragraph blockId="19.[151,1437,1594,2015]" pageId="19" pageNumber="20">
<emphasis box="[199,257,1594,1618]" italics="true" pageId="19" pageNumber="20">Male</emphasis>
(
<typeStatus box="[275,378,1594,1618]" pageId="19" pageNumber="20">paratype</typeStatus>
