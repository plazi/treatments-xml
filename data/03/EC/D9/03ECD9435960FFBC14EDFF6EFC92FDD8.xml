<subSubSection pageId="7" pageNumber="8" type="diagnosis">
<paragraph blockId="7.[151,1436,355,631]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[151,275,355,380]" pageId="7" pageNumber="8">Diagnosis:</emphasis>
