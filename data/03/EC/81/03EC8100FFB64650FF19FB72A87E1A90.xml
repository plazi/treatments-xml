<paragraph blockId="36.[151,1437,152,683]" pageId="36" pageNumber="37">
<emphasis bold="true" box="[199,376,404,431]" pageId="36" pageNumber="37">Type material.</emphasis>
<materialsCitation collectingDate="2010-06-19" collectionCode="MHBU, SCAU" collectorName="Tian" country="China" county="between Sengge Township and Lhozhag County" elevation="4423" location="between Sengge Township and Lhozhag County" pageId="36" pageNumber="37" specimenCount="1" specimenCount-male="1" stateProvince="Xizang" typeStatus="holotype">
<emphasis bold="true" box="[385,503,405,431]" pageId="36" pageNumber="37">
