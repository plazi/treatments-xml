<collectingRegion box="[710,803,890,915]" country="Mexico" name="Chiapas" pageId="14" pageNumber="475">Chiapas</collectingRegion>
, 
<collectingCountry box="[814,903,890,915]" name="Mexico" pageId="14" pageNumber="475">
