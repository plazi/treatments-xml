<taxonomicName authorityName="Forel" authorityYear="1910" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="919" phylum="Arthropoda" rank="subSpecies" species="bicolor" subSpecies="hirsutum">
<emphasis box="[926,947,1389,1410]" italics="true" pageId="12" pageNumber="919">M</emphasis>
