<collectingCountry box="[330,391,539,560]" name="Egypt" pageId="36" pageNumber="943">Egypt</collectingCountry>
(
<collectingRegion box="[405,460,538,560]" country="Egypt" name="Shamal Sina'" pageId="36" pageNumber="943">Sinai</collectingRegion>
