<paragraph blockId="36.[178,1085,509,590]" pageId="36" pageNumber="943">
<taxonomicName baseAuthorityName="Rossi" baseAuthorityYear="1790" box="[178,388,509,531]" class="Insecta" family="Crabronidae" genus="Philanthus" kingdom="Animalia" order="Hymenoptera" pageId="36" pageNumber="943" phylum="Arthropoda" rank="species" species="venustus">
<emphasis box="[178,388,509,531]" italics="true" pageId="36" pageNumber="943">Philanthus venustus</emphasis>
</taxonomicName>
