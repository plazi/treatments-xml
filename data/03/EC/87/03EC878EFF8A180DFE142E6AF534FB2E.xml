Based on 
<typeStatus box="[323,423,1618,1642]" pageId="21" pageNumber="599">holotype</typeStatus>
male, 
<quantity box="[496,584,1618,1642]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" pageId="21" pageNumber="599" unit="mm" value="6.1">6.1 mm</quantity>
, 
