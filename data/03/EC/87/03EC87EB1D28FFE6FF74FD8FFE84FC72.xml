, 
<collectingCountry box="[345,395,881,903]" name="Serbia" pageId="29" pageNumber="936">Mali</collectingCountry>
, 
<collectingCountry box="[410,470,881,903]" name="Niger" pageId="29" pageNumber="936">Niger</collectingCountry>
