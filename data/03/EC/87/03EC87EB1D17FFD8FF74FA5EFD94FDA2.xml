<paragraph blockId="35.[178,1085,417,527]" pageId="35" pageNumber="942">
<taxonomicName baseAuthorityName="Spinola" baseAuthorityYear="1839" box="[178,352,417,438]" class="Insecta" family="Crabronidae" genus="Cerceris" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="942" phylum="Arthropoda" rank="species" species="fischeri">
