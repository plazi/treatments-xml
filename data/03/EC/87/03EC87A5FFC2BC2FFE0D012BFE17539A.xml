<taxonomicName authority="Walter, 1925" authorityName="Walter" authorityYear="1925" box="[792,1094,1420,1444]" class="Arachnida" family="Pontarachnidae" genus="Litarachna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="1989" phylum="Arthropoda" rank="species" species="duboscqi">
