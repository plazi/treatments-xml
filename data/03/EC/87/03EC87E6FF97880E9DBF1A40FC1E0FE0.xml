<subSubSection pageId="20" pageNumber="321" type="etymology">
<paragraph blockId="20.[151,1436,151,429]" pageId="20" pageNumber="321">
<emphasis bold="true" box="[199,334,223,248]" pageId="20" pageNumber="321">Etymology.</emphasis>
