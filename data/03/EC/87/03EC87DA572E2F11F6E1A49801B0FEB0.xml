<typeStatus box="[151,207,1454,1478]" pageId="4" pageNumber="47">Type</typeStatus>
species: 
<taxonomicName authority="Krishnankutty &amp; Dietrich, 2012" authorityName="Krishnankutty &amp; Dietrich" authorityYear="2012" box="[309,573,1453,1477]" class="Insecta" family="Cicadellidae" genus="Neotrocnada" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="napoensis" status="sp. nov.">
<emphasis box="[309,573,1453,1477]" italics="true" pageId="4" pageNumber="47">Neotrocnada napoensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[580,667,1454,1477]" pageId="4" pageNumber="47" rank="species">sp. nov.</taxonomicNameLabel>
</paragraph>
