<taxonomicName authority="Nakane, 1982" authorityName="Nakane" authorityYear="1982" box="[983,1297,764,789]" class="Insecta" family="Leiodidae" genus="Ptomaphaginus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="332" phylum="Arthropoda" rank="species" species="takaosanus">
