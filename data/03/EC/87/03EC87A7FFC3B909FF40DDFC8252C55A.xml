<specimenCount box="[345,516,1661,1687]" pageId="12" pageNumber="55" type="generic">
<number box="[345,376,1661,1687]" pageId="12" pageNumber="55" value="24.0">24</number>
specimens
</specimenCount>
(
<specimenCount box="[533,673,1661,1687]" pageId="12" pageNumber="55" type="female">
<number box="[533,566,1661,1687]" pageId="12" pageNumber="55" value="22.0">22</number>
females
</specimenCount>
and 
<specimenCount box="[736,824,1661,1687]" pageId="12" pageNumber="55" type="male">
<number box="[736,752,1661,1687]" pageId="12" pageNumber="55" value="2.0">2</number>
male
