<number box="[696,744,1496,1522]" pageId="10" pageNumber="53" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
; lateral seta (
<emphasis box="[906,939,1497,1522]" italics="true" pageId="10" pageNumber="53">ft”</emphasis>
) 
<number box="[957,1035,1496,1522]" pageId="10" pageNumber="53" value="18.0" valueMax="19.0" valueMin="17.0">17–19</number>
