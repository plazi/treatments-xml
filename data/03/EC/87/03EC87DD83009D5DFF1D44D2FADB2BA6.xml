<taxonomicName authorityName="Friese" authorityYear="1901" box="[643,743,1266,1291]" class="Insecta" family="Apidae" genus="Trigona" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="200" phylum="Arthropoda" rank="species" species="schultzei">schultzei</taxonomicName>
