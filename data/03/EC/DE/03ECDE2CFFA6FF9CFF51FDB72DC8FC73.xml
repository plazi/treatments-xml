<paragraph blockId="28.[151,1437,150,897]" pageId="28" pageNumber="29">
<emphasis bold="true" box="[199,317,799,825]" pageId="28" pageNumber="29">Remarks:</emphasis>
<taxonomicName authority="Wu" authorityName="Wu" authorityYear="1982" box="[326,650,799,825]" class="Insecta" family="Megachilidae" genus="Anthidium" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="nigroventrale">
