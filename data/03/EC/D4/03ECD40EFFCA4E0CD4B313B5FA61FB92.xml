<taxonomicName authorityName="Sivec &amp; Stark" authorityYear="2010" box="[719,798,1646,1670]" class="Insecta" family="Perlidae" genus="Phanoperla" kingdom="Animalia" order="Plecoptera" pageId="13" pageNumber="111" phylum="Arthropoda" rank="species" species="hubleyi">
