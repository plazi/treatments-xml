<paragraph blockId="9.[151,575,1495,1559]" box="[151,575,1495,1521]" pageId="9" pageNumber="52">
<heading bold="true" box="[151,575,1495,1521]" fontSize="11" level="1" pageId="9" pageNumber="52" reason="1">
<emphasis bold="true" box="[151,575,1495,1521]" pageId="9" pageNumber="52">
<taxonomicName box="[151,417,1495,1521]" class="Insecta" family="Psychomyiidae" genus="Paduniella" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="52" phylum="Arthropoda" rank="species" species="sabarmata" status="new species">
<emphasis bold="true" box="[151,417,1495,1521]" italics="true" pageId="9" pageNumber="52">Paduniella sabarmata</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[431,575,1495,1521]" pageId="9" pageNumber="52" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="9.[151,575,1495,1559]" box="[151,282,1533,1559]" pageId="9" pageNumber="52">
<figureCitation box="[151,282,1533,1559]" captionStart="FIGURES 19 – 21" captionStartId="10.[151,272,1573,1597]" captionTargetBox="[154,1432,355,1545]" captionTargetId="figure@10.[150,1436,343,1552]" captionTargetPageId="10" captionText="FIGURES 19 – 21. Paduniella sabarmata, new species, male holotype. 19 — genitalia, lateral; 20 — genitalia, ventral; 21 — phallic apparatus, lateral. Scale bar = 0.2 mm, refers to all figures." httpUri="https://zenodo.org/record/196785/files/figure.png" pageId="9" pageNumber="52">
Figs 
<number box="[209,282,1533,1559]" pageId="9" pageNumber="52" value="20.0" valueMax="21.0" valueMin="19.0">19–21</number>
</figureCitation>
</paragraph>
