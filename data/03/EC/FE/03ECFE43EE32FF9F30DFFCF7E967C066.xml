<paragraph blockId="5.[151,531,855,919]" box="[151,531,855,881]" pageId="5" pageNumber="48">
<heading bold="true" box="[151,531,855,881]" fontSize="11" level="1" pageId="5" pageNumber="48" reason="1">
<emphasis bold="true" box="[151,531,855,881]" pageId="5" pageNumber="48">
<taxonomicName box="[151,373,855,881]" class="Insecta" family="Psychomyiidae" genus="Paduniella" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="48" phylum="Arthropoda" rank="species" species="monga" status="new species">
<emphasis bold="true" box="[151,373,855,881]" italics="true" pageId="5" pageNumber="48">Paduniella monga</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[387,531,855,881]" pageId="5" pageNumber="48" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="5.[151,531,855,919]" box="[151,281,893,919]" pageId="5" pageNumber="48">
<figureCitation box="[151,281,893,919]" captionStart="FIGURES 10 – 12" captionStartId="6.[151,271,1698,1722]" captionTargetBox="[155,1432,448,1645]" captionTargetId="figure@6.[154,1433,421,1677]" captionTargetPageId="6" captionText="FIGURES 10 – 12. Paduniella monga, new species, male holotype. 10 — genitalia, lateral; 11 — genitalia, ventral; 12 — phallic apparatus, lateral. Scale bar = 0.2 mm, refers to all figures." httpUri="https://zenodo.org/record/196782/files/figure.png" pageId="5" pageNumber="48">
Figs 
<number box="[209,281,893,919]" pageId="5" pageNumber="48" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
</figureCitation>
</paragraph>
