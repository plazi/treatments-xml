<emphasis box="[482,629,1270,1291]" italics="true" pageId="10" pageNumber="715">D. acuminifer</emphasis>
PUTHZ
</taxonomicName>
, 
<taxonomicName authority="PUTHZ" authorityName="PUTHZ" authorityYear="2000" box="[725,933,1269,1291]" class="Insecta" family="Staphylinidae" genus="Dianous" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="715" phylum="Arthropoda" rank="species" species="angulifer">
