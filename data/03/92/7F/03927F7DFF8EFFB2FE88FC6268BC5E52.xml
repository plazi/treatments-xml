<subSubSection box="[312,1143,443,470]" pageId="3" pageNumber="56" type="etymology">
<paragraph blockId="3.[264,1323,283,630]" box="[312,1143,443,470]" pageId="3" pageNumber="56">
<emphasis bold="true" box="[312,452,443,469]" pageId="3" pageNumber="56">Etymology.</emphasis>
