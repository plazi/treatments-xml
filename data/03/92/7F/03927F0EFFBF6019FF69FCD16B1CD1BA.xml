species: 
<taxonomicName authority="Bergh, 1889" authorityName="Bergh" authorityYear="1889" class="Gastropoda" family="Discodorididae" genus="Carminodoris" kingdom="Animalia" order="Nudibranchia" pageId="80" pageNumber="615" phylum="Mollusca" rank="species" species="mauritiana">
