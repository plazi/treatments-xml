<quantity box="[1102,1208,687,708]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.3" metricValueMin="0.7" pageId="39" pageNumber="574" unit="mm" value="10.0" valueMax="13.0" valueMin="7.0">7–13 mm</quantity>
