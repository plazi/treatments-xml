, S 
<number box="[429,440,404,428]" pageId="2" pageNumber="47" value="1.0">1</number>
setae length 
<number box="[588,660,404,428]" pageId="2" pageNumber="47" value="83.0" valueMax="85.0" valueMin="81.0">81–85</number>
, S 
<number box="[687,702,404,428]" pageId="2" pageNumber="47" value="2.0">2</number>
setae length 
