<number box="[976,1036,223,247]" pageId="2" pageNumber="47" value="1500.0">1500</number>
. Head length 
<number box="[1193,1234,223,247]" pageId="2" pageNumber="47" value="115.0">115</number>
(length including anterior prolongation 
<number box="[399,444,260,284]" pageId="2" pageNumber="47" value="125.0">125</number>
), width of behind compound eyes 
<number box="[841,884,260,284]" pageId="2" pageNumber="47" value="115.0">115</number>
, width across cheeks 
<number box="[1134,1179,260,284]" pageId="2" pageNumber="47" value="120.0">120</number>
; compound eye dorsal length 
<number box="[228,259,295,319]" pageId="2" pageNumber="47" value="75.0">75</number>
, width 
<number box="[343,374,295,319]" pageId="2" pageNumber="47" value="43.0">43</number>
; ocellar setae III length 
<number box="[648,721,295,319]" pageId="2" pageNumber="47" value="42.5" valueMax="44.0" valueMin="41.0">41–44</number>
, interval 
<number box="[826,843,295,319]" pageId="2" pageNumber="47" value="5.0">5</number>
. Pronotal median length 
<number box="[1126,1170,295,319]" pageId="2" pageNumber="47" value="108.0">108</number>
, width, 
<number box="[1261,1305,295,319]" pageId="2" pageNumber="47" value="168.0">168</number>
, posteroangular setae I length 
<number box="[376,448,332,356]" pageId="2" pageNumber="47" value="53.5" valueMax="54.0" valueMin="53.0">53–54</number>
, setae II length 
<number box="[630,703,332,356]" pageId="2" pageNumber="47" value="47.5" valueMax="49.0" valueMin="46.0">46–49</number>
, posteromarginal setae I length 
<number box="[1064,1137,332,356]" pageId="2" pageNumber="47" value="28.5" valueMax="29.0" valueMin="28.0">28–29</number>
, setae II length 
<number box="[1318,1349,332,356]" pageId="2" pageNumber="47" value="23.0">23</number>
; metascutal median length 
<number box="[381,411,367,391]" pageId="2" pageNumber="47" value="78.0">78</number>
, median setae length 
<number box="[655,729,367,391]" pageId="2" pageNumber="47" value="29.0" valueMax="30.0" valueMin="28.0">28–30</number>
; fore wings length 
<number box="[946,991,367,391]" pageId="2" pageNumber="47" value="740.0">740</number>
