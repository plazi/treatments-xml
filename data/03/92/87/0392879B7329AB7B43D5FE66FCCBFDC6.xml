<taxonomicName baseAuthorityName="Cresson" baseAuthorityYear="1872" box="[230,486,581,606]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="texanum">
