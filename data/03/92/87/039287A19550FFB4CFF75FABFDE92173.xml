<specimenCount box="[413,477,222,249]" pageId="58" pageNumber="59" type="generic">
<quantity box="[413,473,222,249]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="58" pageNumber="59" unit="m" value="50.0">50 m</quantity>
.
</specimenCount>
</paragraph>
