<paragraph blockId="9.[151,1437,597,1810]" box="[151,964,597,622]" pageId="9" pageNumber="410">
<emphasis bold="true" box="[151,416,597,622]" pageId="9" pageNumber="410">
<typeStatus box="[151,210,597,622]" pageId="9" pageNumber="410">Type</typeStatus>
and only species.
</emphasis>
<taxonomicName authority="Scudder, 1890" authorityName="Scudder" authorityYear="1890" box="[423,798,597,622]" class="Insecta" family="Inocelliidae" genus="Inocellia" kingdom="Animalia" order="Raphidioptera" pageId="9" pageNumber="410" phylum="Arthropoda" rank="species" species="veterana">
