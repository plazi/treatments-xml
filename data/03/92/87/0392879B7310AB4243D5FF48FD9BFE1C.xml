<taxonomicName baseAuthorityName="Cresson" baseAuthorityYear="1864" box="[1202,1342,294,319]" class="Insecta" family="Megachilidae" genus="Hoplitis" kingdom="Animalia" order="Hymenoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="producta">
