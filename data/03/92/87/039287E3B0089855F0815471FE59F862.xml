<geoCoordinate box="[1045,1139,1652,1679]" degrees="66" direction="east" minutes="29" orientation="longitude" pageId="1" pageNumber="474" precision="925" value="66.48333">66°29ʹE</geoCoordinate>
, 
<date box="[1151,1332,1650,1677]" pageId="1" pageNumber="474" value="2019-10-05">
<collectingDate box="[1151,1332,1650,1677]" pageId="1" pageNumber="474" value="2019-10-05">5 October 2019</collectingDate>
