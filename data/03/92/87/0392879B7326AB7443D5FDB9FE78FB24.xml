<emphasis bold="true" box="[151,317,617,643]" italics="true" pageId="83" pageNumber="84">Lasioglossum</emphasis>
(
<emphasis bold="true" box="[334,443,617,643]" italics="true" pageId="83" pageNumber="84">Dialictus</emphasis>
) 
<emphasis bold="true" box="[460,592,617,643]" italics="true" pageId="83" pageNumber="84">oceanicum</emphasis>
