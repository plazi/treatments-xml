<taxonomicName authority="Gunnerus, 1765: 33" authorityName="Gunnerus" authorityPageNumber="33" authorityYear="1765" box="[812,941,655,681]" class="Elasmobranchii" family="Cetorhinidae" genus="Squalus" kingdom="Animalia" order="Lamniformes" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="maximus">
