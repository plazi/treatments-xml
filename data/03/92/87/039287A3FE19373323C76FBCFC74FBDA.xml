<emphasis box="[296,581,780,802]" italics="true" pageId="33" pageNumber="434">
<taxonomicName box="[296,500,780,802]" class="Insecta" family="Raphidiidae" genus="Florissantoraphidia" higherTaxonomySource="CoL" kingdom="Animalia" order="Raphidioptera" pageId="33" pageNumber="434" phylum="Arthropoda" rank="genus">Florissantoraphidia</taxonomicName>
<taxonomicName authority="Rohwer, 1909" authorityName="Rohwer" authorityYear="1909" box="[508,581,781,802]" pageId="33" pageNumber="434" rank="species" species="mortua">mortua</taxonomicName>
