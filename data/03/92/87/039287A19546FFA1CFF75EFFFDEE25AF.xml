<taxonomicName authority="Voigt 1832" authorityName=": Voigt" authorityPageNumber="128" authorityYear="1832" box="[556,903,1374,1400]" class="Elasmobranchii" family="Scyliorhinidae" genus="Scyllium" kingdom="Animalia" order="Carcharhiniformes" pageId="47" pageNumber="48" phylum="Chordata" rank="species" species="edwardsii">
