<collectingCountry box="[775,853,1590,1615]" name="Russia" pageId="8" pageNumber="409">Russia</collectingCountry>
; 
<taxonomicName authority="Willmann, 1994" authorityName="Willmann" authorityYear="1994" box="[866,1226,1590,1615]" class="Insecta" family="Baissopteridae" genus="Austroraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="genus">
<emphasis box="[866,1036,1590,1615]" italics="true" pageId="8" pageNumber="409">Austroraphidia</emphasis>
<bibRefCitation author="Willmann" box="[1044,1226,1590,1615]" pageId="8" pageNumber="409" refString="Willmann, R. (1994) Raphidiodea aus dem Lias und die Phylogenie der Kamelhalsfliegen (Insecta: Holometabola). Palaontologische Zeitschrift, 68, 167 - 197. http: // dx. doi. org / 10.1007 / bf 02989439" type="journal article" year="1994">
Willmann, 
<number box="[1170,1226,1591,1615]" pageId="8" pageNumber="409" value="1994.0">1994</number>
</bibRefCitation>
</taxonomicName>
(monotypic) from the Crato Formation of 
<collectingCountry box="[415,487,1626,1651]" name="Brazil" pageId="8" pageNumber="409">Brazil</collectingCountry>
; 
<taxonomicName box="[498,669,1626,1651]" class="Insecta" family="Raphidiidae" genus="Dictyoraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="genus">
<emphasis box="[498,669,1626,1651]" italics="true" pageId="8" pageNumber="409">Dictyoraphidia</emphasis>
