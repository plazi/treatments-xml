<taxonomicName authorityName="Ament &amp; Amorim" authorityYear="2013" box="[1240,1429,1735,1758]" class="Insecta" family="Phoridae" genus="Neopleurophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="prionotopyga">
<emphasis box="[1240,1429,1735,1758]" italics="true" pageId="10" pageNumber="11">N. prionotopyga</emphasis>
