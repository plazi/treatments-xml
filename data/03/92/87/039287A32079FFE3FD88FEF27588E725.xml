, 
<taxonomicName box="[1066,1236,1779,1805]" class="Amphibia" family="Nyctibatrachidae" genus="Nyctibatrachus" kingdom="Animalia" order="Anura" pageId="32" pageNumber="33" phylum="Chordata" rank="species" species="pulivijayani">
<emphasis box="[1066,1236,1779,1805]" italics="true" pageId="32" pageNumber="33">N. pulivijayani</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1297,1470,1779,1805]" class="Amphibia" family="Nyctibatrachidae" genus="Nyctibatrachus" kingdom="Animalia" order="Anura" pageId="32" pageNumber="33" phylum="Chordata" rank="species" species="robinmoorei">
