<emphasis box="[672,691,765,788]" italics="true" pageId="10" pageNumber="39">C</emphasis>
. 
<emphasis box="[706,804,765,788]" italics="true" pageId="10" pageNumber="39">pyrisuga</emphasis>
</taxonomicName>
) and 
<taxonomicName box="[869,983,764,789]" class="Insecta" family="Psyllidae" genus="Psyllopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis box="[869,983,764,789]" italics="true" pageId="10" pageNumber="39">Psyllopsis</emphasis>
