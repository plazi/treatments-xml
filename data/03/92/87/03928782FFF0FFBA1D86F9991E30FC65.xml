<taxonomicName authorityName="Haupt" authorityYear="1935" box="[1213,1389,728,751]" class="Insecta" family="Psyllidae" genus="Camarotoscena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="genus">
