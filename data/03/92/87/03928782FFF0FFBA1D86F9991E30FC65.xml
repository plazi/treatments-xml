<taxonomicName baseAuthorityName="Flor" baseAuthorityYear="1861" class="Insecta" family="Psyllidae" genus="Camarotoscena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="species" species="speciosa">
<emphasis box="[1410,1429,728,751]" italics="true" pageId="10" pageNumber="39">C</emphasis>
. 
<emphasis box="[151,247,765,788]" italics="true" pageId="10" pageNumber="39">speciosa</emphasis>
</taxonomicName>
), 
<taxonomicName authorityName="Ossiannilsson" authorityYear="1970" box="[271,397,764,789]" class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis box="[271,397,764,789]" italics="true" pageId="10" pageNumber="39">Cacopsylla</emphasis>
</taxonomicName>
(
<taxonomicName box="[415,522,764,789]" class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="species" species="bidens">
<emphasis box="[415,522,764,789]" italics="true" pageId="10" pageNumber="39">C. bidens</emphasis>
