<subSubSection pageId="27" pageNumber="28" type="diagnosis">
<paragraph blockId="27.[151,1437,1005,1888]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[151,266,1005,1030]" pageId="27" pageNumber="28">Diagnosis</emphasis>
