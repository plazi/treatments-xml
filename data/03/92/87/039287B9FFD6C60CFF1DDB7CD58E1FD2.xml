<geoCoordinate box="[905,1070,1578,1605]" degrees="56" direction="west" minutes="24" orientation="longitude" pageId="3" pageNumber="78" precision="1" seconds="01.0" value="-56.40028">56°24’01.0”W</geoCoordinate>
, semi-natural riparian forest, leg. 
<collectorName box="[151,243,1614,1641]" pageId="3" pageNumber="78">W. Jahn</collectorName>
, 
<date box="[254,411,1614,1641]" pageId="3" pageNumber="78" value="1991-03-05" valueMax="1991-03-10" valueMin="1991-03-05">
<collectingDate box="[254,411,1614,1641]" pageId="3" pageNumber="78" value="1991-03-05" valueMax="1991-03-10" valueMin="1991-03-05">5–10.III.1991</collectingDate>
</date>
, 
<specimenCount box="[422,457,1614,1640]" pageId="3" pageNumber="78" type="female">1♀</specimenCount>
