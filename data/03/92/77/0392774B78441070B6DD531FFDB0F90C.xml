<taxonomicName authorityName="Koch &amp; Berendt" authorityYear="1854" box="[489,587,1575,1601]" class="Arachnida" family="Phalangiidae" genus="Opilio" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="19" pageNumber="366" phylum="Arthropoda" rank="species" species="ovalis">
