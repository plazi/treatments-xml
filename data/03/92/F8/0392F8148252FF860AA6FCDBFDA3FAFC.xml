<taxonomicName authority="Wagner, 1964" authorityName="Wagner" authorityYear="1964" box="[140,495,1187,1213]" class="Insecta" family="Cydnidae" genus="Byrsinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="minor">
