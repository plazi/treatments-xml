<date box="[907,995,1311,1332]" pageId="6" pageNumber="555" value="2010-11-11">
<number box="[907,931,1311,1332]" pageId="6" pageNumber="555" value="11.0">11</number>
- 
<number box="[938,962,1311,1332]" pageId="6" pageNumber="555" value="11.0">11</number>
