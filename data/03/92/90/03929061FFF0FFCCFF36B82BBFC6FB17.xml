<number box="[747,760,1194,1215]" pageId="6" pageNumber="555" value="8.0">8</number>
- 
<number box="[768,781,1194,1215]" pageId="6" pageNumber="555" value="8.0">8</number>
- 
<number box="[788,801,1194,1215]" pageId="6" pageNumber="555" value="8.0">8</number>
; leg 
<date box="[847,901,1194,1215]" pageId="6" pageNumber="555">
II 
<number box="[869,881,1194,1215]" pageId="6" pageNumber="555" value="8.0">8</number>
- 
<number box="[889,901,1194,1215]" pageId="6" pageNumber="555" value="8.0">8</number>
