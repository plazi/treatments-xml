<typeStatus box="[608,656,1058,1082]" pageId="6" pageNumber="431">type</typeStatus>
locality (
<figureCitation box="[762,831,1057,1082]" captionStart="FIGURE 7" captionStartId="8.[151,250,1518,1540]" captionTargetBox="[151,1436,904,1497]" captionTargetId="figure@8.[151,1436,904,1497]" captionTargetPageId="8" captionText="FIGURE 7. Distribution records of Holcobunus species." httpUri="https://zenodo.org/record/241201/files/figure.png" pageId="6" pageNumber="431">
