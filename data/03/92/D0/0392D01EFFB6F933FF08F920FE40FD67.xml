<subSubSection pageId="37" pageNumber="234" type="biology_ecology">
<paragraph blockId="37.[151,1436,158,713]" pageId="37" pageNumber="234">
<emphasis bold="true" box="[196,303,649,675]" pageId="37" pageNumber="234">Ecology.</emphasis>
