<taxonomicName baseAuthorityName="Paykull" baseAuthorityYear="1811" box="[587,758,158,184]" class="Insecta" family="Histeridae" genus="Hypocaccus" kingdom="Animalia" order="Coleoptera" pageId="55" pageNumber="252" phylum="Arthropoda" rank="species" species="brasiliensis">
