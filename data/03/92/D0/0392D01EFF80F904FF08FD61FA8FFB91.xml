<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[158,288,1061,1087]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Coleoptera" pageId="18" pageNumber="215" phylum="Tracheophyta" rank="order">Coleoptera</taxonomicName>
