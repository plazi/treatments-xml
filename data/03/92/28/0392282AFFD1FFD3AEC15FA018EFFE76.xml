<taxonomicName authority="Naumov, 1955" authorityName="Naumov" authorityYear="1955" box="[752,1056,447,472]" class="Hydrozoa" family="Halopterididae" genus="Astrolabia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="55" pageNumber="56" phylum="Cnidaria" rank="genus">
