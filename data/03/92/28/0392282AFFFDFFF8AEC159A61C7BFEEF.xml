<taxonomicName authority="Fraser, 1935 b" authorityName="Fraser" authorityYear="1935" box="[329,651,1985,2010]" class="Hydrozoa" family="Haleciidae" genus="Endothecium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="27" pageNumber="28" phylum="Cnidaria" rank="genus">
