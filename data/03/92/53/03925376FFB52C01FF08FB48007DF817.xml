<paragraph blockId="8.[151,1437,1435,2036]" pageId="8" pageNumber="71">
<emphasis bold="true" box="[151,392,1435,1460]" pageId="8" pageNumber="71">Material examined.</emphasis>
Female 
<typeStatus box="[502,603,1435,1460]" pageId="8" pageNumber="71">holotype</typeStatus>
, one male 
<typeStatus box="[744,840,1436,1460]" pageId="8" pageNumber="71">paratype</typeStatus>
