<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[995,1072,1245,1266]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="5" pageNumber="467">MNHN</collectionCode>
