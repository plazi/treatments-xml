locality, 
<collectingRegion box="[1088,1152,368,391]" country="Ecuador" name="Napo" pageId="21" pageNumber="460">Napo</collectingRegion>
, 
<collectingCountry box="[1162,1255,367,392]" name="Ecuador" pageId="21" pageNumber="460">Ecuador</collectingCountry>
