<subSubSection box="[199,1221,727,752]" pageId="7" pageNumber="8" type="etymology">
<paragraph blockId="7.[151,1437,151,968]" box="[199,1221,727,752]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[199,334,727,752]" pageId="7" pageNumber="8">Etymology.</emphasis>
