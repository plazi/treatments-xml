; 
<bibRefCitation author="Haviland" box="[542,726,882,908]" pageId="10" pageNumber="67" refString="Haviland, M. D. (1931) The Reduviidae of Kartabo Bartica District, British Guiana. Zoologica, 7, 129 - 154." type="journal article" year="1931">
Haviland 
<number box="[662,726,882,908]" pageId="10" pageNumber="65" value="1931.0">1931</number>
</bibRefCitation>
; 
<bibRefCitation author="Wygodzinsky" box="[739,978,882,908]" pageId="10" pageNumber="67" refString="Wygodzinsky, P. (1949) Elenco sistematico de los reduviiformes americanos. Instituto de Medicina Regional de la Universidad Nacional de Tucuman, Monografia, 1, 1 - 102." type="journal article" year="1949">
Wygodzinsky 
<number box="[914,978,882,908]" pageId="10" pageNumber="65" value="1949.0">1949</number>
</bibRefCitation>
; Maldonado 
<number box="[1138,1202,882,908]" pageId="10" pageNumber="65" value="1990.0">1990</number>
; 
<bibRefCitation author="Carvalho" pageId="10" pageNumber="66" refString="Carvalho, J. C. M. &amp; Costa, L. A. A. (1993) Revision of the known species of Notocyrtus Burmeister, 1835 (Hemiptera- Heteroptera, Reduviidae; Harpactorinae). Revista Brasileira de Biologia, 53, 365 - 384." type="journal article" year="1993">
Carvalho &amp; Costa 
<number box="[151,214,921,947]" pageId="10" pageNumber="65" value="1993.0">1993</number>
</bibRefCitation>
). The specimen examined from 
<collectingCountry box="[587,686,921,947]" name="Suriname" pageId="10" pageNumber="65">Surinam</collectingCountry>
