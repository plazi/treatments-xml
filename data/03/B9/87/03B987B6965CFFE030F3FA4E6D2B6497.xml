<taxonomicName authority="Vitale, 1903" authorityName="Vitale" authorityYear="1903" box="[948,1350,799,824]" class="Insecta" family="Curculionidae" genus="Pseudomeira" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="43" phylum="Arthropoda" rank="species" species="reitteri">
