<subSubSection box="[179,990,261,280]" pageId="7" pageNumber="246" type="description">
<paragraph blockId="7.[179,990,261,280]" box="[179,990,261,280]" pageId="7" pageNumber="246">
<heading allCaps="true" box="[179,990,261,280]" centered="true" fontSize="9" level="2" pageId="7" pageNumber="246" reason="9">
(Apterae of 
<taxonomicName authorityName="Takahashi. However" authorityYear="1934" box="[305,452,261,280]" class="Insecta" family="Aphididae" genus="Anomalosiphum" kingdom="Animalia" order="Aphidomorpha" pageId="7" pageNumber="246" phylum="Arthropoda" rank="species" species="pithecolobii">
<emphasis box="[305,452,261,280]" italics="true" pageId="7" pageNumber="246">A. pithecolobii</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Quednau &amp; Martin" authorityYear="2006" box="[466,577,261,280]" class="Insecta" family="Aphididae" genus="Anomalosiphum" kingdom="Animalia" order="Aphidomorpha" pageId="7" pageNumber="246" phylum="Arthropoda" rank="species" species="mendeli">
<emphasis box="[466,577,261,280]" italics="true" pageId="7" pageNumber="246">A. mendeli</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Quednau &amp; Martin" authorityYear="2006" box="[590,675,261,280]" class="Insecta" family="Aphididae" genus="Anomalosiphum" kingdom="Animalia" order="Aphidomorpha" pageId="7" pageNumber="246" phylum="Arthropoda" rank="species" species="essigi">
<emphasis box="[590,675,261,280]" italics="true" pageId="7" pageNumber="246">A. essigi</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Qiao &amp; G. X. Zhang" authorityYear="2001" box="[730,877,261,280]" class="Insecta" family="Aphididae" genus="Anomalosiphum" kingdom="Animalia" order="Aphidomorpha" pageId="7" pageNumber="246" phylum="Arthropoda" rank="species" species="scleroticum">
