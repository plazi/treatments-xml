<typeStatus box="[936,984,1125,1149]" pageId="5" pageNumber="444">type</typeStatus>
locality, 
<collectingRegion box="[1087,1144,1124,1148]" country="Brazil" name="Acre" pageId="5" pageNumber="444">Acre</collectingRegion>
, 
<collectingCountry box="[1156,1224,1124,1149]" name="Brazil" pageId="5" pageNumber="444">Brazil</collectingCountry>
(
<figureCitation box="[1240,1338,1124,1149]" captionStart="FIGURES 182" captionStartId="30.[151,264,1634,1657]" captionTargetBox="[242,1345,625,1604]" captionTargetId="figure@30.[242,1345,625,1604]" captionTargetPageId="30" captionText="FIGURES 182. Distribution of Amazunculus species. 182, A. acreanus sp. nov., A. besti Rafael, A. bethoi sp. nov., A. cordigaster Galinkin &amp; Rafael and A. deargentatus Galinkin &amp; Rafael." httpUri="https://zenodo.org/record/2632291/files/figure.png" pageId="5" pageNumber="444">Fig. 182</figureCitation>
).
</paragraph>
