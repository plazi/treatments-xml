BL. 
<quantity box="[542,680,1830,1854]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="4.7" metricValueMin="4.3" pageId="8" pageNumber="597" unit="mm" value="4.5" valueMax="4.7" valueMin="4.3">4.3–4.7 mm</quantity>
, WL. 
<quantity box="[751,868,1830,1854]" metricMagnitude="-3" metricUnit="m" metricValue="3.85" metricValueMax="4.0" metricValueMin="3.7" pageId="8" pageNumber="597" unit="mm" value="3.85" valueMax="4.0" valueMin="3.7">3.7–4 mm</quantity>
, AL. 
<quantity box="[933,1029,1829,1854]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="8" pageNumber="597" unit="mm" value="0.85">0.85mm</quantity>
; Male. BL. 
<quantity box="[1164,1281,1830,1854]" metricMagnitude="-3" metricUnit="m" metricValue="3.85" metricValueMax="4.0" metricValueMin="3.7" pageId="8" pageNumber="597" unit="mm" value="3.85" valueMax="4.0" valueMin="3.7">3.7–4 mm</quantity>
, WL. 
