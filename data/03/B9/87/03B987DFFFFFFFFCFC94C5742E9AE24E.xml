<taxonomicName authorityName="Lacordaire" authorityYear="1842" box="[841,1130,1747,1769]" class="Insecta" family="Erotylidae" genus="Mycotretus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="969" phylum="Arthropoda" rank="species" species="rhodosomus">
<emphasis box="[841,1130,1747,1769]" italics="true" pageId="16" pageNumber="969">Mycotretus rhodosomus</emphasis>
</taxonomicName>
+ 
<taxonomicName authorityName="Crotch" authorityYear="1876" box="[1202,1381,1747,1769]" class="Insecta" family="Erotylidae" genus="Mycotretus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="969" phylum="Arthropoda" rank="species" species="reticulatus">
<emphasis box="[1202,1381,1747,1769]" italics="true" pageId="16" pageNumber="969">M. reticulatus</emphasis>
</taxonomicName>
+ 
<taxonomicName authorityName="Lacordaire" authorityYear="1842" box="[841,1063,1778,1800]" class="Insecta" family="Erotylidae" genus="Mycotretus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="969" phylum="Arthropoda" rank="species" species="flavomarginatus">
