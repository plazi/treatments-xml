<collectingCountry box="[579,700,1577,1601]" name="Guatemala" pageId="13" pageNumber="14">Guatemala</collectingCountry>
(
<emphasis box="[718,824,1577,1601]" italics="true" pageId="13" pageNumber="14">Guatalón</emphasis>
