<collectingCountry box="[1045,1131,1541,1565]" name="Mexico" pageId="13" pageNumber="14">Mexico</collectingCountry>
(
<emphasis box="[1148,1284,1541,1565]" italics="true" pageId="13" pageNumber="14">
<collectingRegion box="[1148,1284,1541,1565]" country="Mexico" name="Nuevo Leon" pageId="13" pageNumber="14">Nuevo León</collectingRegion>
</emphasis>
, 
<emphasis box="[1299,1429,1541,1565]" italics="true" pageId="13" pageNumber="14">
<collectingRegion box="[1299,1429,1541,1565]" country="Mexico" name="Tamaulipas" pageId="13" pageNumber="14">Tamaulipas</collectingRegion>
</emphasis>
, 
<emphasis box="[151,230,1577,1601]" italics="true" pageId="13" pageNumber="14">
<collectingRegion box="[151,230,1577,1601]" country="Mexico" name="Jalisco" pageId="13" pageNumber="14">Jalisco</collectingRegion>
