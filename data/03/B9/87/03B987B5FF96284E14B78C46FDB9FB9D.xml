<number box="[516,526,334,358]" pageId="4" pageNumber="25" value="1.0">1</number>
A
</figureCitation>
) L 
<number box="[594,636,334,358]" pageId="4" pageNumber="25" value="223.0">223</number>
(
<number box="[653,700,334,358]" pageId="4" pageNumber="25" value="226.0">226</number>
); Cx- 
<number box="[766,778,334,358]" pageId="4" pageNumber="25" value="3.0">3</number>
W 
<number box="[822,864,334,358]" pageId="4" pageNumber="25" value="252.0">252</number>
(
