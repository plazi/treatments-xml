<subSubSection pageId="34" pageNumber="35" type="distribution">
<paragraph blockId="34.[151,1436,151,895]" pageId="34" pageNumber="35">
<emphasis bold="true" box="[199,345,583,608]" pageId="34" pageNumber="35">Distribution</emphasis>
. 
<collectingCountry box="[360,482,583,607]" name="Costa Rica" pageId="34" pageNumber="35">Costa Rica</collectingCountry>
