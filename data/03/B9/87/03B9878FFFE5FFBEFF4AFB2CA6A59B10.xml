<emphasis box="[201,332,1826,1845]" italics="true" pageId="3" pageNumber="172">Benthana carijos</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[337,401,1827,1846]" pageId="3" pageNumber="172" rank="species">sp. nov.</taxonomicNameLabel>
(male paratype). 
<emphasis bold="true" box="[543,562,1826,1845]" pageId="3" pageNumber="172">A:</emphasis>
pereopod 1; 
<emphasis bold="true" box="[670,689,1826,1845]" pageId="3" pageNumber="172">B:</emphasis>
pereopod 7; 
<emphasis bold="true" box="[798,818,1826,1845]" pageId="3" pageNumber="172">C:</emphasis>
uropod; 
<emphasis bold="true" box="[894,917,1826,1845]" pageId="3" pageNumber="172">D:</emphasis>
genital papilla; 
<emphasis bold="true" box="[1048,1066,1826,1845]" pageId="3" pageNumber="172">E:</emphasis>
pleopod 1; 
