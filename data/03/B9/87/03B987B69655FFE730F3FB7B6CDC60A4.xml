: Messina: Peloritani: Calamarà (
<collectionCode box="[788,846,1724,1748]" pageId="14" pageNumber="49">SOL</collectionCode>
), Campo 
<collectingCountry box="[954,1012,1724,1748]" name="Italy" pageId="14" pageNumber="49">Italia</collectingCountry>
(
<collectionCode box="[1027,1085,1724,1748]" pageId="14" pageNumber="49">BEL</collectionCode>
), 
<taxonomicName box="[1105,1206,1724,1748]" class="Magnoliopsida" family="Fagaceae" genus="Castanea" kingdom="Plantae" order="Fagales" pageId="14" pageNumber="49" phylum="Tracheophyta" rank="genus">Castanea</taxonomicName>
(
<collectionCode box="[1221,1279,1724,1748]" pageId="14" pageNumber="49">SOL</collectionCode>
), Colla (
<collectionCode box="[1375,1432,1724,1748]" pageId="14" pageNumber="49">SOL</collectionCode>
, 
<collectionCode box="[151,215,1759,1783]" pageId="14" pageNumber="49">RAG</collectionCode>
), Cudduttà (
<collectionCode box="[359,411,1759,1783]" pageId="14" pageNumber="49">VIT</collectionCode>
), Dinnamare (
<collectionCode box="[576,633,1759,1783]" pageId="14" pageNumber="49">BAV</collectionCode>
, 
<collectionCode box="[648,705,1759,1783]" pageId="14" pageNumber="49">BEL</collectionCode>
), Portella Armacera (
<collectionCode box="[954,1011,1759,1783]" pageId="14" pageNumber="49">BAV</collectionCode>
, 
<collectionCode box="[1026,1083,1759,1783]" pageId="14" pageNumber="49">BEL</collectionCode>
), Portella Croce Cumia (
<collectionCode box="[1375,1432,1759,1783]" pageId="14" pageNumber="49">BAV</collectionCode>
, 
<collectionCode box="[151,208,1796,1820]" pageId="14" pageNumber="49">BEL</collectionCode>
), Portella S. Rizzo (
<collectionCode box="[435,497,1796,1820]" pageId="14" pageNumber="49">BAV</collectionCode>
