<collectionCode box="[943,1000,1831,1855]" pageId="14" pageNumber="49">BAV</collectionCode>
, OSE, 
<collectionCode box="[1084,1130,1831,1855]" pageId="14" pageNumber="49">PIE</collectionCode>
