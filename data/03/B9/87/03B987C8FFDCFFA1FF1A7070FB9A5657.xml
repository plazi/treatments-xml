<number box="[1254,1307,260,285]" pageId="48" pageNumber="49" value="5.75">5.75</number>
; ovipositor stylets length, 
<number box="[312,343,296,320]" pageId="48" pageNumber="49" value="31.0">31</number>
. (
<typeStatus box="[361,463,296,320]" pageId="48" pageNumber="49">Paratype</typeStatus>
, slide)—ovipositor length, 
<number box="[767,820,295,320]" pageId="48" pageNumber="49" value="34.5">34.5</number>
; third valvula length, 
<number box="[1062,1078,295,320]" pageId="48" pageNumber="49" value="6.0">6</number>
; ovipositor stylets length, 
<number box="[1371,1425,295,320]" pageId="48" pageNumber="49" value="32.5">32.5</number>
).
</paragraph>
