<collectingCountry box="[358,465,1124,1148]" name="Suriname" pageId="32" pageNumber="33">Suriname</collectingCountry>
and 
<collectingCountry box="[519,591,1124,1148]" name="Brazil" pageId="32" pageNumber="33">Brazil</collectingCountry>
