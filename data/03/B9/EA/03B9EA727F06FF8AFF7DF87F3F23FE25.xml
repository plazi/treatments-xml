<paragraph blockId="50.[151,1436,151,429]" pageId="50" pageNumber="51">
<emphasis bold="true" box="[199,315,367,392]" pageId="50" pageNumber="51">Remarks.</emphasis>
<taxonomicName authorityName="Newman &amp; Ross" authorityYear="1998" box="[321,538,367,392]" class="Maxillopoda" family="Scalpellidae" genus="Vertebroscalpellum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pedunculata" pageId="50" pageNumber="51" phylum="Arthropoda" rank="genus">
