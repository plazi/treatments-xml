<subSubSection pageId="21" pageNumber="22" type="nomenclature">
<paragraph blockId="21.[151,519,1663,1723]" box="[151,519,1663,1690]" pageId="21" pageNumber="22">
<heading bold="true" box="[151,519,1663,1690]" fontSize="11" level="1" pageId="21" pageNumber="22" reason="1">
<emphasis bold="true" box="[151,519,1663,1690]" pageId="21" pageNumber="22">
<taxonomicName box="[151,418,1663,1689]" class="Insecta" family="Phoridae" genus="Myriophora" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="flavicosta" status="sp. nov.">
<emphasis bold="true" box="[151,418,1663,1689]" italics="true" pageId="21" pageNumber="22">Myriophora flavicosta</emphasis>
