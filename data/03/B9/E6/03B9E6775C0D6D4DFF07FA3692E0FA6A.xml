<paragraph blockId="23.[151,500,1482,1542]" box="[151,448,1517,1542]" pageId="23" pageNumber="24">
(Figs 
<number box="[215,253,1518,1542]" pageId="23" pageNumber="24" value="4.2">4.2</number>
