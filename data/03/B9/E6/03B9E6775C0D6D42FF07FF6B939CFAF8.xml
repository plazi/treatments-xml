<taxonomicName box="[151,407,151,177]" class="Insecta" family="Phoridae" genus="Myriophora" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="gigantea" status="sp. nov.">
<emphasis bold="true" box="[151,407,151,177]" italics="true" pageId="23" pageNumber="24">Myriophora gigantea</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[414,508,152,178]" pageId="23" pageNumber="24" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="23.[151,508,151,211]" box="[151,384,186,211]" pageId="23" pageNumber="24">
(Figs 
<number box="[215,252,187,211]" pageId="23" pageNumber="24" value="4.1">4.1</number>
, 
<number box="[264,314,187,211]" pageId="23" pageNumber="24" value="11.1">11.1</number>
, 
<number box="[326,376,187,211]" pageId="23" pageNumber="24" value="17.1">17.1</number>
