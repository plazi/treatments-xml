<paragraph blockId="18.[151,575,151,211]" box="[151,386,186,211]" pageId="18" pageNumber="19">
(Figs 
<number box="[215,252,187,211]" pageId="18" pageNumber="19" value="3.1">3.1</number>
, 
<number box="[264,315,186,211]" pageId="18" pageNumber="19" value="10.5">10.5</number>
