<taxonomicName authority="Allen (1994)" authorityName="Allen" authorityYear="1994" box="[756,1074,188,213]" class="Entognatha" family="Campodeidae" genus="Parallocampa" kingdom="Animalia" order="Diplura" pageId="7" pageNumber="304" phylum="Arthropoda" rank="genus">
