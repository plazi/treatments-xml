(
<figureCitation box="[159,256,522,548]" captionStart="FIGURE 13" captionStartId="18.[151,255,1909,1933]" captionTargetBox="[151,1436,1013,1887]" captionTargetId="figure@18.[151,1436,1013,1888]" captionTargetPageId="18" captionText="FIGURE 13. Lateral habitus of C. lateventris holotype female, including label information." httpUri="https://zenodo.org/record/276007/files/figure.png" pageId="18" pageNumber="19">Figs. 13</figureCitation>
, 
<figureCitation box="[270,298,522,548]" captionStart="FIGURE 14" captionStartId="19.[151,256,1953,1977]" captionTargetBox="[175,1383,198,1917]" captionTargetId="figure@19.[168,1389,194,1923]" captionTargetPageId="19" captionText="FIGURE 14. Holotype and paratype females of C. lateventris. A – D, F, G, holotype; E, paratype. A, anterior head; B, lateral head; C, dorsal head; D, dorsal mesosoma; E, lateral mesosoma; F, dorsal metasoma; G, forewing." httpUri="https://zenodo.org/record/276008/files/figure.png" pageId="18" pageNumber="19">14</figureCitation>
