<paragraph blockId="13.[151,594,1010,1071]" box="[151,351,1045,1071]" pageId="13" pageNumber="14">
(
<figureCitation box="[159,242,1045,1071]" captionStart="FIGURE 9" captionStartId="14.[151,255,1596,1620]" captionTargetBox="[151,1436,407,1571]" captionTargetId="figure@14.[151,1436,407,1573]" captionTargetPageId="14" captionText="FIGURE 9. Lateral habitus of C. catemacoensis holotype female, including label information." httpUri="https://zenodo.org/record/276003/files/figure.png" pageId="13" pageNumber="14">Figs. 9</figureCitation>
, 
<figureCitation box="[255,283,1045,1071]" captionStart="FIGURE 10" captionStartId="15.[151,260,1980,2004]" captionTargetBox="[238,1346,199,1960]" captionTargetId="figure@15.[233,1351,194,1966]" captionTargetPageId="15" captionText="FIGURE 10. Holotype female of C. catemacoensis. A, anterior head; B, lateral head; C, dorsal head; D, dorsal mesosoma; E, lateral mesosoma; F, dorsal metasoma; G, forewing." httpUri="https://zenodo.org/record/276004/files/figure.png" pageId="13" pageNumber="14">10</figureCitation>
