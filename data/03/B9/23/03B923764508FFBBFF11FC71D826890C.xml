(
<figureCitation box="[159,257,1037,1063]" captionStart="FIGURE 25" captionStartId="33.[151,255,1948,1972]" captionTargetBox="[224,1341,682,1918]" captionTargetId="figure@33.[224,1341,682,1918]" captionTargetPageId="33" captionText="FIGURE 25. Lateral habitus of C. sarmientoi holotype female, including label information." httpUri="https://zenodo.org/record/276019/files/figure.png" pageId="32" pageNumber="33">Figs. 25</figureCitation>
, 
<figureCitation box="[270,299,1037,1063]" captionStart="FIGURE 26" captionStartId="34.[151,255,1975,1999]" captionTargetBox="[197,1391,201,1952]" captionTargetId="figure@34.[189,1402,194,1958]" captionTargetPageId="34" captionText="FIGURE 26. Holotype female of C. sarmientoi. A, anterior head; B, lateral head; C, dorsal head; D, dorsal mesosoma; E, lateral mesosoma; F, dorsal metasoma; G, forewing." httpUri="https://zenodo.org/record/276020/files/figure.png" pageId="32" pageNumber="33">26</figureCitation>
