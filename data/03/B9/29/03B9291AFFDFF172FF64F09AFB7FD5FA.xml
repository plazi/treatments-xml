<geoCoordinate box="[694,778,1958,1984]" direction="west" orientation="longitude" pageId="14" pageNumber="51" precision="555" value="-0.71">
w 
<number box="[723,778,1958,1984]" pageId="14" pageNumber="51" value="0.71">0.71</number>
