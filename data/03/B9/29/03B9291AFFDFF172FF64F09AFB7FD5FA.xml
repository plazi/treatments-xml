<paragraph blockId="14.[151,1437,1069,2023]" lastBlockId="15.[151,1436,152,255]" lastPageId="15" lastPageNumber="52" pageId="14" pageNumber="51">
Female (
<typeStatus box="[298,407,1726,1752]" pageId="14" pageNumber="51">holotype</typeStatus>
). Total length 
<number box="[572,627,1726,1752]" pageId="14" pageNumber="51" value="2.74">2.74</number>
. Carapace length 
<number box="[833,887,1726,1752]" pageId="14" pageNumber="51" value="1.18">1.18</number>
, 
<geoCoordinate box="[899,981,1726,1752]" direction="west" orientation="longitude" pageId="14" pageNumber="51" precision="555" value="-1.0">w 1.00</geoCoordinate>
, with a brown, short fovea length 
<number box="[1377,1431,1726,1752]" pageId="14" pageNumber="51" value="0.1">0.10</number>
