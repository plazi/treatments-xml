<geoCoordinate box="[761,846,1765,1791]" direction="west" orientation="longitude" pageId="14" pageNumber="51" precision="555" value="-0.32">
w 
<number box="[791,846,1765,1791]" pageId="14" pageNumber="51" value="0.32">0.32</number>
</geoCoordinate>
