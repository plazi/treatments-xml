<paragraph blockId="78.[151,1436,402,578]" box="[151,765,494,516]" pageId="78" pageNumber="79">
<taxonomicName authority="(Thomson, 1863)" authorityName="Thomson" authorityYear="1863" baseAuthorityName="Thomson" baseAuthorityYear="1863" box="[151,576,494,516]" class="Insecta" family="Tenthredinidae" genus="Phyllocolpa" kingdom="Animalia" order="Hymenoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="ischnocera">
