<emphasis bold="true" box="[151,394,1393,1419]" pageId="77" pageNumber="78">
<taxonomicName authorityName="Liston &amp; Heibo &amp; Prous &amp; Vårdal &amp; Nyman &amp; Vikberg" authorityYear="2017" baseAuthorityName="Thomson" baseAuthorityYear="1863" box="[151,283,1393,1419]" class="Insecta" family="Tenthredinidae" genus="Euura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="ischnocera">
