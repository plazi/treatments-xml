<taxonomicName authorityName=": Westwood" authorityYear="1839" baseAuthorityName="Jurine" baseAuthorityYear="1807" box="[151,200,1036,1062]" class="Insecta" family="Tenthredinidae" genus="Euura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="atra">
<emphasis bold="true" box="[151,200,1036,1062]" italics="true" pageId="56" pageNumber="57">atra</emphasis>
</taxonomicName>
