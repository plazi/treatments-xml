</taxonomicName>
, 
<taxonomicName baseAuthorityName="Brischke" baseAuthorityYear="1883" box="[623,746,223,246]" class="Insecta" family="Tenthredinidae" genus="Euura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="testaceipes">
<emphasis box="[623,746,223,246]" italics="true" pageId="54" pageNumber="55">testaceipes</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Brischke" baseAuthorityYear="1883" box="[803,889,223,246]" class="Insecta" family="Tenthredinidae" genus="Euura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="venusta">
<emphasis box="[803,889,223,246]" italics="true" pageId="54" pageNumber="55">venusta</emphasis>
