<emphasis bold="true" box="[577,664,884,910]" italics="true" pageId="1" pageNumber="264">
<taxonomicNameLabel box="[577,664,884,910]" pageId="1" pageNumber="264" rank="species">sp. nov.</taxonomicNameLabel>
