<paragraph blockId="5.[151,1436,438,2015]" pageId="5" pageNumber="170">
<emphasis bold="true" box="[199,345,546,571]" pageId="5" pageNumber="170">Description.</emphasis>
Body length 
<quantity box="[513,657,546,570]" metricMagnitude="-3" metricUnit="m" metricValue="8.75" metricValueMax="9.8" metricValueMin="7.7" pageId="5" pageNumber="170" unit="mm" value="8.75" valueMax="9.8" valueMin="7.7">7.7–9.8 mm</quantity>
. HL 
<quantity box="[724,896,546,570]" metricMagnitude="-4" metricUnit="m" metricValue="5.8999999999999995" metricValueMax="6.0" metricValueMin="5.8" pageId="5" pageNumber="170" unit="mm" value="0.59" valueMax="0.6" valueMin="0.58">0.58–0.60 mm</quantity>
, HW 
<quantity box="[972,1144,546,570]" metricMagnitude="-3" metricUnit="m" metricValue="1.21" metricValueMax="1.24" metricValueMin="1.18" pageId="5" pageNumber="170" unit="mm" value="1.21" valueMax="1.24" valueMin="1.18">1.18–1.24 mm</quantity>
, PL 
<quantity box="[1205,1378,546,570]" metricMagnitude="-3" metricUnit="m" metricValue="1.305" metricValueMax="1.38" metricValueMin="1.23" pageId="5" pageNumber="170" unit="mm" value="1.305" valueMax="1.38" valueMin="1.23">1.23–1.38 mm</quantity>
, PW 
<quantity box="[151,316,582,606]" metricMagnitude="-3" metricUnit="m" metricValue="1.81" metricValueMax="1.9" metricValueMin="1.72" pageId="5" pageNumber="170" unit="mm" value="1.81" valueMax="1.9" valueMin="1.72">1.72–1.90 mm</quantity>
, EL 
<quantity box="[369,534,582,606]" metricMagnitude="-3" metricUnit="m" metricValue="1.74" metricValueMax="1.78" metricValueMin="1.7" pageId="5" pageNumber="170" unit="mm" value="1.74" valueMax="1.78" valueMin="1.7">1.70–1.78 mm</quantity>
, EW 
<quantity box="[596,762,582,606]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" metricValueMax="1.86" metricValueMin="1.74" pageId="5" pageNumber="170" unit="mm" value="1.8" valueMax="1.86" valueMin="1.74">1.74–1.86 mm</quantity>
