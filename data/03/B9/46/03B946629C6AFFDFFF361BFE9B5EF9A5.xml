, HW 
<quantity box="[972,1144,1910,1934]" metricMagnitude="-3" metricUnit="m" metricValue="1.24" metricValueMax="1.26" metricValueMin="1.22" pageId="7" pageNumber="172" unit="mm" value="1.24" valueMax="1.26" valueMin="1.22">1.22–1.26 mm</quantity>
, PL 
<quantity box="[1205,1378,1910,1934]" metricMagnitude="-3" metricUnit="m" metricValue="1.54" metricValueMax="1.6" metricValueMin="1.48" pageId="7" pageNumber="172" unit="mm" value="1.54" valueMax="1.6" valueMin="1.48">1.48–1.60 mm</quantity>
