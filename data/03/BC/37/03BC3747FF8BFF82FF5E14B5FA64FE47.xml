<quantity box="[420,539,445,466]" metricMagnitude="-3" metricUnit="m" metricValue="5.25" metricValueMax="5.6" metricValueMin="4.9" pageId="17" pageNumber="252" unit="mm" value="5.25" valueMax="5.6" valueMin="4.9">4.9–5.6 mm</quantity>
