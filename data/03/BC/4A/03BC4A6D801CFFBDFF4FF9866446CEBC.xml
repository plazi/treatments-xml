</emphasis>
and 
<taxonomicName baseAuthorityName="Stein" baseAuthorityYear="1911" box="[692,809,1866,1892]" class="Insecta" family="Muscidae" genus="Stomopogon" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="312" phylum="Arthropoda" rank="species" species="albiseta">
