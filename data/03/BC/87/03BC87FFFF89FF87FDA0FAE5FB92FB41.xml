<paragraph blockId="13.[189,334,1871,1897]" box="[189,334,1871,1897]" pageId="13" pageNumber="14">
<heading bold="true" box="[189,334,1871,1897]" fontSize="11" level="3" pageId="13" pageNumber="14" reason="3">
<emphasis bold="true" box="[189,334,1871,1897]" pageId="13" pageNumber="14">Description</emphasis>
