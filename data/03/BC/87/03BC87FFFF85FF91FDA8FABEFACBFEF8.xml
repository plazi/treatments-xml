<taxonomicName authority="Bresseel &amp; Constant, 2017" authorityName="Bresseel &amp; Constant" authorityYear="2017" box="[753,890,1621,1647]" class="Insecta" family="Diapheromeridae" genus="Capuyanus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus" status="gen. nov.">
