/ 
<number box="[971,1021,1015,1040]" pageId="4" pageNumber="263" value="0.09">0.09</number>
, hind leg: 
<number box="[1152,1199,1015,1040]" pageId="4" pageNumber="263" value="1.52">1.52</number>
/ 
<number box="[1208,1255,1015,1040]" pageId="4" pageNumber="263" value="1.66">1.66</number>
