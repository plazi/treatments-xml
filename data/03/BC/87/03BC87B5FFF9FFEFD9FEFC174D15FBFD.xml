<subSubSection box="[384,754,1011,1035]" pageId="3" pageNumber="498" type="nomenclature">
<paragraph blockId="3.[384,754,1011,1035]" box="[384,754,1011,1035]" pageId="3" pageNumber="498">
<heading box="[384,754,1011,1035]" centered="true" fontSize="10" level="2" pageId="3" pageNumber="498" reason="3">
Genus 
<taxonomicName authority="Vandel, 1968" authorityName="Vandel" authorityYear="1968" box="[466,754,1011,1035]" class="Malacostraca" family="Philosciidae" genus="Caraiboscia" kingdom="Animalia" order="Isopoda" pageId="3" pageNumber="498" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[466,596,1011,1035]" italics="true" pageId="3" pageNumber="498">Caraiboscia</emphasis>
<bibRefCitation author="VANDEL, A." box="[607,754,1011,1035]" pageId="3" pageNumber="498" pagination="168" refId="ref6017" refString="VANDEL, A., 1968, Isopodes terrestres, in N. and J. Leleup (eds), Mission zoologique belge aux oAles Galapagos et en Ecuador, vol 1, 37 ± 168." type="journal article" year="1968">Vandel, 1968</bibRefCitation>
