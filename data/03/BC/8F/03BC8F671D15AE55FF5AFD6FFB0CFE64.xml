<superScript attach="left" box="[376,391,1651,1664]" fontSize="5" pageId="42" pageNumber="43">73</superScript>
<taxonomicName authority="Taekul &amp; Johnson &amp; Masner &amp; Shu-Pei, 2008" authorityName="Taekul &amp; Johnson &amp; Masner &amp; Shu-Pei" authorityYear="2008" box="[397,613,1654,1677]" class="Insecta" family="Scelionidae" genus="Fusicornia" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="skopelos" status="n. sp.">
