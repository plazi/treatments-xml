<number box="[383,413,1066,1092]" pageId="7" pageNumber="40" value="18.0">18</number>
ɗ 
<number box="[444,459,1066,1092]" pageId="7" pageNumber="40" value="9.0">9</number>
Ψ): 
<emphasis bold="true" box="[508,599,1066,1092]" pageId="7" pageNumber="40">
<collectingCountry box="[508,594,1066,1092]" name="Serbia" pageId="7" pageNumber="40">Serbia</collectingCountry>
:
</emphasis>
mountain Fruška gora (Stari Ledinci, 
<date box="[1073,1207,1066,1092]" pageId="7" pageNumber="40" value="1988-04-19">
<number box="[1073,1143,1066,1092]" pageId="7" pageNumber="40" value="19.04">19.04</number>
. 
<number box="[1146,1207,1066,1092]" pageId="7" pageNumber="40" value="1988.0">1988</number>
</date>
, ɗ, leg. Radnović, 0 
<number box="[166,219,1106,1132]" pageId="7" pageNumber="40" value="1.05">1.05</number>
. 
<number box="[223,285,1106,1132]" pageId="7" pageNumber="40" value="1988.0">1988</number>
, ɗ, leg. Vujić; Glavica, 
<date box="[589,723,1106,1132]" pageId="7" pageNumber="40" value="1989-04-25">
<number box="[589,660,1106,1132]" pageId="7" pageNumber="40" value="25.04">25.04</number>
. 
<number box="[663,723,1106,1132]" pageId="7" pageNumber="40" value="1989.0">1989</number>
</date>
, ɗ, leg. Vujić); mountain Seličevica, 0 
<number box="[1203,1259,1106,1132]" pageId="7" pageNumber="40" value="2.04">2.04</number>
.1989, 6ɗ, leg. Vujić; mountain Kopaonik (Srebrenac, 
<date box="[645,779,1146,1172]" pageId="7" pageNumber="40" value="1987-05-24">
<number box="[645,716,1146,1172]" pageId="7" pageNumber="40" value="24.05">24.05</number>
. 
<number box="[719,779,1146,1172]" pageId="7" pageNumber="40" value="1987.0">1987</number>
</date>
, ɗ, leg. Vujić); marsh Obedska bara (Debela gora, 
<date box="[151,285,1186,1212]" pageId="7" pageNumber="40" value="1990-04-15">
<number box="[151,221,1186,1212]" pageId="7" pageNumber="40" value="15.04">15.04</number>
. 
<number box="[224,285,1186,1212]" pageId="7" pageNumber="40" value="1990.0">1990</number>
</date>
, Ψ, leg. Vujić). 
<emphasis bold="true" box="[473,635,1186,1212]" pageId="7" pageNumber="40">
<collectingCountry box="[473,630,1186,1212]" name="Serbia" pageId="7" pageNumber="40">Montenegro</collectingCountry>
