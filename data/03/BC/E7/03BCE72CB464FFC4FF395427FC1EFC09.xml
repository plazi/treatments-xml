<taxonomicName baseAuthorityName="Kutzing" baseAuthorityYear="1849" box="[1032,1215,893,917]" class="Magnoliopsida" family="Rhizophoraceae" genus="Diatoma" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="217" phylum="Tracheophyta" rank="species" species="liberum">
