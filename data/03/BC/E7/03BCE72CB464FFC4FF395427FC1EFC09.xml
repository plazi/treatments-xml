<taxonomicName box="[1069,1254,929,953]" class="Magnoliopsida" family="Rhizophoraceae" genus="Diatoma" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="217" phylum="Tracheophyta" rank="species" species="linerum">
<emphasis box="[1069,1254,929,953]" italics="true" pageId="2" pageNumber="217">Diatoma linerum</emphasis>
</taxonomicName>
Agardh’ (in 
<treatmentCitation author="Mills, F. W." page="282" pageId="2" pageNumber="217" year="1933">
<bibRefCitation author="Mills, F. W." pageId="2" pageNumber="217" refId="ref4801" refString="Mills, F. W. (1933) An Index to the Genera and Species of the Diatomaceae and their Synonyms. 181 - 1932. Part 4 (At-Bi), Wheldon &amp; Wesley Limited, London." type="book" year="1933">Mills 1933</bibRefCitation>
, p. 282
</treatmentCitation>
, under 
<taxonomicName authorityName="Boyer" authorityYear="1900" baseAuthorityName="J. E. Smith" box="[350,617,965,989]" class="Bacillariophyceae" family="Biddulphiaceae" genus="Biddulphia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Biddulphiales" pageId="2" pageNumber="217" phylum="Ochrophyta" rank="species" species="biddulphiana">
<emphasis box="[350,617,965,989]" italics="true" pageId="2" pageNumber="217">Biddulphia biddulphiana</emphasis>
</taxonomicName>
) 
<superScript attach="none" box="[626,634,963,977]" fontSize="6" pageId="2" pageNumber="217">5</superScript>
