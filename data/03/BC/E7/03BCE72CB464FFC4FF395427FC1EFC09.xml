<treatmentCitation author="Kutzing, F. T." box="[553,985,893,917]" page="137" pageId="2" pageNumber="217" year="1849">
<taxonomicName authority="(Kutzing 1849, p. 137)" baseAuthorityName="Kutzing" baseAuthorityPageNumber="137" baseAuthorityYear="1849" box="[553,985,893,917]" class="Magnoliopsida" family="Rhizophoraceae" genus="Diatoma" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="217" phylum="Tracheophyta" rank="species" species="liberum">
<emphasis box="[553,735,893,917]" italics="true" pageId="2" pageNumber="217">Diatoma liberum</emphasis>
