<taxonomicName authorityName="Boyer" authorityYear="1900" baseAuthorityName="J. E. Smith" box="[696,965,929,953]" class="Bacillariophyceae" family="Biddulphiaceae" genus="Biddulphia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Biddulphiales" pageId="2" pageNumber="217" phylum="Ochrophyta" rank="species" species="biddulphiana">
<emphasis box="[696,965,929,953]" italics="true" pageId="2" pageNumber="217">Biddulphia biddulphiana</emphasis>
</taxonomicName>
