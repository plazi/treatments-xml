<location LSID="urn:lsid:plazi:treatment:03BCE11BB13AFF81FF20FD85FB91FE41:8ECA06D6B124FF81FB92FEA6FBFEFEE1" box="[1052,1144,323,345]" country="Belgium" county="It" municipality="Belgian Federal Office" name="Tervuren" pageId="17" pageNumber="128" stateProvince="Washington">Tervuren</location>
