<taxonomicName authority="(Fabricius, 1775)" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[293,928,1143,1166]" class="Insecta" family="Chrysomelidae" genus="Aspidimorpha" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="672" phylum="Arthropoda" rank="species" species="interrupta" subGenus="Aspidimorpha">
