<collectingMunicipality box="[380,713,1753,1777]" pageId="2" pageNumber="67">northern Rocky Mountains</collectingMunicipality>
, Sierra 
<collectingRegion box="[227,323,1789,1813]" country="United States of America" name="Nevada" pageId="2" pageNumber="67">Nevada</collectingRegion>
