and 
<taxonomicName box="[198,338,1212,1235]" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="2" pageNumber="408" phylum="Chordata" rank="species" species="zanaensis">
