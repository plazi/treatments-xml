<taxonomicName box="[1249,1391,1678,1702]" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="411" phylum="Chordata" rank="species" species="laosensis">
<emphasis box="[1249,1391,1678,1702]" italics="true" pageId="5" pageNumber="411">G. laosensis</emphasis>
</taxonomicName>
, 
<taxonomicName class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="411" phylum="Chordata" rank="species" species="longinema">
<emphasis italics="true" pageId="5" pageNumber="411">G. longinema</emphasis>
</taxonomicName>
, 
<taxonomicName box="[284,419,1714,1738]" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="411" phylum="Chordata" rank="species" species="strabonis">
<emphasis box="[284,419,1714,1738]" italics="true" pageId="5" pageNumber="411">G. strabonis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[433,579,1714,1738]" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="411" phylum="Chordata" rank="species" species="trilineatus">
<emphasis box="[433,579,1714,1738]" italics="true" pageId="5" pageNumber="411">G. trilineatus</emphasis>
</taxonomicName>
