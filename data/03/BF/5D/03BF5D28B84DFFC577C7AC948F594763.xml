<taxonomicName baseAuthorityName="Krober" baseAuthorityYear="1928" box="[1214,1436,943,969]" class="Insecta" family="Therevidae" genus="Argolepida" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="171" phylum="Arthropoda" rank="species" species="rivulosa">
