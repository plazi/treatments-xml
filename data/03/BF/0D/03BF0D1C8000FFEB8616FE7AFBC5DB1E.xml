and 
<taxonomicName box="[750,872,1865,1890]" class="Insecta" family="Asilidae" genus="Machimus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="pastshenkoae">Machimus</taxonomicName>
