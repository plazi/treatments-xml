<taxonomicName authorityName="McCracken &amp; Egbert" authorityYear="1922" box="[1059,1205,1976,2002]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="variegatus">
