<typeStatus box="[378,426,1673,1699]" pageId="79" pageNumber="80">type</typeStatus>
material of both 
<taxonomicName authorityName="McCracken &amp; Egbert" authorityYear="1922" box="[629,841,1672,1698]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="pacificus">
<emphasis box="[629,841,1672,1698]" italics="true" pageId="79" pageNumber="80">Synergus pacificus</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="McCracken &amp; Egbert" authorityYear="1922" box="[901,1027,1672,1698]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="profusus">
<emphasis box="[901,1027,1672,1698]" italics="true" pageId="79" pageNumber="80">S. profusus</emphasis>
</taxonomicName>
