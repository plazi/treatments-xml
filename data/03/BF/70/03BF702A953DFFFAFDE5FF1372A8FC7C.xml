<taxonomicName authorityName="Ashmead" authorityYear="1885" box="[917,1076,320,347]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="obtusilobae">
