<emphasis bold="true" box="[199,345,546,572]" pageId="73" pageNumber="74">Distribution</emphasis>
. 
<collectingCountry box="[359,419,547,573]" name="United States of America" pageId="73" pageNumber="74">USA</collectingCountry>
: 
<collectingRegion box="[430,511,547,573]" country="United States of America" name="Florida" pageId="73" pageNumber="74">Florida</collectingRegion>
(
<bibRefCitation author="Ashmead, W. H." box="[526,699,546,573]" pageId="73" pageNumber="74" pagination="291 - 304" refId="ref101510" refString="Ashmead, W. H. (1885) A bibliographical and synonymical catalogue of the North American Cynipidae, with description of new species. Transactions of the American Entomological Society, 12 (3 - 4), 291 - 304. https: // doi. org / 10.2307 / 25076460" type="journal article" year="1885">Ashmead 1885</bibRefCitation>
), 
<collectingRegion box="[719,813,547,573]" country="United States of America" name="Virginia" pageId="73" pageNumber="74">Virginia</collectingRegion>
, 
<collectingRegion box="[824,1018,547,573]" country="United States of America" name="District of Columbia" pageId="73" pageNumber="74">Washington D.C.</collectingRegion>
, and 
<collectingRegion box="[1078,1178,547,573]" country="United States of America" name="Missouri" pageId="73" pageNumber="74">Missouri</collectingRegion>
