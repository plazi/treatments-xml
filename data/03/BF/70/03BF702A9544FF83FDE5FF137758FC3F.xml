<subSubSection pageId="65" pageNumber="66" type="biology_ecology">
<paragraph blockId="65.[151,1436,381,588]" pageId="65" pageNumber="66">
<emphasis bold="true" box="[199,289,489,515]" pageId="65" pageNumber="66">Biology</emphasis>
