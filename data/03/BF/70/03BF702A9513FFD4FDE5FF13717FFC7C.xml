<subSubSection pageId="22" pageNumber="23" type="biology_ecology">
<paragraph blockId="22.[151,1437,320,527]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[199,295,392,418]" pageId="22" pageNumber="23">Biology.</emphasis>
