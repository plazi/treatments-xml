(
<bibRefCitation author="Harris, T. W." box="[827,971,1229,1255]" pageId="57" pageNumber="58" refId="ref103050" refString="Harris, T. W. (1841) A report on the insects of Massachusetts, injurious to vegetation. Folsom, Wells, and Thurston, Printers to the University, Cambridge, Massachusetts, 459 pp." type="book" year="1841">Harris, 1841</bibRefCitation>
)
</taxonomicName>
on 
<taxonomicName authority="(Osten-Sacken 1861)" baseAuthorityName="Osten-Sacken" baseAuthorityYear="1861" box="[1022,1370,1229,1256]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="rubra">
<emphasis box="[1022,1121,1229,1255]" italics="true" pageId="57" pageNumber="58">Q. rubra</emphasis>
