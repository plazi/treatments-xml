(
<bibRefCitation author="Osten-Sacken, C. R. von" box="[606,837,1265,1292]" pageId="57" pageNumber="58" pagination="241 - 259" refId="ref105085" refString="Osten-Sacken, C. R. von (1862) Additions and corrections to the paper ' On the Cynipidae of the North American oaks and their galls'. Proceedings of the Entomological Society of Philadelphia, 1 (8), 241 - 259." type="journal article" year="1862">Osten-Sacken, 1862</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Osten-Sacken, 1861)" baseAuthorityName="Osten-Sacken" baseAuthorityYear="1861" box="[855,1401,1265,1292]" class="Insecta" family="Cynipidae" genus="Atrusca" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="quercuscentricola">
<emphasis box="[855,1150,1265,1291]" italics="true" pageId="57" pageNumber="58">Atrusca quercuscentricola</emphasis>
(
<bibRefCitation author="Osten-Sacken, C. R. von" box="[1164,1393,1265,1292]" pageId="57" pageNumber="58" pagination="47 - 72" refId="ref105046" refString="Osten-Sacken, C. R. von (1861) On the Cynipidae of the North American oaks and their galls. Proceedings of the Entomological Society of Philadelphia, 1 (3), 47 - 72." type="journal article" year="1861">Osten-Sacken, 1861</bibRefCitation>
)
</taxonomicName>
