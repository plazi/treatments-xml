<paragraph blockId="7.[151,1437,591,2036]" pageId="7" pageNumber="25">
<emphasis bold="true" box="[198,290,778,804]" pageId="7" pageNumber="25">Female</emphasis>
(n = 
<number box="[359,376,778,804]" pageId="7" pageNumber="25" value="5.0">5</number>
). Body fusiform, 
<number box="[591,636,778,804]" pageId="7" pageNumber="25" value="201.0">201</number>
(
<number box="[644,751,778,804]" pageId="7" pageNumber="25" value="206.0" valueMax="218.0" valueMin="194.0">194–218</number>
), 
<number box="[774,802,778,804]" pageId="7" pageNumber="25" value="61.0">61</number>
(
<number box="[811,888,778,804]" pageId="7" pageNumber="25" value="58.5" valueMax="62.0" valueMin="55.0">55–62</number>
) wide. 
<emphasis bold="true" box="[980,1140,778,804]" pageId="7" pageNumber="25">Gnathosoma</emphasis>
downcurved, apical seta (
<emphasis box="[161,176,816,841]" italics="true" pageId="7" pageNumber="25">d</emphasis>
) 
<number box="[195,211,816,842]" pageId="7" pageNumber="25" value="3.0">3</number>
(
<number box="[221,237,816,842]" pageId="7" pageNumber="25" value="3.0">3</number>
); chelicerae 
<number box="[395,427,816,842]" pageId="7" pageNumber="25" value="12.0">12</number>
(
<number box="[436,515,816,842]" pageId="7" pageNumber="25" value="12.5" valueMax="14.0" valueMin="11.0">11–14</number>
). 
<emphasis bold="true" box="[537,749,815,841]" pageId="7" pageNumber="25">Prodorsal shield</emphasis>
<number box="[758,790,816,842]" pageId="7" pageNumber="25" value="34.0">34</number>
(
<number box="[799,879,816,842]" pageId="7" pageNumber="25" value="35.5" valueMax="37.0" valueMin="34.0">34–37</number>
