........................................................................................... 
<taxonomicName box="[1310,1435,445,466]" class="Insecta" family="Curculionidae" genus="Conotrachelus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="136" phylum="Arthropoda" rank="species" species="posticatus">
