<emphasis bold="true" box="[201,437,619,644]" pageId="5" pageNumber="59">Material examined.</emphasis>
<typeStatus box="[446,553,620,644]" pageId="5" pageNumber="59">Holotype</typeStatus>
, female on card, 
<collectingCountry box="[754,836,620,644]" name="India" pageId="5" pageNumber="59">INDIA</collectingCountry>
, Maharashtra, 
<collectingCountry box="[1006,1104,620,644]" name="India" pageId="5" pageNumber="59">Mumbai</collectingCountry>
, Borivli, 
<date box="[1214,1326,620,644]" pageId="5" pageNumber="59" value="2011-10-11">
<number box="[1214,1240,620,644]" pageId="5" pageNumber="59" value="11.0">11</number>
.x. 
<number box="[1263,1326,620,644]" pageId="5" pageNumber="59" value="2011.0">2011</number>
