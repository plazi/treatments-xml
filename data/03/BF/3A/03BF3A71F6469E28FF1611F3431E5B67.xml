<emphasis box="[201,321,1807,1830]" italics="true" pageId="1" pageNumber="55">Mesosoma</emphasis>
</taxonomicName>
. 
<taxonomicName box="[334,455,1806,1829]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="1" pageNumber="55" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
length = 
<quantity box="[562,728,1805,1829]" metricMagnitude="-4" metricUnit="m" metricValue="6.15" metricValueMax="6.2" metricValueMin="6.1" pageId="1" pageNumber="55" unit="mm" value="0.615" valueMax="0.62" valueMin="0.61">0.61–0.62 mm</quantity>
; 
