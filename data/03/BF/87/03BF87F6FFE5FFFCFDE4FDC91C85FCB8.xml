<taxonomicName authorityName="Hansson" authorityYear="2020" box="[374,483,1653,1676]" class="Insecta" family="Eulophidae" genus="Lasalleistichus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="623" phylum="Arthropoda" rank="species" species="albiclava">
