, Dc 
<number box="[493,503,950,967]" pageId="50" pageNumber="51" value="2.0">2</number>
–Dc 
<number box="[550,560,950,967]" pageId="50" pageNumber="51" value="3.0">3</number>
<number box="[567,656,937,963]" pageId="50" pageNumber="51" value="96.0" valueMax="109.0" valueMin="83.0">83–109</number>
, Dc 
<number box="[702,713,949,968]" pageId="50" pageNumber="51" value="3.0">3</number>
–Dc 
<date box="[761,854,937,968]" pageId="50" pageNumber="51" value="1941-04-28">
