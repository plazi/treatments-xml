<geoCoordinate box="[551,630,1018,1042]" degrees="1.72" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-1.72">w 1.72</geoCoordinate>
; gaster l/mss l 1.10; Gt 
<subScript attach="left" box="[881,891,1029,1045]" fontSize="7" pageId="10" pageNumber="630">7</subScript>
l/ 
<geoCoordinate box="[910,989,1019,1043]" degrees="0.53" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-0.53">w 0.53</geoCoordinate>
