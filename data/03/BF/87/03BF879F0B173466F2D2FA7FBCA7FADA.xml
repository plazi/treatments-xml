<taxonomicName box="[345,459,1230,1252]" class="Insecta" family="Chloropidae" genus="Togeciphus" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="24" phylum="Arthropoda" rank="genus">Togeciphus</taxonomicName>
