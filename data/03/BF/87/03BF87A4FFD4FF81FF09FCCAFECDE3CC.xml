<taxonomicName authority="Lea, 1918" authorityName="Lea" authorityYear="1918" box="[709,969,1744,1766]" class="Insecta" family="Cerambycidae" genus="Potemnemus" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="518" phylum="Arthropoda" rank="species" species="trimaculatus">
