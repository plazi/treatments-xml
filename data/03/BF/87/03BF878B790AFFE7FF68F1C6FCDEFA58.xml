, 
<collectingCountry box="[319,420,1421,1443]" name="Mongolia" pageId="1" pageNumber="478">Mongolia</collectingCountry>
, and 
<collectingCountry box="[481,595,1421,1443]" name="Uzbekistan" pageId="1" pageNumber="478">Uzbekistan</collectingCountry>
(
<bibRefCitation author="SMETANA A" box="[613,778,1421,1443]" pageId="1" pageNumber="478" pagination="687 - 698" refId="ref3773" refString="SMETANA A. (2004): Staphylinidae, tribe Xantholinini ERICHSON, 1839, pp. 687 - 698. - In: LOBL I. &amp; A. SMETANA (eds): Catalogue of Palaearctic Coleoptera. II. Hydrophiloidea- Histeroidea-Staphylinoidea. - Apollo Books, Stenstrup: 942 pp." type="book chapter" year="2004">SMETANA 2004</bibRefCitation>
). The above specimens from 
<collectingCountry box="[178,296,1450,1472]" name="Kazakhstan" pageId="1" pageNumber="478">Kazakhstan</collectingCountry>
