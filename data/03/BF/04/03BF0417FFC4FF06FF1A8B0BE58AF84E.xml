<emphasis bold="true" box="[1317,1352,1466,1490]" pageId="23" pageNumber="24">md</emphasis>
, mandible; 
<emphasis bold="true" box="[196,242,1502,1526]" pageId="23" pageNumber="24">mx1</emphasis>
, maxilla 1 with 
<emphasis bold="true" box="[407,434,1502,1526]" pageId="23" pageNumber="24">op</emphasis>
, outer plate and 
<emphasis bold="true" box="[604,639,1502,1526]" pageId="23" pageNumber="24">plp</emphasis>
