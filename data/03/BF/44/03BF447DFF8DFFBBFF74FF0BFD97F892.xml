<taxonomicName authority="Páll-Gergely &amp; Grego &amp; Vermeulen &amp; Reischütz &amp; Hunyadi &amp; Jochum, 2019" authorityName="Páll-Gergely &amp; Grego &amp; Vermeulen &amp; Reischütz &amp; Hunyadi &amp; Jochum" authorityYear="2019" box="[528,775,1822,1846]" class="Gastropoda" family="Gastrocoptidae" genus="Tonkinospira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="16" pageNumber="533" phylum="Mollusca" rank="species" species="suturata" status="new species">
<emphasis box="[528,775,1822,1846]" italics="true" pageId="16" pageNumber="533">Tonkinospira suturata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[136,269,1854,1878]" pageId="16" pageNumber="533" rank="species">new species</taxonomicNameLabel>
, and 
<taxonomicName authority="Páll-Gergely &amp; Grego &amp; Vermeulen &amp; Reischütz &amp; Hunyadi &amp; Jochum, 2019" authorityName="Páll-Gergely &amp; Grego &amp; Vermeulen &amp; Reischütz &amp; Hunyadi &amp; Jochum" authorityYear="2019" box="[331,481,1854,1878]" class="Gastropoda" family="Gastrocoptidae" genus="Tonkinospira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="16" pageNumber="533" phylum="Mollusca" rank="species" species="triangulata" status="new species">
