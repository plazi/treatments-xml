<taxonomicName authorityName="Jochum, Slapnik &amp; Pall-Gergely" authorityYear="2014" box="[968,1110,1104,1128]" class="Gastropoda" family="Gastrocoptidae" genus="Tonkinospira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="1" pageNumber="518" phylum="Mollusca" rank="genus">
