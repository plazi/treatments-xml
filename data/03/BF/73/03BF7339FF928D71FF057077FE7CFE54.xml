<taxonomicName authorityName="Mound &amp; Tree" authorityYear="2014" box="[1302,1389,439,464]" class="Insecta" family="Phlaeothripidae" genus="Holoengythrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="16" pageNumber="141" phylum="Arthropoda" rank="species" species="kathyae">
<emphasis box="[1302,1389,439,464]" italics="true" pageId="16" pageNumber="141">kathyae</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Mound &amp; Tree" authorityYear="2014" box="[151,236,477,500]" class="Insecta" family="Phlaeothripidae" genus="Holoengythrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="16" pageNumber="141" phylum="Arthropoda" rank="species" species="turcoae">
<emphasis box="[151,236,477,500]" italics="true" pageId="16" pageNumber="141">turcoae</emphasis>
</taxonomicName>
from eastern 
<collectingCountry box="[390,496,476,501]" name="Australia" pageId="16" pageNumber="141">Australia</collectingCountry>
.
</paragraph>
