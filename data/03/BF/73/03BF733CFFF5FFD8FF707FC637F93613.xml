<date box="[1001,1184,1109,1133]" pageId="14" pageNumber="15" value="1824-07-05" valueMax="1824-07-24" valueMin="1824-07-05">
<number box="[1001,1057,1109,1133]" pageId="14" pageNumber="15" value="14.5" valueMax="24.0" valueMin="5.0">5–24</number>
July 
<number box="[1125,1184,1109,1133]" pageId="14" pageNumber="15" value="1824.0">1824</number>
</date>
] in “ 
<collectingCountry box="[1243,1306,1109,1133]" name="Syria" pageId="14" pageNumber="15">Syria</collectingCountry>
” (label) [= Bcharré, 
<collectingCountry box="[253,354,1145,1169]" name="Lebanon" pageId="14" pageNumber="15">Lebanon</collectingCountry>
] (
<bibRefCitation author="Dekker" box="[375,702,1145,1169]" pageId="14" pageNumber="15" refString="Dekker, R. W. R. J. &amp; Quaisser, C. (2006) Type specimens of birds in the National Museum of Natural History, Leiden. Part 3. Passerines: Pachycephalidae-Corvidae (Peters's sequence). Nationaal Natuurhistorisch Museum Technical Bulletin, 9, 1 - 77." type="journal article" year="2006">
