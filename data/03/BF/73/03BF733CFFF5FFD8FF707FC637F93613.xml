<collectionCode LSID="urn:lsid:biocol.org:col:34992" box="[311,395,1110,1133]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="14" pageNumber="15">RMNH</collectionCode>
