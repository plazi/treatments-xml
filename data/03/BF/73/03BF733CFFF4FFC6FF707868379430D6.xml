<number box="[555,614,764,788]" pageId="16" pageNumber="17" value="1929.0">1929</number>
. 
<taxonomicName authority="Hemprich &amp; Ehrenberg, 1829" authorityName="Hemprich &amp; Ehrenberg" authorityYear="1829" box="[627,1095,764,788]" class="Aves" family="Corvidae" genus="Corvus" kingdom="Animalia" order="Passeriformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="stridens">
