<taxonomicName authorityName="Porteri Brolemann" authorityYear="1916" box="[1047,1164,866,888]" class="Diplopoda" family="Dalodesmidae" genus="Chiliosoma" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="515" phylum="Arthropoda" rank="genus">
