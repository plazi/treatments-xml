<taxonomicName authorityName="Demange &amp; Silva" authorityYear="1976" box="[595,720,836,858]" class="Diplopoda" family="Paradoxosomatidae" genus="Anaulacodesmus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="515" phylum="Arthropoda" rank="species" species="intricatus">
