<taxonomicName authorityName="Demange &amp; Silva" authorityYear="1976" box="[1228,1328,866,888]" class="Diplopoda" family="Paradoxosomatidae" genus="Anaulacodesmus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="515" phylum="Arthropoda" rank="species" species="atlanticus">
