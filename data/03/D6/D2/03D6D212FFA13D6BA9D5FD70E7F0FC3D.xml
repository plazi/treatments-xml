<paragraph blockId="8.[151,1437,805,981]" pageId="8" pageNumber="515">
<taxonomicName authorityName="Schubart" authorityYear="1954" box="[199,299,836,858]" class="Diplopoda" family="Paradoxosomatidae" genus="Anaulacodesmus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="515" phylum="Arthropoda" rank="species" species="atlanticus">
