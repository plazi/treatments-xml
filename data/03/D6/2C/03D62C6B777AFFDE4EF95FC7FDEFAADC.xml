<collectingCountry box="[514,603,850,876]" name="Panama" pageId="25" pageNumber="26">Panama</collectingCountry>
(
<collectingCountry box="[619,661,849,875]" name="Iceland" pageId="25" pageNumber="26">Isla</collectingCountry>
Venao = formerly 
<collectingCountry box="[881,921,849,875]" name="Iceland" pageId="25" pageNumber="26">Isla</collectingCountry>
