<collectingCountry box="[787,930,1399,1425]" name="Sint Martin" pageId="31" pageNumber="32">Saint Martin</collectingCountry>
, 
<collectingCountry box="[941,1076,1399,1425]" name="Guadeloupe" pageId="31" pageNumber="32">Guadeloupe</collectingCountry>
and 
<collectingCountry box="[1130,1249,1399,1425]" name="Venezuela" pageId="31" pageNumber="32">Venezuela</collectingCountry>
.
