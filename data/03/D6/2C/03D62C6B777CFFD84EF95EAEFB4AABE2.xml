, 
<collectingCountry box="[949,1038,752,778]" name="Panama" pageId="31" pageNumber="32">Panama</collectingCountry>
(
<collectingRegion box="[1054,1230,751,778]" country="Panama" name="Bocas del Toro" pageId="31" pageNumber="32">Bocas del Toro</collectingRegion>
, Almirante, Bahía Limón / 
