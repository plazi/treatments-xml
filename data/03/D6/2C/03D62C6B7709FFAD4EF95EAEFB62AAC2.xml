<subSubSection pageId="106" pageNumber="107" type="distribution">
<paragraph blockId="106.[151,1437,417,1201]" pageId="106" pageNumber="107">
<emphasis bold="true" box="[199,345,777,803]" pageId="106" pageNumber="107">Distribution</emphasis>
. East Pacific: 
<collectingCountry box="[514,649,778,804]" name="El Salvador" pageId="106" pageNumber="107">El Salvador</collectingCountry>
, 
<collectingCountry box="[663,781,778,804]" name="Nicaragua" pageId="106" pageNumber="107">Nicaragua</collectingCountry>
, 
<collectingCountry box="[794,921,778,804]" name="Costa Rica" pageId="106" pageNumber="107">Costa Rica</collectingCountry>
, 
<collectingCountry box="[935,1024,778,804]" name="Panama" pageId="106" pageNumber="107">Panama</collectingCountry>
(Coiba Is., Río Mar, 
<collectingCountry box="[1273,1362,778,804]" name="Panama" pageId="106" pageNumber="107">Panama</collectingCountry>
Canal area, 
<collectingRegion box="[212,425,814,840]" country="Panama" name="Panama" pageId="106" pageNumber="107">Ciudad de Panamá</collectingRegion>
, Taboga Is., Las Perlas Is.) and 
<collectingCountry box="[779,872,814,840]" name="Ecuador" pageId="106" pageNumber="107">Ecuador</collectingCountry>
(
<bibRefCitation author="Haig, J." box="[887,1009,814,840]" pageId="106" pageNumber="107" pagination="1 - 440" refId="ref72816" refString="Haig, J. (1960) The Porcellanidae (Crustacea: Anomura) of the Eastern Pacific. Allan Hancock Pacific Expedition, 24, 1 - 440." type="journal article" year="1960">Haig 1960</bibRefCitation>
; 
<bibRefCitation author="Abele, L. G. &amp; Kim, W." box="[1020,1238,814,840]" pageId="106" pageNumber="107" pagination="1 - 50" refId="ref69810" refString="Abele, L. G. &amp; Kim, W. (1989) The decapod crustaceans of the Panama Canal. Smithsonian Contributions to Zoology, 482, 1 - 50. https: // doi. org / 10.5479 / si. 00810282.482" type="journal article" year="1989">Abele &amp; Kim 1989</bibRefCitation>
