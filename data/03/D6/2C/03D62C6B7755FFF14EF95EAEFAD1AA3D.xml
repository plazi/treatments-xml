<quantity box="[968,1042,789,815]" metricMagnitude="0" metricUnit="m" metricValue="4.0" metricValueMax="8.0" metricValueMin="0.0" pageId="54" pageNumber="55" unit="m" value="4.0" valueMax="8.0" valueMin="0.0">0–8 m</quantity>
