<collectingCountry box="[606,739,860,886]" name="El Salvador" pageId="46" pageNumber="47">El Salvador</collectingCountry>
, 
<collectingCountry box="[751,875,860,886]" name="Costa Rica" pageId="46" pageNumber="47">Costa Rica</collectingCountry>
, 
<collectingCountry box="[887,976,861,887]" name="Panama" pageId="46" pageNumber="47">Panama</collectingCountry>
(Isla Venao, 
<collectingCountry box="[1122,1211,861,887]" name="Panama" pageId="46" pageNumber="47">Panama</collectingCountry>
Canal area, vicinity of 
<collectingRegion box="[182,396,896,922]" country="Panama" name="Panama" pageId="46" pageNumber="47">Ciudad de Panamá</collectingRegion>
, including Amador Causeway and “Recife de Casa de Putas”), 
<collectingCountry box="[1105,1215,896,922]" name="Colombia" pageId="46" pageNumber="47">Colombia</collectingCountry>
