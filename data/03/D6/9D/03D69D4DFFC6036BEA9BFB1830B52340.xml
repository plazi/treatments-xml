). 
<collectingCountry box="[493,621,482,506]" name="Argentina" pageId="13" pageNumber="12">Argentina</collectingCountry>
, provincias de 
<collectingRegion box="[803,917,482,506]" country="Argentina" name="Mendoza" pageId="13" pageNumber="12">Mendoza</collectingRegion>
