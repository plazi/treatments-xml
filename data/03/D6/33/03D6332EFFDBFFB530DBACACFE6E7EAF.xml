<number box="[1291,1319,1403,1427]" pageId="5" pageNumber="270" value="41.0">41</number>
(
<number box="[1334,1409,1403,1427]" pageId="5" pageNumber="270" value="41.5" valueMax="42.0" valueMin="41.0">41–42</number>
