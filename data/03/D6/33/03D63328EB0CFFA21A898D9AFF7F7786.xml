<taxonomicName authorityName="Gozis" authorityYear="1882" class="Insecta" family="Nemonychidae" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="592" phylum="Arthropoda" rank="subFamily" subFamily="Cimberidinae">Cimberidinae</taxonomicName>
