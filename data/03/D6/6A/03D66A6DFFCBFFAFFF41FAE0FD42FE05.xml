<number box="[1160,1215,272,298]" pageId="6" pageNumber="63" value="0.05">0.05</number>
; AME-AME 
<number box="[1377,1431,272,298]" pageId="6" pageNumber="63" value="0.12">0.12</number>
, AME-ALE 
<number box="[294,347,312,338]" pageId="6" pageNumber="63" value="0.11">0.11</number>
, PME-PME 
<number box="[503,557,312,338]" pageId="6" pageNumber="63" value="0.2">0.20</number>
, PME-PLE 
<number box="[706,760,312,338]" pageId="6" pageNumber="63" value="0.25">0.25</number>
, MOA long 
<number box="[917,968,312,338]" pageId="6" pageNumber="63" value="0.35">0.35</number>
with front width 
<number box="[1188,1240,312,338]" pageId="6" pageNumber="63" value="0.3">0.30</number>
and back width 
<number box="[151,205,352,378]" pageId="6" pageNumber="63" value="0.32">0.32</number>
