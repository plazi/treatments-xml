<typeStatus box="[264,316,1860,1885]" pageId="1" pageNumber="2">Type</typeStatus>
species
</emphasis>
. 
<emphasis box="[433,835,1860,1885]" italics="true" pageId="1" pageNumber="2">
<taxonomicName box="[433,624,1860,1885]" class="Malacostraca" family="Paguridae" genus="Catapaguroides" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">Catapaguroides</taxonomicName>
