<taxonomicName authority="David" authorityName="David" box="[744,1064,1028,1050]" class="Insecta" family="Tephritidae" genus="Acroceratitis" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="416" phylum="Arthropoda" rank="species" species="breviscapa">
