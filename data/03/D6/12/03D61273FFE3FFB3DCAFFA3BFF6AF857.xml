<collectionCode LSID="urn:lsid:biocol.org:col:13589" box="[1274,1328,1974,1997]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13589" name="Ministry of Natural Resources" pageId="2" pageNumber="35">ENT</collectionCode>
<number box="[1343,1430,1973,1997]" pageId="2" pageNumber="35" value="305108.0">305108</number>
