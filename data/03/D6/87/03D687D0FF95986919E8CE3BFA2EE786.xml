<emphasis box="[493,588,1650,1671]" italics="true" pageId="51" pageNumber="352">Juniperus</emphasis>
</taxonomicName>
............................................................ 
<taxonomicName baseAuthorityName="Vayssiere" baseAuthorityYear="1923" box="[1310,1436,1648,1670]" class="Insecta" family="Pseudococcidae" genus="Puto" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="352" phylum="Arthropoda" rank="species" species="peyerimhoffi">
<emphasis bold="true" box="[1310,1436,1648,1670]" italics="true" pageId="51" pageNumber="352">peyerimhoffi</emphasis>
