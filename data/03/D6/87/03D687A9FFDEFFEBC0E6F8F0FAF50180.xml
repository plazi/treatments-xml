<date pageId="37" pageNumber="38" value="1908-06-15">
<number box="[1346,1372,1724,1750]" pageId="37" pageNumber="38" value="15.0">15</number>
–VI– 
<number box="[151,213,1764,1790]" pageId="37" pageNumber="38" value="1908.0">1908</number>
</date>
. The 
<collectionCode box="[287,364,1764,1790]" pageId="37" pageNumber="38">CASC</collectionCode>
