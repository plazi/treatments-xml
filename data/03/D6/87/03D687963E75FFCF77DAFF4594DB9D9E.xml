<keyStep lastPageId="6" lastPageNumber="383" pageId="5" pageNumber="382">
<paragraph blockId="5.[151,1437,1747,1827]" pageId="5" pageNumber="382">
<keyLead pageId="5" pageNumber="382">
<number box="[151,165,1747,1768]" pageId="5" pageNumber="382" value="6.0">6</number>
