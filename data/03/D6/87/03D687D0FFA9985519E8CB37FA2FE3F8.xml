<taxonomicName class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="15" pageNumber="316" phylum="Tracheophyta" rank="species" species="pinaster">
<emphasis italics="true" pageId="15" pageNumber="316">P. pinaster</emphasis>
</taxonomicName>
................................................................................. 
<taxonomicName authorityName="Ducasse" authorityYear="1941" box="[1278,1358,680,702]" class="Insecta" family="Margarodidae" genus="Matsucoccus" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="feytaudi">
