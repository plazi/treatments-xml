<number box="[918,969,834,857]" pageId="3" pageNumber="68" value="4.06">4.06</number>
+ 
<number box="[984,1031,834,857]" pageId="3" pageNumber="68" value="1.65">1.65</number>
), III 
<number box="[1085,1133,834,857]" pageId="3" pageNumber="68" value="9.27">9.27</number>
(
<number box="[1147,1198,834,857]" pageId="3" pageNumber="68" value="2.7">2.70</number>
+ 
<number box="[1213,1263,834,857]" pageId="3" pageNumber="68" value="2.48">2.48</number>
+ 
<number box="[1278,1326,834,857]" pageId="3" pageNumber="68" value="2.91">2.91</number>
+ 
<number box="[1341,1388,834,857]" pageId="3" pageNumber="68" value="1.18">1.18</number>
), IV 
<number box="[151,212,866,889]" pageId="3" pageNumber="68" value="12.91">12.91</number>
(
<number box="[227,277,866,889]" pageId="3" pageNumber="68" value="3.7">3.70</number>
