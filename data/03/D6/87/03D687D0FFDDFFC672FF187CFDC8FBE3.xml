+ 
<number box="[619,668,834,857]" pageId="3" pageNumber="68" value="2.09">2.09</number>
), II 
<number box="[713,774,834,857]" pageId="3" pageNumber="68" value="13.36">13.36</number>
(
<number box="[789,838,834,857]" pageId="3" pageNumber="68" value="3.85">3.85</number>
+ 
<number box="[853,903,834,857]" pageId="3" pageNumber="68" value="3.8">3.80</number>
