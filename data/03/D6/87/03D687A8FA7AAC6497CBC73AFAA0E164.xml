species (
<taxonomicName box="[963,1070,727,752]" class="Insecta" family="Vespidae" kingdom="Animalia" order="Hymenoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="family">Vespidae</taxonomicName>
: 
<taxonomicName box="[1080,1208,727,752]" class="Insecta" family="Vespidae" kingdom="Animalia" order="Hymenoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="subFamily" subFamily="Eumeninae">Eumeninae</taxonomicName>
) (label records).
</paragraph>
