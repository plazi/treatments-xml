<subSubSection pageId="2" pageNumber="285" type="distribution">
<paragraph blockId="2.[151,1437,319,1065]" pageId="2" pageNumber="285">
<emphasis bold="true" box="[199,352,679,704]" pageId="2" pageNumber="285">Distribution.</emphasis>
Nearctic (
<collectingCountry box="[476,562,679,704]" name="Canada" pageId="2" pageNumber="285">Canada</collectingCountry>
and 
<collectingCountry box="[624,920,679,704]" name="United States of America" pageId="2" pageNumber="285">United States of America</collectingCountry>
) and Palaearctic (
<collectingCountry box="[1132,1233,679,704]" name="Belgium" pageId="2" pageNumber="285">Belgium</collectingCountry>
, 
<collectingCountry box="[1247,1432,679,704]" name="Czech Republic" pageId="2" pageNumber="285">Czech Republic</collectingCountry>
, 
<collectingCountry box="[151,331,716,741]" name="Czech Republic" pageId="2" pageNumber="285">Czechoslovakia</collectingCountry>
, 
<collectingCountry box="[348,437,716,741]" name="Finland" pageId="2" pageNumber="285">Finland</collectingCountry>
, 
<collectingCountry box="[454,533,717,740]" name="France" pageId="2" pageNumber="285">France</collectingCountry>
, 
<collectingCountry box="[551,656,716,740]" name="Germany" pageId="2" pageNumber="285">Germany</collectingCountry>
, 
<collectingCountry box="[673,748,716,741]" name="Latvia" pageId="2" pageNumber="285">Latvia</collectingCountry>
, 
<collectingCountry box="[765,903,716,741]" name="Netherlands" pageId="2" pageNumber="285">Netherlands</collectingCountry>
, 
<collectingCountry box="[921,1002,716,741]" name="Poland" pageId="2" pageNumber="285">Poland</collectingCountry>
, 
<collectingCountry box="[1018,1095,716,741]" name="Russia" pageId="2" pageNumber="285">Russia</collectingCountry>
, 
<collectingCountry box="[1114,1181,716,741]" name="Spain" pageId="2" pageNumber="285">Spain</collectingCountry>
, 
<collectingCountry box="[1197,1289,716,741]" name="Sweden" pageId="2" pageNumber="285">Sweden</collectingCountry>
, and 
<collectingCountry name="United Kingdom" pageId="2" pageNumber="285">United Kingdom</collectingCountry>
