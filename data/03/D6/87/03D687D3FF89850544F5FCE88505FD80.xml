), North and central 
<collectingCountry box="[201,412,1262,1287]" isEnumeration="true" pageId="1" pageNumber="302">Thailand, Vietnam</collectingCountry>
, 
<collectingCountry box="[423,549,1262,1287]" name="Philippines" pageId="1" pageNumber="302">Philippines</collectingCountry>
(
<location LSID="urn:lsid:plazi:treatment:03D687D3FF89850544F5FCE88505FD80:8EA0601EFF8985064656FB3983CEFABA" box="[564,654,1262,1287]" country="Cambodia" municipality="Xiang Khoang" name="Visayas" pageId="1" pageNumber="302" stateProvince="Perak">Visayas</location>
