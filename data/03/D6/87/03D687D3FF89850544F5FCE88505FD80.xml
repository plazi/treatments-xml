</collectingDate>
, 5)— 
<collectingCountry box="[833,953,1226,1251]" name="Cambodia" pageId="1" pageNumber="302">Cambodia</collectingCountry>
, 
<collectingCountry box="[964,1021,1227,1251]" name="Laos" pageId="1" pageNumber="302">Laos</collectingCountry>
