<date box="[791,921,1053,1078]" pageId="21" pageNumber="22" value="2004" valueMax="2005">
<number box="[791,921,1053,1078]" pageId="21" pageNumber="22" value="2004.5" valueMax="2005.0" valueMin="2004.0">2004–2005</number>
</date>
