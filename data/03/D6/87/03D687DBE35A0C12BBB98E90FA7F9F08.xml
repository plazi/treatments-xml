<quantity box="[151,302,1583,1608]" metricMagnitude="-3" metricUnit="m" metricValue="1.4249999999999998" metricValueMax="1.45" metricValueMin="1.4" pageId="1" pageNumber="439" unit="mm" value="1.4249999999999998" valueMax="1.45" valueMin="1.4">1.4–1.45 mm</quantity>
