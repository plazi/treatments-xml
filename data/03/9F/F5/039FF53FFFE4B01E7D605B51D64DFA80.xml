<paragraph blockId="4.[813,1420,1673,1766]" pageId="4" pageNumber="331">
<emphasis bold="true" box="[813,1037,1673,1697]" pageId="4" pageNumber="331">Material examined.</emphasis>
<specimenCount box="[1042,1125,1673,1697]" pageId="4" pageNumber="331" type="male">2 males</specimenCount>
and 
