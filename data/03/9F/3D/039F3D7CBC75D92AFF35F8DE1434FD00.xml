<number box="[625,714,332,356]" pageId="9" pageNumber="84" value="2.25" valueMax="2.3" valueMin="2.2">2.2−2.3</number>
; length from apex of clypeus to cuneal fracture 
<number box="[1244,1362,332,356]" pageId="9" pageNumber="84" value="1.7999999999999998" valueMax="1.9" valueMin="1.7">1.70−1.90</number>
; width of head across eyes 
<number box="[384,502,367,392]" pageId="9" pageNumber="84" value="0.76" valueMax="0.77" valueMin="0.75">0.75−0.77</number>
; head height 
