<quantity box="[355,487,1338,1362]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="2.8" metricValueMin="2.7" pageId="54" pageNumber="355" unit="mm" value="2.75" valueMax="2.8" valueMin="2.7">2.7−2.8mm</quantity>
