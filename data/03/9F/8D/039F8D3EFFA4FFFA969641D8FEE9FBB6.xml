</taxonomicName>
-group, 
<taxonomicName box="[648,796,1800,1822]" class="Insecta" family="Scirtidae" genus="Nothocyphon" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="327" phylum="Arthropoda" rank="species" species="vandiemeni">
