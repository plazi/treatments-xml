<taxonomicName authorityName="Navas" authorityYear="1929" box="[1248,1372,752,778]" class="Insecta" family="Berothidae" genus="Lekrugeria" kingdom="Animalia" order="Neuroptera" pageId="8" pageNumber="517" phylum="Arthropoda" rank="genus">
