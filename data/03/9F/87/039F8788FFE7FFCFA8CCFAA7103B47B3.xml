: 
<taxonomicName box="[309,495,1874,1900]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="orientalis">
<emphasis box="[309,495,1874,1900]" italics="true" pageId="7" pageNumber="8">Picea orientalis</emphasis>
</taxonomicName>
(usually), 
<taxonomicName box="[622,684,1874,1900]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="abies">
<emphasis box="[622,684,1874,1900]" italics="true" pageId="7" pageNumber="8">abies</emphasis>
</taxonomicName>
, 
<taxonomicName box="[698,796,1874,1900]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="omorika">
