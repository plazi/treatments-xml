<taxonomicName authority="Length" authorityName="Length" class="Insecta" family="Crambidae" genus="Patissa" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="190" phylum="Arthropoda" rank="species" species="tenuousa">
