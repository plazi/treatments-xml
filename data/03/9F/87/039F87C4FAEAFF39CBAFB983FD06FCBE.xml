<subSubSection pageId="143" pageNumber="144" type="nomenclature">
<paragraph blockId="143.[151,550,151,178]" box="[151,550,151,178]" pageId="143" pageNumber="144">
<heading bold="true" box="[151,550,151,178]" fontSize="11" level="1" pageId="143" pageNumber="144" reason="1">
<emphasis bold="true" box="[151,550,151,178]" pageId="143" pageNumber="144">
Genus 
<taxonomicName authority="Bartsch, 1934" authorityName="Bartsch" authorityYear="1934" box="[238,550,151,178]" class="Gastropoda" family="Drilliidae" genus="Fenimorea" kingdom="Animalia" order="Neogastropoda" pageId="143" pageNumber="144" phylum="Mollusca" rank="genus">
<emphasis bold="true" box="[238,372,151,177]" italics="true" pageId="143" pageNumber="144">Fenimorea</emphasis>
Bartsch, 
<number box="[491,550,152,178]" pageId="143" pageNumber="144" value="1934.0">1934</number>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="143.[151,1325,220,242]" box="[151,1325,220,242]" pageId="143" pageNumber="144">
<typeStatus box="[151,201,221,242]" pageId="143" pageNumber="144">Type</typeStatus>
species: 
<taxonomicName authority="Bartsch, 1934" authorityName="Bartsch" authorityYear="1934" box="[295,633,220,242]" class="Gastropoda" family="Drilliidae" genus="Fenimorea" kingdom="Animalia" order="Neogastropoda" pageId="143" pageNumber="144" phylum="Mollusca" rank="species" species="janetae">
<emphasis box="[295,487,221,242]" italics="true" pageId="143" pageNumber="144">Fenimorea janetae</emphasis>
Bartsch, 
<number box="[582,633,220,242]" pageId="143" pageNumber="144" value="1934.0">1934</number>
</taxonomicName>
by original designation (Recent, off the north coast of 
<collectingCountry box="[1189,1313,220,242]" name="Puerto Rico" pageId="143" pageNumber="144">Puerto Rico</collectingCountry>
