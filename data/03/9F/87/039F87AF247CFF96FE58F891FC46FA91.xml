<collectingRegion box="[309,381,1095,1121]" country="Vietnam" name="Ha Noi" pageId="12" pageNumber="13">Hanoi</collectingRegion>
and 
<collectingRegion box="[447,547,1095,1122]" country="Vietnam" name="Ha Tinh" pageId="12" pageNumber="13">Ha Tinh</collectingRegion>
(
<bibRefCitation author="Wroblewski A." box="[568,785,1095,1121]" pageId="12" pageNumber="13" pagination="175 - 180" refId="ref52571" refString="Wroblewski A. 1962. Notes on Micronectinae from Viet-nam (Heteroptera, Corixidae). Bulletin de l'Academie polonaise des Sciences 10 (5) 175 - 180." type="journal article" year="1962">Wróblewski 1962</bibRefCitation>
