<quantity box="[252,374,656,680]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="6" pageNumber="788" unit="mm" value="16.0" valueMax="17.0" valueMin="15.0">15–17 mm</quantity>
