(
<figureCitation box="[329,420,1823,1846]" captionStart="Fig" captionStartId="12.[837,872,2065,2086]" captionTargetBox="[916,1399,1474,2029]" captionTargetId="figure-785@12.[903,1409,1461,2049]" captionTargetPageId="12" captionText="Fig. 18. Gestroiella siamensis new species, female, dorsal habitus. Drawn by Young Sohn." figureDoi="http://doi.org/10.5281/zenodo.5340386" httpUri="https://zenodo.org/record/5340386/files/figure.png" pageId="20" pageNumber="275">Figs. 18</figureCitation>
, 
<figureCitation box="[432,502,1823,1846]" captionStart="Figs" captionStartId="16.[138,183,2066,2087]" captionTargetBox="[172,1466,275,1962]" captionTargetId="figure-14@16.[121,1519,237,2013]" captionTargetPageId="16" captionText="Figs. 26–30. Gestroiella species, male genitalia, structural details: 26, G. insignis, paramere; 27, G. limnocoroides, paramere; 28, G. siamensis, paramere; 29, G. limnocoroides, phallotheca; 30, G. siamensis, dorsomedial process of genital capsule." figureDoi="http://doi.org/10.5281/zenodo.5340392" httpUri="https://zenodo.org/record/5340392/files/figure.png" pageId="20" pageNumber="275">28, 30</figureCitation>
, 
<figureCitation box="[515,544,1823,1846]" captionStart="Figs" captionStartId="17.[114,158,2093,2114]" captionTargetBox="[203,1329,188,2031]" captionTargetId="figure-16@17.[196,1363,160,2053]" captionTargetPageId="17" captionText="Figs. 31–33. Gestroiella species, apex of abdomen of female, ventral aspect: 31, G. insignis; 32, G. limnocoroides; 33. G. siamensis." figureDoi="http://doi.org/10.5281/zenodo.5340396" httpUri="https://zenodo.org/record/5340396/files/figure.png" pageId="20" pageNumber="275">33</figureCitation>
