</emphasis>
, 
<taxonomicName authorityName="Voss" authorityYear="2010" baseAuthorityName="Moench" box="[473,553,320,346]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="glauca">
<emphasis box="[473,553,320,346]" italics="true" pageId="14" pageNumber="15">glauca</emphasis>
</taxonomicName>
, 
<taxonomicName box="[568,666,320,346]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="omorika">
<emphasis box="[568,666,320,346]" italics="true" pageId="14" pageNumber="15">omorika</emphasis>
