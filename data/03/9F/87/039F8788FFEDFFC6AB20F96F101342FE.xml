<emphasis box="[680,792,320,346]" italics="true" pageId="14" pageNumber="15">orientalis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[807,905,321,346]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="pungens">
<emphasis box="[807,905,321,346]" italics="true" pageId="14" pageNumber="15">pungens</emphasis>
</taxonomicName>
