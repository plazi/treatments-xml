<taxonomicName baseAuthorityName="Carter" baseAuthorityYear="1976" box="[919,1033,320,346]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="sitchensis">
