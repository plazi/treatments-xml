<paragraph blockId="5.[189,1398,1963,2025]" pageId="5" pageNumber="6">
<taxonomicName authority="Hutchison, 1940" authorityName="Hutchison" authorityYear="1940" box="[189,558,1964,1990]" class="Insecta" family="Corixidae" genus="Micronecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subGenus" subGenus="Indonectella">
