<quantity box="[585,700,447,468]" metricMagnitude="-3" metricUnit="m" metricValue="2.95" metricValueMax="3.6" metricValueMin="2.3" pageId="3" pageNumber="706" unit="mm" value="2.95" valueMax="3.6" valueMin="2.3">2.3-3.6 mm</quantity>
