<emphasis bold="true" box="[189,381,1842,1868]" pageId="32" pageNumber="33">Recorded hosts</emphasis>
</heading>
</paragraph>
<paragraph blockId="32.[189,924,1888,1915]" box="[189,924,1888,1915]" pageId="32" pageNumber="33">
<taxonomicName box="[189,298,1888,1914]" class="Pinopsida" family="Pinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
: 
<taxonomicName box="[309,447,1889,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="nigra">
<emphasis box="[309,447,1889,1914]" italics="true" pageId="32" pageNumber="33">Pinus nigra</emphasis>
</taxonomicName>
, rarely 
<taxonomicName box="[537,602,1889,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="mugo">
<emphasis box="[537,602,1889,1914]" italics="true" pageId="32" pageNumber="33">mugo</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Aiton" authorityYear="2010" box="[617,713,1889,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="pinaster">
<emphasis box="[617,713,1889,1914]" italics="true" pageId="32" pageNumber="33">pinaster</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Mordvilko" baseAuthorityYear="1895" box="[727,792,1889,1914]" class="Insecta" family="Aphididae" genus="Cinara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="pinea">
<emphasis box="[727,792,1889,1914]" italics="true" pageId="32" pageNumber="33">pinea</emphasis>
</taxonomicName>
, 
<emphasis box="[807,924,1888,1914]" italics="true" pageId="32" pageNumber="33">
<taxonomicName box="[807,920,1888,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="sylvestris">sylvestris</taxonomicName>
.
</emphasis>
</paragraph>
