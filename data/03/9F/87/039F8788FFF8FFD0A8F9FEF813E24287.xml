<taxonomicName box="[309,473,577,603]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="sibirica">
