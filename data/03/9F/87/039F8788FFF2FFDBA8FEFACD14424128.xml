<taxonomicName authorityName="Du Tour" authorityYear="2010" box="[761,851,1713,1739]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="sibirica">
