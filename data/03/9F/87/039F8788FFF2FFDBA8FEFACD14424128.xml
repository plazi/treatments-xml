</taxonomicName>
, 
<taxonomicName box="[448,560,1713,1739]" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="balsamea">
