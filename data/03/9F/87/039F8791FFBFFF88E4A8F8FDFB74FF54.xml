<subSubSection box="[199,1210,548,573]" pageId="6" pageNumber="119" type="etymology">
<paragraph blockId="6.[151,1437,151,573]" box="[199,1210,548,573]" pageId="6" pageNumber="119">
<emphasis bold="true" box="[199,334,548,573]" pageId="6" pageNumber="119">Etymology.</emphasis>
