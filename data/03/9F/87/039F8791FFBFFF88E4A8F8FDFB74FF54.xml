, PL 
<quantity box="[1019,1156,439,464]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="6" pageNumber="119" unit="mm" value="1.4" valueMax="1.5" valueMin="1.3">1.3–1.5 mm</quantity>
, PW 
<quantity box="[1216,1352,440,464]" metricMagnitude="-3" metricUnit="m" metricValue="1.7999999999999998" metricValueMax="1.9" metricValueMin="1.7" pageId="6" pageNumber="119" unit="mm" value="1.7999999999999998" valueMax="1.9" valueMin="1.7">1.7–1.9 mm</quantity>
, Ediam 
<quantity box="[151,316,476,501]" metricMagnitude="-4" metricUnit="m" metricValue="6.550000000000001" metricValueMax="7.199999999999999" metricValueMin="5.8999999999999995" pageId="6" pageNumber="119" unit="mm" value="0.655" valueMax="0.72" valueMin="0.59">0.59–0.72 mm</quantity>
, Edist 
