<taxonomicName authority="Nakane, 1989" authorityName="Nakane" authorityYear="1989" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="732" phylum="Arthropoda" rank="species" species="kitamurai">
