<taxonomicName authorityName="Schmitz &amp; Rubinoff" authorityYear="2011" box="[1186,1329,1090,1112]" class="Insecta" family="Cosmopterigidae" genus="Hyposmocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="27" phylum="Arthropoda" rank="species" species="kamakou">
<emphasis box="[1186,1329,1090,1112]" italics="true" pageId="12" pageNumber="27">H. kamakou</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Schmitz &amp; Rubinoff" authorityYear="2011" class="Insecta" family="Cosmopterigidae" genus="Hyposmocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="27" phylum="Arthropoda" rank="species" species="wailua">
<emphasis italics="true" pageId="12" pageNumber="27">H. wailua</emphasis>
