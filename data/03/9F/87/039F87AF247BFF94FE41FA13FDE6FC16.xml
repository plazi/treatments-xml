<collectingCountry box="[1045,1123,832,858]" name="Russia" pageId="14" pageNumber="15">Russia</collectingCountry>
(Khanka Lake), 
<collectingCountry box="[1321,1396,833,859]" name="South Korea" pageId="14" pageNumber="15">Korea</collectingCountry>
, 
<collectingCountry box="[188,258,867,893]" name="Japan" pageId="14" pageNumber="15">Japan</collectingCountry>
, eastern 
<collectingCountry box="[360,430,867,893]" name="China" pageId="14" pageNumber="15">China</collectingCountry>
(
<bibRefCitation author="Wroblewski A." box="[445,658,866,893]" pageId="14" pageNumber="15" pagination="301 - 331" refId="ref52542" refString="Wroblewski A. 1960. Notes on some Asiatic species of the genus Micronecta Kirk. Heteroptera: Corixidae). Annales Zoologici 18: 301 - 331." type="journal article" year="1960">Wróblewski 1960</bibRefCitation>
, 
<bibRefCitation author="Wroblewski A." box="[670,732,866,892]" pageId="14" pageNumber="15" pagination="753 - 779" refId="ref52688" refString="Wroblewski A. 1968. Notes on Oriental Micronectinae. Polskie Pismo Entomologiczne 30 (XII): 753 - 779." type="journal article" year="1968">1968</bibRefCitation>
; 
<bibRefCitation author="Nieser N. &amp; Chen P. P. &amp; Yang C. M." box="[744,959,866,893]" pageId="14" pageNumber="15" pagination="189 - 209" refId="ref51916" refString="Nieser N., Chen P. P. &amp; Yang C. M. 2005. A new subgenus and six new species of Nepomorpha (Insecta: Heteroptera) from Yunnan, China. The Raffles Bulletin of Zoology 53 (2): 189 - 209." type="journal article" year="2005">
