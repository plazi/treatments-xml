<taxonomicName box="[788,909,1084,1110]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="koraiensis">
<emphasis box="[788,909,1084,1110]" italics="true" pageId="6" pageNumber="7">koraiensis</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Dreyfus" baseAuthorityYear="1889" box="[923,1035,1084,1110]" class="Insecta" family="Adelgidae" genus="Pineus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="orientalis">
