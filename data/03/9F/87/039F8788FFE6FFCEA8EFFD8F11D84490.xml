: 
<taxonomicName box="[309,445,1084,1110]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="abies">
