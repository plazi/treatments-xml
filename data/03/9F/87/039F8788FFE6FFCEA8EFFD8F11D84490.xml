<taxonomicName box="[667,773,1085,1110]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="jezoensis">
