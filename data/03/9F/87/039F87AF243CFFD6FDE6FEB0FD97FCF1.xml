<collectingCountry box="[1088,1199,568,594]" name="Malaysia" pageId="76" pageNumber="77">Malaysia</collectingCountry>
, 
<collectingCountry box="[1211,1333,568,594]" name="Singapore" pageId="76" pageNumber="77">Singapore</collectingCountry>
, Java (
<bibRefCitation author="Nieser N." box="[198,362,604,631]" pageId="76" pageNumber="77" pagination="263 - 274" refId="ref51745" refString="Nieser N. 2002 b. Guide to aquatic Heteroptera of Singapore and Peninsular Malaysia IV. Corixoidea. The Raffles Bulletin of Zoology 50 (1): 263 - 274." type="journal article" year="2002">Nieser 2002b</bibRefCitation>
), and 
<collectingCountry box="[431,552,604,630]" name="Cambodia" pageId="76" pageNumber="77">Cambodia</collectingCountry>
(
<bibRefCitation author="Polhemus D. A." box="[567,756,604,630]" pageId="76" pageNumber="77" pagination="89 - 138" refId="ref52005" refString="Polhemus D. A. 2017. An initial survey of aquatic and semi-aquatic Heteroptera (Insecta) from the Cardamom Mountains and adjacent uplands of southwestern Cambodia, with descriptions of four new species. Tijdschrift voor Entomologie 160: 89 - 138. https: // doi. org / 10.1163 / 22119434 - 00002068" type="journal article" year="2017">Polhemus 2017</bibRefCitation>
