</taxonomicName>
<taxonomicNameLabel box="[1199,1287,1678,1697]" pageId="23" pageNumber="38" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(slide PS179); B, 
<emphasis bold="true" box="[198,599,1707,1726]" pageId="23" pageNumber="38">
<taxonomicName authority="Schmitz &amp; Rubinoff, 2011" authorityName="Schmitz &amp; Rubinoff" authorityYear="2011" box="[198,495,1707,1726]" class="Insecta" family="Cosmopterigidae" genus="Hyposmocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="38" phylum="Arthropoda" rank="species" species="ipowainui" status="sp. nov.">
<emphasis bold="true" box="[198,495,1707,1726]" italics="true" pageId="23" pageNumber="38">Hyposmocoma ipowainui</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[507,599,1707,1726]" pageId="23" pageNumber="38" rank="species">sp. nov.</taxonomicNameLabel>
