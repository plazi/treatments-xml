<paragraph blockId="4.[151,1127,220,275]" pageId="4" pageNumber="729">
<taxonomicName authorityName="Corporaal &amp; Wiel" authorityYear="1949" box="[151,314,220,243]" class="Insecta" family="Cleridae" genus="Orthocladiscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="729" phylum="Arthropoda" rank="genus">
