, holotype; 2 – 
<taxonomicName authority="Yang &amp; Yang, 2013" authorityName="Yang &amp; Yang" authorityYear="2013" box="[183,437,1543,1562]" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="727" phylum="Arthropoda" rank="species" species="ahsini">
<emphasis box="[183,261,1543,1562]" italics="true" pageId="2" pageNumber="727">D. ahsini</emphasis>
<bibRefCitation author="YANG G. &amp; YANG X. K." box="[267,437,1543,1562]" pageId="2" pageNumber="727" pagination="167 - 171" refId="ref8628" refString="YANG G. &amp; YANG X. K. 2013: Description of a remarkable new species of Diplopherusa Heller from Taiwan (Coleoptera: Cleridae). Pp. 167 - 171. In: LIN M. Y. &amp; CHEN C. C. (eds): In memory of Mr. Wenhsin Lin. Formosa Ecological Company, Taiwan, 233 pp." type="book chapter" year="2013">Yang &amp; Yang, 2013</bibRefCitation>
</taxonomicName>
; 3 – 
<taxonomicName authority="Nakane, 1989" authorityName="Nakane" authorityYear="1989" box="[478,713,1543,1562]" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="727" phylum="Arthropoda" rank="species" species="kitamurai">
<emphasis box="[478,587,1543,1562]" italics="true" pageId="2" pageNumber="727">D. kitamurai</emphasis>
<bibRefCitation author="NAKANE T." box="[592,713,1543,1562]" pageId="2" pageNumber="727" pagination="143 - 152" refId="ref8507" refString="NAKANE T. 1989: Notes on some little-known beetles (Coleoptera) in Japan. 5. Kita Kyushu No Konchu 36: 143 - 152." type="journal article" year="1989">Nakane, 1989</bibRefCitation>
</taxonomicName>
; 4–5 – 
<taxonomicName authority="(Miyatake, 1965)" baseAuthorityName="Miyatake" baseAuthorityYear="1965" box="[775,1025,1543,1562]" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="727" phylum="Arthropoda" rank="species" species="shibatai">
<emphasis box="[775,870,1543,1562]" italics="true" pageId="2" pageNumber="727">D. shibatai</emphasis>
(
<bibRefCitation author="MIYATAKE M." box="[881,1019,1543,1562]" pageId="2" pageNumber="727" pagination="19 - 25" refId="ref8333" refString="MIYATAKE M. 1965: New species and records of the Cleridae from Ryukyu Islands, I (Coleoptera). Entomological Review of Japan 18: 19 - 25." type="journal article" year="1965">Miyatake, 1965</bibRefCitation>
)
</taxonomicName>
. Scale bars: 2.0 mm.
</paragraph>
</caption>
