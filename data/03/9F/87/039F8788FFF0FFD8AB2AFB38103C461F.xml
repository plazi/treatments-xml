<emphasis box="[309,459,1474,1500]" italics="true" pageId="16" pageNumber="17">
<taxonomicName box="[309,448,1474,1500]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="abies">
