: 
<taxonomicName box="[309,451,1219,1244]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="peuce">
<emphasis box="[309,451,1219,1244]" italics="true" pageId="30" pageNumber="31">Pinus peuce</emphasis>
</taxonomicName>
, 
<taxonomicName box="[466,556,1218,1244]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="sibirica">
<emphasis box="[466,556,1218,1244]" italics="true" pageId="30" pageNumber="31">sibirica</emphasis>
</taxonomicName>
, 
<taxonomicName box="[570,655,1218,1244]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="strobus">
<emphasis box="[570,655,1218,1244]" italics="true" pageId="30" pageNumber="31">strobus</emphasis>
</taxonomicName>
.
</paragraph>
