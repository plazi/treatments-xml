Abies 
<taxonomicName box="[381,433,1148,1174]" pageId="18" pageNumber="19" rank="species" species="alba">alba</taxonomicName>
</emphasis>
</taxonomicName>
, 
<taxonomicName box="[448,534,1148,1174]" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="veitchii">
