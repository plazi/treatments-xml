, 
<collectingRegion box="[548,642,669,696]" country="Vietnam" name="Ha Tinh" pageId="61" pageNumber="62">Ha Tinh</collectingRegion>
(
<bibRefCitation author="Wroblewski A." box="[657,870,669,695]" pageId="61" pageNumber="62" pagination="175 - 180" refId="ref52571" refString="Wroblewski A. 1962. Notes on Micronectinae from Viet-nam (Heteroptera, Corixidae). Bulletin de l'Academie polonaise des Sciences 10 (5) 175 - 180." type="journal article" year="1962">Wróblewski 1962</bibRefCitation>
, 
<bibRefCitation author="Wroblewski A." box="[882,943,669,695]" pageId="61" pageNumber="62" pagination="229 - 251" refId="ref52662" refString="Wroblewski A. 1967. Further notes on Micronectinae from Viet-nam (Heteroptera, Corixidae). Polskie pismo entomologiczne 37: 229 - 251." type="journal article" year="1967">1967</bibRefCitation>
, as 
<taxonomicName baseAuthorityName="Fieber" baseAuthorityYear="1844" box="[986,1128,670,695]" class="Insecta" family="Corixidae" genus="Micronecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="punctata">
