<number box="[151,181,496,522]" pageId="1" pageNumber="63" value="35.0">35</number>
(
<number box="[197,276,496,522]" pageId="1" pageNumber="63" value="35.0" valueMax="36.0" valueMin="34.0">34−36</number>
