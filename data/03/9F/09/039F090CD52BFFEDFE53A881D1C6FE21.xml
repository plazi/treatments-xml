(
<emphasis box="[561,679,221,244]" italics="true" pageId="40" pageNumber="861">Plesiovelia</emphasis>
) 
<emphasis box="[697,991,221,244]" italics="true" pageId="40" pageNumber="861">rhadamantha rhadamantha</emphasis>
Hoberlandt, 1941
</taxonomicName>
(see 
<bibRefCitation author="TAMANINI L." box="[260,422,252,276]" pageId="40" pageNumber="861" pagination="17 - 74" refId="ref59317" refString="TAMANINI L. 1947: Contributo ad una revisione del genere Velia Latr. e descrizione di alcune specie nuove (Hemiptera Heteroptera, Veliidae). Bollettino della Societa Entomologica Italiana 26: 17 - 74." type="journal article" year="1947">TAMANINI 1947</bibRefCitation>
, 
<bibRefCitation author="TAMANINI L." box="[433,489,251,275]" pageId="40" pageNumber="861" pagination="139 - 143" refId="ref59606" refString="TAMANINI L. 1965: Valore tassonomico di Velia rhadamantha Hoberl. e di V. cyrenaica Tam. Bollettino della Societa Entomologica Italiana 95: 139 - 143." type="journal article" year="1965">1965</bibRefCitation>
; 
<bibRefCitation author="ANDERSEN N. M." box="[499,681,251,276]" pageId="40" pageNumber="861" pagination="77 - 114" refId="ref50200" refString="ANDERSEN N. M. 1995 a: Infraorder Gerromorpha Popov, 1971 - semiaquatic bugs. Pp. 77 - 114. In: AUKEMA B. &amp; RIEGER CH. (eds): Catalogue of the Heteroptera of the Palaearctic Region. Vol. 1. The Netherlands Entomological Society, Amsterdam, xxvi + 222 pp." type="book chapter" year="1995">ANDERSEN 1995a</bibRefCitation>
).
</paragraph>
