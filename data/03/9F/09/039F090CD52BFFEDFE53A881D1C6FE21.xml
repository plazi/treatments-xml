<taxonomicName authority="rhadamantha Hoberlandt, 1941" authorityName="rhadamantha Hoberlandt" authorityYear="1941" class="Insecta" family="Veliidae" genus="Velia" kingdom="Animalia" order="Hemiptera" pageId="40" pageNumber="861" phylum="Arthropoda" rank="subSpecies" species="rhadamantha" subGenus="Plesiovelia" subSpecies="rhadamantha">
