<paragraph blockId="2.[151,881,1939,2026]" lastBlockId="3.[151,1437,151,448]" lastPageId="3" lastPageNumber="62" pageId="2" pageNumber="61">
<taxonomicName authority="Wu, 1962" authorityName="Wu" authorityYear="1962" box="[151,575,2003,2026]" class="Insecta" family="Nemouridae" genus="Sphaeronemoura" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="hamistyla">
