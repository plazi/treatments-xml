<caption httpUri="https://zenodo.org/record/252138/files/figure.png" pageId="10" pageNumber="337" targetBox="[173,1419,206,1413]" targetPageId="10">
<paragraph blockId="10.[151,1437,1456,1540]" pageId="10" pageNumber="337">
<emphasis bold="true" box="[151,768,1456,1479]" pageId="10" pageNumber="337">
