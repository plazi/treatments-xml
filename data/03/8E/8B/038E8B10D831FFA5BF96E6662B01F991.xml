<emphasis bold="true" box="[238,906,515,539]" pageId="37" pageNumber="36">
<taxonomicName authority="Gordon and Hanley" authorityName="Gordon and Hanley" authorityYear="2017" box="[238,730,515,539]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="36" phylum="Arthropoda" rank="species" species="kayla" status="new species">
