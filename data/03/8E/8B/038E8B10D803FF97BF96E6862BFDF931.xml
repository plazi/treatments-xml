<taxonomicName authorityName="Gordon and Hanley" authorityYear="2017" box="[908,996,1763,1787]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="lena">
