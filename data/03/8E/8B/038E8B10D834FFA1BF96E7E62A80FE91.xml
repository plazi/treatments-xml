<taxonomicName authorityName="Gorham" authorityYear="1897" box="[1147,1259,292,315]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="32" phylum="Arthropoda" rank="genus">
<emphasis box="[1147,1259,292,315]" italics="true" pageId="33" pageNumber="32">Neaporia</emphasis>
</taxonomicName>
