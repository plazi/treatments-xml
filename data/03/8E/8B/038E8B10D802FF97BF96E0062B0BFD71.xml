<paragraph blockId="22.[192,889,1123,1147]" box="[192,889,1123,1147]" pageId="22" pageNumber="21">
<heading bold="true" box="[192,889,1123,1147]" fontSize="10" level="2" pageId="22" pageNumber="21" reason="3">
<emphasis bold="true" box="[192,889,1123,1147]" pageId="22" pageNumber="21">
