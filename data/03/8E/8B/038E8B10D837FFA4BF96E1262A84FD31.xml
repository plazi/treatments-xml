<taxonomicName authorityName="Gorham" authorityYear="1897" box="[1149,1261,708,731]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="35" phylum="Arthropoda" rank="genus">
