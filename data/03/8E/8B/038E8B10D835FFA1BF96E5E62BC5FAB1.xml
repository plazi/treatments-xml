<taxonomicName authorityName="Gorham" authorityYear="1897" box="[1149,1261,1348,1371]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="32" phylum="Arthropoda" rank="genus">
