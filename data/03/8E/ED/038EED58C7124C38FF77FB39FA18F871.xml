<taxonomicName authority="Sharp, 1877" authorityName="Sharp" authorityYear="1877" box="[1206,1436,1537,1559]" class="Insecta" family="Melolonthidae" genus="Systellopus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="124" phylum="Arthropoda" rank="genus">
