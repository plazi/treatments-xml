<caption box="[151,1037,1799,1821]" httpUri="https://zenodo.org/record/1042936/files/figure.png" pageId="14" pageNumber="215" targetBox="[166,1416,215,1765]" targetPageId="14">
<paragraph blockId="14.[151,1037,1799,1821]" box="[151,1037,1799,1821]" pageId="14" pageNumber="215">
<emphasis bold="true" box="[151,274,1799,1821]" pageId="14" pageNumber="215">FIGURE 5.</emphasis>
<taxonomicName authority="Mironov &amp; Chandler, 2017" authorityName="Mironov &amp; Chandler" authorityYear="2017" box="[281,534,1799,1821]" class="Arachnida" family="Proctophyllodidae" genus="Amerodectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="hylocichlae" status="sp. n.">
<emphasis box="[281,534,1799,1821]" italics="true" pageId="14" pageNumber="215">Amerodectes hylocichlae</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[540,597,1799,1820]" pageId="14" pageNumber="215" rank="species">
<emphasis bold="true" box="[540,597,1799,1820]" pageId="14" pageNumber="215">sp. n.</emphasis>
