<collectingCountry box="[638,722,1352,1375]" name="Mexico" pageId="2" pageNumber="63">Mexico</collectingCountry>
, 
<collectingCountry box="[734,793,1352,1375]" name="Cuba" pageId="2" pageNumber="63">Cuba</collectingCountry>
, 
<collectingCountry box="[805,894,1352,1375]" name="Jamaica" pageId="2" pageNumber="63">Jamaica</collectingCountry>
, 
<collectingCountry box="[905,1054,1352,1375]" name="Virgin Islands" pageId="2" pageNumber="63">Virgin Islands</collectingCountry>
and 
<collectingCountry box="[1109,1174,1352,1375]" name="Brazil" pageId="2" pageNumber="63">Brazil</collectingCountry>
(
<collectingRegion box="[1189,1240,1352,1375]" country="Brazil" name="Para" pageId="2" pageNumber="63">Pará</collectingRegion>
, 
