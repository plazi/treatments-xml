<paragraph blockId="9.[151,1436,1340,2014]" pageId="9" pageNumber="28">
<emphasis bold="true" box="[151,297,1341,1366]" pageId="9" pageNumber="28">Dimensions.</emphasis>
<typeStatus box="[307,411,1341,1365]" pageId="9" pageNumber="28">Holotype</typeStatus>
female length 
<number box="[588,630,1341,1365]" pageId="9" pageNumber="28" value="438.0">438</number>
μm, breadth 
<number box="[789,831,1341,1365]" pageId="9" pageNumber="28" value="271.0">271</number>
μm. 
<typeStatus box="[896,994,1342,1365]" pageId="9" pageNumber="28">Paratype</typeStatus>
females (
