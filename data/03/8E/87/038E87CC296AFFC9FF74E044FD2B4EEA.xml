. Abdomen length 
<quantity box="[104,221,1842,1871]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="42" pageNumber="41" unit="mm" value="1.8">1.80 mm</quantity>
, width 
<quantity box="[314,432,1842,1871]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" pageId="42" pageNumber="41" unit="mm" value="2.15">2.15 mm</quantity>
