<quantity box="[1033,1107,770,796]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="51" pageNumber="204" unit="mm" value="4.0">4 mm</quantity>
, 
<collectionCode box="[1122,1169,770,796]" country="Australia" name="Australian Museum" pageId="51" pageNumber="204" type="Museum">AM</collectionCode>
<accessionNumber box="[1180,1270,770,796]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/P62519" pageId="51" pageNumber="204">P62519</accessionNumber>
(
