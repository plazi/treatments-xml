</taxonomicName>
</paragraph>
<paragraph blockId="28.[192,736,291,347]" box="[192,736,323,347]" pageId="28" pageNumber="27">
<taxonomicName authority="(Sepp, [1842])" baseAuthorityName="Sepp" baseAuthorityYear="1842" box="[192,582,323,347]" class="Insecta" family="Hesperiidae" genus="Nascus" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="paulliniae">
