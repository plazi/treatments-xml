<typeStatus box="[1034,1153,1916,1945]" pageId="40" pageNumber="39">Holotype</typeStatus>
male, total length 
<quantity box="[1403,1524,1916,1945]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="40" pageNumber="39" unit="mm" value="4.2">4.20 mm</quantity>
.
</paragraph>
<paragraph blockId="41.[104,717,112,178]" pageId="41" pageNumber="40">
Cephalothorax length 2.00 mm, width 
<number box="[661,717,112,141]" pageId="41" pageNumber="40" value="1.4">1.40</number>
Abdomen length 
<quantity box="[326,443,149,178]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="41" pageNumber="40" unit="mm" value="2.2">2.20 mm</quantity>
, width 
<quantity box="[537,655,149,178]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="41" pageNumber="40" unit="mm" value="1.1">1.10 mm</quantity>
