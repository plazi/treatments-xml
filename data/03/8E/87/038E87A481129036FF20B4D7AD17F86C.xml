<taxonomicName authorityName="Liu &amp; Qi &amp; Wang" authorityYear="2020" box="[283,398,1727,1751]" class="Insecta" family="Crambidae" genus="Agrotera" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="561" phylum="Arthropoda" rank="species" species="aculeata">
<emphasis box="[283,398,1727,1751]" italics="true" pageId="5" pageNumber="561">A. aculeata</emphasis>
</taxonomicName>
, holotype, slide No. LP18393; 8, 
<taxonomicName authorityName="Liu &amp; Qi &amp; Wang" authorityYear="2020" box="[742,896,1727,1751]" class="Insecta" family="Crambidae" genus="Agrotera" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="561" phylum="Arthropoda" rank="species" species="robustispina">
