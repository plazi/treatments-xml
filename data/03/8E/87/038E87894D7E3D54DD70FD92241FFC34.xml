<paragraph blockId="4.[264,1324,667,1894]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[264,455,667,693]" pageId="4" pageNumber="5">DIAGNOSIS ­</emphasis>
Caudal 
<number box="[569,623,668,694]" pageId="4" pageNumber="5" value="9.0" valueMax="10.0" valueMin="8.0">8­10</number>
