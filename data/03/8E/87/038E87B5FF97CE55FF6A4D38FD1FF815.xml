<taxonomicName authority="Lopes-Andrade" authorityName="Lopes-Andrade" authorityYear="2008" class="Insecta" family="Ciidae" genus="Scolytocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="werneri">
