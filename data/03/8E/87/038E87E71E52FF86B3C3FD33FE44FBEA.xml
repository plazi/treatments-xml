<number box="[151,210,1038,1062]" pageId="14" pageNumber="185" value="2004.0">2004</number>
, fig. 
<number box="[276,306,1038,1062]" pageId="14" pageNumber="185" value="84.0">84</number>
E), web modified (Agnarsson 
<number box="[663,722,1038,1062]" pageId="14" pageNumber="185" value="2004.0">2004</number>
, fig. 
<number box="[789,819,1038,1062]" pageId="14" pageNumber="185" value="97.0">97</number>
