<paragraph blockId="14.[104,784,591,694]" pageId="14" pageNumber="13">
<emphasis box="[200,292,591,619]" italics="true" pageId="14" pageNumber="13">MALE.</emphasis>
<typeStatus box="[300,419,591,620]" pageId="14" pageNumber="13">Holotype</typeStatus>
male, total length 
<quantity box="[662,781,591,620]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" pageId="14" pageNumber="13" unit="mm" value="4.9">4.90 mm</quantity>
