<collectingRegion box="[793,988,1988,2015]" country="China" name="Yunnan" pageId="4" pageNumber="260">Yunnan Province</collectingRegion>
, 
<collectingCountry box="[999,1069,1988,2014]" name="China" pageId="4" pageNumber="260">China</collectingCountry>
