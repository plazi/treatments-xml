<quantity metricMagnitude="3" metricUnit="m" metricValue="2.175" metricValueMax="2.45" metricValueMin="1.9" pageId="4" pageNumber="260" unit="m" value="2175.0" valueMax="2450.0" valueMin="1900.0">
