<number box="[790,820,1766,1792]" pageId="11" pageNumber="12" value="24.0">24</number>
(
<number box="[838,912,1766,1792]" pageId="11" pageNumber="12" value="23.5" valueMax="25.0" valueMin="22.0">22­25</number>
). 
<emphasis bold="true" box="[934,993,1766,1792]" pageId="11" pageNumber="12">Legs</emphasis>
