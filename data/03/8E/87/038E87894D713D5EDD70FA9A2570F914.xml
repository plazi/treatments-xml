<number box="[1012,1041,1726,1752]" pageId="11" pageNumber="12" value="15.0">15</number>
(
<number box="[1059,1129,1726,1752]" pageId="11" pageNumber="12" value="16.0" valueMax="18.0" valueMin="14.0">14­18</number>
