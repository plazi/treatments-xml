<number box="[1129,1158,1607,1633]" pageId="11" pageNumber="12" value="13.0">13</number>
(
<number box="[1175,1247,1607,1633]" pageId="11" pageNumber="12" value="13.5" valueMax="15.0" valueMin="12.0">12­15</number>
