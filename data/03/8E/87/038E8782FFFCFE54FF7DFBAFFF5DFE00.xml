<number box="[1221,1250,1512,1538]" pageId="1" pageNumber="64" value="19.0">19</number>
(
<number box="[1268,1346,1512,1538]" pageId="1" pageNumber="64" value="19.5" valueMax="20.0" valueMin="19.0">19–20</number>
