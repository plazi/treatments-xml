<number box="[1108,1159,1552,1578]" pageId="1" pageNumber="64" value="17.5">17.5</number>
(
<number box="[1175,1253,1552,1578]" pageId="1" pageNumber="64" value="18.5" valueMax="20.0" valueMin="17.0">17–20</number>
