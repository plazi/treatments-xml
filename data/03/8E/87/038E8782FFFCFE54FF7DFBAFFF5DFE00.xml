<number box="[775,811,1512,1538]" pageId="1" pageNumber="64" value="2.5">2.5</number>
(
<number box="[829,899,1512,1538]" pageId="1" pageNumber="64" value="2.75" valueMax="3.0" valueMin="2.5">2.5–3</number>
), antaxial genual setae (
<emphasis box="[1183,1204,1513,1538]" italics="true" pageId="1" pageNumber="64">l''</emphasis>
