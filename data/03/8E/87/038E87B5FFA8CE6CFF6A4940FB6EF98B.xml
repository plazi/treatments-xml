<collectingRegion box="[1165,1248,1312,1338]" country="Brazil" name="Parana" pageId="28" pageNumber="29">Paraná</collectingRegion>
, 
<collectingRegion box="[1262,1436,1312,1338]" country="Brazil" name="Santa Catarina" pageId="28" pageNumber="29">Santa Catarina</collectingRegion>
and 
<collectingRegion box="[200,419,1352,1378]" country="Brazil" name="Rio Grande do Sul" pageId="28" pageNumber="29">Rio Grande do Sul</collectingRegion>
