Lorenzen, 
<number box="[575,631,151,175]" pageId="5" pageNumber="44" value="1966.0">1966</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Vitiello, 1971" authorityName="Vitiello" authorityYear="1971" box="[649,974,151,176]" class="Adenophorea" family="Leptolaimidae" genus="Antomicron" kingdom="Animalia" order="Araeolaimida" pageId="5" pageNumber="44" phylum="Nematoda" rank="species" species="profundum">
