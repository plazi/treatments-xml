<paragraph blockId="4.[136,774,844,894]" pageId="4" pageNumber="13">
<emphasis bold="true" box="[136,450,844,865]" pageId="4" pageNumber="13">
<emphasis bold="true" box="[136,325,844,865]" italics="true" pageId="4" pageNumber="13">Material examined.</emphasis>
