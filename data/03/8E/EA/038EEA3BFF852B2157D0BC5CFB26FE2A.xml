<paragraph blockId="1.[151,858,1878,1905]" box="[151,858,1878,1905]" pageId="1" pageNumber="480">
<emphasis bold="true" box="[151,858,1878,1905]" pageId="1" pageNumber="480">
Key to Nymphal Instars of 
<taxonomicName authorityName="Sailer" authorityYear="1952" box="[491,673,1878,1905]" class="Insecta" family="Pentatomidae" genus="Mecidea" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="480" phylum="Arthropoda" rank="species" species="major">
<emphasis bold="true" box="[491,673,1878,1905]" italics="true" pageId="1" pageNumber="480">Mecidea major</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Froeschner" baseAuthorityYear="1988" box="[735,848,1879,1905]" class="Insecta" family="Pentatomidae" genus="Mecidea" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="480" phylum="Arthropoda" rank="species" species="minor">
