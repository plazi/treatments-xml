</taxonomicName>
] 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="3.95" metricValueMax="4.0" metricValueMin="3.9" pageId="0" pageNumber="65" unit="mm" value="39.5" valueMax="40.0" valueMin="39.0">39-40 mm</quantity>
