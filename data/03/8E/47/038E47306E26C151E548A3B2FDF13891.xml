<subSubSection pageId="18" pageNumber="31" type="etymology">
<paragraph blockId="18.[166,794,1225,1278]" pageId="18" pageNumber="31">
<emphasis bold="true" box="[166,289,1225,1249]" pageId="18" pageNumber="31">Etymology</emphasis>
