<paragraph blockId="18.[143,1104,874,1098]" pageId="18" pageNumber="2010">
<materialsCitation collectingDate="2007-04-22" collectionCode="SCAU" collectorName="HW Chen" country="China" elevation="570" location="Xishuangbanna" municipality="Mengla" pageId="18" pageNumber="2010" specimenCount="1" specimenCount-male="1" stateProvince="Yunnan" typeStatus="holotype">
