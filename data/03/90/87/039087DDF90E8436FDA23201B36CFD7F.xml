or 
<taxonomicName authority="(Linnaeus)" baseAuthorityName="Linnaeus" box="[533,903,721,748]" class="Insecta" family="Noctuidae" genus="Lacanobia" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="oleracea">
