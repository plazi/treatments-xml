<taxonomicName authority="(Esper)" baseAuthorityName="Esper" box="[189,494,721,747]" class="Insecta" family="Noctuidae" genus="Blepharita" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="adusta">
