<paragraph blockId="21.[125,722,1277,1301]" box="[125,722,1277,1301]" pageId="21" pageNumber="4050">
Body length 
<quantity box="[276,417,1277,1301]" metricMagnitude="-3" metricUnit="m" metricValue="2.8499999999999996" metricValueMax="2.9" metricValueMin="2.8" pageId="21" pageNumber="4050" unit="mm" value="2.8499999999999996" valueMax="2.9" valueMin="2.8">2.8–2.9 mm</quantity>
; wing length 
<quantity box="[576,718,1277,1301]" metricMagnitude="-3" metricUnit="m" metricValue="2.3499999999999996" metricValueMax="2.4" metricValueMin="2.3" pageId="21" pageNumber="4050" unit="mm" value="2.3499999999999996" valueMax="2.4" valueMin="2.3">2.3–2.4 mm</quantity>
