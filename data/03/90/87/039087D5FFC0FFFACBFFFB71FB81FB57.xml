<location LSID="urn:lsid:plazi:treatment:039087D5FFC0FFFACBFFFB71FB81FB57:8EE66018FFC6FFFACB93FD17FBF6FD4D" box="[1003,1059,685,709]" country="Mongolia" county="Province" latitude="49.25" longLatPrecision="1105" longitude="108.666664" municipality="District Batshireet" name="river Minj Gol" pageId="13" pageNumber="147" stateProvince="Hentiy">river</location>
