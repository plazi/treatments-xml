<quantity metricMagnitude="-3" metricUnit="m" metricValue="1.275" metricValueMax="1.3" metricValueMin="1.25" pageId="1" pageNumber="285" unit="mm" value="1.275" valueMax="1.3" valueMin="1.25">1.25–1.3 mm</quantity>
