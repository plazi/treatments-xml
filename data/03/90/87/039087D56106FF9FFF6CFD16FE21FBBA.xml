<quantity box="[989,1113,1406,1431]" metricMagnitude="-2" metricUnit="m" metricValue="3.4" metricValueMax="3.6" metricValueMin="3.2" pageId="1" pageNumber="50" unit="mm" value="34.0" valueMax="36.0" valueMin="32.0">32–36 mm</quantity>
