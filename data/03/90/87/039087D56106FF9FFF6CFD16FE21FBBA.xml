mm; cercus length 
<quantity box="[1124,1248,1371,1395]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.1" metricValueMin="1.0" pageId="1" pageNumber="50" unit="mm" value="10.5" valueMax="11.0" valueMin="10.0">10–11 mm</quantity>
