<quantity metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="1" pageNumber="50" unit="mm" value="4.5" valueMax="5.0" valueMin="4.0">4–5 mm</quantity>
