; antennae length 
<date box="[828,898,1371,1395]" pageId="1" pageNumber="50" value="1927-04-02">
2 
<quantity box="[842,898,1371,1395]" metricMagnitude="-2" metricUnit="m" metricValue="1.55" metricValueMax="2.7" metricValueMin="0.4" pageId="1" pageNumber="50" unit="mm" value="15.5" valueMax="27.0" valueMin="4.0">4–27</quantity>
