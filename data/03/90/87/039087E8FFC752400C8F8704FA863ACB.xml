<paragraph blockId="11.[151,521,1376,1401]" box="[151,521,1376,1401]" pageId="11" pageNumber="50">
<heading bold="true" box="[151,521,1376,1401]" fontSize="10" level="2" pageId="11" pageNumber="50" reason="2">
<emphasis bold="true" box="[151,521,1376,1401]" pageId="11" pageNumber="50">
<taxonomicName box="[151,452,1376,1401]" class="Insecta" family="Baissogryllidae" genus="Cephalotettix" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="50" phylum="Arthropoda" rank="species" species="chinantecus" status="n. sp.">
<emphasis bold="true" box="[151,452,1376,1401]" italics="true" pageId="11" pageNumber="50">Cephalotettix chinantecus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[459,521,1376,1400]" pageId="11" pageNumber="50" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
