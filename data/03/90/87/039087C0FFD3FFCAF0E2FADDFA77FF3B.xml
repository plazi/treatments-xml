and 
<taxonomicName authority="Engel &amp; Grimaldi" authorityName="Engel &amp; Grimaldi" authorityYear="2006" box="[1110,1436,1542,1563]" class="Insecta" family="Sclerogibbidae" genus="Sclerogibbodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="131" phylum="Arthropoda" rank="genus">
