+ 
<number box="[1307,1358,1374,1399]" pageId="13" pageNumber="584" value="0.78">0.78</number>
), leg II 
<number box="[151,200,1411,1435]" pageId="13" pageNumber="584" value="7.88">7.88</number>
(
<number box="[215,267,1410,1435]" pageId="13" pageNumber="584" value="2.39">2.39</number>
