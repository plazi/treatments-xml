<paragraph blockId="9.[264,1324,1476,1621]" box="[264,383,1476,1501]" pageId="9" pageNumber="10">
<emphasis box="[264,383,1476,1501]" italics="true" pageId="9" pageNumber="10">Diagnosis</emphasis>
</paragraph>
<paragraph blockId="9.[264,1324,1476,1621]" pageId="9" pageNumber="10">
<taxonomicName authorityName="Sterrer" authorityYear="1966" box="[312,466,1516,1541]" family="Pterognathiidae" genus="Pterognathia" kingdom="Animalia" order="Filospermoidea" pageId="9" pageNumber="10" phylum="Gnathostomulida" rank="genus">
