<taxonomicName authority="Bloch" authorityName="Bloch" box="[868,1204,648,674]" class="Actinopterygii" family="Tetraodontidae" genus="Tetraodon" kingdom="Animalia" order="Tetraodontiformes" pageId="9" pageNumber="614" phylum="Chordata" rank="species" species="psittacus">
