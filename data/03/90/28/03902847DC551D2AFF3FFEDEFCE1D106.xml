<geoCoordinate box="[760,903,188,212]" degrees="33" direction="south" minutes="09" orientation="latitude" pageId="16" pageNumber="17" precision="15" seconds="11" value="-33.153057">33° 09' 11&quot;S</geoCoordinate>
