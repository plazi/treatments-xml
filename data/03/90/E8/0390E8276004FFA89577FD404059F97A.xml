(Phi 213). – 
<collectingRegion box="[227,385,1564,1590]" country="Philippines" name="Laguna" pageId="59" pageNumber="60">Laguna Prov.</collectingRegion>
, 
<specimenCount box="[395,462,1564,1590]" pageId="59" pageNumber="60" type="male">2 ♂♂</specimenCount>
, 
<specimenCount box="[474,540,1564,1590]" pageId="59" pageNumber="60" type="female">2 ♀♀</specimenCount>
