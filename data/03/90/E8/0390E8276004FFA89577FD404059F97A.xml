<geoCoordinate box="[1007,1130,1564,1590]" degrees="14.110" direction="north" orientation="latitude" pageId="59" pageNumber="60" precision="55" value="14.11">14.110° N</geoCoordinate>
, 
<geoCoordinate box="[1140,1275,1563,1590]" degrees="121.507" direction="east" orientation="longitude" pageId="59" pageNumber="60" precision="55" value="121.507">121.507° E</geoCoordinate>
), 
<quantity box="[1295,1368,1563,1590]" metricMagnitude="2" metricUnit="m" metricValue="5.6" pageId="59" pageNumber="60" unit="m" value="560.0">560 m</quantity>
a.s.l., near ground, 
