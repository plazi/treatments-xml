<subSubSection pageId="15" pageNumber="16" type="description">
<paragraph blockId="15.[151,1436,1778,2004]" pageId="15" pageNumber="16">
<emphasis bold="true" box="[198,343,1898,1924]" pageId="15" pageNumber="16">Description</emphasis>
. Mature specimens ~ 
<quantity box="[595,664,1898,1924]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="15" pageNumber="16" unit="mm" value="4.0">4 mm</quantity>
