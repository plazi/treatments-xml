<paragraph blockId="107.[151,1436,1579,1748]" pageId="107" pageNumber="108">
<emphasis bold="true" box="[151,260,1579,1604]" pageId="107" pageNumber="108">Remarks</emphasis>
. Female 
<typeStatus box="[372,470,1579,1604]" pageId="107" pageNumber="108">holotype</typeStatus>
