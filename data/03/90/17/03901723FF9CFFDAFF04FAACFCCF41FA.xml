<taxonomicName authority="Sánchez-Ruiz &amp; Brescovit, 2018" authorityName="Sánchez-Ruiz &amp; Brescovit" authorityYear="2018" box="[151,316,1340,1366]" class="Arachnida" family="Caponiidae" genus="Nops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="navassa" status="new species">
