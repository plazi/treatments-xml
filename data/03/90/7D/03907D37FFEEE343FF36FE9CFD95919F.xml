<paragraph blockId="7.[151,1436,627,759]" pageId="7" pageNumber="36">
<taxonomicName box="[199,547,627,651]" class="Insecta" family="Psychodidae" genus="Bichromomyia" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Diptera" pageId="7" pageNumber="36" phylum="Arthropoda" rank="subSpecies" species="olmeca" subSpecies="olmeca">
<emphasis box="[199,547,627,651]" italics="true" pageId="7" pageNumber="36">Bichromomyia olmeca olmeca</emphasis>
</taxonomicName>
