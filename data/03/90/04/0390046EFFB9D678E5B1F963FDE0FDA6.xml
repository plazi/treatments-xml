<emphasis box="[506,686,152,178]" italics="true" pageId="22" pageNumber="173">Solanum hirtum</emphasis>
Vahl
</taxonomicName>
, 
<taxonomicName box="[758,867,153,179]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="22" pageNumber="173" phylum="Tracheophyta" rank="species" species="nigrum">
