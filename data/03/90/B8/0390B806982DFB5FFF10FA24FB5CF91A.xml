<taxonomicName authorityName="Dendy" authorityYear="1922" box="[595,752,765,791]" class="Demospongiae" family="Cladorhizidae" kingdom="Animalia" order="Poecilosclerida" pageId="12" pageNumber="269" phylum="Porifera" rank="family">Cladorhizidae</taxonomicName>
