<taxonomicName authority="(Fallen)" authorityName="Fallen" box="[844,1199,151,176]" class="Insecta" family="Cicadellidae" genus="Macrosteles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="266" phylum="Arthropoda" rank="species" species="sexnotatus">
