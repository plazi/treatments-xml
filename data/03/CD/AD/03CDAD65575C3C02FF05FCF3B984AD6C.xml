localities.
</emphasis>
<typeStatus box="[514,623,151,176]" pageId="37" pageNumber="138">Holotype</typeStatus>
: Central Lakes, Townsville, Queensland, 
<collectingCountry box="[1101,1206,151,176]" name="Australia" pageId="37" pageNumber="138">Australia</collectingCountry>
, along edges, under palm fronds and amongst mussels, 
<geoCoordinate box="[565,704,188,213]" direction="south" orientation="latitude" pageId="37" pageNumber="138" precision="15" value="-19.266945">
<number box="[565,594,188,213]" pageId="37" pageNumber="138" value="19.0">19</number>
° 
<number box="[604,634,188,213]" pageId="37" pageNumber="138" value="16.0">16</number>
'01&quot; S
</geoCoordinate>
, 
<geoCoordinate box="[721,876,188,213]" direction="east" orientation="longitude" pageId="37" pageNumber="138" precision="15" value="146.81694">
<number box="[721,765,188,213]" pageId="37" pageNumber="138" value="146.0">146</number>
° 
<number box="[775,806,188,213]" pageId="37" pageNumber="138" value="49.0">49</number>
'01&quot; E
</geoCoordinate>
, 
<date box="[893,1015,188,213]" pageId="37" pageNumber="138" value="1992-05">
May 
<number box="[955,1015,188,212]" pageId="37" pageNumber="138" value="1992.0">1992</number>
