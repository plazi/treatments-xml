<emphasis box="[869,895,1999,2025]" italics="true" pageId="86" pageNumber="87">C.</emphasis>
(
<emphasis box="[913,931,1999,2024]" italics="true" pageId="86" pageNumber="87">P</emphasis>
). 
<emphasis box="[955,1058,1998,2024]" italics="true" pageId="86" pageNumber="87">spelaeae</emphasis>
from 
<taxonomicName authorityName="Tuxen" authorityYear="1930" box="[1129,1315,1998,2025]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="suensoni" subGenus="Paurocampa">
