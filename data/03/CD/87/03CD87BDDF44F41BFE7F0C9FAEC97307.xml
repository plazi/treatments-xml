<taxonomicName authorityName="Kolibáč" authorityYear="2014" box="[631,727,1529,1552]" class="Insecta" family="Trogossitidae" genus="Afrocyrona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="208" phylum="Arthropoda" rank="species" species="pedestris">
