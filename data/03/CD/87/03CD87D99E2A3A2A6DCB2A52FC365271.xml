</taxonomicName>
(frontal view): 
<emphasis bold="true" box="[764,777,737,758]" pageId="4" pageNumber="405">1</emphasis>
, prosternum wide, 
<taxonomicName box="[990,1181,737,759]" class="Insecta" family="Hybotidae" genus="Syneches" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="405" phylum="Arthropoda" rank="species" species="curvipes">
<emphasis box="[990,1181,737,759]" italics="true" pageId="4" pageNumber="405">Syneches curvipes</emphasis>
</taxonomicName>
; 
<emphasis bold="true" box="[1201,1213,737,758]" pageId="4" pageNumber="405">2</emphasis>
, prosternum narrow, 
<taxonomicName box="[151,358,767,789]" class="Insecta" family="Hybotidae" genus="Syneches" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="405" phylum="Arthropoda" rank="species" species="moraballi">
