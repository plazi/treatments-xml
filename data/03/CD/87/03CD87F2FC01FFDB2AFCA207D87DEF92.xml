<taxonomicName authorityName="Conde" authorityYear="1957" box="[867,1136,594,621]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="44" pageNumber="45" phylum="Arthropoda" rank="subSpecies" species="majorica" subGenus="Campodea" subSpecies="sicula">
