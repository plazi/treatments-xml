<number box="[775,801,855,876]" pageId="12" pageNumber="287" value="23.0">23</number>
</figureCitation>
).................................................. 
<number box="[1412,1435,855,876]" pageId="12" pageNumber="287" value="11.0">11</number>
</keyLead>
</paragraph>
<paragraph blockId="12.[151,1436,151,1141]" box="[151,1435,885,906]" pageId="12" pageNumber="287">
