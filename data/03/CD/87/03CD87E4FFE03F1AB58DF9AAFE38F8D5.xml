<collectingCountry box="[308,390,1300,1321]" name="France" pageId="29" pageNumber="580">France</collectingCountry>
, 
<collectingCountry box="[409,515,1300,1321]" name="Germany" pageId="29" pageNumber="580">Germany</collectingCountry>
, 
<collectingCountry box="[535,618,1299,1321]" name="Mexico" pageId="29" pageNumber="580">Mexico</collectingCountry>
