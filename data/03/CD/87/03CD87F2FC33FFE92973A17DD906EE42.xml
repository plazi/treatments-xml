<taxonomicName authorityName="Silvestri" authorityYear="1912" box="[844,1028,806,833]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="plusiochaeta">
<emphasis box="[844,1028,806,833]" italics="true" pageId="30" pageNumber="31">C. plusiochaeta</emphasis>
