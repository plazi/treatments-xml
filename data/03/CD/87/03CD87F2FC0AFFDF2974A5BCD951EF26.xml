<subSubSection lastPageId="40" lastPageNumber="41" pageId="39" pageNumber="40" type="diagnosis">
<paragraph blockId="39.[189,554,1846,1873]" box="[189,554,1846,1873]" pageId="39" pageNumber="40">
<emphasis bold="true" box="[189,310,1846,1872]" pageId="39" pageNumber="40">Diagnosis</emphasis>
