<paragraph blockId="35.[160,1155,903,927]" box="[160,1155,903,927]" pageId="35" pageNumber="2023">
<typeStatus box="[160,265,903,927]" pageId="35" pageNumber="2023">Holotype</typeStatus>
: Female. Body length = 
<quantity box="[543,645,903,927]" metricMagnitude="-3" metricUnit="m" metricValue="1.59" pageId="35" pageNumber="2023" unit="mm" value="1.59">1.59 mm</quantity>
