<collectingCountry box="[1190,1306,1857,1883]" name="Myanmar" pageId="5" pageNumber="54">Myanmar</collectingCountry>
, 
<collectingCountry box="[1325,1432,1857,1883]" name="Thailand" pageId="5" pageNumber="54">Thailand</collectingCountry>
, 
<collectingCountry box="[151,272,1896,1922]" name="Cambodia" pageId="5" pageNumber="54">Cambodia</collectingCountry>
and 
<collectingCountry box="[328,442,1896,1922]" name="Indonesia" pageId="5" pageNumber="54">Indonesia</collectingCountry>
(
<collectingRegion box="[458,516,1896,1922]" country="Indonesia" name="Jawa Timur" pageId="5" pageNumber="54">Java</collectingRegion>
).
</paragraph>
</subSubSection>
