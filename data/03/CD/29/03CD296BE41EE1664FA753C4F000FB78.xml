to Narara at 
<geoCoordinate box="[657,757,1234,1260]" direction="south" orientation="latitude" pageId="29" pageNumber="30" precision="925" value="-33.383335">
<number box="[657,689,1234,1260]" pageId="29" pageNumber="30" value="33.0">33</number>
° 
<number box="[700,733,1234,1260]" pageId="29" pageNumber="30" value="23.0">23</number>
´S
</geoCoordinate>
. Collected between 
<date box="[993,1296,1234,1260]" pageId="29" pageNumber="30">
<number box="[993,1007,1234,1260]" pageId="29" pageNumber="30" value="2.0">2</number>
