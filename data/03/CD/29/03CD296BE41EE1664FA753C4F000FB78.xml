<collectingCountry box="[1365,1436,1197,1223]" name="United Kingdom" pageId="29" pageNumber="30">Wales</collectingCountry>
from Minyon Falls at 
<geoCoordinate box="[408,505,1234,1260]" direction="south" orientation="latitude" pageId="29" pageNumber="30" precision="925" value="-28.616667">
