<subSubSection pageId="66" pageNumber="67" type="materials_examined">
<paragraph blockId="66.[264,1323,1473,1899]" pageId="66" pageNumber="67">
<emphasis bold="true" box="[264,501,1473,1499]" pageId="66" pageNumber="67">Material examined</emphasis>
. 
<emphasis bold="true" box="[516,575,1473,1499]" pageId="66" pageNumber="67">
<collectingCountry box="[516,575,1473,1499]" name="United States of America" pageId="66" pageNumber="67">USA</collectingCountry>
</emphasis>
: 
<typeStatus box="[590,692,1473,1499]" pageId="66" pageNumber="67">holotype</typeStatus>
<collectionCode LSID="urn:lsid:biocol.org:col:34802" box="[700,785,1473,1499]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34802" name="Natural History Museum of Los Angeles County" pageId="66" pageNumber="67">LACM</collectionCode>
<collectionCode box="[792,878,1473,1499]" pageId="66" pageNumber="67">POLY­</collectionCode>
<number box="[878,933,1473,1499]" pageId="66" pageNumber="67" value="1176.0">1176</number>
, 
<taxonomicName box="[945,1067,1473,1499]" class="Hydrozoa" family="Stylasteridae" genus="Allopora" higherTaxonomySource="CoL" kingdom="Animalia" order="Anthoathecata" pageId="66" pageNumber="68" phylum="Cnidaria" rank="species" species="california">California</taxonomicName>
, Farnsworth Bank off Santa Catalina 
<collectingCountry box="[447,521,1513,1539]" name="Iceland" pageId="66" pageNumber="67">Island</collectingCountry>
