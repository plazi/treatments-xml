<paragraph blockId="16.[313,1268,1521,1736]" box="[313,440,1521,1544]" pageId="16" pageNumber="316">
<emphasis box="[313,440,1521,1544]" italics="true" pageId="16" pageNumber="316">Description</emphasis>
</paragraph>
<paragraph blockId="16.[313,1268,1521,1736]" box="[353,728,1552,1576]" pageId="16" pageNumber="316">
Based on 
<typeStatus box="[467,566,1552,1576]" pageId="16" pageNumber="316">holotype</typeStatus>
male, 
<quantity box="[646,724,1552,1576]" metricMagnitude="-2" metricUnit="m" metricValue="1.7" pageId="16" pageNumber="316" unit="mm" value="17.0">17 mm</quantity>
