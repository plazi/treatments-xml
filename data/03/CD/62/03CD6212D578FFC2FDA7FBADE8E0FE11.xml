<taxonomicName authority="(Haswell, 1879)" baseAuthorityName="Haswell" baseAuthorityYear="1879" box="[417,772,1375,1397]" class="Malacostraca" family="Ampithoidae" genus="Cymadusa" kingdom="Animalia" order="Amphipoda" pageId="27" pageNumber="327" phylum="Arthropoda" rank="species" species="setosa">
