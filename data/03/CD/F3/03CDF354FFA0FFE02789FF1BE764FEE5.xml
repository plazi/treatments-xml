<paragraph blockId="20.[151,976,151,178]" box="[151,976,151,178]" pageId="20" pageNumber="171">
<heading bold="true" box="[151,976,151,178]" fontSize="11" level="1" pageId="20" pageNumber="171" reason="1">
<emphasis bold="true" box="[151,976,151,178]" pageId="20" pageNumber="171">
<taxonomicName authority="(Verall, 1901)" baseAuthorityName="Verrall" baseAuthorityYear="1901" box="[151,604,151,178]" class="Insecta" family="Syrphidae" genus="Melangyna" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="171" phylum="Arthropoda" rank="species" species="labiatarum">
<emphasis bold="true" box="[151,429,151,178]" italics="true" pageId="20" pageNumber="171">Melangyna labiatarum</emphasis>
(Verall, 1901)
</taxonomicName>
/ 
<taxonomicName authority="(Verall, 1873)" baseAuthorityName="Verrall" baseAuthorityYear="1873" box="[628,976,151,177]" class="Insecta" family="Syrphidae" genus="Melangyna" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="171" phylum="Arthropoda" rank="species" species="compositarum">
