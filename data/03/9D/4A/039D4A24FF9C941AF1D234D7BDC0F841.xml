<taxonomicName authority="McLaughlin" authorityName="McLaughlin" authorityYear="2001" box="[299,906,1688,1713]" class="Malacostraca" family="Paguridae" genus="Pylopaguridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="markhami">
