<paragraph blockId="10.[151,1436,151,921]" pageId="10" pageNumber="65">
<emphasis box="[199,381,332,357]" italics="true" pageId="10" pageNumber="65">Ventral Region:</emphasis>
Epimeral setae 
<number box="[578,634,332,357]" pageId="10" pageNumber="65" value="8.0" valueMax="10.0" valueMin="6.0">6–10</number>
long, tapered, slightly roughened. Genital setae 
<number box="[1217,1231,332,357]" pageId="10" pageNumber="65" value="5.0">5</number>
pairs, 
<number box="[1318,1374,332,357]" pageId="10" pageNumber="65" value="8.0" valueMax="10.0" valueMin="6.0">6–10</number>
long acuminate, smooth. Aggenital and anal setae 
<number box="[656,726,367,391]" pageId="10" pageNumber="65" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
long, smooth, acuminate; 
<emphasis box="[1021,1049,367,392]" italics="true" pageId="10" pageNumber="65">ad</emphasis>
longest setae 
<number box="[1206,1279,367,392]" pageId="10" pageNumber="65" value="17.5" valueMax="20.0" valueMin="15.0">15–20</number>
, tapered, with few barbs, 
<emphasis box="[274,302,402,427]" italics="true" pageId="10" pageNumber="65">ad</emphasis>
<number box="[302,310,417,431]" pageId="10" pageNumber="65" value="3.0">
<emphasis box="[302,310,417,431]" italics="true" pageId="10" pageNumber="65">3</emphasis>
</number>
anterolateral to anal plate; 
<emphasis box="[616,652,402,427]" italics="true" pageId="10" pageNumber="65">iad</emphasis>
on level with seta 
<emphasis box="[862,890,403,426]" italics="true" pageId="10" pageNumber="65">an</emphasis>
<emphasis box="[890,898,417,431]" italics="true" pageId="10" pageNumber="65">
<number box="[890,898,417,431]" pageId="10" pageNumber="65" value="2.0">2</number>
