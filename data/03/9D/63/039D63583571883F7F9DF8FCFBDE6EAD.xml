<typeStatus box="[1386,1436,1869,1893]" pageId="8" pageNumber="331">type</typeStatus>
locality of 
<taxonomicName box="[272,394,1905,1929]" class="Insecta" family="Membracidae" genus="Hamma" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="331" phylum="Arthropoda" rank="species" species="mabirensis">
<emphasis box="[272,394,1905,1929]" italics="true" pageId="8" pageNumber="331">mabirensis</emphasis>
</taxonomicName>
, 
<bibRefCitation author="China" box="[408,542,1905,1930]" pageId="8" pageNumber="331" refString="China, W. E. (1923) A new species of Membracidae (Centrotinae) from Uganda. The Annals and Magazine of Natural History, Series 9, XI, 463 - 465." type="book chapter" year="1923">
<collectingCountry box="[408,475,1905,1930]" name="China" pageId="8" pageNumber="331">China</collectingCountry>
<number box="[482,542,1905,1929]" pageId="8" pageNumber="331" value="1923.0">1923</number>
</bibRefCitation>
