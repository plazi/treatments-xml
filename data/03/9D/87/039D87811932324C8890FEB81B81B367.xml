<collectingCountry box="[626,731,1207,1233]" name="Malaysia" pageId="8" pageNumber="94">Malaysia</collectingCountry>
, 
<collectingCountry box="[743,855,1207,1233]" name="Indonesia" pageId="8" pageNumber="94">Indonesia</collectingCountry>
