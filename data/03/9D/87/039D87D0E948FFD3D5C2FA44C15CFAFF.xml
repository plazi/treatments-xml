<emphasis box="[291,561,1552,1575]" italics="true" pageId="12" pageNumber="13">
<taxonomicName box="[291,402,1552,1575]" class="Hexanauplia" family="Cyclopidae" genus="Eucyclops" higherTaxonomySource="CoL" kingdom="Animalia" order="Cyclopoida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">Eucyclops</taxonomicName>
