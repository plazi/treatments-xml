<taxonomicName authority="Scortecci" authorityName="Scortecci" class="Reptilia" family="Typhlopidae" genus="Letheobia" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="146" phylum="Chordata" rank="species" species="erythraea">
