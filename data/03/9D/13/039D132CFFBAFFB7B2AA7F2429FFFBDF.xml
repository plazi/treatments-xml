<taxonomicName authority="Volynkin &amp; Černý, 2017" authorityName="Volynkin &amp; Černý" authorityYear="2017" box="[708,931,1016,1040]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="443" phylum="Arthropoda" rank="species" species="incompletostriga" status="sp. nov.">
<emphasis box="[708,931,1016,1040]" italics="true" pageId="2" pageNumber="443">B. incompletostriga</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[940,1031,1015,1040]" pageId="2" pageNumber="443" rank="species">
<emphasis bold="true" box="[940,1031,1015,1040]" pageId="2" pageNumber="443">sp. nov.</emphasis>
</taxonomicNameLabel>
or 
<taxonomicName authority="Volynkin &amp; Černý, 2017" authorityName="Volynkin &amp; Černý" authorityYear="2017" box="[1071,1204,1017,1040]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="443" phylum="Arthropoda" rank="species" species="rawanga" status="sp. nov.">
<emphasis box="[1071,1204,1017,1040]" italics="true" pageId="2" pageNumber="443">B. rawanga</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1212,1303,1015,1040]" pageId="2" pageNumber="443" rank="species">
<emphasis bold="true" box="[1212,1303,1015,1040]" pageId="2" pageNumber="443">sp. nov.</emphasis>
</taxonomicNameLabel>
, and not 
<taxonomicName authorityName="Hampson" authorityYear="1900" class="Insecta" family="Arctiidae" genus="Miltochrista" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="443" phylum="Arthropoda" rank="species" species="cardinalis">
<emphasis italics="true" pageId="2" pageNumber="443">B. cardinalis</emphasis>
</taxonomicName>
. Record for 
<collectingCountry box="[404,504,1052,1077]" name="Thailand" pageId="2" pageNumber="443">Thailand</collectingCountry>
and 
<collectingCountry box="[558,653,1052,1077]" name="Vietnam" pageId="2" pageNumber="443">Vietnam</collectingCountry>
(
<bibRefCitation author="Cerny" box="[667,943,1052,1077]" pageId="2" pageNumber="443" refString="Cerny, K. &amp; Pinratana, A. (2009) Moths of Thailand. Vol. 6. Arctiidae. Brothers of Saint Gabriel in Thailand, Bangkok, 283 pp." type="book" year="2009">Černý &amp; Pinratana 2009</bibRefCitation>
) belongs to both 
<taxonomicName authorityName="Volynkin &amp; Černý" authorityYear="2017" box="[1135,1358,1052,1076]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="443" phylum="Arthropoda" rank="species" species="pseudocardinalis">
<emphasis box="[1135,1358,1052,1076]" italics="true" pageId="2" pageNumber="443">B. pseudocardinalis</emphasis>
</taxonomicName>
and 
<taxonomicName authority="Volynkin &amp; Černý, 2017" authorityName="Volynkin &amp; Černý" authorityYear="2017" class="Insecta" family="Arctiidae" genus="Miltochrista" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="443" phylum="Arthropoda" rank="subSpecies" species="cardinalis" status="ssp. nov." subSpecies="gemina">
<emphasis italics="true" pageId="2" pageNumber="443">B. cardinalis gemina</emphasis>
