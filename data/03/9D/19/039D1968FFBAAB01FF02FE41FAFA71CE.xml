<number box="[197,311,1671,1695]" pageId="12" pageNumber="13" value="1.6" valueMax="1.63" valueMin="1.57">1.57–1.63</number>
, 
<number box="[323,375,1671,1695]" pageId="12" pageNumber="13" value="1.6">1.60</number>
; HV 
<number box="[434,549,1671,1695]" pageId="12" pageNumber="13" value="0.855" valueMax="0.92" valueMin="0.79">0.79–0.92</number>
, 
<number box="[560,613,1671,1695]" pageId="12" pageNumber="13" value="0.87">0.87</number>
.
</paragraph>
