(
<bibRefCitation author="Metcalf" box="[1171,1328,223,248]" pageId="11" pageNumber="512" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
Metcalf 
<number box="[1269,1328,223,248]" pageId="11" pageNumber="512" value="1963.0">1963</number>
</bibRefCitation>
; 
<bibRefCitation author="Sanborn" pageId="11" pageNumber="512" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book" year="2013">
Sanborn 
<number box="[151,211,260,284]" pageId="11" pageNumber="512" value="2013.0">2013</number>
</bibRefCitation>
), and 
<collectingCountry box="[277,389,260,285]" name="Argentina" pageId="11" pageNumber="512">Argentina</collectingCountry>
