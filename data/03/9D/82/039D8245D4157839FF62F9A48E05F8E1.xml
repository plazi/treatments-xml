<collectingCountry box="[950,1060,1826,1851]" name="Colombia" pageId="11" pageNumber="512">Colombia</collectingCountry>
and 
<collectingCountry box="[1131,1246,1826,1851]" name="Venezuela" pageId="11" pageNumber="512">Venezuela</collectingCountry>
(
