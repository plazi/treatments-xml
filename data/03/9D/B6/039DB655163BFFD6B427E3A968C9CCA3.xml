<taxonomicName authorityName="Santschi" baseAuthorityName="Santschi" baseAuthorityYear="1914" box="[271,529,1709,1732]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="362" phylum="Arthropoda" rank="species" species="transformans">
