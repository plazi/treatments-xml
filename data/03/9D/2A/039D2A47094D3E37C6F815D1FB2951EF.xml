(8) 
<emphasis bold="true" box="[190,295,692,717]" italics="true" pageId="15" pageNumber="116">A. arktos</emphasis>
versus
</emphasis>
<taxonomicName authority="(Thomas)" baseAuthorityName="Thomas" baseAuthorityYear="1904" box="[385,602,692,717]" class="Mammalia" family="Dasyuridae" genus="Antechinus" kingdom="Animalia" order="Dasyuromorphia" pageId="15" pageNumber="116" phylum="Chordata" rank="species" species="bellus">
