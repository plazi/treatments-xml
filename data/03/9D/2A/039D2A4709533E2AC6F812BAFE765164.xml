(16) 
<emphasis bold="true" box="[204,309,1503,1528]" italics="true" pageId="17" pageNumber="118">A. arktos</emphasis>
versus
</emphasis>
<taxonomicName authority="Van Dyck and Crowther" box="[399,844,1503,1528]" class="Mammalia" family="Dasyuridae" genus="Antechinus" kingdom="Animalia" order="Dasyuromorphia" pageId="17" pageNumber="118" phylum="Chordata" rank="species" species="subtropicus">
