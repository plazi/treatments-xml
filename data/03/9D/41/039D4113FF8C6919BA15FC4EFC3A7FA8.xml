<subSubSection pageId="23" pageNumber="380" type="description">
<paragraph blockId="23.[828,1437,1280,2077]" pageId="23" pageNumber="380">
<emphasis bold="true" box="[828,995,1670,1693]" pageId="23" pageNumber="380">Measurements</emphasis>
(in mm). ♁/ 
<specimenCount box="[1125,1144,1666,1694]" pageId="23" pageNumber="380" type="female">♀</specimenCount>
