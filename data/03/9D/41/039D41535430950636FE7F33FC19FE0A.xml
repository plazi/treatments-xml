<taxonomicName authority="Chamberlin, 1914" authorityName="Chamberlin" authorityYear="1914" box="[151,495,1811,1837]" class="Chilopoda" family="Mecistocephalidae" genus="Tygarrup" kingdom="Animalia" order="Geophilomorpha" pageId="67" pageNumber="68" phylum="Arthropoda" rank="genus">
