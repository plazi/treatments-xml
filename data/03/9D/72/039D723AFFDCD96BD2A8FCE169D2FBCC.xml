<specimenCount box="[611,677,1005,1031]" pageId="3" pageNumber="63" type="generic">
<quantity box="[611,674,1005,1031]" metricMagnitude="1" metricUnit="m" metricValue="1.1" pageId="3" pageNumber="63" unit="m" value="11.0">11 m</quantity>
.
</specimenCount>
</paragraph>
</subSubSection>
<subSubSection box="[198,1111,1042,1068]" pageId="3" pageNumber="63" type="discussion">
<paragraph blockId="3.[151,1436,893,1143]" box="[198,1111,1042,1068]" pageId="3" pageNumber="63">
<emphasis bold="true" box="[198,320,1042,1068]" pageId="3" pageNumber="63">Remarks.</emphasis>
