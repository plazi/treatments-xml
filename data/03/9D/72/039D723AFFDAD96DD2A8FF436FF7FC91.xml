<quantity box="[610,697,672,698]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.0" metricValueMin="0.8" pageId="5" pageNumber="65" unit="m" value="14.0" valueMax="20.0" valueMin="8.0">
8- 
<specimenCount box="[632,697,672,698]" pageId="5" pageNumber="65" type="generic">20 m</specimenCount>
