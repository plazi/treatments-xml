<paragraph blockId="11.[677,1209,1072,1160]" box="[677,821,1072,1098]" pageId="11" pageNumber="732">DESCRIPTION</paragraph>
<paragraph blockId="11.[677,1209,1072,1160]" pageId="11" pageNumber="732">
<quantity box="[677,803,1103,1130]" metricMagnitude="-2" metricUnit="m" metricValue="1.55" metricValueMax="1.6" metricValueMin="1.5" pageId="11" pageNumber="732" unit="mm" value="15.5" valueMax="16.0" valueMin="15.0">15-16 mm</quantity>
