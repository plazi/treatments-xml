<taxonomicName baseAuthorityName="O. Pickard-Cambridge" baseAuthorityYear="1871" box="[332,668,295,320]" class="Arachnida" family="Linyphiidae" genus="Porrhomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="microphthalmum">
