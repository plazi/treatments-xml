<taxonomicName authority="Růžička, 2018" authorityName="Růžička" authorityYear="2018" box="[399,518,800,825]" class="Arachnida" family="Linyphiidae" genus="Porrhomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="nekolai" status="new species">
