<taxonomicName authority="(SAUSSURE 1856)" baseAuthorityName="SAUSSURE" baseAuthorityYear="1856" box="[178,619,248,270]" class="Insecta" family="Eumenidae" genus="Microdynerus" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="943" phylum="Arthropoda" rank="species" species="timidus">
