<taxonomicName authorityName="Wailes" authorityYear="1925" box="[861,957,1976,2002]" class="Kinetofragminophora" family="Ephelotidae" genus="Ephelota" higherTaxonomySource="GBIF" kingdom="Chromista" order="Exogenida" pageId="7" pageNumber="158" phylum="Ciliophora" rank="species" species="plana">
