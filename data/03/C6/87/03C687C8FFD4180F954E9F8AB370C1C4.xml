<paragraph blockId="48.[189,1399,1783,2027]" lastBlockId="49.[189,1399,267,329]" lastPageId="49" lastPageNumber="50" pageId="48" pageNumber="49">
MEASUREMENTS. 
<specimenCount box="[379,401,1784,1810]" pageId="48" pageNumber="49" type="male">♂</specimenCount>
<typeStatus box="[409,515,1783,1809]" pageId="48" pageNumber="49">holotype</typeStatus>
