<emphasis box="[635,763,1096,1118]" italics="true" pageId="8" pageNumber="9">Hlavaciellus</emphasis>
</taxonomicName>
, males. 10, 
<taxonomicName box="[885,1016,1096,1118]" class="Insecta" family="Staphylinidae" genus="Hlavaciellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="decoratus" status="sp. n.">
<emphasis box="[885,1016,1096,1118]" italics="true" pageId="8" pageNumber="9">H. decoratus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1022,1079,1096,1117]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[1022,1079,1096,1117]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
