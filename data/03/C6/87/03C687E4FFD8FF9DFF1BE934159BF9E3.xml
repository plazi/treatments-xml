11, 
<taxonomicName box="[1123,1245,1097,1118]" class="Insecta" family="Staphylinidae" genus="Hlavaciellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="smetanai" status="sp. n.">
<emphasis box="[1123,1245,1097,1118]" italics="true" pageId="8" pageNumber="9">H. smetanai</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1252,1308,1096,1117]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[1252,1308,1096,1117]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
12, 
<taxonomicName class="Insecta" family="Staphylinidae" genus="Hlavaciellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="adelphos" status="sp. n.">
<emphasis italics="true" pageId="8" pageNumber="9">H. adelphos</emphasis>
