<paragraph blockId="6.[621,966,1540,1601]" box="[621,966,1540,1567]" pageId="6" pageNumber="7">
<heading box="[621,966,1540,1567]" centered="true" fontSize="11" level="2" pageId="6" pageNumber="7" reason="2">
<taxonomicName authorityName="De Deckker &amp; McKenzie" authorityYear="1981" box="[621,776,1540,1567]" class="Ostracoda" family="Cyprididae" genus="Bennelongia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
