<quantity box="[1033,1110,762,786]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="5" pageNumber="58" unit="m" value="800.0">800 m</quantity>
, 
<date box="[1125,1288,762,787]" pageId="5" pageNumber="58" value="2007-08-13">
