<taxonomicNameLabel box="[248,305,1722,1743]" pageId="17" pageNumber="18" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
31, 32, 
<taxonomicName box="[386,521,1722,1744]" class="Insecta" family="Staphylinidae" genus="Hlavaciellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="bifoveatus" status="sp. n.">
<emphasis box="[386,521,1722,1744]" italics="true" pageId="17" pageNumber="18">H. bifoveatus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[527,584,1722,1743]" pageId="17" pageNumber="18">
<taxonomicNameLabel box="[527,584,1722,1743]" pageId="17" pageNumber="18" rank="species">sp. n.</taxonomicNameLabel>
