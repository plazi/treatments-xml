<subSubSection pageId="7" pageNumber="6" type="etymology">
<paragraph blockId="7.[192,1441,354,410]" pageId="7" pageNumber="6">
<emphasis bold="true" box="[192,343,354,378]" pageId="7" pageNumber="6">Etymology.</emphasis>
