<taxonomicName box="[1020,1108,1169,1194]" class="Insecta" family="Phoridae" genus="Wharia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
