<number box="[1047,1062,1345,1369]" pageId="7" pageNumber="261" value="2.0">2</number>
)- 
<number box="[1079,1107,1345,1369]" pageId="7" pageNumber="261" value="15.0">15</number>
(
<number box="[1116,1131,1345,1369]" pageId="7" pageNumber="261" value="2.0">2</number>
)- 
<number box="[1149,1177,1345,1369]" pageId="7" pageNumber="261" value="15.0">15</number>
- 
<number box="[1185,1214,1345,1369]" pageId="7" pageNumber="261" value="12.0">12</number>
. Seta 
<emphasis box="[1280,1297,1345,1369]" italics="true" pageId="7" pageNumber="261">l’</emphasis>
absent from femur III. Setae 
<emphasis box="[333,357,1382,1406]" italics="true" pageId="7" pageNumber="261">l”</emphasis>
of genua 
<date box="[467,706,1381,1405]" pageId="7" pageNumber="261" value="1921-01-23" valueMax="1921-02-17" valueMin="1921-01-23">
I and II 
<number box="[556,626,1381,1405]" pageId="7" pageNumber="261" value="22.0" valueMax="23.0" valueMin="21.0">21–23</number>
