</taxonomicName>
(
<geoCoordinate box="[523,664,259,285]" degrees="27" direction="north" minutes="01" orientation="latitude" pageId="10" pageNumber="585" precision="15" seconds="32" value="27.025555">27°01’32”N</geoCoordinate>
; 
<geoCoordinate box="[676,812,259,286]" degrees="88" direction="east" minutes="14" orientation="longitude" pageId="10" pageNumber="585" precision="15" seconds="16" value="88.23778">88°14’16”E</geoCoordinate>
), 
<location LSID="urn:lsid:plazi:treatment:03C687A1FFBCFF9D91C2FE89D01CFD83:8EB0606CFFB3FF9D9214FEE5D254FEA3" box="[833,889,259,285]" country="India" county="Zoological Survey" latitude="27.025555" longLatPrecision="20" longitude="88.23778" municipality="Kolkata" name="State" pageId="10" pageNumber="585" stateProvince="West Bengal">State</location>
