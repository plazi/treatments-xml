<subSubSection pageId="16" pageNumber="17" type="description">
<paragraph blockId="16.[151,1437,266,2012]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[151,540,266,292]" pageId="16" pageNumber="17">Localities in the Indian Ocean</emphasis>
. 
<collectingCountry box="[557,661,266,292]" name="Tanzania" pageId="16" pageNumber="17">Zanzibar</collectingCountry>
Island (
<collectingCountry box="[762,874,266,292]" name="Tanzania" pageId="16" pageNumber="17">Tanzania</collectingCountry>
), mangrove area near Pete (southern region of Unguja), sea grass from a large exposed sea grass field (
<taxonomicName box="[857,971,306,331]" class="Liliopsida" family="Hydrocharitaceae" genus="Thalassia" kingdom="Animalia" order="Alismatales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="genus">
<emphasis box="[857,971,306,331]" italics="true" pageId="16" pageNumber="17">Thalassia</emphasis>
</taxonomicName>
) (
<date box="[1001,1137,306,332]" pageId="16" pageNumber="17" value="1995-08-16">
<number box="[1001,1029,306,332]" pageId="16" pageNumber="17" value="16.0">16</number>
/08/ 
<number box="[1078,1137,306,332]" pageId="16" pageNumber="17" value="1995.0">1995</number>
</date>
) (
<typeStatus box="[1165,1217,306,332]" pageId="16" pageNumber="17">type</typeStatus>
