<emphasis bold="true" box="[1348,1429,1495,1517]" pageId="24" pageNumber="287">sp. nov.</emphasis>
</taxonomicNameLabel>
, male holotype; 
<emphasis bold="true" box="[313,354,1526,1548]" pageId="24" pageNumber="287">g −h</emphasis>
, 
<taxonomicName authority="Grismado &amp; Carrión, 2017" authorityName="Grismado &amp; Carrión" authorityYear="2017" box="[369,578,1526,1548]" class="Arachnida" family="Gnaphosidae" genus="Almafuerte" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="287" phylum="Arthropoda" rank="species" species="vigorosa" status="sp. nov.">
<emphasis box="[369,578,1526,1548]" italics="true" pageId="24" pageNumber="287">Almafuerte vigorosa</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[592,675,1526,1548]" pageId="24" pageNumber="287" rank="species">
<emphasis bold="true" box="[592,675,1526,1548]" pageId="24" pageNumber="287">sp. nov.</emphasis>
</taxonomicNameLabel>
