<taxonomicName authorityName="Cherevichne" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[675,943,1722,1743]" class="Actinopterygii" family="Cyprinidae" genus="Scardinius" kingdom="Animalia" order="Cypriniformes" pageId="5" pageNumber="416" phylum="Chordata" rank="species" species="erythrophthalmus">
