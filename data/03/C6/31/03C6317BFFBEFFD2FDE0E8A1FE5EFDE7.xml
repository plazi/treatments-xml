(
<collectingRegion box="[274,370,1849,1875]" country="China" name="Yunnan" pageId="24" pageNumber="25">Yunnan</collectingRegion>
), 
<quantity box="[390,553,1849,1876]" metricMagnitude="3" metricUnit="m" metricValue="1.215" metricValueMax="1.35" metricValueMin="1.08" pageId="24" pageNumber="25" unit="m" value="1215.0" valueMax="1350.0" valueMin="1080.0">1080–1350 m</quantity>
