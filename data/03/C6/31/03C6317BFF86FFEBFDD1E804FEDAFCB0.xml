<taxonomicName authorityName="Wall. ex Horan. The" authorityYear="1862" box="[461,592,790,816]" class="Liliopsida" family="Zingiberaceae" genus="Globba" higherTaxonomySource="GBIF" kingdom="Plantae" order="Zingiberales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="expansa">
