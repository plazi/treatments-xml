<taxonomicName authority="Myers" authorityName="Myers" class="Insecta" family="Termitaphididae" genus="Termitaradus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="genus">
