<taxonomicName box="[1106,1260,1273,1298]" class="Arachnida" family="Eriophyidae" genus="Colopodacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="palaquius">
