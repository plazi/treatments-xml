<taxonomicName authority="Mohanasundaram, 1981" authorityName="Mohanasundaram" authorityYear="1981" class="Arachnida" family="Eriophyidae" genus="Colopodacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="begalensis">
<emphasis box="[1272,1436,1119,1144]" italics="true" pageId="5" pageNumber="54">C. begalensis</emphasis>
<bibRefCitation author="Mohanasundaram" box="[151,437,1157,1183]" pageId="5" pageNumber="62" refString="Mohanasundaram, M. (1981) New gall-mites of the subfamily Nothopodinae (Acarina: Eriophyidae) from India. Oriental Insects, 15 (2), 145 - 147." type="journal article" year="1981">
Mohanasundaram, 
<number box="[375,437,1157,1183]" pageId="5" pageNumber="54" value="1981.0">1981</number>
</bibRefCitation>
</taxonomicName>
; 
<taxonomicName authority="Huang &amp; Wang, 2003" authorityName="Huang &amp; Wang" authorityYear="2003" box="[449,867,1157,1183]" class="Arachnida" family="Eriophyidae" genus="Colopodacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="obovataus">
<emphasis box="[449,603,1157,1182]" italics="true" pageId="5" pageNumber="54">C. obovataus</emphasis>
<bibRefCitation author="Huang" box="[611,867,1157,1183]" pageId="5" pageNumber="62" refString="Huang, K. - W. &amp; Wang, C. - F. (2003) Eriophyoid mites of Taiwan: Description of thirteen species of Nothopodinae from Hueysuen (Acari: Eriophyoidea). Formosan Entomology, 23, 313 - 329." type="journal article" year="2003">
Huang &amp; Wang, 
<number box="[808,867,1157,1183]" pageId="5" pageNumber="54" value="2003.0">2003</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName authority="Ou, Zhu &amp; Wei, 2008" authorityName="Ou, Zhu &amp; Wei" authorityYear="2008" box="[924,1291,1157,1183]" class="Arachnida" family="Eriophyidae" genus="Colopodacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="virens">
<emphasis box="[924,1028,1157,1182]" italics="true" pageId="5" pageNumber="54">C. virens</emphasis>
<bibRefCitation author="Ou" box="[1035,1291,1157,1183]" pageId="5" pageNumber="62" refString="Ou S. - S., Zhu, H. &amp; Wei, S. - G. (2008) Three new species of Nothopodinae (Acari: Eriophyidae) from China. Acta Zootaxonomica Sinica, 33 (3), 475 - 479." type="journal article" year="2008">
Ou, Zhu &amp; Wei, 
<number box="[1231,1291,1157,1183]" pageId="5" pageNumber="54" value="2008.0">2008</number>
</bibRefCitation>
</taxonomicName>
) from 
<taxonomicName box="[1371,1436,1157,1182]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="5" pageNumber="54" phylum="Mollusca" rank="genus">
