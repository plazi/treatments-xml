<taxonomicName authority="Huang, 2001" authorityName="Huang" authorityYear="2001" box="[444,883,1234,1260]" class="Arachnida" family="Eriophyidae" genus="Colopodacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="palaquius">
