<paragraph blockId="3.[151,1436,1814,2034]" pageId="3" pageNumber="41">
<emphasis bold="true" box="[151,293,1814,1840]" pageId="3" pageNumber="41">Etymology.</emphasis>
<taxonomicName box="[301,402,1815,1840]" class="Insecta" family="Tropiduchidae" genus="Scolopsomorpha" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="41" phylum="Arthropoda" rank="species" species="africana">
<emphasis box="[301,402,1815,1840]" italics="true" pageId="3" pageNumber="41">africana</emphasis>
