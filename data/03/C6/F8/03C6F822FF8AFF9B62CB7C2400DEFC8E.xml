<collectingCountry box="[1001,1105,1930,1953]" name="Morocco" pageId="33" pageNumber="34">Morocco</collectingCountry>
, 
<collectingCountry box="[1117,1183,1929,1953]" name="Spain" pageId="33" pageNumber="34">Spain</collectingCountry>
