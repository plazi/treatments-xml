<taxonomicName baseAuthorityName="Walker" baseAuthorityYear="1869" box="[836,961,496,520]" class="Insecta" family="Gryllidae" genus="Velarifictorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="501" phylum="Arthropoda" rank="species" species="aspersus">
