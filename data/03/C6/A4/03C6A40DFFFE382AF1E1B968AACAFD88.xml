<taxonomicName authorityName="Saussure" authorityYear="1877" box="[1201,1310,496,520]" class="Insecta" family="Gryllidae" genus="Scapsipedus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="501" phylum="Arthropoda" rank="species" species="micado">
