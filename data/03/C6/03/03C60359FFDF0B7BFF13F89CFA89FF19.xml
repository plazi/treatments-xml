<subSubSection box="[199,1406,223,248]" pageId="14" pageNumber="455" type="etymology">
<paragraph blockId="14.[199,1406,151,248]" box="[199,1406,223,248]" pageId="14" pageNumber="455">
<emphasis bold="true" box="[199,334,223,248]" pageId="14" pageNumber="455">Etymology.</emphasis>
