<number box="[1105,1121,1722,1745]" pageId="2" pageNumber="48" value="5.0">5</number>
, head capsule, frontal view (palatum); 
<number box="[268,283,1756,1779]" pageId="2" pageNumber="48" value="6.0">6</number>
