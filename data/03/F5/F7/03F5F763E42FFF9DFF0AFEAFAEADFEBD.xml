<paragraph blockId="3.[151,1436,151,320]" pageId="3" pageNumber="489">
<emphasis bold="true" box="[199,314,151,176]" pageId="3" pageNumber="489">Diagnosis</emphasis>
. Length of body 
<quantity box="[517,657,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" metricValueMax="2.3" metricValueMin="2.1" pageId="3" pageNumber="489" unit="mm" value="2.2" valueMax="2.3" valueMin="2.1">2.1–2.3 mm</quantity>
