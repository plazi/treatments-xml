Figs 
<number box="[322,397,319,345]" pageId="11" pageNumber="12" value="18.14" valueMax="19.28" valueMin="17.0">17–19</number>
</figureCitation>
, 
<figureCitation box="[409,485,319,345]" captionStart="FIGURES 28 – 31" captionStartId="13.[264,383,1131,1155]" captionTargetBox="[296,1279,482,1105]" captionTargetId="figure@13.[270,1329,468,1123]" captionTargetPageId="13" captionText="FIGURES 28 – 31. Petrothrincus tsaratananensis, new species, holotype. Male genitalia: 28 — genitalia, lateral; 29 — genitalia, dorsal; 30 — genitalia, ventral; 31 — phallus, ventral." httpUri="https://zenodo.org/record/172381/files/figure.png" pageId="11" pageNumber="12">
28– 
<number box="[457,485,319,345]" pageId="11" pageNumber="12" value="31.59">31</number>
