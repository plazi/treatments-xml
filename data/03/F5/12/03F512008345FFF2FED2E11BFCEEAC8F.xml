<subSubSection pageId="5" pageNumber="6" type="nomenclature">
<paragraph blockId="5.[264,872,1324,1385]" box="[264,872,1324,1350]" pageId="5" pageNumber="6">
<heading bold="true" box="[264,872,1324,1350]" fontSize="11" level="1" pageId="5" pageNumber="6" reason="1">
<emphasis bold="true" box="[264,872,1324,1350]" pageId="5" pageNumber="6">
<taxonomicName box="[264,713,1324,1350]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="sahadika" status="new species" subGenus="Petrotrichia">
<emphasis bold="true" box="[264,713,1324,1350]" italics="true" pageId="5" pageNumber="6">Helicopsyche (Petrotrichia) sahadika</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[728,872,1324,1350]" pageId="5" pageNumber="6" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="5.[264,872,1324,1385]" box="[264,423,1359,1385]" pageId="5" pageNumber="6">
<figureCitation box="[264,381,1359,1385]" captionStart="FIGURES 7 – 11" captionStartId="6.[264,382,1275,1299]" captionTargetBox="[289,1314,326,1260]" captionTargetId="figure@6.[269,1328,318,1279]" captionTargetPageId="6" captionText="FIGURES 7 – 11. Helicopsyche (Petrotrichia) sahadika, new species, holotype. 7 — head and thorax, dorsal; 8 — right wing pair; 9 — genitalia, lateral; 10 — genitalia, dorsal; 11 — genitalia, ventral." httpUri="https://zenodo.org/record/172378/files/figure.png" pageId="5" pageNumber="6">
Figs 
<number box="[322,381,1359,1385]" pageId="5" pageNumber="6" value="9.29" valueMax="11.58" valueMin="7.0">7–11</number>
</figureCitation>
, 
<figureCitation box="[394,423,1359,1385]" captionStart="FIGURE 58" captionStartId="23.[264,368,1765,1789]" captionTargetBox="[418,1164,324,1727]" captionTargetId="figure@23.[396,1191,307,1742]" captionTargetPageId="23" captionText="FIGURE 58. Map of Madagascar showing distributions of Helicopsyche spp. (Helicopsychidae): H. ambodiva (circle); H. ninakosha and H. sahadika (rectangle); H. giboni (diamond); and H. anomana (triangle)." httpUri="https://zenodo.org/record/172389/files/figure.png" pageId="5" pageNumber="6">58</figureCitation>
</paragraph>
