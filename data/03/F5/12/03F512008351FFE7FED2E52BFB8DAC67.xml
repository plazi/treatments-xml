<taxonomicName box="[264,535,284,310]" class="Insecta" family="Petrothrincidae" genus="Petrothrincus" kingdom="Animalia" order="Trichoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="andohel" status="new species">
<emphasis bold="true" box="[264,535,284,310]" italics="true" pageId="17" pageNumber="18">Petrothrincus andohel</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[550,693,283,309]" pageId="17" pageNumber="18" rank="species">new species</taxonomicNameLabel>
