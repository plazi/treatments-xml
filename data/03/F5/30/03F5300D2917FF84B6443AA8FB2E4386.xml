<subSubSection pageId="1" pageNumber="22" type="diagnosis">
<paragraph blockId="1.[866,1479,574,696]" pageId="1" pageNumber="22">
<emphasis italics="true" pageId="1" pageNumber="22">
<taxonomicName box="[866,1115,574,598]" class="Liliopsida" family="Orchidaceae" genus="Pabstiella" kingdom="Plantae" order="Asparagales" pageId="1" pageNumber="22" phylum="Tracheophyta" rank="species" species="pseudotrifida">Pabstiella pseudotrifida</taxonomicName>
