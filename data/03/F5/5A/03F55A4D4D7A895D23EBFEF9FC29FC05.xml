<collectingCountry box="[537,671,646,672]" name="Kazakhstan" pageId="10">Kazakhstan</collectingCountry>
, 
<collectingCountry box="[683,750,646,672]" name="Spain" pageId="10">Spain</collectingCountry>
, 
<collectingCountry box="[761,874,646,672]" name="Tajikistan" pageId="10">Tajikistan</collectingCountry>
