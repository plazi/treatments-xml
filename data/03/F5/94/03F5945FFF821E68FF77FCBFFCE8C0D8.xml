and 
<specimenCode box="[623,759,972,994]" collectionCode="BNHS" country="India" httpUri="http://grbio.org/cool/8g20-8f2r" name="Bombay Natural History Society" pageId="41" pageNumber="415">BNHS 4587</specimenCode>
