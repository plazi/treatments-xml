<specimenCode box="[392,612,1820,1841]" collectionCode="BNHS" country="India" httpUri="http://grbio.org/cool/8g20-8f2r" name="Bombay Natural History Society" pageId="22" pageNumber="396">BNHS 4411–4413</specimenCode>
, 
<specimenCount box="[637,778,1820,1842]" pageId="22" pageNumber="396" type="adult">three adult</specimenCount>
