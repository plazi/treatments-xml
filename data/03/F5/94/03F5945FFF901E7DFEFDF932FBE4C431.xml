<materialsCitation collectingDate="1999-08-13" collectionCode="SVL, SDB, MN" country="India" county="Medium" location="Andiparai Shola" municipality="Description" pageId="59" pageNumber="433" specimenCode="BNHS 4544, HW 10.3, MBE 3.2, EL 3.7, IFE 5.5, IBE 9.5, TYD 1.1" specimenCount="2" stateProvince="Tamil Nadu" typeStatus="holotype">
