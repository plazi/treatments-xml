<paragraph blockId="1.[151,1435,1701,1788]" box="[151,1088,1765,1788]" pageId="1" pageNumber="2">
<emphasis box="[151,284,1765,1788]" italics="true" pageId="1" pageNumber="2">
<typeStatus box="[151,201,1765,1788]" pageId="1" pageNumber="2">Type</typeStatus>
