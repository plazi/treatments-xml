(Rudescu, 
<number box="[1354,1422,1002,1028]" pageId="3" pageNumber="4" value="1967.0">1967</number>
)
</taxonomicName>
, 
<taxonomicName authority="Daday, 1901" authorityName="Daday" authorityYear="1901" box="[151,623,1041,1067]" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="3" pageNumber="4" phylum="Gastrotricha" rank="species" species="ornatus" subGenus="Lepidochaetus">
