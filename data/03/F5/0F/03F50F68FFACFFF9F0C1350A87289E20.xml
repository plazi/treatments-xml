(Daday, 
<number box="[553,615,1041,1067]" pageId="3" pageNumber="4" value="1901.0">1901</number>
)
</taxonomicName>
, and 
<taxonomicName authority="Grunspan, 1908" authorityName="Grunspan" authorityYear="1908" box="[685,1200,1041,1067]" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="3" pageNumber="4" phylum="Gastrotricha" rank="species" species="zelinkai" subGenus="Lepidochaetus">
<emphasis box="[685,982,1041,1066]" italics="true" pageId="3" pageNumber="4">Chaetonotus (L.) zelinkai</emphasis>
