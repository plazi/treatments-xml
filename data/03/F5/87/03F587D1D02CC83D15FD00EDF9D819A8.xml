<subSubSection pageId="9" pageNumber="384" type="nomenclature">
<paragraph blockId="9.[151,286,152,211]" box="[151,286,152,178]" pageId="9" pageNumber="384">
<heading bold="true" box="[151,286,152,178]" fontSize="11" level="1" pageId="9" pageNumber="384" reason="1">
<emphasis bold="true" box="[151,286,152,178]" pageId="9" pageNumber="384">
<taxonomicName box="[151,243,152,178]" class="Liliopsida" family="Orchidaceae" genus="Acineta" kingdom="Plantae" order="Asparagales" pageId="9" pageNumber="384" phylum="Tracheophyta" rank="genus">
<emphasis bold="true" box="[151,243,152,178]" italics="true" pageId="9" pageNumber="384">Acineta</emphasis>
