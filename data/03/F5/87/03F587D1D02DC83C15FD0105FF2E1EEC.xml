<quantity box="[263,323,1194,1219]" metricMagnitude="1" metricUnit="m" metricValue="6.3" pageId="8" pageNumber="383" unit="m" value="63.0">
<elevation box="[263,323,1194,1219]" metricMagnitude="1" metricUnit="m" metricValue="6.3" pageId="8" pageNumber="383" unit="m" value="63.0">63 m</elevation>
</quantity>
