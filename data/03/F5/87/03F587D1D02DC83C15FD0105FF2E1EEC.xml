<collectingRegion box="[445,543,1122,1147]" country="Russia" name="Sakhalin" pageId="8" pageNumber="383">Sakhalin</collectingRegion>
and 
<location LSID="urn:lsid:plazi:treatment:03F587D1D02DC83C15FD0105FF2E1EEC:8E83601CD02DC83C17360417F8261E70" box="[604,806,1122,1147]" county="Host" municipality="Jankowski" name="Kunashyr Islands" pageId="8" pageNumber="383" stateProvince="Sakhalin">Kunashyr Islands</location>
