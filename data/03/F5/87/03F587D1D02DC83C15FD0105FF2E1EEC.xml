<location LSID="urn:lsid:plazi:treatment:03F587D1D02DC83C15FD0105FF2E1EEC:8E83601CD02DC83C140704F3FA991E94" box="[365,409,1158,1183]" county="Host" municipality="Jankowski" name="The" pageId="8" pageNumber="383" stateProvince="Sakhalin">The</location>
