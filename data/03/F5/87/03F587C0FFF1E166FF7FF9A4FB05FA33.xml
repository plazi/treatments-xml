<bibRefCitation author="Agardh, J. G." box="[1043,1275,886,912]" pageId="5" pageNumber="27" pagination="701 - 786" refId="ref12610" refString="Agardh, J. G. (1852) Species genera et ordines algarum. C. W. K. Gleerup, Lund Part 3, fasc. 1. pp. 701 - 786." type="book chapter" year="1852">Agardh (1852: 753)</bibRefCitation>
</taxonomicName>
and 
<taxonomicName authorityName="Nam" authorityYear="2007" baseAuthorityName="Bory de Saint-Vincent" class="Florideophyceae" family="Rhodomelaceae" genus="Palisada" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ceramiales" pageId="5" pageNumber="27" phylum="Rhodophyta" rank="species" species="perforata">
<emphasis italics="true" pageId="5" pageNumber="27">Palisada perforata</emphasis>
</taxonomicName>
. In addition, we observed 
<taxonomicName authorityName="Linnaeus (1753:" authorityYear="1166" box="[576,712,925,950]" class="Ulvophyceae" family="Cladophoraceae" genus="Conferva" higherTaxonomySource="CoL" kingdom="Chromista" order="Cladophorales" pageId="5" pageNumber="27" phylum="Chlorophyta" rank="species" species="catenata">
<emphasis box="[576,712,925,950]" italics="true" pageId="5" pageNumber="27">C. catenata</emphasis>
</taxonomicName>
as a host to 
<taxonomicName authorityName="Agardh" authorityYear="1823" baseAuthorityName="Wulfen" box="[860,978,925,950]" class="Ulvophyceae" family="Anadyomenaceae" genus="Anadyomene" higherTaxonomySource="GBIF,IPNI" kingdom="Plantae" order="Cladophorales" pageId="5" pageNumber="27" phylum="Chlorophyta" rank="species" species="stellata">
<emphasis box="[860,978,925,950]" italics="true" pageId="5" pageNumber="27">A. stellata</emphasis>
</taxonomicName>
, 
<taxonomicName authority="Kutzing (1843 b: 385)" authorityName="Kutzing" authorityYear="1843" class="Florideophyceae" family="Corallinaceae" genus="Pneophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Corallinales" pageId="5" pageNumber="27" phylum="Rhodophyta" rank="species" species="fragile">
