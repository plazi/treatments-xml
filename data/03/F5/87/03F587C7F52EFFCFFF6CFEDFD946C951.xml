<date box="[1348,1436,1715,1739]" pageId="18" pageNumber="469" value="2010-01-11">1 10–11</date>
(10), Mp2-Lp 
<date box="[307,399,1751,1775]" pageId="18" pageNumber="469" value="1919-02-19">2 19–19</date>
(19), Mp1-Mp 
<date box="[572,664,1751,1775]" pageId="18" pageNumber="469" value="1917-02-18">2 17–18</date>
(18), Lp1-Lp 
<date box="[821,914,1751,1775]" pageId="18" pageNumber="469" value="1933-02-31">2 31–33</date>
(32), Mp 
<date box="[1029,1117,1751,1775]" pageId="18" pageNumber="469" value="2013-01-18">1 13–18</date>
(16), Mp 
<date box="[1228,1320,1751,1775]" pageId="18" pageNumber="469" value="1917-02-17">2 17–17</date>
(17), Lp 
<date pageId="18" pageNumber="469" value="1927-01-28">1 27–28</date>
