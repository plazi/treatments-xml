<typeStatus box="[151,210,1869,1894]" pageId="12" pageNumber="13">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Huber" authorityName="Huber" box="[313,687,1869,1894]" class="Insecta" family="Mymaridae" genus="Archigonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="balticus">
