<paragraph blockId="29.[264,1323,524,630]" box="[264,368,524,550]" pageId="29" pageNumber="30">Remarks</paragraph>
<paragraph blockId="29.[264,1323,524,630]" pageId="29" pageNumber="30">
<taxonomicName box="[312,524,564,589]" class="Insecta" family="Cixiidae" genus="Oteana" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="eurynome">
