<paragraph blockId="29.[264,1323,1044,1873]" box="[264,400,1044,1070]" pageId="29" pageNumber="30">Description</paragraph>
<paragraph blockId="29.[264,1323,1044,1873]" box="[312,464,1084,1110]" pageId="29" pageNumber="30">Body length.</paragraph>
<paragraph blockId="29.[264,1323,1044,1873]" box="[312,966,1124,1150]" pageId="29" pageNumber="30">
<collectingCountry box="[312,376,1124,1150]" name="Malta" pageId="29" pageNumber="30">Male</collectingCountry>
. 
<quantity box="[387,543,1124,1150]" metricMagnitude="-3" metricUnit="m" metricValue="5.05" metricValueMax="5.3" metricValueMin="4.8" pageId="29" pageNumber="30" unit="mm" value="5.05" valueMax="5.3" valueMin="4.8">4.8 – 5.3 mm</quantity>
(n= 
<number box="[593,609,1124,1150]" pageId="29" pageNumber="30" value="4.0">4</number>
