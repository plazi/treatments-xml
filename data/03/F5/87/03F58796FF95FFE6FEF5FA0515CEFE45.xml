<subSubSection pageId="5" pageNumber="177" type="etymology">
<paragraph blockId="5.[151,756,1877,1933]" pageId="5" pageNumber="177">
<emphasis bold="true" box="[151,284,1877,1901]" pageId="5" pageNumber="177">Etymology:</emphasis>
