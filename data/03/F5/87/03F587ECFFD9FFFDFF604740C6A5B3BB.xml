<quantity box="[445,547,1209,1233]" metricMagnitude="-3" metricUnit="m" metricValue="5.85" pageId="2" pageNumber="45" unit="mm" value="5.85">5.85 mm</quantity>
long, 
<quantity box="[622,723,1209,1233]" metricMagnitude="-3" metricUnit="m" metricValue="1.54" pageId="2" pageNumber="45" unit="mm" value="1.54">1.54 mm</quantity>
wide across eyes. Female 
<quantity box="[1032,1196,1209,1233]" metricMagnitude="-3" metricUnit="m" metricValue="6.525" metricValueMax="6.6" metricValueMin="6.45" pageId="2" pageNumber="45" unit="mm" value="6.525" valueMax="6.6" valueMin="6.45">6.45–6.60 mm</quantity>
long, 
<quantity box="[1271,1435,1209,1233]" metricMagnitude="-3" metricUnit="m" metricValue="1.81" metricValueMax="1.85" metricValueMin="1.77" pageId="2" pageNumber="45" unit="mm" value="1.81" valueMax="1.85" valueMin="1.77">1.77–1.85 mm</quantity>
wide across eyes.
</paragraph>
