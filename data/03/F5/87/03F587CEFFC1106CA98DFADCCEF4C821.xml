<taxonomicName authorityName="Thompson" authorityYear="1913" box="[946,1025,1578,1603]" class="Reptilia" family="Natricidae" genus="Hebius" higherTaxonomySource="CoL" kingdom="Animalia" order="Squamata" pageId="12" pageNumber="437" phylum="Chordata" rank="genus">
