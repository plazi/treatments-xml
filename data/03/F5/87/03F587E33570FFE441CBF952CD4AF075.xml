<subSubSection pageId="31" pageNumber="32" type="materials_examined">
<paragraph blockId="31.[151,1436,1831,2035]" pageId="31" pageNumber="32">
<emphasis bold="true" box="[151,317,1831,1856]" pageId="31" pageNumber="32">
