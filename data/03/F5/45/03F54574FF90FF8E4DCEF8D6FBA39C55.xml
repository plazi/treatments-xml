, 
<taxonomicName authority="Shiino, 1951" authorityName="Shiino" authorityYear="1951" class="Malacostraca" family="Bopyridae" genus="Pseudione" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="274" phylum="Arthropoda" rank="species" species="nephropsi">
