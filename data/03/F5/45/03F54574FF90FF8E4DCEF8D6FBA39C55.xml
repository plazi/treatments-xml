), 
<taxonomicName authority="Bourdon, 1971" authorityName="Bourdon" authorityYear="1971" box="[825,1131,943,968]" class="Malacostraca" family="Bopyridae" genus="Pseudione" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="274" phylum="Arthropoda" rank="species" species="atlantica">
