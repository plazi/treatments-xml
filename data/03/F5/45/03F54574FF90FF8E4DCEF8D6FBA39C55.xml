, and 
<taxonomicName authorityName="Markham" authorityYear="1999" box="[198,383,1088,1112]" class="Malacostraca" family="Bopyridae" genus="Pseudione" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="274" phylum="Arthropoda" rank="species" species="tanimbarensis">
