</taxonomicName>
, and 
<taxonomicName authorityName="Page" authorityYear="1985" box="[469,654,1053,1076]" class="Malacostraca" family="Bopyridae" genus="Pseudione" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="274" phylum="Arthropoda" rank="species" species="murawaiensis">
