<taxonomicName authority="Page, 1985" authorityName="Page" authorityYear="1985" box="[1071,1400,980,1005]" class="Malacostraca" family="Bopyridae" genus="Pseudione" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="274" phylum="Arthropoda" rank="species" species="murawaiensis">
