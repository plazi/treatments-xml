. 
<emphasis box="[151,238,1052,1076]" italics="true" pageId="23" pageNumber="274">hanseni</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Shiino" authorityYear="1937" box="[252,407,1052,1076]" class="Malacostraca" family="Bopyridae" genus="Pseudione" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="274" phylum="Arthropoda" rank="species" species="longicauda">
