<paragraph blockId="4.[151,527,642,703]" box="[151,397,677,703]" pageId="4" pageNumber="64">
(
<figureCitation box="[159,242,677,703]" captionStart="FIGURES 1 – 3" captionStartId="2.[151,271,1835,1859]" captionTargetBox="[363,1211,262,1805]" captionTargetId="figure@2.[350,1227,246,1821]" captionTargetPageId="2" captionText="FIGURES 1 – 3. Wings. 1, Spartimas ornatipes Enderlein, male. 2, Spartimas apiciniger sp. nov., male. 3, Spartimas hainanensis sp. nov., male." httpUri="https://zenodo.org/record/196684/files/figure.png" pageId="4" pageNumber="64">Figs. 3</figureCitation>
, 
<figureCitation box="[255,271,677,703]" captionStart="FIGURES 4 – 6" captionStartId="2.[151,271,1899,1923]" captionText="FIGURES 4 – 6. Heads and thoraces, lateral view. 4, Spartimas ornatipes Enderlein, male. 5, Spartimas apiciniger sp. nov., male. 6, Spartimas hainanensis sp. nov., male." pageId="4" pageNumber="64">6</figureCitation>
, 
<figureCitation box="[284,301,677,703]" captionStart="FIGURES 7 – 9" captionStartId="2.[151,275,1963,1987]" captionText="FIGURES 7 – 9. Hind femora, tibiae and tarsi, lateral view. 7, Spartimas ornatipes Enderlein, male. 8, Spartimas apiciniger sp. nov., male. 9, Spartimas hainanensis sp. nov., male." pageId="4" pageNumber="64">9</figureCitation>
, 
<figureCitation box="[314,388,677,703]" captionStart="FIGURES 14 – 17" captionStartId="5.[151,269,1681,1705]" captionTargetBox="[157,1434,397,1650]" captionTargetId="figure@5.[157,1436,383,1668]" captionTargetPageId="5" captionText="FIGURES 14 – 17. Spartimas hainanensis sp. nov., male. 14, tergites 9 – 10 and cercus, dorsal view. 15, tergites 9 – 10 and cercus, ventral view. 16, genital capsule, dorsal view. 17, genital capsule, ventral view." httpUri="https://zenodo.org/record/196686/files/figure.png" pageId="4" pageNumber="64">
14– 
<number box="[360,388,677,703]" pageId="4" pageNumber="64" value="17.0">17</number>
</figureCitation>
)
