</taxonomicName>
(
<taxonomicName box="[604,727,1954,1978]" class="Insecta" family="Tortricidae" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="family">Tortricidae</taxonomicName>
), 
<taxonomicName authority="Deni &amp; Schiffermuller" authorityName="Deni &amp; Schiffermuller" box="[751,1239,1954,1978]" class="Insecta" family="Tortricidae" genus="Pandemis" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="species" species="heparana">
<emphasis box="[751,972,1954,1978]" italics="true" pageId="11" pageNumber="75">Pandemis heparana</emphasis>
