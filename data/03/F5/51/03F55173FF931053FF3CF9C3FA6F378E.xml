(
<taxonomicName box="[1272,1392,1954,1978]" class="Insecta" family="Tortricidae" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="family">Tortricidae</taxonomicName>
) and 
<taxonomicName authority="Heinrich" authorityName="Heinrich" box="[136,477,1988,2012]" class="Insecta" family="Tortricidae" genus="Grapholitha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="species" species="inopinata">
<emphasis box="[136,377,1988,2012]" italics="true" pageId="11" pageNumber="75">Grapholitha inopinata</emphasis>
Heinrich
