; 
<specimenCount box="[682,703,223,249]" pageId="51" pageNumber="52" type="female">♀</specimenCount>
<quantity box="[713,795,223,249]" metricMagnitude="-3" metricUnit="m" metricValue="9.4" pageId="51" pageNumber="52" unit="mm" value="9.4">9.4mm</quantity>
. 
<emphasis bold="true" box="[809,873,223,249]" pageId="51" pageNumber="52">Head</emphasis>
. HW 
