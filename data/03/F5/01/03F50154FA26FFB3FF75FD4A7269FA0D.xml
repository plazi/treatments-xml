<subSubSection box="[151,1140,787,812]" pageId="4" pageNumber="41" type="materials_examined">
<paragraph blockId="4.[151,1437,787,2036]" box="[151,1140,787,812]" pageId="4" pageNumber="41">
<emphasis bold="true" box="[151,306,787,812]" pageId="4" pageNumber="41">
<typeStatus box="[151,210,788,812]" pageId="4" pageNumber="41">Type</typeStatus>
species.
</emphasis>
<taxonomicName box="[313,592,788,812]" class="Malacostraca" family="Hypsimetopidae" genus="Andhracoides" kingdom="Animalia" order="Isopoda" pageId="4" pageNumber="41" phylum="Arthropoda" rank="species" species="shabuddin" status="sp. nov.">
<emphasis box="[313,592,788,812]" italics="true" pageId="4" pageNumber="41">Andhracoides shabuddin</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[606,695,788,812]" pageId="4" pageNumber="41" rank="species">
