<typeStatus box="[317,366,1642,1664]" pageId="1" pageNumber="6">Type</typeStatus>
species: 
<taxonomicName authority="EWING, 1909" authorityName="EWING" authorityYear="1909" box="[454,758,1642,1664]" class="Arachnida" family="Phenopelopidae" genus="Pelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="6" phylum="Arthropoda" rank="species" species="bifurcatus">
<emphasis box="[454,623,1642,1664]" italics="true" pageId="1" pageNumber="6">Pelops bifurcatus</emphasis>
<bibRefCitation author="EWING, H. E." box="[629,758,1642,1664]" pageId="1" pageNumber="6" pagination="116 - 136" refId="ref4243" refString="EWING, H. E. (1909) New American Oribatoidea. J. N. Y. Ent. Soc. 17: 116 - 136." type="journal article" year="1909">EWING, 1909</bibRefCitation>
