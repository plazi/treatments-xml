<collectionCode box="[1015,1070,929,953]" country="Madagascar" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13842" lsid="urn:lsid:biocol.org:col:13842" name="Parc de Tsimbazaza" pageId="27" pageNumber="304" type="Herbarium">TAN</collectionCode>
