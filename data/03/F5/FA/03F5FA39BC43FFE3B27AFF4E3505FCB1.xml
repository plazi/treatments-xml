, 
<collectionCode box="[813,848,929,952]" country="USA" lsid="urn:lsid:biocol.org:col:15556" name="William and Lynda Steere Herbarium of the New York Botanical Garden" pageId="27" pageNumber="304" type="Herbarium">NY</collectionCode>
