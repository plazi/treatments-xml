<paragraph blockId="18.[151,1437,442,1042]" pageId="18" pageNumber="22">
<emphasis bold="true" box="[151,307,442,467]" pageId="18" pageNumber="22">
<typeStatus box="[151,208,443,467]" pageId="18" pageNumber="22">Type</typeStatus>
locality
</emphasis>
. 
