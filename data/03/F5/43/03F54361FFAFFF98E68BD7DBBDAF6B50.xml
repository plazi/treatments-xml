<emphasis box="[230,381,1181,1205]" italics="true" pageId="23" pageNumber="27">bergvleitanus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[438,585,1181,1205]" class="Clitellata" family="Megascolecidae" genus="Chilota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="23" pageNumber="27" phylum="Annelida" rank="species" species="laeviseta">
<emphasis box="[438,585,1181,1205]" italics="true" pageId="23" pageNumber="27">
<collectingCountry box="[438,474,1181,1205]" name="Switzerland" pageId="23" pageNumber="27">Ch</collectingCountry>
. laeviseta
</emphasis>
