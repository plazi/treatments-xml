, Riversdale Mts at ca. 
<quantity box="[621,758,412,436]" metricMagnitude="2" metricUnit="m" metricValue="7.25" metricValueMax="10.0" metricValueMin="4.5" pageId="6" pageNumber="10" unit="m" value="725.0" valueMax="1000.0" valueMin="450.0">450-1000 m</quantity>
