<taxonomicName box="[350,544,224,248]" class="Clitellata" family="Acanthodrilidae" genus="Parachilota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="31" pageNumber="35" phylum="Annelida" rank="subSpecies" species="paishi" subSpecies="cerensis">
