<subSubSection pageId="14" pageNumber="15" type="etymology">
<paragraph blockId="14.[151,1436,1762,2002]" pageId="14" pageNumber="15">
<emphasis box="[199,333,1762,1786]" inLineHeading="true" pageId="14" pageNumber="15" reason="1">
