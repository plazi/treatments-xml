(
<figureCitation box="[159,259,319,344]" captionStart="FIGURE 3" captionStartId="5.[151,250,1923,1945]" captionTargetBox="[248,1337,443,1899]" captionTargetId="figure@5.[248,1339,439,1901]" captionTargetPageId="5" captionText="FIGURE 3. Female, dorsal apotomes, in anterior view. (A) Culicoides occidentalis. (B) C. riethi. (C) C. sonorensis. (D) C. variipennis." httpUri="https://zenodo.org/record/2606561/files/figure.png" pageId="16" pageNumber="467">Figs. 3D</figureCitation>
, 
<figureCitation box="[270,304,320,344]" captionStart="FIGURE 7" captionStartId="11.[151,250,1791,1813]" captionTargetBox="[249,1337,193,1770]" captionTargetId="figure@11.[249,1338,193,1770]" captionTargetPageId="11" captionText="FIGURE 7. Male, lateral and dorsal tubercles of abdominal segments 4, in dorsal view (A) Culicoides sonorensis. (B) C. variipennis." httpUri="https://zenodo.org/record/2606569/files/figure.png" pageId="16" pageNumber="467">7B</figureCitation>
