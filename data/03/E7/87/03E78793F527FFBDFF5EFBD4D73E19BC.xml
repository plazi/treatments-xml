; tibia 
<number box="[1194,1208,511,535]" pageId="4" pageNumber="169" value="1.0">1</number>
, 
<number box="[1224,1277,511,535]" pageId="4" pageNumber="169" value="2.52">2.52</number>
; tibia 
<number box="[1349,1365,511,535]" pageId="4" pageNumber="169" value="2.0">2</number>
, 
<number box="[1380,1432,511,535]" pageId="4" pageNumber="169" value="2.31">2.31</number>
; tarsus, 
<number box="[230,282,548,572]" pageId="4" pageNumber="169" value="0.09">0.09</number>
; propodus, 1.0; min claw, 
<number box="[578,630,548,572]" pageId="4" pageNumber="169" value="0.39">0.39</number>
; auxiliary claw, 
<number box="[813,866,548,572]" pageId="4" pageNumber="169" value="0.27">0.27</number>
.
</paragraph>
