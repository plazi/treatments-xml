<subSubSection pageId="22" pageNumber="252" type="biology_ecology">
<paragraph blockId="22.[170,1437,194,808]" pageId="22" pageNumber="252">
<emphasis box="[170,271,268,293]" italics="true" pageId="22" pageNumber="252">Standort</emphasis>
