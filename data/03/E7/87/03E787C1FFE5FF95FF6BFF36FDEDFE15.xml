<taxonomicName box="[330,509,1822,1848]" class="Insecta" family="Archisargidae" genus="Calosargus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="validus" status="sp. nov." subGenus="Calosargus">
