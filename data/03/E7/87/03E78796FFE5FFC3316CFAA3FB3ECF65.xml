<taxonomicName box="[1122,1252,1084,1109]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="10" pageNumber="31" phylum="Tracheophyta" rank="genus">
