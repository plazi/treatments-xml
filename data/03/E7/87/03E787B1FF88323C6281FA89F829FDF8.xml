<subSubSection box="[199,1171,367,394]" pageId="9" pageNumber="460" type="etymology">
<paragraph blockId="9.[151,1437,151,537]" box="[199,1171,367,394]" pageId="9" pageNumber="460">
<emphasis bold="true" box="[199,328,367,393]" pageId="9" pageNumber="460">Etymology</emphasis>
