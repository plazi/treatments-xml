<specimenCode box="[330,459,1029,1055]" collectionCode="ZMB" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="34" pageNumber="35" type="Museum">ZMB 4368</specimenCode>
</materialsCitation>
<materialsCitation collectionCode="ZMB" county="Hotha" location="Dobson" municipality="These" pageId="34" pageNumber="35" specimenCount="2" specimenCount-adult="1" stateProvince="Yunnan">
