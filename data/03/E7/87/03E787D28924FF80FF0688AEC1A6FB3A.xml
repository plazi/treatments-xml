<collectingCounty box="[722,796,1069,1095]" pageId="34" pageNumber="35">Hotha</collectingCounty>
, 
<collectingRegion box="[809,901,1069,1095]" country="China" name="Yunnan" pageId="34" pageNumber="35">Yunnan</collectingRegion>
