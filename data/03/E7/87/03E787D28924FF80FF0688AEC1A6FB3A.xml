<location LSID="urn:lsid:plazi:treatment:03E787D28924FF80FF0688AEC1A6FB3A:8E91601F8924FF80FC0C8E2EC196FBC2" box="[925,974,1109,1135]" county="Hotha" municipality="These" name="One" pageId="34" pageNumber="35" stateProvince="Yunnan">One</location>
