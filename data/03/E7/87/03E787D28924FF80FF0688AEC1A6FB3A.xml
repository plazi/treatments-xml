<location LSID="urn:lsid:plazi:treatment:03E787D28924FF80FF0688AEC1A6FB3A:8E91601F8924FF80FEAC8E06C332FB3A" box="[317,362,1149,1175]" name="The" pageId="34" pageNumber="35" stateProvince="Yunnan">The</location>
