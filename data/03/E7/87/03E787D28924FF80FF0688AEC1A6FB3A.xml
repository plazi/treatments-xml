<collectingMunicipality box="[1133,1203,1069,1095]" pageId="34" pageNumber="35">These</collectingMunicipality>
<specimenCount box="[1211,1385,1069,1095]" pageId="34" pageNumber="35" type="generic">two specimens</specimenCount>
