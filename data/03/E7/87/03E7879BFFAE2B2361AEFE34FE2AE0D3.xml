<emphasis bold="true" box="[654,662,1122,1136]" pageId="8" pageNumber="9">1</emphasis>
</number>
<emphasis bold="true" box="[662,689,1106,1132]" pageId="8" pageNumber="9">M</emphasis>
<number box="[690,698,1122,1136]" pageId="8" pageNumber="9" value="2.0">
