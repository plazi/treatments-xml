<taxonomicName authority="Muller &amp; Troschel" authorityName="Muller &amp; Troschel" box="[514,1036,1607,1633]" class="Asteroidea" family="Echinasteridae" genus="Echinaster" kingdom="Animalia" order="Spinulosida" pageId="6" pageNumber="7" phylum="Echinodermata" rank="species" species="brasiliensis">
