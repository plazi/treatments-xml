<paragraph blockId="64.[151,1436,1415,1762]" pageId="64" pageNumber="65">
<materialsCitation box="[198,1123,1575,1602]" location="Comment" municipality="Noack" pageId="64" pageNumber="65" specimenCount="1" specimenCount-female="1" typeStatus="syntype">
<location LSID="urn:lsid:plazi:treatment:03E787D28946FFE2FF0688FCC6D3F94F:8E91601F8946FFE2FF578C53C362F9EF" box="[198,314,1576,1602]" municipality="Noack" name="Comment" pageId="64" pageNumber="65">Comment</location>
