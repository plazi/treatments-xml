<emphasis bold="true" box="[151,445,1365,1391]" pageId="1" pageNumber="29">
<taxonomicName box="[151,329,1365,1391]" class="Arachnida" family="Tengellidae" genus="Austrotengella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="29" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,329,1365,1391]" italics="true" pageId="1" pageNumber="29">Austrotengella</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[336,445,1365,1391]" pageId="1" pageNumber="29" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
