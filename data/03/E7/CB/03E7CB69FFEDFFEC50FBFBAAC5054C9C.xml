<collectingCountry box="[332,396,1973,1997]" name="China" pageId="1" pageNumber="274">China</collectingCountry>
(
<collectingRegion box="[410,498,1974,1998]" country="China" name="Yunnan" pageId="1" pageNumber="274">Yunnan</collectingRegion>
