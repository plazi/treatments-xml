<subSubSection pageId="9" pageNumber="341" type="etymology">
<paragraph blockId="9.[151,1437,1400,2003]" pageId="9" pageNumber="341">
<emphasis bold="true" box="[151,290,1832,1858]" pageId="9" pageNumber="341">Etymology:</emphasis>
