<collectingMunicipality box="[736,956,691,717]" pageId="18" pageNumber="350">Zoological Institute</collectingMunicipality>
(
<location LSID="urn:lsid:plazi:treatment:03E7A162FFE1684FFF14E114FEE9FC8A:8E9146AFFFE1684FFC4AE2C8FB56FD7E" box="[969,1197,691,717]" county="Bregetova" municipality="Zoological Institute" name="Maria Stanyukovich" pageId="18" pageNumber="350" stateProvince="Leningrad">Maria Stanyukovich</location>
