<location LSID="urn:lsid:plazi:treatment:03E7A162FFFC6852FF14E3A9FBEBFA2E:8E9146AFFFFC6852FD87E540FDCBFAE6" box="[516,560,1339,1365]" country="United States of America" county="No" municipality="Whereabouts" name="The" pageId="15" pageNumber="347" stateProvince="Utah">The</location>
