<subSubSection pageId="10" pageNumber="11" type="materials_examined">
<paragraph blockId="10.[151,1437,658,1207]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[151,522,658,684]" pageId="10" pageNumber="11">Material examined. Stn. I10</emphasis>
, 
<date box="[539,677,658,684]" pageId="10" pageNumber="11" value="2007-11-28">28.xi.2007</date>
