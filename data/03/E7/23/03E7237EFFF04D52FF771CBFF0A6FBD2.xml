<subSubSection box="[151,1057,877,903]" pageId="3" pageNumber="4" type="materials_examined">
<paragraph blockId="3.[151,1436,877,1130]" box="[151,1057,877,903]" pageId="3" pageNumber="4">
<emphasis bold="true" box="[151,387,877,903]" pageId="3" pageNumber="4">Material examined</emphasis>
. 
<emphasis bold="true" box="[402,497,877,903]" pageId="3" pageNumber="4">Stn. I10</emphasis>
, 
<date box="[513,643,877,903]" pageId="3" pageNumber="4" value="2007-11-28">28.xi.2007</date>
