<subSubSection pageId="15" pageNumber="15" type="diagnosis">
<paragraph blockId="15.[160,1156,1119,1421]" box="[160,276,1119,1144]" pageId="15" pageNumber="15">
<heading bold="true" box="[160,276,1119,1144]" fontSize="11" level="5" pageId="15" pageNumber="15" reason="0">
