<taxonomicName authorityName="ERGALATAXINAE" baseAuthorityName="Hirase" baseAuthorityYear="1915" box="[392,593,718,741]" class="Gastropoda" family="Muricidae" genus="Vokesimurex" kingdom="Animalia" order="Neogastropoda" pageId="5" pageNumber="442" phylum="Mollusca" rank="species" species="hirasei">
