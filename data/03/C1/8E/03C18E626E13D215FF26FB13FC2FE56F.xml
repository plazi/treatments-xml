(new record), shells in 
<quantity box="[547,665,1479,1503]" metricMagnitude="2" metricUnit="m" metricValue="2.48" metricValueMax="2.52" metricValueMin="2.44" pageId="4" pageNumber="441" unit="m" value="248.0" valueMax="252.0" valueMin="244.0">244-252 m</quantity>
