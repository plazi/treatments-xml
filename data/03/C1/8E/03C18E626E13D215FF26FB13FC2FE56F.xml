, 
<collectingCountry box="[1147,1240,1445,1469]" name="Australia" pageId="4" pageNumber="441">Australia</collectingCountry>
(
<collectingRegion box="[1255,1388,1445,1469]" country="Australia" name="Queensland" pageId="4" pageNumber="441">Queensland</collectingRegion>
) and now 
