<paragraph blockId="24.[126,452,1210,1234]" box="[126,452,1210,1234]" pageId="24" pageNumber="25">
<heading box="[126,452,1210,1234]" fontSize="10" level="2" pageId="24" pageNumber="25" reason="9">
<taxonomicName authority="AMNHM" authorityName="AMNHM" box="[126,364,1210,1234]" class="Mammalia" family="Pitheciidae" genus="Xenothrix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="24" pageNumber="25" phylum="Chordata" rank="genus">
