<caption box="[151,1149,1291,1313]" httpUri="https://zenodo.org/record/288328/files/figure.png" pageId="10" pageNumber="11" targetBox="[239,1359,199,1265]" targetPageId="10">
<paragraph blockId="10.[151,1149,1291,1313]" box="[151,1149,1291,1313]" pageId="10" pageNumber="11">
