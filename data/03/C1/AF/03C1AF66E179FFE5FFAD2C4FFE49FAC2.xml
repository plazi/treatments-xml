<taxonomicName authority="AMNHM" authorityName="AMNHM" box="[126,364,530,554]" class="Mammalia" family="Pitheciidae" genus="Xenothrix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
