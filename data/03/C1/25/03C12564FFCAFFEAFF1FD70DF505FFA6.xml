<taxonomicName authorityName="Truqui" authorityYear="1855" box="[306,429,1396,1420]" class="Insecta" family="Anthicidae" genus="Anthicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="455" phylum="Arthropoda" rank="species" species="armatus">
<emphasis box="[306,429,1396,1420]" italics="true" pageId="4" pageNumber="455">A. armatus</emphasis>
