. 
<emphasis box="[978,1062,1772,1795]" italics="true" pageId="63" pageNumber="64">maryae</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[1076,1164,1770,1795]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="plata">
<emphasis box="[1076,1093,1772,1795]" italics="true" pageId="63" pageNumber="64">B</emphasis>
. 
<emphasis box="[1106,1164,1770,1795]" italics="true" pageId="63" pageNumber="64">plata</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[1178,1265,1770,1795]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="birksi">
