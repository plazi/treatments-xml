<emphasis box="[248,265,1807,1830]" italics="true" pageId="63" pageNumber="64">B</emphasis>
. 
<emphasis box="[280,345,1807,1830]" italics="true" pageId="63" pageNumber="64">parva</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[360,475,1807,1830]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="maryae">
<emphasis box="[360,377,1807,1830]" italics="true" pageId="63" pageNumber="64">B</emphasis>
. 
