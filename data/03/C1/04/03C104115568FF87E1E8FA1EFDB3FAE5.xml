<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[947,1062,1771,1795]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="maryae">
