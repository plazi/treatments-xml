<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[968,1083,1129,1152]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="maryae">
