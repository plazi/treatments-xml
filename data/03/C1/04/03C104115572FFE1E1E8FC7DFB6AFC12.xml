<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[434,567,1128,1153]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="gemmelli">
