<emphasis box="[806,908,477,502]" italics="true" pageId="56" pageNumber="57">gemmelli</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Hogg" authorityYear="1902" box="[963,1058,478,501]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="aurea">
