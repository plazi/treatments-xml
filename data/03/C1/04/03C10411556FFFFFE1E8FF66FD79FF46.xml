<emphasis box="[387,443,477,502]" italics="true" pageId="56" pageNumber="57">bella</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[457,573,477,502]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="tunstilli">
<emphasis box="[457,474,478,501]" italics="true" pageId="56" pageNumber="57">B</emphasis>
. 
<emphasis box="[488,573,477,502]" italics="true" pageId="56" pageNumber="57">tunstilli</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[588,760,478,501]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="emmottorum">
