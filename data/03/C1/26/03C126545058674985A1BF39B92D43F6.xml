<quantity box="[401,539,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="8.3" metricValueMax="9.5" metricValueMin="7.1" pageId="16" pageNumber="123" unit="mm" value="8.3" valueMax="9.5" valueMin="7.1">7.1–9.5 mm</quantity>
