<subSubSection pageId="5" pageNumber="378" type="etymology">
<paragraph blockId="5.[151,1436,151,881]" pageId="5" pageNumber="378">
<emphasis bold="true" box="[199,334,824,848]" pageId="5" pageNumber="378">Etymology.</emphasis>
