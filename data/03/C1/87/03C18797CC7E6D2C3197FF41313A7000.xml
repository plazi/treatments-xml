, 
<collectingRegion box="[673,756,222,245]" country="China" name="Jiangsu" pageId="5" pageNumber="500">Jiangsu</collectingRegion>
, 
<collectingRegion box="[151,240,254,277]" country="China" name="Shaanxi" pageId="5" pageNumber="500">Shaanxi</collectingRegion>
, 
<collectingRegion box="[252,338,254,277]" country="China" name="Sichuan" pageId="5" pageNumber="500">Sichuan</collectingRegion>
and 
<collectingRegion box="[391,472,254,277]" country="China" name="Yunnan" pageId="5" pageNumber="500">Yunnan</collectingRegion>
