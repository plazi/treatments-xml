<paragraph blockId="1.[264,745,523,550]" box="[264,745,523,550]" pageId="1" pageNumber="2">
<heading bold="true" box="[264,745,523,550]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<taxonomicName authority="Mannerheim" authorityName="Mannerheim" box="[264,745,523,550]" class="Insecta" family="Staphylinidae" genus="Phloeocharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="subtilissima">
<emphasis bold="true" box="[264,745,523,550]" pageId="1" pageNumber="2">
