<number box="[795,854,1513,1537]" pageId="15" pageNumber="450" value="1972.0">1972</number>
</taxonomicName>
, 
<taxonomicName authority="Chopard, 1943" authorityName="Chopard" authorityYear="1943" box="[875,1328,1512,1537]" class="Insecta" family="Pamphagidae" genus="Acinipe" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="450" phylum="Arthropoda" rank="species" species="angustipennis">
