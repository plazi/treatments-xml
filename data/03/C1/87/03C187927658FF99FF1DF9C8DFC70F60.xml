<quantity box="[736,832,672,698]" metricMagnitude="3" metricUnit="m" metricValue="1.04" pageId="4" pageNumber="51" unit="m" value="1040.0">1040 m</quantity>
, 
<date box="[847,1048,672,698]" pageId="4" pageNumber="51" value="1992-07-24" valueMax="1992-09-15" valueMin="1992-07-24">
