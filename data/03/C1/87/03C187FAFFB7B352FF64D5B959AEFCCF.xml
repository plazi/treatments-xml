<taxonomicName authority="Bolivar, 1907" authorityName="Bolivar" authorityYear="1907" box="[151,465,1668,1694]" class="Insecta" family="Pamphagidae" genus="Eunapiodes" kingdom="Animalia" order="Orthoptera" pageId="21" pageNumber="456" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,294,1668,1694]" italics="true" pageId="21" pageNumber="456">Eunapiodes</emphasis>
Bolivar, 
<number box="[406,465,1668,1694]" pageId="21" pageNumber="456" value="1907.0">1907</number>
</taxonomicName>
(type-species: 
<taxonomicName authority="Stal" authorityName="Stal" box="[649,966,1668,1694]" class="Insecta" family="Pamphagidae" genus="Pamphagus" kingdom="Animalia" order="Orthoptera" pageId="21" pageNumber="456" phylum="Arthropoda" rank="species" species="granosus">
<emphasis bold="true" box="[649,910,1668,1694]" italics="true" pageId="21" pageNumber="456">Pamphagus granosus</emphasis>
Stål
</taxonomicName>
)
</emphasis>
