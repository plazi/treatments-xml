<number box="[299,328,826,852]" pageId="8" pageNumber="65" value="12.0">12</number>
(
<number box="[344,422,826,852]" pageId="8" pageNumber="65" value="11.5" valueMax="13.0" valueMin="10.0">10–13</number>
