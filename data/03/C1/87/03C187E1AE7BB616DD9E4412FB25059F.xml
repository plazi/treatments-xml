<paragraph blockId="38.[151,1436,1034,2031]" box="[151,728,1034,1059]" pageId="38" pageNumber="139">
<emphasis bold="true" box="[151,301,1034,1059]" pageId="38" pageNumber="139">Type locality</emphasis>
