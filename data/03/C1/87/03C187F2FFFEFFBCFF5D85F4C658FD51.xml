<taxonomicName authorityName="Yildiz &amp; Senler" authorityYear="2013" box="[1148,1283,549,575]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="species" species="anatolica">
