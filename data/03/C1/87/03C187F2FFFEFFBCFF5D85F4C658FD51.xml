<taxonomicName authorityName="Foissner &amp; Song" authorityYear="2002" box="[151,299,621,647]" class="Oligohymenophorea" family="Frontoniidae" genus="Apofrontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="genus">
<emphasis box="[151,299,621,647]" italics="true" pageId="12" pageNumber="560">Apofrontonia</emphasis>
</taxonomicName>
and 
<taxonomicName box="[354,497,622,647]" class="Oligohymenophorea" family="Parameciidae" genus="Paramecium" kingdom="Protozoa" order="Peniculida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="genus">
<emphasis box="[354,497,622,647]" italics="true" pageId="12" pageNumber="560">Paramecium</emphasis>
</taxonomicName>
than the other 
<taxonomicName authorityName="Ehrenberg" authorityYear="1838" box="[665,778,622,647]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="genus">
<emphasis box="[665,778,622,647]" italics="true" pageId="12" pageNumber="560">Frontonia</emphasis>
</taxonomicName>
members.
