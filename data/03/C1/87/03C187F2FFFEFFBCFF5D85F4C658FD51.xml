</taxonomicName>
(
<accessionNumber box="[1298,1432,548,575]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/MG456578" pageId="12" pageNumber="560">MG456578</accessionNumber>
, 
<accessionNumber box="[151,284,584,611]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/MG456581" pageId="12" pageNumber="560">MG456581</accessionNumber>
), 
<taxonomicName authorityName="Long, Song, AL-Rasheid, Wang, Yi, Al-Quraishy, Lin &amp; AL-Farraj" authorityYear="2008" box="[305,409,585,611]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="species" species="didieri">
<emphasis box="[305,409,585,611]" italics="true" pageId="12" pageNumber="560">F. didieri</emphasis>
</taxonomicName>
, 
<taxonomicName box="[424,545,585,611]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="species" species="ocularis">
<emphasis box="[424,545,585,611]" italics="true" pageId="12" pageNumber="560">F. ocularis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[561,676,585,611]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="species" species="elegans">
<emphasis box="[561,676,585,611]" italics="true" pageId="12" pageNumber="560">F. elegans</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[740,845,585,611]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="12" pageNumber="560" phylum="Ciliophora" rank="species" species="pusilla">
<emphasis box="[740,845,585,611]" italics="true" pageId="12" pageNumber="560">F. pusilla</emphasis>
</taxonomicName>
