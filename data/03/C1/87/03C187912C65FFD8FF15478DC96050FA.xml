<number box="[844,873,1370,1396]" pageId="4" pageNumber="61" value="56.0">56</number>
(
<number box="[890,969,1370,1396]" pageId="4" pageNumber="61" value="57.0" valueMax="61.0" valueMin="53.0">53–61</number>
). 
<emphasis bold="true" box="[989,1193,1370,1396]" pageId="4" pageNumber="61">Prodorsal shield</emphasis>
<number box="[1201,1230,1370,1396]" pageId="4" pageNumber="61" value="34.0">34</number>
(
<number box="[1246,1324,1370,1396]" pageId="4" pageNumber="61" value="34.5" valueMax="35.0" valueMin="34.0">34–35</number>
