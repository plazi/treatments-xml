<number box="[1117,1146,1290,1316]" pageId="4" pageNumber="61" value="70.0">70</number>
(
<number box="[1162,1239,1290,1316]" pageId="4" pageNumber="61" value="70.0" valueMax="72.0" valueMin="68.0">68–72</number>
) thick; light yellow. Gnathosoma 
<number box="[365,394,1330,1356]" pageId="4" pageNumber="61" value="56.0">56</number>
(
<number box="[411,490,1330,1356]" pageId="4" pageNumber="61" value="57.0" valueMax="61.0" valueMin="53.0">53–61</number>
), projecting downwards, pedipalp coxal setae (
<emphasis box="[1047,1075,1331,1356]" italics="true" pageId="4" pageNumber="61">ep</emphasis>
) 
<number box="[1092,1107,1330,1356]" pageId="4" pageNumber="61" value="3.0">3</number>
(
<number box="[1125,1172,1330,1356]" pageId="4" pageNumber="61" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis box="[270,285,1371,1396]" italics="true" pageId="4" pageNumber="61">d</emphasis>
) 
<number box="[302,331,1370,1396]" pageId="4" pageNumber="61" value="10.0">10</number>
(
<number box="[348,410,1370,1396]" pageId="4" pageNumber="61" value="10.0" valueMax="11.0" valueMin="9.0">9–11</number>
). Chelicerae 
<number box="[564,593,1370,1396]" pageId="4" pageNumber="61" value="50.0">50</number>
(
<number box="[609,688,1370,1396]" pageId="4" pageNumber="61" value="50.0" valueMax="52.0" valueMin="48.0">48–52</number>
