<number box="[908,937,1290,1316]" pageId="4" pageNumber="61" value="67.0">67</number>
(
<number box="[953,1029,1290,1316]" pageId="4" pageNumber="61" value="67.5" valueMax="70.0" valueMin="65.0">65–70</number>
