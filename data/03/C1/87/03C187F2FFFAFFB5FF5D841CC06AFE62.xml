<taxonomicName authorityName="Brodsky" authorityYear="1908" baseAuthorityName="Ehrenberg" baseAuthorityYear="1833" box="[281,371,1719,1743]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="4" pageNumber="552" phylum="Ciliophora" rank="species" species="leucas">
