(
<taxonomicName baseAuthorityName="Zimmermann" baseAuthorityYear="1780" box="[746,983,547,571]" class="Mammalia" family="Caviidae" genus="Dolichotis" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="597" phylum="Chordata" rank="species" species="patagonum">
