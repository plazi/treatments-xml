<taxonomicName authorityName="Hershkovitz" authorityYear="1983" box="[841,1140,1212,1258]" class="Mammalia" family="Aotidae" genus="Aotus" kingdom="Animalia" order="Primates" pageId="3" pageNumber="428" phylum="Chordata" rank="species" species="brumbacki">
