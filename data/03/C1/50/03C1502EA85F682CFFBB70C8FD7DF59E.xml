<vernacularName box="[941,1235,491,512]" pageId="2" pageNumber="427">Mico nocturno de vientre gris</vernacularName>
<emphasis box="[96,342,531,552]" pageId="2" pageNumber="427">Other common names:</emphasis>
<vernacularName box="[352,606,531,552]" pageId="2" pageNumber="427">Colombian Night Monkey</vernacularName>
, 
<vernacularName box="[619,885,531,552]" pageId="2" pageNumber="427">Gray-bellied Night Monkey</vernacularName>
