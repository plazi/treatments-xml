<taxonomicName authority="McCosker 1982" authorityName="McCosker" authorityYear="1982" box="[1103,1432,1345,1370]" class="Actinopterygii" family="Ophichthidae" genus="Glenoglossa" kingdom="Animalia" order="Anguilliformes" pageId="2" pageNumber="43" phylum="Chordata" rank="genus">
