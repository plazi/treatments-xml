<collectionCode box="[391,438,907,929]" pageId="13" pageNumber="14">SVL</collectionCode>
~ 
<quantity box="[462,533,907,929]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" pageId="13" pageNumber="14" unit="mm" value="50.0">50 mm</quantity>
