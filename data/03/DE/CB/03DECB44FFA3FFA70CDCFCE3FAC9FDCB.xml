<taxonomicName baseAuthorityName="Meyrick" baseAuthorityYear="1914" box="[930,1087,475,501]" class="Insecta" family="Lecithoceridae" genus="Protolychnis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="368" phylum="Arthropoda" rank="species" species="chlorotoma">
