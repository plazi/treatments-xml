<taxonomicName authority="Jaiswara, 2017" authorityName="Jaiswara" authorityYear="2017" box="[151,486,1017,1044]" class="Insecta" family="Gryllidae" genus="Perugryllus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="573" phylum="Arthropoda" rank="genus">
