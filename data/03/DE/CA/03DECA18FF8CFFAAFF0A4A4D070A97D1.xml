<subSubSection pageId="10" pageNumber="161" type="etymology">
<paragraph blockId="10.[151,1437,151,1508]" pageId="10" pageNumber="161">
<emphasis bold="true" box="[199,334,1340,1365]" pageId="10" pageNumber="161">Etymology.</emphasis>
