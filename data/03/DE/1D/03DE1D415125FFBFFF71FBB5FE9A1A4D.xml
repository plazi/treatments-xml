</taxonomicName>
<taxonomicName box="[1224,1268,1739,1765]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="spp">spp</taxonomicName>
. (
<taxonomicName box="[1289,1424,1739,1765]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
), 
<taxonomicName box="[151,278,1780,1805]" class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="genus">
<emphasis box="[151,278,1780,1805]" italics="true" pageId="5" pageNumber="6">Astragalus</emphasis>
</taxonomicName>
<taxonomicName box="[286,332,1779,1805]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="spp">spp</taxonomicName>
., 
<emphasis box="[349,492,1780,1805]" italics="true" pageId="5" pageNumber="6">Onobrychus</emphasis>
<taxonomicName box="[500,544,1779,1805]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="spp">spp</taxonomicName>
. (
<taxonomicName box="[565,676,1779,1805]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
= Leguminosae), 
<taxonomicName box="[891,1171,1780,1805]" class="Magnoliopsida" family="Brassicaceae" genus="Malcolmia" kingdom="Plantae" order="Brassicales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="turkestanica">
