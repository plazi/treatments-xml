<taxonomicName authority="Silvestri, 1911" authorityName="Silvestri" authorityYear="1911" box="[299,659,1545,1569]" class="Chilopoda" family="Scolopendridae" genus="Alluropus" kingdom="Animalia" order="Scolopendromorpha" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="demangei">
