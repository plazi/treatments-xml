<taxonomicName authority="Leach, 1814" authorityName="Leach" authorityYear="1814" box="[247,598,366,393]" class="Chilopoda" family="Scolopendridae" kingdom="Animalia" order="Scolopendromorpha" pageId="25" pageNumber="26" phylum="Arthropoda" rank="family">Scolopendridae Leach, 1814</taxonomicName>
