<subSubSection pageId="27" pageNumber="28" type="discussion">
<paragraph blockId="27.[151,1437,150,501]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[199,315,403,429]" pageId="27" pageNumber="28">Remarks.</emphasis>
