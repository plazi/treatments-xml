(
<emphasis bold="true" box="[343,470,1577,1600]" italics="true" pageId="7" pageNumber="572">Erythmelus</emphasis>
) 
<emphasis bold="true" box="[486,545,1577,1600]" italics="true" pageId="7" pageNumber="572">agilis</emphasis>
