; head length 
<number box="[1318,1370,1732,1756]" pageId="9" pageNumber="56" value="10.7">10.7</number>
; head width 
<number box="[222,274,1768,1792]" pageId="9" pageNumber="56" value="13.2">13.2</number>
; eye diameter 
<number box="[438,476,1768,1792]" pageId="9" pageNumber="56" value="3.9">3.9</number>
; tympanum diameter 
<number box="[721,759,1768,1792]" pageId="9" pageNumber="56" value="1.4">1.4</number>
; interorbital distance 
<number box="[1002,1040,1768,1792]" pageId="9" pageNumber="56" value="3.1">3.1</number>
; upper eyelid width 3.0; internarial distance 
<number box="[250,288,1804,1828]" pageId="9" pageNumber="56" value="2.9">2.9</number>
; eye–nostril distance 
