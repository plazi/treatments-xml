<collectingCountry box="[643,732,1193,1218]" name="Finland" pageId="11">Finland</collectingCountry>
, 
<taxonomicName box="[750,868,1193,1217]" class="Sphagnopsida" family="Sphagnaceae" genus="Sphagnum" kingdom="Plantae" order="Sphagnales" pageId="11" phylum="Bryophyta" rank="genus">
