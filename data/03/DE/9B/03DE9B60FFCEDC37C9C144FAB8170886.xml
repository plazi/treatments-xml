<taxonomicName authority="Holarctic" authorityName="Holarctic" class="Insecta" family="Braconidae" genus="Rhoptrocentrus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="471" phylum="Arthropoda" rank="species" species="claeopatrae">
<emphasis italics="true" pageId="4" pageNumber="471">R. claeopatrae</emphasis>
, Holarctic
</taxonomicName>
<taxonomicName box="[420,527,621,644]" class="Insecta" family="Braconidae" genus="Rhoptrocentrus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="471" phylum="Arthropoda" rank="species" species="piceus">
<emphasis box="[420,527,621,644]" italics="true" pageId="4" pageNumber="471">R. piceus</emphasis>
</taxonomicName>
, and Australasian 
<taxonomicName authority="Belokobylskij &amp; Maeto 2009" authorityName="Belokobylskij &amp; Maeto" authorityYear="2009" box="[751,1330,620,645]" class="Insecta" family="Braconidae" genus="Rhoptrocentrus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="471" phylum="Arthropoda" rank="species" species="yarramanensis">
<emphasis box="[751,953,621,644]" italics="true" pageId="4" pageNumber="471">R. yarramanensis</emphasis>
(
<bibRefCitation author="Belokobylskij" box="[973,1322,620,645]" pageId="4" pageNumber="471" refString="Belokobylskij, S. A. &amp; Maeto, K. (2009) Doryctinae (Hymenoptera: Braconidae) of Japan, Fauna Mundi. Polish Academy of Sciences, Warsaw, 806 pp., 197 figs., 16 photos." type="book" year="2009">
Belokobylskij &amp; Maeto 
<number box="[1261,1322,620,644]" pageId="4" pageNumber="471" value="2009.0">2009</number>
</bibRefCitation>
)
</taxonomicName>
. Here 
<taxonomicName class="Insecta" family="Braconidae" genus="Rhoptrocentrus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="471" phylum="Arthropoda" rank="species" species="quercusi" status="sp.n.">
<emphasis italics="true" pageId="4" pageNumber="471">R. quercusi</emphasis>
