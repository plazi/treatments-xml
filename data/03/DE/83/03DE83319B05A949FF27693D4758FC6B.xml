<emphasis box="[151,229,859,882]" italics="true" pageId="1" pageNumber="134">
<taxonomicName box="[151,225,859,882]" class="Insecta" family="Braconidae" genus="Biroia" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="134" phylum="Arthropoda" rank="genus">Biroia</taxonomicName>
