<taxonomicName authority="Steiner, 1916" authorityName="Steiner" authorityYear="1916" box="[987,1304,1869,1894]" class="Adenophorea" family="Desmodoridae" genus="Desmodora" kingdom="Animalia" order="Desmodorida" pageId="17" pageNumber="80" phylum="Nematoda" rank="species" species="poseidoni">
