and 
<taxonomicName box="[798,883,66,89]" class="Polychaeta" family="Polynoidae" genus="Leucia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="20" pageNumber="1907" phylum="Annelida" rank="species" species="agisca">
<emphasis box="[798,883,66,89]" italics="true" pageId="20" pageNumber="1907">L agisca</emphasis>
</taxonomicName>
1907 
