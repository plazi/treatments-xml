<taxonomicName authority="(Peringuey 1908)" baseAuthorityName="Peringuey" baseAuthorityYear="1908" box="[305,657,331,352]" class="Insecta" family="Bolboceratidae" genus="Pseudoathyreus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="240" phylum="Arthropoda" rank="species" species="rhodesianus">
