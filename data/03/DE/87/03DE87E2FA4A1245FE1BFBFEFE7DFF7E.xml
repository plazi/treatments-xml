. Length of setae: c 
<subScript attach="left" box="[637,646,216,233]" fontSize="7" pageId="15" pageNumber="476">1</subScript>
46±63, c 
<subScript attach="left" box="[756,765,216,233]" fontSize="7" pageId="15" pageNumber="476">2</subScript>
105±112, d 
<subScript attach="left" box="[906,915,216,233]" fontSize="7" pageId="15" pageNumber="476">1</subScript>
53±59, 
<geoCoordinate degrees="1" direction="east" minutes="59" orientation="longitude" pageId="15" pageNumber="476" precision="925" value="1.9833333">
e 
<subScript attach="left" box="[1025,1034,216,233]" fontSize="7" pageId="15" pageNumber="476">1</subScript>
59
</geoCoordinate>
± 63, f 
