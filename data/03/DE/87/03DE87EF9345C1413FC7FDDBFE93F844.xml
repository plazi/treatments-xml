<paragraph blockId="10.[151,1436,1565,2022]" pageId="10" pageNumber="421">
<materialsCitation ID-GBIF-Occurrence="1572325220" country="Japan" location="South Korea" pageId="10" pageNumber="421" specimenCount="1" typeStatus="holotype">
