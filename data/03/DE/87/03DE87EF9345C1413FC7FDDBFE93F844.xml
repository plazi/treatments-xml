<collectingCountry box="[981,1047,1566,1590]" name="Japan" pageId="10" pageNumber="421">Japan</collectingCountry>
) and 
<taxonomicName box="[1120,1206,1566,1589]" class="Liliopsida" family="Zosteraceae" genus="Zostera" kingdom="Plantae" order="Alismatales" pageId="10" pageNumber="421" phylum="Tracheophyta" rank="genus">
