<location LSID="urn:lsid:plazi:treatment:03DE87EF9345C1413FC7FDDBFE93F844:8EA860229348C1413BA7F982FA27F994" box="[1271,1424,1565,1590]" country="Japan" name="South Korea" pageId="10" pageNumber="421">South Korea</location>
