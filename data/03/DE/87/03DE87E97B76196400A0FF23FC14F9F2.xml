<taxonomicName authorityName="Chu &amp; Yao &amp; Wongprom &amp; Li" authorityYear="2022" baseAuthorityName="Tong &amp; Li" baseAuthorityYear="2009" box="[1304,1429,475,502]" class="Arachnida" family="Pholcidae" genus="Changminia" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="245" phylum="Arthropoda" rank="species" species="huangdi">
