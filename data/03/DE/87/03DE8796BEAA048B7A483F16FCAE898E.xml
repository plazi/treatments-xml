<subSubSection box="[769,1158,1596,1619]" pageId="147" pageNumber="147" type="nomenclature">
<paragraph blockId="147.[721,1206,1596,1678]" box="[769,1158,1596,1619]" pageId="147" pageNumber="147">
<taxonomicName box="[769,1011,1596,1619]" class="Insecta" family="Miridae" genus="Myrtophylus" kingdom="Animalia" order="Hemiptera" pageId="147" pageNumber="147" phylum="Arthropoda" rank="species" species="calytrix" status="new species">
<emphasis bold="true" box="[769,1011,1596,1619]" italics="true" pageId="147" pageNumber="147">Myrtophylus calytrix</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[1026,1158,1597,1619]" pageId="147" pageNumber="147" rank="species">new species</taxonomicNameLabel>
