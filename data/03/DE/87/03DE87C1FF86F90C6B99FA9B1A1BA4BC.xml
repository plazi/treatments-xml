<number box="[1272,1372,188,212]" pageId="111" pageNumber="112" value="214.0" valueMax="215.0" valueMin="213.0">213–215</number>
) 
<number pageId="111" pageNumber="112" value="2.375" valueMax="2.45" valueMin="2.3">2.3– 2.45</number>
