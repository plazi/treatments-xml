<taxonomicName authority="Skuse" authorityName="Skuse" box="[151,361,582,608]" class="Insecta" family="Chironomidae" genus="Procladius" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="36" phylum="Arthropoda" rank="genus">
