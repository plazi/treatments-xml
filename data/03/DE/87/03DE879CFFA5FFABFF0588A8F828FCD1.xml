<number box="[1366,1436,760,785]" pageId="10" pageNumber="455" value="50.0" valueMax="54.0" valueMin="46.0">46–54</number>
[
<number box="[158,190,796,821]" pageId="10" pageNumber="455" value="54.0">54</number>
] long; megaseta 
<number box="[380,436,796,821]" pageId="10" pageNumber="455" value="9.0" valueMax="12.0" valueMin="6.0">6–12</number>
[
