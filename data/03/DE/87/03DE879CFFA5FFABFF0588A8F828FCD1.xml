<number box="[418,502,1638,1662]" pageId="9" pageNumber="454" value="90.5" valueMax="100.0" valueMin="81.0">81–100</number>
[
<number box="[518,564,1638,1662]" pageId="9" pageNumber="454" value="100.0">100</number>
