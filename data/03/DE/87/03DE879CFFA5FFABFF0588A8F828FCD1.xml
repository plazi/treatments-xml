<number box="[1043,1127,1602,1626]" pageId="9" pageNumber="454" value="109.0" valueMax="120.0" valueMin="98.0">98–120</number>
[
<number box="[1143,1189,1602,1626]" pageId="9" pageNumber="454" value="120.0">120</number>
