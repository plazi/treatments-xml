[not observed] divided in 
<number box="[729,772,1566,1590]" pageId="9" pageNumber="454" value="0.5" valueMax="1.0" valueMin="0.0">0–1</number>
[not observed] (
<number box="[956,971,1565,1590]" pageId="9" pageNumber="454" value="6.0">6</number>
) inner verticals, 
<number box="[1160,1202,1566,1590]" pageId="9" pageNumber="454" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
[not observed] outer verticals and 
<number box="[303,345,1602,1626]" pageId="9" pageNumber="454" value="1.0" valueMax="2.0" valueMin="0.0">0–2</number>
[
<number box="[362,379,1602,1626]" pageId="9" pageNumber="454" value="2.0">2</number>
] (
<number box="[404,419,1602,1627]" pageId="9" pageNumber="454" value="6.0">6</number>
) postorbitals. Clypeus with 
<number box="[741,797,1602,1626]" pageId="9" pageNumber="454" value="9.5" valueMax="10.0" valueMin="9.0">9–10</number>
[
<number box="[814,830,1602,1626]" pageId="9" pageNumber="454" value="8.0">8</number>
