<number box="[1298,1342,1637,1662]" pageId="9" pageNumber="454" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
): 
<number box="[1366,1436,1637,1662]" pageId="9" pageNumber="454" value="18.5" valueMax="22.0" valueMin="15.0">15–22</number>
[
<number box="[158,192,1674,1698]" pageId="9" pageNumber="454" value="20.0">20</number>
]; 
<number box="[212,282,1674,1698]" pageId="9" pageNumber="454" value="28.0" valueMax="34.0" valueMin="22.0">22–34</number>
[
<number box="[297,331,1674,1698]" pageId="9" pageNumber="454" value="34.0">34</number>
]; 
<number box="[351,424,1674,1699]" pageId="9" pageNumber="454" value="55.0" valueMax="64.0" valueMin="46.0">46–64</number>
; 
<number box="[436,508,1674,1699]" pageId="9" pageNumber="454" value="62.0" valueMax="73.0" valueMin="51.0">51–73</number>
