<paragraph blockId="20.[151,477,858,884]" box="[151,477,858,884]" pageId="20" pageNumber="21">
<heading bold="true" box="[151,477,858,884]" fontSize="11" level="1" pageId="20" pageNumber="21" reason="1">
<emphasis bold="true" box="[151,477,858,884]" pageId="20" pageNumber="21">
Genus 
<taxonomicName box="[237,359,858,884]" class="Maxillopoda" family="Nannopodidae" genus="Acuticoxa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[237,359,858,884]" italics="true" pageId="20" pageNumber="21">Acuticoxa</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[367,477,858,884]" pageId="20" pageNumber="21" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
