<emphasis bold="true" box="[192,368,1523,1547]" pageId="2" pageNumber="1">
<typeStatus box="[192,261,1523,1547]" pageId="2" pageNumber="1">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Stebnicka, 1993" authorityName="Stebnicka" authorityYear="1993" box="[383,836,1523,1547]" class="Insecta" family="Aphodiidae" genus="Saprovisca" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1" phylum="Arthropoda" rank="species" species="leytensis">
