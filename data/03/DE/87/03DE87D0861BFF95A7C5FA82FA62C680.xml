<taxonomicName baseAuthorityName="Haliday" baseAuthorityYear="1835" box="[917,970,1558,1579]" class="Insecta" family="Braconidae" genus="Meteorus" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="colon">
<emphasis box="[917,970,1558,1579]" italics="true" pageId="25" pageNumber="26">colon</emphasis>
