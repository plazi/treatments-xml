</taxonomicName>
, 
<taxonomicName authority="Distant, 1892 b" authorityName="Distant" authorityYear="1892" box="[991,1295,187,213]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="centralis">
<emphasis box="[991,1121,187,213]" italics="true" pageId="46" pageNumber="47">C. centralis</emphasis>
