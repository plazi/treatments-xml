<elevation box="[1233,1362,690,717]" metricMagnitude="2" metricUnit="m" metricValue="4.965" metricValueMax="5.66" metricValueMin="4.27" pageId="30" pageNumber="442" unit="m" value="496.5" valueMax="566.0" valueMin="427.0">427–566 m</elevation>
</quantity>
, 
<date pageId="30" pageNumber="442" value="2010-11-09">
<collectingDate pageId="30" pageNumber="442" value="2010-11-09">9-XI- 2010</collectingDate>
</date>
: one colony, 
<collectedFrom box="[358,583,726,753]" pageId="30" pageNumber="442">
on 
<taxonomicName authorityName="Ramos et al." authorityYear="2017" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[393,583,726,752]" class="Anthozoa" family="Caryophylliidae" genus="Lophelia" kingdom="Animalia" order="Scleractinia" pageId="30" pageNumber="442" phylum="Cnidaria" rank="species" species="pertusa">
