<taxonomicName authorityName="Hooker &amp; Russell" authorityYear="2012" box="[1214,1361,1544,1565]" class="Mammalia" family="Louisinidae" genus="Walbeckodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="13" pageNumber="869" phylum="Chordata" rank="genus">
