<taxonomicName authorityName="Hooker &amp; Russell" authorityYear="2012" box="[1186,1365,1697,1719]" class="Mammalia" family="Louisinidae" genus="Gigarton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="13" pageNumber="869" phylum="Chordata" rank="species" species="louisi">
