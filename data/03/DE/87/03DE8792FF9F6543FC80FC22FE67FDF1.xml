<taxonomicName authorityName="Hooker &amp; Russell" authorityYear="2012" box="[1092,1218,1758,1779]" class="Mammalia" family="Louisinidae" genus="Berrulestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="13" pageNumber="869" phylum="Chordata" rank="genus">
