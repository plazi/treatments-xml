</taxonomicName>
and 
<taxonomicName box="[1056,1244,1448,1472]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="473" phylum="Arthropoda" rank="species" species="schefflerorum" status="new species">
