, 
<number box="[523,574,151,175]" pageId="8" pageNumber="42" value="5.6">5.60</number>
)[
<quantity box="[588,705,151,175]" metricMagnitude="-1" metricUnit="m" metricValue="1.6382999999999999" metricValueMax="1.905" metricValueMin="1.3716" pageId="8" pageNumber="42" unit="in" value="6.45" valueMax="7.5" valueMin="5.4">5.4–7.5 in</quantity>
<bibRefCitation author="Moore" box="[712,986,151,175]" pageId="8" pageNumber="42" refString="Moore, J. W. &amp; Moore, I. A. (1978) Descriptions of larvae of four species of Procladius from Great Slave Lake (Chironomidae: Diptera). Canadian Journal of Zoology, 56, 2055 - 2057." type="journal article" year="1978">
