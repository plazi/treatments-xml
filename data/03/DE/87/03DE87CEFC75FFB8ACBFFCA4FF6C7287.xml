<number box="[220,335,188,212]" pageId="8" pageNumber="42" value="5.77" valueMax="6.44" valueMin="5.1">5.10–6.44</number>
, 
<number box="[346,395,188,212]" pageId="8" pageNumber="42" value="5.5">5.50</number>
