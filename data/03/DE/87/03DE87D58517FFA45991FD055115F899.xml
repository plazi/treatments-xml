<subSubSection box="[151,1089,871,896]" pageId="37" pageNumber="488" type="etymology">
<paragraph blockId="37.[151,1437,871,1869]" box="[151,1089,871,896]" pageId="37" pageNumber="488">
<emphasis bold="true" box="[151,286,871,896]" pageId="37" pageNumber="488">Etymology.</emphasis>
<taxonomicName box="[292,418,872,896]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="502" phylum="Arthropoda" rank="species" species="subrubrum" status="new species">
