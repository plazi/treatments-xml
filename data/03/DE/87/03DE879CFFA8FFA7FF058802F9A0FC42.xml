[
<number box="[505,521,1344,1368]" pageId="4" pageNumber="449" value="3.0">3</number>
], inner verticals, 
<number box="[716,758,1344,1368]" pageId="4" pageNumber="449" value="1.0" valueMax="2.0" valueMin="0.0">0–2</number>
[0] outer verticals and 
<number box="[1019,1061,1344,1368]" pageId="4" pageNumber="449" value="1.5" valueMax="3.0" valueMin="0.0">0–3</number>
[
<number box="[1077,1090,1344,1368]" pageId="4" pageNumber="449" value="1.0">1</number>
