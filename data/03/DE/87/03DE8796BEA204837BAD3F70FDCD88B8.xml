<subSubSection box="[740,1188,1626,1649]" pageId="155" pageNumber="155" type="nomenclature">
<paragraph blockId="155.[730,1197,1626,1678]" box="[740,1188,1626,1649]" pageId="155" pageNumber="155">
<taxonomicName box="[740,1040,1626,1649]" class="Insecta" family="Miridae" genus="Omnivoriphylus" kingdom="Animalia" order="Hemiptera" pageId="155" pageNumber="155" phylum="Arthropoda" rank="species" species="charleville" status="new species">
<emphasis bold="true" box="[740,1040,1626,1649]" italics="true" pageId="155" pageNumber="155">Omnivoriphylus charleville</emphasis>
</taxonomicName>
