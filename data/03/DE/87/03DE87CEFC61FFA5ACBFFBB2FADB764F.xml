<quantity box="[527,671,439,463]" metricMagnitude="-1" metricUnit="m" metricValue="1.26238" metricValueMax="1.397" metricValueMin="1.12776" pageId="20" pageNumber="54" unit="in" value="4.97" valueMax="5.5" valueMin="4.44">4.44–5.50 in</quantity>
