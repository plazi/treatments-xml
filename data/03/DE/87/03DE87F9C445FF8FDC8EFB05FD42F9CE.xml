<collectingCountry box="[904,973,1666,1690]" name="China" pageId="6" pageNumber="836">China</collectingCountry>
(
<collectingRegion box="[987,1082,1666,1690]" country="China" name="Yunnan" pageId="6" pageNumber="836">Yunnan</collectingRegion>
).
</paragraph>
