<taxonomicName class="Magnoliopsida" family="Myrtaceae" genus="Leptospermum" higherTaxonomySource="CoL" kingdom="Plantae" order="Myrtales" pageId="169" pageNumber="169" phylum="Tracheophyta" rank="species" species="erubescens">
