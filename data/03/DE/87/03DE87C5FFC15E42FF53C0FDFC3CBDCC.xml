<taxonomicName box="[354,510,1355,1377]" class="Insecta" family="Encyrtidae" genus="Globulencyrtus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="270" phylum="Arthropoda" rank="genus">Globulencyrtus</taxonomicName>
