<paragraph blockId="15.[151,1436,1888,2021]" pageId="15" pageNumber="96">
<subSubSection pageId="15" pageNumber="96" type="materials_examined">
<emphasis bold="true" box="[199,432,1888,1913]" pageId="15" pageNumber="96">Material examined.</emphasis>
