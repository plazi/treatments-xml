</quantity>
deep (by 
<location LSID="urn:lsid:plazi:treatment:03CF1D3CF339FFD286B0B7F3FD9DF84A:8EB9FAF1F33AFFD2833EB7A7FA55F972" box="[1305,1432,1702,1727]" country="Brazil" county="Near Parcel do Juliao" municipality="Ilhabella" name="Schockaert" pageId="6" pageNumber="87" stateProvince="Sao Paulo">Schockaert</location>
