, 
<collectingMunicipality box="[694,792,1702,1727]" pageId="6" pageNumber="87">Ilhabella</collectingMunicipality>
(
<collectingRegion box="[808,927,1702,1727]" country="Brazil" name="Sao Paulo" pageId="6" pageNumber="87">São Paulo</collectingRegion>
), coarse sand, 
<quantity box="[1092,1191,1702,1727]" metricMagnitude="1" metricUnit="m" metricValue="1.25" metricValueMax="1.5" metricValueMin="1.0" pageId="6" pageNumber="87" unit="m" value="12.5" valueMax="15.0" valueMin="10.0">
