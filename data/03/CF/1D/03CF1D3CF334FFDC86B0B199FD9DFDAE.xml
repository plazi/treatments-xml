(Las Perlas), coarse sand from near to a mangrove, 
<quantity box="[1281,1347,420,445]" metricMagnitude="0" metricUnit="m" metricValue="2.5" pageId="8" pageNumber="89" unit="m" value="2.5">
<elevation box="[1281,1347,420,445]" metricMagnitude="0" metricUnit="m" metricValue="2.5" pageId="8" pageNumber="89" unit="m" value="2.5">2.5 m</elevation>
</quantity>
deep at medium level tide (
<geoCoordinate box="[369,525,453,478]" direction="north" orientation="latitude" pageId="8" pageNumber="89" precision="1" value="8.387381">8°23'14.57&quot;N</geoCoordinate>
, 
<geoCoordinate box="[536,690,453,478]" direction="west" orientation="longitude" pageId="8" pageNumber="89" precision="1" value="-79.08382">79° 5'1.73”W</geoCoordinate>
) (by Artois, 
<date box="[834,1055,453,478]" pageId="8" pageNumber="89" value="2011-12-12">
