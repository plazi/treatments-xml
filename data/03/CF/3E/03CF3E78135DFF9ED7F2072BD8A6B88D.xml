<collectingCountry box="[329,387,941,966]" name="Israel" pageId="15" pageNumber="1439">Israel</collectingCountry>
and 
<collectingCountry box="[445,520,941,966]" name="Turkey" pageId="15" pageNumber="1439">Turkey</collectingCountry>
(
