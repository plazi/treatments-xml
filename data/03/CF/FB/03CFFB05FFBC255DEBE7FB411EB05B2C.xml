<paragraph blockId="13.[151,1436,1190,1863]" box="[151,583,1190,1215]" pageId="13" pageNumber="514">
<heading bold="true" box="[151,583,1190,1215]" fontSize="10" level="2" pageId="13" pageNumber="514" reason="2">
<taxonomicName authority="Linne, 1763" authorityName="Linne" authorityYear="1763" box="[151,583,1190,1215]" class="Insecta" family="Mantidae" genus="Stagmomantis" kingdom="Animalia" order="Mantodea" pageId="13" pageNumber="514" phylum="Arthropoda" rank="species" species="carolina">
<emphasis bold="true" box="[151,583,1190,1215]" pageId="13" pageNumber="514">
<emphasis bold="true" box="[151,418,1190,1215]" italics="true" pageId="13" pageNumber="514">Stagmomantis carolina</emphasis>
(Linné, 
<number box="[518,575,1190,1215]" pageId="13" pageNumber="514" value="1763.0">1763</number>
)
</emphasis>
</taxonomicName>
</heading>
