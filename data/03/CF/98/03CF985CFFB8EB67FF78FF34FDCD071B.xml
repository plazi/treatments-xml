♀ 
<typeStatus box="[958,1070,333,357]" pageId="24" pageNumber="125">paratypes</typeStatus>
: 
<quantity box="[1082,1247,332,357]" metricMagnitude="-3" metricUnit="m" metricValue="3.965" metricValueMax="4.4" metricValueMin="3.53" pageId="24" pageNumber="125" unit="mm" value="3.965" valueMax="4.4" valueMin="3.53">3.53–4.40 mm</quantity>
in ♂, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="4.0649999999999995" metricValueMax="4.33" metricValueMin="3.8" pageId="24" pageNumber="125" unit="mm" value="4.0649999999999995" valueMax="4.33" valueMin="3.8">3.80–4.33 mm</quantity>
in ♀), THL = 
<quantity box="[359,459,368,392]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="24" pageNumber="125" unit="mm" value="2.1">2.10 mm</quantity>
(
<quantity box="[473,638,367,392]" metricMagnitude="-3" metricUnit="m" metricValue="1.85" metricValueMax="2.13" metricValueMin="1.57" pageId="24" pageNumber="125" unit="mm" value="1.85" valueMax="2.13" valueMin="1.57">1.57–2.13 mm</quantity>
in ♂, 
<quantity box="[708,871,367,392]" metricMagnitude="-3" metricUnit="m" metricValue="1.77" metricValueMax="1.87" metricValueMin="1.67" pageId="24" pageNumber="125" unit="mm" value="1.77" valueMax="1.87" valueMin="1.67">1.67–1.87 mm</quantity>
in ♀), WL = 
<quantity box="[1024,1124,367,392]" metricMagnitude="-3" metricUnit="m" metricValue="5.4" pageId="24" pageNumber="125" unit="mm" value="5.4">5.40 mm</quantity>
(
