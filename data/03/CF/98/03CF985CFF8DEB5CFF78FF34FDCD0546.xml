and 
<number box="[902,920,1827,1851]" pageId="34" pageNumber="135" value="2.0">2</number>
♀ 
<typeStatus box="[945,1057,1828,1852]" pageId="34" pageNumber="135">paratypes</typeStatus>
: 
<quantity box="[1068,1169,1827,1851]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="34" pageNumber="135" unit="mm" value="4.8">4.80 mm</quantity>
in ♂, 
<quantity box="[1243,1407,1827,1852]" metricMagnitude="-3" metricUnit="m" metricValue="4.734999999999999" metricValueMax="4.87" metricValueMin="4.6" pageId="34" pageNumber="135" unit="mm" value="4.734999999999999" valueMax="4.87" valueMin="4.6">4.60–4.87 mm</quantity>
in ♀), THL = 2.00 mm (
