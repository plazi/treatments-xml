in 
<typeStatus box="[595,693,1827,1852]" pageId="34" pageNumber="135">holotype</typeStatus>
(range in 
<specimenCount box="[811,845,1827,1851]" pageId="34" pageNumber="135" type="generic">
<number box="[811,822,1827,1851]" pageId="34" pageNumber="135" value="1.0">1</number>
