<paragraph blockId="39.[151,1436,151,824]" pageId="39" pageNumber="140">
<emphasis bold="true" box="[199,381,151,176]" pageId="39" pageNumber="140">Measurements.</emphasis>
BL = 
<quantity box="[460,562,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="4.87" pageId="39" pageNumber="140" unit="mm" value="4.87">4.87 mm</quantity>
in 
<typeStatus box="[602,700,151,176]" pageId="39" pageNumber="140">holotype</typeStatus>
(range in 
<specimenCount box="[820,856,152,176]" pageId="39" pageNumber="140" type="generic">
<number box="[820,833,152,176]" pageId="39" pageNumber="140" value="3.0">3</number>
♂
</specimenCount>
and 
<number box="[914,932,152,176]" pageId="39" pageNumber="140" value="4.0">4</number>
♀ 
<typeStatus box="[958,1070,152,176]" pageId="39" pageNumber="140">paratypes</typeStatus>
: 
<quantity box="[1082,1247,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" metricValueMax="5.2" metricValueMin="4.6" pageId="39" pageNumber="140" unit="mm" value="4.9" valueMax="5.2" valueMin="4.6">4.60–5.20 mm</quantity>
in ♂, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="4.93" metricValueMin="4.07" pageId="39" pageNumber="140" unit="mm" value="4.5" valueMax="4.93" valueMin="4.07">4.07–4.93 mm</quantity>
in ♀), THL = 
<quantity box="[359,459,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="2.53" pageId="39" pageNumber="140" unit="mm" value="2.53">2.53 mm</quantity>
(
<quantity box="[473,638,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="2.35" metricValueMax="2.5" metricValueMin="2.2" pageId="39" pageNumber="140" unit="mm" value="2.35" valueMax="2.5" valueMin="2.2">2.20–2.50 mm</quantity>
in ♂, 
<quantity box="[708,871,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="2.1149999999999998" metricValueMax="2.3" metricValueMin="1.93" pageId="39" pageNumber="140" unit="mm" value="2.1149999999999998" valueMax="2.3" valueMin="1.93">1.93–2.30 mm</quantity>
in ♀), WL = 
