<typeStatus box="[958,1070,1507,1531]" pageId="39" pageNumber="140">paratypes</typeStatus>
: 
<quantity box="[1082,1247,1506,1531]" metricMagnitude="-3" metricUnit="m" metricValue="4.15" metricValueMax="4.57" metricValueMin="3.73" pageId="39" pageNumber="140" unit="mm" value="4.15" valueMax="4.57" valueMin="3.73">3.73–4.57 mm</quantity>
in ♂, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.7" metricValueMax="4.33" metricValueMin="3.07" pageId="39" pageNumber="140" unit="mm" value="3.7" valueMax="4.33" valueMin="3.07">3.07–4.33 mm</quantity>
in ♀), THL = 
<quantity box="[359,459,1542,1566]" metricMagnitude="-3" metricUnit="m" metricValue="1.97" pageId="39" pageNumber="140" unit="mm" value="1.97">1.97 mm</quantity>
(
<quantity box="[473,638,1542,1566]" metricMagnitude="-3" metricUnit="m" metricValue="1.95" metricValueMax="2.07" metricValueMin="1.83" pageId="39" pageNumber="140" unit="mm" value="1.95" valueMax="2.07" valueMin="1.83">1.83–2.07 mm</quantity>
in ♂, 
<quantity box="[708,871,1542,1566]" metricMagnitude="-3" metricUnit="m" metricValue="1.68" metricValueMax="1.93" metricValueMin="1.43" pageId="39" pageNumber="140" unit="mm" value="1.68" valueMax="1.93" valueMin="1.43">1.43–1.93 mm</quantity>
in ♀), WL = 
<quantity box="[1024,1124,1542,1566]" metricMagnitude="-3" metricUnit="m" metricValue="3.17" pageId="39" pageNumber="140" unit="mm" value="3.17">3.17 mm</quantity>
(
