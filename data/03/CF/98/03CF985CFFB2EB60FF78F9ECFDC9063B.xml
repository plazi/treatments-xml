<paragraph blockId="30.[151,1436,1900,2034]" lastBlockId="31.[151,1436,151,536]" lastPageId="31" lastPageNumber="132" pageId="30" pageNumber="131">
<emphasis bold="true" box="[199,381,1900,1925]" pageId="30" pageNumber="131">Measurements.</emphasis>
BL = 
<quantity box="[460,562,1900,1925]" metricMagnitude="-3" metricUnit="m" metricValue="3.53" pageId="30" pageNumber="131" unit="mm" value="3.53">3.53 mm</quantity>
in 
<typeStatus box="[602,700,1900,1925]" pageId="30" pageNumber="131">holotype</typeStatus>
(range in 
<specimenCount box="[820,855,1901,1925]" pageId="30" pageNumber="131" type="generic">
<number box="[820,835,1901,1925]" pageId="30" pageNumber="131" value="4.0">4</number>
♂
</specimenCount>
and 
<number box="[914,931,1900,1925]" pageId="30" pageNumber="131" value="5.0">5</number>
♀ 
<typeStatus box="[958,1070,1902,1926]" pageId="30" pageNumber="131">paratypes</typeStatus>
: 
<quantity box="[1082,1247,1900,1925]" metricMagnitude="-3" metricUnit="m" metricValue="3.185" metricValueMax="3.77" metricValueMin="2.6" pageId="30" pageNumber="131" unit="mm" value="3.185" valueMax="3.77" valueMin="2.6">2.60–3.77 mm</quantity>
in ♂, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.15" metricValueMax="3.23" metricValueMin="3.07" pageId="30" pageNumber="131" unit="mm" value="3.15" valueMax="3.23" valueMin="3.07">3.07–3.23 mm</quantity>
in ♀), ThL = 
