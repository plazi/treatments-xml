<quantity box="[527,691,441,465]" metricMagnitude="-3" metricUnit="m" metricValue="1.835" metricValueMax="1.9" metricValueMin="1.77" pageId="36" pageNumber="137" unit="mm" value="1.835" valueMax="1.9" valueMin="1.77">1.77–1.90 mm</quantity>
