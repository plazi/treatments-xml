<collectingDate box="[1040,1179,1941,1966]" pageId="7" pageNumber="152" value="1965-08-26">26.viii.1965</collectingDate>
</date>
, 
<location LSID="urn:lsid:plazi:treatment:03CF3C55FF92FFC9FF58F9CBFAA9FE44:8EB9DB98FF93FFC6FB65F869FA82F86D" box="[1194,1345,1941,1966]" country="Peru" municipality="Loreto" name="San Antonio," pageId="7" pageNumber="152" stateProvince="Loreto">San Antonio,</location>
<collectingMunicipality box="[1355,1429,1942,1965]" pageId="7" pageNumber="152">
<emphasis box="[1355,1429,1942,1965]" italics="true" pageId="7" pageNumber="152">Loreto</emphasis>
</collectingMunicipality>
, 
<collectingCountry box="[151,226,1978,2001]" name="Peru" pageId="7" pageNumber="152">PERU</collectingCountry>
, 82, Coll. 
<collectorName box="[340,539,1977,2002]" pageId="7" pageNumber="152">J.C. Hitchcock Jr.</collectorName>
/ 
<taxonomicName box="[554,645,1977,2001]" class="Insecta" family="Cicadellidae" genus="Chinaia" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="152" phylum="Arthropoda" rank="genus" status="n. sp.">
