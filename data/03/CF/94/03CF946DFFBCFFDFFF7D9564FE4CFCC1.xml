.vii. 
<number box="[966,1028,367,392]" pageId="16" pageNumber="17" value="1970.0">1970</number>
</date>
, T.N. Ananthakrishnan; Davikulam, 
<specimenCount box="[151,248,404,429]" pageId="16" pageNumber="17" type="female">
<number box="[151,165,404,428]" pageId="16" pageNumber="17" value="1.0">1</number>
female
</specimenCount>
from dry fern, 
<date box="[420,528,404,429]" pageId="16" pageNumber="17" value="1970-09-09">
<number box="[420,434,404,429]" pageId="16" pageNumber="17" value="9.0">9</number>
