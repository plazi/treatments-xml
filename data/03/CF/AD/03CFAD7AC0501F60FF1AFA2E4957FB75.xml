<subSubSection box="[151,667,1493,1519]" pageId="7" pageNumber="62" type="materials_examined">
<paragraph blockId="7.[151,1437,1493,2004]" box="[151,667,1493,1519]" pageId="7" pageNumber="62">
<emphasis bold="true" box="[151,313,1493,1519]" pageId="7" pageNumber="62">
<typeStatus box="[151,212,1493,1519]" pageId="7" pageNumber="62">Type</typeStatus>
species.
</emphasis>
<taxonomicName box="[320,566,1493,1518]" class="Gastropoda" family="Neritidae" genus="Cuisenerita" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cycloneritimorpha" pageId="7" pageNumber="62" phylum="Mollusca" rank="species" species="tuberosa" status="sp. nov.">
<emphasis box="[320,566,1493,1518]" italics="true" pageId="7" pageNumber="62">Cuisenerita tuberosa</emphasis>
</taxonomicName>
<emphasis bold="true" box="[573,667,1493,1519]" pageId="7" pageNumber="62">
<taxonomicNameLabel box="[573,667,1493,1519]" pageId="7" pageNumber="62" rank="species">sp. nov.</taxonomicNameLabel>
