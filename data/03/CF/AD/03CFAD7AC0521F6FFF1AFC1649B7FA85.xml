<subSubSection lastPageId="7" lastPageNumber="62" pageId="5" pageNumber="60" type="materials_examined">
<paragraph blockId="5.[151,1436,1850,1980]" pageId="5" pageNumber="60">
<emphasis bold="true" box="[151,343,1850,1876]" pageId="5" pageNumber="60">
<typeStatus box="[151,215,1850,1876]" pageId="5" pageNumber="60">Type</typeStatus>
material.
</emphasis>
