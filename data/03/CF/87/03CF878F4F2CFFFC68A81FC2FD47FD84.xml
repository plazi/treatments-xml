<subSubSection pageId="5" pageNumber="49" type="etymology">
<paragraph blockId="5.[151,1437,150,1437]" box="[199,1326,1375,1401]" pageId="5" pageNumber="49">
<emphasis bold="true" box="[199,328,1375,1401]" pageId="5" pageNumber="49">Etymology</emphasis>
