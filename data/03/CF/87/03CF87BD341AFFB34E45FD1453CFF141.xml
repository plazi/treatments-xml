<taxonomicName authorityName="Dohrn" authorityYear="1862" box="[544,690,882,905]" class="Insecta" family="Nabidae" genus="Nabis" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="sareptanus">
<emphasis box="[544,690,882,905]" italics="true" pageId="6" pageNumber="39">N. sareptanus</emphasis>
</taxonomicName>
