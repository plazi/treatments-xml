<quantity box="[288,452,1085,1108]" metricMagnitude="-3" metricUnit="m" metricValue="4.25" metricValueMax="4.6" metricValueMin="3.9" pageId="4" pageNumber="337" unit="mm" value="4.25" valueMax="4.6" valueMin="3.9">3.90–4.60 mm</quantity>
