, 
<number box="[1062,1101,562,588]" pageId="11" pageNumber="12" value="5.4">5.4</number>
, 
<number box="[1114,1153,562,588]" pageId="11" pageNumber="12" value="5.8">5.8</number>
, 
<number box="[1165,1205,562,588]" pageId="11" pageNumber="12" value="1.5">1.5</number>
