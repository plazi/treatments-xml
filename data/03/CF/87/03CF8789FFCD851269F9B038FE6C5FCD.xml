<emphasis box="[929,963,850,877]" italics="true" pageId="2" pageNumber="703">
ST 
<subScript attach="left" box="[953,963,861,877]" fontSize="7" pageId="2" pageNumber="703">1</subScript>
</emphasis>
= 
<emphasis box="[999,1033,850,877]" italics="true" pageId="2" pageNumber="703">
ST 
<subScript attach="left" box="[1023,1033,861,877]" fontSize="7" pageId="2" pageNumber="703">2</subScript>
</emphasis>
15(14 
<emphasis box="[1103,1116,850,874]" italics="true" pageId="2" pageNumber="703">–</emphasis>
16), 
<emphasis box="[192,226,885,911]" italics="true" pageId="2" pageNumber="703">
ST 
<subScript attach="left" box="[216,226,895,911]" fontSize="7" pageId="2" pageNumber="703">3</subScript>
</emphasis>
= 
<emphasis box="[256,290,885,911]" italics="true" pageId="2" pageNumber="703">
ST 
<subScript attach="left" box="[280,290,895,911]" fontSize="7" pageId="2" pageNumber="703">4</subScript>
</emphasis>
= 14 (13 
<emphasis box="[387,400,885,909]" italics="true" pageId="2" pageNumber="703">–</emphasis>
