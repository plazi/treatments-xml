<taxonomicName authority="Pic, 1943" authorityName="Pic" authorityYear="1943" box="[700,1000,1791,1816]" class="Insecta" family="Melyridae" genus="Malachiomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="333" phylum="Arthropoda" rank="genus">
