<number box="[1234,1246,1160,1184]" pageId="3" pageNumber="26" value="1.0">1</number>
/ 
<number box="[1258,1270,1160,1184]" pageId="3" pageNumber="26" value="1.0">1</number>
<number box="[1277,1289,1160,1184]" pageId="3" pageNumber="26" value="1.0">1</number>
/ 
<number box="[1301,1313,1160,1184]" pageId="3" pageNumber="26" value="1.0">1</number>
<number box="[1320,1334,1160,1184]" pageId="3" pageNumber="26" value="1.0">1</number>
, femur 
<number box="[1422,1436,1160,1184]" pageId="3" pageNumber="26" value="2.0">2</number>
<number box="[151,165,1196,1220]" pageId="3" pageNumber="26" value="3.0">3</number>
/ 
<number box="[176,186,1196,1220]" pageId="3" pageNumber="26" value="1.0">1</number>
<number box="[194,208,1196,1220]" pageId="3" pageNumber="26" value="2.0">2</number>
/ 
<number box="[217,229,1196,1220]" pageId="3" pageNumber="26" value="3.0">3</number>
<number box="[236,252,1196,1220]" pageId="3" pageNumber="26" value="2.0">2</number>
, genu 
<number box="[325,339,1196,1220]" pageId="3" pageNumber="26" value="2.0">2</number>
<number box="[347,359,1196,1220]" pageId="3" pageNumber="26" value="3.0">3</number>
/ 
<number box="[368,382,1196,1220]" pageId="3" pageNumber="26" value="2.0">2</number>
