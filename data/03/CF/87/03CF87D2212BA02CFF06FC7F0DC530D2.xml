<number box="[631,690,1009,1033]" pageId="1" pageNumber="205" value="1822.0">1822</number>
, Europe, introduced into North 
<collectingCountry box="[1061,1161,1009,1033]" name="United States of America" pageId="1" pageNumber="205">America</collectingCountry>
; 
<taxonomicName authority="Watanabe" authorityName="Watanabe" box="[1176,1435,1009,1033]" class="Insecta" family="Braconidae" genus="Rhogas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="205" phylum="Arthropoda" rank="species" species="lymantriae">
<emphasis box="[1176,1297,1009,1033]" italics="true" pageId="1" pageNumber="205">lymantriae</emphasis>
(Watanabe)
</taxonomicName>
<number box="[151,210,1044,1068]" pageId="1" pageNumber="205" value="1937.0">1937</number>
, new combination, 
<collectingCountry box="[441,508,1045,1068]" name="Japan" pageId="1" pageNumber="205">Japan</collectingCountry>
, introduced into North 
<collectingCountry box="[784,884,1044,1068]" name="United States of America" pageId="1" pageNumber="205">America</collectingCountry>
; 
<taxonomicName authority="Reardon" authorityName="Reardon" box="[900,1146,1044,1068]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="205" phylum="Arthropoda" rank="species" species="indiscretus">
<emphasis box="[900,1022,1044,1068]" italics="true" pageId="1" pageNumber="205">indiscretus</emphasis>
(Reardon)
</taxonomicName>
<number box="[1157,1216,1044,1068]" pageId="1" pageNumber="205" value="1970.0">1970</number>
, new combination, 
<collectingCountry box="[151,211,1078,1102]" name="India" pageId="1" pageNumber="205">India</collectingCountry>
, introduced into North 
<collectingCountry box="[494,594,1078,1102]" name="United States of America" pageId="1" pageNumber="205">America</collectingCountry>
; 
<taxonomicName box="[611,693,1079,1102]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="205" phylum="Arthropoda" rank="species" species="martini" status="new species">
