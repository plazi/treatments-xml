<taxonomicName box="[382,610,1574,1597]" class="Insecta" family="Corydalidae" genus="Protohermes" higherTaxonomySource="CoL" kingdom="Animalia" order="Megaloptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="sinensis">
<emphasis box="[382,610,1574,1597]" italics="true" pageId="36" pageNumber="37">
<taxonomicName box="[382,519,1574,1597]" class="Insecta" family="Corydalidae" genus="Protohermes" higherTaxonomySource="CoL" kingdom="Animalia" order="Megaloptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">Protohermes</taxonomicName>
sinensis
