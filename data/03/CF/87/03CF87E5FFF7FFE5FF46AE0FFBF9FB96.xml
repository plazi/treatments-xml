<taxonomicName authority="Yang &amp; Yang." authorityName="Yang &amp; Yang." box="[382,770,1574,1597]" class="Insecta" family="Corydalidae" genus="Protohermes" kingdom="Animalia" order="Megaloptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="sinensis">
<taxonomicName authority="Yang &amp; Yang" authorityName="Yang &amp; Yang" box="[382,766,1574,1597]" class="Insecta" family="Corydalidae" genus="Protohermes" higherTaxonomySource="CoL" kingdom="Animalia" order="Megaloptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="sinensis">
