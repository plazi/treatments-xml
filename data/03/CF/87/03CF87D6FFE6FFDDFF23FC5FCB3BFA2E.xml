<number box="[1162,1176,1381,1407]" pageId="14" pageNumber="45" value="1.0">1</number>
male
</specimenCount>
<date box="[1252,1390,1381,1407]" pageId="14" pageNumber="45" value="1983-04-01" valueMax="1983-04-07" valueMin="1983-04-01">
