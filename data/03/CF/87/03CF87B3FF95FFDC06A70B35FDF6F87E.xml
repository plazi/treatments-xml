<number box="[780,809,1813,1838]" pageId="35" pageNumber="36" value="16.0">16</number>
.viii.– 
<number box="[875,888,1813,1838]" pageId="35" pageNumber="36" value="8.0">8</number>
.ix. 
<number box="[920,982,1813,1838]" pageId="35" pageNumber="36" value="1989.0">1989</number>
