<collectionCode LSID="urn:lsid:biocol.org:col:34925" box="[1183,1274,1468,1492]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="3" pageNumber="22">AMNH</collectionCode>
