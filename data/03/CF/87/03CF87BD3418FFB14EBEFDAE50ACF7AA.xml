<collectingCountry box="[151,215,875,899]" name="China" pageId="4" pageNumber="37">China</collectingCountry>
and 
<collectingCountry box="[267,357,875,899]" name="Pakistan" pageId="4" pageNumber="37">Pakistan</collectingCountry>
(
<bibRefCitation author="KERZHNER I. M." box="[371,541,875,899]" pageId="4" pageNumber="37" pagination="84 - 107" refId="ref5851" refString="KERZHNER I. M. 1996: Family Nabidae A. Costa 1983 - damsel bugs. Pp. 84 - 107. In: AUKEMA B. &amp; RIEGER CH. (eds): Catalogue of the Heteroptera of the Palaearctic Region. Vol. 2. The Netherlands Entomological Society, Amsterdam, xiv + 360 pp." type="book chapter" year="1996">KERZHNER 1996</bibRefCitation>
, 
<bibRefCitation author="REN SH &amp; ZH" box="[551,658,875,899]" pageId="4" pageNumber="37" pagination="220 - 239" refId="ref6900" refString="REN SH. - ZH. 1998: Hemiptera: Heteroptera Nabidae. Fauna Sinica, Insecta Vol. 13. Science Press, Beijing, 251 pp + 6 pls (in Chinese with English abstract on pp. 220 - 239)." type="book chapter" year="1998">REN 1998</bibRefCitation>
