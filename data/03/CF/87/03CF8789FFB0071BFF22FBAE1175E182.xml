<emphasis bold="true" box="[198,603,1775,1801]" pageId="16" pageNumber="17">
Description. Male (
<collectionCode box="[437,504,1775,1801]" httpUri="http://grbio.org/cool/23qr-v2gj" name="Instituto Biologico de Sao Paulo" pageId="16" pageNumber="17">IBSP</collectionCode>
<number box="[511,590,1775,1801]" pageId="16" pageNumber="17" value="54468.0">54468</number>
