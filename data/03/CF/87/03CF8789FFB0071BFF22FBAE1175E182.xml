, 
<number box="[1377,1431,1949,1975]" pageId="16" pageNumber="17" value="0.24">0.24</number>
, 
<number box="[151,206,1984,2010]" pageId="16" pageNumber="17" value="0.14">0.14</number>
, 
<number box="[218,274,1984,2010]" pageId="16" pageNumber="17" value="0.12">0.12</number>
