<paragraph blockId="14.[189,1398,310,406]" pageId="14" pageNumber="15">
<taxonomicName authority="Simon 1903" authorityName="Simon" authorityYear="1903" box="[189,503,310,337]" class="Arachnida" family="Anyphaenidae" genus="Tafana" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="riveti">
