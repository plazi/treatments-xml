<subSubSection lastPageId="10" lastPageNumber="411" pageId="6" pageNumber="407" type="description">
<paragraph blockId="6.[151,1437,1220,2037]" box="[199,904,1832,1857]" pageId="6" pageNumber="407">
<emphasis bold="true" box="[199,345,1832,1857]" pageId="6" pageNumber="407">Description.</emphasis>
Based on 
<typeStatus box="[462,563,1832,1857]" pageId="6" pageNumber="407">holotype</typeStatus>
, male, 
<quantity box="[642,743,1833,1857]" metricMagnitude="-2" metricUnit="m" metricValue="1.14" pageId="6" pageNumber="407" unit="mm" value="11.4">11.4 mm</quantity>
