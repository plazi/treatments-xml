(
<figureCitation box="[272,342,999,1025]" captionStart="FIGURE 1" captionStartId="7.[264,367,1692,1716]" captionTargetBox="[272,1312,569,1681]" captionTargetId="figure@7.[272,1314,569,1683]" captionTargetPageId="7" captionText="FIGURE 1. Dorsal habitus of species of Onega Distant. (A) O. avella Distant, male, LACM; (B) O. bracteata Young, male, DZRJ; (C) O. fassli Young, male, INHS; (D) O. freytagi sp. nov., male holotype, UKL; (E) O. krameri sp. nov., male holotype, USNM; (F) O. orphne sp. nov., male holotype, USNM; (G) O. sanguinicollis (Latreille) comb. nov., female, NHMW; (H) O. stipata (Walker), male, OSU. Scale bar = 2.0 mm." httpUri="https://zenodo.org/record/169499/files/figure.png" pageId="9" pageNumber="10">
Figs 
<number box="[331,342,999,1025]" pageId="9" pageNumber="10" value="1.0">1</number>
</figureCitation>
E, 
<number box="[379,394,999,1025]" pageId="9" pageNumber="10" value="3.0">3</number>
)
