and 
<taxonomicName authority="Khanjani &amp; Ueckermann, 2005 (Faraji et al., 2008)" authorityName="Khanjani &amp; Ueckermann, 2005 (Faraji et al." authorityYear="2008" class="Arachnida" family="Laelapidae" genus="Hypoaspis" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="polyphyllae" subGenus="Hypoaspis">
