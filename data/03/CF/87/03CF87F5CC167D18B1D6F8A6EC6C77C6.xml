<bibRefCitation author="Gu" box="[332,536,620,644]" pageId="4" pageNumber="27" refString="Gu, Y. M. &amp; Duan, Q. - X. (1993) Two new species of Laelapidae from Yunnan, China (Acari: Gamasina). Acta Zootaxonomica Sinica, 18, 48 - 53." type="journal article" year="1993">
Gu &amp; Duan, 
<number box="[477,536,620,644]" pageId="4" pageNumber="27" value="1993.0">1993</number>
</bibRefCitation>
</taxonomicName>
), 
<taxonomicName authority="Ma &amp; Zheng, 2000" authorityName="Ma &amp; Zheng" authorityYear="2000" box="[557,943,620,644]" class="Arachnida" family="Laelapidae" genus="Hypoaspis" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="hunanensis">
<emphasis box="[557,577,621,644]" italics="true" pageId="4" pageNumber="27">H</emphasis>
. 
<emphasis box="[591,717,620,644]" italics="true" pageId="4" pageNumber="27">hunanensis</emphasis>
<bibRefCitation author="Ma" box="[724,943,620,644]" pageId="4" pageNumber="27" refString="Ma, L. - M. &amp; Zheng, B. - Y. (2000) A new species of Hypoaspis from Hunan, China (Acari: Gamasina). Acta Zootaxonomica Sinica, 25, 373 - 375." type="journal article" year="2000">
Ma &amp; Zheng, 
<number box="[884,943,620,644]" pageId="4" pageNumber="27" value="2000.0">2000</number>
</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName authority="Ma et al., 2003" authorityName="Ma et al." authorityYear="2003" box="[1002,1382,620,644]" class="Arachnida" family="Laelapidae" genus="Hypoaspis" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="terrestrisimilis">
