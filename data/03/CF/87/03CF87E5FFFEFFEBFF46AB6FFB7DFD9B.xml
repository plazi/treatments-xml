<quantity box="[935,1027,1880,1906]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="42" pageNumber="43" unit="m" value="1100.0">1100 m</quantity>
, 
<date box="[1041,1177,1880,1906]" pageId="42" pageNumber="43" value="1980-06-20">
<number box="[1041,1073,1880,1906]" pageId="42" pageNumber="43" value="20.0">20</number>
.VI. 
<number box="[1114,1177,1880,1906]" pageId="42" pageNumber="43" value="1980.0">1980</number>
