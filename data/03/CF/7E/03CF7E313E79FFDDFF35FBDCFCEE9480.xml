<collectionCode LSID="urn:lsid:biocol.org:col:34784" box="[270,347,1565,1591]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34784" name="Academy of Natural Sciences of Philadelphia" pageId="24" pageNumber="25">ANSP</collectionCode>
