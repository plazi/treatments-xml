<taxonomicName baseAuthorityName="Holland" baseAuthorityYear="1893" box="[851,961,1879,1905]" class="Insecta" family="Erebidae" genus="Cyana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="379" phylum="Arthropoda" rank="species" species="torrida">
