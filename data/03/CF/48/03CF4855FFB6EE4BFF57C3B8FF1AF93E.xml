<subSubSection box="[151,827,222,245]" pageId="36" pageNumber="37" type="materials_examined">
<paragraph blockId="36.[151,827,222,245]" box="[151,827,222,245]" pageId="36" pageNumber="37">
<typeStatus box="[151,200,223,244]" pageId="36" pageNumber="37">Type</typeStatus>
species: 
<taxonomicName box="[294,520,223,245]" class="Arachnidae" family="Proctophyllodidae" genus="Picipterodectes" kingdom="Animalia" order="Astigmata" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="sasiae" status="sp. n." subFamily="Pterodectinae">
