<location LSID="urn:lsid:plazi:treatment:03CF0539AA7DFFA2FEF4FCB2FD0EFA38:8EB9E2F4AA7DFFA2FBBEFC0AFB9CFB88" box="[1090,1114,1022,1048]" country="Mexico, Costa Rica" name="In" pageId="55" pageNumber="56">In</location>
