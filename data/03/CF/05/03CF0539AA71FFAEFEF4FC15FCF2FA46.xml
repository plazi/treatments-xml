<taxonomicName authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="lanceolatus">
