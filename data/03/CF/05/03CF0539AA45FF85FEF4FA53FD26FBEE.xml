<subSubSection box="[312,719,964,990]" pageId="16" pageNumber="17" type="description">
<paragraph blockId="16.[264,1324,284,1150]" box="[312,719,964,990]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[312,437,964,990]" pageId="16" pageNumber="17">Variation.</emphasis>
