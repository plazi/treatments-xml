<emphasis box="[858,1001,1823,1848]" italics="true" pageId="62" pageNumber="63">
<taxonomicName authority="Azevedo, 2003" authorityName="Azevedo" authorityYear="2003" box="[858,996,1823,1848]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="stellatus" status="sp. n.">D. stellatus</taxonomicName>
