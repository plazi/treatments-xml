</taxonomicName>
, and 
<taxonomicName authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="tortuosus">
<emphasis box="[1177,1322,1783,1808]" italics="true" pageId="62" pageNumber="63">D. tortuosus</emphasis>
Azevedo, 1999
</taxonomicName>
