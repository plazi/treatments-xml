<emphasis box="[740,914,1743,1768]" italics="true" pageId="62" pageNumber="63">D. bicarinatus</emphasis>
Azevedo, 1999
</taxonomicName>
, 
<taxonomicName authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="largidentatus">
<emphasis box="[1127,1322,1743,1768]" italics="true" pageId="62" pageNumber="63">D. largidentatus</emphasis>
Azevedo, 1999
</taxonomicName>
, 
<taxonomicName authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" box="[457,780,1783,1809]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="stellatus">
<emphasis box="[457,592,1783,1808]" italics="true" pageId="62" pageNumber="63">D. stellatus</emphasis>
Azevedo, 1999
</taxonomicName>
, 
<taxonomicName authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" box="[792,1114,1783,1809]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="stellatus">
<emphasis box="[792,926,1783,1808]" italics="true" pageId="62" pageNumber="63">D. stellatus</emphasis>
