<collectingCountry box="[1140,1230,1411,1437]" name="Georgia" pageId="23" pageNumber="24">Georgia</collectingCountry>
and 
<collectingRegion box="[1286,1370,1411,1437]" country="United States of America" name="Florida" pageId="23" pageNumber="24">Florida</collectingRegion>
