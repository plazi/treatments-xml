<subSubSection box="[197,1174,1269,1294]" pageId="7" pageNumber="8" type="etymology">
<paragraph blockId="7.[151,1437,474,1473]" box="[197,1174,1269,1294]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[197,335,1270,1294]" pageId="7" pageNumber="8">Etymology:</emphasis>
