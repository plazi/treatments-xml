<subSubSection pageId="15" pageNumber="54" type="materials_examined">
<paragraph blockId="15.[151,1437,151,535]" pageId="15" pageNumber="54">
<emphasis bold="true" box="[197,374,367,392]" pageId="15" pageNumber="54">
<typeStatus box="[197,256,368,392]" pageId="15" pageNumber="54">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[384,491,367,391]" pageId="15" pageNumber="54">Holotype</typeStatus>
, 
<number box="[505,526,368,392]" pageId="15" pageNumber="54" value="3.0">3</number>
, 
<collectingCountry box="[544,636,367,391]" name="China" pageId="15" pageNumber="54">CHINA</collectingCountry>
. 
<emphasis bold="true" box="[650,859,367,392]" pageId="15" pageNumber="54">Yunnan Province</emphasis>
: Dali, altitude 
<quantity box="[1037,1127,367,391]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="15" pageNumber="54" unit="m" value="2500.0">2500 m</quantity>
, 
<taxonomicName box="[1142,1206,368,392]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="54" phylum="Tracheophyta" rank="genus">
<emphasis box="[1142,1206,368,392]" italics="true" pageId="15" pageNumber="54">Alnus</emphasis>
</taxonomicName>
, 
<date box="[1223,1345,367,391]" pageId="15" pageNumber="54" value="1999-11-12">
