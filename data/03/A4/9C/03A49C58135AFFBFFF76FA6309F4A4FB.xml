<number box="[1023,1033,1898,1924]" pageId="3" pageNumber="52" value="1.0">1</number>
° 
<number box="[1044,1078,1898,1924]" pageId="3" pageNumber="52" value="59.0">59</number>
’W
</geoCoordinate>
, 
<date box="[1130,1334,1898,1924]" pageId="3" pageNumber="52" value="2005-10-27">
