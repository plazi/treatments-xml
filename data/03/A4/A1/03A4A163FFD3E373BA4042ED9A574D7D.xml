(Dunker, 
<number box="[614,674,443,469]" pageId="21" pageNumber="22" value="1847.0">1847</number>
)
</taxonomicName>
<taxonomicNameLabel box="[690,824,443,469]" pageId="21" pageNumber="22">comb. nov.</taxonomicNameLabel>
