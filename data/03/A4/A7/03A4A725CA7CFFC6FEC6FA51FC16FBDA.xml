<location LSID="urn:lsid:plazi:treatment:03A4A725CA7CFFC6FEC6FA51FC16FBDA:8ED240E8CA62FFC6FB5AFDC1FA95FDFA" box="[1172,1312,604,630]" country="Malaysia" name="Mt. Goram" pageId="48" pageNumber="49" stateProvince="Sarawak">Mt. Goram</location>
