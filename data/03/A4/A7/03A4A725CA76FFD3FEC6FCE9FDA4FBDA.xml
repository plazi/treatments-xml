<paragraph blockId="37.[264,1323,284,1110]" box="[312,619,884,910]" pageId="37" pageNumber="38">
<collectingCountry box="[312,395,884,910]" name="India" pageId="37" pageNumber="38">INDIA</collectingCountry>
