Murano &amp; 
<collectingRegion box="[964,1067,702,728]" country="Japan" name="Fukuoka" pageId="35" pageNumber="36">Fukuoka</collectingRegion>
<date box="[1074,1137,703,729]" pageId="35" pageNumber="36" value="2008">2008</date>
