<collectingCountry box="[1209,1324,282,306]" name="Cameroon" pageId="11" pageNumber="298">Cameroon</collectingCountry>
and 
<collectingCountry box="[1380,1455,282,306]" name="Gabon" pageId="11" pageNumber="298">Gabon</collectingCountry>
