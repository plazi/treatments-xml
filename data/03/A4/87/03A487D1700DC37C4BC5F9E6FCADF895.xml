<taxonomicName authorityName="Rahmat &amp; Koretsky &amp; Osborne &amp; Alford" authorityYear="2017" box="[1135,1225,1178,1200]" class="Mammalia" family="Phocidae" genus="Terranectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carnivora" pageId="14" pageNumber="235" phylum="Chordata" rank="species" species="parvus">
