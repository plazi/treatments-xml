<taxonomicName authorityName="Rahmat &amp; Koretsky &amp; Osborne &amp; Alford" authorityYear="2017" box="[166,297,1295,1322]" class="Mammalia" family="Phocidae" genus="Terranectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carnivora" pageId="14" pageNumber="235" phylum="Chordata" rank="species" species="magnus">
