<subSubSection pageId="5" pageNumber="6" type="distribution">
<paragraph blockId="5.[151,1436,1266,1330]" box="[198,1436,1266,1292]" pageId="5" pageNumber="6">
<emphasis bold="true" box="[198,351,1266,1292]" pageId="5" pageNumber="6">Distribution</emphasis>
. 
<collectingCountry box="[374,540,1266,1292]" name="Australia" pageId="5" pageNumber="6">AUSTRALIA</collectingCountry>
(
<collectingRegion box="[564,797,1266,1292]" country="Australia" name="New South Wales" pageId="5" pageNumber="6">New South Wales</collectingRegion>
); 
<collectingCountry box="[825,1007,1266,1292]" name="Micronesia" pageId="5" pageNumber="6">MICRONESIA</collectingCountry>
(Island of Guam); PAPUA-NEW
