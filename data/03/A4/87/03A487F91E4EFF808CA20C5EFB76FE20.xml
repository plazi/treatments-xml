<quantity box="[752,834,1268,1292]" metricMagnitude="-2" metricUnit="m" metricValue="1.7" pageId="2" pageNumber="434" unit="mm" value="17.0">17 mm</quantity>
, cerci 
<quantity box="[908,997,1268,1292]" metricMagnitude="-3" metricUnit="m" metricValue="6.7" pageId="2" pageNumber="434" unit="mm" value="6.7">6.7 mm</quantity>
