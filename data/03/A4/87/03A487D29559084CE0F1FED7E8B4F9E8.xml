, 
<collectingCountry box="[527,598,1519,1544]" name="Japan" pageId="10" pageNumber="11">Japan</collectingCountry>
, 
<collectingCountry box="[608,689,1519,1544]" name="Taiwan" pageId="10" pageNumber="11">Taiwan</collectingCountry>
</emphasis>
(
<bibRefCitation author="Gurjanova, E. F." box="[702,886,1518,1545]" pageId="10" pageNumber="11" pagination="1 - 1029" refId="ref22492" refString="Gurjanova, E. F. (1951) Bokoplavy morej SSSR i sopredel'nykh vod (Amphipoda-Gammaridea). Akademiia Nauk SSSR, Opredeliteli po Faune SSSR, 41, 1 - 1029, 705 figs. [in Russian]" type="journal article" year="1951">Gurjanova 1951</bibRefCitation>
, 
