<caption httpUri="https://zenodo.org/record/240388/files/figure.png" pageId="3" pageNumber="206" targetBox="[178,1406,208,1217]" targetPageId="3">
<paragraph blockId="3.[151,1436,1239,1322]" pageId="3" pageNumber="206">
