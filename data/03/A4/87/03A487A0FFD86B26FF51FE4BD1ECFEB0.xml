<number box="[528,538,1757,1781]" pageId="1" pageNumber="564" value="1.0">1</number>
, JV 
<number box="[587,600,1757,1781]" pageId="1" pageNumber="564" value="2.0">2</number>
and ZV 
<number box="[705,719,1757,1781]" pageId="1" pageNumber="564" value="2.0">2</number>
), anal setae (a 
<number box="[896,907,1757,1781]" pageId="1" pageNumber="564" value="1.0">1</number>
, a 
<number box="[937,951,1757,1781]" pageId="1" pageNumber="564" value="2.0">2</number>
and a 
<number box="[1030,1044,1757,1781]" pageId="1" pageNumber="564" value="3.0">3</number>
), one pair of small solenostomes posteromedian of JV 
<number box="[386,399,1793,1817]" pageId="1" pageNumber="564" value="2.0">2</number>
and sigilla posterolaterally. Length of ventrianal shield 
<number box="[1028,1070,1793,1817]" pageId="1" pageNumber="564" value="125.0">125</number>
(
<number box="[1085,1187,1793,1817]" pageId="1" pageNumber="564" value="125.0" valueMax="135.0" valueMin="115.0">115–135</number>
), width 
<number box="[1278,1320,1793,1817]" pageId="1" pageNumber="564" value="106.0">106</number>
(
<number box="[1335,1424,1793,1817]" pageId="1" pageNumber="564" value="106.0" valueMax="117.0" valueMin="95.0">95–117</number>
). Setae JV 
<number box="[257,271,1829,1853]" pageId="1" pageNumber="564" value="4.0">4</number>
, JV 
<number box="[322,335,1829,1853]" pageId="1" pageNumber="564" value="5.0">5</number>
