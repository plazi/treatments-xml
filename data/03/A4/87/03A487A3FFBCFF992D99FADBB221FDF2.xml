<collectingCountry box="[365,531,301,327]" name="Australia" pageId="9" pageNumber="10">AUSTRALIA</collectingCountry>
(
<collectingRegion box="[547,763,301,327]" country="Australia" name="New South Wales" pageId="9" pageNumber="10">New South Wales</collectingRegion>
); 
<collectingCountry box="[783,871,301,327]" name="Japan" pageId="9" pageNumber="10">JAPAN</collectingCountry>
