<collectingCountry box="[313,412,834,858]" name="Namibia" pageId="49" pageNumber="50">Namibia</collectingCountry>
, 
<collectingCountry box="[423,569,834,858]" name="South Africa" pageId="49" pageNumber="50">South Africa</collectingCountry>
, 
<collectingCountry box="[581,704,834,858]" name="Zimbabwe" pageId="49" pageNumber="50">Zimbabwe</collectingCountry>
