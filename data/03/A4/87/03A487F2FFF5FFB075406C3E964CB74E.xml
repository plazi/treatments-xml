<taxonomicName authority="Enghoff" authorityName="Enghoff" authorityYear="2015" box="[963,1265,1268,1294]" class="Diplopoda" family="Odontopygidae" genus="Aquattuor" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Spirostreptida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="udzungwensis" status="sp. nov.">
<emphasis box="[963,1160,1268,1294]" italics="true" pageId="14" pageNumber="15">A. udzungwensis</emphasis>
Enghoff
</taxonomicName>
<taxonomicNameLabel box="[1274,1366,1269,1295]" pageId="14" pageNumber="15" rank="species">sp. nov.</taxonomicNameLabel>
In addition, 
<taxonomicName authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" box="[301,633,1303,1330]" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="basiliscus">
<emphasis box="[301,452,1303,1330]" italics="true" pageId="14" pageNumber="15">C. basiliscus</emphasis>
<bibRefCitation author="Enghoff H." box="[460,633,1303,1329]" pageId="14" pageNumber="15" pagination="1 - 75" refId="ref8067" refString="Enghoff H. 2014. A mountain of millipedes I: An endemic species-group of the genus Chaleponcus Attems, 1914, from the Udzungwa Mountains, Tanzania (Diplopoda, Spirostreptida, Odontopygidae). European Journal of Taxonomy 100: 1 - 75. http: // dx. doi. org / 10.5852 / ejt. 2014.100" type="journal article" year="2014">Enghoff, 2014</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" box="[646,967,1303,1330]" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="gracilior">
<emphasis box="[646,785,1303,1330]" italics="true" pageId="14" pageNumber="15">C. gracilior</emphasis>
<bibRefCitation author="Enghoff H." box="[793,967,1303,1329]" pageId="14" pageNumber="15" pagination="1 - 75" refId="ref8067" refString="Enghoff H. 2014. A mountain of millipedes I: An endemic species-group of the genus Chaleponcus Attems, 1914, from the Udzungwa Mountains, Tanzania (Diplopoda, Spirostreptida, Odontopygidae). European Journal of Taxonomy 100: 1 - 75. http: // dx. doi. org / 10.5852 / ejt. 2014.100" type="journal article" year="2014">Enghoff, 2014</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" box="[979,1238,1303,1330]" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="ibis">
<emphasis box="[979,1056,1303,1330]" italics="true" pageId="14" pageNumber="15">C. ibis</emphasis>
<bibRefCitation author="Enghoff H." box="[1065,1238,1303,1329]" pageId="14" pageNumber="15" pagination="1 - 75" refId="ref8067" refString="Enghoff H. 2014. A mountain of millipedes I: An endemic species-group of the genus Chaleponcus Attems, 1914, from the Udzungwa Mountains, Tanzania (Diplopoda, Spirostreptida, Odontopygidae). European Journal of Taxonomy 100: 1 - 75. http: // dx. doi. org / 10.5852 / ejt. 2014.100" type="journal article" year="2014">Enghoff, 2014</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="tintin">
<emphasis box="[1301,1398,1304,1330]" italics="true" pageId="14" pageNumber="15">C. tintin</emphasis>
<bibRefCitation author="Enghoff H." box="[189,358,1338,1365]" pageId="14" pageNumber="15" pagination="1 - 75" refId="ref8067" refString="Enghoff H. 2014. A mountain of millipedes I: An endemic species-group of the genus Chaleponcus Attems, 1914, from the Udzungwa Mountains, Tanzania (Diplopoda, Spirostreptida, Odontopygidae). European Journal of Taxonomy 100: 1 - 75. http: // dx. doi. org / 10.5852 / ejt. 2014.100" type="journal article" year="2014">Enghoff, 2014</bibRefCitation>
