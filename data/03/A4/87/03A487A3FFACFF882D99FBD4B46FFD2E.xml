. 
<collectingCountry box="[364,530,600,626]" name="Australia" pageId="24" pageNumber="25">AUSTRALIA</collectingCountry>
(
<collectingRegion box="[545,760,600,626]" country="Australia" name="Western Australia" pageId="24" pageNumber="25">Western Australia</collectingRegion>
, 
<collectingRegion box="[772,994,600,626]" country="Australia" name="Northern Territory" pageId="24" pageNumber="25">Northern Territory</collectingRegion>
).
</paragraph>
</subSubSection>
<subSubSection box="[197,1106,637,663]" pageId="24" pageNumber="25" type="etymology">
<paragraph blockId="24.[151,1436,152,663]" box="[197,1106,637,663]" pageId="24" pageNumber="25">
<emphasis bold="true" box="[197,330,637,663]" pageId="24" pageNumber="25">Etymology</emphasis>
