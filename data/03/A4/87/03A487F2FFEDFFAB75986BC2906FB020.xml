<taxonomicName authority="Frederiksen, 2013" authorityName="Frederiksen" authorityYear="2013" box="[404,760,1099,1125]" class="Diplopoda" family="Odontopygidae" genus="Aquattuor" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Spirostreptida" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
