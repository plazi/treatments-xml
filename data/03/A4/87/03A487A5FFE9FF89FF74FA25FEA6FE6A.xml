and in 
<taxonomicName authority="Mohrig &amp; Kauschke &amp; Broadley, 2018" authorityName="Mohrig &amp; Kauschke &amp; Broadley" authorityYear="2018" box="[1267,1410,224,247]" class="Insecta" family="Sciaridae" genus="Corynoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="226" phylum="Arthropoda" rank="genus" sensu="stricto" status="s. str.">
