<taxonomicNameLabel pageId="23" pageNumber="226" sensu="stricto">s. str.</taxonomicNameLabel>
[
<taxonomicName authority="(Shi &amp; Huang)" authorityName="Mohrig &amp; Kauschke &amp; Broadley" authorityYear="2018" baseAuthorityName="Mohrig &amp; Kauschke &amp; Broadley" baseAuthorityYear="2018" box="[200,674,260,285]" class="Insecta" family="Sciaridae" genus="Corynoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="226" phylum="Arthropoda" rank="species" species="longiprojecta" status="comb. n.">
