, 
<taxonomicName authority="(Shi &amp; Huang)" authorityName="Mohrig &amp; Kauschke &amp; Broadley" authorityYear="2018" baseAuthorityName="Mohrig &amp; Kauschke &amp; Broadley" baseAuthorityYear="2018" box="[698,1072,223,248]" class="Insecta" family="Sciaridae" genus="Corynoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="226" phylum="Arthropoda" rank="species" species="shennongjiana" status="comb. n.">
