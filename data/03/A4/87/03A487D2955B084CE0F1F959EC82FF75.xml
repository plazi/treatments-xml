<paragraph blockId="9.[151,1437,1628,2038]" box="[199,570,1732,1758]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[199,387,1732,1758]" pageId="9" pageNumber="10">
Ecological 
<typeStatus box="[329,382,1732,1758]" pageId="9" pageNumber="10">type</typeStatus>
