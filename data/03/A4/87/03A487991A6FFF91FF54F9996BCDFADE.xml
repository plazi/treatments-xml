<subSubSection pageId="15" pageNumber="614" type="type_taxon">
<paragraph blockId="15.[132,775,1645,1696]" pageId="15" pageNumber="614">
<collectionCode box="[132,181,1645,1669]" pageId="15" pageNumber="614">TYPE</collectionCode>
