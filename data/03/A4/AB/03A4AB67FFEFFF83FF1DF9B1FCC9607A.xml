species: 
<taxonomicName authority="Mortensen, 1903" authorityName="Mortensen" authorityYear="1903" box="[294,675,1644,1666]" class="Echinoidea" family="Echinothuriidae" genus="Sperosoma" kingdom="Animalia" order="Echinothurioida" pageId="12" pageNumber="463" phylum="Echinodermata" rank="species" species="koehleri">
