</paragraph>
<paragraph blockId="11.[160,1156,142,271]" pageId="11" pageNumber="595">
<materialsCitation ID-GBIF-Occurrence="2985527311" country="Canada" location="Nearctic Region" pageId="11" pageNumber="595" specimenCount="1" stateProvince="Alberta">
