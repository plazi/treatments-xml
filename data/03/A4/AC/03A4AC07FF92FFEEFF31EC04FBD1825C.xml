<number box="[900,977,1929,1955]" pageId="3" pageNumber="60" value="19.0" valueMax="23.0" valueMin="15.0">15–23</number>
; remainder of plate, 
<number box="[1226,1302,1929,1955]" pageId="3" pageNumber="60" value="13.5" valueMax="16.0" valueMin="11.0">11–16</number>
; with 
<number box="[1377,1436,1929,1955]" pageId="3" pageNumber="60" value="7.5" valueMax="10.0" valueMin="5.0">5–10</number>
