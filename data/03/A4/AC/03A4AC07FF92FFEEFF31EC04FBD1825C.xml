<number box="[385,462,1849,1875]" pageId="3" pageNumber="60" value="32.0" valueMax="35.0" valueMin="29.0">29–35</number>
; II, 
<number box="[513,589,1849,1875]" pageId="3" pageNumber="60" value="38.5" valueMax="41.0" valueMin="36.0">36–41</number>
; III, 
<number box="[649,726,1849,1875]" pageId="3" pageNumber="60" value="42.5" valueMax="46.0" valueMin="39.0">39–46</number>
; IV, 
<number box="[784,861,1849,1875]" pageId="3" pageNumber="60" value="46.0" valueMax="49.0" valueMin="43.0">43–49</number>
; V, 
<number box="[909,986,1849,1875]" pageId="3" pageNumber="60" value="44.5" valueMax="48.0" valueMin="41.0">41–48</number>
; VI, 
<number box="[1048,1125,1849,1875]" pageId="3" pageNumber="60" value="43.0" valueMax="46.0" valueMin="40.0">40–46</number>
