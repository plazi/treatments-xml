<number box="[197,273,1929,1955]" pageId="3" pageNumber="60" value="46.5" valueMax="55.0" valueMin="38.0">38–55</number>
; IV, 
<number box="[331,408,1929,1955]" pageId="3" pageNumber="60" value="50.5" valueMax="55.0" valueMin="46.0">46–55</number>
