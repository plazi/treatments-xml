<typeStatus box="[444,548,1884,1909]" pageId="4" pageNumber="439">Holotype</typeStatus>
♂, 
<collectingCountry box="[595,666,1884,1909]" name="Brazil" pageId="4" pageNumber="439">Brazil</collectingCountry>
, State of Pará, Jacarecanga; 
<date box="[996,1091,1884,1909]" pageId="4" pageNumber="439" value="1968-12">
xii. 
<number box="[1032,1091,1884,1909]" pageId="4" pageNumber="439" value="1968.0">1968</number>
