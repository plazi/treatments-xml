<taxonomicName authorityName="Neumann" authorityYear="1897" box="[344,576,311,335]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="7" pageNumber="45" phylum="Arthropoda" rank="species" species="nitens">
