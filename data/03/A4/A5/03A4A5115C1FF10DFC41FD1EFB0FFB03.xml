<collectingCountry box="[1023,1094,1061,1085]" name="Brazil" pageId="6" pageNumber="44">Brazil</collectingCountry>
, 
<collectingCountry box="[1106,1191,1061,1085]" name="Bolivia" pageId="6" pageNumber="44">Bolivia</collectingCountry>
, 
<collectingCountry box="[1202,1329,1061,1085]" name="Costa Rica" pageId="6" pageNumber="44">Costa Rica</collectingCountry>
, 
<collectingCountry name="Colombia" pageId="6" pageNumber="44">Colombia</collectingCountry>
, 
<collectingCountry box="[874,1005,1096,1120]" name="Guatemala" pageId="6" pageNumber="44">Guatemala</collectingCountry>
, 
<collectingCountry box="[1015,1140,1096,1120]" name="Nicaragua" pageId="6" pageNumber="44">Nicaragua</collectingCountry>
, 
<collectingCountry box="[1150,1247,1097,1120]" name="Panama" pageId="6" pageNumber="44">Panama</collectingCountry>
, 
<collectingCountry box="[1257,1380,1096,1120]" name="Venezuela" pageId="6" pageNumber="44">Venezuela</collectingCountry>
, 
<collectingCountry name="Argentina" pageId="6" pageNumber="44">Argentina</collectingCountry>
, 
<collectingCountry box="[931,1017,1131,1155]" name="Mexico" pageId="6" pageNumber="44">Mexico</collectingCountry>
