<emphasis box="[1039,1273,703,727]" italics="true" pageId="2" pageNumber="600">
......... 
<taxonomicName authority="Zhang et Yang" authorityName="Zhang et Yang" box="[1100,1273,703,727]" class="Insecta" family="Dolichopodidae" genus="Setihercostomus" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="600" phylum="Arthropoda" rank="genus">Setihercostomus</taxonomicName>
