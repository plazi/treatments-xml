</taxonomicName>
; 
<taxonomicName authority="Haenni, 2001" authorityName="Haenni" authorityYear="2001" class="Insecta" family="Scatopsidae" genus="Rhexoza" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="44" phylum="Arthropoda" rank="species" species="flixella">
<emphasis box="[1223,1338,875,900]" italics="true" pageId="3" pageNumber="44">R. flixella</emphasis>
