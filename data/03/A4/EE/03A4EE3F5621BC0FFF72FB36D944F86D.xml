<taxonomicName authority="Duda, 1937" authorityName="Duda" authorityYear="1937" class="Insecta" family="Scatopsidae" genus="Quateiella" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="46" phylum="Arthropoda" rank="species" species="freyi" status="n.comb.">
<emphasis box="[1344,1436,1968,1993]" italics="true" pageId="5" pageNumber="46">Q. freyi</emphasis>
(
<bibRefCitation author="Duda" box="[159,301,2008,2034]" pageId="5" pageNumber="52" refString="Duda, O. (1937) Fam. Scatopsidae. In: Frey, R., Die Dipterenfauna der Kanarischen Inseln und ihre Probleme. Commentationes biologicae, 6 (1) (1936), 1 - 234, 10 plates, Helsinki." type="book chapter" year="1937">
Duda, 
<number box="[239,301,2008,2034]" pageId="5" pageNumber="46" value="1937.0">1937</number>
</bibRefCitation>
)
</taxonomicName>
, 
<emphasis bold="true" box="[322,420,2008,2034]" pageId="5" pageNumber="46">
<taxonomicNameLabel box="[322,420,2008,2034]" pageId="5" pageNumber="46">n.comb.</taxonomicNameLabel>
</emphasis>
; 
<taxonomicName authority="Haenni, 1988" authorityName="Haenni" authorityYear="1988" box="[436,776,2008,2034]" class="Insecta" family="Scatopsidae" genus="Quateiella" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="46" phylum="Arthropoda" rank="species" species="inexpectata">
<emphasis box="[436,457,2008,2033]" italics="true" pageId="5" pageNumber="46">Q</emphasis>
. 
<emphasis box="[472,607,2008,2033]" italics="true" pageId="5" pageNumber="46">inexpectata</emphasis>
<bibRefCitation author="Haenni" box="[615,776,2008,2034]" pageId="5" pageNumber="52" refString="Haenni, J. - P. (1988) First record of Quateiella Cook from Europe, with description of a new species from Spain (Diptera, Scatopsidae). Bulletin de la Societe Entomologique Suisse, 61, 153 - 156." type="journal article" year="1988">
Haenni, 
<number box="[714,776,2008,2034]" pageId="5" pageNumber="46" value="1988.0">1988</number>
</bibRefCitation>
</taxonomicName>
. Neotropical: 
<taxonomicName box="[941,1065,2008,2033]" class="Insecta" family="Scatopsidae" genus="Quateiella" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis box="[941,1065,2008,2033]" italics="true" pageId="5" pageNumber="46">Quateiella</emphasis>
</taxonomicName>
sp. 
<number box="[1113,1129,2008,2034]" pageId="5" pageNumber="46" value="1.0">1</number>
.
</paragraph>
