<paragraph blockId="50.[151,1437,1868,2003]" pageId="50" pageNumber="51">
<materialsCitation ID-GBIF-Occurrence="3026613311" country="Mexico" county="Museo Regionale di Scienze Naturali" location="Bollettino Musei di Zoologia" municipality="Universita di Torino" pageId="50" pageNumber="51" specimenCount="1" specimenCount-female="1" typeStatus="allotype">
