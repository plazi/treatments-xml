<taxonomicName box="[1178,1299,763,789]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
