<emphasis bold="true" box="[151,629,153,180]" pageId="1" pageNumber="594">
<taxonomicName authority="Mound &amp; Li &amp; Zhang, 2020" authorityName="Mound &amp; Li &amp; Zhang" authorityYear="2020" box="[151,432,153,180]" class="Insecta" family="Phlaeothripidae" genus="Allothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="594" phylum="Arthropoda" rank="species" species="yunnanensis" status="sp.n.">
<emphasis bold="true" box="[151,432,153,180]" italics="true" pageId="1" pageNumber="594">Allothrips yunnanensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[440,499,154,180]" pageId="1" pageNumber="594" rank="species">sp.n.</taxonomicNameLabel>
