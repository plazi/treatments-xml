<collectorName box="[556,719,549,576]" pageId="66" pageNumber="67">J. Kekenbosch</collectorName>
leg. (
<specimenCode box="[782,962,549,576]" collectionCode="MRAC" country="Belgium" httpUri="http://grbio.org/cool/z14b-bb31" name="Musée Royal de l’Afrique Centrale" pageId="66" pageNumber="67" type="Museum">MRAC 237633</specimenCode>
)
</materialsCitation>
