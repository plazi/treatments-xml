<emphasis box="[650,877,1935,1957]" italics="true" pageId="2" pageNumber="531">Scolytodes uniseriatus</emphasis>
</taxonomicName>
(1, 4, 7), 
<taxonomicName box="[975,1156,1935,1957]" class="Insecta" family="Curculionidae" genus="Scolytodes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="531" phylum="Arthropoda" rank="species" species="clusiapraelatus">
<emphasis box="[975,1156,1935,1957]" italics="true" pageId="2" pageNumber="531">S. clusiapraelatus</emphasis>
</taxonomicName>
(2, 5, 8), 
<taxonomicName box="[1255,1402,1935,1957]" class="Insecta" family="Curculionidae" genus="Scolytodes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="531" phylum="Arthropoda" rank="species" species="clusiaphilus">
