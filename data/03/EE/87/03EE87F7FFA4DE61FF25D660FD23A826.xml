<subSubSection box="[199,1350,1340,1365]" pageId="8" pageNumber="9" type="etymology">
<paragraph blockId="8.[151,1437,151,1400]" box="[199,1350,1340,1365]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[199,334,1340,1365]" pageId="8" pageNumber="9">Etymology.</emphasis>
