<taxonomicName authority="Huang &amp; Nel, 2009" authorityName="Huang &amp; Nel" authorityYear="2009" box="[305,413,673,694]" class="Insecta" family="Sinembiidae" genus="Sinembia" kingdom="Animalia" order="Embioptera" pageId="1" pageNumber="890" phylum="Arthropoda" rank="genus" status="gen. nov.">Sinembia</taxonomicName>
</emphasis>
<taxonomicNameLabel box="[422,526,673,694]" pageId="1" pageNumber="890" rank="genus">gen. nov.</taxonomicNameLabel>
, other genus 
<taxonomicName authority="Huang &amp; Nel, 2009" authorityName="Huang &amp; Nel" authorityYear="2009" class="Insecta" family="Sinembiidae" genus="Juraembia" kingdom="Animalia" order="Embioptera" pageId="1" pageNumber="890" phylum="Arthropoda" rank="genus" status="gen. nov.">
