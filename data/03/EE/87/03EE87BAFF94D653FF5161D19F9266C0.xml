<number box="[641,654,1895,1919]" pageId="1" pageNumber="128" value="3.0">3</number>
</emphasis>
<number box="[661,689,1894,1919]" pageId="1" pageNumber="128" value="16.0">16</number>
(
<number box="[705,778,1895,1919]" pageId="1" pageNumber="128" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
), 
<emphasis box="[798,879,1894,1919]" italics="true" pageId="1" pageNumber="128">
ps 
<number box="[824,837,1895,1919]" pageId="1" pageNumber="128" value="3.0">3</number>
