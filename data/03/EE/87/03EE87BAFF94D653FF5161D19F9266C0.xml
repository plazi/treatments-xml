<typeStatus box="[744,856,2003,2027]" pageId="1" pageNumber="128">paratypes</typeStatus>
.) Idiosoma 
<number box="[988,1086,2003,2027]" pageId="1" pageNumber="128" value="335.0" valueMax="340.0" valueMin="330.0">330–340</number>
