<taxonomicName authorityName="MULSANT &amp; REY" authorityYear="1873" box="[1167,1285,1536,1559]" class="Insecta" family="Staphylinidae" genus="Myrmoecia" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="35" phylum="Arthropoda" rank="genus">
