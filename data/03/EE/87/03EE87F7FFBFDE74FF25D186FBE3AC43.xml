). 
<collectingRegion box="[464,555,1879,1902]" country="Sri Lanka" name="Western" pageId="28" pageNumber="29">Western</collectingRegion>
<collectingCountry box="[562,633,1878,1903]" name="Nepal" pageId="28" pageNumber="29">Nepal</collectingCountry>
