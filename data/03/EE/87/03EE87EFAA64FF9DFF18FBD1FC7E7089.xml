<taxonomicName authority="Rheims &amp; Jager, 2008" authorityName="Rheims &amp; Jager" authorityYear="2008" box="[730,1116,1646,1671]" class="Arachnida" family="Sparassidae" genus="Vindullus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="573" phylum="Arthropoda" rank="species" species="gibbosus">
