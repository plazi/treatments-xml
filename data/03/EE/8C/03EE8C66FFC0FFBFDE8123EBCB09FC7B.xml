<subSubSection pageId="47" pageNumber="48" type="etymology">
<paragraph blockId="47.[151,1437,151,899]" pageId="47" pageNumber="48">
<emphasis bold="true" box="[199,334,838,863]" pageId="47" pageNumber="48">Etymology.</emphasis>
