<paragraph blockId="2.[151,1321,1563,1765]" lastBlockId="3.[151,1437,152,559]" lastPageId="3" lastPageNumber="68" pageId="2" pageNumber="67">
Tib Fe 
<collectingCountry box="[709,732,1614,1637]" name="Chile" pageId="2" pageNumber="67">Tr</collectingCountry>
Cx Scx 
<number box="[1123,1137,1614,1637]" pageId="2" pageNumber="67" value="2.0">2</number>
Scx 
<number box="[1307,1321,1614,1637]" pageId="2" pageNumber="67" value="1.0">1</number>
Leg I 
<number box="[342,368,1657,1680]" pageId="2" pageNumber="67" value="19.0">19</number>
<date box="[526,906,1657,1680]" pageId="2" pageNumber="67" value="1912-04-05">
<number box="[526,553,1657,1680]" pageId="2" pageNumber="67" value="12.0">12</number>
<number box="[709,722,1657,1680]" pageId="2" pageNumber="67" value="5.0">5</number>
<number box="[893,906,1657,1680]" pageId="2" pageNumber="67" value="4.0">4</number>
</date>
0 
<number box="[1260,1273,1657,1680]" pageId="2" pageNumber="67" value="1.0">1</number>
Leg II 
<number box="[342,368,1700,1723]" pageId="2" pageNumber="67" value="19.0">19</number>
<date box="[526,906,1700,1723]" pageId="2" pageNumber="67" value="1911-07-05">
<number box="[526,551,1700,1723]" pageId="2" pageNumber="67" value="11.0">11</number>
<number box="[709,722,1700,1723]" pageId="2" pageNumber="67" value="5.0">5</number>
