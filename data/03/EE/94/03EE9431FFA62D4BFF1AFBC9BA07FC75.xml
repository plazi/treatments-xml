<taxonomicName box="[196,410,1362,1387]" class="Magnoliopsida" family="Apocynaceae" genus="Calotropis" kingdom="Plantae" order="Gentianales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="species" species="procera">
<emphasis box="[196,410,1362,1387]" italics="true" pageId="43" pageNumber="44">Calotropis procera</emphasis>
</taxonomicName>
and 
<taxonomicName box="[464,589,1363,1386]" class="Magnoliopsida" family="Malvaceae" genus="Gossypium" kingdom="Plantae" order="Malvales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="genus">
