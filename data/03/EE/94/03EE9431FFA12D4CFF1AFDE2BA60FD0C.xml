<paragraph blockId="44.[151,1436,637,870]" pageId="44" pageNumber="45">
<emphasis bold="true" box="[151,524,775,800]" pageId="44" pageNumber="45">General distribution and hosts.</emphasis>
<collectingCountry box="[531,579,776,799]" name="Iran" pageId="44" pageNumber="45">Iran</collectingCountry>
, 
<collectingCountry box="[591,639,776,799]" name="Iraq" pageId="44" pageNumber="45">Iraq</collectingCountry>
, 
<collectingCountry box="[650,808,775,800]" name="Turkmenistan" pageId="44" pageNumber="45">Turkmenistan</collectingCountry>
, 
<collectingCountry box="[819,948,775,800]" name="Uzbekistan" pageId="44" pageNumber="45">Uzbekistan</collectingCountry>
