<collectingCountry box="[1184,1318,775,800]" name="Kazakhstan" pageId="44" pageNumber="45">Kazakhstan</collectingCountry>
, 
<collectingCountry box="[1329,1388,775,800]" name="Syria" pageId="44" pageNumber="45">Syria</collectingCountry>
