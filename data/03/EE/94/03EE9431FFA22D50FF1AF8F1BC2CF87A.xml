<taxonomicName box="[375,441,295,320]" class="Insecta" family="Miridae" genus="Strongylocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="38" phylum="Arthropoda" rank="species" species="niger">
