(
<emphasis box="[701,810,1506,1531]" italics="true" pageId="53" pageNumber="54">Euphorus</emphasis>
) 
<emphasis box="[836,936,1506,1531]" italics="true" pageId="53" pageNumber="54">deficiens</emphasis>
Ruthe, 
<number box="[1043,1099,1506,1531]" pageId="53" pageNumber="54" value="1856.0">1856</number>
</taxonomicName>
(
<taxonomicName box="[1123,1280,1507,1531]" class="Insecta" family="Hymenoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
