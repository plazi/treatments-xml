<collectingCountry box="[1191,1317,1517,1542]" name="Azerbaijan" pageId="65" pageNumber="66">Azerbaijan</collectingCountry>
, 
<collectingCountry box="[1331,1432,1517,1542]" name="Armenia" pageId="65" pageNumber="66">Armenia</collectingCountry>
, 
<collectingCountry box="[151,244,1553,1578]" name="Georgia" pageId="65" pageNumber="66">Georgia</collectingCountry>
, 
<collectingCountry box="[260,308,1554,1577]" name="Iran" pageId="65" pageNumber="66">Iran</collectingCountry>
, 
<collectingCountry box="[325,373,1554,1577]" name="Iraq" pageId="65" pageNumber="66">Iraq</collectingCountry>
, 
<collectingCountry box="[389,454,1553,1578]" name="Israel" pageId="65" pageNumber="66">Israel</collectingCountry>
