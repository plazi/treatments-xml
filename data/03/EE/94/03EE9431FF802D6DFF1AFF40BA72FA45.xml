<collectingCountry box="[1241,1321,330,355]" name="Turkey" pageId="13" pageNumber="14">Turkey</collectingCountry>
and 
<collectingCountry box="[1383,1432,331,354]" name="Iran" pageId="13" pageNumber="14">Iran</collectingCountry>
; collected in 
<collectingCountry box="[291,336,367,390]" name="Iran" pageId="13" pageNumber="14">Iran</collectingCountry>
on 
<taxonomicName authority="Abd-Rabou &amp; Ghahari 2006" authorityName="Abd-Rabou &amp; Ghahari" authorityYear="2006" box="[381,980,366,391]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="species" species="heliosopia">
