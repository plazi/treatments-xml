. 
<taxonomicName box="[151,258,1122,1147]" class="Insecta" family="Hemiptera" genus="Atomophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="maculosa">
