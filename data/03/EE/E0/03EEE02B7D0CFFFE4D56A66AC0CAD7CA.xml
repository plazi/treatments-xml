<quantity box="[1162,1306,1532,1556]" metricMagnitude="-4" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="5" pageNumber="180" unit="mm" value="0.275" valueMax="0.3" valueMin="0.25">0.25–0.30 mm</quantity>
