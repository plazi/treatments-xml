<quantity box="[954,1098,1532,1556]" metricMagnitude="-4" metricUnit="m" metricValue="5.9" metricValueMax="6.5" metricValueMin="5.3" pageId="5" pageNumber="180" unit="mm" value="0.59" valueMax="0.65" valueMin="0.53">0.53–0.65 mm</quantity>
