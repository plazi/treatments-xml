<quantity metricMagnitude="-4" metricUnit="m" metricValue="1.7" metricValueMax="2.0" metricValueMin="1.4" pageId="5" pageNumber="180" unit="mm" value="0.17" valueMax="0.2" valueMin="0.14">0.14– 0.20 mm</quantity>
