<quantity box="[1054,1142,1565,1589]" metricMagnitude="-4" metricUnit="m" metricValue="6.2" pageId="5" pageNumber="180" unit="mm" value="0.62">0.62 mm</quantity>
, H= 
<quantity box="[1197,1285,1565,1589]" metricMagnitude="-4" metricUnit="m" metricValue="2.8" pageId="5" pageNumber="180" unit="mm" value="0.28">0.28 mm</quantity>
