<taxonomicName authorityName="Gahan" authorityYear="1888" box="[293,445,538,564]" class="Insecta" family="Cerambycidae" genus="Trachylophus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="586" phylum="Arthropoda" rank="genus">
<emphasis box="[293,445,538,564]" italics="true" pageId="6" pageNumber="586">Trachylophus</emphasis>
</taxonomicName>
and is junior synonym of 
<taxonomicName authority="Gahan, 1888" authorityName="Gahan" authorityYear="1888" box="[740,1075,538,564]" class="Insecta" family="Cerambycidae" genus="Trachylophus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="586" phylum="Arthropoda" rank="species" species="approximator">
