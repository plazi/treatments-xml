(
<quantity box="[1071,1164,1669,1693]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" metricValueMax="6.0" metricValueMin="4.0" pageId="26" pageNumber="2081" unit="mm" value="5.0" valueMax="6.0" valueMin="4.0">4–6 mm</quantity>
