<number box="[873,921,989,1015]" pageId="0" pageNumber="65" value="167.0">167</number>
°11.61’E
</geoCoordinate>
, 
<quantity box="[1044,1098,989,1015]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="0" pageNumber="65" unit="m" value="30.0">30m</quantity>
