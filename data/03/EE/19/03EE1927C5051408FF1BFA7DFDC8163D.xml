<taxonomicName authority="Pall-Gergely, A. Reischutz &amp; Maassen" authorityName="Pall-Gergely, A. Reischutz &amp; Maassen" authorityYear="2020" class="Gastropoda" family="Diapheridae" genus="Laoennea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="1" pageNumber="2" phylum="Mollusca" rank="species" species="carychioides" status="new species">
