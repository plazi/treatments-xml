<taxonomicName baseAuthorityName="DEJEAN" baseAuthorityYear="1828" box="[382,504,1149,1170]" class="Insecta" family="Carabidae" genus="Pogonistes" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="521" phylum="Arthropoda" rank="species" species="testaceus">
