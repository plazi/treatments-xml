<taxonomicName authority="Anker, 2020" authorityName="Anker" authorityYear="2020" box="[280,512,1764,1788]" class="Malacostraca" family="Alpheidae" genus="Triacanthoneus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="456" phylum="Arthropoda" rank="species" species="blanca" status="sp. nov.">
