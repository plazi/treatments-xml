<taxonomicName authorityName="Mengual &amp; Ghorpadé" authorityYear="2010" box="[819,940,1539,1565]" class="Insecta" family="Syrphidae" genus="Eosphaerophoria" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="50" phylum="Arthropoda" rank="species" species="hermosa">
