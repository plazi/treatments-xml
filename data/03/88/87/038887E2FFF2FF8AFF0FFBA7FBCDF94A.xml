<paragraph blockId="8.[140,1108,1293,1636]" box="[140,337,1293,1319]" pageId="8" pageNumber="47">
<emphasis bold="true" box="[140,207,1293,1319]" pageId="8" pageNumber="47">
<emphasis box="[140,202,1293,1319]" inLineHeading="true" pageId="8" pageNumber="47" reason="1">Male</emphasis>
