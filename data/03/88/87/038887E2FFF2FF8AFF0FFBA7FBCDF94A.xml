<taxonomicName authorityName="Mengual &amp; Ghorpadé" authorityYear="2010" box="[367,496,1575,1601]" class="Insecta" family="Syrphidae" genus="Eosphaerophoria" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="50" phylum="Arthropoda" rank="species" species="adornata">
