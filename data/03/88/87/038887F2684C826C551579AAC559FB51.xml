species 
<taxonomicName authority="Larsen &amp; Błażewicz-Paszkowycz &amp; Cunha, 2006" authorityName="Larsen &amp; Błażewicz-Paszkowycz &amp; Cunha" authorityYear="2006" box="[312,561,1873,1898]" class="Malacostraca" family="Typhlotanaidae" genus="Obesutanais" kingdom="Animalia" order="Tanaidacea" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="sigridae" status="n. sp.">Obesutanais sigridae</taxonomicName>
</emphasis>
<taxonomicNameLabel box="[569,635,1873,1899]" pageId="14" pageNumber="15" rank="species">
