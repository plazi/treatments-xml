<paragraph blockId="1.[151,515,626,652]" box="[151,515,626,652]" pageId="1" pageNumber="2">
<heading bold="true" box="[151,515,626,652]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<emphasis bold="true" box="[151,515,626,652]" pageId="1" pageNumber="2">
Genus 
<taxonomicName authority="Bosc, 1802" authorityName="Bosc" authorityYear="1802" box="[238,515,626,652]" class="Malacostraca" family="Sphaeromatidae" genus="Sphaeroma" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,377,626,652]" italics="true" pageId="1" pageNumber="2">Sphaeroma</emphasis>
