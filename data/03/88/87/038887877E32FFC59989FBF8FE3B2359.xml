<quantity box="[663,803,1856,1880]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" metricValueMax="2.4" metricValueMin="2.1" pageId="1" pageNumber="374" unit="mm" value="2.25" valueMax="2.4" valueMin="2.1">2.1–2.4 mm</quantity>
