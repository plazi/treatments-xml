<quantity box="[394,490,1676,1700]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="1" pageNumber="374" unit="mm" value="1.1">1.1 mm</quantity>
; wIdth 
<quantity box="[591,739,1676,1700]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="1" pageNumber="374" unit="mm" value="0.85" valueMax="0.9" valueMin="0.8">0.8–0.9 mm</quantity>
