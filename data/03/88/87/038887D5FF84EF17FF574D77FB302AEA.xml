<number box="[1113,1143,1846,1870]" pageId="44" pageNumber="45" value="37.0">37</number>
, 
<emphasis box="[1156,1168,1846,1869]" italics="true" pageId="44" pageNumber="45">e</emphasis>
<number box="[1177,1208,1846,1870]" pageId="44" pageNumber="45" value="27.0">27</number>
, 
<emphasis box="[1220,1242,1845,1870]" italics="true" pageId="44" pageNumber="45">la</emphasis>
<number box="[1250,1280,1845,1870]" pageId="44" pageNumber="45" value="25.0">25</number>
, 
