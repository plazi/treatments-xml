<number box="[1329,1359,1846,1870]" pageId="44" pageNumber="45" value="32.0">32</number>
, 
<emphasis box="[1372,1392,1845,1870]" italics="true" pageId="44" pageNumber="45">le</emphasis>
<number box="[1401,1432,1846,1870]" pageId="44" pageNumber="45" value="27.0">27</number>
. Distances: 
<emphasis box="[275,317,1882,1905]" italics="true" pageId="44" pageNumber="45">a–a</emphasis>
<number box="[324,354,1882,1906]" pageId="44" pageNumber="45" value="33.0">33</number>
, 
