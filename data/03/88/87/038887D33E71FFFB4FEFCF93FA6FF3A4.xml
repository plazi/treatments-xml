<collectingRegion box="[1039,1147,827,853]" country="Japan" name="Nagasaki" pageId="5" pageNumber="490">Nagasaki</collectingRegion>
; 
<quantity box="[1160,1291,827,853]" metricMagnitude="2" metricUnit="m" metricValue="4.475" metricValueMax="6.02" metricValueMin="2.93" pageId="5" pageNumber="490" unit="m" value="447.5" valueMax="602.0" valueMin="293.0">
