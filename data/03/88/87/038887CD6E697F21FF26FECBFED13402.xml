<taxonomicName authorityName="Chaudoir" authorityYear="1850" box="[671,790,511,533]" class="Insecta" family="Carabidae" genus="Emphanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="latiplaga">
