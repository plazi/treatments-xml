<date box="[1256,1409,404,428]" pageId="5" pageNumber="264" value="1982-06-12">
<number box="[1256,1285,404,428]" pageId="5" pageNumber="264" value="12.0">12</number>
June 1982
</date>
, 8 and 
<number box="[204,231,440,464]" pageId="5" pageNumber="264" value="11.0">11</number>
March 1983, 
<date box="[394,546,440,464]" pageId="5" pageNumber="264" value="1983-05-19">19 May 1983</date>
, 
<date box="[564,785,439,464]" pageId="5" pageNumber="264" value="1983-09-17">
17 September 
<number box="[725,785,440,464]" pageId="5" pageNumber="264" value="1983.0">1983</number>
</date>
