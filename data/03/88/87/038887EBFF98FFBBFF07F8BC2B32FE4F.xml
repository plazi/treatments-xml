<number box="[531,583,476,500]" pageId="5" pageNumber="264" value="2.19">2.19</number>
–VI. 
<number box="[634,686,476,500]" pageId="5" pageNumber="264" value="2.22">2.22</number>
), 
<collectingCountry box="[718,797,477,500]" name="France" pageId="5" pageNumber="264">France</collectingCountry>
