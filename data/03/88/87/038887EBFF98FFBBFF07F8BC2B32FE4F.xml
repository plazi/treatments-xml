<date box="[1104,1238,583,608]" pageId="5" pageNumber="264" value="2008-07-08">
<number box="[1104,1118,584,608]" pageId="5" pageNumber="264" value="8.0">8</number>
July 
<number box="[1178,1238,584,608]" pageId="5" pageNumber="264" value="2008.0">2008</number>
</date>
