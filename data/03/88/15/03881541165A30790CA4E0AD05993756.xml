<paragraph blockId="14.[151,492,671,732]" box="[151,492,671,697]" pageId="14" pageNumber="15">
<heading bold="true" box="[151,492,671,697]" fontSize="11" level="1" pageId="14" pageNumber="15" reason="1">
<emphasis bold="true" box="[151,492,671,697]" pageId="14" pageNumber="15">
<taxonomicName box="[151,418,671,697]" class="Insecta" family="Tettigoniidae" genus="Xiphidiola" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="lobaticerca" status="n. sp.">
<emphasis bold="true" box="[151,418,671,697]" italics="true" pageId="14" pageNumber="15">Xiphidiola lobaticerca</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[426,492,671,697]" pageId="14" pageNumber="15" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="14.[151,492,671,732]" box="[151,454,706,732]" pageId="14" pageNumber="15">
(Figs. 
<number box="[225,240,706,732]" pageId="14" pageNumber="15" value="2.0">2</number>
D–F; 
<number box="[308,324,706,732]" pageId="14" pageNumber="15" value="4.0">4</number>
J, T; 
<number box="[380,394,706,732]" pageId="14" pageNumber="15" value="6.0">6</number>
F, G)
</paragraph>
