; length of tegmen 45.0– 
<number box="[1382,1432,1662,1686]" pageId="6" pageNumber="7" value="47.5">47.5</number>
; length of body and wings 
<number box="[470,585,1698,1722]" pageId="6" pageNumber="7" value="60.1" valueMax="61.7" valueMin="58.5">58.5–61.7</number>
; length of profemur 
<number box="[837,872,1698,1722]" pageId="6" pageNumber="7" value="8.5">8.5</number>
–9.0; length of mesofemur 13.0– 
<number box="[1264,1313,1698,1722]" pageId="6" pageNumber="7" value="13.5">13.5</number>
; length of postfemur 30.0– 
<number box="[337,387,1734,1758]" pageId="6" pageNumber="7" value="33.5">33.5</number>
; length of protibia 
<number box="[599,693,1734,1758]" pageId="6" pageNumber="7" value="9.4" valueMax="9.5" valueMin="9.3">9.3– 9.5</number>
