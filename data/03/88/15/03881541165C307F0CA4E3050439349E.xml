<subSubSection pageId="9" pageNumber="10" type="materials_examined">
<paragraph blockId="9.[151,1437,151,738]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[197,451,311,337]" pageId="9" pageNumber="10">Material examined.</emphasis>
