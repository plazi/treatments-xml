<quantity box="[979,1126,1350,1376]" metricMagnitude="-2" metricUnit="m" metricValue="2.2987" metricValueMax="3.0987999999999998" metricValueMin="1.4986" pageId="7" pageNumber="8" unit="in" value="0.905" valueMax="1.22" valueMin="0.59">0.59–1.22 in</quantity>
