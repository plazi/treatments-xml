<collectingRegion box="[281,366,330,355]" country="United States of America" name="Hawaii" pageId="119" pageNumber="120">Hawaii</collectingRegion>
), 
<collectingCountry box="[393,455,330,355]" name="Cuba" pageId="119" pageNumber="120">Cuba</collectingCountry>
, 
<collectingCountry box="[474,588,330,355]" name="Argentina" pageId="119" pageNumber="120">Argentina</collectingCountry>
, 
<collectingCountry box="[607,727,330,355]" name="Seychelles" pageId="119" pageNumber="120">Seychelles</collectingCountry>
