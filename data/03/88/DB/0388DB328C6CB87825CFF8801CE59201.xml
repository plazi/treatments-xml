<typeStatus box="[151,255,727,752]" pageId="13" pageNumber="384">Holotype</typeStatus>
of 
<taxonomicName box="[292,409,728,752]" class="Cestoda" family="Anoplocephalidae" genus="Paranoplocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="13" pageNumber="384" phylum="Platyhelminthes" rank="species" species="yoccozi">
<emphasis box="[292,409,728,752]" italics="true" pageId="13" pageNumber="384">C. yoccozi</emphasis>
