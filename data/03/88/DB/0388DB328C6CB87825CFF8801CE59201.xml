<collectionCode box="[424,514,728,752]" httpUri="http://grbio.org/cool/ppqr-es1r" name="United States National Parasite Collection" pageId="13" pageNumber="384">USNPC</collectionCode>
<number box="[521,591,727,752]" pageId="13" pageNumber="384" value="95380.0">95380</number>
