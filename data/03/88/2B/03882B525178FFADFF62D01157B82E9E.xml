<subSubSection pageId="6" pageNumber="7" type="etymology">
<paragraph blockId="6.[151,1436,1872,2014]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[198,429,1949,1975]" pageId="6" pageNumber="7">Derivatio nominis.</emphasis>
The specific name 
<taxonomicName box="[656,764,1949,1974]" class="Insecta" family="Dytiscidae" genus="Hydroporus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="sivrikaya">
<emphasis box="[656,764,1949,1974]" italics="true" pageId="6" pageNumber="7">sivrikaya</emphasis>
</taxonomicName>
