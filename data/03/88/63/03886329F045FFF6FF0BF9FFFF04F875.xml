<taxonomicName box="[751,915,1974,1997]" class="Insecta" family="Lathiceridae" genus="Triamescaptor" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="267" phylum="Arthropoda" rank="genus">
<emphasis box="[751,915,1974,1997]" italics="true" pageId="3" pageNumber="267">Triamescaptor</emphasis>
</taxonomicName>
and 
<taxonomicName box="[977,1136,1973,1998]" class="Insecta" family="Lathiceridae" genus="Gryllotalpella" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="267" phylum="Arthropoda" rank="genus">
