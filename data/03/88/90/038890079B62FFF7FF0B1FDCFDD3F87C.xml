</emphasis>
<taxonomicName box="[369,659,1675,1700]" class="Insecta" family="Reduviidae" genus="Austrokatanga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="monteithi" status="new species">
