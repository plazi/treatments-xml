<taxonomicName authority="Guerrieri &amp; Ghahari, 2018" authorityName="Guerrieri &amp; Ghahari" authorityYear="2018" box="[151,347,1726,1752]" class="Insecta" family="Encyrtidae" genus="Anagyrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="317" phylum="Arthropoda" rank="species" species="osmoi" status="sp. nov.">
