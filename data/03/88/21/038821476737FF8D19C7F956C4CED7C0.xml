<taxonomicName authority="Tinsley, 1898" authorityName="Tinsley" authorityYear="1898" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="318" phylum="Arthropoda" rank="species" species="solenopsis">
<emphasis italics="true" pageId="2" pageNumber="318">Phenacoccus solenopsis</emphasis>
Tinsley, 1898
</taxonomicName>
(
<taxonomicName box="[442,567,1935,1960]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="318" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
: 
<taxonomicName box="[578,759,1935,1960]" class="Insecta" family="Pseudococcidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="318" phylum="Arthropoda" rank="family">Pseudococcidae</taxonomicName>
