<date box="[949,1131,870,895]" pageId="4" value="2005-07-30" valueMax="2005-07-31" valueMin="2005-07-30">30–31.VII.2005</date>
</collectingDate>
, leg. 
<collectorName box="[1191,1306,870,895]" pageId="4">S. Murzin</collectorName>
, slide 
<collectionCode box="[1379,1436,871,895]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="4">ZSM</collectionCode>
