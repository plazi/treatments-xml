<emphasis box="[280,440,772,795]" italics="true" pageId="8" pageNumber="581">
<taxonomicName box="[280,376,772,794]" class="Actinopterygii" family="Cyprinodontidae" genus="Aphanius" higherTaxonomySource="CoL" kingdom="Animalia" order="Cyprinodontiformes" pageId="8" pageNumber="581" phylum="Chordata" rank="genus">Aphanius</taxonomicName>
