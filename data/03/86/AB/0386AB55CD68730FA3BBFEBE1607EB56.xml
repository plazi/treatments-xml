<taxonomicName authorityName="Skoracki &amp; Oconnor" authorityYear="2010" box="[468,639,189,214]" class="Arachnida" family="Syringophilidae" genus="Stibarokris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="phoeniconaias">
