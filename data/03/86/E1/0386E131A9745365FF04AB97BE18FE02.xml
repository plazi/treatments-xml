<paragraph blockId="7.[151,1436,1544,1597]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[151,342,1544,1567]" pageId="7" pageNumber="8">FIGURES 15–18.</emphasis>
<taxonomicName box="[351,731,1544,1566]" class="Insecta" family="Hydropsychidae" genus="Smicridea" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="bifasciata" status="sp. nov." subGenus="Rhyacophylax">
<emphasis box="[351,454,1544,1566]" italics="true" pageId="7" pageNumber="8">Smicridea</emphasis>
