</taxonomicName>
, 
<taxonomicName authority="Simon, 1900" authorityName="Simon" authorityYear="1900" box="[884,1145,223,248]" class="Arachnida" family="Salticidae" genus="Amphidraus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="auriga">
