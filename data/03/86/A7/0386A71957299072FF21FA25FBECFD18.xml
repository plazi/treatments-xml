<emphasis box="[1156,1331,223,248]" italics="true" pageId="2" pageNumber="3">A. colombianus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1339,1429,223,248]" pageId="2" pageNumber="3" rank="species">
<emphasis bold="true" box="[1339,1429,223,248]" pageId="2" pageNumber="3">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName authority="Zhang &amp; Maddison, 2012" authorityName="Zhang &amp; Maddison" authorityYear="2012" box="[151,599,260,285]" class="Arachnida" family="Salticidae" genus="Amphidraus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="complexus">
<emphasis box="[151,300,260,285]" italics="true" pageId="2" pageNumber="3">A. complexus</emphasis>
<bibRefCitation author="Zhang" box="[307,599,260,285]" pageId="2" pageNumber="3" refString="Zhang, J. X. &amp; Maddison, W. P. (2012) New euophryine jumping spiders from Central and South America (Araneae: Salticidae: Euophryinae). Zootaxa, 3578, 1 - 35." type="journal article" year="2012">Zhang &amp; Maddison, 2012</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Galiano, 1967" authorityName="Galiano" authorityYear="1967" box="[610,883,260,285]" class="Arachnida" family="Salticidae" genus="Amphidraus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="duckei">
<emphasis box="[610,715,260,285]" italics="true" pageId="2" pageNumber="3">A. duckei</emphasis>
<bibRefCitation author="Galiano" box="[722,883,260,285]" pageId="2" pageNumber="3" refString="Galiano, M. E. (1967) Dos nuevas especies del genero Amphidraus Simon, 1900 (Araneae, Salticidae). Physis, Revista de la Sociedad Argentina de Ciencias Naturales (C), 27, 95 - 100." type="journal article" year="1967">Galiano, 1967</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Galiano, 1967" authorityName="Galiano" authorityYear="1967" box="[894,1195,260,285]" class="Arachnida" family="Salticidae" genus="Amphidraus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="santanae">
