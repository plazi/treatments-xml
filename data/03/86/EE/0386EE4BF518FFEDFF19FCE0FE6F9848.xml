<quantity box="[623,767,1117,1143]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" metricValueMax="1.3" metricValueMin="1.1" pageId="1" pageNumber="57" unit="mm" value="1.2" valueMax="1.3" valueMin="1.1">1.1–1.3 mm</quantity>
