<subSubSection pageId="17" pageNumber="165" type="biology_ecology">
<paragraph blockId="17.[151,1436,1356,1672]" pageId="17" pageNumber="165">
<emphasis bold="true" box="[199,295,1574,1599]" pageId="17" pageNumber="165">Biology.</emphasis>
<taxonomicName box="[301,540,1575,1599]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="165" phylum="Arthropoda" rank="species" species="froggatti">
<emphasis box="[301,540,1575,1599]" italics="true" pageId="17" pageNumber="165">Sarcophaga froggatti</emphasis>
</taxonomicName>
