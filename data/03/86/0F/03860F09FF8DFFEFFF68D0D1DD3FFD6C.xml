<subSubSection box="[151,457,1446,1473]" pageId="1" pageNumber="302" type="nomenclature">
<paragraph blockId="1.[151,457,1446,1473]" box="[151,457,1446,1473]" pageId="1" pageNumber="302">
<heading bold="true" box="[151,457,1446,1473]" fontSize="11" level="1" pageId="1" pageNumber="302" reason="1">
<emphasis bold="true" box="[151,457,1446,1473]" pageId="1" pageNumber="302">
<taxonomicName authority="Borges &amp; Farias &amp; Mácola &amp; Neves &amp; Son, 2021" authorityName="Borges &amp; Farias &amp; Mácola &amp; Neves &amp; Son" authorityYear="2021" box="[151,340,1446,1473]" class="Maxillopoda" family="Entomolepididae" genus="Parmulopsyllus" kingdom="Animalia" order="Siphonostomatoida" pageId="1" pageNumber="302" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,340,1446,1473]" italics="true" pageId="1" pageNumber="302">Parmulopsyllus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[348,457,1447,1473]" pageId="1" pageNumber="302" rank="genus">gen. nov.</taxonomicNameLabel>
