<emphasis box="[151,379,312,333]" italics="true" pageId="2" pageNumber="66">
Mariguitaia 
<taxonomicName authority="Gonzalez-Sponga, 2004: 64" authorityName="Gonzalez-Sponga" authorityPageNumber="64" authorityYear="2004" box="[273,379,312,333]" class="Arachnida" family="Pholcidae" genus="Micropholcus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="66" phylum="Arthropoda" rank="species" species="divergentis">divergentis</taxonomicName>
</emphasis>
González-Sponga, 
<number box="[567,618,311,332]" pageId="2" pageNumber="66" value="2004.0">2004</number>
: 
<number box="[627,690,311,332]" pageId="2" pageNumber="66" value="65.5" valueMax="67.0" valueMin="64.0">64–67</number>
; pl. 
<number box="[731,743,311,332]" pageId="2" pageNumber="66" value="1.0">1</number>
, figs. 
<number box="[802,840,311,332]" pageId="2" pageNumber="66" value="5.0" valueMax="9.0" valueMin="1.0">1–9</number>
; new synonymy. 
<emphasis box="[151,373,344,365]" italics="true" pageId="2" pageNumber="66">Mariguitaia museorum</emphasis>
González-Sponga, 
<number box="[561,613,343,364]" pageId="2" pageNumber="66" value="2004.0">2004</number>
: 
<number box="[621,684,343,364]" pageId="2" pageNumber="66" value="69.0" valueMax="70.0" valueMin="68.0">68–70</number>
; pl. 
<number box="[724,738,343,364]" pageId="2" pageNumber="66" value="2.0">2</number>
, figs. 
<number box="[797,835,343,364]" pageId="2" pageNumber="66" value="5.0" valueMax="9.0" valueMin="1.0">1–9</number>
; new synonymy. 
<emphasis box="[151,404,376,397]" italics="true" pageId="2" pageNumber="66">Mariguitaia neoespartana</emphasis>
González-Sponga, 
<number box="[591,643,375,396]" pageId="2" pageNumber="66" value="2004.0">2004</number>
: 
<number box="[652,715,375,396]" pageId="2" pageNumber="66" value="71.0" valueMax="72.0" valueMin="70.0">70–72</number>
; pl. 
<number box="[755,768,375,396]" pageId="2" pageNumber="66" value="3.0">3</number>
, figs. 
<number box="[827,866,375,396]" pageId="2" pageNumber="66" value="5.0" valueMax="9.0" valueMin="1.0">1–9</number>
; new synonymy. 
<emphasis box="[151,361,408,429]" italics="true" pageId="2" pageNumber="66">Mariguitaia sucrensis</emphasis>
González-Sponga, 
<number box="[549,601,407,428]" pageId="2" pageNumber="66" value="2004.0">2004</number>
: 
<number box="[610,672,407,428]" pageId="2" pageNumber="66" value="73.5" valueMax="75.0" valueMin="72.0">72–75</number>
