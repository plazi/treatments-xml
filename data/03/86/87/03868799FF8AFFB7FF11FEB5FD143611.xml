<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Cercis" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="353" phylum="Tracheophyta" rank="species" species="canadensis">
<emphasis italics="true" pageId="3" pageNumber="353">Cercis canadensis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[364,495,571,592]" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="353" phylum="Tracheophyta" rank="species" species="nigra">
