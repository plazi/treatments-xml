<collectingRegion box="[484,581,518,539]" country="United States of America" name="Louisiana" pageId="3" pageNumber="353">Louisiana</collectingRegion>
: 
<taxonomicName authority="(Wangenh.) K. Koch" authorityName="K. Koch" authorityYear="1869" baseAuthorityName="Wangenh." class="Magnoliopsida" family="Juglandaceae" genus="Carya" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="353" phylum="Tracheophyta" rank="species" species="illinoinensis">
