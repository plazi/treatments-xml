<collectingCountry box="[359,418,1994,2020]" name="United States of America" pageId="16" pageNumber="17">USA</collectingCountry>
, 
<collectingRegion box="[428,521,1994,2020]" country="United States of America" name="Arizona" pageId="16" pageNumber="17">Arizona</collectingRegion>
, 
