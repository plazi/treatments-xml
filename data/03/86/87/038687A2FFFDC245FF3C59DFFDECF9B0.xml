(
<number box="[1274,1312,1678,1702]" pageId="14" pageNumber="15" value="1.1">1.1</number>
); femur IV length 
<number box="[228,319,1714,1738]" pageId="14" pageNumber="15" value="31.9" valueMax="42.0" valueMin="21.8">21.8–42</number>
