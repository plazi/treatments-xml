<collectingRegion box="[360,539,1879,1905]" country="Mexico" name="San Luis Potosi" pageId="8" pageNumber="231">San Luis Potosí</collectingRegion>
, 
<collectingCountry box="[550,636,1879,1905]" name="Mexico" pageId="8" pageNumber="231">Mexico</collectingCountry>
