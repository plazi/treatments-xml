<taxonomicName baseAuthorityName="Say" baseAuthorityYear="1837" box="[481,638,1331,1352]" class="Insecta" family="Masaridae" genus="Cerceris" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="354" phylum="Arthropoda" rank="species" species="fumipennis">
