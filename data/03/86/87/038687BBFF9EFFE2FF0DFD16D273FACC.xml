<materialsCitation ID-GBIF-Occurrence="2608941062" collectingDate="1996-04-25" collectionCode="SAMC" collectorName="S. van Noort" country="Tanzania" elevation="1600" latitude="-3.8781667" location="Mkomazi Game Reserve" longLatPrecision="13" longitude="37.812" municipality="Maji Kununua" pageId="36" pageNumber="37" specimenCode="OSUC 248286" specimenCount="1" typeStatus="holotype">
<typeStatus box="[434,541,1117,1143]" pageId="36" pageNumber="37">Holotype</typeStatus>
