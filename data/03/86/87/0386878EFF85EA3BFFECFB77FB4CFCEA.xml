<quantity box="[1047,1164,351,375]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.1" metricValueMin="1.0" pageId="7" pageNumber="276" unit="mm" value="10.5" valueMax="11.0" valueMin="10.0">10−11 mm</quantity>
(
<typeStatus box="[1179,1278,351,375]" pageId="7" pageNumber="276">holotype</typeStatus>
: 
<quantity box="[1289,1389,351,375]" metricMagnitude="-2" metricUnit="m" metricValue="1.06" pageId="7" pageNumber="276" unit="mm" value="10.6">10.6 mm</quantity>
); fore wing length 
<quantity box="[949,1094,383,407]" metricMagnitude="-2" metricUnit="m" metricValue="1.03" metricValueMax="1.08" metricValueMin="0.98" pageId="7" pageNumber="276" unit="mm" value="10.3" valueMax="10.8" valueMin="9.8">9.8−10.8 mm</quantity>
(
