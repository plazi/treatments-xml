<subSubSection pageId="29" pageNumber="30" type="etymology">
<paragraph blockId="29.[140,1108,166,1672]" pageId="29" pageNumber="30">
<emphasis bold="true" box="[185,320,1293,1319]" pageId="29" pageNumber="30">
<emphasis box="[185,315,1293,1319]" inLineHeading="true" pageId="29" pageNumber="30" reason="1">Etymology</emphasis>
.
</emphasis>
