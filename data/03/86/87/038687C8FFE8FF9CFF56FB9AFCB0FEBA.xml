; mid femur = 
<quantity box="[362,452,1333,1358]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="1" pageNumber="584" unit="mm" value="3.5">3.5 mm</quantity>
; hind femur = 
<quantity box="[622,712,1334,1358]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="1" pageNumber="584" unit="mm" value="3.4">3.4 mm</quantity>
; fore tibia = 
<quantity box="[861,951,1334,1358]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="1" pageNumber="584" unit="mm" value="2.1">2.1 mm</quantity>
