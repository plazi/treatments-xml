<subSubSection pageId="7" pageNumber="36" type="etymology">
<paragraph blockId="7.[151,1436,1856,2024]" pageId="7" pageNumber="36">
<emphasis bold="true" box="[199,337,1964,1988]" pageId="7" pageNumber="36">Etymology:</emphasis>
