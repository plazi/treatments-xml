sp., 
<taxonomicName box="[836,988,268,293]" class="Hydrozoa" family="Aglaopheniidae" genus="Aglaophenia" kingdom="Animalia" order="Leptothecata" pageId="7" pageNumber="8" phylum="Cnidaria" rank="genus">
<emphasis box="[836,988,268,293]" italics="true" pageId="7" pageNumber="8">Aglaophenia</emphasis>
</taxonomicName>
sp. and 
<taxonomicName authority="Goldfuss, 1820" authorityName="Goldfuss" authorityYear="1820" class="Hydrozoa" family="Pennariidae" genus="Pennaria" kingdom="Animalia" order="Anthoathecata" pageId="7" pageNumber="8" phylum="Cnidaria" rank="species" species="disticha">
