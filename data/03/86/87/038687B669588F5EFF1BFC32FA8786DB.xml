<typeStatus box="[1169,1217,475,501]" pageId="19" pageNumber="20">type</typeStatus>
locality, 
<location LSID="urn:lsid:plazi:treatment:038687B669588F5EFF1BFC32FA8786DB:8EF0607B69598F5EFAA0FE3BFF53860F" country="Ecuador" name="Pululahua Reserve" pageId="19" pageNumber="20">Pululahua Reserve</location>
in 
<collectingCountry box="[277,372,511,537]" name="Ecuador" pageId="19" pageNumber="20">Ecuador</collectingCountry>
, where it was collected in tropical montane forest at 
<quantity box="[963,1048,511,537]" metricMagnitude="3" metricUnit="m" metricValue="3.3" pageId="19" pageNumber="20" unit="m" value="3300.0">
<elevation box="[963,1048,511,537]" metricMagnitude="3" metricUnit="m" metricValue="3.3" pageId="19" pageNumber="20" unit="m" value="3300.0">3300 m</elevation>
