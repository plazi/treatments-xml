<collectionCode box="[527,547,1893,1914]" pageId="2" pageNumber="379">H</collectionCode>
, 
<collectionCode box="[557,575,1893,1914]" pageId="2" pageNumber="379">K</collectionCode>
) sternite VIII. (
<collectionCode box="[725,741,1893,1914]" pageId="2" pageNumber="379">L</collectionCode>
