<quantity box="[789,906,1386,1410]" metricMagnitude="-2" metricUnit="m" metricValue="7.239" metricValueMax="8.128" metricValueMin="6.35" pageId="11" pageNumber="40" unit="in" value="2.85" valueMax="3.2" valueMin="2.5">2.5–3.2 in</quantity>
male. 
