<taxonomicName authority="Perreau, 1993" authorityName="Perreau" authorityYear="1993" box="[798,1085,775,799]" class="Insecta" family="Leiodidae" genus="Ptomaphaminus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="199" phylum="Arthropoda" rank="species" species="leclerci">
