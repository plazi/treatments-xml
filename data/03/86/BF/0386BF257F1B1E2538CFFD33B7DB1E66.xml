<collectionCode LSID="urn:lsid:biocol.org:col:1013" box="[1023,1106,1969,1993]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:1013" name="Essig Museum of Entomology" pageId="34" pageNumber="385">EMEC</collectionCode>
); Qintana Roo, X-Can, 
<date pageId="34" pageNumber="385" value="1966-04">
April 
<number box="[151,210,2004,2029]" pageId="34" pageNumber="385" value="1966.0">1966</number>
</date>
, E.C.W. Leg. (
<specimenCount box="[377,453,2004,2029]" pageId="34" pageNumber="385" type="male">
<number box="[377,389,2005,2029]" pageId="34" pageNumber="385" value="1.0">1</number>
male
</specimenCount>
, 
<collectionCode LSID="urn:lsid:biocol.org:col:1013" box="[465,548,2005,2029]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:1013" name="Essig Museum of Entomology" pageId="34" pageNumber="385">EMEC</collectionCode>
); Nuevo X-can, 
<date box="[731,933,2004,2029]" pageId="34" pageNumber="385" value="1976-11-04">
<number box="[731,745,2005,2029]" pageId="34" pageNumber="385" value="4.0">4</number>
November 
