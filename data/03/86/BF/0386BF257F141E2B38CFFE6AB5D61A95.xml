). 
<typeStatus box="[1332,1436,1712,1737]" pageId="45" pageNumber="396">Holotype</typeStatus>
deposited at 
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[292,373,1749,1773]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="45" pageNumber="396">USNM</collectionCode>
