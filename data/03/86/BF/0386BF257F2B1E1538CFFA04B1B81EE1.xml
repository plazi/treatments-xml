<paragraph blockId="19.[151,1436,151,944]" box="[199,583,517,542]" pageId="19" pageNumber="370">
<emphasis bold="true" box="[199,352,517,542]" pageId="19" pageNumber="370">Distribution.</emphasis>
<collectingCountry box="[359,472,517,542]" name="Colombia" pageId="19" pageNumber="370">Colombia</collectingCountry>
