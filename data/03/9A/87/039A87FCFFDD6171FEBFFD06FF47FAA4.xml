, 
<taxonomicName authority="(Couper 1958)" baseAuthorityName="Couper" baseAuthorityYear="1958" box="[206,578,1261,1288]" class="Pinopsida" family="Cheirolepidiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="28" pageNumber="115" phylum="Tracheophyta" rank="family">
Cheirolepidiaceae (
<bibRefCitation author="COUPER R. A." box="[416,571,1261,1288]" pageId="28" pageNumber="115" pagination="75 - 179" refId="ref18599" refString="COUPER R. A. 1958. - British Mesozoic microspores and pollen grains, a systematic and stratigraphic study. Palaeontographica 103 B: 75 - 179." type="journal article" year="1958">Couper 1958</bibRefCitation>
)
</taxonomicName>
