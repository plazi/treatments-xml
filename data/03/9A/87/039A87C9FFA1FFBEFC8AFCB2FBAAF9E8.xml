<taxonomicName authority="BLAGODEROV &amp; GRIMALDI, 2004" authorityName="BLAGODEROV &amp; GRIMALDI" authorityYear="2004" box="[826,957,779,803]" class="Insecta" family="Mycetophilidae" genus="Gaalomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus" status="new genus">
