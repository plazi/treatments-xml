<typeStatus box="[199,258,1544,1568]" pageId="7" pageNumber="60">Type</typeStatus>
genus.
</emphasis>
<taxonomicName authority="Stephensen, 1915" authorityName="Stephensen" authorityYear="1915" box="[347,658,1544,1568]" class="Malacostraca" family="Thoriellidae" genus="Thoriella" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="60" phylum="Arthropoda" rank="genus">
