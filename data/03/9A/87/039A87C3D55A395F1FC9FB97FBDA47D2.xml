<quantity box="[1165,1241,1687,1711]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="507" unit="m" value="1000.0">1000 m</quantity>
up to 
<quantity box="[1301,1376,1687,1711]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="26" pageNumber="507" unit="m" value="1600.0">1600 m</quantity>
a.s.l. on Mount 
<collectingCountry box="[888,992,1713,1737]" name="Cameroon" pageId="26" pageNumber="507">Cameroon</collectingCountry>
(
<bibRefCitation author="Eisentraut" box="[1006,1163,1713,1737]" firstAuthor="Eisentraut" pageId="26" pageNumber="507" refId="ref25058" refString="EISENTRAUT M. 1963. - Die Wirbeltiere des Kamerungebirges. Verlag Paul Parey, Hamburg, 353 p." type="book" year="1963">Eisentraut 1963</bibRefCitation>
