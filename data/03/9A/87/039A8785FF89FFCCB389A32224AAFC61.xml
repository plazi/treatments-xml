<taxonomicName box="[633,732,152,176]" class="Insecta" family="Carabidae" genus="Oxytrechus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="150" phylum="Arthropoda" rank="species" species="belloi" status="n. sp.">
<emphasis box="[633,732,152,176]" italics="true" pageId="6" pageNumber="150">
<collectingCountry box="[633,656,152,175]" name="Iceland" pageId="6" pageNumber="150">O</collectingCountry>
. belloi
</emphasis>
</taxonomicName>
<emphasis bold="true" box="[739,803,151,176]" pageId="6" pageNumber="150">
<taxonomicNameLabel box="[739,803,151,176]" pageId="6" pageNumber="150" rank="species">n. sp.</taxonomicNameLabel>
