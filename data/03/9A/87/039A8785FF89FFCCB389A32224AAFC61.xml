<taxonomicName box="[809,935,224,248]" class="Insecta" family="Carabidae" genus="Oxytrechus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="150" phylum="Arthropoda" rank="genus">
<emphasis box="[809,935,224,248]" italics="true" pageId="6" pageNumber="150">Oxytrechus</emphasis>
</taxonomicName>
species living in sympatry with 
<taxonomicName box="[1306,1405,224,248]" class="Insecta" family="Carabidae" genus="Oxytrechus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="150" phylum="Arthropoda" rank="species" species="belloi" status="n. sp.">
<emphasis box="[1306,1405,224,248]" italics="true" pageId="6" pageNumber="150">
<collectingCountry box="[1306,1329,224,247]" name="Iceland" pageId="6" pageNumber="150">O</collectingCountry>
