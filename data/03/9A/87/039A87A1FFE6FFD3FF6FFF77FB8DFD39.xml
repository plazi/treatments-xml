</subSubSection>
<subSubSection box="[199,1110,640,667]" pageId="5" pageNumber="56" type="etymology">
<paragraph blockId="5.[151,1437,152,667]" box="[199,1110,640,667]" pageId="5" pageNumber="56">
<emphasis bold="true" box="[199,326,640,666]" italics="true" pageId="5" pageNumber="56">Etymology</emphasis>
