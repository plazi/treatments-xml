</taxonomicName>
, 
<taxonomicName box="[399,520,1660,1684]" class="Magnoliopsida" family="Sapindaceae" genus="Pappea" kingdom="Plantae" order="Sapindales" pageId="34" pageNumber="1907" phylum="Tracheophyta" rank="species" species="capensis">
