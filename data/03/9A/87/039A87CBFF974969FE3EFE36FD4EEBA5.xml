(
<figureCitation box="[527,602,442,461]" captionStart="Fig" captionStartId="13.[113,144,1520,1539]" captionTargetBox="[179,1045,245,1474]" captionTargetId="figure-11@13.[113,1085,219,1493]" captionTargetPageId="13" captionText="Fig. 1. Habitus of the third instar larvae of Acidocerini and Hydrobiusini. A – Agraphydrus narusei (Satô, 1960), dorsal and lateral view; B – Enochrus (Holcophilydrus) simulans (Sharp, 1873), dorsal view; C – E. (H.) umbratus Sharp, 1884, dorsal view; D – E. (Methydrus) japonicus (Sharp, 1873), dorsal view; E – Helochares (Helochares) pallens (MacLeay, 1825), dorsal view; F – H.(Hydrobaticus) nipponicus Hebauer, 1995, dorsal view; G – Hydrobius pauper Sharp, 1884, dorsal and ventral view." figureDoi="http://doi.org/10.5281/zenodo.4272326" httpUri="https://zenodo.org/record/4272326/files/figure.png" pageId="20" pageNumber="17">Figs. 1A</figureCitation>
, 
<figureCitation box="[612,656,442,461]" captionStart-0="Fig" captionStart-1="Fig" captionStart-2="Fig" captionStart-3="Fig" captionStart-4="Fig" captionStart-5="Fig" captionStartId-0="22.[152,183,1271,1290]" captionStartId-1="23.[112,143,1291,1310]" captionStartId-2="25.[113,144,820,839]" captionStartId-3="26.[151,182,1514,1533]" captionStartId-4="27.[113,144,1516,1535]" captionStartId-5="28.[150,181,1516,1535]" captionTargetBox-0="[174,1093,221,1246]" captionTargetBox-1="[123,1042,215,1259]" captionTargetBox-2="[129,1077,222,783]" captionTargetBox-3="[224,1057,221,1360]" captionTargetBox-4="[174,1031,219,1457]" captionTargetBox-5="[154,1125,748,1491]" captionTargetId-0="figure-126@22.[151,1122,215,1260]" captionTargetId-1="figure-83@23.[113,1084,213,1282]" captionTargetId-2="figure-296@25.[113,1084,199,811]" captionTargetId-3="figure-9@26.[151,1122,210,1385]" captionTargetId-4="figure-11@27.[113,1084,197,1472]" captionTargetId-5="figure-215@28.[154,1126,748,1492]" captionTargetPageId-0="22" captionTargetPageId-1="23" captionTargetPageId-2="25" captionTargetPageId-3="26" captionTargetPageId-4="27" captionTargetPageId-5="28" captionText-0="Fig. 7. Head capsule of Agraphydrus narusei (Satô, 1960), first instar. A – dorsal view; B – ventral view; C – detail of anterior margin of head capsule, dorsal view." captionText-1="Fig. 8. Head appendages of Agraphydrus narusei (Satô, 1960), first instar. A – antenna, dorsal view; B–C – mandibles, dorsal view; D – maxilla, dorsal view; E – maxilla, ventral view; F – labium, dorsal view; G – labium, ventral view." captionText-2="Fig. 9. Agraphydrus narusei (Satô, 1960), third instar.A – head, dorsal view; B – spiracular atrium, dorsal view." captionText-3="Fig. 10. Head capsule of Agraphydrus narusei (Satô, 1960), third instar.A – dorsal view; B – ventral view; C – detail of anterior margin of head capsule, dorsal view." captionText-4="Fig. 11. Head appendages of Agraphydrus narusei (Satô, 1960), third instar.A – antenna, dorsal view; B–C – mandibles, dorsal view; D – maxilla, dorsal view; E – maxilla, ventral view; F – labium, dorsal view; G – labium, ventral view." captionText-5="Fig. 12. Agraphydrus narusei (Satô, 1960).A–B – prolegs, ventral view:A – first instar; B – third instar. C–D – shape of setae, dorsal view: C – median part of parietale, third instar, dorsal view; D – thoracic segments, first instar, dorsal view. Seta on PA8 and PA13 missing. Photographs were taken with compound light microscope." figureDoi-0="http://doi.org/10.5281/zenodo.4272338" figureDoi-1="http://doi.org/10.5281/zenodo.4272340" figureDoi-2="http://doi.org/10.5281/zenodo.4272342" figureDoi-3="http://doi.org/10.5281/zenodo.4272344" figureDoi-4="http://doi.org/10.5281/zenodo.4272346" figureDoi-5="http://doi.org/10.5281/zenodo.4272348" httpUri-0="https://zenodo.org/record/4272338/files/figure.png" httpUri-1="https://zenodo.org/record/4272340/files/figure.png" httpUri-2="https://zenodo.org/record/4272342/files/figure.png" httpUri-3="https://zenodo.org/record/4272344/files/figure.png" httpUri-4="https://zenodo.org/record/4272346/files/figure.png" httpUri-5="https://zenodo.org/record/4272348/files/figure.png" pageId="20" pageNumber="17">7–12</figureCitation>
, 
<figureCitation box="[665,704,442,461]" captionStart="Fig" captionStartId="75.[113,144,900,919]" captionTargetBox="[146,1049,203,857]" captionTargetId="figure-11@75.[113,1084,196,870]" captionTargetPageId="75" captionText="Fig. 44. Prosterna of Acidocerini species, third instar, ventral view. A – Agraphydrus narusei (Satô, 1960); B – Enochrus umbratus Sharp, 1884; C – Helochares nipponicus Hebauer, 1995." figureDoi="http://doi.org/10.5281/zenodo.4272415" httpUri="https://zenodo.org/record/4272415/files/figure.png" pageId="20" pageNumber="17">44A</figureCitation>
, 
<figureCitation box="[713,751,442,461]" captionStart="Fig" captionStartId="76.[151,182,1040,1059]" captionTargetBox="[170,1092,224,1013]" captionTargetId="figure-136@76.[151,1122,216,1019]" captionTargetPageId="76" captionText="Fig. 45. Mesothoracic legs of Acidocerini species, third instar, anterior view.A – Agraphydrus narusei (Satô, 1960); B – Enochrus umbratus Sharp, 1884; C – Enochrus japonicus (Sharp, 1873); D – Helochares nipponicus Hebauer, 1995." figureDoi="http://doi.org/10.5281/zenodo.4272417" httpUri="https://zenodo.org/record/4272417/files/figure.png" pageId="20" pageNumber="17">45A</figureCitation>
