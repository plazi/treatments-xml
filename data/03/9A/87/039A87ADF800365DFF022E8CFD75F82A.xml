<quantity box="[252,376,1561,1585]" metricMagnitude="2" metricUnit="m" metricValue="7.35" metricValueMax="7.56" metricValueMin="7.14" pageId="25" pageNumber="138" unit="m" value="735.0" valueMax="756.0" valueMin="714.0">714–756 m</quantity>
