<geoCoordinate box="[253,388,1620,1644]" degrees="178" direction="east" minutes="14.18" orientation="longitude" pageId="25" pageNumber="138" precision="9" value="178.23633">178°14.18'E</geoCoordinate>
, alive, 
<quantity box="[464,534,1620,1644]" metricMagnitude="2" metricUnit="m" metricValue="9.9" pageId="25" pageNumber="138" unit="m" value="990.0">990 m</quantity>
, 2 pr, NIWA V466), in 
<quantity box="[148,282,1649,1673]" metricMagnitude="2" metricUnit="m" metricValue="6.58" metricValueMax="10.0" metricValueMin="3.16" pageId="25" pageNumber="138" unit="m" value="658.0" valueMax="1000.0" valueMin="316.0">316–1000 m</quantity>
