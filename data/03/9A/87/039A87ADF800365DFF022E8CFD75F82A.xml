<collectingCountry box="[705,745,1649,1673]" name="Fiji" pageId="25" pageNumber="138">Fiji</collectingCountry>
, 
<quantity box="[121,242,1678,1702]" metricMagnitude="2" metricUnit="m" metricValue="5.045" metricValueMax="6.99" metricValueMin="3.1" pageId="25" pageNumber="138" unit="m" value="504.5" valueMax="699.0" valueMin="310.0">310–699 m</quantity>
