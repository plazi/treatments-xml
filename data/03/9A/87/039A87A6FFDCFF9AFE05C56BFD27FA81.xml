<taxonomicName box="[125,406,1141,1165]" class="Magnoliopsida" family="Sapindaceae" genus="Lecaniodiscus" kingdom="Plantae" order="Sapindales" pageId="41" pageNumber="1914" phylum="Tracheophyta" rank="species" species="cupanioides">
<emphasis box="[125,406,1141,1165]" italics="true" pageId="41" pageNumber="1914">Lecaniodiscus cupanioides</emphasis>
</taxonomicName>
(
<taxonomicName box="[423,576,1141,1165]" class="Magnoliopsida" family="Sapindaceae" kingdom="Plantae" order="Sapindales" pageId="41" pageNumber="1914" phylum="Tracheophyta" rank="family">Sapindaceae</taxonomicName>
