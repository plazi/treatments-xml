<quantity box="[981,1076,373,397]" metricMagnitude="1" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="68" pageNumber="181" unit="m" value="20.0" valueMax="30.0" valueMin="10.0">10-30 m</quantity>
