<heading bold="true" box="[151,687,1374,1401]" fontSize="11" level="1" pageId="4" pageNumber="351" reason="1">
<taxonomicName authority="(Mereschkowsky, 1877)" baseAuthorityName="Mereschkowsky" baseAuthorityYear="1877" box="[151,687,1374,1401]" class="Kinetofragminophora" family="Acinetidae" genus="Paracineta" kingdom="Protozoa" order="Suctorida" pageId="4" pageNumber="351" phylum="Ciliophora" rank="species" species="saifulae">
<emphasis bold="true" box="[151,687,1374,1401]" pageId="4" pageNumber="351">
<emphasis bold="true" box="[151,387,1374,1401]" italics="true" pageId="4" pageNumber="351">Paracineta saifulae</emphasis>
(
<bibRefCitation author="Mereschkowsky, C." box="[402,678,1374,1400]" pageId="4" pageNumber="351" pagination="203 - 385" refId="ref8711" refString="Mereschkowsky, C. (1877) Studies on the Protozoa of Northern Russia. Travaux de la Societe des naturalistes de St. - Petersbourg, 8 (3), 203 - 385. [in Russian]" type="journal article" year="1877">Mereschkowsky, 1877</bibRefCitation>
)
</emphasis>
