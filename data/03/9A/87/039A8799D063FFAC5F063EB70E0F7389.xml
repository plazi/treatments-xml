<subSubSection lastPageId="13" lastPageNumber="138" pageId="12" pageNumber="137" type="description">
<paragraph blockId="12.[151,1437,1282,1893]" pageId="12" pageNumber="137">
<emphasis bold="true" box="[208,328,1539,1562]" pageId="12" pageNumber="137">Venter –</emphasis>
