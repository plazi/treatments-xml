<collectingCountry box="[1015,1115,1483,1510]" name="Australia" pageId="25" pageNumber="112">Australia</collectingCountry>
(de 
<bibRefCitation author="JERSEY N. J. DE" pageId="25" pageNumber="112" refId="ref19913" refString="JERSEY N. J. DE 1962. - Triassic spores and pollen grains from the Ipswich Coalfield. Geological Survey of Queensland Publication 307, 18 p." type="book" year="1962">
<collectingCountry name="Jersey" pageId="25" pageNumber="112">Jersey</collectingCountry>
1962
</bibRefCitation>
).
</paragraph>
