<emphasis box="[1140,1273,512,536]" italics="true" pageId="4" pageNumber="31">C. maximus</emphasis>
Ehrenberg
</taxonomicName>
; 
<taxonomicName authority="Preobrajenskaja" authorityName="Preobrajenskaja" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="4" pageNumber="31" phylum="Gastrotricha" rank="species" species="microchaetus">
