<typeStatus box="[550,645,539,563]" pageId="4" pageNumber="291">holotype</typeStatus>
<quantity box="[650,733,540,564]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="4" pageNumber="291" unit="mm" value="4.3">4.3 mm</quantity>
, 
<typeStatus box="[742,828,539,563]" pageId="4" pageNumber="291">allotype</typeStatus>
<quantity box="[833,917,540,564]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="4" pageNumber="291" unit="mm" value="4.8">4.8 mm</quantity>
), width 
<quantity box="[1000,1126,540,564]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" metricValueMax="1.7" metricValueMin="1.3" pageId="4" pageNumber="291" unit="mm" value="1.5" valueMax="1.7" valueMin="1.3">1.3–1.7 mm</quantity>
(
<typeStatus box="[158,253,571,595]" pageId="4" pageNumber="291">holotype</typeStatus>
<quantity box="[260,344,572,596]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="4" pageNumber="291" unit="mm" value="1.4">1.4 mm</quantity>
, 
<typeStatus box="[355,441,571,595]" pageId="4" pageNumber="291">allotype</typeStatus>
<quantity box="[447,533,572,596]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="4" pageNumber="291" unit="mm" value="1.7">1.7 mm</quantity>
