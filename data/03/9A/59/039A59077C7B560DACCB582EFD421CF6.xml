<number box="[321,354,1589,1615]" pageId="1" pageNumber="2" value="50.0">50</number>
), 
<number box="[376,405,1589,1615]" pageId="1" pageNumber="2" value="77.0">77</number>
(
<number box="[421,453,1589,1615]" pageId="1" pageNumber="2" value="71.0">71</number>
), 
<number box="[475,504,1589,1615]" pageId="1" pageNumber="2" value="86.0">86</number>
(
<number box="[520,553,1589,1615]" pageId="1" pageNumber="2" value="79.0">79</number>
), 
<number box="[574,603,1589,1615]" pageId="1" pageNumber="2" value="99.0">99</number>
(
<number box="[619,652,1589,1615]" pageId="1" pageNumber="2" value="97.0">97</number>
); IV-L- 
<number box="[741,754,1589,1615]" pageId="1" pageNumber="2" value="6.0">6</number>
