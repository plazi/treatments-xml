(
<number box="[729,762,1629,1655]" pageId="1" pageNumber="2" value="80.0">80</number>
), 
<number box="[783,827,1629,1655]" pageId="1" pageNumber="2" value="104.0">104</number>
(
<number box="[844,877,1629,1655]" pageId="1" pageNumber="2" value="96.0">96</number>
), 
<number box="[898,927,1629,1655]" pageId="1" pageNumber="2" value="99.0">99</number>
(
