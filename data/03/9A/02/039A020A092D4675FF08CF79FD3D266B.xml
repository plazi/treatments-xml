. 
<taxonomicName authority="Nielson, 2011" authorityName="Nielson" authorityYear="2011" box="[375,541,1939,1961]" class="Insecta" family="Cicadellidae" genus="Stalolidia" kingdom="Animalia" order="Hemiptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="lyrica" status="sp. nov.">
<emphasis box="[375,541,1939,1961]" italics="true" pageId="41" pageNumber="42">Stalolidia lyrica</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[556,638,1939,1960]" pageId="41" pageNumber="42" rank="species">
