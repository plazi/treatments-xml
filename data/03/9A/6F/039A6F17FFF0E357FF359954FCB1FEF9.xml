. 
<emphasis box="[1273,1353,188,212]" italics="true" pageId="6" pageNumber="37">furcata</emphasis>
</taxonomicName>
and 
<taxonomicName class="Arachnida" family="Euphthiracaridae" genus="Acrotritia" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="37" phylum="Arthropoda" rank="species" species="penicillata">
