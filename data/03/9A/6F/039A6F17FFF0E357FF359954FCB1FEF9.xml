)
</taxonomicName>
and 
<taxonomicName authority="Mahunka, 1982" authorityName="Mahunka" authorityYear="1982" box="[275,633,188,213]" class="Arachnida" family="Euphthiracaridae" genus="Acrotritia" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="37" phylum="Arthropoda" rank="species" species="penicillata">
<emphasis box="[275,292,188,211]" italics="true" pageId="6" pageNumber="37">A</emphasis>
. 
<emphasis box="[308,428,188,212]" italics="true" pageId="6" pageNumber="37">penicillata</emphasis>
(
<bibRefCitation author="Mahunka" box="[444,625,188,213]" pageId="6" pageNumber="37" refString="Mahunka, S. (1982) Ptychoide Oribatiden aus der Koreanischen Volksdemokratischen Republik (Acari). Acta Zoologica Academiae Scientiarum Hungaricae, 28 (12), 83 - 103." type="journal article" year="1982">
Mahunka, 
<number box="[568,625,188,212]" pageId="6" pageNumber="37" value="1982.0">1982</number>
</bibRefCitation>
)
</taxonomicName>
