<collectingCountry box="[869,997,322,346]" name="Sint Martin" pageId="4" pageNumber="3">St. Martin</collectingCountry>
/ 
<collectingCountry box="[1006,1170,322,346]" name="Sint Martin" pageId="4" pageNumber="3">Sint Maarten</collectingCountry>
