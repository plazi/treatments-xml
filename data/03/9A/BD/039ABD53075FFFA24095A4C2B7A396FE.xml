<quantity box="[411,477,1556,1579]" metricMagnitude="2" metricUnit="m" metricValue="5.25" pageId="9" pageNumber="116" unit="m" value="525.0">525 m</quantity>
; Ziéla 
<quantity box="[555,622,1556,1579]" metricMagnitude="2" metricUnit="m" metricValue="5.25" pageId="9" pageNumber="116" unit="m" value="525.0">525 m</quantity>
; Zouguépo 
