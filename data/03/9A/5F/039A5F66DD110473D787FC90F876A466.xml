<subSubSection pageId="38" pageNumber="39" type="description">
<paragraph blockId="38.[151,1436,151,526]" pageId="38" pageNumber="39">
<emphasis bold="true" box="[197,509,151,176]" pageId="38" pageNumber="39">
Variation in North 
<collectingCountry box="[436,509,151,176]" name="China" pageId="38" pageNumber="39">China</collectingCountry>
