) between 
<quantity box="[1211,1348,577,602]" metricMagnitude="3" metricUnit="m" metricValue="1.8244999999999998" metricValueMax="3.056" metricValueMin="0.593" pageId="39" pageNumber="40" unit="m" value="1824.5" valueMax="3056.0" valueMin="593.0">593–3056 m</quantity>
