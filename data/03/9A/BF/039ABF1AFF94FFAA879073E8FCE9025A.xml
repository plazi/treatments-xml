<collectingCountry box="[417,516,467,487]" name="Czech Republic" pageId="20" pageNumber="1315">Tschechien</collectingCountry>
(
<collectingCountry box="[527,604,467,487]" name="Czech Republic" pageId="20" pageNumber="1315">Böhmen</collectingCountry>
) (WAR), 
<collectingCountry box="[691,800,467,487]" name="Germany" pageId="20" pageNumber="1315">Deutschland</collectingCountry>
