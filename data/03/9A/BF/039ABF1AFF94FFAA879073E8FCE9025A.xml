<collectingRegion box="[229,271,467,487]" country="Latvia" name="Riga" pageId="20" pageNumber="1315">RIG</collectingRegion>
), 
<collectingCountry box="[286,336,467,487]" name="Poland" pageId="20" pageNumber="1315">Polen</collectingCountry>
