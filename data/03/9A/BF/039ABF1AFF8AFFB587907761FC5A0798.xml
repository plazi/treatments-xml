<collectingCountry box="[837,913,1402,1422]" name="United Kingdom" pageId="10" pageNumber="1305">England</collectingCountry>
, 
<collectingCountry box="[926,1020,1402,1422]" name="United Kingdom" pageId="10" pageNumber="1305">Schottland</collectingCountry>
