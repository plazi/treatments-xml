<collectingCountry box="[768,818,1354,1374]" name="Poland" pageId="10" pageNumber="1305">Polen</collectingCountry>
(SAW, WAR), 
<collectingCountry box="[986,1085,1354,1374]" name="Czech Republic" pageId="10" pageNumber="1305">Tschechien</collectingCountry>
(
<collectingRegion box="[229,300,1378,1398]" country="Czech Republic" name="Jihomoravsky" pageId="10" pageNumber="1305">Mähren</collectingRegion>
