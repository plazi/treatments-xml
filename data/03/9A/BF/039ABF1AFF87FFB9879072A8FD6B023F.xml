<taxonomicName authority="(LINNAEUS)" baseAuthorityName="LINNAEUS" class="Insecta" family="Geometridae" genus="Eulithis" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="1302" phylum="Arthropoda" rank="species" species="prunata">
