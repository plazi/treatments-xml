<taxonomicName authority="De Man, 1892" authorityName="De Man" authorityYear="1892" box="[675,984,676,700]" class="Malacostraca" family="Atyidae" genus="Caridina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="18" pageNumber="2139" phylum="Arthropoda" rank="species" species="japonica">
