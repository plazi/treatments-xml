locality, 
<collectingRegion box="[546,649,669,693]" country="Japan" name="Okinawa" pageId="35" pageNumber="2156">Okinawa</collectingRegion>
<collectingMunicipality box="[657,731,669,693]" pageId="35" pageNumber="2156">Island</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:039A9827FFB8FFD7CBE98A2BFC15ECA5:8EEC7FEAFFBAFFD7C8BC89DBFC11ECA5" box="[745,928,669,693]" municipality="Island" name="Ryukyu Islands" pageId="35" pageNumber="2156" stateProvince="Okinawa">Ryukyu Islands</location>
