<taxonomicName authorityName="Erseus" authorityYear="1999" box="[1092,1264,591,612]" class="Clitellata" family="Parvidrilidae" kingdom="Animalia" order="Haplotaxida" pageId="18" pageNumber="548" phylum="Annelida" rank="family">PARVIDRILIDAE</taxonomicName>
