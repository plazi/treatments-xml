+ 
<number box="[290,341,1904,1928]" pageId="7" pageNumber="38" value="0.45">0.45</number>
+ 
<number box="[356,407,1904,1928]" pageId="7" pageNumber="38" value="0.38">0.38</number>
+ 
<number box="[422,472,1904,1928]" pageId="7" pageNumber="38" value="0.33">0.33</number>
), IV, 
<number box="[531,580,1904,1928]" pageId="7" pageNumber="38" value="1.99">1.99</number>
long (
