<collectingRegion box="[1315,1398,971,997]" country="Portugal" name="Acores" pageId="8" pageNumber="9">Azores</collectingRegion>
and 
<collectingRegion box="[239,341,1010,1036]" country="Portugal" name="Madeira" pageId="8" pageNumber="9">Madeira</collectingRegion>
.
