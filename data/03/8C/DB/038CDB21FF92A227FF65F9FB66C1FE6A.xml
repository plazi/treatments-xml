<taxonomicName authority="Matsumoto &amp; Van Ofwegen &amp; Bayer, 2019" authorityName="Matsumoto &amp; Van Ofwegen &amp; Bayer" authorityYear="2019" box="[317,526,1958,1980]" class="Anthozoa" family="Acanthogorgiidae" genus="Calcigorgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="gracilis" status="sp. n.">
<emphasis box="[317,526,1958,1980]" italics="true" pageId="8" pageNumber="9">Calcigorgia gracilis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[536,597,1958,1980]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[536,597,1958,1980]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, holotype USNM 60280. 
<emphasis bold="true" box="[874,895,1958,1980]" pageId="8" pageNumber="9">b.</emphasis>
<taxonomicName authority="Matsumoto &amp; Van Ofwegen &amp; Bayer, 2019" authorityName="Matsumoto &amp; Van Ofwegen &amp; Bayer" authorityYear="2019" box="[905,1015,1958,1980]" class="Anthozoa" family="Acanthogorgiidae" genus="Calcigorgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="gracilis" status="sp. n.">
<emphasis box="[905,1015,1958,1980]" italics="true" pageId="8" pageNumber="9">C. gracilis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1025,1086,1958,1980]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[1025,1086,1958,1980]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
JAMSTEC No.1120034415 
<emphasis bold="true" box="[1391,1402,1958,1980]" pageId="8" pageNumber="9">c</emphasis>
<taxonomicName authority="Matsumoto &amp; Van Ofwegen &amp; Bayer, 2019" authorityName="Matsumoto &amp; Van Ofwegen &amp; Bayer" authorityYear="2019" class="Anthozoa" family="Acanthogorgiidae" genus="Calcigorgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="gracilis" status="sp. n.">
<emphasis italics="true" pageId="8" pageNumber="9">C. gracilis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[235,291,1989,2011]" pageId="8" pageNumber="9">
