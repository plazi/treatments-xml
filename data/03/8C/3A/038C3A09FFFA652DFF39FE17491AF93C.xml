<emphasis bold="true" box="[151,317,476,502]" italics="true" pageId="4" pageNumber="65">Atrichopogon</emphasis>
(
<emphasis bold="true" box="[334,474,476,502]" italics="true" pageId="4" pageNumber="65">Meloehelea</emphasis>
) 
<emphasis bold="true" box="[491,688,476,502]" italics="true" pageId="4" pageNumber="65">monomorphicus</emphasis>
