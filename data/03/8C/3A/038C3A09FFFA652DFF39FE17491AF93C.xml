</taxonomicName>
and 
<taxonomicName authority="Marino &amp; Spinelli" authorityName="Marino &amp; Spinelli" box="[293,662,367,392]" class="Insecta" family="Ceratopogonidae" genus="Atrichopogon" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="carpinteroi">
