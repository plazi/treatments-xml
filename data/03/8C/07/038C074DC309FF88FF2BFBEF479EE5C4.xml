<elevation metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="274" unit="m" value="600.0">
<quantity metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="274" unit="m" value="600.0">600 m</quantity>
