<subSubSection pageId="2" type="type_taxon">
<paragraph blockId="2.[151,638,1682,1729]" pageId="2">
<emphasis bold="true" box="[175,304,1682,1703]" pageId="2">
<typeStatus box="[175,222,1682,1703]" pageId="2">Type</typeStatus>
Species.
</emphasis>
<taxonomicName authority="Ratcliffe and Giraldo" authorityName="Ratcliffe and Giraldo" authorityYear="2014" class="Insecta" family="Dynastidae" genus="Piscoperus" kingdom="Animalia" order="Coleoptera" pageId="2" phylum="Arthropoda" rank="species" species="paracanicola" status="new species">
<emphasis box="[317,551,1682,1703]" italics="true" pageId="2">Piscoperus paracanicola</emphasis>
Ratcliffe and Giraldo
</taxonomicName>
