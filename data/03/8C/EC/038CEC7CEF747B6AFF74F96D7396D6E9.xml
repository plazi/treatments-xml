<collectingCountry box="[689,782,1878,1904]" name="United Kingdom" pageId="8" pageNumber="251">England</collectingCountry>
(
<collectingCountry box="[799,953,1878,1905]" name="United Kingdom" pageId="8" pageNumber="251">Great Britain</collectingCountry>
