<paragraph blockId="5.[151,1437,619,1113]" pageId="5" pageNumber="590">
<materialsCitation ID-GBIF-Occurrence="3107829302" country="Tanzania" county="Jackson" location="Two" municipality="Usambara Mountains" pageId="5" pageNumber="590" specimenCount="3" specimenCount-female="1" specimenCount-male="2" stateProvince="Lindi" typeStatus="holotype">
The 
<typeStatus box="[202,300,619,645]" pageId="5" pageNumber="590">holotype</typeStatus>
