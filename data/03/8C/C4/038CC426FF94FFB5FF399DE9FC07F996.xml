<emphasis bold="true" box="[199,352,1556,1581]" pageId="60" pageNumber="61">Distribution.</emphasis>
Goyder Formation, Amadeus Basin, 
<collectingRegion box="[772,981,1556,1581]" country="Australia" name="Northern Territory" pageId="60" pageNumber="61">Northern Territory</collectingRegion>
. 
<taxonomicName authority="Limestone and O'Hara Shale, Georgina Basin" authorityName="Limestone and O'Hara Shale, Georgina Basin" class="Insecta" family="Morabidae" genus="Georgina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="genus">Georgina Limestone and O’Hara Shale, Georgina Basin</taxonomicName>
, 
<collectingRegion box="[347,557,1591,1616]" country="Australia" name="Northern Territory" pageId="60" pageNumber="61">Northern Territory</collectingRegion>
and 
<collectingRegion box="[619,755,1591,1616]" country="Australia" name="Queensland" pageId="60" pageNumber="61">Queensland</collectingRegion>
