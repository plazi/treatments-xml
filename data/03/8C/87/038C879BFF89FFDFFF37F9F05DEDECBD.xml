<paragraph blockId="10.[151,1436,1770,2010]" box="[151,678,1770,1795]" pageId="10" pageNumber="59">
<emphasis bold="true" box="[151,286,1771,1795]" pageId="10" pageNumber="59">Etymology.</emphasis>
<taxonomicName authority="Latin" authorityName="Latin" box="[292,463,1770,1795]" class="Insecta" family="Eulophidae" genus="Astichus" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="59" phylum="Arthropoda" rank="species" species="gracilis">
