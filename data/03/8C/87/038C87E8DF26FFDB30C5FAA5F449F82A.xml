, 
<quantity box="[812,883,1792,1819]" metricMagnitude="-1" metricUnit="m" metricValue="1.397" metricValueMax="1.524" metricValueMin="1.27" pageId="3" pageNumber="356" unit="in" value="5.5" valueMax="6.0" valueMin="5.0">5–6 in</quantity>
<taxonomicName authority="(Madhavan et al., 2019)" baseAuthorityName="Madhavan" baseAuthorityYear="2019" box="[890,1321,1793,1820]" class="Malacostraca" family="Hippolytidae" genus="Lysmata" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="356" phylum="Arthropoda" rank="species" species="ternatensis">
