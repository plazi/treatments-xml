<quantity box="[245,368,1212,1236]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" metricValueMax="1.3" metricValueMin="1.0" pageId="8" pageNumber="8" unit="mm" value="11.5" valueMax="13.0" valueMin="10.0">10–13 mm</quantity>
