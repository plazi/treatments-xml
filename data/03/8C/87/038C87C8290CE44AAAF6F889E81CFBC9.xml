<number box="[236,274,836,860]" pageId="3" pageNumber="4" value="0.8">0.8</number>
; pedipalpus 8.0; leg I 
<number box="[521,559,836,860]" pageId="3" pageNumber="4" value="8.6">8.6</number>
; II 
<number box="[597,649,836,860]" pageId="3" pageNumber="4" value="17.8">17.8</number>
