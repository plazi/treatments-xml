<taxonomicName authority="Forster, 1851" authorityName="Forster" authorityYear="1851" box="[571,929,510,533]" class="Insecta" family="Eulophidae" genus="Euderus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="arithmeticus">
