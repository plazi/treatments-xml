. G–I, 
<taxonomicName box="[704,812,1317,1338]" class="Insecta" family="Termitidae" genus="Diversitermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isoptera" pageId="6" pageNumber="227" phylum="Arthropoda" rank="species" species="tiapuan" status="sp. n.">
<emphasis box="[704,812,1317,1338]" italics="true" pageId="6" pageNumber="227">D. tiapuan</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[825,882,1316,1337]" pageId="6" pageNumber="227" rank="species">
<emphasis bold="true" box="[825,882,1316,1337]" pageId="6" pageNumber="227">sp. n.</emphasis>
