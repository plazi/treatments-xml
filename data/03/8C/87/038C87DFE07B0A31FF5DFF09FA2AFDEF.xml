<taxonomicName authority="Distant" authorityName="Distant" authorityYear="1916" box="[366,603,153,180]" class="Insecta" family="Cicadellidae" genus="Daimachus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="560" phylum="Arthropoda" rank="genus">
