<taxonomicName authority="Silvestri, 1917" authorityName="Silvestri" authorityYear="1917" box="[410,712,1702,1729]" class="Diplopoda" family="Glomeridae" genus="Rhopalomeris" kingdom="Animalia" order="Glomerida" pageId="2" pageNumber="259" phylum="Arthropoda" rank="species" species="monacha">
