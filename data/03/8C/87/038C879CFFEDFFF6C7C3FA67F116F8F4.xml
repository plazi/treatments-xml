<collectingCountry box="[792,862,1625,1651]" name="China" pageId="1" pageNumber="177">China</collectingCountry>
, 
<collectingCountry box="[873,982,1625,1651]" name="Indonesia" pageId="1" pageNumber="177">Indonesia</collectingCountry>
to 
<collectingCountry box="[1018,1146,1625,1652]" name="Philippines" pageId="1" pageNumber="177">Philippines</collectingCountry>
