<typeStatus box="[151,204,906,929]" pageId="3" pageNumber="42">Type</typeStatus>
species: 
<taxonomicName authority="Rasnitsyn, 1990" authorityName="Rasnitsyn" authorityYear="1990" box="[301,671,906,929]" class="Insecta" family="Anaxyelidae" genus="Eosyntexis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="42" phylum="Arthropoda" rank="species" species="senilis">
