<quantity box="[282,383,258,285]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" pageId="5" pageNumber="17" unit="mm" value="5.1">5.10 mm</quantity>
thickness, fragment 2: 
<quantity box="[650,765,258,285]" metricMagnitude="-2" metricUnit="m" metricValue="2.856" pageId="5" pageNumber="17" unit="mm" value="28.56">28.56 mm</quantity>
