<quantity box="[944,1034,526,550]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="4" pageNumber="39" unit="mm" value="2.6">2.6 mm</quantity>
; ultimate section of CuA 
<number box="[1315,1323,541,555]" pageId="4" pageNumber="39" value="1.0">1</number>
about 
