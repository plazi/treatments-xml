<subSubSection pageId="0" pageNumber="43" type="type_taxon">
<paragraph blockId="0.[820,1432,1760,2077]" pageId="0" pageNumber="43">
<emphasis bold="true" box="[846,986,1760,1784]" pageId="0" pageNumber="43">
<typeStatus box="[846,898,1760,1784]" pageId="0" pageNumber="43">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Ohtaka &amp; Wulandari, 2016" authorityName="Ohtaka &amp; Wulandari" authorityYear="2016" box="[1012,1280,1760,1784]" class="Clitellata" family="Naididae" genus="Kahayandrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="0" pageNumber="43" phylum="Annelida" rank="species" species="tundaiensis" status="sp. nov.">
<emphasis box="[1012,1280,1760,1784]" italics="true" pageId="0" pageNumber="43">Kahayandrilus tundaiensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1289,1370,1760,1783]" pageId="0" pageNumber="43" rank="species">sp. nov.</taxonomicNameLabel>
