<collectingRegion box="[192,491,1509,1535]" country="China" name="Xizang" pageId="3" pageNumber="254">Tibet Autonomous Region</collectingRegion>
, 
<collectingCountry box="[502,573,1509,1535]" name="China" pageId="3" pageNumber="254">China</collectingCountry>
