<subSubSection pageId="13" pageNumber="418" type="discussion">
<paragraph blockId="13.[151,1436,151,213]" pageId="13" pageNumber="418">
<emphasis bold="true" box="[199,315,151,176]" pageId="13" pageNumber="418">Remarks.</emphasis>
<taxonomicName box="[322,671,152,176]" class="Insecta" family="Cicadellidae" genus="Philipposcopus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="418" phylum="Arthropoda" rank="species" species="maquilingensis">
