<quantity box="[704,814,944,968]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" metricValueMax="3.4" metricValueMin="2.0" pageId="9" pageNumber="64" unit="mm" value="2.7" valueMax="3.4" valueMin="2.0">2–3.4mm</quantity>
