<quantity box="[421,483,690,710]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="27" pageNumber="1536" unit="km" value="15.0">15 km</quantity>
Asalem-Pissason, 
<quantity box="[659,711,690,710]" metricMagnitude="2" metricUnit="m" metricValue="2.2" pageId="27" pageNumber="1536" unit="m" value="220.0">220m</quantity>
, 
<date box="[725,825,690,710]" pageId="27" pageNumber="1536" value="1975-08">VIII. 1975</date>
