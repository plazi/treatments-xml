<subSubSection pageId="9" pageNumber="8" type="diagnosis">
<paragraph blockId="9.[192,1441,422,670]" pageId="9" pageNumber="8">
<emphasis bold="true" box="[192,334,422,446]" pageId="9" pageNumber="8">Diagnosis.</emphasis>
