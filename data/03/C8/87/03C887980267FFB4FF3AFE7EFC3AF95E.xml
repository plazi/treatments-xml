<paragraph blockId="11.[724,1248,1379,1851]" pageId="11" pageNumber="12">
<taxonomicName box="[724,958,1379,1402]" class="Insecta" family="Formicidae" genus="Rotastruma" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="stenoceps">
