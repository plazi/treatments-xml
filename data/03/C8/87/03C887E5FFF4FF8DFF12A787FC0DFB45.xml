<typeStatus box="[151,210,258,283]" pageId="31" pageNumber="232">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Johnston, 1836" authorityName="Johnston" authorityYear="1836" box="[313,656,258,283]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="31" pageNumber="232" phylum="Arthropoda" rank="species" species="basteri">
<emphasis box="[313,477,258,283]" italics="true" pageId="31" pageNumber="232">Acarus basteri</emphasis>
<bibRefCitation author="Johnston" box="[484,656,258,283]" pageId="31" pageNumber="232" refString="Johnston, G. (1836) Illustrations in British zoology. Magazine of Natural History, 9 (63), 353 - 357." type="journal article" year="1836">
