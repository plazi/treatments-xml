<taxonomicName authority="Dendy, 1922" authorityName="Dendy" authorityYear="1922" box="[451,807,324,350]" class="Demospongiae" family="Coelosphaeridae" kingdom="Animalia" order="Poecilosclerida" pageId="7" pageNumber="8" phylum="Porifera" rank="family">
