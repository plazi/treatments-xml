<number box="[151,211,1564,1589]" pageId="23" pageNumber="78" value="1969.0">1969</number>
); Middle-Russian Upland: Gherasimovka, Stanovye, Loznoe, Nagolnoye (Prisnyi 
<number box="[1152,1212,1564,1588]" pageId="23" pageNumber="78" value="2001.0">2001</number>
); 
<collectingCountry box="[1235,1333,1564,1589]" name="Ukraine" pageId="23" pageNumber="78">
<emphasis bold="true" box="[1235,1333,1564,1589]" pageId="23" pageNumber="78">Ukraine</emphasis>
</collectingCountry>
