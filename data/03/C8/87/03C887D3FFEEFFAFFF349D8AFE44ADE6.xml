<number box="[977,1037,1528,1552]" pageId="23" pageNumber="78" value="1993.0">1993</number>
); 
<emphasis bold="true" box="[1062,1141,1527,1552]" pageId="23" pageNumber="78">
<collectingCountry box="[1062,1141,1527,1552]" name="Russia" pageId="23" pageNumber="78">Russia</collectingCountry>
</emphasis>
