<collectingCountry box="[850,914,333,356]" name="Japan" pageId="5" pageNumber="46">Japan</collectingCountry>
(Ryukyu Islands) and 
<collectingCountry box="[1168,1282,332,357]" name="Singapore" pageId="5" pageNumber="46">Singapore</collectingCountry>
(
<bibRefCitation author="Anker, A. &amp; De Grave, S." pageId="5" pageNumber="46" pagination="343 - 454" refId="ref5349" refString="Anker, A. &amp; De Grave, S. (2016) An updated and annotated checklist of marine and brackish caridean shrimps of Singapore (Crustacea, Decapoda). Raffles Bulletin of Zoology, 2016, 343 - 454." type="journal article" year="2016">Anker &amp; De Grave 2016</bibRefCitation>
). Western Atlantic: 
<collectingCountry box="[518,586,367,392]" name="Brazil" pageId="5" pageNumber="46">Brazil</collectingCountry>
(
<collectingRegion box="[604,674,368,392]" country="Brazil" name="Ceara" pageId="5" pageNumber="46">Ceará</collectingRegion>
, 
<collectingRegion box="[687,934,367,392]" country="Brazil" name="Rio Grande do Norte" pageId="5" pageNumber="46">Rio Grande do Norte</collectingRegion>
, and 
<collectingRegion box="[997,1084,367,392]" country="Brazil" name="Sergipe" pageId="5" pageNumber="46">Sergipe</collectingRegion>
) (
<bibRefCitation author="Pachelle, P. P. G. &amp; Anker, A. &amp; Mendes, C. B. &amp; Bezerra, L. E. A." box="[1108,1341,367,392]" pageId="5" pageNumber="46" pagination="1 - 63" refId="ref8296" refString="Pachelle, P. P. G., Anker, A., Mendes, C. B. &amp; Bezerra, L. E. A. (2016) Decapod crustaceans from the state of Ceara, northeastern Brazil: An updated checklist of marine and estuarine species, with 23 new records. Zootaxa, 4131 (1), 1 - 63. https: // doi. org / 10.11646 / zootaxa. 4131.1.1" type="journal article" year="2016">
