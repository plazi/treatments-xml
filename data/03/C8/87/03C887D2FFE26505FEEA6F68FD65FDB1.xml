<subSubSection lastPageId="13" lastPageNumber="760" pageId="12" pageNumber="759" type="distribution">
<paragraph blockId="12.[151,1127,1512,1600]" box="[151,923,1512,1536]" pageId="12" pageNumber="759">
<emphasis bold="true" box="[151,298,1512,1536]" pageId="12" pageNumber="759">Distribution.</emphasis>
<collectingCountry box="[304,412,1512,1536]" name="Indonesia" pageId="12" pageNumber="759">Indonesia</collectingCountry>
