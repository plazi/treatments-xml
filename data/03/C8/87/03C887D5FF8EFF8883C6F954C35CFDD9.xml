<collectionCode box="[671,752,2017,2043]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="3" pageNumber="142" type="Museum">FMNH</collectionCode>
