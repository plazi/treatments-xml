, 
<taxonomicName authority="Walker, 1869" authorityName="Walker" authorityYear="1869" box="[687,1170,1683,1706]" class="Insecta" family="Gryllidae" genus="Polinemobius" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="63" phylum="Arthropoda" rank="species" species="taprobanensis">
