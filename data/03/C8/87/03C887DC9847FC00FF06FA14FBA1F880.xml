<taxonomicName authority="Saussure, 1877" authorityName="Saussure" authorityYear="1877" class="Insecta" family="Haglotettigoniidae" genus="Loxoblemmus" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="63" phylum="Arthropoda" rank="species" species="arietulus">
