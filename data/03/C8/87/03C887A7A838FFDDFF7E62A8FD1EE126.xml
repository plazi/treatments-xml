<subSubSection pageId="15" pageNumber="316" type="diagnosis">
<paragraph blockId="15.[151,1436,1432,1817]" pageId="15" pageNumber="316">
<emphasis bold="true" box="[151,273,1432,1457]" pageId="15" pageNumber="316">Diagnosis.</emphasis>
<taxonomicName authorityName="Gray" authorityYear="1867" box="[280,389,1432,1456]" class="Calcarea" family="Clathrinidae" genus="Clathrina" kingdom="Animalia" order="Clathrinida" pageId="15" pageNumber="316" phylum="Porifera" rank="genus">
<emphasis box="[280,389,1432,1456]" italics="true" pageId="15" pageNumber="316">Clathrina</emphasis>
</taxonomicName>
