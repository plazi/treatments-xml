<collectingCountry box="[1205,1312,1322,1348]" name="Bermuda" pageId="15" pageNumber="16">Bermuda</collectingCountry>
[
<geoCoordinate box="[1335,1436,1322,1348]" degrees="32" direction="north" minutes="12" orientation="latitude" pageId="15" pageNumber="16" precision="925" value="32.2">32º12’N</geoCoordinate>
<geoCoordinate box="[151,259,1362,1388]" degrees="52" direction="west" minutes="36" orientation="longitude" pageId="15" pageNumber="16" precision="925" value="-52.6">52º36’W</geoCoordinate>
] by 
<bibRefCitation author="Shoemaker, C. R." box="[323,550,1362,1388]" pageId="15" pageNumber="16" pagination="185 - 266" refId="ref75240" refString="Shoemaker, C. R. (1945) The Amphipoda of the Bermuda Oceanographic Expeditions, 1929 - 1931. Zoologica, New York, 30, 185 - 266." type="journal article" year="1945">Shoemaker (1945)</bibRefCitation>
; the North–west Pacific, S.E. of 
<collectingCountry box="[973,1040,1362,1388]" name="Japan" pageId="15" pageNumber="16">Japan</collectingCountry>
[
<geoCoordinate box="[1061,1162,1362,1388]" degrees="30" direction="north" minutes="52" orientation="latitude" pageId="15" pageNumber="16" precision="925" value="30.866667">30º52’N</geoCoordinate>
<geoCoordinate box="[1177,1286,1362,1388]" degrees="153" direction="east" minutes="16" orientation="longitude" pageId="15" pageNumber="16" precision="925" value="153.26666">153º16’E</geoCoordinate>
&amp; 
<geoCoordinate box="[1337,1436,1362,1388]" degrees="30" direction="north" minutes="53" orientation="latitude" pageId="15" pageNumber="16" precision="925" value="30.883333">30º53’N</geoCoordinate>
<geoCoordinate box="[151,263,1402,1428]" degrees="153" direction="east" minutes="09" orientation="longitude" pageId="15" pageNumber="16" precision="925" value="153.15">153º09’E</geoCoordinate>
] by 
<bibRefCitation author="Vinogradov, M. E." box="[326,546,1402,1428]" pageId="15" pageNumber="16" pagination="186 - 227" refId="ref75975" refString="Vinogradov, M. E. (1957) Hyperiids (Amphipoda) of the Northwest Pacific Ocean. 1. Tribe Hyperiidea Physosomata. Trudy Instituta Okeanologii Akademiya Nauk SSSR 20: 186 - 227. [In Russian]. English translation by American Institute of Biological Sciences, Washington D. C., 1959, Marine Biology, 149 - 184." type="journal article" year="1957">Vinogradov (1957</bibRefCitation>
&amp; 
<bibRefCitation author="Vinogradov, M. E." box="[595,668,1402,1428]" pageId="15" pageNumber="16" pagination="198 - 247" refId="ref76041" refString="Vinogradov, M. E. (1960 a) Hyperiidea Physosomata of the Tropical Pacific Ocean. Trudy Instituta Okeanologii Akademiya Nauk SSSR 41, 198 - 247. [In Russian]." type="journal article" year="1960">1960a</bibRefCitation>
) and N.E. of Marshall Is [
<geoCoordinate box="[1008,1109,1402,1428]" degrees="16" direction="north" minutes="08" orientation="latitude" pageId="15" pageNumber="16" precision="925" value="16.133333">16º08’N</geoCoordinate>
<geoCoordinate box="[1123,1234,1402,1428]" degrees="173" direction="east" minutes="10" orientation="longitude" pageId="15" pageNumber="16" precision="925" value="173.16667">173º10’E</geoCoordinate>
] by 
