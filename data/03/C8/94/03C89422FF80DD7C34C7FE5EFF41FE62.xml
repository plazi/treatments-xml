of 
<taxonomicName authorityName="Woltereck" authorityYear="1909" box="[580,728,760,785]" class="Malacostraca" family="Chuneolidae" genus="Chuneola" kingdom="Animalia" order="Amphipoda" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="paradoxa">
