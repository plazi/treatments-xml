<taxonomicName authority="Matsuo" authorityName="Matsuo" box="[331,565,1248,1271]" class="Insecta" family="Torymidae" genus="Torymus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="species" species="itoi" status="sp. nov.">
<taxonomicName box="[331,472,1248,1271]" class="Insecta" family="Torymidae" genus="Torymus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="species" species="itoi">
<emphasis box="[331,472,1248,1271]" italics="true" pageId="2" pageNumber="55">
<taxonomicName box="[331,426,1248,1271]" class="Insecta" family="Torymidae" genus="Torymus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="genus">Torymus</taxonomicName>
itoi
</emphasis>
</taxonomicName>
Matsuo
</taxonomicName>
, 
<taxonomicNameLabel box="[578,668,1248,1272]" pageId="2" pageNumber="55" rank="species">
<emphasis bold="true" box="[578,668,1248,1272]" pageId="2" pageNumber="55">sp. nov.</emphasis>
</taxonomicNameLabel>
