<collectingRegion box="[826,964,1422,1444]" country="Australia" name="Queensland" pageId="17" pageNumber="262">Queensland</collectingRegion>
, 
<collectingCountry box="[981,1091,1422,1444]" name="Australia" pageId="17" pageNumber="262">Australia</collectingCountry>
, between a depth of 36 and 
