<emphasis bold="true" box="[192,1202,1278,1303]" pageId="3" pageNumber="2">
Subgenus 
<taxonomicName authority="Klimaszewski" authorityName="Klimaszewski" box="[334,814,1278,1303]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="subGenus" subGenus="Hydrosmectomorpha">
<emphasis bold="true" box="[334,617,1278,1302]" italics="true" pageId="3" pageNumber="2">Hydrosmectomorpha</emphasis>
Klimaszewski
</taxonomicName>
and 
<taxonomicName authority="Klimaszewski &amp; Webster &amp; Davies &amp; Bourdon, 2018" authorityName="Klimaszewski &amp; Webster &amp; Davies &amp; Bourdon" authorityYear="2018" box="[881,994,1279,1303]" pageId="3" pageNumber="2" rank="subGenus" status="new subgenus" subGenus="Webster">Webster</taxonomicName>
