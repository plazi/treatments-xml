<taxonomicName authority="Cyclanthacaeae" authorityName="Cyclanthacaeae" class="Liliopsida" family="Cyclanthaceae" genus="Carludovica" kingdom="Plantae" order="Pandanales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="palmata">
<emphasis box="[956,1210,1492,1517]" italics="true" pageId="17" pageNumber="18">Carludovica palmata</emphasis>
(Cyclanthacaeae)
</taxonomicName>
and 
<taxonomicName box="[438,647,1532,1557]" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" kingdom="Animalia" order="Zingiberales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="mariae">
