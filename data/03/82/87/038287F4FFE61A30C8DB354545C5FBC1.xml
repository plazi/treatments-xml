<subSubSection pageId="40" pageNumber="41" type="description">
<paragraph blockId="40.[151,1437,465,1684]" pageId="40" pageNumber="41">
<emphasis bold="true" box="[197,379,610,634]" pageId="40" pageNumber="41">Measurements.</emphasis>
