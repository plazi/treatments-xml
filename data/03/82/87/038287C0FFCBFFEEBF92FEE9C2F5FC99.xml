, 
<collectingCountry box="[1140,1239,664,688]" name="Hungary" pageId="7" pageNumber="228">Hungary</collectingCountry>
, 
<collectingCountry box="[1252,1353,663,688]" name="Slovakia" pageId="7" pageNumber="228">Slovakia</collectingCountry>
, 
<collectingCountry name="Czech Republic" pageId="7" pageNumber="228">Czech Republic</collectingCountry>
, 
<collectingCountry box="[266,347,700,725]" name="Poland" pageId="7" pageNumber="228">Poland</collectingCountry>
, 
<collectingCountry box="[358,448,700,725]" name="Ukraine" pageId="7" pageNumber="228">Ukraine</collectingCountry>
(
<bibRefCitation author="Csuzdi" box="[462,701,700,725]" pageId="7" pageNumber="228" refString="Csuzdi, C. &amp; Zicsi, A. (2003) Earthworms of Hungary (Annelida: Oligochaeta, Lumbricidae). Pedozoologica Hungarica, 1, 1 - 272." type="journal article" year="2003">
