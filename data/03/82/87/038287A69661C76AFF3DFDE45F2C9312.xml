<collectingRegion box="[735,840,152,175]" country="Russia" name="Tatarstan" pageId="3" pageNumber="130">Tatarstan</collectingRegion>
, 
<collectingCountry box="[857,935,151,176]" name="Russia" pageId="3" pageNumber="130">Russia</collectingCountry>
, and 
<taxonomicName box="[1004,1127,151,176]" class="Adenophorea" family="Trichinellidae" genus="Trichuris" kingdom="Animalia" order="Trichocephalida" pageId="3" pageNumber="130" phylum="Nematoda" rank="species" species="spalacis">
<emphasis box="[1004,1127,151,176]" italics="true" pageId="3" pageNumber="130">T. spalacis</emphasis>
</taxonomicName>
in the mole rat, 
<emphasis italics="true" pageId="3" pageNumber="130">
<taxonomicName box="[1341,1436,151,176]" class="Adenophorea" family="Trichinellidae" genus="Trichuris" kingdom="Animalia" order="Trichocephalida" pageId="3" pageNumber="135" phylum="Nematoda" rank="species" species="spalacis">Spalacis</taxonomicName>
