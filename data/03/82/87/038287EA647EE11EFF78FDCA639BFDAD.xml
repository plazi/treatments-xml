<taxonomicName authorityName="Leach" authorityYear="1815" box="[651,787,548,573]" class="Insecta" family="Buprestidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="505" phylum="Arthropoda" rank="family">Buprestidae</taxonomicName>
