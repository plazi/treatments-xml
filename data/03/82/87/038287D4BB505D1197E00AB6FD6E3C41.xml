<subSubSection pageId="65" pageNumber="66" type="biology_ecology">
<paragraph blockId="65.[151,1437,710,1890]" pageId="65" pageNumber="66">
<emphasis bold="true" box="[199,295,1214,1240]" pageId="65" pageNumber="66">Biology.</emphasis>
