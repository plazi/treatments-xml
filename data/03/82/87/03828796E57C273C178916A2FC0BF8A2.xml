</taxonomicName>
. 
<taxonomicName baseAuthorityName="Okajima" baseAuthorityYear="1922" box="[151,373,963,982]" class="Insecta" family="Blissidae" genus="Cavelerius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="121" phylum="Arthropoda" rank="species" species="saccharivorus">
