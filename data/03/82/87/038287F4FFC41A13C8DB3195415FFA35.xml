<paragraph blockId="10.[151,1437,1641,2032]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[151,225,1642,1666]" pageId="10" pageNumber="11">
<typeStatus box="[151,218,1642,1666]" pageId="10" pageNumber="11">Types</typeStatus>
.
</emphasis>
