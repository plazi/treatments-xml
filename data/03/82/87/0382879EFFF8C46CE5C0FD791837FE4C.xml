(
<figureCitation box="[532,700,731,755]" captionStart="Figures 13–18" captionStartId="10.[125,194,1358,1377]" captionTargetBox="[125,1173,133,1322]" captionTargetId="figure-33@10.[125,1173,133,1322]" captionTargetPageId="10" captionText="Figures 13–18. Posterior face of hind femur. (13, 14) Dohrniphora seriata [LACM ENT 184595]. (15, 16) D. seriata [LACM ENT 210889]. (17, 18) D. sexspinosa [LACM ENT 179771]." figureDoi="http://doi.org/10.5281/zenodo.5232333" httpUri="https://zenodo.org/record/5232333/files/figure.png" pageId="9" pageNumber="1939">Figures 13–16</figureCitation>
, 
<figureCitation box="[714,791,731,755]" captionStart="Figures 23–32" captionStartId="13.[138,207,1218,1237]" captionTargetBox="[262,1060,134,1182]" captionTargetId="figure-144@13.[262,1060,133,1182]" captionTargetPageId="13" captionText="Figures 23–32. Male genitalia, right and left lateral views. (23, 24) Dohrniphora cornuta. (25, 26) D. dispar. (27, 28) D. divaricata. (29, 30) D. seriata. (31, 32) D. sexspinosa." figureDoi="http://doi.org/10.5281/zenodo.5232339" httpUri="https://zenodo.org/record/5232339/files/figure.png" pageId="9" pageNumber="1939">29, 30</figureCitation>
