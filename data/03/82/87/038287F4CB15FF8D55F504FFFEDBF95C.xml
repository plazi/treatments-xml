<collectingCountry box="[632,748,1605,1629]" name="Colombia" pageId="23" pageNumber="1599">Colombia</collectingCountry>
, 
<collectingCountry box="[761,880,1605,1629]" name="Venezuela" pageId="23" pageNumber="1599">Venezuela</collectingCountry>
, 
<collectingCountry box="[893,964,1605,1629]" name="Brazil" pageId="23" pageNumber="1599">Brazil</collectingCountry>
, 
<collectingCountry box="[977,1030,1605,1628]" name="Peru" pageId="23" pageNumber="1599">Peru</collectingCountry>
and 
<collectingCountry box="[1091,1172,1605,1629]" name="Bolivia" pageId="23" pageNumber="1599">Bolivia</collectingCountry>
(
<bibRefCitation author="Metcalf ZP" box="[219,388,1637,1661]" pageId="23" pageNumber="1599" pagination="1 - 492" refId="ref12909" refString="Metcalf ZP. 1963 c. General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. NC St Coll Contr. 1564: 1 - 492." type="journal article" year="1963">Metcalf 1963c</bibRefCitation>
; 
<bibRefCitation author="Sanborn AF" box="[403,580,1637,1661]" pageId="23" pageNumber="1599" pagination="21 - 32" refId="ref13247" refString="Sanborn AF. 2007 a. Additions to the cicada fauna of Venezuela with the description of a new species and checklist of the Venezuelan cicada fauna (Hemiptera: Cicadoidea: Cicadidae). Zootaxa 1503: 21 - 32." type="journal article" year="2007">Sanborn 2007a</bibRefCitation>
