<collectingCountry box="[450,566,1255,1279]" name="Venezuela" pageId="22" pageNumber="1598">Venezuela</collectingCountry>
and 
<collectingCountry box="[622,735,1255,1279]" name="Colombia" pageId="22" pageNumber="1598">Colombia</collectingCountry>
