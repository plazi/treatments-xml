<taxonomicName authorityName="Det. H. S. Lopes" baseAuthorityName="Wulp" baseAuthorityYear="1895" box="[302,563,367,393]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="trivialis">
