<taxonomicName authority="Mill." authorityName="Mill." box="[454,724,403,429]" class="Magnoliopsida" family="Lauraceae" genus="Persea" kingdom="Plantae" order="Laurales" pageId="103" pageNumber="104" phylum="Tracheophyta" rank="species" species="americana">
