<subSubSection pageId="75" pageNumber="76" type="biology_ecology">
<paragraph blockId="75.[151,1437,325,1108]" pageId="75" pageNumber="76">
<emphasis bold="true" box="[199,295,865,891]" pageId="75" pageNumber="76">Biology.</emphasis>
Collected above 
<taxonomicName baseAuthorityName="Westwood" baseAuthorityYear="1842" box="[496,686,866,892]" class="Insecta" family="Formicidae" genus="Eciton" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="burchellii">
<emphasis box="[496,686,866,892]" italics="true" pageId="75" pageNumber="76">Eciton burchellii</emphasis>
