, pétale; 
<emphasis bold="true" box="[716,730,1635,1652]" pageId="3" pageNumber="16">C</emphasis>
, sépale médian; 
<emphasis bold="true" box="[877,891,1635,1652]" pageId="3" pageNumber="16">D</emphasis>
, sépale latéral; 
<emphasis bold="true" box="[1026,1038,1635,1652]" pageId="3" pageNumber="16">E</emphasis>
, labelle; 
<emphasis bold="true" box="[1116,1127,1635,1652]" pageId="3" pageNumber="16">F</emphasis>
, colonne, vue du dessus; 
<emphasis bold="true" box="[245,261,1657,1674]" pageId="3" pageNumber="16">F’</emphasis>
, profil de la colonne,vue latérale; 
<emphasis bold="true" box="[544,558,1657,1674]" pageId="3" pageNumber="16">G</emphasis>
