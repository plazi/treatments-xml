Figures 
<number box="[359,404,1434,1460]" pageId="9" pageNumber="10" value="3.6">3, 6</number>
</figureCitation>
, 
<number box="[417,536,1434,1460]" pageId="9" pageNumber="10" value="17.59" valueMax="20.0" valueMin="15.18">
