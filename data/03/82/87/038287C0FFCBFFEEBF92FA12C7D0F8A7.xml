<collectingCountry box="[814,919,1730,1754]" name="Germany" pageId="7" pageNumber="228">Germany</collectingCountry>
, 
<collectingCountry box="[934,1017,1729,1754]" name="Austria" pageId="7" pageNumber="228">Austria</collectingCountry>
and former 
<collectingCountry box="[1165,1289,1729,1754]" name="Serbia" pageId="7" pageNumber="228">Yugoslavia</collectingCountry>
(
<bibRefCitation author="Rota" pageId="7" pageNumber="228" refString="Rota, E. (2014) Fauna Europaea: Annelida, Lumbricidae. Fauna Europaea version 2. 6. 2, Available from: http: // www. faunaeur. org (27 August 2014)" type="journal article" year="2014">
