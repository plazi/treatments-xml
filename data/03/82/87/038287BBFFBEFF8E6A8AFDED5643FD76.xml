<taxonomicName authorityName=", Kraatz" authorityYear="1882" box="[940,1040,585,611]" class="Insecta" family="Melolonthidae" genus="Haplidia" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="538" phylum="Arthropoda" rank="genus">
