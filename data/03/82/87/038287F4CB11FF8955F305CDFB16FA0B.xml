<collectingCountry box="[621,692,1394,1418]" name="Brazil" pageId="19" pageNumber="1595">Brazil</collectingCountry>
, 
<collectingCountry box="[703,819,1394,1418]" name="Colombia" pageId="19" pageNumber="1595">Colombia</collectingCountry>
, 
<collectingCountry box="[831,957,1394,1418]" name="Costa Rica" pageId="19" pageNumber="1595">Costa Rica</collectingCountry>
and 
<collectingCountry box="[1014,1067,1395,1418]" name="Peru" pageId="19" pageNumber="1595">Peru</collectingCountry>
(
<bibRefCitation author="Metcalf ZP" pageId="19" pageNumber="1595" pagination="1 - 585" refId="ref12831" refString="Metcalf ZP. 1963 a. General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. NC St Coll Contr. 1502: 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
