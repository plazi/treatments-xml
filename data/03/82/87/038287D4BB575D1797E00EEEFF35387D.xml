<subSubSection pageId="71" pageNumber="72" type="biology_ecology">
<paragraph blockId="71.[151,1437,150,861]" pageId="71" pageNumber="72">
<emphasis bold="true" box="[199,295,222,248]" pageId="71" pageNumber="72">Biology.</emphasis>
