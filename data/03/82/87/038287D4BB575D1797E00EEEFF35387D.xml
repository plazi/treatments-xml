<taxonomicName authorityName="Lopes" authorityYear="1946" box="[639,743,511,537]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="major">
