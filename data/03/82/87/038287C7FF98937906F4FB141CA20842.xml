<quantity metricMagnitude="3" metricUnit="m" metricValue="1.15" pageId="14" pageNumber="355" unit="m" value="1150.0">1150 m</quantity>
, 
<date box="[178,295,249,268]" pageId="14" pageNumber="355" value="1991-05-19">19 May 1991</date>
, A. Carapezza, 
<specimenCount box="[428,460,246,269]" pageId="14" pageNumber="355" type="female">1 ♀</specimenCount>
(AMNH_PBI 00307366) (
<collectionCode box="[691,744,249,268]" pageId="14" pageNumber="355">ACPI</collectionCode>
), 1 ♁ (AMNH_PBI 00307365) (
