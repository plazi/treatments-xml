, 
<collectingCountry box="[1043,1168,733,757]" name="Guatemala" pageId="11" pageNumber="1587">Guatemala</collectingCountry>
, 
<collectingCountry box="[212,325,765,789]" name="Honduras" pageId="11" pageNumber="1587">Honduras</collectingCountry>
, 
<collectingCountry box="[337,454,765,789]" name="Nicaragua" pageId="11" pageNumber="1587">Nicaragua</collectingCountry>
, 
