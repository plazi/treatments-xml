, 
<collectingCountry box="[694,809,765,789]" name="Venezuela" pageId="11" pageNumber="1587">Venezuela</collectingCountry>
, French Guiana, 
<collectingCountry box="[1003,1100,765,789]" name="Ecuador" pageId="11" pageNumber="1587">Ecuador</collectingCountry>
, 
<collectingCountry box="[1112,1168,765,788]" name="Peru" pageId="11" pageNumber="1587">Peru</collectingCountry>
, 
<collectingCountry box="[212,283,797,821]" name="Brazil" pageId="11" pageNumber="1587">Brazil</collectingCountry>
, 
<collectingCountry box="[298,413,797,821]" name="Argentina" pageId="11" pageNumber="1587">Argentina</collectingCountry>
and South America (
<bibRefCitation author="Metcalf ZP" box="[674,845,797,821]" pageId="11" pageNumber="1587" pagination="1 - 585" refId="ref12831" refString="Metcalf ZP. 1963 a. General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. NC St Coll Contr. 1502: 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
; 
<bibRefCitation author="Duffels JP &amp; van der Laan PA" pageId="11" pageNumber="1587" refId="ref12245" refString="Duffels JP, van der Laan PA. 1985. Catalogue of the Cicadoidea (Homoptera, Auchenorhyncha) 1956 - 1980. Series Entomologica 34. Dordrect: Dr. W. Junk Publishers." type="journal volume" year="1985">Duffels and van der Laan 1985</bibRefCitation>
; 
<bibRefCitation author="Sanborn AF &amp; Heath JE &amp; Heath MS &amp; Noriega FG" box="[278,512,829,853]" pageId="11" pageNumber="1587" pagination="319 - 328" refId="ref13419" refString="Sanborn AF, Heath JE, Heath MS, Noriega FG. 1995 a. Thermoregulation by endogenous heat production in two South American grass dwelling cicadas (Homoptera: Cicadidae: Proarna). Fla Entomol. 78: 319 - 328." type="journal article" year="1995">Sanborn et al. 1995a</bibRefCitation>
; 
<bibRefCitation author="Sanborn AF" box="[522,697,829,853]" pageId="11" pageNumber="1587" pagination="75 - 79" refId="ref13213" refString="Sanborn AF. 2006 b. New records for the cicada fauna from four Central American countries (Hemiptera: Cicadoidea: Cicadidae). Fla Entomol. 89: 75 - 79." type="journal article" year="2006">Sanborn 2006b</bibRefCitation>
, 
<bibRefCitation author="Sanborn AF" box="[707,780,829,853]" pageId="11" pageNumber="1587" pagination="21 - 32" refId="ref13247" refString="Sanborn AF. 2007 a. Additions to the cicada fauna of Venezuela with the description of a new species and checklist of the Venezuelan cicada fauna (Hemiptera: Cicadoidea: Cicadidae). Zootaxa 1503: 21 - 32." type="journal article" year="2007">2007a</bibRefCitation>
,b, 2009). It has been reported from the Dept. 
<collectingRegion box="[324,452,861,885]" country="Colombia" name="Magdalena" pageId="11" pageNumber="1587">Magdalena</collectingRegion>
(
<bibRefCitation author="Davis WT" box="[466,594,861,885]" pageId="11" pageNumber="1587" pagination="173 - 199" refId="ref11728" refString="Davis WT. 1935. New cicadas with notes on North American and West Indian species. J NY Entomol Soc. 43: 173 - 199." type="journal article" year="1935">Davis 1935</bibRefCitation>
