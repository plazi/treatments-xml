<subSubSection lastPageId="18" lastPageNumber="19" pageId="17" pageNumber="18" type="description">
<paragraph blockId="17.[151,1436,1580,2036]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[199,381,1652,1676]" pageId="17" pageNumber="18">Measurements.</emphasis>
