<quantity box="[873,1039,1652,1676]" metricMagnitude="-3" metricUnit="m" metricValue="4.225" metricValueMax="4.25" metricValueMin="4.2" pageId="17" pageNumber="18" unit="mm" value="4.225" valueMax="4.25" valueMin="4.2">4.20–4.25 mm</quantity>
; HL 
<quantity box="[1094,1259,1652,1676]" metricMagnitude="-4" metricUnit="m" metricValue="9.25" metricValueMax="9.5" metricValueMin="9.0" pageId="17" pageNumber="18" unit="mm" value="0.925" valueMax="0.95" valueMin="0.9">0.90–0.95 mm</quantity>
; HW 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="7.75" metricValueMax="8.0" metricValueMin="7.5" pageId="17" pageNumber="18" unit="mm" value="0.775" valueMax="0.8" valueMin="0.75">0.75–0.80 mm</quantity>
; SL 
<quantity box="[249,415,1687,1712]" metricMagnitude="-4" metricUnit="m" metricValue="7.1499999999999995" metricValueMax="7.3" metricValueMin="7.0" pageId="17" pageNumber="18" unit="mm" value="0.715" valueMax="0.73" valueMin="0.7">0.70–0.73 mm</quantity>
; ML 
