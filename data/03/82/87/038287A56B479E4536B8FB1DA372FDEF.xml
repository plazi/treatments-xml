), 
<location LSID="urn:lsid:plazi:treatment:038287A56B479E4536B8FB1DA372FDEF:8EF460686B479E4632FAFA0EA50EFA28" box="[1154,1305,1454,1478]" country="Myanmar" municipality="District of Columbia" name="Washington" pageId="5" pageNumber="4">Washington</location>
, 
<collectingMunicipality pageId="5" pageNumber="4">District of Columbia</collectingMunicipality>
