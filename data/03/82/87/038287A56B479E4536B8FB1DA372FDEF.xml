</materialsCitation>
<materialsCitation ID-GBIF-Occurrence="2565950088" collectionCode="USNM" country="Myanmar" location="Washington" municipality="District of Columbia" pageId="5" pageNumber="4" specimenCount="1" typeStatus="holotype">
