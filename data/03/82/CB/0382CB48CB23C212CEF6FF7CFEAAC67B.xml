<typeStatus box="[561,612,1661,1687]" pageId="63" pageNumber="64">type</typeStatus>
locality of Loreto, Puerto Escondido, 
<collectingRegion box="[1056,1285,1661,1687]" country="Mexico" name="Baja California Sur" pageId="63" pageNumber="64">Baja California Sur</collectingRegion>
, 
<collectingCountry box="[1295,1388,1661,1687]" name="Mexico" pageId="63" pageNumber="64">Mexico</collectingCountry>
