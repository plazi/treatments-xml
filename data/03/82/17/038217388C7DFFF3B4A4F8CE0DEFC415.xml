<taxonomicName authority="Qin and Liu" authorityName="Qin and Liu" box="[350,887,1311,1334]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="25" phylum="Arthropoda" rank="species" species="biprocessa" status="sp. nov." subGenus="Matsumurasca">
