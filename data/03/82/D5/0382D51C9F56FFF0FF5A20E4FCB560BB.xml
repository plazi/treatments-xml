<number box="[232,291,332,356]" pageId="14" pageNumber="33" value="2007.0">2007</number>
, as 
<taxonomicName box="[333,442,332,356]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="14" pageNumber="33" phylum="Cnidaria" rank="genus">
<emphasis box="[333,442,332,356]" italics="true" pageId="14" pageNumber="33">Halecium</emphasis>
</taxonomicName>
