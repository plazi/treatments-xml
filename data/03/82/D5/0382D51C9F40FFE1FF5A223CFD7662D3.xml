<taxonomicName authority="Hartlaub, 1901" authorityName="Hartlaub" authorityYear="1901" box="[348,766,1802,1824]" class="Hydrozoa" family="Sertulariidae" genus="Symplectoscyphus" kingdom="Animalia" order="Leptothecata" pageId="30" pageNumber="49" phylum="Cnidaria" rank="species" species="flexilis">
