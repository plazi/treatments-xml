<number box="[230,303,1306,1330]" pageId="44" pageNumber="63" value="79635.0">79635</number>
). 
<emphasis bold="true" box="[320,430,1306,1331]" pageId="44" pageNumber="63">Stn. YBU</emphasis>
— 
<date box="[474,593,1306,1330]" pageId="44" pageNumber="63" value="2011-04-26">
<number box="[474,504,1306,1330]" pageId="44" pageNumber="63" value="26.0">26</number>
.iv. 
<number box="[534,593,1306,1330]" pageId="44" pageNumber="63" value="2011.0">2011</number>
</date>
, S 
<number box="[620,649,1306,1330]" pageId="44" pageNumber="63" value="14.0">14</number>
