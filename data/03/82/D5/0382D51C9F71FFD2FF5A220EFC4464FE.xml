<number box="[607,669,1270,1294]" pageId="44" pageNumber="63" value="2008.0">2008</number>
</date>
, S02 (
<quantity box="[747,798,1270,1294]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="44" pageNumber="63" unit="m" value="5.0">5 m</quantity>
