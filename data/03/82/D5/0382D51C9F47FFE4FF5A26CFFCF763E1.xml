<quantity box="[735,797,346,370]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="26" pageNumber="45" unit="m" value="20.0">20 m</quantity>
): several sterile stems, 
<quantity box="[1056,1162,346,370]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="11.0" metricValueMin="4.0" pageId="26" pageNumber="45" unit="mm" value="7.5" valueMax="11.0" valueMin="4.0">4–11 mm</quantity>
