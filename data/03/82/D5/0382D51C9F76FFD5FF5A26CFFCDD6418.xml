<date box="[151,270,428,452]" pageId="43" pageNumber="62" value="2011-04-26">
<number box="[151,181,428,452]" pageId="43" pageNumber="62" value="26.0">26</number>
.iv. 
<number box="[211,270,428,452]" pageId="43" pageNumber="62" value="2011.0">2011</number>
