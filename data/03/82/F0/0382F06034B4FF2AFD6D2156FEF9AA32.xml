<quantity metricMagnitude="-4" metricUnit="m" metricValue="9.0" metricValueMax="9.6" metricValueMin="8.4" pageId="143" pageNumber="143" unit="mm" value="0.8999999999999999" valueMax="0.96" valueMin="0.84">0.84–0.96 mm</quantity>
