<quantity box="[108,273,1284,1308]" metricMagnitude="-3" metricUnit="m" metricValue="3.2199999999999998" metricValueMax="3.36" metricValueMin="3.08" pageId="160" pageNumber="160" unit="mm" value="3.2199999999999998" valueMax="3.36" valueMin="3.08">3.08–3.36 mm</quantity>
, pronotal width 
<quantity box="[468,633,1284,1308]" metricMagnitude="-4" metricUnit="m" metricValue="9.55" metricValueMax="10.2" metricValueMin="8.9" pageId="160" pageNumber="160" unit="mm" value="0.955" valueMax="1.02" valueMin="0.89">0.89–1.02 mm</quantity>
