<subSubSection pageId="1" pageNumber="564" type="etymology">
<paragraph blockId="1.[826,1441,1602,1685]" pageId="1" pageNumber="564">
<emphasis box="[826,952,1602,1623]" italics="true" pageId="1" pageNumber="564">Etymology:</emphasis>
