<subSubSection box="[199,1225,961,988]" pageId="27" pageNumber="478" type="etymology">
<paragraph blockId="27.[151,1437,817,2032]" box="[199,1225,961,988]" pageId="27" pageNumber="478">
<emphasis bold="true" box="[199,334,961,987]" pageId="27" pageNumber="478">Etymology.</emphasis>
