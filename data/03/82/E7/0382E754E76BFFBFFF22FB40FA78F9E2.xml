<taxonomicName baseAuthorityName="Say" baseAuthorityYear="1830" box="[1274,1436,388,410]" class="Insecta" family="Dytiscidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="128" phylum="Arthropoda" rank="species" species="bimarginatus">
