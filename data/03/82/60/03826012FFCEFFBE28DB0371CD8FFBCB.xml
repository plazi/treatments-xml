<collectingRegion box="[174,259,1056,1079]" country="China" name="Yunnan" pageId="25" pageNumber="350">Yunnan</collectingRegion>
and 
<collectingRegion box="[311,404,1056,1080]" country="China" name="Guangxi" pageId="25" pageNumber="350">Guangxi</collectingRegion>
provinces, 
<collectingCountry box="[526,593,1056,1080]" name="China" pageId="25" pageNumber="350">China</collectingCountry>
(
<bibRefCitation author="LIN, P." box="[608,706,1056,1080]" pageId="25" pageNumber="350" pagination="503 - 524" refId="ref29101" refString="LIN, P. 1992: Coleoptera: Rutelidae: 503 - 524. - In: CHEN, S. (ed.): Insects of the Hengduan Mountains region. Volume 1., Beijing: Science Press, i-xii, 865 pp. (in Chinese and English)." type="book chapter" year="1992">LIN 1992</bibRefCitation>
