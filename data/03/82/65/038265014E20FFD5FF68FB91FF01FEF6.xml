<collectingCountry box="[951,1015,188,213]" name="Chile" pageId="4" pageNumber="441">Chile</collectingCountry>
, where the 
<typeStatus box="[1143,1241,188,213]" pageId="4" pageNumber="441">holotype</typeStatus>
was collected. 
<emphasis bold="true" box="[199,494,223,248]" pageId="4" pageNumber="441">
<typeStatus box="[199,258,223,248]" pageId="4" pageNumber="441">Type</typeStatus>
material. 
<typeStatus box="[385,494,223,248]" pageId="4" pageNumber="441">Holotype</typeStatus>
</emphasis>
<specimenCount box="[503,550,224,248]" pageId="4" pageNumber="441" type="female">1 ♀</specimenCount>
: 
<emphasis bold="true" box="[565,629,223,248]" pageId="4" pageNumber="441">
<collectingCountry box="[565,629,223,248]" name="Chile" pageId="4" pageNumber="441">Chile</collectingCountry>
</emphasis>
, Rio Tinguiririca, Valle Colchagua, 
<quantity box="[1048,1138,223,248]" metricMagnitude="3" metricUnit="m" metricValue="1.55" pageId="4" pageNumber="441" unit="m" value="1550.0">1550 m</quantity>
, 
<date box="[1152,1258,224,248]" pageId="4" pageNumber="441" value="1978-02-02">2.II.1978</date>
, leg. Luis Peña (
<collectionCode box="[159,231,260,284]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:1008" name="American Entomological Institute" pageId="4" pageNumber="441">AEIC</collectionCode>
