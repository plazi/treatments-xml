<subSubSection pageId="6" pageNumber="443" type="nomenclature">
<paragraph blockId="6.[151,542,799,859]" box="[151,542,799,826]" pageId="6" pageNumber="443">
<heading bold="true" box="[151,542,799,826]" fontSize="11" level="1" pageId="6" pageNumber="443" reason="1">
<emphasis bold="true" box="[151,542,799,826]" pageId="6" pageNumber="443">
<taxonomicName box="[151,442,799,825]" class="Insecta" family="Ichneumonidae" genus="Atractodes" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="443" phylum="Arthropoda" rank="species" species="saragurensis" status="sp. nov.">
<emphasis bold="true" box="[151,442,799,825]" italics="true" pageId="6" pageNumber="443">Atractodes saragurensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[449,542,800,826]" pageId="6" pageNumber="443" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="6.[151,542,799,859]" box="[151,486,834,859]" pageId="6" pageNumber="443">
(
<figureCitation box="[159,226,834,859]" captionStart="FIGURE 1" captionStartId="2.[151,250,1914,1936]" captionTargetBox="[151,1436,337,1891]" captionTargetId="figure@2.[151,1436,337,1893]" captionTargetPageId="2" captionText="FIGURE 1. Habitus of A. pleuripunctatus species-group. A— A. colchaguensis (holotype); B— A. pleuripunctatus (holotype); C— A. saragurensis (holotype)." httpUri="https://zenodo.org/record/264476/files/figure.png" pageId="6" pageNumber="443">Figs 1</figureCitation>
C, 2B, 2E, 3C, 4C, 4F)
</paragraph>
