. 
<materialsCitation ID-GBIF-Occurrence="1572325040" collectingDate="1969-09-27" collectionCode="USNM" collectorName="El Tor &amp; V. G. Springer" country="Egypt" elevation="5" location="El Tor" pageId="8" pageNumber="211" specimenCode="USNM 270676, 1" specimenCount="1" stateProvince="Suez">
<emphasis bold="true" box="[576,657,1087,1112]" pageId="8" pageNumber="211">
<collectingCountry box="[576,652,1087,1112]" name="Egypt" pageId="8" pageNumber="211">Egypt</collectingCountry>
:
