<emphasis box="[1136,1375,1980,2002]" italics="true" pageId="17" pageNumber="200">
Stephanothrips 
<taxonomicName box="[1299,1375,1980,2002]" pageId="17" pageNumber="200" rank="species" species="barretti">barretti</taxonomicName>
</emphasis>
</taxonomicName>
; 
