<emphasis bold="true" box="[151,342,1950,1972]" pageId="17" pageNumber="200">FIGURES 27–32</emphasis>
. 
<taxonomicName box="[363,522,1950,1972]" class="Insecta" family="Phlaeothripidae" kingdom="Animalia" order="Thysanoptera" pageId="17" pageNumber="200" phylum="Arthropoda" rank="family">Phlaeothripidae</taxonomicName>
species. 
<emphasis bold="true" box="[632,674,1950,1972]" pageId="17" pageNumber="200">(27)</emphasis>
<taxonomicName box="[690,1093,1950,1972]" class="Insecta" family="Phlaeothripidae" genus="Macrophthalmothrips" kingdom="Animalia" order="Thysanoptera" pageId="17" pageNumber="200" phylum="Arthropoda" rank="species" species="neocaledonensis">
<emphasis box="[690,1093,1950,1972]" italics="true" pageId="17" pageNumber="200">Macrophthalmothrips neocaledonensis</emphasis>
</taxonomicName>
large male; 
<emphasis bold="true" box="[1246,1287,1950,1972]" pageId="17" pageNumber="200">(28)</emphasis>
<taxonomicName class="Insecta" family="Phlaeothripidae" genus="Plectrothrips" kingdom="Animalia" order="Thysanoptera" pageId="17" pageNumber="200" phylum="Arthropoda" rank="species" species="australis">
<emphasis italics="true" pageId="17" pageNumber="200">Plectrothrips australis</emphasis>
</taxonomicName>
; 
<emphasis bold="true" box="[257,299,1980,2002]" pageId="17" pageNumber="200">(29)</emphasis>
<taxonomicName box="[310,525,1980,2002]" class="Insecta" family="Phlaeothripidae" genus="Psalidothrips" kingdom="Animalia" order="Thysanoptera" pageId="17" pageNumber="200" phylum="Arthropoda" rank="species" species="taylori">
<emphasis box="[310,525,1980,2002]" italics="true" pageId="17" pageNumber="200">
Psalidothrips 
<taxonomicName box="[457,525,1980,2002]" pageId="17" pageNumber="200" rank="species" species="taylori">taylori</taxonomicName>
</emphasis>
</taxonomicName>
apterous female; 
<emphasis bold="true" box="[719,761,1980,2002]" pageId="17" pageNumber="200">(30)</emphasis>
