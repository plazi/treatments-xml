), 
<taxonomicName authority="Thunberg, 1822" authorityName="Thunberg" authorityYear="1822" box="[1024,1357,1671,1696]" class="Insecta" family="Rutelidae" genus="Rutela" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="230" phylum="Arthropoda" rank="species" species="lacunosa">
