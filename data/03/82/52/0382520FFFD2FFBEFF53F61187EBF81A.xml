(
<figureCitation box="[367,455,1639,1664]" captionStart="FIGURES 25 – 26" captionStartId="28.[151,264,1873,1895]" captionTargetBox="[161,1428,416,1839]" captionTargetId="figure@28.[151,1436,401,1852]" captionTargetPageId="28" captionText="FIGURES 25 – 26. Distribution of Platyceroides species. 25, Platyceroides agassii species group: P. agassii (blue squares); P. pacificus (orange circles). 26, P. pampinatus group: P. infernus (blue squares); P. pampinatus (orange circles)." httpUri="https://zenodo.org/record/582463/files/figure.png" pageId="5" pageNumber="351">Fig. 25</figureCitation>
). 
<emphasis bold="true" box="[484,673,1639,1664]" pageId="5" pageNumber="351">
<collectingRegion box="[484,669,1639,1664]" country="United States of America" name="California" pageId="5" pageNumber="351">CALIFORNIA</collectingRegion>
:
</emphasis>
MENDOCINO: Caspar; Fort Bragg; Hendy Woods State Park; Mendocino; Pygmy Forest; Van Damme State Park. 
<collectionCode box="[746,802,1676,1700]" country="Malaysia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15310" name="Forest Research Centre" pageId="5" pageNumber="351">SAN</collectionCode>
<collectionCode box="[810,911,1676,1700]" pageId="5" pageNumber="351">MATEO</collectionCode>
