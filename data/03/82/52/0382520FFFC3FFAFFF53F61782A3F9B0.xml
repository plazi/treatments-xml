<location LSID="urn:lsid:plazi:treatment:0382520FFFC3FFAFFF53F61782A3F9B0:8EF4B5C2FFC3FFAFFEDFF1F6870FFA00" box="[283,327,1477,1502]" county="Tulare County" municipality="Fall" name="The" pageId="20" pageNumber="366" stateProvince="California">The</location>
