<number box="[480,550,1509,1533]" pageId="12" pageNumber="272" value="11.5" valueMax="13.0" valueMin="10.0">10–13</number>
smooth spiny chaetae; 
<quantity box="[820,909,1508,1533]" metricMagnitude="-1" metricUnit="m" metricValue="2.286" metricValueMax="2.54" metricValueMin="2.032" pageId="12" pageNumber="272" unit="in" value="9.0" valueMax="10.0" valueMin="8.0">8−10 in</quantity>
