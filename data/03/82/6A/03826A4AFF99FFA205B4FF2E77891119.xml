<taxonomicName box="[1142,1436,1206,1228]" class="Insecta" family="Mantidae" genus="Stagmatoptera" kingdom="Animalia" order="Mantodea" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="septentrionalis">
<emphasis bold="true" box="[1142,1436,1206,1228]" italics="true" pageId="7" pageNumber="8">Stagmatoptera septentrionalis</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph blockId="7.[151,1437,258,1375]" pageId="7" pageNumber="8">
<keyLead pageId="7" pageNumber="8">
