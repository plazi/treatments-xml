<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" box="[1065,1436,1449,1475]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="harlequinus">
<emphasis box="[1065,1233,1449,1475]" italics="true" pageId="40" pageNumber="41">O. harlequinus</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[1240,1436,1449,1475]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[213,325,1485,1511]" name="Honduras" pageId="40" pageNumber="41">Honduras</collectingCountry>
, 
<collectingCountry box="[337,452,1485,1511]" name="Nicaragua" pageId="40" pageNumber="41">Nicaragua</collectingCountry>
and 
<collectingCountry box="[507,633,1485,1511]" name="Costa Rica" pageId="40" pageNumber="41">Costa Rica</collectingCountry>
, 
<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" box="[645,1040,1485,1511]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="nicaraguensis">
<emphasis box="[645,837,1485,1511]" italics="true" pageId="40" pageNumber="41">O. nicaraguensis</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[844,1040,1485,1511]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[1110,1225,1485,1511]" name="Nicaragua" pageId="40" pageNumber="41">Nicaragua</collectingCountry>
, 
<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="tenuiflagellum">
<emphasis box="[1240,1436,1485,1511]" italics="true" pageId="40" pageNumber="41">O. tenuiflagellum</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[151,348,1521,1547]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[418,469,1521,1547]" name="Peru" pageId="40" pageNumber="41">Peru</collectingCountry>
, 
<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" box="[484,864,1521,1547]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="umbrocciput">
<emphasis box="[484,660,1521,1547]" italics="true" pageId="40" pageNumber="41">O. umbrocciput</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[667,864,1521,1547]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[934,1055,1521,1547]" name="Guatemala" pageId="40" pageNumber="41">Guatemala</collectingCountry>
and 
<collectingCountry box="[1111,1220,1521,1547]" name="Honduras" pageId="40" pageNumber="41">Honduras</collectingCountry>
, 
<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="wahlbergi">
<emphasis box="[1235,1383,1521,1547]" italics="true" pageId="40" pageNumber="41">O. wahlbergi</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[379,503,1557,1583]" name="Guatemala" pageId="40" pageNumber="41">Guatemala</collectingCountry>
, 
<collectingCountry box="[515,624,1557,1583]" name="Honduras" pageId="40" pageNumber="41">Honduras</collectingCountry>
and 
<collectingCountry box="[679,805,1557,1583]" name="Costa Rica" pageId="40" pageNumber="41">Costa Rica</collectingCountry>
. The group characterised by the antennae being inserted on a rather low protruding shelf, face with papillate sculpture and a largely coriaceous second metasomal tergite, nearly straight thin ovipositor without subapical notch. Two species were tentatively included there: 
<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="mishana">
<emphasis box="[1305,1436,1629,1655]" italics="true" pageId="40" pageNumber="41">O. mishana</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[151,350,1665,1691]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[424,517,1665,1691]" name="Ecuador" pageId="40" pageNumber="41">Ecuador</collectingCountry>
and 
<collectingCountry box="[577,631,1665,1691]" name="Peru" pageId="40" pageNumber="41">Peru</collectingCountry>
, and 
<taxonomicName authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" box="[695,1030,1665,1691]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="pallidus">
<emphasis box="[695,822,1665,1691]" italics="true" pageId="40" pageNumber="41">O. pallidus</emphasis>
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[832,1030,1665,1691]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry box="[1104,1178,1665,1691]" name="Belize" pageId="40" pageNumber="41">Belize</collectingCountry>
, 
<collectingCountry box="[1192,1290,1665,1691]" name="Trinidad and Tobago" pageId="40" pageNumber="41">Trinidad</collectingCountry>
, 
<collectingCountry box="[1304,1432,1665,1691]" name="Costa Rica" pageId="40" pageNumber="41">Costa Rica</collectingCountry>
, 
<collectingCountry box="[151,244,1701,1727]" name="Ecuador" pageId="40" pageNumber="41">Ecuador</collectingCountry>
and 
<collectingCountry box="[299,353,1701,1727]" name="Peru" pageId="40" pageNumber="41">Peru</collectingCountry>
, as they share a similar antennal insertion and finely striate second tergite with several species of this group, although the facial sculpture differs (
<bibRefCitation author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[690,949,1737,1763]" pageId="40" pageNumber="41" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">
Veijalainen 
<emphasis box="[825,881,1737,1763]" italics="true" pageId="40" pageNumber="41">et al.</emphasis>
2014
</bibRefCitation>
).
</paragraph>
</subSubSection>
<subSubSection pageId="40" pageNumber="41" type="description">
<paragraph blockId="40.[151,1437,1413,1907]" pageId="40" pageNumber="41">
Thirteen Mexican species included there: 
<emphasis italics="true" pageId="40" pageNumber="41">
<taxonomicName authorityName="Humala" authorityYear="2019" box="[667,786,1773,1799]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="aztecus">O. aztecus</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[798,920,1773,1799]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="collaris">O. collaris</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[931,1082,1773,1799]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="contrerasi">O. contrerasi</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[1093,1248,1773,1799]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="mexicanus">O. mexicanus</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[1259,1360,1773,1799]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="minor">O. minor</taxonomicName>
, 
<taxonomicName class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="nicaraguensis">O. nicaraguensis</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[300,429,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="oaxacae">O. oaxacae</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[439,543,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="pictus">O. pictus</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[553,715,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="podagricus">O. podagricus</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[725,873,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="scabrosus">O. scabrosus</taxonomicName>
, 
<taxonomicName authorityName="Humala" authorityYear="2019" box="[883,1068,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="tamaulipecus">O. tamaulipecus</taxonomicName>
, 
<taxonomicName box="[1078,1227,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="veracuzus">O. veracuzus</taxonomicName>
,
</emphasis>
and 
<taxonomicName box="[1283,1430,1809,1835]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="wahlbergi">
<emphasis box="[1283,1430,1809,1835]" italics="true" pageId="40" pageNumber="41">O. wahlbergi</emphasis>
</taxonomicName>
. Three more Holarctic species occurring in 
<collectingCountry box="[633,719,1845,1871]" name="Mexico" pageId="40" pageNumber="41">Mexico</collectingCountry>
can also be placed in this group: 
<taxonomicName authority="(Gravenhorst, 1829)" authorityName="Gravenhorst" authorityYear="1829" baseAuthorityName="Gravenhorst" baseAuthorityYear="1829" box="[1097,1431,1845,1871]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="asper">
<emphasis box="[1097,1194,1845,1871]" italics="true" pageId="40" pageNumber="41">O. asper</emphasis>
(
<bibRefCitation author="Gravenhorst, J. L. C." box="[1209,1424,1845,1871]" pageId="40" pageNumber="41" refId="ref62535" refString="Gravenhorst, J. L. C. (1829) Ichneumonologia Europaea. Pars I. sumtibus auctoris, Vratislaviae, 827 pp. https: // doi. org / 10.5962 / bhl. title. 65750" type="book" year="1829">Gravenhorst, 1829</bibRefCitation>
)
</taxonomicName>
, 
