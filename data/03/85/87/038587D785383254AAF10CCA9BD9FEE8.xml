1655 
<emphasis bold="true" box="[327,353,1654,1676]" pageId="15" pageNumber="70">I3</emphasis>
</date>
<taxonomicName authority="Roque-Albelo &amp; Landry, 2009" authorityName="Roque-Albelo &amp; B. Landry" authorityYear="2009" box="[359,442,1654,1676]" class="Insecta" family="Arctiidae" genus="Utetheisa" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="70" phylum="Arthropoda" rank="species" species="henrii" status="sp. n.">
<emphasis box="[359,442,1654,1676]" italics="true" pageId="15" pageNumber="70">U. henrii</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[448,500,1655,1676]" pageId="15" pageNumber="70" rank="species">sp. n.</taxonomicNameLabel>
, slide BL 
<date box="[592,672,1654,1676]" pageId="15" pageNumber="70" value="1653-01-04">
1653 
<emphasis bold="true" box="[646,672,1654,1676]" pageId="15" pageNumber="70">I4</emphasis>
</date>
<taxonomicName authority="Roque-Albelo &amp; Landry, 2009" authorityName="Roque-Albelo &amp; B. Landry" authorityYear="2009" box="[678,814,1655,1676]" class="Insecta" family="Arctiidae" genus="Utetheisa" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="70" phylum="Arthropoda" rank="species" species="connerorum" status="sp. n.">
