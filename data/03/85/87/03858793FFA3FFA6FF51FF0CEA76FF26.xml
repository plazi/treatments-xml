<subSubSection pageId="7" pageNumber="56" type="etymology">
<paragraph blockId="7.[151,1437,1285,2031]" pageId="7" pageNumber="56">
<emphasis bold="true" box="[198,339,1805,1831]" pageId="7" pageNumber="56">Etymology.</emphasis>
