<collectingCountry box="[151,249,2010,2036]" name="Ecuador" pageId="13" pageNumber="46">Ecuador</collectingCountry>
(
<emphasis bold="true" box="[265,405,2010,2036]" pageId="13" pageNumber="46">new record</emphasis>
