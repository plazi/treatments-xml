, 
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="69" pageNumber="70">AMNH 6534</specimenCode>
)
</materialsCitation>
