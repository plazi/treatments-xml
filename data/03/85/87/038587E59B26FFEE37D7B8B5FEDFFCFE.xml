<collectingCountry box="[471,520,693,713]" name="United Arab Emirates" pageId="5" pageNumber="556">UAE</collectingCountry>
: Wadi Wuraya, 
<geoCoordinate box="[666,731,693,713]" degrees="24.24" direction="north" orientation="latitude" pageId="5" pageNumber="556" precision="555" value="24.24">N24.24</geoCoordinate>
<geoCoordinate box="[739,804,693,713]" degrees="56.17" direction="east" orientation="longitude" pageId="5" pageNumber="556" precision="555" value="56.17">E56.17</geoCoordinate>
, 10.- 
<date box="[843,948,693,713]" pageId="5" pageNumber="556" value="2006-12-26">26.12.2006</date>
, 1, leg. A. v. Harten 6361. Wadi Maidaq, 
<geoCoordinate box="[482,547,717,737]" degrees="25. 18" direction="north" orientation="latitude" pageId="5" pageNumber="556" precision="555" value="25.18">N25.18</geoCoordinate>
<geoCoordinate box="[555,619,717,737]" degrees="56.07" direction="east" orientation="longitude" pageId="5" pageNumber="556" precision="555" value="56.07">E56.07</geoCoordinate>
