<taxonomicName authorityName="Li &amp; Liu &amp; He" authorityYear="2022" box="[802,915,1901,1928]" class="Insecta" family="Trigonidiidae" genus="Zilingxi" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="216" phylum="Arthropoda" rank="species" species="danmai">
