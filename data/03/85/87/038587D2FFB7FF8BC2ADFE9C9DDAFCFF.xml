<taxonomicName authority="Kontschán, 2013" authorityName="Kontschán" authorityYear="2013" box="[248,408,1207,1229]" class="Arachnida" family="Oplitidae" genus="Oplitis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="8" pageNumber="1346" phylum="Arthropoda" rank="species" species="hallidayi" status="sp. nov.">
