<paragraph blockId="16.[443,791,646,703]" box="[490,744,678,703]" pageId="16" pageNumber="1722">
(gures 1a, 2e, 3h, 
<quantity box="[707,734,678,702]" metricMagnitude="-3" metricUnit="kg" metricValue="4.0" pageId="16" pageNumber="1722" unit="g" value="4.0">4g</quantity>
