<emphasis bold="true" box="[151,267,1246,1272]" pageId="7" pageNumber="8">Content.</emphasis>
<taxonomicName box="[276,460,1247,1272]" class="Reptilia" family="Scincidae" genus="Eremiascincus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="genus">
