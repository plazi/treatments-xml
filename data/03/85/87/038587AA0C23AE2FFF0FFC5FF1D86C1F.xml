<emphasis box="[1249,1436,702,725]" italics="true" pageId="5" pageNumber="6">Philautus pardus</emphasis>
</taxonomicName>
(square), 
<taxonomicName box="[252,346,737,760]" class="Amphibia" family="Rhacophoridae" genus="Philautus" kingdom="Animalia" order="Anura" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="viridis">
<emphasis box="[252,346,737,760]" italics="true" pageId="5" pageNumber="6">P. viridis</emphasis>
</taxonomicName>
(rhombus), 
<taxonomicName box="[478,575,737,760]" class="Amphibia" family="Rhacophoridae" genus="Philautus" kingdom="Animalia" order="Anura" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="stuarti">
<emphasis box="[478,575,737,760]" italics="true" pageId="5" pageNumber="6">P. stuarti</emphasis>
</taxonomicName>
(star), and 
<taxonomicName box="[697,825,737,760]" class="Amphibia" family="Rhacophoridae" genus="Philautus" kingdom="Animalia" order="Anura" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="variabilis">
