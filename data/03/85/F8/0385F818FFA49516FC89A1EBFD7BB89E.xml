<quantity box="[1279,1385,1739,1764]" metricMagnitude="-2" metricUnit="m" metricValue="3.75" metricValueMax="5.5" metricValueMin="2.0" pageId="1" pageNumber="242" unit="cm" value="3.75" valueMax="5.5" valueMin="2.0">2 – 5.5 cm</quantity>
