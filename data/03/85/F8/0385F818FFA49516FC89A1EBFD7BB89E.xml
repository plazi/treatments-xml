<quantity box="[280,381,278,303]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" metricValueMax="10.0" metricValueMin="6.0" pageId="3" pageNumber="244" unit="mm" value="8.0" valueMax="10.0" valueMin="6.0">6–10 mm</quantity>
