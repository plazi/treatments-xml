<taxonomicName baseAuthorityName="Walker" baseAuthorityYear="1858" class="Insecta" family="Noctuidae" genus="Ophiusa" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="1116" phylum="Arthropoda" rank="species" species="disjungens">
