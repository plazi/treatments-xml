<taxonomicName class="Actinopterygii" family="Gobiesocidae" genus="Lepadogaster" kingdom="Animalia" order="Gobiesociformes" pageId="12" pageNumber="13" phylum="Chordata" rank="species" species="purpurea">
<emphasis italics="true" pageId="12" pageNumber="13">L. purpurea</emphasis>
</taxonomicName>
