<taxonomicName authorityName="J. Feldmann" authorityYear="1942" baseAuthorityName="Dickie" box="[745,955,1518,1539]" class="Florideophyceae" family="Schizymeniaceae" genus="Titanophora" kingdom="Plantae" order="Gigartinales" pageId="9" pageNumber="3028" phylum="Rhodophyta" rank="species" species="pikeana">
<emphasis box="[745,955,1518,1539]" italics="true" pageId="9" pageNumber="3028">Titanophora pikeana</emphasis>
</taxonomicName>
(HEC 14875). (5, 6) 
<taxonomicName authorityName="J. Agardh" authorityYear="1851" baseAuthorityName="J. Agardh" box="[283,512,1545,1566]" class="Florideophyceae" family="Halymeniaceae" genus="Cryptonemia" kingdom="Plantae" order="Halymeniales" pageId="9" pageNumber="3028" phylum="Rhodophyta" rank="species" species="crenulata">
<emphasis box="[283,512,1545,1566]" italics="true" pageId="9" pageNumber="3028">Cryptonemia crenulata</emphasis>
</taxonomicName>
