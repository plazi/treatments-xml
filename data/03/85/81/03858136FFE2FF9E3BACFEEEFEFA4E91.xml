<quantity box="[1295,1400,1863,1887]" metricMagnitude="-1" metricUnit="m" metricValue="5.588" metricValueMax="6.096" metricValueMin="5.08" pageId="11" pageNumber="466" unit="in" value="22.0" valueMax="24.0" valueMin="20.0">20–24 in</quantity>
