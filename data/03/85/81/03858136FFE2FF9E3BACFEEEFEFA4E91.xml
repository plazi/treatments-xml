<emphasis box="[623,755,1763,1787]" italics="true" pageId="11" pageNumber="466">C. littoralis</emphasis>
</taxonomicName>
, 
<quantity box="[771,873,1763,1787]" metricMagnitude="-1" metricUnit="m" metricValue="9.906" metricValueMax="11.43" metricValueMin="8.382" pageId="11" pageNumber="466" unit="in" value="39.0" valueMax="45.0" valueMin="33.0">33–45 in</quantity>
<taxonomicName box="[883,1071,1763,1787]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="466" phylum="Arthropoda" rank="species" species="venetacaudus">
<emphasis box="[883,1071,1763,1787]" italics="true" pageId="11" pageNumber="466">C. venetacaudus</emphasis>
