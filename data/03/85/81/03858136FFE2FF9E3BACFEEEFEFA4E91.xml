<quantity box="[1130,1232,1763,1787]" metricMagnitude="-1" metricUnit="m" metricValue="8.763" metricValueMax="9.652" metricValueMin="7.874" pageId="11" pageNumber="466" unit="in" value="34.5" valueMax="38.0" valueMin="31.0">31–38 in</quantity>
<taxonomicName box="[1241,1380,1764,1787]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="466" phylum="Arthropoda" rank="species" species="cyanurus">
<emphasis box="[1241,1380,1764,1787]" italics="true" pageId="11" pageNumber="466">C. cyanurus</emphasis>
