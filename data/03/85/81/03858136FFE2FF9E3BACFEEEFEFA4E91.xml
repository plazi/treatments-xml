subgroup; 
<quantity box="[385,490,1796,1820]" metricMagnitude="-1" metricUnit="m" metricValue="6.35" metricValueMax="6.604" metricValueMin="6.096" pageId="11" pageNumber="466" unit="in" value="25.0" valueMax="26.0" valueMin="24.0">24–26 in</quantity>
