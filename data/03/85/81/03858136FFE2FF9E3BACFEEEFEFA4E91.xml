</taxonomicName>
subgroup); five supraciliaries (
<quantity box="[1132,1205,1829,1853]" metricMagnitude="-1" metricUnit="m" metricValue="1.651" metricValueMax="1.778" metricValueMin="1.524" pageId="11" pageNumber="466" unit="in" value="6.5" valueMax="7.0" valueMin="6.0">6–7 in</quantity>
