<quantity box="[1261,1418,1144,1171]" metricMagnitude="3" metricUnit="m" metricValue="3.35" metricValueMax="3.5" metricValueMin="3.2" pageId="2" pageNumber="226" unit="m" value="3350.0" valueMax="3500.0" valueMin="3200.0">
