<collectingCountry box="[620,704,460,486]" name="Bolivia" pageId="2" pageNumber="226">Bolivia</collectingCountry>
, 
<collectingCountry box="[714,807,460,486]" name="Ecuador" pageId="2" pageNumber="226">Ecuador</collectingCountry>
(
<bibRefCitation author="Clark, L. G." box="[822,950,460,486]" pageId="2" pageNumber="226" pagination="1 - 128" refId="ref8205" refString="Clark, L. G. (2000) Neurolepis. In: Judziewicz, E. J., Soreng, R. J., Davidse, G., Peterson, P. M., Filgueiras, T. S. &amp; Zuloaga, F. O. (Eds.) Catalogue of New World Grasses (Poaceae): I. Subfamilies Anomochlooideae, Bambusoideae, Ehrhartoideae, and Pharoideae. Contributions from the United States National Herbarium 39: 1 - 128." type="journal article" year="2000">Clark 2000</bibRefCitation>
) and 
<collectingCountry box="[1010,1065,461,487]" name="Peru" pageId="2" pageNumber="226">Peru</collectingCountry>
