<quantity box="[393,551,496,523]" metricMagnitude="3" metricUnit="m" metricValue="3.2" metricValueMax="3.4" metricValueMin="3.0" pageId="2" pageNumber="226" unit="m" value="3200.0" valueMax="3400.0" valueMin="3000.0">3000–3400 m</quantity>
