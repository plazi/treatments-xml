<date box="[860,919,726,752]" pageId="5" pageNumber="6" value="2004">2004</date>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName authority="Pan, Zhang &amp; Chen, 2006" authorityName="Pan, Zhang &amp; Chen" authorityYear="2006" box="[978,1368,726,753]" class="Collembola" family="Entomobryidae" genus="Willowsia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="shi">
