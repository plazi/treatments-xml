<typeStatus box="[201,293,872,896]" pageId="28" pageNumber="229">Syntype</typeStatus>
<accessionNumber box="[303,444,872,896]" httpUri="http://www.uniprot.org/uniprot/ZMB30217" pageId="28" pageNumber="229">
