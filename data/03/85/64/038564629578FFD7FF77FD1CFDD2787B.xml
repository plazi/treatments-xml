, as 
<taxonomicName baseAuthorityName="Simroth" baseAuthorityYear="1889" class="Gastropoda" family="Athoracophoridae" genus="Pseudaneitea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="28" pageNumber="229" phylum="Mollusca" rank="species" species="marmorata">
