<taxonomicName authorityName="Malm" authorityYear="1874" box="[1118,1204,1557,1580]" class="Gastropoda" family="Athoracophoridae" genus="Ottonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="30" pageNumber="231" phylum="Mollusca" rank="genus">
