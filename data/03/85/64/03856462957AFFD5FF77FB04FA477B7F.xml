<taxonomicName authorityName="Malm" authorityYear="1874" box="[827,913,1520,1543]" class="Gastropoda" family="Athoracophoridae" genus="Ottonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="30" pageNumber="231" phylum="Mollusca" rank="genus">
