<taxonomicName authority="Strand, 1932" authorityName="Strand" authorityYear="1932" box="[201,446,1591,1616]" class="Gastropoda" family="Athoracophoridae" genus="Ottonita" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="30" pageNumber="231" phylum="Mollusca" rank="genus">
