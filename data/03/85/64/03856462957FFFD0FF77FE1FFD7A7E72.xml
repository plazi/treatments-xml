<taxonomicName baseAuthorityName="Quoy &amp; Gaimard" baseAuthorityYear="1832" box="[686,1036,655,680]" class="Gastropoda" family="Athoracophoridae" genus="Athoracophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="27" pageNumber="228" phylum="Mollusca" rank="species" species="bitentaculatus">
