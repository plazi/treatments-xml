<taxonomicName authorityName="Collinge" authorityYear="1894" box="[601,799,764,789]" class="Gastropoda" family="Athoracophoridae" genus="Janella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="27" pageNumber="228" phylum="Mollusca" rank="species" species="maculata">
