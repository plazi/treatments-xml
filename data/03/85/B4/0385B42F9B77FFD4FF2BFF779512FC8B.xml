(
<collectingRegion box="[510,655,829,854]" country="India" name="Maharashtra" pageId="3" pageNumber="176">Maharashtra</collectingRegion>
; 
<collectingRegion box="[670,820,829,854]" country="India" name="West Bengal" pageId="3" pageNumber="176">West Bengal</collectingRegion>
), 
<collectingCountry box="[844,892,830,853]" name="Iran" pageId="3" pageNumber="176">Iran</collectingCountry>
, 
<collectingCountry box="[907,1021,829,854]" name="Sri Lanka" pageId="3" pageNumber="176">Sri Lanka</collectingCountry>
and 
<collectingCountry box="[1084,1184,829,854]" name="Thailand" pageId="3" pageNumber="176">Thailand</collectingCountry>
(
<bibRefCitation author="Anichtchenko" box="[1202,1431,829,854]" pageId="3" pageNumber="176" refString="Anichtchenko, A. (2016) Carabidae of the World. Available from: http: // carabidae. org (accessed 2 December 2016)" type="journal article" year="2016">Anichtchenko 2016</bibRefCitation>
