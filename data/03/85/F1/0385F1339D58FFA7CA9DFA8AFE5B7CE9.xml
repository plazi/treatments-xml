<paragraph blockId="23.[189,1399,1468,2028]" lastBlockId="24.[189,1399,267,1012]" lastPageId="24" lastPageNumber="25" pageId="23" pageNumber="24">
<taxonomicName baseAuthorityName="Siebold &amp; Zuccarini" baseAuthorityYear="1846" box="[189,372,1788,1814]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="species" species="heterophylla">
