<collectingCountry box="[357,424,1869,1893]" name="China" pageId="12" pageNumber="181">China</collectingCountry>
(
<collectingCountry box="[439,528,1869,1893]" name="Taiwan" pageId="12" pageNumber="181">Taiwan</collectingCountry>
