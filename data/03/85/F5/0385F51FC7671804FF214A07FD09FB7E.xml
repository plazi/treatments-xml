, HW 
<quantity box="[1266,1432,886,911]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" metricValueMax="6.7" metricValueMin="6.3" pageId="30" pageNumber="527" unit="mm" value="0.65" valueMax="0.67" valueMin="0.63">0.63–0.67 mm</quantity>
, PL 
<quantity box="[191,357,922,947]" metricMagnitude="-4" metricUnit="m" metricValue="6.6" metricValueMax="6.8999999999999995" metricValueMin="6.3" pageId="30" pageNumber="527" unit="mm" value="0.6599999999999999" valueMax="0.69" valueMin="0.63">0.63–0.69 mm</quantity>
, PW 
<quantity box="[420,586,922,947]" metricMagnitude="-4" metricUnit="m" metricValue="6.75" metricValueMax="7.1" metricValueMin="6.4" pageId="30" pageNumber="527" unit="mm" value="0.675" valueMax="0.71" valueMin="0.64">0.64–0.71 mm</quantity>
, EL 
<quantity box="[640,806,922,947]" metricMagnitude="-4" metricUnit="m" metricValue="8.2" metricValueMax="8.8" metricValueMin="7.6" pageId="30" pageNumber="527" unit="mm" value="0.82" valueMax="0.88" valueMin="0.76">0.76–0.88 mm</quantity>
, EW 
<quantity box="[870,1036,923,947]" metricMagnitude="-3" metricUnit="m" metricValue="1.18" metricValueMax="1.29" metricValueMin="1.07" pageId="30" pageNumber="527" unit="mm" value="1.18" valueMax="1.29" valueMin="1.07">1.07–1.29 mm</quantity>
, AL 
<quantity box="[1093,1259,922,947]" metricMagnitude="-3" metricUnit="m" metricValue="1.2799999999999998" metricValueMax="1.4" metricValueMin="1.16" pageId="30" pageNumber="527" unit="mm" value="1.2799999999999998" valueMax="1.4" valueMin="1.16">1.16–1.40 mm</quantity>
, AW 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="1.3199999999999998" metricValueMax="1.43" metricValueMin="1.21" pageId="30" pageNumber="527" unit="mm" value="1.3199999999999998" valueMax="1.43" valueMin="1.21">1.21–1.43 mm</quantity>
.
</paragraph>
