<subSubSection pageId="15" pageNumber="512" type="etymology">
<paragraph blockId="15.[151,1436,151,429]" box="[199,1031,188,213]" pageId="15" pageNumber="512">
<emphasis bold="true" box="[199,334,188,213]" pageId="15" pageNumber="512">Etymology.</emphasis>
