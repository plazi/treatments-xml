, PW 
<quantity box="[372,534,1713,1738]" metricMagnitude="-4" metricUnit="m" metricValue="6.05" metricValueMax="6.2" metricValueMin="5.8999999999999995" pageId="15" pageNumber="512" unit="mm" value="0.605" valueMax="0.62" valueMin="0.59">0.59–0.62 mm</quantity>
, EL 
<quantity box="[585,747,1713,1738]" metricMagnitude="-4" metricUnit="m" metricValue="6.85" metricValueMax="7.0" metricValueMin="6.7" pageId="15" pageNumber="512" unit="mm" value="0.685" valueMax="0.7" valueMin="0.67">0.67–0.70 mm</quantity>
, EW 
<quantity box="[807,969,1713,1738]" metricMagnitude="-3" metricUnit="m" metricValue="1.04" metricValueMax="1.05" metricValueMin="1.03" pageId="15" pageNumber="512" unit="mm" value="1.04" valueMax="1.05" valueMin="1.03">1.03–1.05 mm</quantity>
, AL 
