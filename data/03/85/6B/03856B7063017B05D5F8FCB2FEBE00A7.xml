<caption httpUri="https://zenodo.org/record/1457645/files/figure.png" pageId="4" pageNumber="147" startId="4.[151,264,1839,1862]" targetBox="[159,1404,210,1812]" targetPageId="4">
<paragraph blockId="4.[151,1436,1839,1922]" pageId="4" pageNumber="147">
<emphasis bold="true" box="[151,342,1839,1862]" pageId="4" pageNumber="147">FIGURES 10–18.</emphasis>
<taxonomicName authority="Gonçalves &amp; Takiya &amp; Mejdalani, 2018" authorityName="Gonçalves &amp; Takiya &amp; Mejdalani" authorityYear="2018" box="[351,660,1839,1861]" class="Insecta" family="Cicadellidae" genus="Polana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="147" phylum="Arthropoda" rank="species" species="maculosa" status="sp. nov." subGenus="Declivella">
<emphasis box="[351,424,1839,1861]" italics="true" pageId="4" pageNumber="147">Polana</emphasis>
(
<emphasis box="[442,546,1839,1861]" italics="true" pageId="4" pageNumber="147">Declivella</emphasis>
) 
<emphasis box="[563,660,1839,1861]" italics="true" pageId="4" pageNumber="147">maculosa</emphasis>
</taxonomicName>
