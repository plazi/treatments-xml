<quantity box="[617,720,1687,1711]" metricMagnitude="-4" metricUnit="m" metricValue="4.3" pageId="1" pageNumber="42" unit="mm" value="0.43">0.43 mm</quantity>
, FWL: 
<quantity box="[803,904,1687,1711]" metricMagnitude="-3" metricUnit="m" metricValue="3.27" pageId="1" pageNumber="42" unit="mm" value="3.27">3.27 mm</quantity>
