, 
<collectingCountry box="[606,677,367,392]" name="Brazil" pageId="57" pageNumber="58">Brazil</collectingCountry>
, 
<collectingCountry box="[689,743,368,391]" name="Peru" pageId="57" pageNumber="58">Peru</collectingCountry>
, 
<collectingCountry box="[754,849,367,392]" name="Suriname" pageId="57" pageNumber="58">Surinam</collectingCountry>
and 
<collectingCountry box="[903,1022,367,392]" name="Venezuela" pageId="57" pageNumber="58">Venezuela</collectingCountry>
.
</paragraph>
