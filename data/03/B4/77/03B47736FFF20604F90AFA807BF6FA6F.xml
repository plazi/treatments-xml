<caption ID-DOI="http://doi.org/10.5281/zenodo.5220536" ID-Zenodo-Dep="5220536" box="[291,1033,935,956]" httpUri="https://zenodo.org/record/5220536/files/figure.png" pageId="25" pageNumber="3393" startId="25.[291,351,936,955]" targetBox="[201,1122,136,897]" targetPageId="25">
<paragraph blockId="25.[291,1033,935,956]" box="[291,1033,935,956]" pageId="25" pageNumber="3393">
