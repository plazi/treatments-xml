(
<emphasis bold="true" box="[612,707,1939,1963]" pageId="4" pageNumber="21">A,C,D,E</emphasis>
) and 
<taxonomicName box="[769,991,1940,1963]" class="Insecta" family="Haglotettigoniidae" genus="Agnotecous" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="21" phylum="Arthropoda" rank="species" species="nekando" status="n. sp.">
