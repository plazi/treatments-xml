<taxonomicName authority="Vila &amp; F. Caballero (2007: 41)" authorityName="Vila &amp; F. Caballero" authorityPageNumber="41" authorityYear="2007" box="[476,1002,338,365]" class="Agaricomycetes" family="Entolomataceae" genus="Entoloma" kingdom="Fungi" order="Agaricales" pageId="9" pageNumber="10" phylum="Basidiomycota" rank="species" species="phaeodiscum">
