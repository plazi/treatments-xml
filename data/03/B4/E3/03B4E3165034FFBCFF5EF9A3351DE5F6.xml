<taxonomicName authorityName="Lamarck" authorityYear="1818" box="[953,1091,403,429]" class="Polychaeta" family="Pectinariidae" genus="Amphictene" kingdom="Animalia" order="Terebellida" pageId="25" pageNumber="26" phylum="Annelida" rank="genus">
