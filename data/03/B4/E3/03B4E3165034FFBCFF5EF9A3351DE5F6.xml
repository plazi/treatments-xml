<paragraph blockId="25.[189,1399,267,364]" box="[193,1399,338,364]" pageId="25" pageNumber="26">
<keyLead box="[193,1399,338,364]" pageId="25" pageNumber="26">– Cephalic veil cirrate; scaphe distinctly separated from abdomen; dc/vu not 17/14 .......................... 2</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="25" pageNumber="26">
<paragraph blockId="25.[189,1399,403,501]" pageId="25" pageNumber="26">
<keyLead pageId="25" pageNumber="26">
