<paragraph blockId="34.[151,1436,1606,1954]" pageId="34" pageNumber="35">
<emphasis bold="true" box="[151,266,1606,1631]" pageId="34" pageNumber="35">Diagnosis</emphasis>
. 
<taxonomicName box="[280,422,1606,1630]" class="Insecta" family="Ichneumonidae" genus="Ophion" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="betanimenus">
<emphasis box="[280,422,1606,1630]" italics="true" pageId="34" pageNumber="35">betanimenus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[476,638,1606,1630]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="transvaalensis">
