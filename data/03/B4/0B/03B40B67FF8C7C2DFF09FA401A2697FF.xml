<subSubSection pageId="22" pageNumber="23" type="diagnosis">
<paragraph blockId="22.[151,1436,1650,2034]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[151,266,1650,1675]" pageId="22" pageNumber="23">Diagnosis</emphasis>
. 
<taxonomicName box="[280,492,1651,1675]" class="Insecta" family="Ichneumonidae" genus="Temelucha" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pestifer">
<emphasis box="[280,492,1651,1675]" italics="true" pageId="22" pageNumber="23">Temelucha pestifer</emphasis>
</taxonomicName>
