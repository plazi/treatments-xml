<number box="[280,311,402,426]" pageId="5" pageNumber="6" value="42.0">42</number>
, dorsal fin rays ~ 
<number box="[519,549,402,426]" pageId="5" pageNumber="6" value="35.0">35</number>
, anal fin rays ~ 
<number box="[735,765,402,426]" pageId="5" pageNumber="6" value="31.0">31</number>
. Interorbital width 
<number box="[990,1006,402,426]" pageId="5" pageNumber="6" value="9.0">9</number>
, mandible-anus 
<number box="[1196,1226,402,426]" pageId="5" pageNumber="6" value="39.0">39</number>
, anus-anal fin 
<number box="[1401,1431,402,426]" pageId="5" pageNumber="6" value="17.0">17</number>
, lower jaw 
<number box="[269,297,438,462]" pageId="5" pageNumber="6" value="12.0">12</number>
% SL.
</paragraph>
