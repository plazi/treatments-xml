<subSubSection pageId="10" pageNumber="1215" type="distribution">
<paragraph blockId="10.[810,1420,968,1445]" pageId="10" pageNumber="1215">
<emphasis bold="true" box="[810,955,1286,1310]" pageId="10" pageNumber="1215">Distribution.</emphasis>
<taxonomicName authorityName="Weber" authorityYear="1938" box="[962,1232,1287,1311]" class="Insecta" family="Formicidae" genus="Cyphomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="1215" phylum="Arthropoda" rank="species" species="laevigatus">
<emphasis box="[962,1232,1287,1311]" italics="true" pageId="10" pageNumber="1215">Cyphomyrmex laevigatus</emphasis>
