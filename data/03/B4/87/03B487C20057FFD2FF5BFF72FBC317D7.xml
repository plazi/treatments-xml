, 
<taxonomicName authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[1254,1393,332,358]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="worleyae">
