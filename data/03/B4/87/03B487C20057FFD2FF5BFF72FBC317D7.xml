<taxonomicName authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[886,994,404,430]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="beardi">
<emphasis box="[886,994,404,430]" italics="true" pageId="69" pageNumber="70">O. beardi</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[1009,1147,404,430]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="dorrellae">
<emphasis box="[1009,1029,404,430]" italics="true" pageId="69" pageNumber="70">O</emphasis>
. 
<emphasis box="[1044,1147,404,430]" italics="true" pageId="69" pageNumber="70">dorrellae</emphasis>
</taxonomicName>
