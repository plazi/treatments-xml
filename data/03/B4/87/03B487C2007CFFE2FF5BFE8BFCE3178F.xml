<paragraph blockId="111.[151,1437,151,1725]" pageId="111" pageNumber="112">
<materialsCitation ID-GBIF-Occurrence="3069296367" collectionCode="RA" pageId="111" pageNumber="112" specimenCode="RP2, RP1, RP3-4" specimenCount="3" typeStatus="paratype">
<typeStatus box="[199,297,151,177]" pageId="111" pageNumber="112">Paratype</typeStatus>
