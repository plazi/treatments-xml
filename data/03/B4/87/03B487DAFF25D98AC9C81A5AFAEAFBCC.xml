and 
<taxonomicName box="[652,906,1886,1910]" class="Phaeophyceae" family="Desmarestiaceae" genus="Phyllogigas" higherTaxonomySource="GBIF" kingdom="Chromista" order="Desmarestiales" pageId="144" pageNumber="145" phylum="Ochrophyta" rank="species" species="grandifolius">
