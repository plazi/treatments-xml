<paragraph blockId="18.[189,1398,520,582]" pageId="18" pageNumber="19">
Peninsular 
<collectingCountry box="[322,434,520,546]" name="Malaysia" pageId="18" pageNumber="19">Malaysia</collectingCountry>
: 
<collectingRegion box="[446,516,520,546]" country="Malaysia" name="Perak" pageId="18" pageNumber="19">Perak</collectingRegion>
, 
<collectingRegion box="[529,596,520,546]" country="Malaysia" name="Johor" pageId="18" pageNumber="19">Johor</collectingRegion>
