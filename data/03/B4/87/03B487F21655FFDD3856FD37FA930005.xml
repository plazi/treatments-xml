<quantity box="[592,727,785,807]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" metricValueMax="9.0" metricValueMin="5.0" pageId="24" pageNumber="25" unit="mm" value="0.7" valueMax="0.9" valueMin="0.5">0.5–0.9 mm</quantity>
