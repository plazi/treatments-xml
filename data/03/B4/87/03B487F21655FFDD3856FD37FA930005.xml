<quantity box="[348,426,754,776]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" pageId="24" pageNumber="25" unit="mm" value="45.0">45 mm</quantity>
<potCollectionCode box="[433,484,755,777]" pageId="24" pageNumber="25">SVL</potCollectionCode>
