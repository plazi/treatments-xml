<materialsCitation ID-GBIF-Occurrence="3069296372" collectingDate="1998-04-08" collectionCode="A" collectorName="Republic &amp; Carolyn Threadgill" location="Stonerose Interpretive" pageId="43" pageNumber="44" specimenCode="A0307" specimenCount="1" stateProvince="Centre" typeStatus="holotype">
