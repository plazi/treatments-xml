<taxonomicName authority="(Kobayashi, 1936)" baseAuthorityName="Kobayashi" baseAuthorityYear="1936" box="[514,813,1079,1103]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="1248" phylum="Annelida" rank="species" species="vallis">
