and 
<taxonomicName baseAuthorityName="Chen" baseAuthorityYear="1946" box="[467,631,1111,1135]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="1248" phylum="Annelida" rank="species" species="mucrorimus">
