<taxonomicName baseAuthorityName="Kobayashi" baseAuthorityYear="1936" box="[144,291,1143,1167]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="1248" phylum="Annelida" rank="species" species="koryoensis">
<emphasis box="[144,291,1143,1167]" italics="true" pageId="20" pageNumber="1248">A. koryoensis</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Kobayashi" baseAuthorityYear="1936" box="[305,393,1143,1167]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="1248" phylum="Annelida" rank="species" species="vallis">
<emphasis box="[305,393,1143,1167]" italics="true" pageId="20" pageNumber="1248">A. vallis</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Hong" authorityYear="2001" box="[408,563,1143,1167]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="1248" phylum="Annelida" rank="species" species="pagyeiensis">
<emphasis box="[408,563,1143,1167]" italics="true" pageId="20" pageNumber="1248">A. pagyeiensis</emphasis>
