</paragraph>
<paragraph blockId="10.[151,1436,153,467]" pageId="10" pageNumber="537">
<emphasis bold="true" box="[199,455,188,215]" pageId="10" pageNumber="537">Specimens examined.</emphasis>
<typeStatus box="[462,566,189,215]" pageId="10" pageNumber="537">Holotype</typeStatus>
female and 
<typeStatus box="[704,811,189,215]" pageId="10" pageNumber="537">paratypes</typeStatus>
