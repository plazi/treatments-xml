<date box="[428,549,1869,1895]" pageId="9" pageNumber="536">
<emphasis box="[428,462,1869,1895]" italics="true" pageId="9" pageNumber="536">iv2</emphasis>
and 
<emphasis box="[514,549,1869,1895]" italics="true" pageId="9" pageNumber="536">iv3</emphasis>
</date>
); 
<date box="[572,606,1869,1895]" pageId="9" pageNumber="536">
<emphasis box="[572,606,1869,1895]" italics="true" pageId="9" pageNumber="536">iv1</emphasis>
</date>
posterior to 
<emphasis box="[745,778,1869,1895]" italics="true" pageId="9" pageNumber="536">st1</emphasis>
, 
<date box="[791,825,1869,1895]" pageId="9" pageNumber="536">
<emphasis box="[791,825,1869,1895]" italics="true" pageId="9" pageNumber="536">iv2</emphasis>
