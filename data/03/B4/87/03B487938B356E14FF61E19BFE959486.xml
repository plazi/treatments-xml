<emphasis box="[226,258,1905,1931]" italics="true" pageId="9" pageNumber="536">st2</emphasis>
7, 
<emphasis box="[294,327,1905,1931]" italics="true" pageId="9" pageNumber="536">st3</emphasis>
8, 
<emphasis box="[362,394,1905,1931]" italics="true" pageId="9" pageNumber="536">st4</emphasis>
