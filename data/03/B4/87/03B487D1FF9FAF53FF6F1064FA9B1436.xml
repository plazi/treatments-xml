<number box="[527,642,1956,1980]" pageId="10" pageNumber="11" value="76.75" valueMax="84.2" valueMin="69.3">69.3–84.2</number>
, LPL 46.0, md 
<number box="[844,959,1956,1980]" pageId="10" pageNumber="11" value="42.150000000000006" valueMax="43.1" valueMin="41.2">41.2–43.1</number>
, disk 
<number box="[1038,1132,1956,1980]" pageId="10" pageNumber="11" value="22.35" valueMax="24.7" valueMin="20.0">20–24.7</number>
, preD 118.0–119.0, preA 
<number box="[151,214,1992,2016]" pageId="10" pageNumber="11" value="159.8">159.8</number>
–182.0, ma 
<number box="[346,461,1992,2016]" pageId="10" pageNumber="11" value="79.5" valueMax="81.8" valueMin="77.2">77.2–81.8</number>
, aAf 
<number box="[521,636,1992,2016]" pageId="10" pageNumber="11" value="87.0" valueMax="99.6" valueMin="74.4">74.4–99.6</number>
, LLD 
