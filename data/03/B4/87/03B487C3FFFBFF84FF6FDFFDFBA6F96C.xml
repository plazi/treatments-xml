<emphasis box="[901,995,1059,1083]" italics="true" pageId="13" pageNumber="12">Aesalus</emphasis>
</taxonomicName>
/ 
<taxonomicName box="[1020,1176,1059,1083]" class="Insecta" family="Lucanidae" genus="Aesalus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="12" phylum="Arthropoda" rank="species" species="neotropicalis">
