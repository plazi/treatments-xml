<paragraph blockId="28.[151,1436,1302,2010]" pageId="28" pageNumber="29">
<emphasis bold="true" box="[199,321,1482,1507]" pageId="28" pageNumber="29">Diagnosis.</emphasis>
Vertebrae 
<number box="[448,478,1482,1506]" pageId="28" pageNumber="29" value="79.0">79</number>
, dorsal fin rays ~ 
<number box="[686,716,1482,1506]" pageId="28" pageNumber="29" value="71.0">71</number>
, anal fin rays ~ 
<number box="[910,940,1482,1506]" pageId="28" pageNumber="29" value="67.0">67</number>
