<taxonomicName authority="(Knight) Antennal" authorityName="Antennal" baseAuthorityName="Knight" baseAuthorityYear="1923" class="Insecta" family="Miridae" genus="Tropidosteptes" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="species" species="glaber">
