<subSubSection pageId="10" pageNumber="11" type="diagnosis">
<paragraph blockId="10.[189,310,873,899]" box="[189,310,873,899]" pageId="10" pageNumber="11">
<heading bold="true" box="[189,310,873,899]" fontSize="11" level="3" pageId="10" pageNumber="11" reason="3">
