<date box="[151,277,332,356]" pageId="3" pageNumber="138" value="2011-06-22">
<number box="[151,181,332,356]" pageId="3" pageNumber="138" value="22.0">22</number>
.VI. 
<number box="[219,277,332,356]" pageId="3" pageNumber="138" value="2011.0">2011</number>
