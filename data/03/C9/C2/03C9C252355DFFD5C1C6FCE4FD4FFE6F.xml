<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[451,665,332,357]" class="Magnoliopsida" family="Elaeagnaceae" genus="Eleagnus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="138" phylum="Magnoliophyta" rank="genus">
