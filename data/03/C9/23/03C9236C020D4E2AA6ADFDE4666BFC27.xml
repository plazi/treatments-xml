<taxonomicName authority="BORDONI, 2002" authorityName="BORDONI" authorityYear="2002" box="[174,520,832,856]" class="Insecta" family="Staphylinidae" genus="Erymus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="260" phylum="Arthropoda" rank="species" species="heumorphus">
