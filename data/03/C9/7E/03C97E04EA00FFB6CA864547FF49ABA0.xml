<taxonomicName box="[305,395,1424,1446]" class="Insecta" family="Chalcididae" genus="Dirhinus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="327" phylum="Arthropoda" rank="genus">
