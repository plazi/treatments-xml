<number box="[1056,1064,1803,1817]" pageId="7" pageNumber="528" value="3.0">3</number>
<number box="[1071,1106,1788,1813]" pageId="7" pageNumber="528" value="2.5">2.5</number>
and 
<number box="[1161,1197,1788,1813]" pageId="7" pageNumber="528" value="4.5">4.5</number>
