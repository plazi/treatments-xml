<paragraph blockId="28.[151,1437,1830,2035]" box="[151,996,1830,1855]" pageId="28" pageNumber="379">
<emphasis bold="true" box="[151,297,1830,1855]" pageId="28" pageNumber="379">Description.</emphasis>
Female. Body length 
<emphasis box="[544,578,1831,1854]" italics="true" pageId="28" pageNumber="379">ca.</emphasis>
<quantity box="[578,666,1830,1855]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="28" pageNumber="379" unit="mm" value="1.6">1.6 mm</quantity>
, fore wing length 
<emphasis box="[869,895,1831,1854]" italics="true" pageId="28" pageNumber="379">ca</emphasis>
