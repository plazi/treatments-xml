<taxonomicName authority="Liu" authorityName="Liu" box="[298,587,1953,1975]" class="Insecta" family="Braconidae" genus="Pholetesor" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="383" phylum="Arthropoda" rank="species" species="teresitergum">
