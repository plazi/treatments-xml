<number box="[852,882,1986,2010]" pageId="1" pageNumber="46" value="10.0">10</number>
, in 
<taxonomicName box="[922,1026,1987,2011]" class="Entognatha" family="Neanuridae" genus="Endonura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="1" pageNumber="46" phylum="Arthropoda" rank="species" species="alavensis">
