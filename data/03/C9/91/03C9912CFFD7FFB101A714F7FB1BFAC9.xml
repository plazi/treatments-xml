<typeStatus box="[151,204,1762,1786]" pageId="5" pageNumber="6">Type</typeStatus>
genus
</emphasis>
. 
<taxonomicName authority="Quatrefages, 1866" authorityName="Quatrefages" authorityYear="1866" box="[285,560,1762,1786]" class="Polychaeta" family="Orbiniidae" genus="Orbinia" kingdom="Animalia" pageId="5" pageNumber="6" phylum="Annelida" rank="genus">
