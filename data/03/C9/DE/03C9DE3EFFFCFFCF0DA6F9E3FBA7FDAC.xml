(
<emphasis box="[477,578,1945,1969]" italics="true" pageId="34" pageNumber="185">Polynema</emphasis>
)
</taxonomicName>
? 
<taxonomicName authorityName="Triapitsyn" authorityYear="2021" box="[605,712,1945,1968]" class="Insecta" family="Mymaridae" genus="Polynema" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="185" phylum="Arthropoda" rank="species" species="imperatrix">
