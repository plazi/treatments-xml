<materialsCitation ID-GBIF-Occurrence="3400686302" collectedFrom="delphacid leafhopper Sogata eupompe Kirkaldy" collectingDate="1936" collectorName="O. H. Swezey" country="Guam" location="Agat" pageId="0" pageNumber="173" specimenCount="1">
