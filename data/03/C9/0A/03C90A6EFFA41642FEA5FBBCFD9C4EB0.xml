<collectingCountry box="[1421,1486,1545,1587]" name="Fiji" pageId="0" pageNumber="173" value="Fiji">Fiji</collectingCountry>
under the name 
<taxonomicName authority="Kirby" authorityName="Kirby" box="[390,882,1596,1638]" class="Insecta" family="Elenchidae" genus="Elenchus" kingdom="Animalia" order="Strepsiptera" pageId="0" pageNumber="173" phylum="Arthropoda" rank="species" species="tenuicornis">
<emphasis box="[390,760,1596,1638]" italics="true" pageId="0" pageNumber="173">Elenchus tenuicornis</emphasis>
Kirby
</taxonomicName>
