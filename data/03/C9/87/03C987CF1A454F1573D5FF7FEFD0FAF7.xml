<emphasis box="[354,443,836,861]" italics="true" pageId="9">Cedrela</emphasis>
</taxonomicName>
and 
<taxonomicName box="[497,566,837,860]" class="Magnoliopsida" family="Meliaceae" genus="Toona" kingdom="Plantae" order="Sapindales" pageId="9" phylum="Tracheophyta" rank="genus">
<emphasis box="[497,566,837,860]" italics="true" pageId="9">Toona</emphasis>
</taxonomicName>
(
<taxonomicName box="[581,700,836,861]" class="Magnoliopsida" family="Meliaceae" kingdom="Plantae" order="Sapindales" pageId="9" phylum="Tracheophyta" rank="family">Meliaceae</taxonomicName>
) (Burckhardt 
<emphasis box="[856,912,836,861]" italics="true" pageId="9">et al.</emphasis>
