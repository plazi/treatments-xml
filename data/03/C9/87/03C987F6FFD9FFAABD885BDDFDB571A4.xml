<taxonomicName authority="(Johannsen, 1912)" authorityName="Johannsen" authorityYear="1912" baseAuthorityName="Johannsen" baseAuthorityYear="1912" box="[1120,1436,1646,1671]" class="Insecta" family="Sciaridae" genus="Pnyxia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="414" phylum="Arthropoda" rank="species" species="hartii">
