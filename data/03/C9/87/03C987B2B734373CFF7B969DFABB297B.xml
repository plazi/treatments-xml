<number box="[781,836,351,377]" pageId="18" pageNumber="19" value="1.46">1.46</number>
(
<number box="[855,983,351,377]" pageId="18" pageNumber="19" value="1.465" valueMax="1.57" valueMin="1.36">1.36–1.57</number>
