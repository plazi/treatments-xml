<emphasis box="[340,479,437,461]" italics="true" pageId="6" pageNumber="285">Scrophularia</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName box="[530,760,437,461]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="285" phylum="Tracheophyta" rank="species" species="ambiguum">
<emphasis box="[530,760,437,461]" italics="true" pageId="6" pageNumber="285">Trifolium ambiguum</emphasis>
