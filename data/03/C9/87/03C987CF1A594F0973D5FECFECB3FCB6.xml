<collectingRegion box="[554,655,584,609]" country="Israel" name="Southern" pageId="21">Southern</collectingRegion>
<collectingCountry box="[667,824,584,609]" name="United States of America" pageId="21">United States</collectingCountry>
, 
<taxonomicName authority="(Ashmead)" authorityName="Ashmead" baseAuthorityName="Ashmead" baseAuthorityYear="1881" box="[841,1248,584,609]" class="Insecta" family="Triozidae" genus="Baeoalitriozus" kingdom="Animalia" order="Hemiptera" pageId="21" phylum="Arthropoda" rank="species" species="diospyri">
<emphasis box="[841,1108,584,609]" italics="true" pageId="21">Baeoalitriozus diospyri</emphasis>
