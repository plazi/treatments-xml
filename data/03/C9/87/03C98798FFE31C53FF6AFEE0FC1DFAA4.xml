<number box="[1159,1215,1045,1071]" pageId="7" pageNumber="64" value="0.65">0.65</number>
, III 
<number box="[1266,1322,1045,1071]" pageId="7" pageNumber="64" value="0.6">0.60</number>
, IV 
<number box="[1375,1431,1045,1071]" pageId="7" pageNumber="64" value="0.95">0.95</number>
; metatarsus + tarsus: I 
<number box="[410,465,1082,1108]" pageId="7" pageNumber="64" value="0.55">0.55</number>
