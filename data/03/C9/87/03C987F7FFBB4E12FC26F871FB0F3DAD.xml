</taxonomicName>
, 
<taxonomicName authority="(W. T. Blanford, 1881)" baseAuthorityName="W. T. Blanford" baseAuthorityYear="1881" box="[378,770,1981,2005]" class="Gastropoda" family="Diapheridae" genus="Platylennea" kingdom="Animalia" order="Stylommatophora" pageId="15" pageNumber="697" phylum="Mollusca" rank="species" species="macrodon">
