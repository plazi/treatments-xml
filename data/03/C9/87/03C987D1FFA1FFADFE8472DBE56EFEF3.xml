<number box="[1141,1205,1516,1541]" pageId="1" pageNumber="2" value="1997.0">1997</number>
: 
<number box="[1209,1239,1516,1541]" pageId="1" pageNumber="2" value="31.0">31</number>
)
</bibRefCitation>
stated 
<taxonomicName box="[265,535,1557,1582]" class="Magnoliopsida" family="Fabaceae" genus="Pueraria" kingdom="Plantae" order="Fabales" pageId="1" pageNumber="2" phylum="Tracheophyta" rank="species" species="thunbergiana">
