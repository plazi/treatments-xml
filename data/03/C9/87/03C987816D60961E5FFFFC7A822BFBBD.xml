<collectingCountry box="[359,426,1052,1077]" name="China" pageId="12" pageNumber="313">China</collectingCountry>
(
<collectingRegion box="[440,529,1052,1077]" country="China" name="Yunnan" pageId="12" pageNumber="313">Yunnan</collectingRegion>
) (
<figureCitation box="[552,627,1052,1077]" captionStart="FIGURE 46" captionStartId="12.[151,250,2002,2024]" captionTargetBox="[157,1424,1215,1977]" captionTargetId="figure-461@12.[150,1436,1204,1981]" captionTargetPageId="12" captionText="FIGURE 46. Geographical distribution of Baptista of the world." figureDoi="http://doi.org/10.5281/zenodo.4918955" httpUri="https://zenodo.org/record/4918955/files/figure.png" pageId="12" pageNumber="313">Fig 46</figureCitation>
)
