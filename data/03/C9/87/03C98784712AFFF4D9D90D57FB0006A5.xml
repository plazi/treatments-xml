<taxonomicName baseAuthorityName="Gravenhorst" baseAuthorityYear="1820" box="[383,658,922,948]" class="Insecta" family="Ichneumonidae" genus="Cylloceria" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="488" phylum="Arthropoda" rank="species" species="melancholica">
