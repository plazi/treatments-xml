<taxonomicName authority="in Mt. Laoban" authorityName="in Mt. Laoban" box="[1028,1404,1261,1285]" class="Magnoliopsida" family="Platanaceae" genus="Platanus" kingdom="Plantae" order="Proteales" pageId="3" pageNumber="192" phylum="Tracheophyta" rank="species" species="orientalis">
