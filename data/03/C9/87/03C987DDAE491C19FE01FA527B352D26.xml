<taxonomicName baseAuthorityName="Kramer" baseAuthorityYear="1882" box="[1071,1274,830,852]" class="Arachnida" family="Trematuridae" genus="Trematurella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="23" pageNumber="705" phylum="Arthropoda" rank="species" species="elegans">
