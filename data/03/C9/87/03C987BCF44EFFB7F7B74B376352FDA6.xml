<subSubSection pageId="4" pageNumber="405" type="description">
<paragraph blockId="4.[151,1437,1854,2023]" pageId="4" pageNumber="405">
<emphasis bold="true" box="[199,374,1854,1879]" pageId="4" pageNumber="405">Measurements</emphasis>
