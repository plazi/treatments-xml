<number box="[757,799,1319,1344]" pageId="3" pageNumber="296" value="615.0">615</number>
(
<quantity box="[816,946,1319,1344]" metricMagnitude="1" metricUnit="m" metricValue="1.6002000000000003" metricValueMax="1.6891000000000003" metricValueMin="1.5112999999999999" pageId="3" pageNumber="296" unit="in" value="630.0" valueMax="665.0" valueMin="595.0">595–665 in</quantity>
<number box="[955,969,1319,1344]" pageId="3" pageNumber="296" value="5.0">5</number>
<typeStatus box="[978,1090,1319,1343]" pageId="3" pageNumber="296">paratypes</typeStatus>
