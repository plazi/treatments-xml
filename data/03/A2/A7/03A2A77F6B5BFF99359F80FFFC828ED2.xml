<collectingCountry box="[350,419,317,341]" name="China" pageId="11" pageNumber="936">China</collectingCountry>
Seas, 
<collectingRegion box="[505,593,317,341]" country="China" name="Hainan" pageId="11" pageNumber="936">Hainan</collectingRegion>
, 
<collectingCountry box="[612,758,317,341]" name="Hong Kong" pageId="11" pageNumber="936">Hong Kong</collectingCountry>
, 
<collectingCountry box="[777,865,317,341]" name="Taiwan" pageId="11" pageNumber="936">Taiwan</collectingCountry>
, Fujian, Zhenjiang, Yellow Sea, Shandong to 
<collectingRegion box="[444,563,349,373]" country="Japan" name="Hokkaido" pageId="11" pageNumber="936">Hokkaido</collectingRegion>
, 
<collectingCountry box="[575,646,349,373]" name="Japan" pageId="11" pageNumber="936">Japan</collectingCountry>
