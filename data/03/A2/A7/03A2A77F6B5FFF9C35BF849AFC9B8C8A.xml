<collectingCountry box="[495,565,885,909]" name="Japan" pageId="14" pageNumber="939">Japan</collectingCountry>
, 
<collectingCountry box="[585,657,885,909]" name="South Korea" pageId="14" pageNumber="939">Korea</collectingCountry>
