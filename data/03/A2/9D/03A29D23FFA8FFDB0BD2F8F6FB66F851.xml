<paragraph blockId="4.[151,783,1844,1870]" box="[151,783,1844,1870]" pageId="4" pageNumber="255">
<heading bold="true" box="[151,783,1844,1870]" fontSize="11" level="1" pageId="4" pageNumber="255" reason="1">
<emphasis bold="true" box="[151,783,1844,1870]" pageId="4" pageNumber="255">
Genus 
<taxonomicName authority="Xu, Tang &amp; Skelley" authorityName="Xu, Tang &amp; Skelley" box="[238,640,1844,1870]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="4" pageNumber="255" phylum="Arthropoda" rank="genus">
