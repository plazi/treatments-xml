<quantity box="[355,502,1235,1261]" metricMagnitude="-3" metricUnit="m" metricValue="6.699999999999999" metricValueMax="6.8" metricValueMin="6.6" pageId="11" pageNumber="60" unit="mm" value="6.699999999999999" valueMax="6.8" valueMin="6.6">6.6−6.8 mm</quantity>
