<subSubSection box="[201,1303,1539,1564]" pageId="5" pageNumber="56" type="etymology">
<paragraph blockId="5.[151,1437,151,1564]" box="[201,1303,1539,1564]" pageId="5" pageNumber="56">
<emphasis bold="true" box="[201,336,1540,1564]" pageId="5" pageNumber="56">Etymology.</emphasis>
