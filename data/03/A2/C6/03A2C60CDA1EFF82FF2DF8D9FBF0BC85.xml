<location LSID="urn:lsid:plazi:treatment:03A2C60CDA1EFF82FF2DF8D9FBF0BC85:8ED421C1DA1CFF82FAA4FBCCFA72BD76" box="[1310,1429,1063,1087]" country="Mexico" municipality="Los Tuxtlas" name="Bonaparte" pageId="7" pageNumber="186" stateProvince="Veracruz">Bonaparte</location>
) (
<taxonomicName box="[158,321,1099,1124]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="186" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
: 
<taxonomicName authorityName="Cabanis" authorityYear="1847" box="[332,464,1099,1124]" class="Aves" family="Thraupidae" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="186" phylum="Chordata" rank="family">Thraupidae</taxonomicName>
), 
<collectingCountry box="[482,600,1099,1124]" name="Mexico" pageId="7" pageNumber="186">
<emphasis bold="true" box="[482,600,1099,1124]" pageId="7" pageNumber="186">MEXICO</emphasis>
</collectingCountry>
: 
<collectingRegion box="[615,718,1100,1123]" country="Mexico" name="Veracruz de Ignacio de la Llave" pageId="7" pageNumber="186">Veracruz</collectingRegion>
, 
<collectingMunicipality box="[728,863,1099,1124]" pageId="7" pageNumber="186">Los Tuxtlas</collectingMunicipality>
, 
<collectingDate box="[873,1011,1099,1124]" pageId="7" pageNumber="186" value="2008-05-06">
