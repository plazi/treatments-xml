</taxonomicName>
and 
<taxonomicName authority="Selys, 1876" authorityName="Selys" authorityYear="1876" box="[767,1031,637,664]" class="Insecta" family="Coenagrionidae" genus="Pseudagrion" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="90" pageNumber="535" phylum="Arthropoda" rank="species" species="nubicum">
