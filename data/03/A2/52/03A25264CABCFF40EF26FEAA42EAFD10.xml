<caption pageId="167" pageNumber="612" targetBox="[158,1035,528,1427]" targetPageId="167">
<paragraph pageId="167" pageNumber="612">
<emphasis bold="true" box="[159,239,1468,1492]" pageId="167" pageNumber="612">Map 9.</emphasis>
