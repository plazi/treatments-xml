<emphasis box="[536,655,1468,1492]" italics="true" pageId="167" pageNumber="612">Eleuthemis</emphasis>
</taxonomicName>
: 
<emphasis box="[671,795,1468,1492]" italics="true" pageId="167" pageNumber="612">
<taxonomicName box="[671,795,1468,1492]" class="Insecta" family="Libellulidae" genus="Eleuthemis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="167" pageNumber="612" phylum="Arthropoda" rank="species" species="eogaster" status="sp. nov.">E. eogaster</taxonomicName>
</emphasis>
<taxonomicNameLabel box="[804,893,1468,1492]" pageId="167" pageNumber="612" rank="species">sp. nov.</taxonomicNameLabel>
, 
<emphasis box="[905,995,1468,1492]" italics="true" pageId="167" pageNumber="612">
<taxonomicName box="[905,995,1468,1492]" class="Insecta" family="Libellulidae" genus="Eleuthemis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="167" pageNumber="612" phylum="Arthropoda" rank="species" species="libera" status="sp. nov.">E. libera</taxonomicName>
</emphasis>
<taxonomicNameLabel pageId="167" pageNumber="612" rank="species">sp. nov.</taxonomicNameLabel>
, 
<emphasis box="[217,371,1497,1522]" italics="true" pageId="167" pageNumber="612">
<taxonomicName box="[217,371,1497,1522]" class="Insecta" family="Libellulidae" genus="Eleuthemis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="167" pageNumber="612" phylum="Arthropoda" rank="species" species="quadrigutta">E. quadrigutta</taxonomicName>
</emphasis>
, 
<emphasis box="[384,502,1497,1522]" italics="true" pageId="167" pageNumber="612">
<taxonomicName box="[384,502,1497,1522]" class="Insecta" family="Libellulidae" genus="Eleuthemis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="167" pageNumber="612" phylum="Arthropoda" rank="species" species="umbrina" status="sp. nov.">E. umbrina</taxonomicName>
</emphasis>
<taxonomicNameLabel box="[509,596,1497,1522]" pageId="167" pageNumber="612" rank="species">sp. nov.</taxonomicNameLabel>
