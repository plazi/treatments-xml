<subSubSection box="[159,755,441,504]" pageId="57" pageNumber="502" type="etymology">
<paragraph box="[159,292,441,467]" pageId="57" pageNumber="502">
<heading bold="true" box="[159,292,441,467]" fontSize="11" level="3" pageId="57" pageNumber="502" reason="6">
<emphasis bold="true" box="[159,292,441,467]" pageId="57" pageNumber="502">Etymology</emphasis>
</heading>
</paragraph>
<paragraph box="[159,755,477,504]" pageId="57" pageNumber="502">
