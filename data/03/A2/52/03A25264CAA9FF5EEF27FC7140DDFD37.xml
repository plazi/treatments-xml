, and 
<taxonomicName class="Insecta" family="Gomphidae" genus="Lestinogomphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="179" pageNumber="624" phylum="Arthropoda" rank="species" species="venustus">
