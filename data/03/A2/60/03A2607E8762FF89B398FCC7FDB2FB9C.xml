(
<bibRefCitation author="Attems" box="[262,411,1122,1146]" pageId="13" pageNumber="14" refString="Attems, C. (1929) Myriapoda. 1. Geophilomorpha. Das Tierreich 52. De Gruyter, Berlin, 388 pp." type="book" year="1929">
Attems 
<number box="[352,411,1122,1146]" pageId="13" pageNumber="14" value="1929.0">1929</number>
</bibRefCitation>
; 
<bibRefCitation author="Mercurio" box="[422,595,1122,1146]" pageId="13" pageNumber="14" refString="Mercurio, R. J. (2010) An annotated catalog of centipedes (Chilopoda) from the United States of America, Canada and Greenland (1758 - 2008). Xlibris, Milton Keynes, 560 pp." type="book chapter" year="2010">
Mercurio 
<number box="[534,595,1122,1146]" pageId="13" pageNumber="14" value="2010.0">2010</number>
</bibRefCitation>
).
</paragraph>
