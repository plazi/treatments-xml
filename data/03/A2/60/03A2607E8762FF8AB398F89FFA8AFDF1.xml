Attems 
<number box="[618,677,1970,1994]" pageId="13" pageNumber="14" value="1903.1929">1903</number>
</bibRefCitation>
, 
<bibRefCitation author="Attems" box="[690,749,1970,1994]" pageId="13" pageNumber="14" refString="Attems, C. (1929) Myriapoda. 1. Geophilomorpha. Das Tierreich 52. De Gruyter, Berlin, 388 pp." type="book" year="1929">1929</bibRefCitation>
; 
<bibRefCitation author="Verhoeff" box="[762,940,1970,1994]" pageId="13" pageNumber="14" refString="Verhoeff, K. W. (1938 a) Chilopoden-Studien zur Kenntnis der Epimorphen. Zoologische Jahrbucher, Abteilung fur Systematik, Okologie und Geographie der Tiere, 71, 339 - 388." type="journal article" year="1938" yearSuffix="a">
Verhoeff 
<number box="[869,927,1970,1994]" pageId="13" pageNumber="14" value="1938.0">1938</number>
a
</bibRefCitation>
, 
<bibRefCitation author="Verhoeff" box="[953,1022,1970,1994]" pageId="13" pageNumber="14" refString="Verhoeff, K. W. (1938 c) Uber einige amerikanische Myriapoden. Zoologischer Anzeiger, 122, 273 - 284." type="journal article" year="1938" yearSuffix="c">
