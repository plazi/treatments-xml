Verhoeff 
<number box="[639,695,689,713]" pageId="12" pageNumber="13" value="1935.0">1935</number>
</bibRefCitation>
(also sub 
<taxonomicName box="[819,1107,689,713]" class="Chilopoda" family="Linotaeniidae" genus="Scolioplanes" kingdom="Animalia" order="Geophilomorpha" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="mendelanus">
<emphasis box="[819,1107,689,713]" italics="true" pageId="12" pageNumber="13">Scolioplanes mendelanus</emphasis>
</taxonomicName>
and 
<taxonomicName class="Chilopoda" family="Linotaeniidae" genus="Scolioplanes" kingdom="Animalia" order="Geophilomorpha" pageId="12" pageNumber="13" phylum="Arthropoda" rank="subSpecies" species="engadinus" subSpecies="rodnaensis">
<emphasis italics="true" pageId="12" pageNumber="13">Scolioplanes engadinus rodnaensis</emphasis>
