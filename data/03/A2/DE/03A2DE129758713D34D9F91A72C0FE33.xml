<subSubSection box="[199,1072,404,429]" pageId="5" pageNumber="341" type="biology_ecology">
<paragraph blockId="5.[151,1436,151,464]" box="[199,1072,404,429]" pageId="5" pageNumber="341">
<emphasis bold="true" box="[199,295,404,429]" pageId="5" pageNumber="341">Biology.</emphasis>
