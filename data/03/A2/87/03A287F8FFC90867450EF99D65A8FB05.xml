<subSubSection pageId="38" pageNumber="39" type="etymology">
<paragraph blockId="38.[264,1323,1665,1891]" pageId="38" pageNumber="39">
<emphasis bold="true" box="[312,452,1825,1851]" pageId="38" pageNumber="39">Etymology.</emphasis>
