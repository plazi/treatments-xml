<quantity box="[1123,1199,1870,1894]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="6.0" metricValueMin="3.0" pageId="1" pageNumber="56" unit="mm" value="4.5" valueMax="6.0" valueMin="3.0">3-6 mm</quantity>
long in
</emphasis>
<taxonomicName authority="Meve &amp; Gâteblé &amp; Liede-Schumann, 2017" authorityName="Meve &amp; Gâteblé &amp; Liede-Schumann" authorityYear="2017" box="[1275,1449,1870,1894]" class="Magnoliopsida" family="Apocynaceae" genus="Marsdenia" kingdom="Plantae" order="Gentianales" pageId="1" pageNumber="56" phylum="Tracheophyta" rank="species" species="neocaledonica" status="sp. nov.">M. neocaledonica</taxonomicName>
, 
<emphasis box="[813,1114,1897,1921]" italics="true" pageId="1" pageNumber="56">
<taxonomicNameLabel box="[813,886,1897,1921]" pageId="1" pageNumber="56" rank="species">sp. nov.</taxonomicNameLabel>
), triangular corona lobes
</emphasis>
c. 
<emphasis italics="true" pageId="1" pageNumber="56">
<quantity box="[1141,1196,1897,1921]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="56" unit="mm" value="1.0">1 mm</quantity>
long (vs ovate, 
<quantity box="[1337,1411,1897,1921]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="1" pageNumber="56" unit="mm" value="0.5">0.5 mm</quantity>
long in
</emphasis>
<taxonomicName authority="Meve &amp; Gâteblé &amp; Liede-Schumann, 2017" authorityName="Meve &amp; Gâteblé &amp; Liede-Schumann" authorityYear="2017" box="[837,1010,1923,1947]" class="Magnoliopsida" family="Apocynaceae" genus="Marsdenia" kingdom="Plantae" order="Gentianales" pageId="1" pageNumber="56" phylum="Tracheophyta" rank="species" species="neocaledonica" status="sp. nov.">M. neocaledonica</taxonomicName>
, 
<emphasis italics="true" pageId="1" pageNumber="56">
<taxonomicNameLabel box="[1021,1094,1924,1947]" pageId="1" pageNumber="56" rank="species">sp. nov.</taxonomicNameLabel>
), anther appendages 
<quantity box="[1284,1358,1924,1948]" metricMagnitude="-1" metricUnit="m" metricValue="5.0" pageId="1" pageNumber="56" unit="mm" value="500.0">500 µm</quantity>
