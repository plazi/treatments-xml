<subSubSection pageId="5" pageNumber="6" type="etymology">
<paragraph blockId="5.[813,1437,525,1925]" pageId="5" pageNumber="6">
<emphasis box="[834,947,1516,1540]" italics="true" pageId="5" pageNumber="6">Etymology</emphasis>
