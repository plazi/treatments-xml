<taxonomicName authority="Smith, 1899" authorityName="Smith" authorityYear="1899" box="[264,608,1433,1457]" class="Insecta" family="Erebidae" genus="Heteranassa" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Lepidoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="minor" status="NEW SYNONYM">
