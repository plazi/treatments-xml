</paragraph>
</caption>
<paragraph blockId="23.[160,1156,1365,1701]" box="[160,333,1365,1390]" pageId="23" pageNumber="1965">
<emphasis bold="true" box="[160,333,1365,1390]" italics="true" pageId="23" pageNumber="1965">Measurements</emphasis>
</paragraph>
<paragraph blockId="23.[160,1156,1365,1701]" lastBlockId="24.[160,1156,142,201]" lastPageId="24" lastPageNumber="1966" pageId="23" pageNumber="1965">
BL = 3.00 mm in 
<typeStatus box="[380,482,1400,1424]" pageId="23" pageNumber="1965">holotype</typeStatus>
(range in 
<specimenCount box="[615,741,1400,1424]" pageId="23" pageNumber="1965" type="male">four males</specimenCount>
and 
<specimenCount box="[809,951,1400,1424]" pageId="23" pageNumber="1965" type="female">five females</specimenCount>
<typeStatus box="[963,1078,1400,1424]" pageId="23" pageNumber="1965">paratypes</typeStatus>
: 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="2.785" metricValueMax="2.97" metricValueMin="2.6" pageId="23" pageNumber="1965" unit="mm" value="2.785" valueMax="2.97" valueMin="2.6">2.60– 2.97 mm</quantity>
in males, 
<quantity box="[380,541,1434,1459]" metricMagnitude="-3" metricUnit="m" metricValue="3.465" metricValueMax="3.69" metricValueMin="3.24" pageId="23" pageNumber="1965" unit="mm" value="3.465" valueMax="3.69" valueMin="3.24">3.24–3.69 mm</quantity>
in females), ThL = 
<quantity box="[765,866,1435,1459]" metricMagnitude="-3" metricUnit="m" metricValue="1.47" pageId="23" pageNumber="1965" unit="mm" value="1.47">1.47 mm</quantity>
(
<quantity box="[880,1044,1434,1459]" metricMagnitude="-3" metricUnit="m" metricValue="1.35" metricValueMax="1.47" metricValueMin="1.23" pageId="23" pageNumber="1965" unit="mm" value="1.35" valueMax="1.47" valueMin="1.23">1.23–1.47 mm</quantity>
in males, 
<quantity box="[160,324,1469,1493]" metricMagnitude="-3" metricUnit="m" metricValue="1.515" metricValueMax="1.63" metricValueMin="1.4" pageId="23" pageNumber="1965" unit="mm" value="1.515" valueMax="1.63" valueMin="1.4">1.40–1.63 mm</quantity>
in females), WL = 
<quantity box="[550,653,1469,1493]" metricMagnitude="-3" metricUnit="m" metricValue="2.33" pageId="23" pageNumber="1965" unit="mm" value="2.33">2.33 mm</quantity>
(
<quantity box="[669,835,1469,1493]" metricMagnitude="-3" metricUnit="m" metricValue="2.165" metricValueMax="2.5" metricValueMin="1.83" pageId="23" pageNumber="1965" unit="mm" value="2.165" valueMax="2.5" valueMin="1.83">1.83–2.50 mm</quantity>
in males, 
<quantity box="[960,1124,1469,1493]" metricMagnitude="-3" metricUnit="m" metricValue="2.635" metricValueMax="2.8" metricValueMin="2.47" pageId="23" pageNumber="1965" unit="mm" value="2.635" valueMax="2.8" valueMin="2.47">2.47–2.80 mm</quantity>
in females), WW = 
<quantity box="[349,451,1504,1528]" metricMagnitude="-3" metricUnit="m" metricValue="1.07" pageId="23" pageNumber="1965" unit="mm" value="1.07">1.07 mm</quantity>
(
<quantity box="[465,630,1504,1528]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" metricValueMax="1.17" metricValueMin="0.93" pageId="23" pageNumber="1965" unit="mm" value="1.05" valueMax="1.17" valueMin="0.93">0.93–1.17 mm</quantity>
