<subSubSection pageId="0" pageNumber="159" type="etymology">
<paragraph blockId="0.[832,1436,1679,1767]" pageId="0" pageNumber="159">
<emphasis bold="true" box="[832,965,1680,1704]" pageId="0" pageNumber="159">Etymology:</emphasis>
