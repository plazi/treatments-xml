<collectingCountry box="[189,311,1047,1073]" name="Sweden" pageId="3" pageNumber="73">SWEDEN</collectingCountry>
• 
<specimenCount box="[362,442,1047,1074]" pageId="3" pageNumber="73" type="female">2 ♀♀</specimenCount>
; same collection data as for holotype; 
<specimenCode box="[986,1286,1047,1073]" pageId="3" pageNumber="73">NHRS-HEVA000011636</specimenCode>
, 
<specimenCode pageId="3" pageNumber="73">NHRS- HEVA000011637</specimenCode>
