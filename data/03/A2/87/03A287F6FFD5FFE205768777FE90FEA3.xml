in females), WW = 
<quantity box="[347,446,1165,1189]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="15" pageNumber="1957" unit="mm" value="1.2">1.20 mm</quantity>
(
<quantity box="[462,623,1165,1189]" metricMagnitude="-3" metricUnit="m" metricValue="1.2999999999999998" metricValueMax="1.4" metricValueMin="1.2" pageId="15" pageNumber="1957" unit="mm" value="1.2999999999999998" valueMax="1.4" valueMin="1.2">1.20–1.40 mm</quantity>
