(
<emphasis bold="true" box="[443,590,895,921]" italics="true" pageId="14" pageNumber="1956">Oxyphortica</emphasis>
) 
<emphasis bold="true" box="[606,690,895,921]" italics="true" pageId="14" pageNumber="1956">dainuo</emphasis>
