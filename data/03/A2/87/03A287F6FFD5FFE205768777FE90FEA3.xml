in males, 
<quantity box="[160,322,1130,1154]" metricMagnitude="-3" metricUnit="m" metricValue="1.665" metricValueMax="1.7" metricValueMin="1.63" pageId="15" pageNumber="1957" unit="mm" value="1.665" valueMax="1.7" valueMin="1.63">1.63–1.70 mm</quantity>
in females), WL = 
<quantity box="[551,653,1130,1154]" metricMagnitude="-3" metricUnit="m" metricValue="2.77" pageId="15" pageNumber="1957" unit="mm" value="2.77">2.77 mm</quantity>
(
<quantity box="[670,834,1130,1154]" metricMagnitude="-3" metricUnit="m" metricValue="3.065" metricValueMax="3.23" metricValueMin="2.9" pageId="15" pageNumber="1957" unit="mm" value="3.065" valueMax="3.23" valueMin="2.9">2.90–3.23 mm</quantity>
