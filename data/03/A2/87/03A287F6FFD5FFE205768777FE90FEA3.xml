<quantity box="[381,541,1096,1120]" metricMagnitude="-3" metricUnit="m" metricValue="3.94" metricValueMax="4.13" metricValueMin="3.75" pageId="15" pageNumber="1957" unit="mm" value="3.94" valueMax="4.13" valueMin="3.75">3.75–4.13 mm</quantity>
in females), ThL = 
<quantity box="[766,866,1096,1120]" metricMagnitude="-3" metricUnit="m" metricValue="1.57" pageId="15" pageNumber="1957" unit="mm" value="1.57">1.57 mm</quantity>
