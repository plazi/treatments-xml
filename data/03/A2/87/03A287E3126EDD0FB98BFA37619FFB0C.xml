(
<bibRefCitation author="Gertsch, W. J. &amp; Davis, L. I." box="[265,527,870,896]" pageId="2" pageNumber="288" pagination="1 - 22" refId="ref4076" refString="Gertsch, W. J. &amp; Davis, L. I. (1940) Report on a collection of spiders from Mexico. III. American Museum Novitates, 1069, 1 - 22." type="journal article" year="1940">Gertsch &amp; Davis, 1940</bibRefCitation>
)
</taxonomicName>
<emphasis bold="true" box="[542,671,870,896]" pageId="2" pageNumber="288">
<taxonomicNameLabel box="[542,671,870,896]" pageId="2" pageNumber="288" rank="species">comb. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="(Gertsch &amp; Davis, 1940)" authorityName="Chamé-Vázquez &amp; Jiménez" authorityYear="2022" baseAuthorityName="Gertsch &amp; Davis" baseAuthorityYear="1940" box="[685,1151,870,897]" class="Arachnida" family="Phrurolithidae" genus="Scotinella" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="288" phylum="Arthropoda" rank="species" species="approximatus" status="comb. nov.">
