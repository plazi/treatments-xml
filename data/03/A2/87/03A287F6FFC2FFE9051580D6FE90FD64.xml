<quantity box="[594,698,1482,1506]" metricMagnitude="-3" metricUnit="m" metricValue="1.08" pageId="26" pageNumber="1968" unit="mm" value="1.08">1.08 mm</quantity>
(
<quantity box="[717,882,1482,1506]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" metricValueMax="1.4" metricValueMin="1.1" pageId="26" pageNumber="1968" unit="mm" value="1.25" valueMax="1.4" valueMin="1.1">1.10–1.40 mm</quantity>
in males, 1.00– 
<quantity box="[1074,1155,1482,1506]" metricMagnitude="-2" metricUnit="m" metricValue="3.7338" pageId="26" pageNumber="1968" unit="in" value="1.47">1.47 in</quantity>
females), WL = 
<quantity box="[386,501,1517,1541]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" pageId="26" pageNumber="1968" unit="mm" value="2.15">2.15 mm</quantity>
(
<quantity box="[533,710,1516,1541]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.23" metricValueMin="1.87" pageId="26" pageNumber="1968" unit="mm" value="2.05" valueMax="2.23" valueMin="1.87">1.87–2.23 mm</quantity>
in males, 
<quantity box="[878,1030,1516,1541]" metricMagnitude="-2" metricUnit="m" metricValue="6.0325" metricValueMax="6.731" metricValueMin="5.334" pageId="26" pageNumber="1968" unit="in" value="2.375" valueMax="2.65" valueMin="2.1">2.10–2.65 in</quantity>
females); WW = 
<quantity box="[245,347,1551,1575]" metricMagnitude="-4" metricUnit="m" metricValue="8.4" pageId="26" pageNumber="1968" unit="mm" value="0.84">0.84 mm</quantity>
