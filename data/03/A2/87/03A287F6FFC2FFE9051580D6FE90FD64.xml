in 
<typeStatus box="[354,454,1447,1471]" pageId="26" pageNumber="1968">holotype</typeStatus>
(range in 
<specimenCount box="[570,686,1447,1471]" pageId="26" pageNumber="1968" type="male">five males</specimenCount>
, 
<specimenCount box="[697,836,1447,1471]" pageId="26" pageNumber="1968" type="female">four females</specimenCount>
<typeStatus box="[844,957,1447,1471]" pageId="26" pageNumber="1968">paratypes</typeStatus>
: 
<quantity box="[968,1126,1447,1471]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" metricValueMax="2.7" metricValueMin="2.3" pageId="26" pageNumber="1968" unit="mm" value="2.5" valueMax="2.7" valueMin="2.3">2.30–2.70 mm</quantity>
in males, 
<quantity box="[244,385,1482,1506]" metricMagnitude="-2" metricUnit="m" metricValue="6.5786" metricValueMax="6.9342" metricValueMin="6.223" pageId="26" pageNumber="1968" unit="in" value="2.59" valueMax="2.73" valueMin="2.45">2.45–2.73 in</quantity>
