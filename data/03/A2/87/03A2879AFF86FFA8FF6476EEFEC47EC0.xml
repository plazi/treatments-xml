<typeStatus box="[151,201,223,244]" pageId="1" pageNumber="196">Type</typeStatus>
species: 
<taxonomicName box="[294,448,222,245]" class="Insecta" family="Geometridae" genus="Mohacolora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="196" phylum="Arthropoda" rank="species" species="bezverkhovi" status="sp. n.">
