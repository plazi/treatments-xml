<paragraph blockId="13.[160,1156,1217,1588]" box="[160,333,1217,1242]" pageId="13" pageNumber="1955">
<emphasis bold="true" box="[160,333,1217,1242]" italics="true" pageId="13" pageNumber="1955">Measurements</emphasis>
</paragraph>
<paragraph blockId="13.[160,1156,1217,1588]" pageId="13" pageNumber="1955">
BL = 3.00 mm in 
<typeStatus box="[350,450,1252,1276]" pageId="13" pageNumber="1955">holotype</typeStatus>
(range in 
<specimenCount box="[565,677,1252,1276]" pageId="13" pageNumber="1955" type="male">five males</specimenCount>
and 
<specimenCount box="[734,867,1252,1276]" pageId="13" pageNumber="1955" type="female">five females</specimenCount>
<typeStatus box="[875,988,1252,1276]" pageId="13" pageNumber="1955">paratypes</typeStatus>
: 
<quantity box="[997,1155,1252,1276]" metricMagnitude="-3" metricUnit="m" metricValue="3.21" metricValueMax="3.69" metricValueMin="2.73" pageId="13" pageNumber="1955" unit="mm" value="3.21" valueMax="3.69" valueMin="2.73">2.73–3.69 mm</quantity>
in males, 
<quantity box="[277,440,1286,1311]" metricMagnitude="-3" metricUnit="m" metricValue="4.115" metricValueMax="4.36" metricValueMin="3.87" pageId="13" pageNumber="1955" unit="mm" value="4.115" valueMax="4.36" valueMin="3.87">3.87–4.36 mm</quantity>
in females), ThL = 
<quantity box="[679,782,1287,1311]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="13" pageNumber="1955" unit="mm" value="1.4">1.40 mm</quantity>
(
<quantity box="[801,966,1286,1311]" metricMagnitude="-3" metricUnit="m" metricValue="1.53" metricValueMax="1.73" metricValueMin="1.33" pageId="13" pageNumber="1955" unit="mm" value="1.53" valueMax="1.73" valueMin="1.33">1.33–1.73 mm</quantity>
in males, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="1.6" metricValueMax="1.78" metricValueMin="1.42" pageId="13" pageNumber="1955" unit="mm" value="1.6" valueMax="1.78" valueMin="1.42">1.42– 1.78 mm</quantity>
in females), WL = 
<quantity box="[470,569,1321,1345]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="13" pageNumber="1955" unit="mm" value="2.7">2.70 mm</quantity>
(
<quantity box="[583,744,1321,1345]" metricMagnitude="-3" metricUnit="m" metricValue="2.865" metricValueMax="3.1" metricValueMin="2.63" pageId="13" pageNumber="1955" unit="mm" value="2.865" valueMax="3.1" valueMin="2.63">2.63–3.10 mm</quantity>
in males, 
<quantity box="[860,1018,1321,1345]" metricMagnitude="-3" metricUnit="m" metricValue="3.445" metricValueMax="3.56" metricValueMin="3.33" pageId="13" pageNumber="1955" unit="mm" value="3.445" valueMax="3.56" valueMin="3.33">3.33–3.56 mm</quantity>
in females), WW = 
<quantity box="[237,336,1356,1380]" metricMagnitude="-3" metricUnit="m" metricValue="1.13" pageId="13" pageNumber="1955" unit="mm" value="1.13">1.13 mm</quantity>
(
<quantity box="[350,510,1356,1380]" metricMagnitude="-3" metricUnit="m" metricValue="1.265" metricValueMax="1.33" metricValueMin="1.2" pageId="13" pageNumber="1955" unit="mm" value="1.265" valueMax="1.33" valueMin="1.2">1.20–1.33 mm</quantity>
