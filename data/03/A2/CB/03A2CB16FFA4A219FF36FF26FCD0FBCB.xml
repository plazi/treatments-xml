<elevation box="[1271,1370,474,499]" metricMagnitude="1" metricUnit="m" metricValue="8.15" metricValueMax="9.8" metricValueMin="6.5" pageId="81" pageNumber="82" unit="m" value="81.5" valueMax="98.0" valueMin="65.0">65–98 m</elevation>
</quantity>
(135, 
<specimenCode box="[151,342,510,535]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="81" pageNumber="82" type="Museum">USNM 1490745</specimenCode>
)
</materialsCitation>
.
</paragraph>
</subSubSection>
