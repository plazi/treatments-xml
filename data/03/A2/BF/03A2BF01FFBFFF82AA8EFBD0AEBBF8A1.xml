<paragraph blockId="10.[151,1437,1283,1850]" pageId="10" pageNumber="550">
<emphasis bold="true" box="[151,266,1283,1310]" pageId="10" pageNumber="550">Diagnosis</emphasis>
: 
<quantity box="[282,446,1283,1310]" metricMagnitude="-3" metricUnit="m" metricValue="2.45" metricValueMax="2.5" metricValueMin="2.4" pageId="10" pageNumber="550" unit="mm" value="2.45" valueMax="2.5" valueMin="2.4">2.40–2.50 mm</quantity>
