<collectingCountry box="[214,303,1389,1415]" name="Bolivia" pageId="4" pageNumber="41">Bolivia</collectingCountry>
, 
<collectingCountry box="[315,424,1389,1415]" name="Paraguay" pageId="4" pageNumber="41">Paraguay</collectingCountry>
