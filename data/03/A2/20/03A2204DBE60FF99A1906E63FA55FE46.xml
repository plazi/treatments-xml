<paragraph blockId="6.[151,1437,152,498]" pageId="6" pageNumber="43">
<taxonomicName authority="Linsley &amp; Chemsak, 1976" authorityName="Linsley &amp; Chemsak" authorityYear="1976" box="[151,653,152,178]" class="Insecta" family="Cerambycidae" genus="Lycochoriolaus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="43" phylum="Arthropoda" rank="genus">
