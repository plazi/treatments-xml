<taxonomicName authority="Pretner, 1963" authorityName="Pretner" authorityYear="1963" box="[360,874,188,213]" class="Insecta" family="Leiodidae" genus="Redensekia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="subSpecies" species="likana" subSpecies="kosiniensis">
<emphasis box="[360,705,188,213]" italics="true" pageId="7" pageNumber="158">Redensekia likana kosiniensis</emphasis>
Pretner, 
<number box="[815,874,188,213]" pageId="7" pageNumber="158" value="1963.0">1963</number>
</taxonomicName>
, 
<taxonomicName authority="Schaum, 1862" authorityName="Schaum" authorityYear="1862" class="Insecta" family="Carabidae" genus="Typhlotrechus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="subSpecies" species="bilimekii" subSpecies="kiesenwetteri">
<emphasis box="[890,1318,188,213]" italics="true" pageId="7" pageNumber="158">Typhlotrechus bilimekii kiesenwetteri</emphasis>
(Schaum, 
<number box="[151,211,223,248]" pageId="7" pageNumber="158" value="1862.0">1862</number>
)
</taxonomicName>
, and 
<taxonomicName box="[277,357,224,247]" class="Insecta" family="Leiodidae" genus="Catops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="genus">
<emphasis box="[277,357,224,247]" italics="true" pageId="7" pageNumber="158">Catops</emphasis>
</taxonomicName>
sp. (all 
<taxonomicName box="[447,575,223,248]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
).
</paragraph>
