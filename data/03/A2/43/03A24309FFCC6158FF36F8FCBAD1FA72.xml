. 
<taxonomicName authorityName="Belizin" authorityYear="1952" box="[329,527,296,320]" class="Insecta" family="Figitidae" genus="Aspicera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="enormis">
