<emphasis bold="true" box="[656,670,1335,1352]" pageId="10" pageNumber="87">C</emphasis>
, inflorescence; 
<emphasis bold="true" box="[803,817,1335,1352]" pageId="10" pageNumber="87">D</emphasis>
