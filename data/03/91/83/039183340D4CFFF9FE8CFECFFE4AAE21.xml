<taxonomicName authority="Thomson, 1866" authorityName="Thomson" authorityYear="1866" box="[427,675,1634,1657]" class="Insecta" family="Chrysomelidae" genus="Hydrothassa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="fairmairei">
