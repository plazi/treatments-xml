<typeStatus box="[151,218,427,453]" pageId="15" pageNumber="16">Type</typeStatus>
:—
</emphasis>
<collectingCountry box="[256,480,428,454]" name="New Zealand" pageId="15" pageNumber="16">NEW ZEALAND</collectingCountry>
: 
<location LSID="urn:lsid:plazi:treatment:0391A5712119C67D78ADFC984EA4FADD:8EE742BC2119C67D79D7FDA34D1CFE24" box="[493,611,428,454]" country="New Zealand" municipality="Mt Philistine" name="Westland" pageId="15" pageNumber="16">Westland</location>
: 
<collectingMunicipality box="[625,791,428,454]" pageId="15" pageNumber="16">Mt Philistine</collectingMunicipality>
, 
<quantity box="[805,901,428,454]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="15" pageNumber="16" unit="m" value="1400.0">
<elevation box="[805,901,428,454]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="15" pageNumber="16" unit="m" value="1400.0">1400 m</elevation>
