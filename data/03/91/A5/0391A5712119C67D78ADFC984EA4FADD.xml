<materialsCitation ID-GBIF-Occurrence="3128888307" collectingDate="2007-11-30" collectionCode="CHR" country="New Zealand" elevation="1400" location="Westland" municipality="Mt Philistine" pageId="15" pageNumber="16" specimenCode="CHR610485" specimenCount="1" typeStatus="holotype">
