<taxonomicName authorityName="Smirnov" authorityYear="1992" box="[588,797,1795,1814]" class="Branchiopoda" family="Branchiopoda (awaiting allocation)" genus="Archeoxus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Branchiopoda (awaiting allocation)" pageId="1" pageNumber="254" phylum="Arthropoda" rank="species" species="mirabilis">
