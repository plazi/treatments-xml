<subSection pageId="6" pageNumber="259" type="multiple">
<subSubSection box="[144,440,513,535]" pageId="6" pageNumber="259" type="materials_examined">
<paragraph blockId="6.[144,440,513,535]" box="[144,440,513,535]" pageId="6" pageNumber="259">
<heading box="[144,440,513,535]" centered="true" fontSize="9" level="2" pageId="6" pageNumber="259" reason="8">
<emphasis box="[144,250,513,534]" italics="true" pageId="6" pageNumber="259">
<typeStatus box="[144,244,513,534]" pageId="6" pageNumber="259">Holotype</typeStatus>
:
</emphasis>
<collectionCode box="[260,306,514,535]" country="Russia" httpUri="http://grbio.org/cool/kjjc-ziy5" name="Paleontological Institute, Russian Academy of Sciences" pageId="6" pageNumber="259">PIN</collectionCode>
