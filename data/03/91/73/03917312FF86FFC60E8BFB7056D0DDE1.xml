<collectingRegion box="[894,992,1688,1711]" country="Argentina" name="Mendoza" pageId="1" pageNumber="66">Mendoza</collectingRegion>
/ 3-11-52] [
<collectingCountry box="[1131,1260,1688,1711]" name="Argentina" pageId="1" pageNumber="66">Argentinien</collectingCountry>
