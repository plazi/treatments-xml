<taxonomicName authority="K. H. Barnard 1916" authorityName="K. H. Barnard" authorityYear="1916" class="Malacostraca" family="Caprellidae" genus="Orthoprotella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="1040" phylum="Arthropoda" rank="species" species="mayeri">
<emphasis italics="true" pageId="18" pageNumber="1040">O. mayeri</emphasis>
K.H. 
<bibRefCitation author="Barnard KH" box="[294,446,316,340]" pageId="18" pageNumber="1040" pagination="26 - 28" refId="ref13961" refString="Barnard KH. 1916. Contributions to the Crustacean fauna of South Africa. 5. Amphipoda. Vol. 12 i Ann S Afr Mus. Cape Towni South Africa; p. 105 - 302 + pls 26 - 28." type="book chapter" year="1916">Barnard 1916</bibRefCitation>
</taxonomicName>
) and 
<collectingCountry box="[513,573,316,340]" name="Brazil" pageId="18" pageNumber="1040">Brazil</collectingCountry>
(
<taxonomicName authority="Quitetei 1975" authorityName="Quitetei" authorityYear="1975" box="[590,849,316,340]" class="Malacostraca" family="Caprellidae" genus="Orthoprotella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="1040" phylum="Arthropoda" rank="species" species="melloi">
<emphasis box="[590,688,316,340]" italics="true" pageId="18" pageNumber="1040">O. melloi</emphasis>
Quiteteı 1975
</taxonomicName>
).
</paragraph>
