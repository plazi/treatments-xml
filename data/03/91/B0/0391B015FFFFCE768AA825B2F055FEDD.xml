: 
<taxonomicName authority="Takeuchi et al. 2014" authorityName="Takeuchi" authorityYear="2014" class="Malacostraca" family="Caprellidae" genus="Protella" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="species" species="amamiensis">
<emphasis box="[893,1048,1669,1693]" italics="true" pageId="16" pageNumber="1038">P. amamiensis</emphasis>
Takeuchi et al. 2014
</taxonomicName>
ı 
<taxonomicName authority="Danai 1853" authorityName="Danai" authorityYear="1853" box="[289,525,1703,1727]" class="Malacostraca" family="Caprellidae" genus="Protella" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis box="[289,392,1703,1727]" italics="true" pageId="16" pageNumber="1038">P. gracilis</emphasis>
Danaı 1853
</taxonomicName>
ı 
<taxonomicName authority="Mayeri 1903" authorityName="Mayeri" authorityYear="1903" box="[538,774,1703,1727]" class="Malacostraca" family="Caprellidae" genus="Protella" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="species" species="similis">
<emphasis box="[538,630,1703,1727]" italics="true" pageId="16" pageNumber="1038">P. similis</emphasis>
Mayerı 1903
</taxonomicName>
and 
<taxonomicName authority="McCain and Grayi 1971" authorityName="McCain and Grayi" authorityYear="1971" class="Malacostraca" family="Caprellidae" genus="Protella" kingdom="Animalia" lastPageId="18" lastPageNumber="1040" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="species" species="trilobata">
<emphasis box="[831,951,1703,1727]" italics="true" pageId="16" pageNumber="1038">P. trilobata</emphasis>
McCain and Grayı 1971
