. While 
<taxonomicName authorityName="McCain and Grayi" authorityYear="1971" box="[303,424,142,166]" class="Malacostraca" family="Caprellidae" genus="Protella" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="1040" phylum="Arthropoda" rank="species" species="trilobata">
