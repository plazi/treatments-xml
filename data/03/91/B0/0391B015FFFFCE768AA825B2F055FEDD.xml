<taxonomicName authorityName="Mayeri" authorityYear="1903" box="[954,1100,212,236]" class="Malacostraca" family="Caprellidae" genus="Orthoprotella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="1040" phylum="Arthropoda" rank="genus">
<emphasis box="[954,1100,212,236]" italics="true" pageId="18" pageNumber="1040">Orthoprotella</emphasis>
</taxonomicName>
(see remarks of 
<taxonomicName authorityName="Mayeri" authorityYear="1903" box="[290,436,246,270]" class="Malacostraca" family="Caprellidae" genus="Orthoprotella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="1040" phylum="Arthropoda" rank="genus">
<emphasis box="[290,436,246,270]" italics="true" pageId="18" pageNumber="1040">Orthoprotella</emphasis>
</taxonomicName>
