<taxonomicName authorityName="Takeuchi and Lowryi" authorityYear="2007" box="[828,967,1495,1519]" class="Malacostraca" family="Caprellidae" genus="Orthoprotella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="species" species="berentsae">
<emphasis box="[828,967,1495,1519]" italics="true" pageId="16" pageNumber="1038">O. berentsae</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Guileri" authorityYear="1954" box="[1031,1150,1495,1519]" class="Malacostraca" family="Caprellidae" genus="Notoprotella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="species" species="gordoni">
<emphasis box="[1031,1150,1495,1519]" italics="true" pageId="16" pageNumber="1038">O. gordoni</emphasis>
