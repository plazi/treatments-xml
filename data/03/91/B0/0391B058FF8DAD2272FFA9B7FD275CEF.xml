<collectingCountry box="[554,606,985,1007]" name="India" pageId="33" pageNumber="2058">India</collectingCountry>
and 
<collectingCountry box="[655,757,986,1007]" name="Myanmar" pageId="33" pageNumber="2058">Myanmar</collectingCountry>
