<paragraph blockId="20.[264,1325,284,1790]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[312,523,724,750]" italics="true" pageId="20" pageNumber="21">
<collectingCountry box="[312,374,724,750]" name="Malta" pageId="20" pageNumber="21">Male</collectingCountry>
macroptera
</emphasis>
