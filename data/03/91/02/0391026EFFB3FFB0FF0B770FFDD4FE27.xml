<quantity box="[1139,1268,617,641]" metricMagnitude="-3" metricUnit="m" metricValue="2.45" metricValueMax="2.6" metricValueMin="2.3" pageId="26" pageNumber="227" unit="mm" value="2.45" valueMax="2.6" valueMin="2.3">2.3–2.6 mm</quantity>
