<quantity box="[602,729,1519,1544]" metricMagnitude="2" metricUnit="m" metricValue="6.7" metricValueMax="6.99" metricValueMin="6.409999999999999" pageId="22" pageNumber="223" unit="m" value="670.0" valueMax="699.0" valueMin="641.0">641–699 m</quantity>
(median 
<quantity box="[835,907,1519,1544]" metricMagnitude="2" metricUnit="m" metricValue="6.529999999999999" pageId="22" pageNumber="223" unit="m" value="653.0">653 m</quantity>
