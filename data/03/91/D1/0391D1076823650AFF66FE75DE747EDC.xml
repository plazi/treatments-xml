<taxonomicName baseAuthorityName="ZETTERSTEDT" baseAuthorityYear="1838" box="[579,686,537,558]" class="Insecta" family="Drosophilidae" genus="Chymomyza" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="480" phylum="Arthropoda" rank="species" species="costata">
