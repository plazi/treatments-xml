<paragraph blockId="10.[151,1059,638,719]" pageId="10" pageNumber="485">
<taxonomicName authorityName="VON ROSER" authorityYear="1840" box="[151,359,638,660]" class="Agaricomycetes" family="Psathyrellaceae" genus="Drosophila" kingdom="Fungi" order="Agaricales" pageId="10" pageNumber="485" phylum="Basidiomycota" rank="species" species="testacea">
