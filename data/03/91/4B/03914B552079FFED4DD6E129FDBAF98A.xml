<number box="[192,253,1577,1601]" pageId="4" pageNumber="25" value="1971.0">1971</number>
</date>
, TNA 
<number box="[329,374,1577,1601]" pageId="4" pageNumber="25" value="474.0">474</number>
, 
<collectionCode box="[385,443,1577,1601]" httpUri="http://grbio.org/cool/nwky-2b63" name="National Pusa Collection" pageId="4" pageNumber="25">NPC</collectionCode>
