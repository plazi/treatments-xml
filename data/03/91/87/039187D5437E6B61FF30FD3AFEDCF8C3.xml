5 genera. (
<taxonomicName box="[323,497,1653,1678]" class="Polypodiopsida" family="Athyriaceae" genus="Anisocampium" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">
<emphasis box="[323,497,1653,1678]" italics="true" pageId="9" pageNumber="16">Anisocampium</emphasis>
</taxonomicName>
, 
<emphasis box="[511,1026,1653,1678]" italics="true" pageId="9" pageNumber="16">
<taxonomicName box="[511,623,1653,1678]" class="Polypodiopsida" family="Athyriaceae" genus="Athyrium" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Athyrium</taxonomicName>
