, 
<taxonomicName box="[791,891,1653,1678]" class="Polypodiopsida" family="Athyriaceae" genus="Deparia" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Deparia</taxonomicName>
, 
<taxonomicName box="[904,1026,1653,1678]" class="Polypodiopsida" family="Athyriaceae" genus="Diplazium" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Diplazium</taxonomicName>
