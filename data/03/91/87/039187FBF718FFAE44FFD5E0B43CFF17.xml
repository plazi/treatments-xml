<paragraph blockId="6.[151,1436,1547,2003]" pageId="6" pageNumber="99">
<taxonomicName box="[151,453,1547,1571]" class="Insecta" family="Reduviidae" genus="Rhyparoclopius" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="6" pageNumber="99" phylum="Arthropoda" rank="species" species="desiccatus">
<emphasis box="[151,453,1547,1571]" italics="true" pageId="6" pageNumber="99">Rhyparoclopius desiccatus</emphasis>
</taxonomicName>
