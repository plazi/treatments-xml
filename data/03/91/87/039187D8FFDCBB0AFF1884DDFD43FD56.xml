<taxonomicName authority="Gompel, 2020" authorityName="Gompel" authorityYear="2020" class="Insecta" family="Aderidae" genus="Zarcosia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="177" phylum="Arthropoda" rank="species" species="lemairei" status="n. sp.">
