<taxonomicName authority="Ching" authorityName="Ching" box="[313,571,654,680]" class="Polypodiopsida" family="Rhachidosoridaceae" genus="Rhachidosorus" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">
