<number box="[867,912,1843,1867]" pageId="4" pageNumber="483" value="830.0">830</number>
. Head length (width across cheeks) 
<number box="[1330,1358,1842,1867]" pageId="4" pageNumber="483" value="45.0">45</number>
(
<number box="[1377,1422,1842,1867]" pageId="4" pageNumber="483" value="125.0">125</number>
), compound eye dorsal length (width) 
<number box="[571,599,1878,1903]" pageId="4" pageNumber="483" value="45.0">45</number>
(
<number box="[616,647,1878,1903]" pageId="4" pageNumber="483" value="35.0">35</number>
), distance between posterior ocelli 
<number box="[1050,1080,1879,1903]" pageId="4" pageNumber="483" value="37.0">37</number>
