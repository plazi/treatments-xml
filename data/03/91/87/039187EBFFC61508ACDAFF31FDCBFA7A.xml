<number box="[823,880,1405,1428]" pageId="50" pageNumber="51" value="4601.0">4601</number>
) from 
<taxonomicName authority="Columbus Park, Discovery Bay" authorityName="Columbus Park, Discovery Bay" class="Demospongiae" family="Halichondriidae" genus="Hymeniacidon" kingdom="Animalia" order="Halichondrida" pageId="50" pageNumber="51" phylum="Porifera" rank="species" species="caerulea">
<emphasis box="[960,1236,1405,1428]" italics="true" pageId="50" pageNumber="51">Hymeniacidon caerulea</emphasis>
