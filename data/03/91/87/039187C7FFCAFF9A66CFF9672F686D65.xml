<emphasis bold="true" box="[151,457,1688,1713]" pageId="7" pageNumber="8">
<taxonomicName box="[151,386,1688,1713]" class="Insecta" family="Oligotomidae" genus="Eosembia" kingdom="Animalia" order="Embioptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="paradorni" status="sp. n.">
<emphasis bold="true" box="[151,386,1688,1713]" italics="true" pageId="7" pageNumber="8">Eosembia paradorni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[393,457,1688,1712]" pageId="7" pageNumber="8" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
