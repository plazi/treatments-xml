, 
<taxonomicName box="[151,263,1923,1948]" class="Polypodiopsida" family="Tectariaceae" genus="Pteridrys" kingdom="Plantae" order="Polypodiales" pageId="10" pageNumber="17" phylum="Tracheophyta" rank="genus">Pteridrys</taxonomicName>
