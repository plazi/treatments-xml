<typeStatus box="[826,879,522,543]" pageId="29" pageNumber="342">Type</typeStatus>
species: 
<taxonomicName authorityName="O. F. Muller" authorityYear="1776" box="[1013,1085,522,543]" class="Bivalvia" family="Pectinidae" genus="Pecten" kingdom="Animalia" order="Pectinida" pageId="29" pageNumber="342" phylum="Mollusca" rank="genus">Pecten</taxonomicName>
schafhäutli
</emphasis>
Winkler, 1859 (= 
<emphasis box="[855,1015,552,574]" italics="true" pageId="29" pageNumber="342">
<taxonomicName authorityName="O. F. Muller" authorityYear="1776" box="[855,927,553,574]" class="Bivalvia" family="Pectinidae" genus="Pecten" kingdom="Animalia" order="Pectinida" pageId="29" pageNumber="342" phylum="Mollusca" rank="genus">Pecten</taxonomicName>
velatus
</emphasis>
Schafhäutl, 1852; non 
<emphasis box="[1281,1442,552,574]" italics="true" pageId="29" pageNumber="342">
<taxonomicName authorityName="O. F. Muller" authorityYear="1776" box="[1281,1353,553,574]" class="Bivalvia" family="Pectinidae" genus="Pecten" kingdom="Animalia" order="Pectinida" pageId="29" pageNumber="342" phylum="Mollusca" rank="genus">Pecten</taxonomicName>
