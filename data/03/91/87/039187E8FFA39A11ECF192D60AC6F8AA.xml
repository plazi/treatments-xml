<number box="[873,929,1951,1974]" pageId="1" pageNumber="590" value="2001.0">2001</number>
, Li 
<emphasis box="[972,1026,1950,1975]" italics="true" pageId="1" pageNumber="590">et al.</emphasis>
<number box="[1034,1090,1951,1974]" pageId="1" pageNumber="590" value="2013.0">2013</number>
a, Yang &amp; Yang 
<number box="[1266,1322,1951,1974]" pageId="1" pageNumber="590" value="1995.0">1995</number>
, Qin 
<emphasis box="[1381,1436,1950,1975]" italics="true" pageId="1" pageNumber="590">et al.</emphasis>
<number box="[151,207,1986,2009]" pageId="1" pageNumber="590" value="2013.0">2013</number>
, Cao 
