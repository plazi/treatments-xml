<taxonomicName box="[1123,1272,1132,1157]" class="Insecta" family="Braconidae" genus="Parapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="365" phylum="Arthropoda" rank="genus">
<emphasis box="[1123,1272,1132,1157]" italics="true" pageId="2" pageNumber="365">Parapanteles</emphasis>
