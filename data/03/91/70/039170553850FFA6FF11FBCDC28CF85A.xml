tibia length = 
<quantity box="[730,833,583,608]" metricMagnitude="-4" metricUnit="m" metricValue="6.1" pageId="2" pageNumber="365" unit="mm" value="0.61">0.61 mm</quantity>
; ovipositor sheath = 
<quantity box="[1067,1170,584,608]" metricMagnitude="-4" metricUnit="m" metricValue="2.9" pageId="2" pageNumber="365" unit="mm" value="0.29">0.29 mm</quantity>
