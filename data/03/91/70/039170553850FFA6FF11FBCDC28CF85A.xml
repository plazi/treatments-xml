<taxonomicName box="[1275,1422,1096,1121]" class="Magnoliopsida" family="Myrsinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Primulales" pageId="2" pageNumber="365" phylum="Magnoliophyta" rank="family">Myrsinaceae</taxonomicName>
