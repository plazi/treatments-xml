<taxonomicName box="[470,591,368,391]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="2" pageNumber="365" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
length = 
<quantity box="[698,801,368,392]" metricMagnitude="-4" metricUnit="m" metricValue="8.9" pageId="2" pageNumber="365" unit="mm" value="0.89">0.89 mm</quantity>
; 
<taxonomicName box="[812,930,368,391]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="2" pageNumber="365" phylum="Arthropoda" rank="genus">mesosoma</taxonomicName>
