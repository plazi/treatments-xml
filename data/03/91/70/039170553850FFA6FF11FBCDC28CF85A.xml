Stoll
</taxonomicName>
(
<taxonomicName box="[529,670,1096,1121]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="365" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName box="[683,808,1096,1121]" class="Insecta" family="Riodinidae" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="365" phylum="Arthropoda" rank="family">Riodinidae</taxonomicName>
) (Figs. 
<number box="[898,949,1097,1121]" pageId="2" pageNumber="365" value="10.5" valueMax="12.0" valueMin="9.0">9-12</number>
) on host plant 
<taxonomicName box="[1124,1217,1096,1121]" class="Magnoliopsida" family="Primulaceae" genus="Embelia" kingdom="Plantae" order="Ericales" pageId="2" pageNumber="365" phylum="Tracheophyta" rank="genus">
<emphasis box="[1124,1217,1096,1121]" italics="true" pageId="2" pageNumber="365">Embelia</emphasis>
</taxonomicName>
