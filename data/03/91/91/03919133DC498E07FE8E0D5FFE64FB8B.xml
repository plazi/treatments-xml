<collectingCountry box="[394,530,351,375]" name="Venezuela" pageId="10" pageNumber="686">VENEZUELA</collectingCountry>
, 
<collectingRegion box="[538,617,351,375]" country="Venezuela" name="Trujillo" pageId="10" pageNumber="686">Trujillo</collectingRegion>
, Cerro La Virgen, 
<quantity box="[812,897,351,375]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="10" pageNumber="686" unit="m" value="1100.0">1100 m</quantity>
, 
