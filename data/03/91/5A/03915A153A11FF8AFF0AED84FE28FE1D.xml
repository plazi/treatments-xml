, 
<collectingCountry box="[826,948,315,336]" name="Hong Kong" pageId="19" pageNumber="1152">Hong Kong</collectingCountry>
, 
<collectingCountry box="[959,1019,315,336]" name="Japan" pageId="19" pageNumber="1152">Japan</collectingCountry>
, 
<collectingCountry box="[1029,1081,315,336]" name="Laos" pageId="19" pageNumber="1152">Laos</collectingCountry>
, 
<collectingCountry box="[178,273,344,366]" name="Malaysia" pageId="19" pageNumber="1152">Malaysia</collectingCountry>
, 
<collectingCountry box="[287,392,344,366]" name="Singapore" pageId="19" pageNumber="1152">Singapore</collectingCountry>
, and 
<collectingCountry box="[453,555,344,366]" name="Indonesia" pageId="19" pageNumber="1152">Indonesia</collectingCountry>
. The record from 
<collectingCountry box="[752,812,344,366]" name="China" pageId="19" pageNumber="1152">China</collectingCountry>
in 
<bibRefCitation author="CAMERON M" box="[852,1033,344,366]" pageId="19" pageNumber="1152" refId="ref27790" refString="CAMERON M. (1931): The Fauna of British India including Ceylon and Burma. Coleoptera. Staphylinidae. Volume 2. - London, Taylor and Francis: viii + 257 pp." type="book" year="1931">CAMERON (1931)</bibRefCitation>
