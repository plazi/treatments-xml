<collectingCountry box="[359,426,1590,1615]" name="China" pageId="11" pageNumber="462">China</collectingCountry>
(
<collectingRegion box="[441,533,1591,1615]" country="China" name="Yunnan" pageId="11" pageNumber="462">Yunnan</collectingRegion>
