<taxonomicName authority="Golovatch, 2018" authorityName="Golovatch" authorityYear="2018" box="[210,474,1068,1089]" class="Diplopoda" family="Cryptodesmidae" genus="Carinocryptus" kingdom="Animalia" order="Polydesmida" pageId="6" pageNumber="367" phylum="Arthropoda" rank="species" species="semenyukae" status="new species">
