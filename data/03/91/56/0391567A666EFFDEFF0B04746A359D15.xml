<taxonomicName authorityName="Rafinesque" authorityYear="1810" box="[442,542,1187,1212]" class="Chondrichthyes" family="Rajidae" genus="Dipturus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="8" pageNumber="289" phylum="Chordata" rank="genus">
