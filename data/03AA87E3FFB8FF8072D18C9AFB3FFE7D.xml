<taxonomicName id="4C034D76FFB8FF87768E8D85FB6DF82D" box="[1224,1276,1983,2007]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="11" pageNumber="290" phylum="Tracheophyta" rank="genus">
