<number id="3DFE24F5FB22FEEFC86BBFBDFC9EF914" box="[851,886,1702,1726]" pageId="327" pageNumber="328" value="8.3">8.3</number>
x 
<number id="3DFE24F5FB22FEEFC8AEBFBDFC54F914" box="[918,956,1702,1726]" pageId="327" pageNumber="328" value="2.7">2.7</number>
