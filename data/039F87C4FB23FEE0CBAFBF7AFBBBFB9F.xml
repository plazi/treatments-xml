<collectingCountry id="F3217642FB22FEEFC990BFF6FCE3F8AC" box="[680,779,1773,1798]" name="Curaçao" pageId="327" pageNumber="328">Curaçao</collectingCountry>
). Taken from 
<quantity id="4CCE9B37FB22FEEFC89EBFF5FBC0F8AF" box="[934,1064,1774,1798]" metricMagnitude="2" metricUnit="m" metricValue="2.02" metricValueMax="2.7399999999999998" metricValueMin="1.3" pageId="327" pageNumber="328" unit="m" value="202.0" valueMax="274.0" valueMin="130.0">
