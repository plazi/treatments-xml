<taxonomicName id="4C16BABF954AFF8DFEF7F90C776EF8D1" authorityName="McCracken &amp; Egbert" authorityYear="1922" box="[901,1027,1672,1698]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="profusus">
