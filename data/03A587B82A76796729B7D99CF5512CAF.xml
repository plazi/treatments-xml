<collectingCountry id="F31B763E2A7679672BB9DF7DF51E2C43" box="[665,778,1081,1106]" name="Colombia" pageId="41" pageNumber="42">Colombia</collectingCountry>
, 
<collectingCountry id="F31B763E2A7679672A35DF7EF5852C43" box="[789,913,1081,1106]" name="Costa Rica" pageId="41" pageNumber="42">
<collectingRegion id="49C8F84C2A7679672A35DF7EF5412C43" box="[789,853,1082,1106]" country="New Zealand" name="West Coast" pageId="41" pageNumber="42">Costa</collectingRegion>
Rica
</collectingCountry>
, 
<collectingCountry id="F31B763E2A7679672ABCDF7DF5EF2C43" box="[924,1019,1081,1106]" name="Ecuador" pageId="41" pageNumber="42">Ecuador</collectingCountry>
, 
<collectingRegion id="49C8F84C2A7679672D26DF7DF2962C43" box="[1030,1154,1081,1106]" country="Guatemala" name="Guatemala" pageId="41" pageNumber="42">
<collectingCountry id="F31B763E2A7679672D26DF7DF2962C43" box="[1030,1154,1081,1106]" name="Guatemala" pageId="41" pageNumber="42">Guatemala</collectingCountry>
</collectingRegion>
, 
<collectingCountry id="F31B763E2A7679672DADDF7DF3172C43" box="[1165,1283,1081,1106]" name="Nicaragua" pageId="41" pageNumber="42">Nicaragua</collectingCountry>
, 
<collectingRegion id="49C8F84C2A7679672C2DDF7EF37D2C43" box="[1293,1385,1082,1106]" country="Panama" name="Panama" pageId="41" pageNumber="42">
<collectingCountry id="F31B763E2A7679672C2DDF7EF37D2C43" box="[1293,1385,1082,1106]" name="Panama" pageId="41" pageNumber="42">Panamá</collectingCountry>
</collectingRegion>
, and 
<collectingCountry id="F31B763E2A76796729B7DF1AF6DF2C64" box="[151,203,1118,1141]" name="Peru" pageId="41" pageNumber="42">Peru</collectingCountry>
(
<bibRefCitation id="EF9D4B5F2A76796729FCDF19F7932C67" box="[220,391,1117,1142]" pageId="41" pageNumber="42" refId="ref62351" refString="Metcalf, Z. P. (1963 c) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i - vi + 1 - 492." type="journal article">Metcalf 1963c</bibRefCitation>
; Duffels &amp; van der 
<bibRefCitation id="EF9D4B5F2A7679672B56DF1AF4E72C67" box="[630,755,1117,1142]" pageId="41" pageNumber="42" refId="ref60588" refString="Duffels, J. P. &amp; van der Laan, P. A. (1985) Catalogue of the Cicadoidea (Homoptera, Auchenorhyncha) 1956 - 1980. In: Series Entomologica 34. Dr. W. Junk Publishers, Dordrect, pp. 1 - 414." type="book chapter">Laan 1985</bibRefCitation>
; Sanborn &amp; Maes 2012; Maes 
<emphasis id="B978EABC2A7679672D44DF1AF2B42C67" box="[1124,1184,1117,1142]" italics="true" pageId="41" pageNumber="42">et al.</emphasis>
2012; 
<bibRefCitation id="EF9D4B5F2A7679672DD4DF19F3832C67" box="[1268,1431,1117,1142]" pageId="41" pageNumber="42" refId="ref63516" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book">Sanborn 2013</bibRefCitation>
; 2014). The species has been reported from 
<collectingRegion id="49C8F84C2A7679672B5DDFC5F4CE2C8B" box="[637,730,1153,1178]" country="Panama" name="Chiriqui" pageId="41" pageNumber="42">Chiriquí</collectingRegion>
in 
<collectingRegion id="49C8F84C2A7679672BDEDFC6F5432C88" box="[766,855,1154,1177]" country="Panama" name="Panama" pageId="41" pageNumber="42">
<collectingCountry id="F31B763E2A7679672BDEDFC6F5432C88" box="[766,855,1154,1177]" name="Panama" pageId="41" pageNumber="42">Panama</collectingCountry>
</collectingRegion>
(
<bibRefCitation id="EF9D4B5F2A7679672A46DFC5F5E62C8B" box="[870,1010,1153,1178]" pageId="41" pageNumber="42" refId="ref61201" refString="Jacobi, A. (1907) Homoptera Andina. Die Zikaden des kordillerengebietes von Sudamerika nach systematik und Verbreitung. I. Cicadidae. Abhandlungen und Berichte des Koniglichen Zoologischen und Anthropologisch-Enthnographischen Museums zu Dresden, 11, 1 - 28." type="journal article">Jacobi 1907</bibRefCitation>
). It was reported as being collected in 
<collectingRegion id="49C8F84C2A76796729B7DFE2F7062CAF" box="[151,274,1189,1214]" country="Panama" name="Panama" pageId="41" pageNumber="42">
<collectingCountry id="F31B763E2A76796729B7DFE2F6E42CAC" box="[151,240,1190,1213]" name="Panama" pageId="41" pageNumber="42">Panama</collectingCountry>
by
</collectingRegion>
<bibRefCitation id="EF9D4B5F2A7679672839DFE1F4312CAF" box="[281,549,1189,1214]" pageId="41" pageNumber="42" refId="ref65723" refString="Wolda, H. &amp; Ramos, J. A. (1992) Cicadas in Panama, their distribution, seasonality and diversity. In: Quintero, D. &amp; Aiello, A. (Eds.), The insects of Panama and Mesoamerica. Selected studies. Oxford University Press, New York, pp. 271 - 279." type="book chapter">Wolda &amp; Ramos (1992)</bibRefCitation>
but without locality data.
</paragraph>
