<taxonomicName id="4C3254C1FFB1FF9EFD6EF891FB853316" authority="Vinogradova, Riss et Spies, 2009" authorityName="Vinogradova, Riss et Spies" authorityYear="2009" box="[691,1115,1870,1892]" class="Insecta" family="Chironomidae" genus="Tanytarsus" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="288" phylum="Arthropoda" rank="species" species="kiche">
