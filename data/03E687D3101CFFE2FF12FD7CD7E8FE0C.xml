from &quot;? 
<collectingCountry id="F3587655101CFFE1FC08FC83D4F6FCD4" box="[909,1012,864,890]" name="Spain" pageId="5" pageNumber="6">Espagne</collectingCountry>
&quot; [=? 
<collectingCountry id="F3587655101CFFE1FBB8FC83D381FCD4" box="[1085,1155,864,890]" name="Spain" pageId="5" pageNumber="6">Spain</collectingCountry>
] (
<bibRefCitation id="EFDE4B34101CFFE1FB1FFC83D251FCD4" author="Pic" box="[1178,1363,864,890]" pageId="5" pageNumber="29" refString="Pic, M. (1893 b) [Essai d'une etude sur les Anthicus (lagenicolles) Cyclodinus Muls, continuation]. L'Echange, Revue Linneenne, 9, 125 - 128." type="journal article" year="1893" yearSuffix="b">
Pic 
<number id="3D8724E2101CFFE1FB4CFC83D205FCD4" box="[1225,1287,864,890]" pageId="5" pageNumber="6" value="1893.0">1893</number>
