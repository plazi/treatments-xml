<taxonomicName id="5974F0158FA8D65B80F5D828161D3DCB" LSID="http://zoobank.org/4229EC75-64B5-4066-8A2D-54334DC46503" authority="Tong &amp; Li" class="Arachnida" family="Oonopidae" genus="Kachinia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Kachinia mahmolae" order="Araneae" pageId="4" pageNumber="35" phylum="Arthropoda" rank="species" species="mahmolae">Kachinia mahmolae Tong &amp; Li</taxonomicName>
