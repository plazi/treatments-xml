<subSubSection id="1DB1655110336825FF19FAA3E1D92E18" lastPageId="12" lastPageNumber="203" pageId="11" pageNumber="202" type="distribution">
<paragraph id="551436DA10336822FF19FAA3E7E32E0D" blockId="11.[136,1451,2030,2093]" box="[189,1451,2030,2057]" pageId="11" pageNumber="202">
<emphasis id="67DFEAC810336822FF19FAA3E3BC2E0C" bold="true" box="[189,500,2030,2056]" pageId="11" pageNumber="202">Distribution and habitat:</emphasis>
— 
<taxonomicName id="92AB4D5910336822FDB4FAA3E1572E0C" authorityName="Y. S. Chen" authorityYear="2014" box="[528,799,2030,2056]" class="Magnoliopsida" family="Asteraceae" genus="Saussurea" kingdom="Plantae" order="Asterales" pageId="11" pageNumber="202" phylum="Tracheophyta" rank="species" species="lhozhagensis">
