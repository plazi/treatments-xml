<taxonomicName id="6183B088FFB5E02AFD1964F7FCE6FA29" box="[708,815,1493,1518]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="novercae">
