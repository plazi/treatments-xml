<taxonomicName id="6183B088FFB5E02AFD926316FCAAFD8A" box="[591,867,564,589]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="novercae">
