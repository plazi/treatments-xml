<quantity id="4CBE50BD6B6CFFD7FE2015D7FDE2FC45" box="[465,548,967,992]" metricMagnitude="2" metricUnit="m" metricValue="7.3152" pageId="15" unit="ft" value="2400.0">2400 ft</quantity>
