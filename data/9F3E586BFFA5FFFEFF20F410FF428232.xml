<geoCoordinate id="72A38FBAFFA0FFFEFD34F772FD498760" box="[643,765,332,357]" direction="west" orientation="longitude" pageId="4" pageNumber="83" precision="5" value="-49.45">49.4500W</geoCoordinate>
; 
<geoCoordinate id="72A38FBAFFA0FFFEFCBEF772FCDF8760" box="[777,875,332,357]" direction="south" orientation="latitude" pageId="4" pageNumber="83" precision="5" value="-4.6166">
