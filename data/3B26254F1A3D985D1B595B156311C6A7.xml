<taxonomicName id="F728641EBD2DD5E151BF01525E1C6A6F" LSID="http://zoobank.org/FC344590-1068-4C17-8B7A-AE6919AEB5D7" class="Arachnida" family="Archaeidae" genus="Eriauchenius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eriauchenius milajaneae" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="milajaneae">Eriauchenius milajaneae</taxonomicName>
