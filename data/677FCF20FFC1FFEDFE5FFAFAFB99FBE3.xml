, 
<collectingCountry id="97C13EA6FFC6FFEDFE82FBDAFE54FBE3" box="[382,490,1084,1107]" name="Colombia" pageId="28" pageNumber="303">Colombia</collectingCountry>
, 
<collectingCountry id="97C13EA6FFC6FFEDFE09FBDAFD98FBE3" box="[501,550,1084,1107]" name="Peru" pageId="28" pageNumber="303">Peru</collectingCountry>
