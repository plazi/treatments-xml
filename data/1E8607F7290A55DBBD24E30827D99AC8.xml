<taxonomicName id="A153ACECCE7E7CCE1E0C81ADAC57872F" class="Gastropoda" family="Orthalicidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="39" pageNumber="40" phylum="Mollusca" rank="family">Orthalicidae</taxonomicName>
, 
<taxonomicName id="CC93D5E5924526E1ABADF4FE1F394250" baseAuthorityName="Swainson" baseAuthorityYear="1823" class="Bacillariophyceae" family="Not" genus="Corona" higherTaxonomySource="CoL" kingdom="Chromista" lsidName="" order="Not" pageId="39" pageNumber="40" phylum="Ochrophyta" rank="species" species="perversa">
<emphasis id="7A47AC31A49865F7F593C3B87466E879" italics="true" pageId="39" pageNumber="40">Corona perversa</emphasis>
</taxonomicName>
(Swainson, 1821) (
<emphasis id="00970E89E31A5E37B1D512680EEDC07F" bold="true" pageId="39" pageNumber="40">comb. n., syn. n.</emphasis>
).
</paragraph>
</subSubSection>
