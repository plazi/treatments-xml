<taxonomicName id="4C00A6FAFFAAFFA04A51FEDB8670FE91" authority="Kandasamy, 1986" authorityName="Kandasamy" authorityYear="1986" box="[347,835,295,320]" class="Insecta" family="Triozidae" genus="Ceropsylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="longivenata">
