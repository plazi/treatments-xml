<taxonomicName id="939D4D55FFC9FF930DB6F8A36E92A81E" authority="Radhadevi &amp; Kurian, 1981" authorityName="Radhadevi &amp; Kurian" authorityYear="1981" class="Malacostraca" family="Bodotriidae" genus="Cyclaspis" kingdom="Animalia" order="Cumacea" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="mexicansis">
