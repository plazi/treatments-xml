<taxonomicName id="4C66CFACAA70FFAFFCA4F923FB11F960" authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" box="[856,1239,1750,1776]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="curvifoveatus">
<emphasis id="B912683DAA70FFAFFCA4F923FBDDF960" box="[856,1051,1751,1776]" italics="true" pageId="58" pageNumber="59">D. curvifoveatus</emphasis>
