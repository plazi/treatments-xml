<taxonomicName id="F1AFDF1D6E151C6EE6DB79CACE7453FA" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia standleyi" order="Zingiberales" pageId="142" pageNumber="143" phylum="Tracheophyta" rank="species" species="standleyi">Heliconia standleyi</taxonomicName>
J. F. Mador. (
<taxonomicName id="F2CC97A9525E4853A2B74704C158B8F4" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="142" pageNumber="143" rank="genus">Heliconiaceae</taxonomicName>
) and 
<taxonomicName id="502A54C7F056E6DD511762AB3710062C" class="Liliopsida" family="Arecaceae" genus="Elaeis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Elaeis guineensis" order="Arecales" pageId="142" pageNumber="143" phylum="Tracheophyta" rank="species" species="guineensis">Elaeis guineensis</taxonomicName>
Jacq. (
<taxonomicName id="CAFD2E02A7B81244FE876669A8F4BF99" genus="Arecaceae" lsidName="Arecaceae" pageId="142" pageNumber="143" rank="genus">Arecaceae</taxonomicName>
