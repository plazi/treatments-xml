<taxonomicName id="3A944D2D0510FD52FAB6F9A8FE80F932" authority="Rathbun, 1909" authorityName="Rathbun" authorityYear="1909" class="Malacostraca" family="Hymenosomatidae" genus="Lucascinus" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="502" phylum="Arthropoda" rank="species" species="coralicola" status="n. comb.">
