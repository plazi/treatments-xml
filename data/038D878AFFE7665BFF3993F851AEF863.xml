</taxonomicName>
, 
<taxonomicName id="4C244D1FFFE7665BFD2C94305292F8AB" box="[642,735,1872,1895]" class="Insecta" family="Cicadellidae" genus="Evinus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="422" phylum="Arthropoda" rank="species" species="sinuatus">
