</taxonomicName>
, 
<taxonomicName id="4C244D1FFFE7665BFD8C942F5220F8AB" box="[546,621,1871,1895]" class="Insecta" family="Cicadellidae" genus="Evinus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="422" phylum="Arthropoda" rank="species" species="longus">
