<taxonomicName id="46DD42D9B657FFF5FC7BFE490608FDD2" box="[896,1091,492,516]" class="Magnoliopsida" family="Anacardiaceae" genus="Mangifera" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="36" pageNumber="37" phylum="Tracheophyta" rank="species" species="indica">
