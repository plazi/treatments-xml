<taxonomicName id="DA9B2AC7BA92489EDD5F8D02F136DF5C" LSID="http://zoobank.org/E6F92137-F91C-479C-8B8D-C0055517E109" authority="Engel, Thomas &amp; Alqarni, 2017" authorityName="Engel, Thomas &amp; Alqarni" authorityYear="2017" class="Insecta" family="Braconidae" genus="Rhetinorhyssalites" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhetinorhyssalites" order="Hymenoptera" pageId="2" pageNumber="105" phylum="Arthropoda" rank="genus">Rhetinorhyssalites</taxonomicName>
