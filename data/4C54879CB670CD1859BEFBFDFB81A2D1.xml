<collectingCountry id="BCEA761AB670CD185C35F9EAFA6DA265" box="[1308,1432,1546,1572]" name="Guatemala" pageId="45" pageNumber="46">Guatemala</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD1859BEF9CEFF04A209" box="[151,241,1582,1608]" name="Guyana" pageId="45" pageNumber="46">Guyana</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD1859D6F9CEFE9AA209" box="[255,367,1582,1608]" name="Honduras" pageId="45" pageNumber="46">Honduras</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185855F9CEFE21A209" box="[380,468,1582,1608]" name="Mexico" pageId="45" pageNumber="46">Mexico</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD1858CBF9CEFDADA209" box="[482,600,1582,1608]" name="Nicaragua" pageId="45" pageNumber="46">Nicaragua</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185B4CF9CFFD34A208" box="[613,705,1583,1609]" name="Panama" pageId="45" pageNumber="46">Panama</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185BE7F9CFFCC2A208" box="[718,823,1583,1609]" name="Paraguay" pageId="45" pageNumber="46">Paraguay</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185A6CF9CFFC8EA208" box="[837,891,1583,1609]" name="Peru" pageId="45" pageNumber="46">Peru</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185AA1F9CEFB93A209" box="[904,1126,1582,1608]" name="Trinidad and Tobago" pageId="45" pageNumber="46">Trinidad &amp; Tobago</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185D5DF9CEFA6DA209" box="[1140,1432,1582,1608]" name="United States of America" pageId="45" pageNumber="46">United States of America</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD1859BEF9B3FF0FA22C" box="[151,250,1619,1645]" name="Uruguay" pageId="45" pageNumber="46">Uruguay</collectingCountry>
, 
<collectingCountry id="BCEA761AB670CD185821F9B2FE8BA22D" box="[264,382,1618,1644]" name="Venezuela" pageId="45" pageNumber="46">Venezuela</collectingCountry>
, and the West Indies (
<bibRefCitation id="A06C4B7BB670CD185B56F9B2FCDFA22D" author="Metcalf, Z. P." box="[639,810,1618,1644]" pageId="45" pageNumber="46" pagination="1 - 585" refId="ref106666" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
; 
<bibRefCitation id="A06C4B7BB670CD185A11F9B2FB64A22D" author="Duffels, J. P. &amp; van der Laan, P. A." box="[824,1169,1617,1645]" pageId="45" pageNumber="46" refId="ref104819" refString="Duffels, J. P. &amp; van der Laan, P. A. (1985) Catalogue of the Cicadoidea (Homoptera, Auchenorhyncha) 1956 - 1980. Series Entomologica 34. Dr. W. Junk Publishers, Dordrect, 414 pp." type="book" year="1985">Duffels &amp; van der Laan 1985</bibRefCitation>
