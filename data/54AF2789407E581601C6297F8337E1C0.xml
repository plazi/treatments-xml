<paragraph id="09EEF787AE32BC3754AB7A1ABF16074C" pageNumber="627">
<taxonomicName id="849BD59A1090AB11400A84B505CC9E26" class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageNumber="627" phylum="Arthropoda" rank="species" species="arnoldi-group">arnoldi-group</taxonomicName>
</paragraph>
