<taxonomicName id="4C345DE4FF971A2F4745DF9EFA7EFD62" authority="Biodiversity Inventory." authorityName="Biodiversity Inventory." box="[1073,1437,655,680]" kingdom="Plantae" order="Diptera" pageId="15" pageNumber="540" rank="order">Diptera Biodiversity Inventory.</taxonomicName>
<taxonomicName id="4C345DE4FF971A2F43E3DFA4FE83FD06" box="[151,352,692,716]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="540" phylum="Arthropoda" rank="species" species="zadbi">
