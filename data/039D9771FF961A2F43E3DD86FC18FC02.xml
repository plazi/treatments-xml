<taxonomicName id="4C345DE4FF971A2F4128DFA4FD01FD06" box="[604,738,693,716]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="540" phylum="Arthropoda" rank="genus">
