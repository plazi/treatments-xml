<taxonomicName id="3BCAE0CDFFBAFFA52EA10656FBAE5711" authorityName="Karny" authorityYear="1933" box="[675,1075,556,581]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="278" phylum="Arthropoda" rank="species" species="brevicauda" subGenus="Gymnaeta">
