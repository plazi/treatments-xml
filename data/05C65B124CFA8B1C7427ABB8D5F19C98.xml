<geoCoordinate id="6A67212FCE31ACD25F5A5765C5BFC67D" degrees="36" direction="north" minutes="55" orientation="latitude" precision="925" value="36.916668">36°55'N</geoCoordinate>
, 
<geoCoordinate id="7913D54A6EA54625B3946DFDBB0AEE67" degrees="7" direction="east" minutes="47" orientation="longitude" precision="925" value="7.7833333">7°47'E</geoCoordinate>
