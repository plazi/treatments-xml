<taxonomicName id="A6BC97666284CE03F1DB4686849BF73F" class="Malacostraca" family="Melitidae" genus="Melita" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Melita" order="Amphipoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
