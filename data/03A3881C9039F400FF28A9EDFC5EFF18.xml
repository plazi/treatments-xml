http://lsid.speciesfile.org/urn:lsid: 
<taxonomicName id="4C0A42899039F407FDB4A9D0FD19F8C4" box="[523,652,1776,1801]" class="Insecta" kingdom="Animalia" order="Orthoptera" pageId="27" pageNumber="328" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
