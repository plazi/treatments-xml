<collectingCountry id="D2A6761FFF92FF975D0DF99592EFF9B2" box="[1111,1172,1538,1562]" name="India" pageId="24" pageNumber="25">India</collectingCountry>
, 
<collectingCountry id="D2A6761FFF92FF975DFDF9959365F9B2" box="[1191,1310,1538,1562]" name="Sri Lanka" pageId="24" pageNumber="25">Sri Lanka</collectingCountry>
