<quantity id="AF079E88996BFFF1FC4DFCDACA82FC91" box="[924,991,831,852]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="14" pageNumber="15" unit="mm" value="13.0">13 mm</quantity>
