<paragraph id="8BD9294AFFB7EB67FF78FC0DFDCF07E4" blockId="24.[151,721,908,967]" box="[151,551,942,967]" pageId="24" pageNumber="125">
(
<figureCitation id="135D35CFFFB7EB67FF70FC0DFF0D07E4" box="[159,229,942,967]" captionStart="FIGURE 2" captionStartId="6.[151,250,1863,1885]" captionTargetBox="[151,1436,193,1841]" captionTargetId="figure@6.[151,1436,193,1842]" captionTargetPageId="6" captionText="FIGURE 2. Wing of male (Scale bars = 1 mm): A. Leucophenga ornata; B. L. saigusai; C. L. sculpta; D. L. todai; E. L. apunctata sp. nov.; F. L. arcuata sp. nov.; G. L. brevifoliacea sp. nov.; H. L. euryphylla sp. nov.; I. L. falcata sp. nov.; J. L. fuscinotata sp. nov." httpUri="https://zenodo.org/record/249342/files/figure.png" pageId="24" pageNumber="125">
Figs 
<number id="3DAE3B6DFFB7EB67FF38FC0CFF0D07E4" box="[215,229,943,967]" pageId="24" pageNumber="125" value="2.0">2</number>
</figureCitation>
F, 
<number id="3DAE3B6DFFB7EB67FEEEFC0DFEE707E4" box="[257,271,942,967]" pageId="24" pageNumber="125" value="5.0">5</number>
A, 
<number id="3DAE3B6DFFB7EB67FEDEFC0DFED707E4" box="[305,319,942,967]" pageId="24" pageNumber="125" value="5.0">5</number>
B, 
<number id="3DAE3B6DFFB7EB67FE8FFC0CFE8507E4" box="[352,365,943,967]" pageId="24" pageNumber="125" value="8.0">8</number>
F, 
<number id="3DAE3B6DFFB7EB67FE66FC0CFE4B07E4" box="[393,419,943,967]" pageId="24" pageNumber="125" value="11.0">11</number>
A, 
<number id="3DAE3B6DFFB7EB67FE29FC0CFE0907E4" box="[454,481,943,967]" pageId="24" pageNumber="125" value="11.0">11</number>
B, 
