<subSubSection id="C320D8FFFFCC760175F9A2DF52516DBE" pageId="8" pageNumber="343" type="etymology">
<paragraph id="8B858B74FFCC760175F9A2DF52516DBE" blockId="8.[707,1194,1203,1729]" pageId="8" pageNumber="343">
<emphasis id="B94E5766FFCC760175F9A2DF551B6D95" bold="true" box="[731,845,1203,1224]" pageId="8" pageNumber="343">Etymology.</emphasis>
