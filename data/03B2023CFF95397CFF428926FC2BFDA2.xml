</geoCoordinate>
), 
<quantity id="4CE31ECFFF94397CFD4B8E9BFC84FF2F" box="[670,837,192,218]" metricMagnitude="3" metricUnit="m" metricValue="1.815" metricValueMax="1.95" metricValueMin="1.68" pageId="11" pageNumber="216" unit="m" value="1815.0" valueMax="1950.0" valueMin="1680.0">1680–1950 m</quantity>
