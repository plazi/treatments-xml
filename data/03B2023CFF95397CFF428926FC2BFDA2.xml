° 
<number id="3DD3A10DFF94397CFE188E9BFE2FFF2F" box="[461,494,192,218]" pageId="11" pageNumber="216" value="40.0">40</number>
'N
</geoCoordinate>
, 
<geoCoordinate id="EE2FD5EDFF94397CFDC28E9BFD47FF2F" box="[535,646,192,218]" direction="east" orientation="longitude" pageId="11" pageNumber="216" precision="925" value="126.5">
<number id="3DD3A10DFF94397CFDC28E9BFD87FF2F" box="[535,582,192,218]" pageId="11" pageNumber="216" value="126.0">126</number>
