<number id="5F125850FF9B2853F97BA4B43DBFF932" box="[631,641,1713,1737]" pageId="5" pageNumber="56" value="1.0">1</number>
/ 
<number id="5F125850FF9B2853F980A4B43DA2F932" box="[652,668,1713,1737]" pageId="5" pageNumber="56" value="2.0">2</number>
