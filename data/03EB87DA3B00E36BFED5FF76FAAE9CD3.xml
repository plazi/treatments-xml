<taxonomicName id="4C424D4F3B07E36AFF04F838FEF99540" authority="Rahayu &amp; Ng, 2014" authorityName="Rahayu &amp; Ng" authorityYear="2014" box="[194,378,2033,2054]" class="Malacostraca" family="Hexapodidae" genus="Mariaplax" kingdom="Animalia" order="Decapoda" pageId="60" pageNumber="456" phylum="Arthropoda" rank="species" species="sinensis" status="new species">
