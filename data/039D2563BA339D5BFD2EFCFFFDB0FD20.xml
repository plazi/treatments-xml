<date id="FF8AB2B5BA329D5BFE70FEA1FDEAFE40" box="[415,597,369,392]" pageId="71" pageNumber="72" value="1908-03-19">
<collectingDate id="EFCE4B5DBA329D5BFE70FEA1FDEAFE40" box="[415,597,369,392]" pageId="71" pageNumber="72" value="1908-03-19">19 March 1908</collectingDate>
</date>
