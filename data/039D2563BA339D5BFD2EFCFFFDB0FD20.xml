<quantity id="4CCC3990BA329D5BFED3FEA1FE2CFE40" box="[316,403,369,392]" metricMagnitude="3" metricUnit="m" metricValue="1.2192" pageId="71" pageNumber="72" unit="ft" value="4000.0">
<elevation id="00197346BA329D5BFED3FEA1FE2CFE40" box="[316,403,369,392]" metricMagnitude="3" metricUnit="m" metricValue="1.2192" pageId="71" pageNumber="72" unit="ft" value="4000.0">4000 ft</elevation>
</quantity>
