<collectionCode id="ED250CB0BA329D5BFEB5FEC9FE0CFEF8" box="[346,435,281,304]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="71" pageNumber="72">AMNH</collectionCode>
