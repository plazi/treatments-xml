<materialsCitation id="3B5C9E28BA329D5BFD8EFEA1FDE4FE0B" collectingDate="1908-05-03" collectionCode="AMNH" collectorName="Harington." pageId="71" pageNumber="72" specimenCode="AMNH 586090" specimenCount="1" typeStatus="holotype">
and 
<specimenCode id="DB923C0EBA329D5BFF94FE5EFEFDFE6D" box="[123,322,398,421]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="71" pageNumber="72">AMNH 586090</specimenCode>
