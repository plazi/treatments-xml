<taxonomicName id="5ACCF1D9FFE7FF9DFD31FF4DFCCCD1DE" box="[730,848,153,179]" class="Insecta" family="Mimallonidae" genus="Cicinnus" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="430" phylum="Arthropoda" rank="species" species="orthane">
