<emphasis id="AFB85648FFE7FF9DFEEDFF69FD8ED1BA" box="[262,530,189,215]" italics="true" pageId="5" pageNumber="430">
<taxonomicName id="5ACCF1D9FFE7FF9DFEEDFF69FEE1D1BA" box="[262,381,189,215]" class="Insecta" family="Mimallonidae" genus="Cicinnus" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="430" phylum="Arthropoda" rank="species" species="orthane">C. orthane</taxonomicName>
