<accessionNumber id="78A3AB2B2A3F3F63FB02E2F4DF059AF2" box="[1250,1341,457,482]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/Z15016" pageId="3" pageNumber="593">
Z 
<number id="D13824EF2A3F3F63FB14E2F4DF059AF2" box="[1268,1341,457,482]" pageId="3" pageNumber="593" value="15016.0">15016</number>
</accessionNumber>
), 
<collectionCode id="01E1AE0D2A3F3F63FAB3E2F4DFA49AF1" box="[1363,1436,457,481]" httpUri="http://grbio.org/cool/nyq3-3g6s" name="National Institute of Water and Atmospheric Research" pageId="3" pageNumber="593">NIWA</collectionCode>
<number id="D13824EF2A3F3F63FF77E2D3DAD39916" box="[151,235,494,518]" pageId="3" pageNumber="593" value="104910.0">104910</number>
(
<number id="D13824EF2A3F3F63FF1AE2D3DB319916" box="[250,265,494,518]" pageId="3" pageNumber="593" value="2.0">2</number>
ovigerous females, 
<number id="D13824EF2A3F3F63FE0BE2D0D8289916" box="[491,528,493,518]" pageId="3" pageNumber="593" value="9.5">9.5</number>
, 
<quantity id="A0089B2D2A3F3F63FDFCE2D3D8BC9916" box="[540,644,494,518]" metricMagnitude="-2" metricUnit="m" metricValue="1.01" pageId="3" pageNumber="593" unit="mm" value="10.1">10.1 mm</quantity>
).
</paragraph>
