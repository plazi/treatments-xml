<figureCitation id="16F22A2F324AFFFDFE0AF9433A26DF3F" box="[439,532,1791,1815]" captionStart="Figure 1" captionStartId="1.[168,230,1069,1089]" captionTargetBox="[168,1419,160,1045]" captionTargetId="figure-443@1.[168,1419,160,1045]" captionTargetPageId="1" captionText="Figure 1. Location of collection sites in Montenegro, Romania, and Serbia of fungus gnats." pageId="8" pageNumber="541">Figure 1</figureCitation>
. 
<collectingCountry id="F6DE763A324AFFFDFD9EF8BC3A98DF30" box="[547,682,1792,1816]" name="Serbia" pageId="8" pageNumber="541">Montenegro</collectingCountry>
: Šavnik, Petnjica, dry beech forest, 
<quantity id="49319B4F324AFFFDFE64F89D3A02DF11" box="[473,560,1825,1849]" metricMagnitude="3" metricUnit="m" metricValue="1.057" pageId="8" pageNumber="541" unit="m" value="1057.0">1057 m</quantity>
a.s.l., 
<geoCoordinate id="EBFD506D324AFFFDFD3DF89E3B31DF11" box="[640,771,1826,1850]" degrees="42.9836" direction="north" orientation="latitude" pageId="8" pageNumber="541" precision="5" value="42.9836">42.9836° N</geoCoordinate>
, 
<geoCoordinate id="EBFD506D324AFFFDFF15F8F83901DF74" box="[168,307,1860,1884]" degrees="019.0733" direction="east" orientation="longitude" pageId="8" pageNumber="541" precision="5" value="19.0733">019.0733° E</geoCoordinate>
