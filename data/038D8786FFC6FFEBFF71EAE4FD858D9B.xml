<quantity id="4CDC9B75FFC6FFEBFDD7EC36FD488D0F" box="[561,649,1117,1141]" metricMagnitude="1" metricUnit="m" metricValue="2.75" metricValueMax="5.0" metricValueMin="0.5" pageId="57" pageNumber="58" unit="m" value="27.5" valueMax="50.0" valueMin="5.0">
5– 
<specimenCount id="9D22FD19FFC6FFEBFDAAEC36FD488D0F" box="[588,649,1117,1141]" pageId="57" pageNumber="58" type="generic">50 m</specimenCount>
</quantity>
