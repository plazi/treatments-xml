<taxonomicName id="9A150EDD1ABB9B177D26881F77512882" authority="" authorityName="" class="Gastropoda" family="Chromodorididae" genus="Risbecia" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Risbecia apolegma" order="Nudibranchia" pageId="37" pageNumber="38" phylum="Mollusca" rank="species" species="apolegma">Risbecia apolegma</taxonomicName>
