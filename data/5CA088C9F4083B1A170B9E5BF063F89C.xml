<taxonomicName id="5C17604094394164E9CE27FFB9CF517B" authority="" authorityName="" class="Gastropoda" family="Chromodorididae" genus="Risbecia" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Risbecia bullockii" order="Nudibranchia" pageId="37" pageNumber="38" phylum="Mollusca" rank="species" species="bullockii">Risbecia bullockii</taxonomicName>
