<taxonomicName id="4C03B8A0FFA4FF8EFEA4FC5386DDFC70" authorityName="De Candolle" authorityYear="1837" baseAuthorityName="Spreng." box="[269,387,1001,1021]" class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="26" pageNumber="1029" phylum="Tracheophyta" rank="species" species="selloi">Senecio selloi</taxonomicName>
