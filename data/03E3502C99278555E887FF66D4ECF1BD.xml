42. 
<taxonomicName id="4C4A9AB999278555E8D1FF67D711F0DD" authority="Meigen, 1824" authorityName="Meigen" authorityYear="1824" box="[193,576,152,179]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="atripes">
