<taxonomicName id="18D54D3EFF8FFB0CFD06FD9249B5875F" authority="Kuang &amp; Hong, 1991" authorityName="Kuang &amp; Hong" authorityYear="1991" box="[739,1272,564,590]" class="Arachnida" family="Diptilomiopidae" genus="Rhyncaphytoptus" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="celtis">
