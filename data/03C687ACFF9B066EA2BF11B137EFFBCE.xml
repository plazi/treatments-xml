<taxonomicName id="4C6F4D39FF98066EA64E115837E7FBCE" box="[1126,1255,1124,1148]" class="Arachnida" family="Oribatellidae" genus="Ferolocella" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="260" phylum="Arthropoda" rank="genus">
