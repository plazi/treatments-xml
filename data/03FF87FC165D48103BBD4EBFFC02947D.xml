<collectingRegion id="4992F808165D48103AE849DFFDA4945E" box="[473,560,1258,1284]" country="United States of America" name="Indiana" pageId="8" pageNumber="345">Indiana</collectingRegion>
and 
<collectingRegion id="4992F808165D4810395649DFFD4A945E" box="[615,734,1258,1284]" country="United States of America" name="Wisconsin" pageId="8" pageNumber="345">Wisconsin</collectingRegion>
(
<bibRefCitation id="EFC74B1B165D481039DD49DFFC1D945E" author="Parsons" box="[748,905,1258,1284]" firstAuthor="Parsons" pageId="8" pageNumber="345" pagination="121 - 278" refId="ref11921" refString="Parsons CT (1943) A revision of Nearctic Nitidulidae (Coleoptera). Bulletin of the Museum of Comparative Zoology 92: 121 - 278." type="journal article" year="1943">Parsons 1943</bibRefCitation>
