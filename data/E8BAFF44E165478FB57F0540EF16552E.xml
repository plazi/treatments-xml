<paragraph id="13E69A16CB49447357B66A29BBFCBBD3" pageId="25" pageNumber="78">
<taxonomicName id="B616CB87A3B134E2EC03CE3719A8F4FD" LSID="http://species-id.net/wiki/Calocosmus_nigripennis" authority="Chevrolat" class="Insecta" family="Cerambycidae" genus="Calocosmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calocosmus nigripennis" order="Coleoptera" pageId="25" pageNumber="78" phylum="Arthropoda" rank="species" species="nigripennis">
<pageBreakToken id="84210C7BCFC0B2724649086AE0ADE66B" pageId="25" pageNumber="78" start="start">Calocosmus</pageBreakToken>
nigripennis Chevrolat
</taxonomicName>
