<quantity id="4CEF9B25FF0CFF289C3BFAC8FECA3550" box="[1094,1163,1338,1365]" metricMagnitude="-1" metricUnit="m" metricValue="3.3" pageId="157" pageNumber="158" unit="cm" value="33.0">33 cm</quantity>
