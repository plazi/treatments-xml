<taxonomicName id="4C5578D8FFFFC179FA99FEC993B0FEEB" authorityName="Pettibone" authorityYear="1989" box="[1350,1435,357,378]" class="Polychaeta" family="Polynoidae" genus="Levensteiniella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="7" pageNumber="109" phylum="Annelida" rank="species" species="raisae">
