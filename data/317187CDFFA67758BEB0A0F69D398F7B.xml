<taxonomicName id="7ED84D58FFA67758BCDBA6A59AE08EB7" box="[764,910,1157,1182]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="74" pageNumber="75" phylum="Platyhelminthes" rank="genus">
