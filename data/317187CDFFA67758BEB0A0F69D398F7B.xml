</taxonomicName>
[
<taxonomicName id="7ED84D58FFA67758BD85A6A59D5A8EB7" box="[930,1076,1157,1182]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="74" pageNumber="75" phylum="Platyhelminthes" rank="genus">
