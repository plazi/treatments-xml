<taxonomicName id="4C5D4D75BD744654EBB1FBB7C911FB80" box="[704,820,1028,1051]" class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="genus">
<emphasis id="B929EAE4BD744654EBB1FBB7C911FB80" box="[704,820,1028,1051]" italics="true" pageId="17" pageNumber="18">Astragalus</emphasis>
</taxonomicName>
