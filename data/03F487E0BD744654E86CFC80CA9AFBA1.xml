<taxonomicName id="4C5D4D75BD744654EA1AFBB7C9F6FB80" box="[875,979,1028,1051]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="genus">
