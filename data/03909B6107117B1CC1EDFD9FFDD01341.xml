<collectingCountry id="F32E6AE7071E7B1CC01DFDD7FE501339" box="[359,426,513,539]" name="China" pageId="8" pageNumber="390">China</collectingCountry>
(
<collectingRegion id="49FDE495071E7B1CC0C3FDD7FDF41339" box="[441,526,513,539]" country="China" name="Hainan" pageId="8" pageNumber="390">Hainan</collectingRegion>
, 
<collectingRegion id="49FDE495071E7B1CC362FDD7FD891339" box="[536,627,513,539]" country="China" name="Yunnan" pageId="8" pageNumber="390">Yunnan</collectingRegion>
