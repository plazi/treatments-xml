, 
<collectorName id="268D32F4986A7076C47FF929FDC4F974" box="[314,534,1706,1733]" pageId="57" pageNumber="58">M. Lopez Figueiras</collectorName>
25163 (
<collectionCode id="ED69CFE7986A7076C737F928FD67F974" box="[626,693,1706,1732]" country="Venezuela" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15589" lsid="urn:lsid:biocol.org:col:15589" name="Universidad de Los Andes" pageId="57" pageNumber="58" type="Herbarium">MER</collectionCode>
