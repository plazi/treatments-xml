, Ƥ 
<quantity id="8E3B9B29FFCDFF8BB6D23E3EFCE8F9A6" box="[722,862,1601,1625]" metricMagnitude="-3" metricUnit="m" metricValue="3.65" metricValueMax="3.8" metricValueMin="3.5" pageId="6" pageNumber="136" unit="mm" value="3.65" valueMax="3.8" valueMin="3.5">3.5–3.8 mm</quantity>
; length of thorax, 
<number id="FF0B24EBFFCDFF8BB0343E3EFBFFF9A6" box="[1076,1097,1601,1625]" pageId="6" pageNumber="136" value="3.0">3</number>
<quantity id="8E3B9B29FFCDFF8BB0533E3EFB68F9A6" box="[1107,1246,1601,1625]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="6" pageNumber="136" unit="mm" value="1.6" valueMax="1.7" valueMin="1.5">1.5–1.7 mm</quantity>
, Ƥ 2.0– 
<quantity id="8E3B9B29FFCDFF8BB13E3E3EFA2EF9A6" box="[1342,1432,1601,1625]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="6" pageNumber="136" unit="mm" value="2.1">2.1 mm</quantity>
