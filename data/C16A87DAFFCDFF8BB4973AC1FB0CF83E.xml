<number id="FF0B24EBFFCDFF8BB5483E1AFEEBF982" box="[328,349,1637,1661]" pageId="6" pageNumber="136" value="3.0">3</number>
<quantity id="8E3B9B29FFCDFF8BB5643E1AFE5BF982" box="[356,493,1637,1661]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="2.8" metricValueMin="2.7" pageId="6" pageNumber="136" unit="mm" value="2.75" valueMax="2.8" valueMin="2.7">2.7–2.8 mm</quantity>
, Ƥ 
<quantity id="8E3B9B29FFCDFF8BB6143E1AFDD8F982" box="[532,622,1637,1661]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="6" pageNumber="136" unit="mm" value="3.4">3.4 mm</quantity>
