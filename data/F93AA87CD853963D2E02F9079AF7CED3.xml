<paragraph id="01596A7F61DD5BE67B06E7010F49DE0C" pageId="663" pageNumber="664">
<taxonomicName id="8D9D6E76F25739A21826639B3D199621" authorityName="Fabricius" authorityYear="1775" class="Hexapoda" family="Carabidae" genus="Carabus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="663" pageNumber="664" phylum="Arthropoda" rank="species" species="lineola">Carabus lineola</taxonomicName>
