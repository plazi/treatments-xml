<taxonomicName id="22497BB72D3943528DA3014CC0CE1BF0" class="Insecta" family="Eumenidae" genus="Polistes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polistes dawnae" order="Hymenoptera" pageId="2" pageNumber="47" phylum="Arthropoda" rank="species" species="dawnae">Polistes dawnae</taxonomicName>
