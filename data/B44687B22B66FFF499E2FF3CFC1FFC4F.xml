<taxonomicName id="FBEF4D272B64FFF39837F883FE45F89D" box="[322,411,1832,1854]" class="Insecta" family="Staphylinidae" genus="Anotylus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="genus">
