<emphasis id="7A9AEAD0FFE0FFCBFF07FCC165F8C8C7" bold="true" box="[151,187,870,896]" pageId="9" pageNumber="10">15.</emphasis>
<taxonomicName id="8FEE4D41FFE0FFCBFF57FCC167BAC8C6" authority="Schulze and Schlottke, 1929 b" authorityName="Schulze and Schlottke" authorityYear="1929" box="[199,761,870,897]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="arboricola">
