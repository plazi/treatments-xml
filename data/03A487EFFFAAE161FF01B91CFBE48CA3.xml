<taxonomicName id="4C0D4D7AFFA8E16EFEF0BAB0FDEA8AB9" box="[358,607,1216,1238]" class="Insecta" family="Ptychopteridae" genus="Ptychoptera" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="548" phylum="Arthropoda" rank="species" species="lushuiensis" status="sp. nov.">
