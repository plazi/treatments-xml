<taxonomicName id="07993B8FFFF3FFB3FE33FC60FD9AEEA9" authorityName="Casey" authorityYear="1911" box="[426,636,976,1002]" class="Insecta" family="Staphylinidae" genus="Myllaena" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="203" phylum="Arthropoda" rank="species" species="ludificans">
