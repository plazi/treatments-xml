<taxonomicName id="041C6EC95E67B9A7AB1CC315796BDA74" class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cacopsylla bidens" order="Hemiptera" pageId="4" pageNumber="173" phylum="Arthropoda" rank="species" species="bidens">Cacopsylla bidens</taxonomicName>
, from 
<taxonomicName id="3296483B15A0203285C536E723E13B66" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pyrus communis" order="Rosales" pageId="4" pageNumber="173" phylum="Tracheophyta" rank="species" species="communis">Pyrus communis</taxonomicName>
, 
<normalizedToken id="C698DFED9E0D0F8DF904C2A5F6CA3707" originalValue="Nemenikuće">Nemenikuce</normalizedToken>
