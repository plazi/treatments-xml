<paragraph id="74B97709FD28EEC4A40C5B05A5972BD9" pageId="4" pageNumber="173">
<taxonomicName id="37DE42A3DCCCD32F939D6932424D4AC4" class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cacopsylla affinis" order="Hemiptera" pageId="4" pageNumber="173" phylum="Arthropoda" rank="species" species="affinis">Cacopsylla affinis</taxonomicName>
