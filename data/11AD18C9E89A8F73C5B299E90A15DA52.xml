<taxonomicName id="79DC61CC239A25A382800D5E8225F000" class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cacopsylla melanoneura" order="Hemiptera" pageId="4" pageNumber="173" phylum="Arthropoda" rank="species" species="melanoneura">Cacopsylla melanoneura</taxonomicName>
