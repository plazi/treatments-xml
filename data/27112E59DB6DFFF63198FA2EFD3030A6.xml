<taxonomicName id="68B8E4CCDB6FFFF635B8FEBEFABE3236" box="[1207,1312,325,348]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="187" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
