<taxonomicName id="4C1E4D2D1D33FFBDFF21E106B509F9BF" authority="(Hubbard 1901)" baseAuthorityName="Hubbard" baseAuthorityYear="1901" box="[232,583,1549,1570]" class="Liliopsida" family="Asparagaceae" genus="Dasylirion" kingdom="Plantae" order="Asparagales" pageId="7" pageNumber="426" phylum="Tracheophyta" rank="species" species="wheeleri">
