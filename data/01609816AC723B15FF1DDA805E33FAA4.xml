., 
<date id="FD770FC0AC723B15FDB0DBDB5EFBFC13" box="[609,696,926,946]" pageId="62" pageNumber="455" value="1957-05">
<collectingDate id="ED33F628AC723B15FDB0DBDB5EFBFC13" box="[609,696,926,946]" pageId="62" pageNumber="455" value="1957-05">mai 1957</collectingDate>
