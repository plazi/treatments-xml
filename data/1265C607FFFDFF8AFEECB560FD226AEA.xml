<taxonomicName id="5DCC0C92FFFCFF8AFBE0B765FEC86A32" authority="Fabricius, 1792" authorityName="Fabricius" authorityYear="1792" class="Insecta" family="Curculionidae" genus="Larinus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="64" phylum="Arthropoda" rank="species" species="ursus">
