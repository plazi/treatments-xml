<taxonomicName id="44D94D57FFCBFA541BD5FF1CFC8DFF1A" box="[654,809,152,177]" class="Insecta" family="Aradidae" genus="Barcinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="bhoutanensis">
