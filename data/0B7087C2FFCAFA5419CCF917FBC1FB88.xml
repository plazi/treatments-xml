<taxonomicName id="44D94D57FFCBFA5418C2FB88FE5CFB88" authorityName="Usinger &amp; Matsuda" authorityYear="1959" box="[409,504,1036,1059]" class="Insecta" family="Aradidae" genus="Barcinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
