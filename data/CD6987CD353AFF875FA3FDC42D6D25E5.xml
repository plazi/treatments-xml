<collectingCountry id="3DD7764B353AFF875E53FB192EC025AD" box="[359,430,1179,1205]" name="China" pageId="1" pageNumber="266">China</collectingCountry>
: 
<collectingRegion id="8704F839353AFF875E8DFB192D6825AD" box="[441,518,1179,1205]" country="China" name="Hunan" pageId="1" pageNumber="266">Hunan</collectingRegion>
, 
