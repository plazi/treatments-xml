<taxonomicName id="E75623557BF423037386DB95D6E20E40" authority="Hübner, 1825" authorityName="Hübner" authorityYear="1825" class="Insecta" family="Eupterotidae" genus="Euchera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Euchera fractifasciata" order="Lepidoptera" pageId="11" pageNumber="130" phylum="Arthropoda" rank="species" species="fractifasciata">Euchera fractifasciata</taxonomicName>
