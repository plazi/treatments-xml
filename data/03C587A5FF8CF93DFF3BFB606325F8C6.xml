<taxonomicName id="4C6C4D30FF8CF93DFF3BFB0A64F8FAC9" authority="Melone &amp; Taviani, 1980" authorityName="Melone &amp; Taviani" authorityYear="1980" box="[151,653,1258,1280]" class="Gastropoda" family="Architectonicidae" genus="Architectonica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="72" pageNumber="73" phylum="Mollusca" rank="species" species="bannocki">
