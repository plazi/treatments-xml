<collectionCode id="42B61DCEFF3F83CFFB515AD5FB37FD36" box="[1173,1191,693,717]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="130" pageNumber="130" type="Herbarium">G</collectionCode>
