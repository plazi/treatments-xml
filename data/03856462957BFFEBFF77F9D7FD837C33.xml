<collectionCode id="ED3D4DB1957BFFD4FC3CF961FBF47BCF" box="[988,1070,1737,1760]" country="New Zealand" httpUri="http://grbio.org/cool/fg7h-t7tk" name="Museum of New Zealand Te Papa Tongarewa" pageId="31" pageNumber="232">NMNZ</collectionCode>
