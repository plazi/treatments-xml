<taxonomicName id="4C706AF3FFF1FFB2F2C11B77FC95F8FC" authority="(Millar &amp; Goodbody, 1974)" authorityName="Millar &amp; Goodbody" authorityYear="1974" baseAuthorityName="Millar &amp; Goodbody" baseAuthorityYear="1974" box="[151,768,1839,1865]" class="Ascidiacea" family="Ascidiidae" genus="Phallusia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enterogona" pageId="11" pageNumber="462" phylum="Chordata" rank="species" species="caguayensis">
