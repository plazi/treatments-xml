<collectionCode id="ED0DAE784F12155BFDDF7F4EFDF0F8CF" box="[537,571,1840,1866]" country="Russia" name="Laboratory of Palaeontology" pageId="37" pageNumber="38">LP</collectionCode>
39: height = 
<emphasis id="B968EAAF4F12155BFD137F4EFD2AF8CC" box="[725,737,1840,1865]" italics="true" pageId="37" pageNumber="38">c</emphasis>
