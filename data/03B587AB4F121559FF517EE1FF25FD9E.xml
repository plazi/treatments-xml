<collectionCode id="ED0DAE784F12155BFBFF7F4EFB90F8CF" box="[1081,1115,1840,1866]" country="Russia" name="Laboratory of Palaeontology" pageId="37" pageNumber="38">LP</collectionCode>
64: height = 
<emphasis id="B968EAAF4F12155BFB307F4EFAC9F8CC" box="[1270,1282,1840,1865]" italics="true" pageId="37" pageNumber="38">c</emphasis>
