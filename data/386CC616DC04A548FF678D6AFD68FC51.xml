<taxonomicName id="77C50C83DC05A546FED68F86FDBBF83B" authority="(Jenkin, 1908)" authorityName="Jenkin" authorityYear="1908" baseAuthorityName="Jenkin" baseAuthorityYear="1908" box="[294,629,1932,1954]" class="Calcarea" family="Heteropiidae" genus="Sycettusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosolenida" pageId="119" pageNumber="120" phylum="Porifera" rank="species" species="simplex">
