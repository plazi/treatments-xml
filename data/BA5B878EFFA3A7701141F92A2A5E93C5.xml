<taxonomicName id="F5F24D1BFFA3A7711141F92A285794F4" baseAuthorityName="Vanschuytbroeck" baseAuthorityYear="1957" box="[540,880,1678,1705]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="67" phylum="Arthropoda" rank="species" species="stuckenbergi">
<emphasis id="0086EA8AFFA3A7711141F92A285794F4" bold="true" box="[540,880,1678,1705]" italics="true" pageId="20" pageNumber="67">Amblypsilopus stuckenbergi</emphasis>
