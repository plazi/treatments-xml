; length of hind tibia: 
<quantity id="6902AA8EFFDFFFF93575DDEFBDF6FCB5" box="[1124,1269,749,775]" metricMagnitude="-3" metricUnit="m" metricValue="1.2999999999999998" metricValueMax="1.4" metricValueMin="1.2" pageId="14" pageNumber="67" unit="mm" value="1.2999999999999998" valueMax="1.4" valueMin="1.2">1.2–1.4 mm</quantity>
