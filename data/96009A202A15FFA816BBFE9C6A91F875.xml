<taxonomicName id="D9A950B52A15FFA8159CFA3A6A17FA34" box="[944,1078,1388,1413]" class="Aves" family="Fringillidae" kingdom="Animalia" order="Passeriformes" pageId="65" pageNumber="66" phylum="Chordata" rank="family">Fringillidae</taxonomicName>
