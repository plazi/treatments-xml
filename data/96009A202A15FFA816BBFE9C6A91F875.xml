<taxonomicName id="D9A950B52A15FFA8145BFA3A6D28FA34" box="[631,777,1388,1413]" class="Aves" family="Cardinalidae" kingdom="Animalia" order="Passeriformes" pageId="65" pageNumber="66" phylum="Chordata" rank="family">Cardinalidae</taxonomicName>
