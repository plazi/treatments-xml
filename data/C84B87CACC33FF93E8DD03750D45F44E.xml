<taxonomicName id="87E24D5FCC33FF90EA03057A0C4FF006" box="[585,848,1341,1366]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="37" pageNumber="38" phylum="Cnidaria" rank="species" species="nonscripta">
