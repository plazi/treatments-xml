<quantity id="871A9B39CC33FF90EA5A04050D74F10A" box="[528,619,1090,1114]" metricMagnitude="-1" metricUnit="m" metricValue="1.1" pageId="37" pageNumber="38" unit="mm" value="110.0">110 mm</quantity>
