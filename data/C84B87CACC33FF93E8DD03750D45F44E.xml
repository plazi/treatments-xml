<quantity id="871A9B39CC33FF90EAE904590DECF166" box="[675,755,1054,1078]" metricMagnitude="-2" metricUnit="m" metricValue="9.0" pageId="37" pageNumber="38" unit="mm" value="90.0">90 mm</quantity>
