<quantity id="871A9B39CC33FF90E99505ED0D24F092" box="[479,571,1450,1474]" metricMagnitude="-1" metricUnit="m" metricValue="1.2" pageId="37" pageNumber="38" unit="mm" value="120.0">120 mm</quantity>
