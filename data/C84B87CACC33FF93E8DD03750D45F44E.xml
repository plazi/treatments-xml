<quantity id="871A9B39CC33FF90ED7705890A83F0B6" box="[1341,1436,1486,1510]" metricMagnitude="-1" metricUnit="m" metricValue="1.8" pageId="37" pageNumber="38" unit="mm" value="180.0">180 mm</quantity>
