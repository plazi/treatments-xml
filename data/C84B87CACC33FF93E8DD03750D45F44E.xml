<quantity id="871A9B39CC33FF90ECE004050A18F10A" box="[1194,1287,1090,1114]" metricMagnitude="-1" metricUnit="m" metricValue="1.9" pageId="37" pageNumber="38" unit="mm" value="190.0">190 mm</quantity>
