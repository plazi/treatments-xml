<quantity id="871A9B39CC33FF90EC9804950A3DF1BA" box="[1234,1314,1234,1258]" metricMagnitude="-2" metricUnit="m" metricValue="8.0" pageId="37" pageNumber="38" unit="mm" value="80.0">80 mm</quantity>
