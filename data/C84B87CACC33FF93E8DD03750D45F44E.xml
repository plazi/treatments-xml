<quantity id="871A9B39CC33FF90EB5605B50C66F35A" box="[796,889,1522,1546]" metricMagnitude="-1" metricUnit="m" metricValue="2.1" pageId="37" pageNumber="38" unit="mm" value="210.0">210 mm</quantity>
