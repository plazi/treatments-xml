<quantity id="871A9B39CC33FF90EB2A05C10CA2F0CE" box="[864,957,1414,1438]" metricMagnitude="-1" metricUnit="m" metricValue="1.7" pageId="37" pageNumber="38" unit="mm" value="170.0">170 mm</quantity>
