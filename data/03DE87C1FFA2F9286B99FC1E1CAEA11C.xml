<quantity id="4C8F9B32FFA1F9296E48FEAB1DCDA147" box="[1350,1436,295,319]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="74" pageNumber="75" unit="mm" value="0.1">0.1 mm</quantity>
