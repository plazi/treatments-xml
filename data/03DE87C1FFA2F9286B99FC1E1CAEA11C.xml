<quantity id="4C8F9B32FFA2F92A6822F9091BC9A6E5" box="[812,920,1669,1693]" metricMagnitude="-5" metricUnit="m" metricValue="4.0" pageId="73" pageNumber="74" unit="mm" value="0.04">0.04 mm</quantity>
