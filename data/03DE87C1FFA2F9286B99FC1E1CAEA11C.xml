<quantity id="4C8F9B32FFA2F92A6F8EF9251CAEA6B9" box="[1152,1279,1705,1729]" metricMagnitude="-5" metricUnit="m" metricValue="3.0" pageId="73" pageNumber="74" unit="mm" value="0.03">0.03 mm</quantity>
