<quantity id="4C8F9B32FFA1F9296933FEC01B55A11C" box="[573,772,332,356]" metricMagnitude="-5" metricUnit="m" metricValue="2.55" metricValueMax="2.6" metricValueMin="2.5" pageId="74" pageNumber="75" unit="mm" value="0.0255" valueMax="0.026" valueMin="0.025">0.025–0.026 mm</quantity>
