<subSubSection id="C3554AC2FFFEAF4AFF33FF2668DCF888" pageId="10" pageNumber="11" type="materials_examined">
<paragraph id="8BF01949FFFEAF4AFF33FF266F88FF10" blockId="10.[151,1417,228,252]" box="[151,1417,228,252]" pageId="10" pageNumber="11">
<emphasis id="B93BC55BFFFEAF4AFF33FF266AF3FF10" bold="true" box="[151,242,228,252]" pageId="10" pageNumber="11">Type:—</emphasis>
<materialsCitation id="3B271314FFFEAF4AFF56FF276F84FF10" box="[242,1413,229,252]" collectionCode="SHTU" country="China" location="Little Kalakule Lake" municipality="Kunlun Mountains" pageId="10" pageNumber="11" specimenCount="1" typeStatus="holotype">
<collectingCountry id="F35859D9FFFEAF4AFF56FF276B4BFF10" box="[242,330,229,252]" name="China" pageId="10" pageNumber="11">CHINA</collectingCountry>
. 
<location id="8E904F92FFFEAF4AFEF0FF276832FF10" LSID="urn:lsid:plazi:treatment:03E6A85FFFFEAF4AFF33FF5569D7F834:8E904F92FFFEAF4AFEF0FF276832FF10" box="[340,563,229,252]" country="China" municipality="Kunlun Mountains" name="Little Kalakule Lake" pageId="10" pageNumber="11">Little Kalakule Lake</location>
