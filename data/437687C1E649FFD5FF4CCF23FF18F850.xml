<taxonomicName id="0CDF4D54E64EFFD5FBADCBA3FB0AF918" box="[1142,1278,1702,1728]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="84" pageNumber="85" phylum="Chordata" rank="species" species="undetermined">
