, 
<taxonomicName id="0CDF4D54E64EFFD5FF4CCB7BFE4EF940" authority="(Borkhausen)" baseAuthorityName="Borkhausen" baseAuthorityYear="1797" box="[151,442,1662,1688]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="84" pageNumber="85" phylum="Chordata" rank="species" species="myotis">
