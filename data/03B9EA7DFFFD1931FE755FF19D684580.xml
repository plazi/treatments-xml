<taxonomicName id="4C1020E8FFE21931FEB6521A9B2F45EA" box="[369,541,2140,2179]" class="Arachnida" family="Miturgidae" genus="Palicanus" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">Palicanus</taxonomicName>
