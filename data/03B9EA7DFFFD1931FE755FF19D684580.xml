from the 
<taxonomicName id="4C1020E8FFE21931FB21521A9C8545EA" box="[1254,1463,2140,2179]" class="Arachnida" family="Clubionidae" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="family">Clubionidae</taxonomicName>
