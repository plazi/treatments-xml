<taxonomicName id="258CA68AFB79FFA1FD57E22DFC40FEDB" box="[732,943,330,353]" class="Insecta" family="Noctuidae" genus="Aedophron" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="rhodites">
