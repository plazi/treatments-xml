<paragraph id="80011A1EAAD9881949D861CDC983118E" pageId="0" pageNumber="1">
<taxonomicName id="8B3094879A66A0C7356AB247BFC5365A" class="Malacostraca" family="Gnathiidae" genus="Gnathia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gnathia termitoides" order="Isopoda" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="termitoides">
<emphasis id="9717B13FBECDF5F6F838795AE195F83B" italics="true" pageId="0" pageNumber="1">Gnathia termitoides</emphasis>
