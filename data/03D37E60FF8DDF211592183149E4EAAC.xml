<taxonomicName id="4C7AB4F5FF8DDF2117E518F84D80EBE8" authority="Fabricius, 1798" authorityName="Fabricius" authorityYear="1798" box="[736,1157,1193,1215]" class="Insecta" family="Lygaeidae" genus="Lygaeus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="239" phylum="Arthropoda" rank="species" species="melanocephalus">
