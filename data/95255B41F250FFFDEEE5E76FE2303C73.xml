<taxonomicName id="DA8C91D4F250FFFFEF6CE002E207386F" box="[286,525,1377,1400]" class="Gymnolaemata" family="Lepraliellidae" genus="Drepanophora" kingdom="Animalia" order="Cheilostomatida" pageId="21" pageNumber="322" phylum="Bryozoa" rank="species" species="birbira">
