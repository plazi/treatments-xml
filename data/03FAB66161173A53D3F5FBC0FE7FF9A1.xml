</taxonomicName>
and designated 
<taxonomicName id="4C537CF461173A53D274FA29FDAFFA5D" authority="Koch" authorityName="Koch" box="[278,536,1421,1446]" class="Arachnida" family="Tydeidae" genus="Tydeus" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="498" phylum="Arthropoda" rank="species" species="cruciatus">
