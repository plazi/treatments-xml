<taxonomicName id="4C537CF461173A53D26FFA16FED4FA32" box="[269,355,1458,1481]" class="Arachnida" family="Tydeidae" genus="Lorryia" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="498" phylum="Arthropoda" rank="genus">
