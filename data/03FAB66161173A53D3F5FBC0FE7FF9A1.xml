<typeStatus id="54E8B9D561173A53D102FA2AFD27FA5D" box="[608,656,1422,1446]" pageId="3" pageNumber="498">type</typeStatus>
species, except for 
<taxonomicName id="4C537CF461173A53D013FA2AFB1CFA5D" authority="Oudemans" authorityName="Oudemans" box="[881,1195,1421,1446]" class="Arachnida" family="Tydeidae" genus="Lorryia" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="498" phylum="Arthropoda" rank="species" species="superba">
