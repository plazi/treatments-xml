<taxonomicName id="59A688FC9AD9510CD35B068698079446" class="Insecta" family="Curculionidae" genus="Dendroctonus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dendroctonus valens" order="Coleoptera" pageId="2" pageNumber="53" phylum="Arthropoda" rank="species" species="valens">Dendroctonus valens</taxonomicName>
