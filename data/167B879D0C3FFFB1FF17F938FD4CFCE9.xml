<taxonomicName id="59D24D080C3BFFB0FECBF93BFDCCF958" authorityName="Panesar" authorityYear="2004" box="[331,632,1749,1771]" class="Arachnida" family="Anisitsiellidae" genus="Utaxatax" kingdom="Animalia" order="Trombidiformes" pageId="4" pageNumber="449" phylum="Arthropoda" rank="species" species="brahmeri" subGenus="Utaxatax">
