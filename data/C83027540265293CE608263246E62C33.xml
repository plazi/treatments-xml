<taxonomicName id="E255D13A25791053AF494D8D8D1435BF" class="Magnoliopsida" family="Rutaceae" genus="Citrus" higherTaxonomySource="CoL" hybrid-sign="x" kingdom="Plantae" lsidName="Citrus sinensis" order="Sapindales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="sinensis">
