<taxonomicName id="9A954D43F965FF92FE70F9A1FD9D1D73" box="[444,546,1638,1660]" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="polytes">
