<taxonomicName id="9A954D43F965FF92FE31FAB4FDF51E8F" authorityName="Panzer" authorityYear="1806" box="[509,586,1394,1415]" class="Insecta" family="Ichneumonidae" genus="Trogus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis id="6FE1EAD2F965FF92FE31FAB4FDF51E8F" box="[509,586,1394,1415]" italics="true" pageId="38" pageNumber="39">Trogus</emphasis>
</taxonomicName>
to 
<taxonomicName id="9A954D43F965FF92FDBFFAB7FD4D1E8E" authorityName="Cameron" authorityYear="1902" box="[627,754,1393,1414]" class="Insecta" family="Ichneumonidae" genus="Holcojoppa" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis id="6FE1EAD2F965FF92FDBFFAB7FD4D1E8E" box="[627,754,1393,1414]" italics="true" pageId="38" pageNumber="39">Holcojoppa</emphasis>
