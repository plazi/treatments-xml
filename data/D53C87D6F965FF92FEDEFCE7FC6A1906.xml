</taxonomicName>
(
<taxonomicName id="9A954D43F965FF92FDB5F89AFD4E1C7A" box="[633,753,1884,1906]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="38" pageNumber="39" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
) (
<bibRefCitation id="39044B31F965FF92FCFDFF03FC0B1BD3" author="Iwata K" box="[817,948,197,219]" pageId="38" pageNumber="39" pagination="315 - 325" refId="ref42730" refString="Iwata K. 1961. Biological observations on the Ichneumonidae, Part I (Hymenoptera). Acta Hymenopterologica 1: 315 - 325." type="journal article" year="1961">Iwata 1961</bibRefCitation>
) and possibly from the umbellifer-feeder 
<taxonomicName id="9A954D43F965FF92FCE5FF22FC161BF1" authorityName="Linnaeus" authorityYear="1758" box="[809,937,228,249]" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="machaon">
<emphasis id="6FE1EAD2F965FF92FCE5FF22FC161BF1" box="[809,937,228,249]" italics="true" pageId="38" pageNumber="39">P. machaon</emphasis>
</taxonomicName>
(
<bibRefCitation id="39044B31F965FF92FC75FF22FBF11BF1" author="Uchida T" box="[953,1102,228,250]" pageId="38" pageNumber="39" pagination="195 - 256" refId="ref44740" refString="Uchida T. 1924. Uber die einigen Hymenopteren von Japan. Journal of the Sapporo Society for Agriculture and Forestry 16: 195 - 256." type="journal article" year="1924">Uchida 1924</bibRefCitation>
, 
<bibRefCitation id="39044B31F965FF92FB90FF22FB2A1BF2" author="Uchida T" box="[1116,1173,228,250]" pageId="38" pageNumber="39" pagination="43 - 173" refId="ref44767" refString="Uchida T. 1926. Erster Beitrag zur Ichneumoniden Japans. Journal of the Faculty of Agriculture, Hokkaido Imperial University 18: 43 - 173." type="journal article" year="1926">1926</bibRefCitation>
; 
<bibRefCitation id="39044B31F965FF92FB68FF22FB621BF2" author="Uchida T" box="[1188,1245,228,250]" pageId="38" pageNumber="39" pagination="2 - 8" refId="ref44869" refString="Uchida T. 1955. [Ichneumonidae parasitic on butterflies]. Shin Konchu 8: 2 - 8." type="journal article" year="1955">1955</bibRefCitation>
), in addition to the Rutaceae-feeders 
<taxonomicName id="9A954D43F965FF92FBF2FEC5FA321A11" authority="(Watanabe 1979)" baseAuthorityName="Watanabe" baseAuthorityYear="1979" box="[1086,1421,258,281]" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="xuthus">
<emphasis id="6FE1EAD2F965FF92FBF2FEC5FB1C1A1F" box="[1086,1187,258,280]" italics="true" pageId="38" pageNumber="39">P. xuthus</emphasis>
(
<bibRefCitation id="39044B31F965FF92FB0DFEC4FA3A1A11" author="Watanabe M" box="[1217,1413,258,281]" pageId="38" pageNumber="39" pagination="85 - 93" refId="ref45250" refString="Watanabe M. 1979. Natural mortalities of the swallowtail butterfly, Papilio xuthus L., at patchy habitats along the flyways in a hilly region. Japanese Journal of Ecology 29: 85 - 93." type="journal article" year="1979">Watanabe 1979</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="9A954D43F965FF92FCE5FEE4FB531A3F" authority="Menetries (Tosquinet, 1889)" authorityName="Menetries (Tosquinet" authorityYear="1889" box="[809,1260,289,311]" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="maackii">
