<taxonomicName id="9A954D43F965FF92FE0BF89AFDDB1C79" box="[455,612,1884,1905]" class="Magnoliopsida" family="Lauraceae" genus="Cinnamonum" kingdom="Plantae" order="Laurales" pageId="38" pageNumber="39" phylum="Tracheophyta" rank="genus">
