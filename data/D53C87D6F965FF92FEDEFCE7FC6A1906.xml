<taxonomicName id="9A954D43F965FF92FF5DF8D9FDB21C3D" authority="(Mell &amp; Heinrich 1931)" baseAuthorityName="Mell &amp; Heinrich" baseAuthorityYear="1931" box="[145,525,1822,1845]" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="xuthus">
