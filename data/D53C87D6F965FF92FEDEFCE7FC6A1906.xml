<taxonomicName id="9A954D43F965FF92FC3CFE86FB5F1A5D" baseAuthorityName="Tosquinet" baseAuthorityYear="1889" box="[1008,1248,320,341]" class="Insecta" family="Ichneumonidae" genus="Holcojoppa" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="mactator">
<emphasis id="6FE1EAD2F965FF92FC3CFE86FB5F1A5D" box="[1008,1248,320,341]" italics="true" pageId="38" pageNumber="39">Holcojoppa mactator</emphasis>
</taxonomicName>
