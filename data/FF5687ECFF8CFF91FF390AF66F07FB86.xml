<number id="C13724DDFF8DFF97FA820D1A6E06F8FB" box="[1324,1338,1841,1865]" pageId="3" pageNumber="521" value="1.0">1</number>
slide (
<number id="C13724DDFF8DFF97FA230D1A6EA0F8FB" box="[1421,1436,1841,1865]" pageId="3" pageNumber="521" value="4.0">4</number>
