</taxonomicName>
(
<taxonomicName id="B0FF4D79FF8DFF97FF300D1A6A21F8FB" box="[158,285,1841,1865]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="521" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
), 
