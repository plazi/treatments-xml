Data same as 
<typeStatus id="A8448858FF8DFF97FCD10D1A68D8F8FB" box="[895,996,1841,1865]" pageId="3" pageNumber="521">holotype</typeStatus>
, 
<number id="C13724DDFF8DFF97FC5C0D1A6F3CF8FB" box="[1010,1024,1841,1865]" pageId="3" pageNumber="521" value="2.0">2</number>
slides (
<number id="C13724DDFF8DFF97FBF00D1A6F51F8FB" box="[1118,1133,1841,1865]" pageId="3" pageNumber="521" value="6.0">6</number>
