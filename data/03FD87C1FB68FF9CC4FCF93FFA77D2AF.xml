<paragraph id="8BEB36D7FB6BFF9CC4ACFA2DFB43D288" blockId="10.[151,1437,1441,1532]" pageId="10" pageNumber="11">
<emphasis id="B920EAC5FB6BFF9CC4ACFA2DFEB3D2E9" bold="true" box="[199,345,1441,1466]" pageId="10" pageNumber="11">Distribution</emphasis>
. 
<taxonomicName id="4C544D54FB6BFF9CC50DFA2EFDCBD2E9" box="[358,545,1441,1466]" class="Insecta" family="Tettigoniidae" genus="Conocephalus" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="conocephalus">
<emphasis id="B920EAC5FB6BFF9CC50DFA2EFDCBD2E9" box="[358,545,1441,1466]" italics="true" pageId="10" pageNumber="11">C. conocephalus</emphasis>
</taxonomicName>
