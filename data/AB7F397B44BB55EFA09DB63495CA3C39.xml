<taxonomicName id="DD62B5DA0ED35513005ED2554448A643" class="Pinopsida" family="Pinaceae" genus="Beta" kingdom="Plantae" lsidName="" order="Pinales" pageId="0" pageNumber="77092" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
