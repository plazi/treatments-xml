<elevation id="6983D1A1BA57FF8EAA60FA331C7BF1D5" box="[1289,1401,1438,1464]" metricMagnitude="4" metricUnit="m" metricValue="3.876" pageId="51" pageNumber="52" unit="m" value="38760.0">38, 760m</elevation>
</quantity>
, 
<date id="96101052BA57FF8EAAECFA3319F1F18D" pageId="51" pageNumber="52" value="1977-09-05">
