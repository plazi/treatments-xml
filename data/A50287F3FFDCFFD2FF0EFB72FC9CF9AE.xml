<location id="2874603EFFDCFFD2FCA9FAD9FC4CFADE" LSID="urn:lsid:plazi:treatment:A50287F3FFDCFFD2FF0EFB72FC9CF9AE:2874603EFFDCFFD2FCA9FAD9FC4CFADE" box="[816,937,1375,1400]" country="Ethiopia, Kenya" name="Rift valley" pageId="49" pageNumber="50" stateProvince="A">Rift valley</location>
and 
<collectingCountry id="55BC7675FFDCFFD2FC79FAD9FBDCFADE" box="[992,1081,1375,1400]" name="Malawi" pageId="49" pageNumber="50">Malawi</collectingCountry>
. Known host-plants are species of 
<taxonomicName id="EAAB4D66FFDCFFD2FF2CFA03FEEBFA3A" box="[181,270,1413,1436]" class="Magnoliopsida" family="Violaceae" genus="Rinorea" kingdom="Plantae" order="Malpighiales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="genus">
<emphasis id="1FDFEAF7FFDCFFD2FF2CFA03FEEBFA3A" box="[181,270,1413,1436]" italics="true" pageId="49" pageNumber="50">Rinorea</emphasis>
</taxonomicName>
(
<taxonomicName id="EAAB4D66FFDCFFD2FE84FA02FE6AFA3B" box="[285,399,1412,1437]" class="Magnoliopsida" family="Violaceae" kingdom="Plantae" order="Malpighiales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="family">Violaceae</taxonomicName>
) and 
<taxonomicName id="EAAB4D66FFDCFFD2FE55FA03FDD4FA3A" box="[460,561,1413,1436]" class="Magnoliopsida" family="Putranjivaceae" genus="Drypetes" kingdom="Plantae" order="Malpighiales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="genus">
<emphasis id="1FDFEAF7FFDCFFD2FE55FA03FDD4FA3A" box="[460,561,1413,1436]" italics="true" pageId="49" pageNumber="50">Drypetes</emphasis>
</taxonomicName>
(
<taxonomicName id="EAAB4D66FFDCFFD2FDD9FA02FD0BFA3B" box="[576,750,1412,1437]" class="Magnoliopsida" family="Euphorbiaceae" kingdom="Plantae" order="Malpighiales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
).
</materialsCitation>
</paragraph>
