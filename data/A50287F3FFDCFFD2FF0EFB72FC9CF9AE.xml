</materialsCitation>
, 
<materialsCitation id="9DC33CB8FFDCFFD2FAFFFABAFD1CFA3B" ID-GBIF-Occurrence="1423853027" country="Ethiopia, Kenya" location="Rift valley" pageId="49" pageNumber="50" specimenCount="1" stateProvince="A">
<emphasis id="1FDFEAF7FFDCFFD2FAFFFABAFF22FADE" italics="true" pageId="49" pageNumber="50">
<collectingRegion id="EF6FF807FFDCFFD2FAFFFABAFA9FFAF5" box="[1382,1402,1340,1363]" country="Nigeria" name="Rivers" pageId="49" pageNumber="50">A</collectingRegion>
. q. rosa
</emphasis>
, is known from 
<collectingCountry id="55BC7675FFDCFFD2FE1BFAD9FDD9FADE" box="[386,572,1375,1400]" isEnumeration="true" pageId="49" pageNumber="50">
<collectingCountry id="55BC7675FFDCFFD2FE1BFAD9FE01FADE" box="[386,484,1375,1400]" name="Ethiopia" pageId="49" pageNumber="50">Ethiopia</collectingCountry>
, 
<collectingCountry id="55BC7675FFDCFFD2FE69FAE6FDD9FADE" box="[496,572,1376,1400]" name="Kenya" pageId="49" pageNumber="50">Kenya</collectingCountry>
</collectingCountry>
, 
<collectingCountry id="55BC7675FFDCFFD2FDD1FAD9FD48FADE" box="[584,685,1375,1400]" name="Tanzania" pageId="49" pageNumber="50">Tanzania</collectingCountry>
