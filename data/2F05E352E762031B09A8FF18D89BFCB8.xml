<collectingCountry id="DFBB12D4E762031B08D4FDEEDF4DFDD9" box="[491,589,609,632]" name="Armenia" pageId="19" pageNumber="20">Armenia</collectingCountry>
, 
<collectingCountry id="DFBB12D4E762031B0B68FDEEDFCFFDD9" box="[599,719,609,632]" name="Azerbaijan" pageId="19" pageNumber="20">Azerbaijan</collectingCountry>
, 
<collectingCountry id="DFBB12D4E762031B0BE6FDEEDE27FDD9" box="[729,807,609,632]" name="Turkey" pageId="19" pageNumber="20">Turkey</collectingCountry>
) ................................................. 
<taxonomicName id="60AC29C7E762031B0DB6FDEED89BFDD8" authority="(Lelej)" baseAuthorityName="Lelej" box="[1161,1435,609,633]" class="Insecta" family="Mutillidae" genus="Skorikovia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="transcaucasica">
