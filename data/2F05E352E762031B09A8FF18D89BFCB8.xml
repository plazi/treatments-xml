<collectingCountry id="DFBB12D4E762031B0B38FD8EDF50FDB9" box="[519,592,513,536]" name="Russia" pageId="19" pageNumber="20">Russia</collectingCountry>
(South of European territory, South Ural), 
<collectingCountry id="DFBB12D4E762031B0D19FD8ED97CFDB9" box="[1062,1148,513,536]" name="Ukraine" pageId="19" pageNumber="20">Ukraine</collectingCountry>
(
<collectingRegion id="65689CA6E762031B0DB3FD8ED9E0FDB9" box="[1164,1248,513,536]" country="Ukraine" name="Crimea" pageId="19" pageNumber="20">Crimea</collectingRegion>
), 
<collectingCountry id="DFBB12D4E762031B0DCBFD8ED825FDB9" box="[1268,1317,513,536]" name="Italy" pageId="19" pageNumber="20">Italy</collectingCountry>
