<collectingCountry id="DFBB12D4E762031B0A27FD2EDE95FD19" box="[792,917,673,696]" name="Kyrgyzstan" pageId="19" pageNumber="20">Kyrgyzstan</collectingCountry>
, 
<collectingCountry id="DFBB12D4E762031B0AA0FD2ED91AFD19" box="[927,1050,673,696]" name="Uzbekistan" pageId="19" pageNumber="20">Uzbekistan</collectingCountry>
, 
<collectingCountry id="DFBB12D4E762031B0D1AFD2ED9BBFD19" box="[1061,1211,673,696]" name="Turkmenistan" pageId="19" pageNumber="20">Turkmenistan</collectingCountry>
, and North 
