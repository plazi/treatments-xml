<caption id="CB551E7763DB30C66BB46E32433CC350" doi="10.3897/zookeys.1035.61584.figure15" httpUri="https://binary.pensoft.net/fig/536440" pageId="0" pageNumber="115" start="Figure 15" startId="F15">
<paragraph id="2A805C05477B450B9B5A4E8B9EFA08A2" pageId="0" pageNumber="115">
<emphasis id="8E87EC60F820CF377403BB276FB7AF43" bold="true" pageId="0" pageNumber="115">Figure 15.</emphasis>
<taxonomicName id="A79BE98A8C7CE2D6AFBE1526889192C5" authorityName="Bradford" authorityYear="1967" class="Hydrozoa" family="Harpacticidae" genus="Tigriopus" kingdom="Animalia" lsidName="Tigriopus raki" order="Harpacticoida" pageId="0" pageNumber="115" phylum="Porifera" rank="species" species="raki">
