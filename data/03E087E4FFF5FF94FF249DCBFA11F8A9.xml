<taxonomicName id="4C494D71FFF5FF94FF249DCBFDB4FC49" authority="Hoffmannsegg" authorityName="Hoffmannsegg" authorityYear="1817" box="[192,517,959,984]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="genus">
