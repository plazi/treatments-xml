<location id="9C55600DFFC5FFF99258FB78B8C846C5" LSID="urn:lsid:plazi:treatment:112387C0FFC5FFF990D7FC4BBBBA4699:9C55600DFFC5FFF99258FB78B8C846C5" box="[543,680,1173,1199]" country="Switzerland" county="Adventivart" municipality="Wasser" name="Commugny" pageId="3" pageNumber="90" stateProvince="Vaud">Commugny</location>
