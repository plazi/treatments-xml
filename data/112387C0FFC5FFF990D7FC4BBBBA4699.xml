<location id="9C55600DFFC5FFF993F5FBBCBE604601" LSID="urn:lsid:plazi:treatment:112387C0FFC5FFF990D7FC4BBBBA4699:9C55600DFFC5FFF993F5FBBCBE604601" box="[946,1024,1105,1131]" country="Switzerland" county="Adventivart" municipality="Wasser" name="Belege" pageId="3" pageNumber="90" stateProvince="Vaud">Belege</location>
