<location id="9C55600DFFC5FFF99129FB78BB8A46C5" LSID="urn:lsid:plazi:treatment:112387C0FFC5FFF990D7FC4BBBBA4699:9C55600DFFC5FFF99129FB78BB8A46C5" box="[366,490,1173,1199]" country="Switzerland" county="Adventivart" municipality="Wasser" name="Exemplare" pageId="3" pageNumber="90" stateProvince="Vaud">Exemplare</location>
