<taxonomicName id="A6DD4D71FF9589443C57FAC0FCF1E031" authority="Hemp, 2017" authorityName="Hemp" authorityYear="2017" box="[605,833,1328,1349]" class="Insecta" family="Phaneropteridae" genus="Eurycorypha" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="481" phylum="Arthropoda" rank="species" species="elongata" status="n. sp.">
<emphasis id="53A9EAE0FF9589443C57FAC0FCF1E031" box="[605,833,1328,1349]" italics="true" pageId="10" pageNumber="481">Eurycorypha elongata</emphasis>
</taxonomicName>
<emphasis id="53A9EAE0FF9589443D4DFAC0FC0EE032" bold="true" box="[839,958,1328,1350]" pageId="10" pageNumber="481">
