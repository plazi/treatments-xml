<taxonomicName id="4C7CDEF10232FF90F7C86051C8C2FAFA" authority="(Klug, 1816)" baseAuthorityName="Klug" baseAuthorityYear="1816" box="[410,643,1305,1324]" class="Insecta" family="Tenthredinidae" genus="Dineura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="793" phylum="Arthropoda" rank="species" species="testaceipes">
<emphasis id="B90879600232FF90F7C86051C850FAFA" box="[410,529,1305,1324]" italics="true" pageId="6" pageNumber="793">D. testaceipes</emphasis>
