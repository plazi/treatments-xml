<subSubSection id="C3196FBBFFBEFF9882C2FE50FC582204" box="[151,930,510,533]" pageId="1" pageNumber="55" type="materials_examined">
<paragraph id="8BBC3C30FFBEFF9882C2FE50FC582204" blockId="1.[151,930,510,533]" box="[151,930,510,533]" pageId="1" pageNumber="55">
<typeStatus id="54B88292FFBEFF9882C2FE50FF322204" box="[151,200,511,532]" pageId="1" pageNumber="55">Type</typeStatus>
species: 
