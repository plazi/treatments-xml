<collectingCountry id="F1DA763CFFF4FF8A805AFA049CB3CEF5" box="[584,643,1419,1439]" name="Kenya" pageId="3" pageNumber="468">Kenya</collectingCountry>
, 
<collectingRegion id="4B09F84EFFF4FF8A8087FA049CF7CEF5" box="[661,711,1419,1439]" country="Kenya" name="Meru" pageId="3" pageNumber="468">Meru</collectingRegion>
, Nkubu, 
<quantity id="4E359B49FFF4FF8A813AFA049D46CEF5" box="[808,886,1419,1439]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="3" pageNumber="468" unit="m" value="1500.0">1500 m</quantity>
