<taxonomicName id="4ECD4D2FFFF4FF8A8136FA2C9D48CEDD" box="[804,888,1443,1463]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="468" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
