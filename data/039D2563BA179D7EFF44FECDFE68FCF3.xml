<taxonomicName id="4C34EFF6BA179D7EFDA1FDA4FF7EFD60" baseAuthorityName="Ruppell" baseAuthorityYear="1840" class="Aves" family="Sylviidae" genus="Pseudoalcippe" kingdom="Animalia" order="Passeriformes" pageId="98" pageNumber="99" phylum="Chordata" rank="species" species="abyssinica">
