<taxonomicName id="4C34EFF6BA179D7EFE9BFD41FE72FD60" authorityName="Blyth" authorityYear="1844" box="[372,461,657,680]" class="Aves" family="Pellorneidae" genus="Alcippe" kingdom="Animalia" order="Passeriformes" pageId="98" pageNumber="99" phylum="Chordata" rank="genus">
