<caption id="ED9CADB89C682A22EFC15991FB1A5AC3" ID-DOI="http://doi.org/10.5281/zenodo.4909389" ID-Zenodo-Dep="4909389" httpUri="https://zenodo.org/record/4909389/files/figure.png" pageId="16" pageNumber="17" startId="16.[151,249,1966,1988]" targetBox="[228,1350,203,1932]" targetPageId="16">
