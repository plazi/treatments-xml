<number id="881D24970D30C22EFA8E9182FAA0F8BD" box="[1282,1311,1774,1800]" pageId="20" pageNumber="21" value="12.0">12</number>
while it is less than 
<number id="881D24970D30C22EFE88907FFE9EF898" box="[260,289,1811,1837]" pageId="20" pageNumber="21" value="10.0">10</number>
(usually 
<number id="881D24970D30C22EFE03907FFE05F898" box="[399,442,1811,1837]" pageId="20" pageNumber="21" value="7.0" valueMax="8.0" valueMin="6.0">6–8</number>
) in 
<taxonomicName id="F9D54D330D30C22EFE659078FDC7F898" box="[489,632,1812,1837]" class="Insecta" family="Fulgoridae" genus="Penthicodes" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
<emphasis id="0CA1EAA20D30C22EFE659078FDC7F898" box="[489,632,1812,1837]" italics="true" pageId="20" pageNumber="21">Penthicodes</emphasis>
</taxonomicName>
