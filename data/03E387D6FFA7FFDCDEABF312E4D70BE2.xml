<taxonomicName id="4C4A4D43FFA0FFDCDF9CF77EE4D30BE2" authority="(Morelet, 1863)" baseAuthorityName="Morelet" baseAuthorityYear="1863" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" kingdom="Animalia" order="Stylommatophora" pageId="12" pageNumber="69" phylum="Mollusca" rank="species" species="dentritis">
