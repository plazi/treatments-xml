51. 
<taxonomicName id="8FEE4D41FF89FFA2FF57FA6D64F3CEA2" authority="Neumann, 1899" box="[199,432,1482,1509]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="goeldii">
