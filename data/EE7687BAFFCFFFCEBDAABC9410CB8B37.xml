<quantity id="A1279B49FFCFFFCEBF26BFBF160F8DC9" box="[539,631,733,757]" metricMagnitude="2" metricUnit="m" metricValue="6.91896" pageId="56" pageNumber="57" unit="ft" value="2270.0">
