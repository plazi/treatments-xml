<emphasis id="B92FEAF190040908FCFFFAA2FA0D369A" bold="true" box="[821,1437,1391,1418]" pageId="9" pageNumber="8">
<taxonomicName id="4C5B4D6090040908FCFFFAA2FB32369A" authority="Ahmed and Miller" authorityName="Ahmed and Miller" authorityYear="2020" box="[821,1186,1391,1418]" class="Insecta" family="Margarodidae" genus="Matsucoccus" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="species" species="krystalae" status="new species">
<emphasis id="B92FEAF190040908FCFFFAA2FCDE3699" bold="true" box="[821,846,1392,1418]" italics="true" pageId="9" pageNumber="8">M</emphasis>
. 
<emphasis id="B92FEAF190040908FC91FABDFC543699" bold="true" box="[859,964,1391,1418]" italics="true" pageId="9" pageNumber="8">krystalae</emphasis>
Ahmed and Miller
</taxonomicName>
, 
<taxonomicNameLabel id="A21C578A90040908FB67FABDFAA5369A" box="[1197,1333,1391,1417]" pageId="9" pageNumber="8" rank="species">new species</taxonomicNameLabel>
