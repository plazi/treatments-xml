<taxonomicName id="4C3D9187FFE5C04D508DFE13D16F3C88" box="[1015,1130,484,505]" class="Elasmobranchii" family="Pseudotriakidae" genus="Gollum" kingdom="Animalia" order="Carcharhiniformes" pageId="12" pageNumber="29" phylum="Chordata" rank="species" species="suluensis" status="sp. nov.">
