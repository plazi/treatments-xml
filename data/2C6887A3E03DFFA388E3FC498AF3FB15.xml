<collectionCode id="C2D0AE70E03DFFA388EBFB168EA9FB15" LSID="urn:lsid:biocol.org:col:34795" box="[159,251,1245,1271]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="10" pageNumber="33">FMNH</collectionCode>
