<taxonomicName id="6AD247ADFFDCFFE1FEBDFF5BE8F6FF53" authorityName="Breddin" authorityYear="1906" box="[276,493,150,177]" class="Insecta" family="Pentatomidae" genus="Stiretrus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="bifrenatus">
