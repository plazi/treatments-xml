, 
<taxonomicName id="4C7E4D33FFB7FF85E2A46254FE79FBC0" authority="Van Duzee" authorityName="Van Duzee" box="[1018,1306,1109,1132]" class="Insecta" family="Dolichopodidae" genus="Tachytrechus" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="transversus">
