<taxonomicName id="4C734D3934125E78FF0266A2FEB2F9C9" authority="Fujii &amp; Reimer, 2013" authorityName="Fujii &amp; Reimer" authorityYear="2013" box="[164,328,1518,1539]" class="Anthozoa" family="Nanozoanthidae" genus="Nanozoanthus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Zoantharia" pageId="3" pageNumber="512" phylum="Cnidaria" rank="genus" status="gen. nov.">
<emphasis id="B907EAA834125E78FF0266A2FEB2F9C9" box="[164,328,1518,1539]" italics="true" pageId="3" pageNumber="512">Nanozoanthus</emphasis>
