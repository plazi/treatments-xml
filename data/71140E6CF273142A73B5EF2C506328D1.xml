<geoCoordinate id="9C89D9BDF272142D7633EB0951462961" box="[1297,1379,333,359]" direction="west" orientation="longitude" pageId="27" pageNumber="28" precision="555" value="-0.17">
W 
<quantity id="3E45129FF272142D7610EB0951462961" box="[1330,1379,333,359]" metricMagnitude="-4" metricUnit="m" metricValue="1.7" pageId="27" pageNumber="28" unit="mm" value="0.17">0.17</quantity>
</geoCoordinate>
