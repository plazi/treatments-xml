<quantity id="3E45129FF272142D7729EB6D50542945" box="[1035,1137,297,323]" metricMagnitude="-4" metricUnit="m" metricValue="3.5999999999999996" pageId="27" pageNumber="28" unit="mm" value="0.36">0.36 mm</quantity>
, 
<geoCoordinate id="9C89D9BDF272142D775EEB6D50E82945" box="[1148,1229,296,323]" direction="west" orientation="longitude" pageId="27" pageNumber="28" precision="555" value="-0.15">
