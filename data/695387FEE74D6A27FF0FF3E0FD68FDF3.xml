<paragraph id="E14536E8E74D6A27FF0FF3E0FCA4FF16" blockId="12.[178,857,216,238]" box="[178,857,216,238]" pageId="12" pageNumber="1171">
<heading id="BA0D8184E74D6A27FF0FF3E0FCA4FF16" box="[178,857,216,238]" fontSize="8" level="2" pageId="12" pageNumber="1171" reason="2">
<emphasis id="D38EEAFAE74D6A27FF0FF3E0FCA4FF16" bold="true" box="[178,857,216,238]" pageId="12" pageNumber="1171">
<taxonomicName id="26FA4D6BE74D6A27FF0FF3E0FD1EFF15" authority="MAIDL" authorityName="MAIDL" authorityYear="2012" baseAuthorityName="MAIDL" baseAuthorityYear="2012" box="[178,739,216,238]" class="Insecta" family="Eumenidae" genus="Synagris" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="1171" phylum="Arthropoda" rank="subSpecies" species="versicolor" status="comb.nov." subGenus="Pseudagris" subSpecies="meadewaldoi">
