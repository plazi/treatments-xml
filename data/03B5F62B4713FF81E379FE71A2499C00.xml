<quantity id="4CE4EAD84713FF81E0DEFD9BA5249D00" box="[715,810,590,614]" metricMagnitude="1" metricUnit="m" metricValue="1.95" metricValueMax="2.0" metricValueMin="1.9" pageId="7" pageNumber="837" unit="m" value="19.5" valueMax="20.0" valueMin="19.0">19±20 m</quantity>
