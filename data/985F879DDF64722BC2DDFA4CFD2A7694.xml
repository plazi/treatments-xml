<number id="A63E24ACDF6A722BC3D3FE8DFE46760F" box="[409,422,260,284]" pageId="8" pageNumber="9" value="1.0">1</number>
). 
<taxonomicName id="D7F64D08DF6A722BC3F5FE8DFD63760F" box="[447,643,260,284]" class="Insecta" family="Blastobasidae" genus="Koleps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="angulatus">
