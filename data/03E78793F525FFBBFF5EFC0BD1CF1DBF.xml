<taxonomicName id="4C4E4D06F526FFBAFEECF8FFD1761930" box="[293,582,1896,1919]" class="Pycnogonida" family="Ammotheidae" genus="Sericosura" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="2" pageNumber="167" phylum="Arthropoda" rank="species" species="gemmaemonsis" status="sp. nov.">
<emphasis id="B93AEA97F526FFBAFEECF8FFD1761930" box="[293,582,1896,1919]" italics="true" pageId="2" pageNumber="167">Sericosura gemmaemonsis</emphasis>
</taxonomicName>
