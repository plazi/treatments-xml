<collectingRegion id="8568F81C0D797A08FBCDFD2BD3FEFD5E" box="[1101,1247,685,709]" country="Brazil" name="Espirito Santo" pageId="13" pageNumber="170">Espírito Santo</collectingRegion>
and 
<collectingRegion id="8568F81C0D797A08FD75FD41D4A0FD44" box="[757,897,711,735]" country="Brazil" name="Rio de Janeiro" pageId="13" pageNumber="170">Rio de Janeiro</collectingRegion>
