<taxonomicName id="8FEE4D41FFD7FFFCFB61FE8065F2CA22" authorityName="Schulze and Schlottke" authorityYear="1929" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="rugicollis">
