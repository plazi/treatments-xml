<quantity id="4C88DF860D3F2E520D07C6FF8EEE5241" box="[1104,1184,1018,1042]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="10" pageNumber="343" unit="m" value="1100.0">1100m</quantity>
, 
<date id="FFCE54A30D3F2E520DFCC6FF8F605241" box="[1195,1326,1017,1042]" pageId="10" pageNumber="343" value="1999-12-13">
