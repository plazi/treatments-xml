<subSubSection id="ABEF78E1475FFFD9C797E2F0FB3EF90F" pageId="14" pageNumber="415" type="distribution">
<paragraph id="E34A2B6A475FFFD9C797E2F0FDDBF99F" blockId="14.[151,1436,1516,2009]" pageId="14" pageNumber="415">
<emphasis id="D181F778475FFFD9C797E2F0FE5BF9D6" bold="true" box="[151,398,1516,1541]" pageId="14" pageNumber="415">
Distribution in 
<collectingCountry id="9BE26BFA475FFFD9C652E2F0FE5CF9D6" box="[338,393,1516,1541]" name="Iran" pageId="14" pageNumber="415">Iran</collectingCountry>
.
</emphasis>
Guilan (
<bibRefCitation id="8764569B475FFFD9C6F4E2F0FD13F9D7" author="Amini" box="[500,710,1516,1541]" pageId="14" pageNumber="415" refString="Amini, S., Hosseini, R. &amp; Sohani, M. M. (2013) A faunal study of bark beetles (Coleoptera: Curculionidae: Scolytinae) in Guilan province in North of Iran. Entomofauna, 34, 169 - 176." type="journal article" year="2013">
Amini 
<emphasis id="D181F778475FFFD9C546E2F1FDAFF9D6" box="[582,634,1516,1541]" italics="true" pageId="14" pageNumber="415">et al</emphasis>
. 
<number id="553D394D475FFFD9C58AE2F0FD13F9D7" box="[650,710,1516,1540]" pageId="14" pageNumber="415" value="2013.0">2013</number>
</bibRefCitation>
), Tehran and other northern provinces (
<bibRefCitation id="8764569B475FFFD9C38AE2F0FAA9F9D7" author="Modarres" box="[1162,1404,1516,1541]" pageId="14" pageNumber="415" refString="Modarres Awal, M. (1997) Platypodidae, Scolytidae. In: Modarres Awal, M. (Ed.), List of agricultural pests and their natural enemies in Iran. Ferdowsi University Press, Mashhad, pp. 185, 192 - 194." type="book chapter" year="1997">
Modarres Awal 
<number id="553D394D475FFFD9C244E2F0FAA9F9D7" box="[1348,1404,1516,1540]" pageId="14" pageNumber="415" value="1997.0">1997</number>
</bibRefCitation>
as 
<taxonomicName id="24F550E9475FFFD9C797E113FDDAF9FB" authority="Brulle, 1832" authorityName="Brulle" authorityYear="1832" box="[151,527,1551,1576]" class="Insecta" family="Curculionidae" genus="Phloeosinus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="415" phylum="Arthropoda" rank="species" species="bicolor">
<emphasis id="D181F778475FFFD9C797E113FEACF9FB" box="[151,377,1551,1576]" italics="true" pageId="14" pageNumber="415">Phloeosinus bicolor</emphasis>
Brullé, 
<number id="553D394D475FFFD9C6D7E10CFDDAF9FB" box="[471,527,1552,1576]" pageId="14" pageNumber="415" value="1832.0">1832</number>
</taxonomicName>
and 
<taxonomicName id="24F550E9475FFFD9C548E113FC30F9FB" authority="Hopkins, 1903" authorityName="Hopkins" authorityYear="1903" box="[584,997,1551,1576]" class="Insecta" family="Curculionidae" genus="Phloeosinus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="415" phylum="Arthropoda" rank="species" species="cupressi">
<emphasis id="D181F778475FFFD9C548E113FCE3F9FB" box="[584,822,1551,1576]" italics="true" pageId="14" pageNumber="415">Phloeosinus cupressi</emphasis>
Hopkins, 
<number id="553D394D475FFFD9C4ADE10CFC30F9FB" box="[941,997,1552,1576]" pageId="14" pageNumber="415" value="1903.0">1903</number>
</taxonomicName>
(see 
<bibRefCitation id="8764569B475FFFD9C323E113FA85F9FB" author="Ghahari" box="[1059,1360,1551,1576]" pageId="14" pageNumber="415" refString="Ghahari, H. &amp; Colonelli, E. (2012) Curculionoidea from Golestan province, Northern Iran. Fragmenta Entomologica, 44, 101 - 161. http: // dx. doi. org / 10.4081 / fe. 2012.37" type="journal article" year="2012">
Ghahari &amp; Colonelli 
<number id="553D394D475FFFD9C215E10CFA85F9FB" box="[1301,1360,1552,1576]" pageId="14" pageNumber="415" value="2012.0">2012</number>
</bibRefCitation>
)), 
<collectingCountry id="9BE26BFA475FFFD9C26FE10CFA49F9F4" box="[1391,1436,1552,1575]" name="Iran" pageId="14" pageNumber="415">Iran</collectingCountry>
(no locality cited) (
<bibRefCitation id="8764569B475FFFD9C66CE128FDD7F99F" author="Knizek" box="[364,514,1588,1613]" pageId="14" pageNumber="415" refString="Knizek, M. (2011) Platypodinae and Scolytinae. In: Lobl, I. &amp; Smetana, A. (Eds.), Catalogue of Palaearctic Coleoptera. Vol. 7. Curculionoidea I. Apollo Books, Stenstrup, pp. 201 - 251." type="book" year="2011">
Knížek 
<number id="553D394D475FFFD9C6C6E128FDD7F99F" box="[454,514,1588,1612]" pageId="14" pageNumber="415" value="2011.0">2011</number>
</bibRefCitation>
).
</paragraph>
<paragraph id="E34A2B6A475FFFD9C7C7E14BFD8BF96B" blockId="14.[151,1436,1516,2009]" pageId="14" pageNumber="415">
<emphasis id="D181F778475FFFD9C7C7E14BFE12F9A3" bold="true" box="[199,455,1623,1648]" pageId="14" pageNumber="415">General distribution.</emphasis>
Central and southern Europe, North Africa, Middle East, 
<collectingCountry id="9BE26BFA475FFFD9C373E14BFAC4F9A3" box="[1139,1297,1623,1648]" name="Turkmenistan" pageId="14" pageNumber="415">Turkmenistan</collectingCountry>
, and 
<collectingCountry id="9BE26BFA475FFFD9C252E14BFA4DF9A3" box="[1362,1432,1623,1648]" name="China" pageId="14" pageNumber="415">China</collectingCountry>
, 
<collectingCountry id="9BE26BFA475FFFD9C797E161FF0AF947" box="[151,223,1661,1684]" name="South Korea" pageId="14" pageNumber="415">Korea</collectingCountry>
. 
<bibRefCitation id="8764569B475FFFD9C7EAE160FE5DF946" author="Schedl" box="[234,392,1660,1685]" pageId="14" pageNumber="415" refString="Schedl, K. E. (1960) Scolytidae und Platypodidae Afrikas. Band I. Scolytidae (Fortsetzung). Revista Entomologia de Mocambique, 3, 75 - 154." type="journal article" year="1960">
Schedl (
<number id="553D394D475FFFD9C646E160FE55F946" box="[326,384,1660,1685]" pageId="14" pageNumber="415" value="1960.0">1960</number>
)
</bibRefCitation>
and 
<bibRefCitation id="8764569B475FFFD9C6C0E161FDBFF946" author="Browne" box="[448,618,1660,1685]" pageId="14" pageNumber="415" refString="Browne, F. G. (1968) Pests and diseases of forest plantation trees. Clarendon Press, Oxford, 1330 pp." type="book" year="1968">
Browne (
<number id="553D394D475FFFD9C528E160FDB7F946" box="[552,610,1660,1685]" pageId="14" pageNumber="415" value="1968.0">1968</number>
)
</bibRefCitation>
record it from 
<collectingCountry id="9BE26BFA475FFFD9C416E160FCADF946" box="[790,888,1660,1685]" name="Ethiopia" pageId="14" pageNumber="415">Ethiopia</collectingCountry>
, 
<collectingCountry id="9BE26BFA475FFFD9C484E161FC05F946" box="[900,976,1661,1685]" name="Kenya" pageId="14" pageNumber="415">Kenya</collectingCountry>
, 
<collectingCountry id="9BE26BFA475FFFD9C4DBE160FB95F946" box="[987,1088,1660,1685]" name="Tanzania" pageId="14" pageNumber="415">Tanzania</collectingCountry>
and 
<collectingCountry id="9BE26BFA475FFFD9C378E160FB07F946" box="[1144,1234,1660,1685]" name="Uganda" pageId="14" pageNumber="415">Uganda</collectingCountry>
, but these records are probably of a closely related species.
</paragraph>
<paragraph id="E34A2B6A475FFFD9C7C7E1D8FB3EF90F" blockId="14.[151,1436,1516,2009]" box="[199,1259,1732,1757]" pageId="14" pageNumber="415">
<emphasis id="D181F778475FFFD9C7C7E1D8FE9FF90E" bold="true" box="[199,330,1732,1757]" pageId="14" pageNumber="415">Taxonomy.</emphasis>
The species is often recorded in the literature as its synonym, 
<taxonomicName id="24F550E9475FFFD9C304E1D8FB36F90E" box="[1028,1251,1732,1757]" class="Insecta" family="Curculionidae" genus="Phloeosinus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="415" phylum="Arthropoda" rank="species" species="bicolor">
<emphasis id="D181F778475FFFD9C304E1D8FB36F90E" box="[1028,1251,1732,1757]" italics="true" pageId="14" pageNumber="415">Phloeosinus bicolor</emphasis>
</taxonomicName>
.
</paragraph>
</subSubSection>
<subSubSection id="ABEF78E1475FFFD9C7C7E1FBFF3AF80A" pageId="14" pageNumber="415" type="biology_ecology">
<paragraph id="E34A2B6A475FFFD9C7C7E1FBFF3AF80A" blockId="14.[151,1436,1516,2009]" pageId="14" pageNumber="415">
<emphasis id="D181F778475FFFD9C7C7E1FBFEF2F8D3" bold="true" box="[199,295,1767,1792]" pageId="14" pageNumber="415">Biology.</emphasis>
This species breeds in various genera of 
<taxonomicName id="24F550E9475FFFD9C409E1F4FC74F8D3" box="[777,929,1768,1792]" class="Pinopsida" family="Cupressaceae" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="family">Cupressaceae</taxonomicName>
including 
<taxonomicName id="24F550E9475FFFD9C31EE1F4FB47F92C" box="[1054,1170,1768,1791]" class="Pinopsida" family="Cupressaceae" genus="Cupressus" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F778475FFFD9C31EE1F4FB47F92C" box="[1054,1170,1768,1791]" italics="true" pageId="14" pageNumber="415">Cupressus</emphasis>
</taxonomicName>
, 
<taxonomicName id="24F550E9475FFFD9C3A3E1F5FAC4F8D3" box="[1187,1297,1769,1792]" class="Pinopsida" family="Cupressaceae" genus="Juniperus" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F778475FFFD9C3A3E1F5FAC4F8D3" box="[1187,1297,1769,1792]" italics="true" pageId="14" pageNumber="415">Juniperus</emphasis>
</taxonomicName>
, 
<taxonomicName id="24F550E9475FFFD9C222E1FBFAB6F8D3" box="[1314,1379,1767,1792]" class="Pinopsida" family="Cupressaceae" genus="Thuja" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F778475FFFD9C222E1FBFAB6F8D3" box="[1314,1379,1767,1792]" italics="true" pageId="14" pageNumber="415">Thuja</emphasis>
</taxonomicName>
, and 
<taxonomicName id="24F550E9475FFFD9C797E010FEF6F8F6" box="[151,291,1804,1829]" class="Pinopsida" family="Cupressaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Coniferophyta" rank="family">Taxodiaceae</taxonomicName>
(
<emphasis id="D181F778475FFFD9C637E010FD88F8F6" box="[311,605,1804,1829]" italics="true" pageId="14" pageNumber="415">
<taxonomicName id="24F550E9475FFFD9C637E010FE35F8F6" box="[311,480,1804,1829]" class="Pinopsida" family="Cupressaceae" genus="Cunninghamia" kingdom="Fungi" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="genus">Cunninghamia</taxonomicName>
, 
<taxonomicName id="24F550E9475FFFD9C6F0E010FD88F8F6" box="[496,605,1804,1829]" class="Pinopsida" family="Cupressaceae" genus="Taxodium" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="genus">Taxodium</taxonomicName>
</emphasis>
). It is recorded in 
<collectingCountry id="9BE26BFA475FFFD9C442E011FCBAF8F7" box="[834,879,1805,1828]" name="Iran" pageId="14" pageNumber="415">Iran</collectingCountry>
from 
<taxonomicName id="24F550E9475FFFD9C4BCE011FB1AF8F7" box="[956,1231,1805,1828]" class="Pinopsida" family="Cupressaceae" genus="Cupressus" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="species" species="sempervirens">
<emphasis id="D181F778475FFFD9C4BCE011FB1AF8F7" box="[956,1231,1805,1828]" italics="true" pageId="14" pageNumber="415">Cupressus sempervirens</emphasis>
</taxonomicName>
<taxonomicName id="24F550E9475FFFD9C3DBE011FA40F8F6" box="[1243,1429,1804,1829]" pageId="14" pageNumber="415" rank="variety" variety="horizontalis">
var. 
<emphasis id="D181F778475FFFD9C20FE010FA40F8F6" box="[1295,1429,1804,1829]" italics="true" pageId="14" pageNumber="415">horizontalis</emphasis>
</taxonomicName>
, 
<taxonomicName id="24F550E9475FFFD9C797E02DFED0F89B" box="[151,261,1841,1864]" class="Pinopsida" family="Cupressaceae" genus="Juniperus" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F778475FFFD9C797E02DFED0F89B" box="[151,261,1841,1864]" italics="true" pageId="14" pageNumber="415">Juniperus</emphasis>
</taxonomicName>
sp. and 
<taxonomicName id="24F550E9475FFFD9C664E033FD8FF89B" box="[356,602,1839,1864]" class="Pinopsida" family="Cupressaceae" genus="Platycladus" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="species" species="orientalis">
<emphasis id="D181F778475FFFD9C664E033FD8FF89B" box="[356,602,1839,1864]" italics="true" pageId="14" pageNumber="415">Platycladus orientalis</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E9475FFFD9C569E02CFCD0F89B" box="[617,773,1840,1864]" class="Pinopsida" family="Cupressaceae" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="415" phylum="Tracheophyta" rank="family">Cupressaceae</taxonomicName>
) (
<bibRefCitation id="8764569B475FFFD9C41CE033FC3DF89B" author="Amini" box="[796,1000,1839,1864]" pageId="14" pageNumber="415" refString="Amini, S., Hosseini, R. &amp; Sohani, M. M. (2013) A faunal study of bark beetles (Coleoptera: Curculionidae: Scolytinae) in Guilan province in North of Iran. Entomofauna, 34, 169 - 176." type="journal article" year="2013">
Amini 
<emphasis id="D181F778475FFFD9C46CE02DFC4BF89B" box="[876,926,1839,1864]" italics="true" pageId="14" pageNumber="415">et al</emphasis>
. 
<number id="553D394D475FFFD9C4ACE02CFC3DF89B" box="[940,1000,1840,1864]" pageId="14" pageNumber="415" value="2013.0">2013</number>
</bibRefCitation>
; 
