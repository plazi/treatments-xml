<taxonomicName id="3CA613C0FFEBFF894EBCFBB7FDA0FBD8" authority="Womersley, 1956" authorityName="Womersley" authorityYear="1956" box="[223,590,1040,1064]" class="Arachnida" family="Laelapidae" genus="Gymnolaelaps" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="23" phylum="Arthropoda" rank="species" species="australicus">
