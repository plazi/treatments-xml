<geoCoordinate id="18FA5079FFB1ED03769698F2FE5E50E0" box="[410,493,1437,1458]" degrees="107" direction="east" minutes="47" orientation="longitude" pageId="5" pageNumber="168" precision="925" value="107.78333">107°47'E</geoCoordinate>
