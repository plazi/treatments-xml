<number id="3DFD24994E38FFFE97C10035FE8A47F8" box="[222,281,619,643]" pageId="31" pageNumber="232" value="8.3">8, 30</number>
, 
<number id="3DFD24994E38FFFE963B0035FEFF47F8" box="[292,364,618,643]" pageId="31" pageNumber="232" value="49.68">49, 68</number>
, 86, 114, 132, 176– 
<number id="3DFD24994E38FFFE95540034FDE647F8" box="[587,629,618,643]" pageId="31" pageNumber="232" value="179.0">179</number>
