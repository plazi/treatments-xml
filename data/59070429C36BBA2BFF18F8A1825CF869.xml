<emphasis id="E3DA692DC36BBA2BFF18F8A18211F8F8" bold="true" box="[151,781,1819,1846]" pageId="23" pageNumber="24">
<taxonomicName id="16AECEBCC36BBA2BFF18F8A180E2F8FB" authority="(Cockerell, 1929: 329)" baseAuthorityName="Cockerell" baseAuthorityPageNumber="329" baseAuthorityYear="1929" box="[151,510,1819,1846]" class="Insecta" family="Dactylopiidae" genus="Dactylopius" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="greeni">
<emphasis id="E3DA692DC36BBA2BFF18F8A181FFF8F8" bold="true" box="[151,227,1820,1846]" italics="true" pageId="23" pageNumber="24">greeni</emphasis>
(Cockerell, 1929: 329)
</taxonomicName>
, 
<taxonomicName id="16AECEBCC36BBA2BFD83F8A18211F8F8" authority="" box="[524,781,1820,1846]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="subGenus" subGenus="Dactylopius">
