<taxonomicName id="4E16073F67C852D33654888F7D2A0049" LSID-ZBK="urn:lsid:zoobank.org:act:0C362EB6-9EA5-4AE0-AC17-00853786187C" class="Elasmobranchii" family="Urolophidae" genus="Urolophus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Urolophus Müller &amp; Henle 1837:117" order="Myliobatiformes" pageNumber="41" phylum="Chordata" rank="genus">Urolophus Mueller &amp; Henle</taxonomicName>
