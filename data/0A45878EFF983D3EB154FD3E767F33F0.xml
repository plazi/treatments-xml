<number id="342424BFFF983D3CB11DFBCD730C365F" box="[321,336,1119,1145]" pageId="5" pageNumber="36" value="5.0">5</number>
(
<number id="342424BFFF983D3CB13FFBCD73C8365F" box="[355,404,1119,1145]" pageId="5" pageNumber="36" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
