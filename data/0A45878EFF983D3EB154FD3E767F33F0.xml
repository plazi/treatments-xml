<number id="342424BFFF983D3CB133FC2D73D031FF" box="[367,396,959,985]" pageId="5" pageNumber="36" value="15.0">15</number>
(
<number id="342424BFFF983D3CB1FEFC2D73B031FF" box="[418,492,959,985]" pageId="5" pageNumber="36" value="14.5" valueMax="15.0" valueMin="14.0">14–15</number>
) apart, scapular setae (
<emphasis id="B098EA8AFF983D3CB34DFC2D717531FE" box="[785,809,959,984]" italics="true" pageId="5" pageNumber="36">sc</emphasis>
) 
<number id="342424BFFF983D3CB31CFC2D711231FF" box="[832,846,959,985]" pageId="5" pageNumber="36" value="4.0">4</number>
(
<number id="342424BFFF983D3CB339FC2D71C931FF" box="[869,917,959,985]" pageId="5" pageNumber="36" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
), projecting mediad. 
<emphasis id="B098EA8AFF983D3CB4C1FC2C777731FE" bold="true" box="[1181,1323,958,984]" pageId="5" pageNumber="36">Coxal area</emphasis>
smooth; anterolateral setae on coxisternum (
<emphasis id="B098EA8AFF983D3CB37CFC7571613626" box="[800,829,999,1024]" italics="true" pageId="5" pageNumber="36">
<number id="342424BFFF983D3CB37CFC7571713626" box="[800,813,999,1024]" pageId="5" pageNumber="36" value="1.0">1</number>
b
