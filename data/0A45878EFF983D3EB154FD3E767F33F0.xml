) 
<number id="342424BFFF983D3CB11FFB9D733C360F" box="[323,352,1039,1065]" pageId="5" pageNumber="36" value="15.0">15</number>
(
<number id="342424BFFF983D3CB12FFB9D739C360F" box="[371,448,1039,1065]" pageId="5" pageNumber="36" value="15.5" valueMax="17.0" valueMin="14.0">14–17</number>
), proximal setae on coxisternum (
<emphasis id="B098EA8AFF983D3CB326FB9D71CB360E" box="[890,919,1039,1064]" italics="true" pageId="5" pageNumber="36">
<number id="342424BFFF983D3CB326FB9D71D4360E" box="[890,904,1039,1064]" pageId="5" pageNumber="36" value="2.0">2</number>
a
</emphasis>
) 
<number id="342424BFFF983D3CB3F7FB9D7194360F" box="[939,968,1039,1065]" pageId="5" pageNumber="36" value="30.0">30</number>
(
<number id="342424BFFF983D3CB386FB9D7675360F" box="[986,1065,1039,1065]" pageId="5" pageNumber="36" value="30.0" valueMax="32.0" valueMin="28.0">28–32</number>
