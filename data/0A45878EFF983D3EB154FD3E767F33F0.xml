(
<number id="342424BFFF983D3CB2ECFB4570BC36D7" box="[688,736,1239,1265]" pageId="5" pageNumber="36" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
), antaxial genual setae (
<emphasis id="B098EA8AFF983D3CB45CFB45764936D6" box="[1024,1045,1239,1264]" italics="true" pageId="5" pageNumber="36">l''</emphasis>
