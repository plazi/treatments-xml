<number id="342424BFFF983D3CB2D4FCFD70F931AF" box="[648,677,879,905]" pageId="5" pageNumber="36" value="15.0">15</number>
(
<number id="342424BFFF983D3CB2EAFCFD715F31AF" box="[694,771,879,905]" pageId="5" pageNumber="36" value="15.5" valueMax="17.0" valueMin="14.0">14–17</number>
), projecting obliquely down. 
<emphasis id="B098EA8AFF983D3CB402FCFC777731AE" bold="true" box="[1118,1323,878,904]" pageId="5" pageNumber="36">Prodorsal shield</emphasis>
<number id="342424BFFF983D3CB154FC0573793197" box="[264,293,919,945]" pageId="5" pageNumber="36" value="45.0">45</number>
(
<number id="342424BFFF983D3CB166FC0573D53197" box="[314,393,919,945]" pageId="5" pageNumber="36" value="44.5" valueMax="46.0" valueMin="43.0">43–46</number>
), 
<number id="342424BFFF983D3CB1FFFC05739C3197" box="[419,448,919,945]" pageId="5" pageNumber="36" value="45.0">45</number>
(
<number id="342424BFFF983D3CB189FC05707E3197" box="[469,546,919,945]" pageId="5" pageNumber="36" value="44.5" valueMax="46.0" valueMin="43.0">43–46</number>
