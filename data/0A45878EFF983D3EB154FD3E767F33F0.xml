FEMALE: (n = 
<number id="342424BFFF983D3CB1A4FCD5705B3147" box="[504,519,839,865]" pageId="5" pageNumber="36" value="5.0">5</number>
) Body fusiform, 
<number id="342424BFFF983D3CB28BFCD5715F3147" box="[727,771,839,865]" pageId="5" pageNumber="36" value="165.0">165</number>
(
<number id="342424BFFF983D3CB348FCD571213147" box="[788,893,839,865]" pageId="5" pageNumber="36" value="166.5" valueMax="171.0" valueMin="162.0">162–171</number>
