<taxonomicName id="45EC4D1BFF9A3D3EB2CBFED676DA3378" authority="Huang, 2001" authorityName="Huang" authorityYear="2001" box="[663,1158,324,350]" class="Arachnida" family="Eriophyidae" genus="Calepitrimerus" kingdom="Animalia" order="Prostigmata" pageId="7" pageNumber="38" phylum="Arthropoda" rank="species" species="similisvas">
