(
<number id="342424BFFF983D3CB331FC7571C03627" box="[877,924,999,1025]" pageId="5" pageNumber="36" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), proximal setae on coxisternum (
<emphasis id="B098EA8AFF983D3CB14EFB9D7373360E" box="[274,303,1039,1064]" italics="true" pageId="5" pageNumber="36">
<number id="342424BFFF983D3CB14EFB9D7343360E" box="[274,287,1039,1064]" pageId="5" pageNumber="36" value="1.0">1</number>
a
