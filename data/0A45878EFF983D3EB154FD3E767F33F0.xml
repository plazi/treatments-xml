(
<number id="342424BFFF983D3CB43FFB3D76CE36EF" box="[1123,1170,1199,1225]" pageId="5" pageNumber="36" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), basiventral femoral setae (
<emphasis id="B098EA8AFF983D3CB1E5FB45738936D6" box="[441,469,1239,1264]" italics="true" pageId="5" pageNumber="36">bv</emphasis>
) 
<number id="342424BFFF983D3CB1BBFB4573AA36D7" box="[487,502,1239,1265]" pageId="5" pageNumber="36" value="4.0">4</number>
(
<number id="342424BFFF983D3CB25BFB45706B36D7" box="[519,567,1239,1265]" pageId="5" pageNumber="36" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
); genu 
