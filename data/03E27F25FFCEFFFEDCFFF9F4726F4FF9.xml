<taxonomicName id="4C4BB5B0FFCEFFFFDFCEF8397510488F" authority="Rees, 1957 a" authorityName="Rees" authorityYear="1957" box="[934,1282,1938,1964]" class="Hydrozoa" family="Hydrocorynidae" kingdom="Animalia" order="Anthoathecata" pageId="48" pageNumber="49" phylum="Cnidaria" rank="family">
