: 
<taxonomicName authority="Medik." authorityName="Medik." box="[403,689,826,853]" class="Magnoliopsida" family="Fabaceae" genus="Apios" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="203" phylum="Tracheophyta" rank="species" species="americana">
<emphasis box="[403,596,827,853]" italics="true" pageId="2" pageNumber="203">Apios americana</emphasis>
Medik.
</taxonomicName>
, 
<taxonomicName box="[702,959,826,852]" class="Magnoliopsida" family="Fabaceae" genus="Desmodium" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="203" phylum="Tracheophyta" rank="species" species="canescens">
<emphasis box="[702,959,826,852]" italics="true" pageId="2" pageNumber="203">Desmodium canescens</emphasis>
