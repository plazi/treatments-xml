<emphasis bold="true" box="[199,273,153,179]" pageId="14" pageNumber="215">Hosts.</emphasis>
<taxonomicName box="[280,432,153,179]" class="Magnoliopsida" family="Cannabaceae" kingdom="Plantae" order="Rosales" pageId="14" pageNumber="215" phylum="Tracheophyta" rank="family">Cannabaceae</taxonomicName>
: 
<taxonomicName authority="Willd." authorityName="Willd." box="[443,698,153,179]" class="Magnoliopsida" family="Cannabaceae" genus="Celtis" kingdom="Plantae" order="Rosales" pageId="14" pageNumber="215" phylum="Tracheophyta" rank="species" species="laevigata">
<emphasis box="[443,619,153,179]" italics="true" pageId="14" pageNumber="215">Celtis laevigata</emphasis>
Willd.
</taxonomicName>
, 
<taxonomicName box="[709,877,153,179]" class="Magnoliopsida" family="Cannabaceae" genus="Celtis" kingdom="Plantae" order="Rosales" pageId="14" pageNumber="215" phylum="Tracheophyta" rank="species" species="occidentalis">
