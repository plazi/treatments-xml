(
<bibRefCitation author="Handfield, L." box="[1145,1326,404,431]" pageId="12" pageNumber="213" pagination="1 - 155" refId="ref14377" refString="Handfield, L. (1997) Liste des Lepidopteres du Quebec et du Labrador. Fabreries, Supplement 7, 1 - 155." type="journal article" year="1997">Handfield 1997</bibRefCitation>
