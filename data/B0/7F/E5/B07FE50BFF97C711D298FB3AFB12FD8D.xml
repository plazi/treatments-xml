<taxonomicName authority="I.C. Thompson" authorityName="I.C. Thompson" box="[352,726,1645,1668]" class="Hexanauplia" family="Paracalanidae" genus="Mecynocera" higherTaxonomySource="CoL" kingdom="Animalia" order="Calanoida" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="clausi">
<emphasis box="[352,555,1645,1668]" italics="true" pageId="2" pageNumber="61">
Mecynocera 
<taxonomicName authority="I.C. Thompson" authorityName="I.C. Thompson" box="[491,555,1645,1668]" pageId="2" pageNumber="61" rank="species" species="clausi">clausi</taxonomicName>
