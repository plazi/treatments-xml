<taxonomicName authorityName="Yakovlev &amp; Witt" authorityYear="2016" box="[929,1087,295,320]" class="Insecta" family="Cossidae" genus="Phragmataecia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="593" phylum="Arthropoda" rank="species" species="valikhanovi">
