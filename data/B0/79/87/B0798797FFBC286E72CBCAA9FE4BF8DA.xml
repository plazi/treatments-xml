, early 
<location LSID="urn:lsid:plazi:treatment:B0798797FFBC286E72CBCAA9FE4BF8DA:3D0F605AFFBC286E766ACB3EFB15FAF8" box="[1078,1179,1345,1370]" country="Argentina" name="Miocene" pageId="6" pageNumber="201" stateProvince="Santa Cruz">Miocene</location>
, 
