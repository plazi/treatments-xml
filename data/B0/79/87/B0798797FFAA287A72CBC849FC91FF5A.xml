<taxonomicName authorityName="Ameghino" authorityYear="1894" box="[1108,1333,1509,1531]" class="Mammalia" family="Interatheriidae" genus="Icochilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Notoungulata" pageId="17" pageNumber="212" phylum="Chordata" rank="species" species="crassiramis">
<emphasis bold="true" box="[1108,1333,1509,1531]" italics="true" pageId="17" pageNumber="212">Icochilus crassiramis</emphasis>
