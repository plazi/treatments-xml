<subSubSection lastPageId="20" lastPageNumber="21" pageId="19" pageNumber="20" type="diagnosis">
<paragraph blockId="19.[120,1225,1508,1759]" box="[168,858,1508,1535]" pageId="19" pageNumber="20">
DIAGNOSIS: See Diagnosis of 
<emphasis box="[515,775,1508,1534]" italics="true" pageId="19" pageNumber="20">
<taxonomicName authorityName="Toro" authorityYear="1976" box="[515,771,1508,1534]" class="Insecta" family="Apidae" genus="Chilimalopsis" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="parvula">Chilimalopsis parvula</taxonomicName>
