................................................................................................................................................................ 
<taxonomicName authorityName="A.Touw" authorityYear="2001" baseAuthorityName="Mitt." box="[1273,1436,344,368]" class="Bryopsida" family="Thuidiaceae" genus="Pelekium" kingdom="Plantae" order="Hypnales" pageId="4" pageNumber="64" phylum="Bryophyta" rank="species" species="contortulum">
