<paragraph pageId="253" pageNumber="254">
<taxonomicName class="Insecta" family="Scarabaeidae" genus="Eucranium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucranium cyclosoma" order="Coleoptera" pageId="253" pageNumber="254" phylum="Arthropoda" rank="species" species="cyclosoma">Eucranium cyclosoma</taxonomicName>
