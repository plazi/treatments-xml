<location LSID="urn:lsid:plazi:treatment:B007F528FFCEFFC316D7FC2AFE465C2D:3D7112E5FFCEFFC3126CFBBAFBA65D4E" box="[1068,1140,1051,1077]" name="Catete" pageId="22" pageNumber="23" stateProvince="Bengo">Catete</location>
[-9.11667º, 13.70000º, 
<elevation metricMagnitude="1" metricUnit="m" metricValue="2.1" pageId="22" pageNumber="23" unit="m" value="21.0">
