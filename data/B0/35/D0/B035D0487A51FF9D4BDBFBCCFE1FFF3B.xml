<emphasis box="[1313,1435,1965,1990]" italics="true" pageId="8" pageNumber="9">L. aldretei</emphasis>
Badonnel
</taxonomicName>
(
<collectingCountry box="[280,403,2000,2026]" name="Colombia" pageId="8" pageNumber="9">Colombia</collectingCountry>
), 
<taxonomicName authority="Garcia Aldrete" authorityName="Garcia Aldrete" box="[423,838,2000,2026]" class="Insecta" family="Lachesillidae" genus="Lachesilla" kingdom="Animalia" order="Psocodea" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="assymetriproctus">
<emphasis box="[423,655,2000,2025]" italics="true" pageId="8" pageNumber="9">L. assymetriproctus</emphasis>
García Aldrete
</taxonomicName>
(
<collectingCountry box="[855,915,2000,2026]" name="Peru" pageId="8" pageNumber="9">Peru</collectingCountry>
), 
<taxonomicName authority="Garcia Aldrete" authorityName="Garcia Aldrete" box="[936,1315,2000,2026]" class="Insecta" family="Lachesillidae" genus="Lachesilla" kingdom="Animalia" order="Psocodea" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="convexicornis">
<emphasis box="[936,1131,2000,2025]" italics="true" pageId="8" pageNumber="9">L. convexicornis</emphasis>
García Aldrete
</taxonomicName>
(
<collectingCountry box="[1331,1391,2000,2026]" name="Peru" pageId="8" pageNumber="9">Peru</collectingCountry>
), 
<emphasis box="[1412,1436,2000,2025]" italics="true" pageId="8" pageNumber="9">L.</emphasis>
