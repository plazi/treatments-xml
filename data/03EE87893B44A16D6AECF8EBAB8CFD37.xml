</emphasis>
and 
<taxonomicName id="4C474D1C3B43A16C69E8F8BDADFFF8A9" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[915,1020,1792,1817]" class="Arachnida" family="Prodidomidae" genus="Eleleis" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="crinita" status="sp. nov.">
