<typeStatus id="54EF8FB9A351FFA5FF05ABF2CF4153C9" box="[199,258,727,752]" pageId="11" pageNumber="119">Type</typeStatus>
material. 
<typeStatus id="54EF8FB9A351FFA5FEBEABF2CFAA53C9" box="[380,489,727,752]" pageId="11" pageNumber="119">Holotype</typeStatus>
</emphasis>
, 
<number id="3D9C233CA351FFA5FE34ABFDCC4753C9" box="[502,516,728,752]" pageId="11" pageNumber="119" value="1.0">1</number>
♀: 
<emphasis id="B920ED09A351FFA5FDEDABF2CC2C53C9" bold="true" box="[559,623,727,752]" pageId="11" pageNumber="119">
<collectingCountry id="F343718BA351FFA5FDEDABF2CC2C53C9" box="[559,623,727,752]" name="Chile" pageId="11" pageNumber="119">Chile</collectingCountry>
</emphasis>
, La Raya, 
<quantity id="4CAC9CFEA351FFA5FD2BABFDCD0353D6" box="[745,832,728,752]" metricMagnitude="3" metricUnit="m" metricValue="4.3" pageId="11" pageNumber="119" unit="m" value="4300.0">4300 m</quantity>
, 
<date id="FFEA17DBA351FFA5FC8EABF2CD8053C9" box="[844,963,727,752]" pageId="11" pageNumber="119" value="1950-02-21">
