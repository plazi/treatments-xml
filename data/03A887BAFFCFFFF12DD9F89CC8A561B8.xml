<taxonomicName id="4C014D2FFFD0FFF22F63F8F9C8416624" baseAuthorityName="Klug" baseAuthorityYear="1842" box="[557,793,1839,1862]" class="Insecta" family="Cleridae" genus="Iontoclerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="humeralis">
