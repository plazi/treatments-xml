<subSubSection id="4893652E4D51797F978B17384D58DB0A" box="[197,1142,1125,1151]" pageId="2" pageNumber="49" type="etymology">
<paragraph id="003636A54D51797F978B17384D58DB0A" blockId="2.[151,1437,152,1338]" box="[197,1142,1125,1151]" pageId="2" pageNumber="49">
<emphasis id="32FDEAB74D51797F978B1738487CDB0A" bold="true" box="[197,338,1125,1151]" pageId="2" pageNumber="49">Etymology.</emphasis>
