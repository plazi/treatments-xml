<geoCoordinate id="37773BEA2C208C7C98FF102164CABF69" degrees="61" direction="north" minutes="4" orientation="latitude" precision="1" seconds="50.4" value="61.080666">61°4'50.4&quot;N</geoCoordinate>
, 
<geoCoordinate id="E1C655B847B987A3C4C60F07DC80F2AE" degrees="69" direction="east" minutes="28" orientation="longitude" precision="1" seconds="51.9" value="69.48109">69°28'51.9&quot;E</geoCoordinate>
) and Kabaniy stream (
<geoCoordinate id="A39374D71846A0141C89DFDF3298CBD9" degrees="60" direction="north" minutes="54" orientation="latitude" precision="15" seconds="4" value="60.90111">60°54'4&quot;N</geoCoordinate>
, 
<geoCoordinate id="6CFECC3F4FB3C76CF205E77D1E5D49DD" degrees="68" direction="east" minutes="42" orientation="longitude" precision="15" seconds="55" value="68.71527">68°42'55&quot;E</geoCoordinate>
).
</paragraph>
</subSubSection>
