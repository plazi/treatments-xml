<paragraph id="B895DD81D629329CE17EAE2173F8F69D" pageId="0" pageNumber="115">
Genus 
<taxonomicName id="2404C23F1A2BBB8A0A640A85FAD6E1F5" LSID="78853A0E-0EF8-5119-8908-87A4A3E15C01" authority="Ren, Lu, Guo &amp; Ji, 1995" authorityName="Ren, Lu, Guo &amp; Ji" authorityYear="1995" class="Insecta" family="Pamphilioidea" genus="Jibaissodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Jibaissodes" order="Hymenoptera" pageId="0" pageNumber="115" phylum="Arthropoda" rank="genus">Jibaissodes Ren, Lu, Guo &amp; Ji, 1995</taxonomicName>
