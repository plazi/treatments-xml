<collectingRegion id="49F815F7A3607F70FD3CF9098842DFF0" box="[716,798,1703,1728]" country="Sri Lanka" name="Central" pageId="13" pageNumber="314">Central</collectingRegion>
<collectingCountry id="F32B9B85A3607F70FCD6F9098830DFF0" box="[806,876,1703,1728]" name="Nepal" pageId="13" pageNumber="314">Nepal</collectingCountry>
, at an altitude of 
<quantity id="4CC476F0A3607F70FBDCF9068FD8DF8F" box="[1068,1156,1704,1728]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="13" pageNumber="314" unit="m" value="2300.0">2300 m</quantity>
.
