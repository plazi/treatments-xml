<taxonomicName id="259875F96E475B50FCDCF8417D96C78F" authorityName="Lencioni" authorityYear="2008" box="[860,1006,1930,1955]" class="Insecta" family="Coenagrionidae" genus="Angelagrion" kingdom="Animalia" order="Odonata" pageId="6" pageNumber="29" phylum="Arthropoda" rank="species" species="fredericoi">
<emphasis id="D0ECD2686E475B50FCDCF8417D96C78F" box="[860,1006,1930,1955]" italics="true" pageId="6" pageNumber="29">A. fredericoi</emphasis>
</taxonomicName>
varying from 30–31, and of 
<taxonomicName id="259875F96E475B50FABEF8417EAAC7E7" authorityName="Lencioni" authorityYear="2008" class="Insecta" family="Coenagrionidae" genus="Angelagrion" kingdom="Animalia" order="Odonata" pageId="6" pageNumber="29" phylum="Arthropoda" rank="species" species="nathaliae">
