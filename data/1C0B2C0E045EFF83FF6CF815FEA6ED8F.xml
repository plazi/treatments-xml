<typeStatus id="4B1923BA045EFF83FEE8F860FE47E81F" box="[275,389,866,892]" pageId="3" pageNumber="58">Holotype</typeStatus>
</emphasis>
: male, 
<collectingCountry id="ECB5DD88045EFF83FE26F860FDE4E81F" box="[477,550,866,892]" name="China" pageId="3" pageNumber="58">China</collectingCountry>
, Yunnan, Tengchong, Zhengding, 
<quantity id="535A30FD045EFF83FC42F860FBCFE81F" box="[953,1037,866,892]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="3" pageNumber="58" unit="m" value="1800.0">1800m</quantity>
, 
<date id="E01CBBD8045EFF83FBE1F860FB79E81F" box="[1050,1211,866,892]" pageId="3" pageNumber="58" value="2006-08-15">
<number id="226A8F3F045EFF83FBE1F860FBF7E81F" box="[1050,1077,866,892]" pageId="3" pageNumber="58" value="15.0">15</number>
-VIII- 
