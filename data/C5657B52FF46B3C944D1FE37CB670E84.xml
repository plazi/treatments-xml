<quantity id="8A3467A1FF46B3C94006FBE1CC8A0F13" box="[1088,1164,1064,1088]" metricMagnitude="2" metricUnit="m" metricValue="3.9" pageId="218" pageNumber="219" unit="m" value="390.0">
<elevation id="C6E12D77FF46B3C94006FBE1CC8A0F13" box="[1088,1164,1064,1088]" metricMagnitude="2" metricUnit="m" metricValue="3.9" pageId="218" pageNumber="219" unit="m" value="390.0">390 m</elevation>
</quantity>
but usually subtidal in 
<collectingRegion id="8F0804A6FF46B3C944D1FB85C9910F48" box="[151,407,1100,1124]" isEnumeration="true" pageId="218" pageNumber="219">California and Oregon</collectingRegion>
</materialsCitation>
