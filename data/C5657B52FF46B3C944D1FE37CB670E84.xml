<location id="48139C9FFF46B3C947EAFBB9CC350FA4" LSID="urn:lsid:plazi:treatment:C5657B52FF46B3C944D1FE37CB670E84:48139C9FFF46B3C947EAFBB9CC350FA4" box="[940,1075,1136,1160]" country="Japan" county="Point Arena" municipality="Monterey Bay" name="Bristol Bay" pageId="218" pageNumber="219" stateProvince="Chiba">Bristol Bay</location>
