6. 
<taxonomicName id="F9E2ABED49F84C0F66E7131D0B7E9694" LSID="http://zoobank.org/F7AFE612-F2D1-4AED-A5C4-3B0212B6F5CE" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus analis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="analis">Trigonopterus analis Riedel</taxonomicName>
