<quantity id="075AC4DCFF84127A9DBAFEF5FDA00E11" box="[439,541,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="7.18" pageId="2" pageNumber="437" unit="mm" value="7.18">7.18 mm</quantity>
