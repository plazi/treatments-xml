<quantity id="075AC4DCFF84127A9F1FFEF5FCC50E11" box="[786,888,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="2.88" pageId="2" pageNumber="437" unit="mm" value="2.88">2.88 mm</quantity>
, EW: 
<quantity id="075AC4DCFF84127A9FB0FEF5FB9E0E11" box="[957,1059,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="3.62" pageId="2" pageNumber="437" unit="mm" value="3.62">3.62 mm</quantity>
, ED: 
<quantity id="075AC4DCFF84127A986EFEF5FB770E11" box="[1123,1226,259,286]" metricMagnitude="-4" metricUnit="m" metricValue="4.3" pageId="2" pageNumber="437" unit="mm" value="0.43">0.43 mm</quantity>
