species: 
<taxonomicName id="8DEE4D50BF41FFE5FF24F1B16B446391" authority="Ghauri &amp; Ghauri, 1983" authorityName="Ghauri &amp; Ghauri" authorityYear="1983" box="[199,570,466,490]" class="Insecta" family="Miridae" genus="Totta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="169" phylum="Arthropoda" rank="species" species="zaherii">
<emphasis id="789AEAC1BF41FFE5FF24F1B168396391" box="[199,327,466,490]" italics="true" pageId="18" pageNumber="169">Totta zaherii</emphasis>
<bibRefCitation id="2E7F4B22BF41FFE5FEAEF1B06B446391" author="Ghauri, M. S. K. &amp; Ghauri, F. Y. K." box="[333,570,466,490]" pageId="18" pageNumber="169" pagination="19 - 25" refId="ref18274" refString="Ghauri, M. S. K. &amp; Ghauri, F. Y. K., (1983) A new genus and a new species of Isometopidae from North India, with a key to world genera. Reichenbachia, 21 (3), 19 - 25." type="journal article" year="1983">Ghauri &amp; Ghauri, 1983</bibRefCitation>
</taxonomicName>
from 
<collectingCountry id="32F97643BF41FFE5FD9BF1B06BD16391" box="[632,687,466,490]" name="India" pageId="18" pageNumber="169">India</collectingCountry>
