<taxonomicName id="4C1CBCF5FFB6FFBDFEC381DAFE47383F" authorityName="Walker" authorityYear="1843" box="[347,469,1773,1794]" class="Insecta" family="Eulophidae" genus="Horismenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="178" phylum="Arthropoda" rank="genus">
