</taxonomicName>
spp. 34–36. 1 
<superScript id="7C696A3EFFB6FFBDFDFF81DEFDE239C8" attach="left" box="[615,624,1769,1781]" fontSize="5" pageId="10" pageNumber="178">st</superScript>
funicular segment, female; 34, 
<taxonomicName id="4C1CBCF5FFB6FFBDFC2E81DAFB4D383F" authority="Hansson &amp; Aebi" authorityName="Hansson &amp; Aebi" box="[950,1247,1772,1794]" class="Insecta" family="Eulophidae" genus="Horismenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="178" phylum="Arthropoda" rank="species" species="butcheri">
<emphasis id="B9681B64FFB6FFBDFC2E81DAFBB8383F" box="[950,1066,1772,1794]" italics="true" pageId="10" pageNumber="178">H. butcheri</emphasis>
Hansson &amp; Aebi
</taxonomicName>
; 35, 
<taxonomicName id="4C1CBCF5FFB6FFBDFA8A81DAFAE5383F" authority="Hansson &amp; Kenyon &amp; Benrey, 2019" authorityName="Hansson &amp; Kenyon &amp; Benrey" authorityYear="2019" box="[1298,1399,1772,1794]" class="Insecta" family="Eulophidae" genus="Horismenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="178" phylum="Arthropoda" rank="species" species="dennoi" status="sp. nov.">
<emphasis id="B9681B64FFB6FFBDFA8A81DAFAE5383F" box="[1298,1399,1772,1794]" italics="true" pageId="10" pageNumber="178">H. dennoi</emphasis>
</taxonomicName>
<emphasis id="B9681B64FFB6FFBDFAE581DBFF51381D" bold="true" pageId="10" pageNumber="178">
<taxonomicNameLabel id="A25BA61FFFB6FFBDFAE581DBFF51381D" pageId="10" pageNumber="178" rank="species">sp. nov.</taxonomicNameLabel>
