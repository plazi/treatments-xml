<taxonomicName id="4C1AF7C1FFBFFF8EDA13FBB04062FB80" box="[292,605,1123,1146]" class="Malacostraca" family="Odiidae" genus="Odius" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="42" phylum="Arthropoda" rank="subSpecies" species="cassigerous" status="ssp. n." subSpecies="ochoticus">
