<taxonomicName id="25CC7F5FA62C194C31BD6D6BA9FCB21A" authorityName="Kaltenbach &amp; Garces &amp; Gattolliat" authorityYear="2020" class="Insecta" family="Baetidae" genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis lachicae" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="lachicae">
