<taxonomicName id="D6F4CE96FFB4E863FF6CFB2D2F55B4D0" box="[160,317,1186,1210]" class="Magnoliopsida" family="Myristicaceae" kingdom="Plantae" order="Magnoliales" pageId="12" pageNumber="173" phylum="Tracheophyta" rank="family">Myristicaceae</taxonomicName>
