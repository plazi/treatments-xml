<taxonomicName id="D8919351A650B440E11191F8626AD23B" LSID="A6D326A4-1300-B9B9-42CE-153C2BC83F1A" authority="(Pfeiffer, 1862)" baseAuthorityName="Pfeiffer" baseAuthorityYear="1862" class="Gastropoda" family="Pupinidae" genus="Pollicaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Pollicaria mouhoti" order="Architaenioglossa" pageId="11" pageNumber="30" phylum="Mollusca" rank="species" species="mouhoti">
