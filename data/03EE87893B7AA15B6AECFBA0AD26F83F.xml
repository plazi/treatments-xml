<taxonomicName id="4C474D1C3B7AA15568CEFA73AD23FA57" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[693,800,1485,1511]" class="Arachnida" family="Prodidomidae" genus="Purcelliana" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="khabus">
