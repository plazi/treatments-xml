<emphasis id="B94CEAE6FB0FFFF6D054F948FE7AF974" bold="true" box="[317,430,1703,1724]" pageId="12" pageNumber="133">Figs 25–31.</emphasis>
<taxonomicName id="4C384D77FB0FFFF6D0DBF947FC8DF974" authority="VANSCHUYTBROECK" authorityName="VANSCHUYTBROECK" box="[434,857,1704,1725]" class="Insecta" family="Sphaeroceridae" genus="Opacifrons" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="133" phylum="Arthropoda" rank="species" species="rubrifrons">
