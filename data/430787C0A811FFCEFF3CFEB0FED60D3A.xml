<emphasis id="F9DAEAC4A811FFCEFEA6FDC7FE530B3D" bold="true" box="[307,494,530,551]" pageId="9" pageNumber="9">
<specimenCode id="9B089EADA811FFCEFEA6FDC7FE530B3D" box="[307,494,530,551]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="9" pageNumber="9">AMNH 715254</specimenCode>
