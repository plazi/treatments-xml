<taxonomicName id="D6F85B5A110E3CFF1D1F0DE8DFCD2C19" LSID="http://zoobank.org/AF02A670-1C21-49A3-A7F3-5B7C35F494C8" authority="Waterhouse, 1879" authorityName="Waterhouse" authorityYear="1879" class="Insecta" family="Lycidae" genus="Eniclases" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eniclases infuscatus" order="Coleoptera" pageId="6" pageNumber="21" phylum="Arthropoda" rank="species" species="infuscatus">Eniclases infuscatus</taxonomicName>
