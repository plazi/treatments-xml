<taxonomicName id="4C1741A1EB6DFFDEFE7BED77FDEDF8C1" box="[408,513,1862,1885]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="19" pageNumber="362" phylum="Mollusca" rank="species" species="surrentina">
