<taxonomicName id="4C1741A1EB6DFFDEFB87EDBCFA9AF839" box="[1124,1398,1933,1957]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="19" pageNumber="362" phylum="Mollusca" rank="species" species="surrentina" subGenus="Murella">
