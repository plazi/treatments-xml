<quantity id="4CBEE1EDE229FFAED199FC4CFEC6FC48" box="[251,326,983,1007]" metricMagnitude="2" metricUnit="m" metricValue="4.4" pageId="90" pageNumber="699" unit="m" value="440.0">440 m</quantity>
