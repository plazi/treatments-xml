<taxonomicName id="4C1CD3985A4C141C482FF8AF1FEBF899" authority="Deng et al., 2012" authorityName="Deng" authorityYear="2012" class="Insecta" family="Tetrigidae" genus="Macromotettixoides" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="564" phylum="Arthropoda" rank="species" species="undulatifemura">
