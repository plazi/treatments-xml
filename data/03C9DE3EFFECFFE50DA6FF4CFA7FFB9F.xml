<emphasis id="B914B33AFFECFFE509B1FE09FA7FFE62" bold="true" box="[1152,1436,409,431]" pageId="14" pageNumber="165">
<taxonomicName id="4C6014ABFFECFFE509B1FE09FA83FE62" authority="Triapitsyn, 2021" authorityName="Triapitsyn" authorityYear="2021" box="[1152,1376,409,431]" class="Insecta" family="Mymaridae" genus="Polynema" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="165" phylum="Arthropoda" rank="species" species="markiza" status="sp. n." subGenus="Polynema">
<emphasis id="B914B33AFFECFFE509B1FE09FB71FE62" bold="true" box="[1152,1170,410,431]" italics="true" pageId="14" pageNumber="165">P.</emphasis>
(
<emphasis id="B914B33AFFECFFE50991FE0AFAE0FE62" bold="true" box="[1184,1283,409,431]" italics="true" pageId="14" pageNumber="165">Polynema</emphasis>
