<emphasis id="B914B33AFFECFFE50969FEEFFA7FFE5C" bold="true" box="[1112,1436,380,402]" pageId="14" pageNumber="165">
<taxonomicName id="4C6014ABFFECFFE50969FEEFFA83FE5F" authority="Triapitsyn, 2021" authorityName="Triapitsyn" authorityYear="2021" box="[1112,1376,380,402]" class="Insecta" family="Mymaridae" genus="Polynema" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="165" phylum="Arthropoda" rank="species" species="aristokratka" status="sp. n." subGenus="Polynema">
