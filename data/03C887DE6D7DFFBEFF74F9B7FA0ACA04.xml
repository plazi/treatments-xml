<taxonomicName id="4C614D4B6D7EFFB0FEB9FAA2FE60CC97" box="[346,497,1395,1417]" class="Insecta" family="Chrysomelidae" genus="Amphimela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="species" species="raydahensis" status="sp. nov.">
