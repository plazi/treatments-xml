<quantity id="4C999B2D6D70FFBEFBE1FCD2FBDACA04" box="[1026,1099,772,794]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="8" pageNumber="438" unit="mm" value="3.3">3.3 mm</quantity>
........ 
<taxonomicName id="4C614D4B6D70FFBEFB48FCD2FA0ACA04" box="[1195,1435,772,794]" class="Insecta" family="Chrysomelidae" genus="Amphimela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="438" phylum="Arthropoda" rank="species" species="quadrinotata">
<emphasis id="B915EADA6D70FFBEFB48FCD2FA0ACA04" box="[1195,1435,772,794]" italics="true" pageId="8" pageNumber="438">Amphimela quadrinotata</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
