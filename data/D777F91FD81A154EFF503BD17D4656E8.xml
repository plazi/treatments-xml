<subSubSection id="17C41B82D81A1543FF0E39457D63500E" box="[201,1128,1842,1867]" pageId="7" pageNumber="90" type="etymology">
<paragraph id="5F614809D81A1543FF0E39457D63500E" blockId="7.[151,1436,1554,2010]" box="[201,1128,1842,1867]" pageId="7" pageNumber="90">
<emphasis id="6DAA941BD81A1543FF0E39457844500F" bold="true" box="[201,335,1843,1867]" inLineHeading="true" pageId="7" pageNumber="90" reason="1">Etymology.</emphasis>
