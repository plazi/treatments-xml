<taxonomicName id="4C5942EEFFFAFFABFE9ADF0FBF71DF6E" box="[356,542,1346,1369]" class="Insecta" family="Cleridae" genus="Apteropilo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="46" phylum="Arthropoda" rank="species" species="humerofuscus">
