<taxonomicName id="CBC74D321D484418A7D67433FD5A1B27" box="[496,669,264,289]" class="Malacostraca" family="Albuneidae" genus="Albunea" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="danai">
