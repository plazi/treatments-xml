<taxonomicName id="CBC74D321D484418A7D175D9FD0E1AFD" box="[503,713,226,251]" class="Malacostraca" family="Albuneidae" genus="Albunea" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="thurstoni">
