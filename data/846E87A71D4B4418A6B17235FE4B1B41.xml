<taxonomicName id="CBC74D321D484418A3A57433FE4F1B41" authority="Boyko 1999" authorityName="Boyko" authorityYear="1999" class="Malacostraca" family="Albuneidae" genus="Albunea" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="danai">
