<taxonomicName id="4C26D3D15846FF8081AF18B6FBF7FD1F" authorityName="Spence Bate" authorityYear="1861" box="[933,1082,655,681]" class="Malacostraca" family="Brachyscelidae" genus="Brachyscelus" kingdom="Animalia" order="Amphipoda" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
