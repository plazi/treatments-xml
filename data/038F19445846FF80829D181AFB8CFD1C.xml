<taxonomicName id="4C26D3D15846FF80812518A9FCB2FD1F" box="[815,895,656,681]" class="Malacostraca" family="Lycaeidae" genus="Lycaea" kingdom="Animalia" order="Amphipoda" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
