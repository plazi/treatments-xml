<taxonomicName id="FD904D49FF81FFC65CEDD602FBEAFCE3" baseAuthorityName="Austen" baseAuthorityYear="1893" box="[941,1103,776,800]" class="Insecta" family="Diptera (awaiting allocation)" genus="Asiobaccha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="2592" phylum="Arthropoda" rank="species" species="nubilipennis">
