<quantity id="A5393981395CFF942D43FAA142B7FA3F" box="[545,716,1404,1430]" metricMagnitude="-4" metricUnit="m" metricValue="4.75" metricValueMax="5.0" metricValueMin="4.5" pageId="6" pageNumber="7" unit="mm" value="0.475" valueMax="0.5" valueMin="0.45">0.45–0.50 mm</quantity>
