<quantity id="A5393981395CFF942CF8FA89443EFAC7" box="[922,1093,1364,1390]" metricMagnitude="-4" metricUnit="m" metricValue="5.75" metricValueMax="6.0" metricValueMin="5.5" pageId="6" pageNumber="7" unit="mm" value="0.575" valueMax="0.6" valueMin="0.55">0.55–0.60 mm</quantity>
