(mean = 
<number id="D4098643395CFF942B24FAD94406FAB7" box="[1094,1149,1284,1310]" pageId="6" pageNumber="7" value="1.56">1.56</number>
; S.D. = 
<number id="D4098643395CFF942B85FAD94564FAB7" box="[1255,1311,1284,1310]" pageId="6" pageNumber="7" value="0.1">0.10</number>
); elytral length: 1.0– 
<quantity id="A5393981395CFF942E8BFAF14228FAEF" box="[489,595,1324,1350]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" pageId="6" pageNumber="7" unit="mm" value="1.15">1.15 mm</quantity>
