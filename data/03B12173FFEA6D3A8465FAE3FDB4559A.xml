<number id="3DD08242FFE86D3A8746FD5BFDAC536A" box="[555,570,764,790]" pageId="10" pageNumber="11" value="1.0">1</number>
<number id="3DD08242FFE86D3A872DFD5BFDE0536A" box="[576,630,764,790]" pageId="10" pageNumber="11" value="1.75">1.75</number>
, tibia 
