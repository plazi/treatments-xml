<subSubSection id="24BD4252337C9D26C7ED8EF58FEE0709" pageId="6" pageNumber="25" type="reference_group">
<paragraph id="D0BD71C39C3B042720A91F10B40B8FFB" pageId="6" pageNumber="25">
<taxonomicName id="8B3657C78F8A58FB4868BCFA8E08729D" authority="Wasbauer, 1958" authorityName="Wasbauer" authorityYear="1958" class="Insecta" family="Tiphiidae" genus="Acanthetropis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acanthetropis lamellatus" order="Hymenoptera" pageId="6" pageNumber="25" phylum="Arthropoda" rank="species" species="lamellatus">
<pageBreakToken id="C8CD5AF7E81A4A9980CE6124CD057B35" pageId="6" pageNumber="25" start="start">Acanthetropis</pageBreakToken>
lamellatus
</taxonomicName>
<bibRefCitation id="66D76DEF45D0E565C32AEC05AE84AA43" author="Wasbauer, MS" journalOrPublisher="Pan-Pacific Entomologist" pageId="10" pageNumber="29" pagination="139 - 142" title="A new genus of brachycistidine wasps." volume="34" year="1958">Wasbauer 1958</bibRefCitation>
