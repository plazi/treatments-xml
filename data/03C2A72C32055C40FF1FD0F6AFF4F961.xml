<taxonomicName id="4C6B6DB932055C40FE4BD597AE56FB21" baseAuthorityName="Hendel" baseAuthorityYear="1915" box="[404,542,1186,1210]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="28" phylum="Arthropoda" rank="species" species="latifrons">
