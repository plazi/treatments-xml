<taxonomicName id="4C6B6DB932055C40FD52D577AF5FFBC1" baseAuthorityName="Hendel" baseAuthorityYear="1915" box="[653,791,1090,1114]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="28" phylum="Arthropoda" rank="species" species="latifrons">
