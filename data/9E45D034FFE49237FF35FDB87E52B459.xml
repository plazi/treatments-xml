and M 
<number id="A0247305FFE79231FC3DF90F79F1B7BC" box="[927,935,1755,1769]" pageId="2" pageNumber="87" value="1.0">1</number>
; ground color light brown; costal strigula 
<number id="A0247305FFE79231FA2CF9187FCAB7B1" box="[1422,1436,1740,1764]" pageId="2" pageNumber="87" value="1.0">1</number>
indistinct, strigulae 
<number id="A0247305FFE79231FED5F9267BF4B65F" box="[375,418,1778,1802]" pageId="2" pageNumber="87" value="3.0" valueMax="4.0" valueMin="2.0">2â€“4</number>
and 
<number id="A0247305FFE79231FE79F9267852B65F" box="[475,516,1778,1802]" pageId="2" pageNumber="87" value="8.0" valueMax="9.0" valueMin="7.0">7â€“9</number>
paired, strigulae 
<number id="A0247305FFE79231FD6EF926788CB65F" box="[716,730,1778,1802]" pageId="2" pageNumber="87" value="5.0">5</number>
