<taxonomicName id="D1EC1AA1FFE69230FE6DF8AD786DB6DA" box="[463,571,1913,1935]" class="Insecta" family="Tortricidae" genus="Fibuloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="88" phylum="Arthropoda" rank="genus">
