<typeStatus id="7F57FCD5FFF8F664D65EFAAD84B84621" box="[829,930,1375,1401]" pageId="14" pageNumber="574">holotype</typeStatus>
, 
<typeStatus id="7F57FCD5FFF8F664D6CDFAAD83124621" box="[942,1032,1375,1401]" pageId="14" pageNumber="574">allotype</typeStatus>
and 
<specimenCount id="B6EA89FEFFF8F664D121FAAD83CB4622" box="[1090,1233,1375,1402]" pageId="14" pageNumber="574" type="generic" typeStatus="paratypes">86 paratypes</specimenCount>
