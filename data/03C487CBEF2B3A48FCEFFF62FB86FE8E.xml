<td id="768C5DE3EF2CC5CAFD9DFAFFFCCDFA8D" box="[600,776,1329,1356]" gridcol="4" gridrow="1" pageId="125" pageNumber="126">0-1; 0-1; 1, 2, 3</td>
</tr>
<tr id="355D349FEF2CC5CAFF4DFA9DFCCDFAAF" box="[136,776,1363,1390]" gridrow="2" pageId="125" pageNumber="126">
<th id="768C5DE3EF2CC5CAFF4DFA9DFF0CFAAF" box="[136,201,1363,1390]" gridcol="0" gridrow="2" pageId="125" pageNumber="126">Leg 2</th>
<td id="768C5DE3EF2CC5CAFF2DFA9DFEE6FAAF" box="[232,291,1363,1390]" gridcol="1" gridrow="2" pageId="125" pageNumber="126">0-1</td>
