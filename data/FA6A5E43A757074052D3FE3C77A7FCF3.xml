<quantity id="B53B42B0A75707405771FE1676D2FE01" box="[1382,1440,420,443]" metricMagnitude="-1" metricUnit="m" metricValue="2.794" pageId="6" pageNumber="5" unit="in" value="11.0">11 in</quantity>
