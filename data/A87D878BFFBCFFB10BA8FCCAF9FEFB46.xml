<taxonomicName id="E7D44D1EFFBCFFB10BA8FCCAF954FCCD" authorityName="Cooper" authorityYear="1956" box="[151,306,827,853]" class="Lingulata" family="Torynelasmatidae" genus="Torynelasma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acrotretida" pageId="74" pageNumber="75" phylum="Brachiopoda" rank="genus">
<emphasis id="12A0EA8FFFBCFFB10BA8FCCAF954FCCD" bold="true" box="[151,306,827,853]" italics="true" pageId="74" pageNumber="75">Torynelasma</emphasis>
