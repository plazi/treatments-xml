<keyLead id="92798C5D2074651CFF0D1FAFFA7A2DBF" pageId="8" pageNumber="357">
<number id="9F403D4F2074651CFF0D1FAFFF442D99" box="[151,165,966,988]" pageId="8" pageNumber="357" value="2.0">2</number>
