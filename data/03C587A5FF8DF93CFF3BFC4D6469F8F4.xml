<taxonomicName id="4C6C4D30FF8DF93CFC3AF9E462E9F9D4" box="[918,1180,1540,1565]" class="Gastropoda" family="Trochidae" genus="Trochus" higherTaxonomySource="CoL" kingdom="Animalia" order="Trochida" pageId="73" pageNumber="74" phylum="Mollusca" rank="species" species="elegantissimus">
