<taxonomicName id="4C6D4D5EEF1E3A7AFAA2F89CFC60F84F" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Ascidiacea" family="Cionidae" genus="Ciona" kingdom="Animalia" order="Enterogona" pageId="79" pageNumber="80" phylum="Chordata" rank="species" species="intestinalis">
