<taxonomicName id="4C6D4D5EEF1E3A7AFB3EF8BAFC0DF86E" authority="Savigny, 1816" authorityName="Savigny" authorityYear="1816" class="Ascidiacea" family="Ascidiidae" genus="Phallusia" kingdom="Animalia" order="Enterogona" pageId="79" pageNumber="80" phylum="Chordata" rank="species" species="nigra">
<emphasis id="B919EACFEF1E3A7AFB3EF8BAFA69F84C" box="[1275,1452,1908,1934]" italics="true" pageId="79" pageNumber="80">Phallusia nigra</emphasis>
Savigny, 
<quantity id="4C959B38EF1E3A7AFC55F85BFC0DF86E" box="[912,968,1941,1967]" metricMagnitude="1" metricUnit="m" metricValue="4.61264" pageId="79" pageNumber="80" unit="in" value="1816.0">1816</quantity>
