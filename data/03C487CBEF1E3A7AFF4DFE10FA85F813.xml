<taxonomicName id="4C6D4D5EEF1E3A7AFA92F858FC0AF813" authorityName="Heller" authorityYear="1878" class="Ascidiacea" family="Ascidiidae" genus="Ascidia" kingdom="Animalia" order="Enterogona" pageId="79" pageNumber="80" phylum="Chordata" rank="species" species="depressiuscula">
