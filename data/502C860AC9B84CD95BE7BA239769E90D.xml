<taxonomicName id="DE6A01D49C3B893BB82B72979DAEE3FE" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia polyandra" order="Myrtales" pageId="136" pageNumber="137" phylum="Tracheophyta" rank="species" species="polyandra">
