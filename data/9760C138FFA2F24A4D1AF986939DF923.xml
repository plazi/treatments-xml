<taxonomicName id="D8C90BADFFA3F24A4ED4F9459664F95C" box="[246,348,1762,1786]" class="Magnoliopsida" family="Poaceae" genus="Candollea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="3" pageNumber="78" phylum="Tracheophyta" rank="genus">
