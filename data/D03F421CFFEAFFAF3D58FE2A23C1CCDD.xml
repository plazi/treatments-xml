Tul.
</taxonomicName>
, 
<taxonomicName id="9F968889FFE4FFAE3AA5F90D26EEC862" authority="S. Moore" authorityName="S. Moore" box="[1206,1491,1774,1798]" class="Magnoliopsida" family="Loganiaceae" genus="Strychnos" kingdom="Plantae" order="Gentianales" pageId="8" pageNumber="53" phylum="Tracheophyta" rank="species" species="mitis">
