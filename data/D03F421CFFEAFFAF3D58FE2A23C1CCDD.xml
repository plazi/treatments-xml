L., 
<taxonomicName id="9F968889FFE4FFAE3DDEF92D2052C862" authority="Labat, M. Pignal &amp; O. Pascal" authorityName="Labat, M. Pignal &amp; O. Pascal" class="Magnoliopsida" family="Sapotaceae" genus="Labramia" kingdom="Plantae" order="Ericales" pageId="8" pageNumber="53" phylum="Tracheophyta" rank="species" species="mayottensis">
