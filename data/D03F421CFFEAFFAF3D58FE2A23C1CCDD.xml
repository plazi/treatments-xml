</taxonomicName>
, 
<taxonomicName id="9F968889FFE4FFAE3D68F90D2796C862" authority="Tul." authorityName="Tul." box="[891,1195,1774,1798]" class="Magnoliopsida" family="Hamamelidaceae" genus="Dicoryphe" kingdom="Plantae" order="Saxifragales" pageId="8" pageNumber="53" phylum="Tracheophyta" rank="species" species="platyphylla">
