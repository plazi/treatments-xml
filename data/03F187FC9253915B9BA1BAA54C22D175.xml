<taxonomicName id="4C584D69925391549905BB4A4C22D2DA" box="[563,776,1905,1929]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="81" phylum="Arthropoda" rank="genus">Venezolanoconnus</taxonomicName>
