<taxonomicName id="3CB6EB6E86A74967AE095DBFCE7DA1A8" authority="" authorityName="" baseAuthorityName="Say" baseAuthorityYear="1835" class="Insecta" family="Cleridae" genus="Cymatodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cymatodes inornata" order="Coleoptera" pageId="33" pageNumber="298" phylum="Arthropoda" rank="species" species="inornata">
