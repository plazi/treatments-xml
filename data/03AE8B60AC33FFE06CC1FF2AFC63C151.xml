<subSubSection id="C31D69FDAC33FFED6C91FE04FBC6C183" pageId="53" pageNumber="54" type="etymology">
<paragraph id="8BB83A76AC33FFED6C91FE04FBC5C29F" blockId="53.[151,1437,258,1291]" pageId="53" pageNumber="54">
<emphasis id="B973E664AC33FFED6C91FE04FEFAC2A3" bold="true" box="[199,334,438,463]" pageId="53" pageNumber="54">Etymology.</emphasis>
