<taxonomicName id="5B964D26FFC8FF8EFC029192FB91F500" authorityName="Illiger" authorityYear="1811" box="[923,1037,2702,2727]" class="Mammalia" family="Herpestidae" genus="Herpestes" kingdom="Animalia" order="Carnivora" pageId="4" pageNumber="309" phylum="Chordata" rank="genus">Herpestes</taxonomicName>
