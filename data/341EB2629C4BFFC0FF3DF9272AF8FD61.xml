<taxonomicName id="7BB778F79C4BFFC3FF3DF89E2A00F89F" authority="Fauvel, 1903: 328" authorityName="Fauvel" authorityPageNumber="328" authorityYear="1903" box="[151,603,1856,1878]" class="Insecta" family="Coccinellidae" genus="Coelophora" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="species" species="quadrivittata">
