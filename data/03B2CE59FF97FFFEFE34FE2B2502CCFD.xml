<taxonomicName id="4C1B04CCFF97FFFEFF10FC9D2536CC3A" authority="Woll." authorityName="Wollaston" authorityYear="1864" box="[138,284,778,802]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="2031" phylum="Arthropoda" rank="species" species="obsitus">
