<collectionCode id="ED0AE78AFF97FFFEFEB5FC9D2552CC3A" box="[303,376,778,802]" country="USA" httpUri="http://grbio.org/cool/asxa-80fx" name="University of Maine" pageId="31" pageNumber="2031">UMO</collectionCode>
