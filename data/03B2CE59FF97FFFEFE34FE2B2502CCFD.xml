<collectionCode id="ED0AE78AFF97FFFEFD28FC9D273ACC3A" box="[690,784,778,802]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="31" pageNumber="2031">MNHN</collectionCode>
