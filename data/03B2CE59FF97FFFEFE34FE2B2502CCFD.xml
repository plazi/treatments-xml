<taxonomicName id="4C1B04CCFF97FFFEFE86FD3D254BCDDA" authorityName="Wollaston" authorityYear="1864" box="[284,353,682,706]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="2031" phylum="Arthropoda" rank="species" species="obsitus">
