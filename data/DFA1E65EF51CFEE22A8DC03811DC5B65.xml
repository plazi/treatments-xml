<taxonomicName id="50F297AC28DB060E5790AEFB546D3E15" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145123" authority="Forel, 1890" authorityName="Forel" authorityYear="1890" class="Hexapoda" family="Formicidae" genus="Triglyphothrix" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Triglyphothrix parvispina var. formosae Forel" order="Hymenoptera" pageNumber="52" phylum="Arthropoda" rank="variety" species="parvispina" variety="formosae">Triglyphothrix parvispina Em. var. Formosae</taxonomicName>
<taxonomicNameLabel id="6D224B0ED23B54E418ADEEFE93E423B9" rank="variety">n. var.</taxonomicNameLabel>
