<paragraph id="8BD9368A8BCFFF7CFE9FFBFAFBF4FBE0" blockId="129.[264,1069,1049,1115]" box="[312,1069,1089,1115]" pageId="129" pageNumber="130">
<taxonomicName id="4C664D098BCFFF7CFE9FFBFAFE01FBE1" box="[312,472,1089,1114]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="129" pageNumber="130" phylum="Arthropoda" rank="species" species="faceta">
