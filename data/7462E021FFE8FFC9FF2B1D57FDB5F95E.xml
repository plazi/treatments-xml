<taxonomicName id="3BCB2AB4FFE8FFC9FAFA1CD4FA06FA46" authorityName="de Jussieu" authorityYear="1789" box="[1297,1431,1409,1433]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
