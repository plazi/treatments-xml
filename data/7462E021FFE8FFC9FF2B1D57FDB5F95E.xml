, 
<taxonomicName id="3BCB2AB4FFE8FFC9FB461DA9FE90FAEB" authority="(Rich.) Nees" authorityName="Nees" authorityYear="1836" baseAuthorityName="Rich." class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="puberula">
