<taxonomicName id="3BCB2AB4FFE8FFC9FDA71DA9FCF0FACC" authority="(Nees) Mez" authorityName="Mez" authorityYear="1889" baseAuthorityName="Nees" box="[588,865,1276,1300]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="cernua">
<emphasis id="CEBF8D25FFE8FFC9FDA71DA9FD56FACC" box="[588,711,1276,1299]" italics="true" pageId="6" pageNumber="5">O. cernua</emphasis>
