<emphasis id="B953EAB08E47077CFF615950C306FE89" box="[199,308,305,331]" italics="true" pageId="4" pageNumber="5">Adelphos</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C274D218E47077CFEE45950C389FE89" authorityName="Cobb" authorityYear="1920" box="[322,443,305,331]" class="Adenophorea" family="Desmodoridae" genus="Catanema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="4" pageNumber="5" phylum="Nematoda" rank="genus">
