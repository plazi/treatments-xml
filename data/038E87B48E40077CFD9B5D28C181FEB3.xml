<taxonomicName id="4C274D218E47077CFE6F5950C04CFE89" authorityName="Tchesunov, Ingels &amp; Popova" authorityYear="2012" box="[457,638,305,331]" class="Adenophorea" family="Desmodoridae" genus="Parabostrichus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="4" pageNumber="5" phylum="Nematoda" rank="genus">
<emphasis id="B953EAB08E47077CFE6F5950C04CFE89" box="[457,638,305,331]" italics="true" pageId="4" pageNumber="5">Parabostrichus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C274D218E47077CFD105950C122FE89" authorityName="Gerlach" authorityYear="1956" box="[694,784,305,331]" class="Adenophorea" family="Desmodoridae" genus="Robbea" kingdom="Animalia" order="Desmodorida" pageId="4" pageNumber="5" phylum="Nematoda" rank="genus">
