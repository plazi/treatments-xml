<taxonomicName id="4C274D218E40077BFCAC5FB3C14BF829" authorityName="Vincx &amp; Gourbault" authorityYear="1989" baseAuthorityName="Wieser &amp; Hopper" baseAuthorityYear="1967" box="[778,889,2002,2027]" class="Adenophorea" family="Desmodoridae" genus="Perspiria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="3" pageNumber="4" phylum="Nematoda" rank="genus">
