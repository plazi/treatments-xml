<collectionCode id="ED043EAC9921FFF61990FED4FE4BFEC6" box="[453,474,305,331]" pageId="12" pageNumber="13">A</collectionCode>
– 
<collectionCode id="ED043EAC9921FFF619BDFED7FE6DFEC1" box="[488,508,306,332]" pageId="12" pageNumber="13">B</collectionCode>
