<taxonomicName id="4C434D27FF9DFFC8FDB6FA79FD1B9C4E" box="[519,688,1519,1542]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="35" pageNumber="336" phylum="Arthropoda" rank="species" species="cyaneus" subGenus="Lanocyrtus">
