<taxonomicName id="4C764D47FF51ABC89D6215B345F13D8C" authorityName="Borkent" authorityYear="2008" box="[829,940,1430,1455]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="189" pageNumber="190" phylum="Arthropoda" rank="species" species="evenhuisi">
