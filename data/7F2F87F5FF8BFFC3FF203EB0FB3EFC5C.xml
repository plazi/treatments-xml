<taxonomicName id="30864D60FF8BFFC3FD383EFFFC67FCA4" authority="Latham" authorityName="Latham" box="[655,944,762,787]" class="Aves" family="Sittidae" genus="Sitta" kingdom="Animalia" order="Passeriformes" pageId="9" pageNumber="563" phylum="Chordata" rank="species" species="carolinensis">
