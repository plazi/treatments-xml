<taxonomicName id="689310336975B577C98A12AE11853149" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138847" class="Insecta" family="Formicidae" genus="Ponera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Ponera ludovicae Forel" order="Hymenoptera" pageNumber="249" phylum="Arthropoda" rank="species" species="ludovicae">PONERA LUDOVICAE</taxonomicName>
