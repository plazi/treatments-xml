<geoCoordinate id="EE620CAEE2351A31FF3B18AFD547FC4F" box="[223,321,982,1006]" degrees="109" direction="east" minutes="33" orientation="longitude" pageId="7" pageNumber="138" precision="925" value="109.55">109°33’E</geoCoordinate>
