<taxonomicName id="A5D34D0CFF86FFB3FC89FBA54613FB78" authority=": Tedesco &amp; Cei, 1999: 314" authorityName=": Tedesco &amp; Cei" authorityPageNumber="314" authorityYear="1999" box="[806,1365,1146,1168]" class="Reptilia" family="Gymnophthalmidae" genus="Pantodactylus" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="105" phylum="Chordata" rank="species" species="parkeri">
