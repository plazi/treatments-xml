<taxonomicName id="3D4692413307CA549DC09D61B7B22899" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" class="Insecta" family="Gasteruptiidae" genus="Gasteruption" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gasteruption boreale" order="Hymenoptera" pageId="36" pageNumber="87" phylum="Arthropoda" rank="species" species="boreale">Gasteruption boreale</taxonomicName>
