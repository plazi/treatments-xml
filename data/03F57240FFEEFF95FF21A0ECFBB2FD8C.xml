<taxonomicName id="4C5CB8D5FFEEFF96FED2A590FDAAF9B1" authority="Gaiani, 2017" authorityName="Gaiani" authorityYear="2017" box="[356,580,1515,1537]" class="Insecta" family="Membracidae" genus="Problematode" kingdom="Animalia" order="Hemiptera" pageId="5" phylum="Arthropoda" rank="species" species="inerme" status="sp. nov.">
<emphasis id="B9281F44FFEEFF96FED2A590FDAAF9B1" box="[356,580,1515,1537]" italics="true" pageId="5">Problematode inerme</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A21BA23FFFEEFF96FDE6A597FD4BF9B1" box="[592,677,1516,1537]" pageId="5" rank="species">
<emphasis id="B9281F44FFEEFF96FDE6A597FD4BF9B1" bold="true" box="[592,677,1516,1537]" pageId="5">sp. nov.</emphasis>
