<taxonomicName id="4C7298F23C71FFE2E998F92FFD8C58C2" authority="Robineau-Desvoidy" authorityName="Robineau-Desvoidy" box="[238,579,1714,1740]" class="Insecta" family="Fanniidae" genus="Fannia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis id="B9063F633C71FFE2E998F92FFE8758C2" bold="true" box="[238,328,1714,1740]" italics="true" pageId="1" pageNumber="39">Fannia</emphasis>
