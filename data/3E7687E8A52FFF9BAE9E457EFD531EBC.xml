<taxonomicName id="71DF4D7DA52FFF9AAB214271FB1C1CFA" box="[1042,1153,1133,1158]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="66" pageNumber="1180" phylum="Arthropoda" rank="subFamily" subFamily="Cryptinae">Cryptinae</taxonomicName>
