<taxonomicName id="4C7B4D0FB373FFD0DD23FB339D78FB22" box="[618,793,1234,1259]" class="Magnoliopsida" family="Plantaginaceae" kingdom="Animalia" order="Lamiales" pageId="19" pageNumber="220" phylum="Tracheophyta" rank="family">Plantaginaceae</taxonomicName>
