<taxonomicName id="69C297C0F70FFF9DFDA8F9FB45E4F992" box="[577,707,1623,1648]" class="Collembola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="79" pageNumber="125" phylum="Arthropoda" rank="order">collembola</taxonomicName>
