<taxonomicName id="86B8233765BA940F4DBCEF309FF447DA" authorityName="O. P. - Cambridge" authorityYear="1894" class="Arachnida" family="Thomisidae" genus="Xysticus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xysticus pellax" order="Araneae" pageId="414" pageNumber="415" phylum="Arthropoda" rank="species" species="pellax">
