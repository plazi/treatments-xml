<taxonomicName id="B6822E79BDF2FCB65A0ADF321377ACDD" authorityName="Keyserling" authorityYear="1880" class="Arachnida" family="Thomisidae" genus="Xysticus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xysticus gulosus" order="Araneae" pageId="414" pageNumber="415" phylum="Arthropoda" rank="species" species="gulosus">
