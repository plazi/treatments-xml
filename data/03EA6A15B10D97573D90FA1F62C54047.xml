<taxonomicName id="4C43A080B10D97563C58FA2660C44560" authority="Balthasar" authorityName="Balthasar" authorityYear="1967" box="[359,723,1522,1543]" class="Insecta" family="Aphodiidae" genus="Saprositellus" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="451" phylum="Arthropoda" rank="species" species="denticulatus">
