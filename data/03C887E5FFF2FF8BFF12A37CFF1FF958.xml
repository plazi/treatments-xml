</emphasis>
<taxonomicName id="4C614D70FFF2FF8BFEBCA3B6FD67FB44" authority="Thor, 1910" authorityName="Thor" authorityYear="1910" box="[313,655,1193,1218]" class="Arachnida" family="Halacaridae" genus="Halacarus" kingdom="Animalia" order="Trombidiformes" pageId="25" pageNumber="226" phylum="Arthropoda" rank="species" species="alpinus">
