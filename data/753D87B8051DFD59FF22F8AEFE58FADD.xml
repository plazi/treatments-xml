<taxonomicName id="3A944D2D051BFD59FE42FBE8FD47FB90" box="[503,649,1052,1077]" class="Malacostraca" family="Hymenosomatidae" genus="Rhynchoplax" higherTaxonomySource="CoL" kingdom="Animalia" order="Decapoda" pageId="29" pageNumber="509" phylum="Arthropoda" rank="genus">
<emphasis id="CFE0EABC051BFD59FE42FBE8FD47FB90" box="[503,649,1052,1077]" italics="true" pageId="29" pageNumber="509">Rhynchoplax</emphasis>
</taxonomicName>
); 
<taxonomicName id="3A944D2D051BFD59FD17FBE9FBD0FB91" authority="Yang &amp; Sun, 1998" authorityName="Yang &amp; Sun" authorityYear="1998" box="[674,1054,1052,1077]" class="Malacostraca" family="Hymenosomatidae" genus="Stimpsoplax" kingdom="Animalia" order="Decapoda" pageId="29" pageNumber="509" phylum="Arthropoda" rank="species" species="unidentata" status="n. comb.">
<emphasis id="CFE0EABC051BFD59FD17FBE9FCF8FB90" box="[674,822,1052,1077]" italics="true" pageId="29" pageNumber="509">S. unidentata</emphasis>
(Yang &amp; Sun, 
<number id="4B5C2489051BFD59FC68FBE8FBD8FB91" box="[989,1046,1052,1076]" pageId="29" pageNumber="509" value="1998.0">1998</number>
)
</taxonomicName>
<taxonomicNameLabel id="D4D357C7051BFD59FB90FBE8FB45FB90" box="[1061,1163,1052,1077]" pageId="29" pageNumber="509">
<emphasis id="CFE0EABC051BFD59FB90FBE8FB45FB90" bold="true" box="[1061,1163,1052,1077]" pageId="29" pageNumber="509">n. comb.</emphasis>
</taxonomicNameLabel>
(ex 
<taxonomicName id="3A944D2D051BFD59FB08FBE8FA80FB90" box="[1213,1358,1052,1077]" class="Malacostraca" family="Hymenosomatidae" genus="Halicarcinus" kingdom="Animalia" order="Decapoda" pageId="29" pageNumber="509" phylum="Arthropoda" rank="genus">
<emphasis id="CFE0EABC051BFD59FB08FBE8FA80FB90" box="[1213,1358,1052,1077]" italics="true" pageId="29" pageNumber="509">Halicarcinus</emphasis>
