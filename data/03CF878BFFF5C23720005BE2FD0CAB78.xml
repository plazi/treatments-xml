<quantity id="4C9E9B78FFF5C23720135AA4FE89A9B2" box="[282,363,1425,1449]" metricMagnitude="3" metricUnit="m" metricValue="1.1099999999999999" pageId="97" pageNumber="394" unit="m" value="1110.0">1110 m</quantity>
