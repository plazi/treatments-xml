</taxonomicName>
(
<taxonomicName id="5DD368AC6F9B62ABE5EE56B9EDB302DE" class="Magnoliopsida" family="Primulaceae" kingdom="Plantae" order="Ericales" phylum="Tracheophyta" rank="family">Primulaceae</taxonomicName>
