<taxonomicName id="2DD44D74FF6EF78AFF2AF8BCFDDA7A49" baseAuthorityName="Martynov" baseAuthorityYear="1930" box="[195,522,1831,1857]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="species" species="chinensis">
<emphasis id="D8A0EAE5FF6EF78AFF2AF8BCFDDA7A49" bold="true" box="[195,522,1831,1857]" italics="true" pageId="144" pageNumber="145">Cheumatopsyche chinensis</emphasis>
</taxonomicName>
