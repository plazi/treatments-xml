<number id="3DA82483B7333726FE0A9631FE182F1D" box="[486,501,1296,1322]" pageId="8" pageNumber="9" value="9.0">9</number>
a, 
<number id="3DA82483B7333726FDFC9631FDC12F1D" box="[528,556,1296,1322]" pageId="8" pageNumber="9" value="13.0">13</number>
a–b, 
<number id="3DA82483B7333726FD8B9631FD682F1D" box="[615,645,1296,1322]" pageId="8" pageNumber="9" value="14.0">14</number>
