<heading id="BF110B44CF5AFFF8FF53FB30FE6FFB2B" bold="true" box="[178,395,1252,1274]" fontSize="9" level="2" pageId="0" pageNumber="1123" reason="3">
<emphasis id="D692603ACF5AFFF8FF53FB30FE6FFB2B" bold="true" box="[178,395,1252,1274]" pageId="0" pageNumber="1123">
<taxonomicName id="23E6C7ABCF5AFFF8FF53FB30FEDCFB2B" authority="J.M, 2011" authorityName="J.M" authorityYear="2011" box="[178,312,1252,1274]" class="Insecta" family="Eumenidae" genus="Zethus" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="1123" phylum="Arthropoda" rank="species" species="josefi" status="nov.sp.">
<emphasis id="D692603ACF5AFFF8FF53FB30FEDCFB2B" bold="true" box="[178,312,1252,1274]" italics="true" pageId="0" pageNumber="1123">Zethus josefi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CDA1DD41CF5AFFF8FEDFFB31FE6FFB2B" box="[318,395,1253,1274]" pageId="0" pageNumber="1123" rank="species">nov.sp.</taxonomicNameLabel>
