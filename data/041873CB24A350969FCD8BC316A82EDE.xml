<geoCoordinate id="105F5EEE7109235DFBF09F5A7E76F030" degrees="20" direction="north" minutes="48" orientation="latitude" precision="15" seconds="46" value="20.812777">20°48'46&quot;N</geoCoordinate>
, 
<geoCoordinate id="08C33E3E62F42372DABC96F48A63CFA1" degrees="105" direction="east" minutes="37" orientation="longitude" precision="15" seconds="58" value="105.63278">105°37'58&quot;E</geoCoordinate>
