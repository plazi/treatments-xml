<taxonomicName id="27CA4D35FEC0FEA65D5CFA0EFE6D64B8" authority="Phil. R." authorityName="Phil. R." class="Insecta" family="Syrphidae" genus="Syrphus" kingdom="Animalia" order="Diptera" pageId="269" pageNumber="270" phylum="Arthropoda" rank="species" species="decemmaculatus">
