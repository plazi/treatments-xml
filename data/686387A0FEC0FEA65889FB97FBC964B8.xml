<taxonomicName id="27CA4D35FEC0FEA65A6EFAB6FC23642C" authorityName="Rondani" authorityYear="1863" box="[624,912,1339,1365]" class="Insecta" family="Syrphidae" genus="Syrphus" kingdom="Animalia" order="Diptera" pageId="269" pageNumber="270" phylum="Arthropoda" rank="species" species="decemmaculatus">
