<paragraph id="EE7569569E94393D0EEE211EE1F867EE" pageId="null" pageNumber="603">
Artengruppe des 
<taxonomicName id="5EE78B080D468486D98B14F86C613647" authority="Silvester" class="Magnoliopsida" family="Fabaceae" genus="Lathyrus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="603" phylum="Tracheophyta" rank="genus">
Lathyrus 
<normalizedToken id="D8B1D7DCD6C7B2F11EA1E811E25BF772" originalValue="silvéster" pageId="null" pageNumber="603">silvester</normalizedToken>
</taxonomicName>
