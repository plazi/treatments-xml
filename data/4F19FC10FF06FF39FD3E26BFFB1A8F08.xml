<taxonomicName id="00B03685FF04FF37FCF72459FC598F5B" baseAuthorityName="Thomas" baseAuthorityYear="1901" box="[773,978,1611,1632]" class="Mammalia" family="Molossidae" genus="Molossops" kingdom="Animalia" order="Chiroptera" pageId="151" pageNumber="151" phylum="Chordata" rank="species" species="paranus">
