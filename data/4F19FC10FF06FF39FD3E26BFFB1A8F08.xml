<taxonomicName id="00B03685FF04FF37FC2C2474FBE58F40" baseAuthorityName="Goodwin" baseAuthorityYear="1958" box="[990,1134,1637,1659]" class="Mammalia" family="Molossidae" genus="Molossops" kingdom="Animalia" order="Chiroptera" pageId="151" pageNumber="151" phylum="Chordata" rank="species" species="greenhalli">
<emphasis id="F5C49114FF04FF37FC2C2474FC7F8F40" box="[990,1012,1638,1659]" italics="true" pageId="151" pageNumber="151">M</emphasis>
. 
<emphasis id="F5C49114FF04FF37FBF12474FBE58F40" box="[1027,1134,1638,1659]" italics="true" pageId="151" pageNumber="151">greenhalli</emphasis>
