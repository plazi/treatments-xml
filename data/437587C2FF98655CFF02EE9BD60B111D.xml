<materialsCitation id="7BB43C89FF98655DFEDBEF2CD16313EB" ID-GBIF-Occurrence="2451575349" box="[334,1353,1907,1934]" collectionCode="NHMUK" pageId="92" pageNumber="93" specimenCode="NHMUK 20141112" specimenCount="1" typeStatus="syntype">
