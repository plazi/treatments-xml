<caption id="3EEA663A8D10BD419B80D1A1FE0FF820" httpUri="https://zenodo.org/record/224660/files/figure.png" pageId="3" pageNumber="353" targetBox="[205,1362,207,1886]" targetPageId="3">
<paragraph id="6A2A36B28D10BD419B80D1A1FE0FF820" blockId="3.[151,1436,1932,2017]" pageId="3" pageNumber="353">
