<emphasis id="1B91B82F1740FFB7FDFBF81EFD40558D" box="[570,724,1948,1970]" italics="true" pageId="4" pageNumber="5">
<taxonomicName id="EEE51FBE1740FFB7FDFBF81EFD5B558D" box="[570,719,1948,1970]" class="Insecta" family="Aclopidae" genus="Aclopus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="intermedius">A. intermedius</taxonomicName>
,
</emphasis>
<number id="9F2D761A1740FFB7FD1BF81EFD73558E" box="[730,743,1948,1969]" pageId="4" pageNumber="5" value="3.0">3</number>
<emphasis id="1B91B82F1740FFB7FD26F81EFCFB558D" box="[743,879,1948,1970]" italics="true" pageId="4" pageNumber="5">
