<paragraph id="8BBE369FFF1299E0F197F942FBD0F8F1" blockId="158.[151,1437,1769,2012]" box="[151,1048,1769,1796]" pageId="158" pageNumber="159">
<emphasis id="B975EA8DFF1299E0F197F942FEE1F8F1" bold="true" box="[151,297,1769,1796]" pageId="158" pageNumber="159">Description.</emphasis>
<typeStatus id="54BA883DFF1299E0F030F941FE54F8F1" box="[304,412,1770,1796]" pageId="158" pageNumber="159">Holotype</typeStatus>
. Female. Body length 
<quantity id="4CF99B7AFF1299E0F397F942FD27F8F1" box="[663,751,1769,1796]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="158" pageNumber="159" unit="mm" value="2.5">2.5 mm</quantity>
, fore wing length 
<quantity id="4CF99B7AFF1299E0F2BBF942FBDCF8F1" box="[955,1044,1769,1796]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="158" pageNumber="159" unit="mm" value="2.5">2.5 mm</quantity>
