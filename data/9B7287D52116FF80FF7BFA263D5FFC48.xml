<emphasis id="21AFEAD12116FF81FB47F9913DF7F97F" box="[1195,1210,1639,1664]" italics="true" pageId="4" pageNumber="57">d</emphasis>
) 
<number id="A51324E42116FF81FB20F9913D96F97E" box="[1228,1243,1639,1665]" pageId="4" pageNumber="57" value="8.0">8</number>
(
<number id="A51324E42116FF81FB01F9913C52F97E" box="[1261,1311,1639,1665]" pageId="4" pageNumber="57" value="8.0" valueMax="9.0" valueMin="7.0">7–9</number>
), pedipalp coxal setae (
<emphasis id="21AFEAD12116FF81FED9F978381FF958" box="[309,338,1678,1703]" italics="true" pageId="4" pageNumber="57">ep</emphasis>
) 
<number id="A51324E42116FF81FE8AF97B383BF958" box="[358,374,1677,1703]" pageId="4" pageNumber="57" value="5.0">5</number>
(
<number id="A51324E42116FF81FE64F97B38F1F958" box="[392,444,1677,1703]" pageId="4" pageNumber="57" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
), palp tarsal ventral setae (
<emphasis id="21AFEAD12116FF81FCFEF9783A52F958" box="[786,799,1678,1703]" italics="true" pageId="4" pageNumber="57">v</emphasis>
) 
<number id="A51324E42116FF81FCDFF97B3A0FF958" box="[819,834,1677,1703]" pageId="4" pageNumber="57" value="6.0">6</number>
(
<number id="A51324E42116FF81FCBAF97B3AC7F958" box="[854,906,1677,1703]" pageId="4" pageNumber="57" value="6.0" valueMax="7.0" valueMin="5.0">5–7</number>
