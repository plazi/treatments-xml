<number id="A51324E42116FF81FB92F97B3DD0F958" box="[1150,1181,1677,1703]" pageId="4" pageNumber="57" value="58.0">58</number>
(
<number id="A51324E42116FF81FB43F97B3C49F958" box="[1199,1284,1677,1703]" pageId="4" pageNumber="57" value="57.5" valueMax="62.0" valueMin="53.0">53–62</number>
