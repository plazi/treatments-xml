<number id="A51324E42116FF81FB41F9B63D80F9A5" box="[1197,1229,1600,1626]" pageId="4" pageNumber="57" value="80.0">80</number>
(
<number id="A51324E42116FF81FB09F9B63C7BF9A5" box="[1253,1334,1600,1626]" pageId="4" pageNumber="57" value="80.0" valueMax="87.0" valueMin="73.0">73–87</number>
) thick. 
<emphasis id="21AFEAD12116FF81FF7BF990381AF97F" bold="true" box="[151,343,1638,1664]" pageId="4" pageNumber="57">Gnathosoma—</emphasis>
<number id="A51324E42116FF81FEBBF9913838F97E" box="[343,373,1639,1665]" pageId="4" pageNumber="57" value="47.0">47</number>
(
<number id="A51324E42116FF81FE6AF991389AF97E" box="[390,471,1639,1665]" pageId="4" pageNumber="57" value="46.5" valueMax="50.0" valueMin="43.0">43–50</number>
