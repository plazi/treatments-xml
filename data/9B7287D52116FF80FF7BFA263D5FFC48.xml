<subSubSection id="5BC165482117FF80FF29FCF43D5FFC48" pageId="5" pageNumber="58" type="etymology">
<paragraph id="136436C32117FF80FF29FCF43C45FCE3" blockId="5.[151,1436,152,951]" box="[197,1288,770,796]" pageId="5" pageNumber="58">
<emphasis id="21AFEAD12117FF80FF29FCF4381FFCE3" bold="true" box="[197,338,770,796]" pageId="5" pageNumber="58">Etymology.</emphasis>
