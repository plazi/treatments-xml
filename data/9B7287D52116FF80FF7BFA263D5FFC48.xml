<emphasis id="21AFEAD12116FF81FD09F8B93A49F897" box="[741,772,1871,1896]" italics="true" pageId="4" pageNumber="57">
<number id="A51324E42116FF81FD09F8B93BB9F897" box="[741,756,1871,1896]" pageId="4" pageNumber="57" value="2.0">2</number>
