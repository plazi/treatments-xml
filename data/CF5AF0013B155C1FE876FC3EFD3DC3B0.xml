<taxonomicName id="80F33A943B0A5C1EE855FD84FC19C46C" authority="Monniot &amp; Monniot, 2008" authorityName="Monniot &amp; Monniot" authorityYear="2008" box="[775,931,618,635]" class="Ascidiacea" family="Didemnidae" genus="Didemnum" kingdom="Animalia" order="Enterogona" pageId="16" pageNumber="815" phylum="Chordata" rank="species" species="valgum" status="n. sp.">
