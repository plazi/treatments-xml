<taxonomicName id="37E7FEFBFFA28444FF2FFEBBFB45D162" authority="Kastschenko, 1899" authorityName="Kastschenko" authorityYear="1899" box="[777,1224,259,285]" class="Mammalia" family="Sciuridae" genus="Marmota" kingdom="Animalia" order="Rodentia" pageId="3" pageNumber="105" phylum="Chordata" rank="species" species="baibacina">
