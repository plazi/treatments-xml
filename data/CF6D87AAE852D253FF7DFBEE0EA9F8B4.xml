<subSubSection id="0FDE6537E852D252FF7DFB4B0EE4FB54" box="[151,930,1221,1246]" pageId="28" pageNumber="29" type="materials_examined">
<paragraph id="477B36BCE852D252FF7DFB4B0EE4FB54" blockId="28.[151,1437,1221,2038]" box="[151,930,1221,1246]" pageId="28" pageNumber="29">
<emphasis id="75B0EAAEE852D252FF7DFB4B0C2EFB57" bold="true" box="[151,360,1221,1246]" pageId="28" pageNumber="29">Figured material.</emphasis>
<typeStatus id="987F881EE852D252FE85FB4B0C9CFB57" box="[367,474,1221,1246]" pageId="28" pageNumber="29">Holotype</typeStatus>
