<taxonomicName id="62F993D8AD07D941F9B7FE0C8A8AFE3E" box="[151,247,460,481]" class="Magnoliopsida" family="Asteraceae" genus="Baccharis" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="198" phylum="Tracheophyta" rank="genus">
