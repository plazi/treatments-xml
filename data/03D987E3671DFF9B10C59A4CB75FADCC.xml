<collectingCountry id="F3677665671AFF9B11359F7BB4B9AD50" box="[359,464,803,827]" name="Paraguay" pageId="28" pageNumber="179">Paraguay</collectingCountry>
</collectingRegion>
, southern 
<collectingCountry id="F3677665671AFF9B12109F7AB7EFAD50" box="[578,646,802,827]" name="Brazil" pageId="28" pageNumber="179">Brazil</collectingCountry>
(locus typicus: 
<collectingCountry id="F3677665671AFF9B136A9F7AB616AD50" box="[824,895,802,827]" name="Brazil" pageId="28" pageNumber="179">Brazil</collectingCountry>
, Matto Grosso, Burity, 
<quantity id="4C889B10671AFF9B14D79F7BB18CAD50" box="[1157,1253,802,827]" metricMagnitude="4" metricUnit="m" metricValue="4.828032" pageId="28" pageNumber="179" unit="mi" value="30.0">30 miles</quantity>
