<taxonomicName id="4C314D53FFF09E17FB0D4425FB18097C" box="[1195,1253,513,536]" class="Polychaeta" family="Terebellidae" genus="Pista" kingdom="Animalia" order="Terebellida" pageId="26" pageNumber="510" phylum="Annelida" rank="genus">
