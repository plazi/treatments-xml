<subSubSection id="C3264B81AC65FF9294DCFDC0FC9DFD13" box="[159,920,623,687]" pageId="20" pageNumber="319" type="etymology">
<paragraph id="8B83180AAC65FF9294DCFDC0FE21FD35" blockId="20.[159,920,623,687]" box="[159,292,623,649]" pageId="20" pageNumber="319">
<heading id="D0CBAF66AC65FF9294DCFDC0FE21FD35" bold="true" box="[159,292,623,649]" fontSize="11" level="3" pageId="20" pageNumber="319" reason="6">
