<taxonomicName id="D6956CACCE79FFC1FD98CE2958B45032" authorityName="Moure" authorityYear="1950" box="[541,660,770,795]" class="Insecta" family="Apidae" genus="Trigonisca" kingdom="Animalia" order="Hymenoptera" pageId="4" phylum="Arthropoda" rank="genus">
