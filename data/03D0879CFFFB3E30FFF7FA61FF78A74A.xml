<quantity id="4C819B6FFFFB3E31FE3BF9F6FEE3A352" box="[355,383,1537,1562]" metricMagnitude="-3" metricUnit="kg" metricValue="1.0" pageId="18" pageNumber="615" unit="g" value="1.0">1 g</quantity>
