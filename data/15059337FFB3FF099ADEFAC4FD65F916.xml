<typeStatus id="42179C83FFB3FF099F21F9C4FA68F986" box="[1384,1437,1538,1564]" pageId="10" pageNumber="35">type</typeStatus>
locality of 
<taxonomicName id="5AAC59A2FFB3FF099B5FF9EEFDCBF9DB" box="[278,574,1576,1601]" class="Actinopterygii" family="Macrouridae" genus="Hymenocephalus" kingdom="Animalia" order="Gadiformes" pageId="10" pageNumber="35" phylum="Chordata" rank="species" species="billsam">
