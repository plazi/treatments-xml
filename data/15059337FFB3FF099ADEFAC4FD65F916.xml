<quantity id="5A548FC4FFB3FF099E23F9C4FAD6F986" box="[1130,1315,1538,1564]" metricMagnitude="2" metricUnit="m" metricValue="7.54" metricValueMax="12.75" metricValueMin="2.33" pageId="10" pageNumber="35" unit="m" value="754.0" valueMax="1275.0" valueMin="233.0">233 to 1275 m</quantity>
