<materialsCitation id="C1DF5B431037FFBCFEA1DEA69A7F6A19" ID-GBIF-Occurrence="2849122355" box="[333,1081,1269,1293]" collectionCode="BMNH" pageId="14" pageNumber="143" specimenCount="3" specimenCount-male="3" typeStatus="paratype">
