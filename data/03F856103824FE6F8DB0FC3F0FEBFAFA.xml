specific epithet, 
<taxonomicName id="4C519C853826FE6F8D16FD6E0DB7FD73" authorityName="Grisales &amp; Wolff &amp; de Carvalho" authorityYear="2012" box="[373,471,666,690]" class="Florideophyceae" family="Solieriaceae" genus="Euryomma" kingdom="Plantae" order="Gigartinales" pageId="14" pageNumber="816" phylum="Rhodophyta" rank="species" species="chitarera">
