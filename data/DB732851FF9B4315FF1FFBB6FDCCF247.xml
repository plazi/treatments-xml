<emphasis id="61AE4555FF9A4315FE35FF59FDA7F624" box="[445,577,232,257]" italics="true" pageId="33" pageNumber="34">
<taxonomicName id="94DAE2C4FF9A4315FE35FF59FDDCF624" box="[445,570,232,257]" class="Magnoliopsida" family="Combretaceae" genus="Terminalia" kingdom="Plantae" order="Myrtales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="genus">Terminalia</taxonomicName>
