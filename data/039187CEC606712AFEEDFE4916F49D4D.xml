<taxonomicName id="4C384D5BC606712AFEF2FDE416F49944" authority="Chopard, 1970" authorityName="Chopard" authorityYear="1970" box="[308,660,602,626]" class="Insecta" family="Haglotettigoniidae" genus="Euscyrtus" kingdom="Animalia" order="Orthoptera" pageId="39" pageNumber="444" phylum="Arthropoda" rank="species" species="subapterus">
