<taxonomicName id="8FEE4D41FF9BFFB0FC18FC2D61E4C8E3" authorityName="Keirans, Hoogstraal and Clifford" authorityYear="1973" box="[904,1191,906,932]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="macfarlandi">Amblyomma macfarlandi</taxonomicName>
