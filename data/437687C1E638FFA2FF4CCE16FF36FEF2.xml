<typeStatus id="14648875E638FFA3FF4CCE79FF38FC4C" box="[151,204,893,916]" pageId="34" pageNumber="35">Type</typeStatus>
genus: 
<taxonomicName id="0CDF4D54E638FFA3FEC4CE79FDDEFC4C" authority="Fain, 1955" authorityName="Fain" authorityYear="1955" box="[287,554,893,916]" class="Arachnida" family="Gastronyssidae" genus="Gastronyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="34" pageNumber="35" phylum="Arthropoda" rank="genus">
<emphasis id="F9ABEAC5E638FFA3FEC4CE79FE59FC4C" box="[287,429,893,916]" italics="true" pageId="34" pageNumber="35">Gastronyssus</emphasis>
<bibRefCitation id="AF4E4B26E638FFA3FE6FCE79FDDEFC4C" author="Fain, A." box="[436,554,893,916]" pageId="34" pageNumber="35" pagination="681 - 688" refId="ref75884" refString="Fain, A. (1955) Un acarien remarquable vivant dans l'estomac d'une chauve-souris: Gastronyssus bakeri n. g., n. sp. Annales de la Societe Belge de Medicine Tropicale, 35, 681 - 688." type="journal article" year="1955">Fain, 1955</bibRefCitation>
