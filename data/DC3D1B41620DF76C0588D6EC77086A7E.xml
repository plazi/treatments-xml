<taxonomicName id="BF23F4BEB47D9517E97DEB810A673766" class="Polychaeta" family="Onuphidae" genus="Sarsonuphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sarsonuphis fragosa" order="Eunicida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="fragosa">Sarsonuphis fragosa</taxonomicName>
