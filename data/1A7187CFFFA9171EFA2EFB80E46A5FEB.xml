<taxonomicName id="55D84D5AFFA9171EFA2EFB80E4EB5E27" authorityName="Simpson" authorityYear="1927" box="[1428,1683,1086,1117]" class="Mammalia" family="Pediomyidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Didelphimorphia" pageId="67" pageNumber="384" phylum="Chordata" rank="family">
<emphasis id="A0ACEACBFFA9171EFA2EFB80E4EB5E27" box="[1428,1683,1086,1117]" italics="true" pageId="67" pageNumber="384">PEDIOMYIDAE</emphasis>
