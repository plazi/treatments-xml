<emphasis id="B959EAA37A52FFE3FB43B752EAB912F5" box="[1224,1428,361,386]" italics="true" pageId="14" pageNumber="15">
<taxonomicName id="4C2D4D327A52FFE3FB43B752EABD12F5" box="[1224,1424,361,386]" class="Liliopsida" family="Poaceae" genus="Leersia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="hexandra">Leersia hexandra</taxonomicName>
.
