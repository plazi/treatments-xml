<collectingCountry id="F33A76217A52FFE3FEDBB77DEEC81229" box="[336,485,325,350]" name="South Africa" pageId="14" pageNumber="15">
<collectingRegion id="49E9F8537A52FFE3FEDBB77DEEBC1229" box="[336,401,325,350]" country="Tanzania" name="Kusini Unguja" pageId="14" pageNumber="15">South</collectingRegion>
