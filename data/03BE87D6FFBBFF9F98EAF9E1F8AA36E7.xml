<taxonomicName id="4C174D43FFBBFF9F9A39F93AF8A536E4" authorityName="Gilbert" authorityYear="1890" box="[580,740,1736,1761]" class="Actinopterygii" family="Dussumieriidae" genus="Etrumeus" kingdom="Animalia" order="Clupeiformes" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="acuminatus">
<emphasis id="B963EAD2FFBBFF9F9A39F93AF8A536E4" box="[580,740,1736,1761]" italics="true" pageId="42" pageNumber="43">E. acuminatus</emphasis>
