<quantity id="4CEF9B25FFBBFF9F9BDCF9E1F9BE362B" box="[929,1023,1555,1582]" metricMagnitude="-1" metricUnit="m" metricValue="3.05" pageId="42" pageNumber="43" unit="cm" value="30.5">30.5 cm</quantity>
(
<quantity id="4CEF9B25FFBBFF9F9C6EF9E1FE13362B" box="[1043,1106,1555,1582]" metricMagnitude="-1" metricUnit="m" metricValue="3.048" pageId="42" pageNumber="43" unit="in" value="12.0">12 in</quantity>
) TL (
<bibRefCitation id="EF864B31FFBBFF9F9CE4F9E1FFD03628" author="Miller, D. J. &amp; Lea, R. N." box="[1177,1425,1555,1582]" pageId="42" pageNumber="43" refId="ref302113" refString="Miller, D. J. &amp; Lea, R. N. (1972) Guide to the coastal marine fishes of California. California Department of Fish and Game Fish Bulletin, 157." type="book" year="1972">Miller and Lea 1972</bibRefCitation>
). Circumglobal; 
<collectingCountry id="F3007650FFBBFF9F9916F9CAFBEB3657" box="[363,426,1592,1618]" name="Japan" pageId="42" pageNumber="43">Japan</collectingCountry>
(Aonuma in 
<bibRefCitation id="EF864B31FFBBFF9F9A4AF9C5F88F3654" author="Nakabo, T." box="[567,718,1591,1617]" pageId="42" pageNumber="43" refId="ref304533" refString="Nakabo, T. (Ed.). (2002) Fishes of Japan with Pictorial Keys to the Species. Tokai University Press, Tokyo." type="book" year="2002">Nakabo 2002</bibRefCitation>
); Monterey Bay, central California to 
<collectingCountry id="F3007650FFBBFF9F9C18F9C5FEE03654" box="[1125,1185,1591,1617]" name="Chile" pageId="42" pageNumber="43">Chile</collectingCountry>
(
<bibRefCitation id="EF864B31FFBBFF9F9CD3F9C5FFD13654" author="Miller, D. J. &amp; Lea, R. N." box="[1198,1424,1591,1618]" pageId="42" pageNumber="43" refId="ref302113" refString="Miller, D. J. &amp; Lea, R. N. (1972) Guide to the coastal marine fishes of California. California Department of Fish and Game Fish Bulletin, 157." type="book" year="1972">Miller and Lea 1972</bibRefCitation>
), including Gulf of California (Whitehead and Rodríguez-Sánchez in 
<bibRefCitation id="EF864B31FFBBFF9F9BADF9A9FEE93673" author="Fischer, W. &amp; Krupp, F. &amp; Schneider, W. &amp; Sommer, C. &amp; Carpenter, K. E. &amp; Niem, V. H." box="[976,1192,1627,1654]" pageId="42" pageNumber="43" refId="ref278931" refString="Fischer, W., Krupp, F., Schneider, W., Sommer, C., Carpenter, K. E. &amp; Niem, V. H. (1995) Guia FAO para la identificacion para los fines de la pesca. Pacifico centro-oriental. Volume II, Vertebrados, Parte 1. Volume III, Vertebrados, Parte 2. FAO, Rome." type="book" year="1995">
Fischer 
<emphasis id="B963EAD2FFBBFF9F9C56F9AEFE273670" box="[1067,1126,1627,1653]" italics="true" pageId="42" pageNumber="43">et al.</emphasis>
1995
</bibRefCitation>
) and Islas Galápagos (
<bibRefCitation id="EF864B31FFBBFF9F98B0F98DF840369C" author="Grove, J. S. &amp; Lavenberg, R. J." box="[205,513,1663,1689]" pageId="42" pageNumber="43" refId="ref284033" refString="Grove, J. S. &amp; Lavenberg, R. J. (1997) The Fishes of the Galapagos Islands. Stanford University Press, Stanford." type="book" year="1997">Grove and Lavenberg 1997</bibRefCitation>
). Nearshore (
<bibRefCitation id="EF864B31FFBBFF9F9AEEF98DF987369C" author="Grove, J. S. &amp; Lavenberg, R. J." box="[659,966,1663,1689]" pageId="42" pageNumber="43" refId="ref284033" refString="Grove, J. S. &amp; Lavenberg, R. J. (1997) The Fishes of the Galapagos Islands. Stanford University Press, Stanford." type="book" year="1997">Grove and Lavenberg 1997</bibRefCitation>
); depth: intertidal to 
<quantity id="4CEF9B25FFBBFF9F9CD3F98DFEB7369F" box="[1198,1270,1663,1690]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="42" pageNumber="43" unit="m" value="200.0">200 m</quantity>
(
<quantity id="4CEF9B25FFBBFF9F9D79F98DFF0B369F" box="[1284,1354,1663,1690]" metricMagnitude="2" metricUnit="m" metricValue="1.999488" pageId="42" pageNumber="43" unit="ft" value="656.0">656 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FFBBFF9F98BBF951FBC536B8" author="Suda, Y. &amp; Inoue, T. &amp; Uchida, H." box="[198,388,1699,1725]" pageId="42" pageNumber="43" pagination="81 - 96" refId="ref326628" refString="Suda, Y., Inoue, T. &amp; Uchida, H. (2002) Fish communities in the surf zone of a protected sandy beach at Doigahama, Yamaguchi Prefecture, Japan. Estuarine and Coastal Shelf Science, 55, 81 - 96. https: // doi. org / 10.1006 / ecss. 2001.0888" type="journal article" year="2002">
Suda 
<emphasis id="B963EAD2FFBBFF9F997BF956FB0136B8" box="[262,320,1699,1725]" italics="true" pageId="42" pageNumber="43">et al.</emphasis>
2002
</bibRefCitation>
; max.: 
<bibRefCitation id="EF864B31FFBBFF9F99A5F951F94636B8" author="Robertson, D. R. &amp; Allen, G. R." box="[472,775,1699,1725]" pageId="42" pageNumber="43" refId="ref316751" refString="Robertson, D. R. &amp; Allen, G. R. (2008) Shorefishes of the Tropical Eastern Pacific: an Information System. Version 1.0 (2008). Smithsonian Tropical Research Institute, Balboa." type="book" year="2008">Robertson and Allen 2008</bibRefCitation>
). We follow 
<bibRefCitation id="EF864B31FFBBFF9F9BE4F951FE3736B8" author="Fricke, R. &amp; Eschmeyer, W. N. &amp; van der Laan, R." box="[921,1142,1699,1725]" pageId="42" pageNumber="43" refId="ref280407" refString="Fricke, R., Eschmeyer, W. N. &amp; van der Laan, R. (Eds). 2020. Eschmeyer's Catalog of Fishes: Genera, Species, Reference. Available from: http: // researcharchive. calacademy. org / research / ichthyology / catalog / fishcatmain. asp (accessed 17 April 2021)." type="url" year="2020">
Fricke 
<emphasis id="B963EAD2FFBBFF9F9B94F956FE6236B8" box="[1001,1059,1699,1725]" italics="true" pageId="42" pageNumber="43">et al.</emphasis>
(2020)
</bibRefCitation>
and place 
<taxonomicName id="4C174D43FFBBFF9F9C8EF956FB2D36E4" authority="(DeKay, 1842)" baseAuthorityName="DeKay" baseAuthorityYear="1842" class="Actinopterygii" family="Dussumieriidae" genus="Etrumeus" kingdom="Animalia" order="Clupeiformes" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="teres">
<emphasis id="B963EAD2FFBBFF9F9C8EF956FFDD36B8" box="[1267,1436,1700,1725]" italics="true" pageId="42" pageNumber="43">Etrumeus teres</emphasis>
(DeKay, 1842)
</taxonomicName>
in synonymy with 
<taxonomicName id="4C174D43FFBBFF9F9A39F93AF8A536E4" authorityName="Gilbert" authorityYear="1890" box="[580,740,1736,1761]" class="Actinopterygii" family="Dussumieriidae" genus="Etrumeus" kingdom="Animalia" order="Clupeiformes" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="acuminatus">
<emphasis id="B963EAD2FFBBFF9F9A39F93AF8A536E4" box="[580,740,1736,1761]" italics="true" pageId="42" pageNumber="43">E. acuminatus</emphasis>
</taxonomicName>
.
</paragraph>
