<number id="D124584DC26CFFA33EE44476FC02F9F8" box="[947,964,1627,1651]" pageId="14" pageNumber="281" value="4.0">4</number>
; caudal-fin rays 
<number id="D124584DC26CFFA339D24476FB50F9F8" box="[1157,1174,1627,1651]" pageId="14" pageNumber="281" value="9.0">9</number>
