<number id="D124584DC26CFFA33C8D4453FDC8F91C" box="[474,526,1662,1687]" pageId="14" pageNumber="281" value="23.5">23.5</number>
; illicium 
<number id="D124584DC26CFFA33F2F4452FD51F91C" box="[632,663,1663,1687]" pageId="14" pageNumber="281" value="32.0">32</number>
