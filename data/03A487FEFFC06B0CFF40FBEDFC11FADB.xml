<taxonomicName id="4C0D4D6BFFC06B0CFD93FB29FBE3FB33" authority="Walley &amp; MacKay, 1963" authorityName="Walley &amp; MacKay" authorityYear="1963" box="[580,1091,1157,1182]" class="Insecta" family="Braconidae" genus="Streblocera" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="258" phylum="Arthropoda" rank="species" species="pulvillicornis">
