<taxonomicName id="4C454D3BF63AF03A893F62AD99A7FA0D" authority="Audouin, 1826" authorityName="Audouin" authorityYear="1826" box="[839,1182,1436,1460]" class="Gymnolaemata" family="Celleporidae" genus="Cellepora" kingdom="Animalia" order="Cheilostomatida" pageId="3" pageNumber="126" phylum="Bryozoa" rank="species" species="bernardii">
