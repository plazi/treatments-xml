</taxonomicName>
, occasionallyon 
<taxonomicName id="4C6CA1CF66F5B48DC3CA480F3B4422C9" box="[758,862,895,920]" class="Magnoliopsida" family="Asteraceae" genus="Anthemis" kingdom="Plantae" order="Asterales" pageId="160" pageNumber="161" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E66F5B48DC3CA480F3B4422C9" box="[758,862,895,920]" italics="true" pageId="160" pageNumber="161">Anthemis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6CA1CF66F5B48DC253480F3C3D22C9" box="[879,1063,895,920]" class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" kingdom="Plantae" order="Asterales" pageId="160" pageNumber="161" phylum="Tracheophyta" rank="genus">
