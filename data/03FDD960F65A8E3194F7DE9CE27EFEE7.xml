<taxonomicName id="4C5413F5F65A8E3096B1D9D9E529FB60" authority="Millidge &amp; Russell-Smith, 1992" authorityName="Millidge &amp; Russell-Smith" authorityYear="1992" box="[721,1315,1225,1250]" class="Arachnida" family="Linyphiidae" genus="Ketambea" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="335" phylum="Arthropoda" rank="species" species="rostrata">
