<taxonomicName id="269449C0FFE60F0DFEEE76F971F0C1F3" authority="(Svenson) W. P. Adams" authorityName="W. P. Adams" baseAuthorityName="Svenson" class="Magnoliopsida" family="Hypericaceae" genus="Hypericum" kingdom="Plantae" order="Malpighiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="lloydii">
