</taxonomicName>
; and peelbark St. Johnswort, 
<taxonomicName id="269449C0FFE60F0DFE2D7611702AC1AB" authority="Lam" authorityName="Lam" class="Magnoliopsida" family="Hypericaceae" genus="Hypericum" kingdom="Plantae" order="Malpighiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="fasciculatus">
<emphasis id="D3E0EE51FFE60F0DFE2D761171E1C1AA" italics="true" pageId="14" pageNumber="15">Hypericum fasciculatus</emphasis>
Lam
</taxonomicName>
(previously 
<taxonomicName id="269449C0FFE60F0DFE4276367365C1AB" box="[427,582,1642,1666]" class="Magnoliopsida" family="Hypericaceae" kingdom="Plantae" order="Malpighiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="family">Hypericaceae</taxonomicName>
