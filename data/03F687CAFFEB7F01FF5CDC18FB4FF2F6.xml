<quantity id="4CA79B39FFEB7F01FDC8DE83FDFFF6E5" box="[515,624,982,1010]" metricMagnitude="-4" metricUnit="m" metricValue="5.6" pageId="14" pageNumber="215" unit="mm" value="0.56">0.56 mm</quantity>
). 
