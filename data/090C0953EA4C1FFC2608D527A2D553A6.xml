<taxonomicName id="ECD696F721591124F13173901EB01483" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136561" class="Insecta" family="Formicidae" genus="Cataulacus" kingdom="Animalia" lsidName-HNS="Cataulacus traegaordhi var. plectroniae Wheeler" order="Hymenoptera" pageNumber="198" phylum="Arthropoda" rank="variety" species="traegaordhi" variety="plectroniae">Cataulacus traegaordhi Santschi variety plectroniae</taxonomicName>
