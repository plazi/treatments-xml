<taxonomicName id="4C204D1FFF97216BD9DFF81DFD2865C3" authority="Chenoll et al. 2009" authorityName="Chenoll" authorityYear="2009" box="[232,647,1931,1956]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="26" pageNumber="2808" phylum="Firmicutes" rank="species" species="tucceti">
