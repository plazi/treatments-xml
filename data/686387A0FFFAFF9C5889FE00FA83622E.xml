<location id="E515606DFFFAFF9C58F3FE74FE82636D" LSID="urn:lsid:plazi:treatment:686387A0FFFAFF9C5889FE00FA83622E:E515606DFFFAFF9C58F3FE74FE82636D" box="[237,305,505,532]" county="Napo River" municipality="America" name="South" pageId="55" pageNumber="56">South</location>
