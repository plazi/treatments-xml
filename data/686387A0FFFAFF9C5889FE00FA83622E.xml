<location id="E515606DFFFAFF9C5C4DFE5BFB0D6096" LSID="urn:lsid:plazi:treatment:686387A0FFFAFF9C5889FE00FA83622E:E515606DFFFAFF9C5C4DFE5BFB0D6096" box="[1107,1214,470,495]" county="Napo River" municipality="America" name="Rio Napo" pageId="55" pageNumber="56">Rio Napo</location>
