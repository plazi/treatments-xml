<location id="E515606DFFFAFF9C5CD5FE58FAF06096" LSID="urn:lsid:plazi:treatment:686387A0FFFAFF9C5889FE00FA83622E:E515606DFFFAFF9C5CD5FE58FAF06096" box="[1227,1347,469,495]" county="Napo River" municipality="America" name="Equatorial" pageId="55" pageNumber="56">Equatorial</location>
