<taxonomicName id="340922BA7D5EEB4D1608A5A3A4C60FC1" authorityName="S. Fuentes, Uotila &amp; Borsch, Willdenowia 42 (1): 14" authorityYear="2012" baseAuthorityName="L." class="Magnoliopsida" family="Chenopodiacea" genus="Chenopodiastrum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Chenopodiastrum murale" order="Caryophyllales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="species" species="murale">
