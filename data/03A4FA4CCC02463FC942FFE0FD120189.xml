<heading id="D0FAFC36CC024638C942FFE0FC4606E1" box="[584,1003,1751,1778]" centered="true" fontSize="11" level="2" pageId="2" pageNumber="3" reason="2">
<taxonomicName id="4C0D30D9CC024638C942FFE0FCEE06E1" authorityName="Paulson" authorityYear="1875" box="[584,835,1751,1778]" class="Malacostraca" family="Mysidae" genus="Siriella" kingdom="Animalia" order="Mysida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="brevicaudata">
<emphasis id="B9799748CC024638C942FFE0FCEE06E1" bold="true" box="[584,835,1751,1778]" italics="true" pageId="2" pageNumber="3">Siriella brevicaudata</emphasis>
