<number id="62560404FFDEBA45FCFCB4E4F461D2A3" box="[835,887,920,944]" pageId="36" pageNumber="37" value="23.5">23.5</number>
, LW 
<number id="62560404FFDEBA45FC0CB4E4F4C4D2A3" box="[947,978,920,944]" pageId="36" pageNumber="37" value="52.0">52</number>
, 
<number id="62560404FFDEBA45FC62B4E4F316D2A3" box="[989,1024,920,944]" pageId="36" pageNumber="37" value="3.9">3.9</number>
