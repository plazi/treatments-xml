<emphasis id="B90FEADCFFE0FFA0FF5E141BFE86FE16" bold="true" box="[151,344,362,387]" pageId="32" pageNumber="619">Included species.</emphasis>
<taxonomicName id="4C7B4D4DFFE0FFA0FE97141CFD5DFE17" baseAuthorityName="Gerstaecker" baseAuthorityYear="1855" box="[350,643,364,387]" class="Insecta" family="Ripiphoridae" genus="Macrosiagon" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="619" phylum="Arthropoda" rank="species" species="octomaculata">
