<taxonomicName id="FD1E0B47958281A9E5AC0D894360BB6E" family="Hesperiidae" lsidName="" pageId="146" pageNumber="147" rank="family">Hesperiidae</taxonomicName>
, 
<taxonomicName id="9962AF037AF0E9C109972AC3322B94EA" class="Insecta" family="Hesperiidae" genus="Xenophanes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xenophanes tryxus" order="Lepidoptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="tryxus">Xenophanes tryxus</taxonomicName>
