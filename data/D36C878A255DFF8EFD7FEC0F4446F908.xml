<taxonomicName id="9CC54D1F2553FF8EFEE9EA4040C0FABD" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="89" pageNumber="89" phylum="Tracheophyta" rank="species" species="camaldulensis">
<emphasis id="69B1EA8E2553FF8EFEE9EA4040C0FABD" italics="true" pageId="89" pageNumber="89">Eucalyptus camaldulensis</emphasis>
</taxonomicName>
(
<taxonomicName id="9CC54D1F2553FF8EFF03EA61414DFABD" box="[148,267,1383,1407]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="89" pageNumber="89" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
: 
<taxonomicName id="9CC54D1F2553FF8EFE82EA6141DAFABD" authorityName="Peter G.Wilson" authorityYear="2005" box="[277,412,1383,1407]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="89" pageNumber="89" phylum="Tracheophyta" rank="tribe" tribe="Eucalypteae">Eucalypteae</taxonomicName>
