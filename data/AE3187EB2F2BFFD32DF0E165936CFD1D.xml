<taxonomicName id="E1984D7E2F2BFFD42DEFE165947CF9BC" authority="(GAUDANT, 1977)" baseAuthorityName="GAUDANT" baseAuthorityYear="1977" box="[918,1350,1611,1635]" class="Actinopterygii" family="Latidae" genus="Eolates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="132" phylum="Chordata" rank="species" species="aquensis">
<emphasis id="14ECEAEF2F2BFFD42DEFE165954DF9BE" box="[918,1143,1611,1634]" italics="true" pageId="3" pageNumber="84">EOLATES AQUENSIS</emphasis>
(
<bibRefCitation id="42094B0C2F2BFFD42AFFE1659405F9BC" author="Gaudant J" box="[1158,1343,1611,1635]" pageId="3" pageNumber="84" pagination="205 - 210" refId="ref37160" refString="Gaudant J. 1977. Additions a l'ichthyofaune stampienne d'Aix-en-Provence (Bouches-du-Rhone). Geologie Mediterraneenne 4: 205 - 210." type="journal article" year="1977">GAUDANT, 1977</bibRefCitation>
