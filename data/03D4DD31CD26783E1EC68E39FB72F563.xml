<heading id="D08ADB4BCD26783D1EC68E39FDF0F568" bold="true" box="[151,581,1627,1654]" fontSize="11" level="1" pageId="5" pageNumber="123" reason="1">
<emphasis id="B909B035CD26783D1EC68E39FDF0F568" bold="true" box="[151,581,1627,1654]" pageId="5" pageNumber="123">
<number id="3DB57E00CD26783D1EC68E39FF12F568" box="[151,167,1628,1654]" pageId="5" pageNumber="123" value="3.0">3</number>
. 
<taxonomicName id="4C7D17A4CD26783D1EE58E3EFE55F56B" box="[180,480,1627,1653]" class="Insecta" family="Cicadellidae" genus="Drabescus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="123" phylum="Arthropoda" rank="species" species="multidentatus" status="sp. nov.">
<emphasis id="B909B035CD26783D1EE58E3EFE55F56B" bold="true" box="[180,480,1627,1653]" italics="true" pageId="5" pageNumber="123">Drabescus multidentatus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23A0D4ECD26783D1FB68E39FDF0F568" box="[487,581,1628,1654]" pageId="5" pageNumber="123" rank="species">sp. nov.</taxonomicNameLabel>
