<collectingCountry id="F32876420530B168FBE7D15DFB96FAAB" box="[1032,1149,1382,1405]" name="Madagascar" pageId="16" pageNumber="55">Madagascar</collectingCountry>
, Prov. 
<collectingRegion id="49FBF8300530B168FD49D1BAFCF7FA4E" box="[678,796,1409,1432]" country="Madagascar" name="Antsiranana" pageId="16" pageNumber="55">Antsiranana</collectingRegion>
, 
<collectingCounty id="62E14E5E0530B168FCCBD1BAFC9EFA4E" box="[804,885,1409,1432]" pageId="16" pageNumber="55">Nosy Be</collectingCounty>
, 
<collectingMunicipality id="6BE4ACA80530B168FC91D1BAFC2DFA4E" box="[894,966,1409,1432]" pageId="16" pageNumber="55">Lokobe</collectingMunicipality>
RNI, [
<geoCoordinate id="EE0B50150530B168FBE7D1BAFBBDFA4E" box="[1032,1110,1409,1432]" degrees="13" direction="south" minutes="24" orientation="latitude" pageId="16" pageNumber="55" precision="925" value="-13.4">13°24’S</geoCoordinate>
