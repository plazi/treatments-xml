<geoCoordinate id="EE0B50150530B168FBB0D1BAFB5AFA4E" box="[1119,1201,1409,1432]" degrees="48" direction="east" minutes="19" orientation="longitude" pageId="16" pageNumber="55" precision="925" value="48.316666">48°19’E</geoCoordinate>
], 1853, 
