<location id="8EE060090530B168FD0BD1A0FCC8FA64" LSID="urn:lsid:plazi:treatment:039687C40530B169FCC3D116FD93FD78:8EE060090530B168FD0BD1A0FCC8FA64" box="[740,803,1435,1458]" country="Madagascar" county="Nosy Be" latitude="-13.4" longLatPrecision="1291" longitude="48.316666" municipality="Lokobe" name="Boivin" pageId="16" pageNumber="55" stateProvince="Antsiranana">Boivin</location>
