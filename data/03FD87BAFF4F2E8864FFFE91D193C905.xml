. North-east Atlantic, 
<geoCoordinate id="EE60506BFF4F2E886625FD6BD0EEC8DD" box="[589,673,722,746]" degrees="41" direction="north" minutes="1" orientation="latitude" pageId="163" pageNumber="164" precision="925" value="41.016666">41°1’N</geoCoordinate>
, 
<geoCoordinate id="EE60506BFF4F2E8866C5FD68D149C8DD" box="[685,774,721,746]" degrees="9" direction="west" minutes="54" orientation="longitude" pageId="163" pageNumber="164" precision="925" value="-9.9">9°54’W</geoCoordinate>
and 
<geoCoordinate id="EE60506BFF4F2E886755FD6BD134C8DD" box="[829,891,722,746]" degrees="38" direction="north" orientation="latitude" pageId="163" pageNumber="164" precision="55555" value="38.0">38°N</geoCoordinate>
, 67– 
<geoCoordinate id="EE60506BFF4F2E8867DAFD68D1BCC8DD" box="[946,1011,721,746]" degrees="71" direction="west" orientation="longitude" pageId="163" pageNumber="164" precision="55555" value="-71.0">71°W</geoCoordinate>
