<number id="9310B22DFFEDFFFC895BF9FCFE8BF9DE" box="[291,305,1555,1579]" pageId="10" pageNumber="325" value="3.0">3</number>
c, 
<number id="9310B22DFFEDFFFC8933F9FDFEE2F9DE" box="[331,344,1554,1579]" pageId="10" pageNumber="325" value="5.0">5</number>
c–e, 
