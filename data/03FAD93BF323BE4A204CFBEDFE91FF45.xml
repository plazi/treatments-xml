<taxonomicName id="4C5313AEF326BE4B2097F9CBFD96F9FE" authorityName="Reis &amp; Jr &amp; Flechtmann &amp; Navia" authorityYear="2014" box="[444,545,1635,1659]" class="Arachnida" family="Eriophyidae" genus="Thamnacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="1147" phylum="Arthropoda" rank="species" species="paubrasil">
