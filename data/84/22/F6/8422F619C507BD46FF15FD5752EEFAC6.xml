locality:
</emphasis>
<collectingCountry box="[368,484,1225,1251]" name="Argentina" pageId="23" pageNumber="254">Argentina</collectingCountry>
: 
<collectingRegion box="[494,705,1225,1251]" country="Argentina" name="Neuquen" pageId="23" pageNumber="254">Neuquén province</collectingRegion>
