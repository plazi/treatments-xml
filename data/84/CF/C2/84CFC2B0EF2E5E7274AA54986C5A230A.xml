<pageBreakToken pageId="null" pageNumber="412" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName authority="Anderson" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="412" phylum="Tracheophyta" rank="species" species="suberectus">
