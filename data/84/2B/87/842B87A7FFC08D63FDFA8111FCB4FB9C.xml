<paragraph blockId="20.[189,310,1835,1861]" box="[189,310,1835,1861]" pageId="20" pageNumber="21">
<heading bold="true" box="[189,310,1835,1861]" fontSize="11" level="5" pageId="20" pageNumber="21" reason="2">
<emphasis bold="true" box="[189,310,1835,1861]" pageId="20" pageNumber="21">Diagnosis</emphasis>
