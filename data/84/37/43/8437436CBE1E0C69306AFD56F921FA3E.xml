<paragraph blockId="56.[212,426,1476,1500]" box="[212,426,1476,1500]" pageId="56" pageNumber="2365">
<heading box="[212,426,1476,1500]" fontSize="10" level="3" pageId="56" pageNumber="2365" reason="5">
<emphasis box="[212,290,1476,1500]" italics="true" pageId="56" pageNumber="2365">Length</emphasis>
. 
<quantity box="[306,422,1476,1500]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="56" pageNumber="2365" unit="mm" value="2.75" valueMax="3.0" valueMin="2.5">2.5–3 mm</quantity>
