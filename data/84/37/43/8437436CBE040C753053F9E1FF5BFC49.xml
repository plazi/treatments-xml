<taxonomicName authorityName="Reid" authorityYear="1951" box="[333,458,810,834]" class="Malacostraca" family="Stenothoidae" genus="Stenothoe" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="38" pageNumber="2347" phylum="Arthropoda" rank="species" species="gallensis">
