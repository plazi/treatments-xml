<subSubSection pageId="2" pageNumber="91" type="diagnosis">
<paragraph blockId="2.[189,457,896,922]" box="[189,457,896,922]" pageId="2" pageNumber="91">
<heading bold="true" box="[189,457,896,922]" fontSize="11" level="3" pageId="2" pageNumber="91" reason="3">
