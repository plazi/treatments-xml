<collectingCountry box="[360,427,1678,1703]" name="China" pageId="14" pageNumber="57">China</collectingCountry>
(Guangdong, 
<collectingCountry box="[588,674,1678,1703]" name="Taiwan" pageId="14" pageNumber="57">Taiwan</collectingCountry>
), 
<collectingCountry box="[693,805,1678,1703]" name="Indonesia" pageId="14" pageNumber="57">Indonesia</collectingCountry>
, 
<collectingCountry box="[816,945,1678,1703]" name="Philippines" pageId="14" pageNumber="57">Philippines</collectingCountry>
, 
<collectingCountry box="[956,1070,1678,1703]" name="Sri Lanka" pageId="14" pageNumber="57">Sri Lanka</collectingCountry>
.
</paragraph>
</subSubSection>
