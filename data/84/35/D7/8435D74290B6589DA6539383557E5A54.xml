<subSubSection pageId="0" pageNumber="101" type="reference_group">
<paragraph pageId="0" pageNumber="101">
≡ 
<taxonomicName class="Sordariomycetes" family="Nectriaceae" genus="Nectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Nectria rugulosa" order="Hypocreales" pageId="0" pageNumber="101" phylum="Ascomycota" rank="species" species="rugulosa">Nectria rugulosa</taxonomicName>
