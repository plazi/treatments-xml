<paragraph blockId="1.[136,359,481,508]" box="[136,359,481,508]" pageId="1" pageNumber="81">
<heading bold="true" box="[136,359,481,508]" fontSize="11" level="1" pageId="1" pageNumber="81" reason="1">
<taxonomicName authorityName="Stephani" authorityYear="1902" box="[136,359,481,508]" class="Jungermanniopsida" family="Adelanthaceae" genus="Syzygiella" kingdom="Plantae" order="Jungermanniales" pageId="1" pageNumber="81" phylum="Marchantiophyta" rank="species" species="contigua">
<emphasis bold="true" box="[136,359,481,508]" italics="true" pageId="1" pageNumber="81">Syzygiella contigua</emphasis>
</taxonomicName>
