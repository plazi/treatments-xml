(
<bibRefCitation author="Linnaeus" box="[159,342,338,364]" pageId="5" pageNumber="60" refString="Linnaeus, C. (1758) Systema naturae per regna tria naturae, secundum classes, ordines, genera, species, cum characteribus, differentis, synonymis, locis. Laur. Salvii, Holmiae, 824 pp." type="book" year="1758">
Linnaeus, 
<number box="[282,342,338,364]" pageId="5" pageNumber="54" value="1758.0">1758</number>
