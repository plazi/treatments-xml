<subSubSection pageId="4" pageNumber="53" type="distribution">
<paragraph blockId="4.[151,1436,151,402]" pageId="4" pageNumber="53">
<emphasis bold="true" box="[198,359,226,252]" pageId="4" pageNumber="53">Distribution.</emphasis>
From 
<collectingCountry box="[441,545,226,252]" name="Tanzania" pageId="4" pageNumber="53">Zanzibar</collectingCountry>
and 
<collectingCountry box="[607,748,226,252]" name="Madagascar" pageId="4" pageNumber="53">Madagascar</collectingCountry>
north to 
<collectingCountry box="[862,934,226,252]" name="Oman" pageId="4" pageNumber="53">Oman</collectingCountry>
and eastward to the 
<collectingCountry box="[1187,1319,226,252]" name="Philippines" pageId="4" pageNumber="53">Philippines</collectingCountry>
and 
<collectingCountry name="New Caledonia" pageId="4" pageNumber="53">
New 
<taxonomicName box="[151,273,264,290]" class="Malacostraca" family="Albuneidae" genus="Austrolepidopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="54" phylum="Arthropoda" rank="species" species="caledonia">Caledonia</taxonomicName>
</collectingCountry>
