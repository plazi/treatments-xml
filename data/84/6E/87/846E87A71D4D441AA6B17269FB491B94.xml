<quantity box="[603,692,264,290]" metricMagnitude="1" metricUnit="m" metricValue="2.4" metricValueMax="4.5" metricValueMin="0.3" pageId="4" pageNumber="53" unit="m" value="24.0" valueMax="45.0" valueMin="3.0">3–45 m</quantity>
