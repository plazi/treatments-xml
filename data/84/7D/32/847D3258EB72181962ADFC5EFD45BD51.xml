<subSubSection box="[151,811,1084,1106]" pageId="6" pageNumber="180" type="materials_examined">
<paragraph blockId="6.[151,811,1084,1106]" box="[151,811,1084,1106]" pageId="6" pageNumber="180">
Type species: 
<taxonomicName authority="Bartsch, 2018" authorityName="Bartsch" authorityYear="2018" box="[294,546,1084,1105]" class="Insecta" family="Sesiidae" genus="Cinctosesia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="180" phylum="Arthropoda" rank="species" species="polistiformis" status="sp. nov.">
<emphasis box="[294,546,1084,1105]" italics="true" pageId="6" pageNumber="180">Cinctosesia polistiformis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[553,633,1084,1106]" pageId="6" pageNumber="180" rank="species">
