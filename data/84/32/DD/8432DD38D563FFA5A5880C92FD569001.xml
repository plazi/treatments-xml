head; 3 
<emphasis box="[773,795,1790,1811]" italics="true" pageId="1" pageNumber="472">—</emphasis>
pronotum; 4 
<emphasis box="[927,949,1790,1811]" italics="true" pageId="1" pageNumber="472">—</emphasis>
metascutum; 5 
