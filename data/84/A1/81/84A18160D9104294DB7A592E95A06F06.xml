<taxonomicName class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pinus banksiana" order="Pinales" pageId="15" pageNumber="46" phylum="Tracheophyta" rank="species" species="banksiana">Pinus banksiana</taxonomicName>
