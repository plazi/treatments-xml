<quantity id="5F439B6FE65B683B316D248BB41AFA5B" box="[559,618,1485,1511]" metricMagnitude="1" metricUnit="m" metricValue="4.76758" pageId="2" pageNumber="403" unit="in" value="1877.0">1877</quantity>
