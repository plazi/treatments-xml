<taxonomicName id="E49D2BADDF0AFFA9B53C1BC8FD48F99E" authority="Evans, 1966" authorityName="Evans" authorityYear="1966" box="[307,674,1524,1548]" class="Insecta" family="Cicadellidae" genus="Soractellus" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="297" phylum="Arthropoda" rank="species" species="brunneus">
