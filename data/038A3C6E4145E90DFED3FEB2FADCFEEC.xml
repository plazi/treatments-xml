: 
<taxonomicName id="4C23F6FB4145E90BFE0DFD09FD49FD1A" box="[470,651,679,704]" class="Trematoda" family="Fellodistomidae" genus="Steringovermes" kingdom="Animalia" order="Plagiorchiida" pageId="2" pageNumber="3" phylum="Platyhelminthes" rank="genus">
