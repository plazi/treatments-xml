</taxonomicName>
and 
<taxonomicName id="4C23F6FB4145E90BFB07FD09FEAAFD32" class="Trematoda" family="Fellodistomidae" genus="Steringotrema" kingdom="Animalia" order="Plagiorchiida" pageId="2" pageNumber="3" phylum="Platyhelminthes" rank="genus">
<emphasis id="B957516A4145E90BFB07FD09FEAAFD32" italics="true" pageId="2" pageNumber="3">Steringotrema</emphasis>
</taxonomicName>
