<taxonomicName id="4C23F6FB4145E90BFEA3FD61FE37FD32" box="[376,501,719,744]" class="Trematoda" family="Fellodistomidae" genus="Steringovermes" kingdom="Animalia" order="Plagiorchiida" pageId="2" pageNumber="3" phylum="Platyhelminthes" rank="species" species="notacanthi">
<emphasis id="B957516A4145E90BFEA3FD61FE37FD32" box="[376,501,719,744]" italics="true" pageId="2" pageNumber="3">notacanthi</emphasis>
</taxonomicName>
