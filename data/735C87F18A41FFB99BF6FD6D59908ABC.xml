<taxonomicName id="3CF54D648A41FFB99EEDFC245B1E8ABD" authorityName="Rafinesque" authorityYear="1810" class="Chondrichthyes" family="Lamnidae" genus="Isurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lamniformes" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="oxyrinchus">
