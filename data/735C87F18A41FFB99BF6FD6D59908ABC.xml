<taxonomicName id="3CF54D648A41FFB9991BFC0159998ABD" baseAuthorityName="Bibron" baseAuthorityYear="1839" box="[634,917,996,1021]" class="Chondrichthyes" family="Carcharhinidae" genus="Carcharhinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carcharhiniformes" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="falciformis">
