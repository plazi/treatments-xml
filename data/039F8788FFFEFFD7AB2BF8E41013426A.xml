<taxonomicName id="4C364D1DFFFFFFD7AA84FE5712C841E6" box="[912,1021,420,446]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="species" species="sylvestris">
