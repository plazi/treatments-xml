<number id="7A6CAA5317615936CDF3FC2D96F92E85" box="[413,442,946,972]" pageId="2" pageNumber="21" value="18.0">18</number>
(
<number id="7A6CAA5317615936CDA2FC2D955A2E85" box="[460,537,946,972]" pageId="2" pageNumber="21" value="18.5" valueMax="19.0" valueMin="18.0">18–19</number>
