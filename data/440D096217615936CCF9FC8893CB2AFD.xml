<taxonomicName id="0BA4C3F717615936CEEDF96495BF2A5D" box="[643,764,1787,1812]" class="Arachnida" family="Eriophyidae" genus="Tegolophus" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="21" phylum="Arthropoda" rank="species" species="changbais">
