<number id="7A6CAA5317615936CE1AFC4595D22EBD" box="[628,657,986,1012]" pageId="2" pageNumber="21" value="15.0">15</number>
(
<number id="7A6CAA5317615936CECFFC4595AD2EBD" box="[673,750,986,1012]" pageId="2" pageNumber="21" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
