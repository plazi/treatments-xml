<th id="78365DFC182E007F975AFEE0FCC7FEA2" box="[352,847,310,333]" gridcol="1" gridrow="0" pageId="5" pageNumber="148">
<emphasis id="B7A3EAD0182EFF85975AFEE1FDECFEA2" box="[352,612,310,333]" italics="true" pageId="5" pageNumber="148">
<taxonomicName id="42D74D41182EFF85975AFEE1FDCBFEA3" box="[352,579,310,332]" class="Insecta" family="Tetrigidae" genus="Criotettix" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="148" phylum="Arthropoda" rank="species" species="gariyabandi">Criotettix gariyabandi</taxonomicName>
<emphasis id="B7A3EAD0182EFF859479FEE1FDC6FEA3" bold="true" box="[579,590,311,332]" italics="true" pageId="5" pageNumber="148">c</emphasis>
us
