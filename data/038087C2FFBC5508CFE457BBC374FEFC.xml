<emphasis id="B95DEAC6FFBC5509CFE454FDC1C9F91D" bold="true" box="[151,397,1689,1715]" pageId="24" pageNumber="25">Material examined:</emphasis>
<specimenCount id="9D2FFD5DFFBC5509CEE754FEC245F91A" box="[404,513,1690,1716]" pageId="24" pageNumber="25" type="larva">10 larvae</specimenCount>
deriving from 
<specimenCount id="9D2FFD5DFFBC5509CDC254FEC371F91A" box="[689,821,1690,1716]" pageId="24" pageNumber="25" type="female">10 females</specimenCount>
