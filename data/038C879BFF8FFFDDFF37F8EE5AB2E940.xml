<heading id="D0D281E1FF8FFFDFFF37F8EE5FD6ED2E" bold="true" box="[151,424,1888,1915]" fontSize="11" level="1" pageId="12" pageNumber="61" reason="1">
<emphasis id="B951EA9FFF8FFFDFFF37F8EE5FD6ED2E" bold="true" box="[151,424,1888,1915]" pageId="12" pageNumber="61">
<taxonomicName id="4C254D0EFF8FFFDFFF37F8EE5F20ED2F" box="[151,350,1889,1915]" class="Insecta" family="Eulophidae" genus="Astichus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="61" phylum="Arthropoda" rank="species" species="naiadis" status="n. sp.">
<emphasis id="B951EA9FFF8FFFDFFF37F8EE5F20ED2F" bold="true" box="[151,350,1889,1915]" italics="true" pageId="12" pageNumber="61">Astichus naiadis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26257E4FF8FFFDFFEC5F8EF5FD6ED2E" box="[357,424,1888,1914]" pageId="12" pageNumber="61" rank="species">n. sp.</taxonomicNameLabel>
