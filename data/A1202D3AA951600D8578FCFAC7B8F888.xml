spp. and 
<taxonomicName id="33944E03A165CC569FF473DA1087CCEF" class="Maxillopoda" family="Platylepadidae" genus="Platylepas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platylepas" order="Sessilia" pageId="4" pageNumber="39" phylum="Arthropoda" rank="genus">Platylepas</taxonomicName>
spp.
