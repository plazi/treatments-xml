<td id="908EE58C0AEEA135B2A17D2DAA708AA1" align="left" pageId="null" pageNumber="405" valign="top">
1. 
<normalizedToken id="D4D57BA891950CF029180466BDC77CAE" originalValue="Äußere" pageId="null" pageNumber="405">Aeussere</normalizedToken>
<normalizedToken id="B1D12A0FFCC8516DC59CF141A772CAFB" originalValue="Hüllblätter" pageId="null" pageNumber="405">Huellblaetter</normalizedToken>
abgerundet, auf dem 
<normalizedToken id="1E8A44CD8B69799B4FFE7F87222D2778" originalValue="Rücken" pageId="null" pageNumber="405">Ruecken</normalizedToken>
filzig behaart; 
<taxonomicName id="3E40446B5CCB6873F3C5DC132BE3FE60" class="Insecta" family="Miridae" genus="Pappus" kingdom="Animalia" order="Hemiptera" pageId="null" pageNumber="405" phylum="Arthropoda" rank="genus">Pappus</taxonomicName>
