1*. 
<normalizedToken id="E033D9C09E39EB666872FD0B1A940BA3" originalValue="Äußere" pageId="null" pageNumber="405">Aeussere</normalizedToken>
<normalizedToken id="13BAD85728E3E28FF15452BD35CD7F43" originalValue="Hüllblätter" pageId="null" pageNumber="405">Huellblaetter</normalizedToken>
