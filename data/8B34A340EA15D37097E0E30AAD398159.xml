<taxonomicName id="CBFE40DF57CDDCB1AA90D5DD7204EF30" class="Insecta" family="Miridae" genus="Pappus" kingdom="Animalia" order="Hemiptera" pageId="null" pageNumber="405" phylum="Arthropoda" rank="genus">Pappus</taxonomicName>
