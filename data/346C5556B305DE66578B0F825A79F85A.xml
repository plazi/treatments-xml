<collectingCountry id="C4D2A4D0B305DE66523108845C6CF88E" box="[1325,1393,1785,1809]" name="Brazil" pageId="5" pageNumber="26">Brazil</collectingCountry>
the species is also cited on 
<taxonomicName id="7BC59FC3B305DE6656BF09605B39F8AA" box="[419,548,1821,1845]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
, as well for 
<taxonomicName id="7BC59FC3B305DE6655AD09605A8AF8AA" box="[689,919,1821,1845]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="species" species="melongena">
<emphasis id="8EB13852B305DE6655AD09605A8AF8AA" box="[689,919,1821,1845]" italics="true" pageId="5" pageNumber="26">Solanum melongena</emphasis>
</taxonomicName>
; 
<taxonomicName id="7BC59FC3B305DE6654BA09605D7FF8AA" box="[934,1122,1821,1845]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="species" species="nigrum">
<emphasis id="8EB13852B305DE6654BA09605D7FF8AA" box="[934,1122,1821,1845]" italics="true" pageId="5" pageNumber="26">Solanum nigrum</emphasis>
</taxonomicName>
; 
<taxonomicName id="7BC59FC3B305DE66536E09635C89F8AA" box="[1138,1428,1821,1845]" class="Magnoliopsida" family="Solanaceae" genus="Lycopersicum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="26" phylum="Magnoliophyta" rank="species" species="esculentum">
<emphasis id="8EB13852B305DE66536E09635C89F8AA" box="[1138,1428,1821,1845]" italics="true" pageId="5" pageNumber="26">Lycopersicum esculentum</emphasis>
</taxonomicName>
; 
<taxonomicName id="7BC59FC3B305DE66578B093F581AF8C6" box="[151,263,1858,1881]" class="Magnoliopsida" family="Solanaceae" genus="Capsicum" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="genus">
<emphasis id="8EB13852B305DE66578B093F581AF8C6" box="[151,263,1858,1881]" italics="true" pageId="5" pageNumber="26">Capsicum</emphasis>
</taxonomicName>
sp., and 
<taxonomicName id="7BC59FC3B305DE665677093F5BC8F8C6" authority="Martinez, 1992" authorityName="Martinez" authorityYear="1992" box="[363,725,1857,1881]" class="Magnoliopsida" family="Convolvulaceae" genus="Ipomea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="26" phylum="Magnoliophyta" rank="species" species="batata">
<emphasis id="8EB13852B305DE665677093F5B13F8C6" box="[363,526,1857,1881]" italics="true" pageId="5" pageNumber="26">Ipomea batata</emphasis>
(
<bibRefCitation id="D85499B1B305DE665500093C5BD0F8C6" author="Martinez" box="[540,717,1857,1881]" pageId="5" pageNumber="26" refString="Martinez, A. (1992) Los Meloideos de Salta, Argentina (Coleoptera). Insecta Mundi, 6, 1 - 12." type="journal article" year="1992">
Martínez, 
<number id="0A0DF667B305DE66558D093C5BD0F8C6" box="[657,717,1857,1881]" pageId="5" pageNumber="26" value="1992.0">1992</number>
</bibRefCitation>
)
</taxonomicName>
.
</paragraph>
</subSubSection>
