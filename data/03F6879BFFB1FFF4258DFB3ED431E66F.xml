<taxonomicName id="4C5F4D0EFFB3FFF52610F8D6D59BE703" box="[778,932,1826,1851]" class="Arachnida" family="Eriophyidae" genus="Colopodacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="obovataus">
<emphasis id="B92BEA9FFFB3FFF52610F8D6D59BE703" box="[778,932,1826,1851]" italics="true" pageId="6" pageNumber="7">C. obovataus</emphasis>
</taxonomicName>
