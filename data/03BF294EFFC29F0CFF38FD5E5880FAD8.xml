<number id="3DDE8A7FFFC29F0CFB31FC5B5CB7FC54" box="[1182,1240,906,931]" pageId="2" pageNumber="231" value="1976.0">1976</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C16E3DBFFC29F0CFB5FFC5B5903FC30" authority="Vishniakova, 1976" authorityName="Vishniakova" authorityYear="1976" class="Insecta" family="Archipsyllidae" genus="Archipsyllopsis" higherTaxonomySource="CoL" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="231" phylum="Arthropoda" rank="genus">
