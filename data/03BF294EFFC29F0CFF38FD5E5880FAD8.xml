<taxonomicName id="4C16E3DBFFC29F0CFB09FC7F58BCFC1C" authority="Argyropulo, 1946" authorityName="Argyropulo" authorityYear="1946" class="Insecta" family="Hystrichopsyllidae" genus="Eopsylla" kingdom="Animalia" order="Siphonaptera" pageId="2" pageNumber="231" phylum="Arthropoda" rank="genus">
