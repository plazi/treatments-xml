<subSubSection id="1C956539E57CFFE468AC0B5E1CA95C53" pageId="64" pageNumber="65" type="etymology">
<paragraph id="543036B2E57CFFE468AC0B5E1CA95C53" blockId="64.[247,1332,1559,1620]" pageId="64" pageNumber="65">
<emphasis id="66FBEAA0E57CFFE468AC0B5E1C685C30" bold="true" box="[247,383,1560,1584]" pageId="64" pageNumber="65">Etymology.</emphasis>
