<taxonomicName id="C24F418FAA5EA2416FA25EC7AF52C8CB" LSID="urn:lsid:zoobank.org:act:7DBC53F5-0678-4639-8D36-C698DA87655C" authority="Wichard, Ross &amp; Ross, 2011" authorityName="Wichard, Ross &amp; Ross" authorityYear="2011" class="Insecta" family="Psychomyiidae" genus="Palerasnitsynus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Palerasnitsynus ohlhoffi" order="Trichoptera" pageId="2" pageNumber="325" phylum="Arthropoda" rank="species" species="ohlhoffi">
