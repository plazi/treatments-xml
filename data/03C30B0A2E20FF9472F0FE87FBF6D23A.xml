<taxonomicName id="4C6AC19F2E20FF947347FE41FE5ED1F2" authorityName="Dendy" authorityYear="1922" box="[288,445,430,455]" class="Demospongiae" family="Cladorhizidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poecilosclerida" pageId="2" pageNumber="166" phylum="Porifera" rank="family">Cladorhizidae</taxonomicName>
