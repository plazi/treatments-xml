<taxonomicName id="4C5A3DEAFF89FF95FDFED5C2BDBDFA65" authorityName="Cantor" authorityYear="1846" box="[639,758,1455,1480]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="galeritus">
<emphasis id="B92E9A7BFF89FF95FDFED5C2BDD8FA65" box="[639,659,1456,1479]" italics="true" pageId="23" pageNumber="24">H</emphasis>
. 
<emphasis id="B92E9A7BFF89FF95FD1FD5DDBDBDFA65" box="[670,758,1455,1479]" italics="true" pageId="23" pageNumber="24">galeritus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A21D2700FF89FF95FD7CD5C2BCCAFA6A" box="[765,897,1456,1480]" pageId="23" pageNumber="24" sensu="stricto">sensu stricto</taxonomicNameLabel>
