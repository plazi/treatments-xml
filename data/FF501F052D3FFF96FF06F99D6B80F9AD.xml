<taxonomicName id="B0F9D5902D3EFF96FE26FA5A6D11FA4D" authorityName="Scarbrough" authorityYear="2008" box="[480,542,1412,1435]" class="Insecta" family="Asilidae" genus="Ommatius" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="12" phylum="Arthropoda" rank="species" species="tepui">
