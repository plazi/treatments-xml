<taxonomicName class="Insecta" family="Nymphalidae" genus="Dryas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dryas" order="Lepidoptera" pageId="0" pageNumber="17" phylum="Arthropoda" rank="genus">
