<taxonomicName class="Insecta" family="Nymphalidae" genus="Dryas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dryas octopetala" order="Lepidoptera" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="octopetala">
<emphasis italics="true" pageId="0" pageNumber="17">Dryas octopetala</emphasis>
</taxonomicName>
, 
<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex misandra" order="Poales" pageId="0" pageNumber="17" phylum="Tracheophyta" rank="species" species="misandra">
<emphasis italics="true" pageId="0" pageNumber="17">Carex misandra</emphasis>
</taxonomicName>
, 
<taxonomicName authority="S.F.Gray, 1821" authorityName="Gray" authorityYear="1821" baseAuthorityName="Sw." class="Lecanoromycetes" family="Icmadophilaceae" genus="Cerania" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Cerania vermicularis" order="Pertusariales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="vermicularis">
