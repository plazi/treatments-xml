<taxonomicName class="Lecanoromycetes" family="Parmeliaceae" genus="Cetraria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Cetraria nivalis" order="Lecanorales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="nivalis">
<emphasis italics="true" pageId="0" pageNumber="17">Cetraria nivalis</emphasis>
