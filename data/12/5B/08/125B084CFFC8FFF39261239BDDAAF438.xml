<paragraph blockId="20.[264,1324,809,1315]" pageId="20" pageNumber="21">
<emphasis box="[264,487,809,834]" italics="true" pageId="20" pageNumber="21">Material examined</emphasis>
. 
<number box="[503,518,809,835]" pageId="20" pageNumber="21" value="1.0">1</number>
ɗ (
<taxonomicName box="[563,854,809,834]" class="Insecta" family="Chironomidae" genus="Metriocnemus" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="eurynotus">
