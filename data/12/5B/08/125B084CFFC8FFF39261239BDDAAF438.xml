: New Siberian Islands, west coast, 
<date box="[339,466,849,875]" pageId="20" pageNumber="21" value="1903-06-24">
<number box="[339,368,849,875]" pageId="20" pageNumber="21" value="24.0">24</number>
vi 
<number box="[405,466,849,875]" pageId="20" pageNumber="21" value="1903.0">1903</number>
</date>
, M. I. Brussnew. 
<number box="[671,686,849,875]" pageId="20" pageNumber="21" value="1.0">1</number>
ɗ (
<taxonomicName box="[728,972,849,874]" class="Insecta" family="Chironomidae" genus="Metriocnemus" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="brusti">
