<taxonomicName authority="Holmgren" authorityName="Holmgren" class="Insecta" family="Chironomidae" genus="Limnophyes" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="pumilio">
