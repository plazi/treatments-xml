<typeStatus box="[295,453,463,489]" pageId="20" pageNumber="21">paralectotype</typeStatus>
of 
<taxonomicName box="[498,628,463,488]" class="Insecta" family="Chironomidae" genus="Camptocladius" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="globifer">
