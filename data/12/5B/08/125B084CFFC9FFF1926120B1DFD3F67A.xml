<paragraph blockId="21.[264,1323,855,1523]" pageId="21" pageNumber="22">
Total length 
<quantity box="[467,644,895,921]" metricMagnitude="-3" metricUnit="m" metricValue="2.565" metricValueMax="2.87" metricValueMin="2.26" pageId="21" pageNumber="22" unit="mm" value="2.565" valueMax="2.87" valueMin="2.26">2.26–2.87 mm</quantity>
. Wing length 
<quantity box="[820,996,895,921]" metricMagnitude="-4" metricUnit="m" metricValue="7.25" metricValueMax="7.9" metricValueMin="6.6" pageId="21" pageNumber="22" unit="mm" value="0.725" valueMax="0.79" valueMin="0.66">0.66–0.79 mm</quantity>
