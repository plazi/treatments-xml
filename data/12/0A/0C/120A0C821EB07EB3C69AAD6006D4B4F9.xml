<subSubSection pageId="413" pageNumber="414" type="distribution">
<paragraph pageId="413" pageNumber="414">Distribution.</paragraph>
<paragraph pageId="413" pageNumber="414">Childress, Coryell, Dickens, Erath, Hidalgo</paragraph>
</subSubSection>
<subSubSection pageId="413" pageNumber="414" type="time of activity">
<paragraph pageId="413" pageNumber="414">Time of activity.</paragraph>
<paragraph pageId="413" pageNumber="414">Male (July); female (July - September)</paragraph>
</subSubSection>
<subSubSection pageId="413" pageNumber="414" type="habitat">
<paragraph pageId="413" pageNumber="414">Habitat.</paragraph>
<paragraph pageId="413" pageNumber="414">(crops: peanuts); (soil/woodland: post oak savanna with pasture)</paragraph>
</subSubSection>
<subSubSection pageId="413" pageNumber="414" type="method">
<paragraph pageId="413" pageNumber="414">Method.</paragraph>
<paragraph pageId="413" pageNumber="414">pitfall trap [f]</paragraph>
</subSubSection>
<subSubSection pageId="413" pageNumber="414" type="type">
<paragraph pageId="413" pageNumber="414">Type.</paragraph>
<paragraph pageId="413" pageNumber="414">Texas (female, Hidalgo Co., Edinburg, no date, S. Mulaik, holotype, AMNH)</paragraph>
</subSubSection>
