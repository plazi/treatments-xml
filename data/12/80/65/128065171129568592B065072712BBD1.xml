<emphasis italics="true" pageId="0" pageNumber="1">Neptunia oleracea</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Exell" authorityYear="1959" class="Dicotyledoneae" family="Malvaceae" genus="Hibiscus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Hibiscus meeusei" order="Malvales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="meeusei">
<emphasis italics="true" pageId="0" pageNumber="1">Hibiscus meeusei</emphasis>
</taxonomicName>
, 
<taxonomicName class="Monocotyledoneae" family="Cyperaceae" genus="Cyperus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Cyperus compressus" order="Cyperales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="compressus">
