<taxonomicName class="Magnoliopsida" family="Vahliaceae" genus="Vahlia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vahlia capensis" order="Boraginales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="capensis">
