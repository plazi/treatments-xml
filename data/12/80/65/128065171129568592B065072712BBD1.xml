, 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Senna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Senna obtusifolia" order="Fabales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="obtusifolia">
