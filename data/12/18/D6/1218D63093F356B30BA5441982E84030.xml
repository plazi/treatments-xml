<td align="left" pageId="null" pageNumber="474" valign="bottom">
<taxonomicName class="Insecta" family="Calliphoridae" genus="Cyanus" kingdom="Animalia" order="Diptera" pageId="null" pageNumber="474" phylum="Arthropoda" rank="species" species="arvensis">
