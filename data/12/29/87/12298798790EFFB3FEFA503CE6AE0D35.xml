<paragraph blockId="4.[264,1324,666,972]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[312,569,666,692]" pageId="4" pageNumber="5">
Referred specimen 
<number box="[554,569,666,692]" pageId="4" pageNumber="5" value="4.0">4</number>
