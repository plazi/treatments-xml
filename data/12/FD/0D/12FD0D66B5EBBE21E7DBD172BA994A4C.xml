, 
<taxonomicName genus="X." lsidName="X. planicollis" pageId="25" pageNumber="44" rank="species" species="planicollis">X. planicollis</taxonomicName>
, 
<taxonomicName genus="X." lsidName="X. pubescens" pageId="25" pageNumber="44" rank="species" species="pubescens">X. pubescens</taxonomicName>
