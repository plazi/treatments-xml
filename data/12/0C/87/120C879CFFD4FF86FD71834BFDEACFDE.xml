<taxonomicName authority="Jacq." authorityName="Jacq." class="Magnoliopsida" family="Apocynaceae" genus="Apocynum" kingdom="Plantae" order="Gentianales" pageId="21" pageNumber="29" phylum="Tracheophyta" rank="species" species="sibiricum">
