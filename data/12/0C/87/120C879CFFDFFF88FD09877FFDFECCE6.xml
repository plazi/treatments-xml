<quantity box="[1016,1172,1496,1518]" metricMagnitude="-4" metricUnit="m" metricValue="1.55" metricValueMax="1.6" metricValueMin="1.5" pageId="26" pageNumber="34" unit="mm" value="0.155" valueMax="0.16" valueMin="0.15">0.15–0.16 mm</quantity>
