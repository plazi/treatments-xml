<quantity box="[1030,1172,1416,1438]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" metricValueMax="1.07" metricValueMin="0.93" pageId="26" pageNumber="34" unit="mm" value="1.0" valueMax="1.07" valueMin="0.93">0.93–1.07 mm</quantity>
(mean = 1.00 mm, 
<emphasis box="[859,871,1443,1464]" italics="true" pageId="26" pageNumber="34">n</emphasis>
= 2); L/W = 1.82–1.83 (mean = 1.83, 
<emphasis box="[744,756,1470,1491]" italics="true" pageId="26" pageNumber="34">n</emphasis>
= 2); HW = 
<quantity box="[927,1068,1469,1491]" metricMagnitude="-4" metricUnit="m" metricValue="6.699999999999999" metricValueMax="7.1" metricValueMin="6.3" pageId="26" pageNumber="34" unit="mm" value="0.6699999999999999" valueMax="0.71" valueMin="0.63">0.63–0.71 mm</quantity>
(mean = 
<quantity box="[685,788,1496,1517]" metricMagnitude="-4" metricUnit="m" metricValue="6.7" pageId="26" pageNumber="34" unit="mm" value="0.67">0.67 mm</quantity>
, 
<emphasis box="[808,820,1496,1517]" italics="true" pageId="26" pageNumber="34">n</emphasis>
