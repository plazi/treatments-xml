(mean = 
<quantity box="[782,872,1417,1438]" metricMagnitude="-3" metricUnit="m" metricValue="1.83" pageId="26" pageNumber="34" unit="mm" value="1.83">1.83 mm</quantity>
, 
<emphasis box="[885,897,1417,1438]" italics="true" pageId="26" pageNumber="34">n</emphasis>
