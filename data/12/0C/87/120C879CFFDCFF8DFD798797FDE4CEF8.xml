<emphasis box="[362,374,354,375]" italics="true" pageId="30" pageNumber="38">n</emphasis>
= 9); L/W = 1.78–1.88 (mean = 1.84, 
<emphasis box="[315,327,380,401]" italics="true" pageId="30" pageNumber="38">n</emphasis>
= 9); HW = 0.86–1.00 mm (mean = 
<quantity box="[281,387,407,428]" metricMagnitude="-4" metricUnit="m" metricValue="9.5" pageId="30" pageNumber="38" unit="mm" value="0.95">0.95 mm</quantity>
, 
<emphasis box="[416,428,407,428]" italics="true" pageId="30" pageNumber="38">n</emphasis>
= 9); IOD = 
<quantity box="[151,293,433,455]" metricMagnitude="-4" metricUnit="m" metricValue="3.55" metricValueMax="4.3" metricValueMin="2.8" pageId="30" pageNumber="38" unit="mm" value="0.355" valueMax="0.43" valueMin="0.28">0.28–0.43 mm</quantity>
(mean = 
<quantity box="[400,490,434,455]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="30" pageNumber="38" unit="mm" value="0.35">0.35 mm</quantity>
, 
<emphasis box="[503,515,434,455]" italics="true" pageId="30" pageNumber="38">n</emphasis>
= 9); IOD/ HW = 0.33–0.43 (mean = 0.36, 
<emphasis box="[479,491,460,481]" italics="true" pageId="30" pageNumber="38">n</emphasis>
