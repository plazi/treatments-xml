<taxonomicName baseAuthorityName="Navas" baseAuthorityYear="1914" box="[1047,1149,1750,1775]" class="Insecta" family="Myrmeleontidae" genus="Dendroleon" kingdom="Animalia" order="Neuroptera" pageId="7" pageNumber="351" phylum="Arthropoda" rank="species" species="regius">
