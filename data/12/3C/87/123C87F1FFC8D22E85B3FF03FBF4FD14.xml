<paragraph blockId="2.[151,1437,1256,2038]" box="[199,1437,1256,1283]" pageId="2" pageNumber="346">
<emphasis bold="true" box="[199,345,1256,1282]" pageId="2" pageNumber="346">Description.</emphasis>
<emphasis box="[347,392,1257,1283]" italics="true" pageId="2" pageNumber="346">Size</emphasis>
.Headwidth 
<quantity box="[527,688,1256,1283]" metricMagnitude="-3" metricUnit="m" metricValue="2.545" metricValueMax="2.7" metricValueMin="2.39" pageId="2" pageNumber="346" unit="mm" value="2.545" valueMax="2.7" valueMin="2.39">2.39–2.70mm</quantity>
;forewinglength 
<quantity box="[869,1058,1256,1283]" metricMagnitude="-2" metricUnit="m" metricValue="3.5245" metricValueMax="3.71" metricValueMin="3.339" pageId="2" pageNumber="346" unit="mm" value="35.245" valueMax="37.1" valueMin="33.39">33.39–37.10mm</quantity>
;hindwinglength 
<quantity box="[1244,1433,1256,1283]" metricMagnitude="-2" metricUnit="m" metricValue="3.5065" metricValueMax="3.681" metricValueMin="3.332" pageId="2" pageNumber="346" unit="mm" value="35.065" valueMax="36.81" valueMin="33.32">33.32–36.81mm</quantity>
