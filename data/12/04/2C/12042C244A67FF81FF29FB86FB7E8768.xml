. 
<emphasis box="[365,474,577,602]" italics="true" pageId="23" pageNumber="313">
<collectingCountry box="[365,474,577,602]" name="Australia" pageId="23" pageNumber="313">Australia</collectingCountry>
</emphasis>
. 
<collectingRegion box="[490,633,577,603]" country="Australia" name="Queensland" pageId="23" pageNumber="313">Queensland</collectingRegion>
