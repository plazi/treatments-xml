<specimenCount box="[473,555,1378,1404]" pageId="33" pageNumber="323" type="male">1 male</specimenCount>
, ~ 
<quantity box="[581,675,1378,1404]" metricMagnitude="-3" metricUnit="m" metricValue="7.7" pageId="33" pageNumber="323" unit="mm" value="7.7">7.7 mm</quantity>
, 
