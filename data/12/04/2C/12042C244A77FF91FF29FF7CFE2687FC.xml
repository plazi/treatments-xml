<subSubSection pageId="7" pageNumber="297" type="discussion">
<paragraph blockId="7.[151,1436,151,719]" pageId="7" pageNumber="297">
<emphasis bold="true" box="[198,319,151,177]" pageId="7" pageNumber="297">Remarks.</emphasis>
<taxonomicName authorityName="Mayer" authorityYear="1890" box="[328,561,152,177]" class="Malacostraca" family="Caprellidae" genus="Hemiaegina" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="297" phylum="Arthropoda" rank="species" species="minuta">
