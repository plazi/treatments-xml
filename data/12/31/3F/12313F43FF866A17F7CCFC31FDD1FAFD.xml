<taxonomicName authority="Richardson, 1904" authorityName="Richardson" authorityYear="1904" box="[128,506,1325,1348]" class="Malacostraca" family="Bopyridae" genus="Urobopyrus" kingdom="Animalia" order="Isopoda" pageId="47" pageNumber="47" phylum="Arthropoda" rank="genus">
