<taxonomicName box="[1093,1215,1676,1698]" class="Malacostraca" family="Alpheidae" kingdom="Animalia" order="Decapoda" pageId="39" pageNumber="39" phylum="Arthropoda" rank="family">Alpheidae</taxonomicName>
), 
<collectingRegion box="[701,926,1706,1728]" country="China" name="Shandong" pageId="39" pageNumber="39">Shandong Province</collectingRegion>
