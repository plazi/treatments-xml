<collectingCountry box="[701,774,1737,1759]" name="China" pageId="39" pageNumber="39">China</collectingCountry>
, 
<quantity box="[787,876,1737,1759]" metricMagnitude="1" metricUnit="m" metricValue="1.3" metricValueMax="2.0" metricValueMin="0.6" pageId="39" pageNumber="39" unit="m" value="13.0" valueMax="20.0" valueMin="6.0">
6– 
<specimenCount box="[814,876,1737,1759]" pageId="39" pageNumber="39" type="generic">20 m</specimenCount>
