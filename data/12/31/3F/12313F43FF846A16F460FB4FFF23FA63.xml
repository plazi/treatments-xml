<subSubSection box="[818,1140,1218,1240]" pageId="45" pageNumber="45" type="nomenclature">
<paragraph blockId="45.[818,1140,1218,1240]" box="[818,1140,1218,1240]" pageId="45" pageNumber="45">
<heading box="[818,1140,1218,1240]" fontSize="9" level="2" pageId="45" pageNumber="45" reason="6">
<taxonomicName authority="Pearse, 1953" authorityName="Pearse" authorityYear="1953" box="[818,1140,1218,1240]" class="Malacostraca" family="Bopyridae" genus="Capitetragonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="45" pageNumber="45" phylum="Arthropoda" rank="genus">
<emphasis box="[818,987,1219,1240]" italics="true" pageId="45" pageNumber="45">Capitetragonia</emphasis>
<bibRefCitation author="Pearse, A. S." box="[996,1140,1218,1240]" pageId="45" pageNumber="45" pagination="187 - 243" refId="ref56269" refString="Pearse, A. S. 1953. Parasitic crustaceans from Alligator Harbor, Florida. Quarterly Journal of the Florida Academy of Sciences 15: 187 - 243." type="journal article" year="1953">Pearse, 1953</bibRefCitation>
