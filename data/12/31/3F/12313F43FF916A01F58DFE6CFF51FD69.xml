<taxonomicName authority="(Hay, 1917)" baseAuthorityName="Hay" baseAuthorityYear="1917" box="[160,543,634,657]" class="Malacostraca" family="Bopyridae" genus="Bopyrella" kingdom="Animalia" order="Isopoda" pageId="57" pageNumber="57" phylum="Arthropoda" rank="species" species="deformans">
