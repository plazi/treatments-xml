(
<figureCitation box="[159,252,1302,1327]" captionStart="FIGURE 5" captionStartId="10.[151,250,1489,1511]" captionTargetBox="[409,1180,317,1459]" captionTargetId="figure-244@10.[399,1187,303,1463]" captionTargetPageId="10" captionText="FIGURE 5. First antennae. A, Tanycypris centa (after Chang et al. 2012); B, T. pellucida; C, T. siamensis (after Savatenalinton &amp; Martens 2009a); D, N. obtusa; E, T. madagascarensis. Scale bar = 100 µm for A–E." figureDoi="http://doi.org/10.5281/zenodo.4920158" httpUri="https://zenodo.org/record/4920158/files/figure.png" pageId="9" pageNumber="410">Figs 5A</figureCitation>
, 
<figureCitation box="[264,337,1302,1327]" captionStart="FIGURE 6" captionStartId="11.[151,250,1811,1833]" captionTargetBox="[409,1163,638,1777]" captionTargetId="figure-288@11.[399,1187,628,1788]" captionTargetPageId="11" captionText="FIGURE 6. Caudal rami and first thoracopod. A, CR Nealecypris obtusa; B, CR Tanycypris pellucida; C, CR T. siamensis (after Savatenalinton &amp; Martens 2009a); D, CR T. centa; E, CR T. madagascarensis; F, T1 (female) N. obtusa; G, T1 (female) T. pellucida; H, T1 (female) T. siamensis (after Savatenalinton &amp; Martens 2009a); I, T1 (female) T. centa (after Chang et al. 2012); J, T1 (male) T. madagascarensis. Scale bar = 100 µm for A–J." figureDoi="http://doi.org/10.5281/zenodo.4920160" httpUri="https://zenodo.org/record/4920160/files/figure.png" pageId="9" pageNumber="410">6D, 6I</figureCitation>
, 
<figureCitation box="[349,383,1302,1327]" captionStart="FIGURE 7" captionStartId="12.[151,250,1358,1380]" captionTargetBox="[408,1180,189,1316]" captionTargetId="figure-200@12.[399,1187,185,1345]" captionTargetPageId="12" captionText="FIGURE 7. Caudal ramus attachments. A, Nealecypris obtusa; B, Tanycypris madagascarensis; C, T. centa; D, T. pellucida; E, T. siamensis (after Savatenalinton &amp; Martens 2009a). Scale bar = 100 µm for A–E." figureDoi="http://doi.org/10.5281/zenodo.4920162" httpUri="https://zenodo.org/record/4920162/files/figure.png" pageId="9" pageNumber="410">7C</figureCitation>
