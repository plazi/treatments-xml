<taxonomicName authority="Humbert" authorityName="Humbert" box="[361,663,1259,1283]" class="Liliopsida" family="Arecaceae" genus="Marojejya" kingdom="Plantae" order="Arecales" pageId="4" pageNumber="45" phylum="Tracheophyta" rank="species" species="insignis">
<emphasis box="[361,559,1259,1283]" italics="true" pageId="4" pageNumber="45">Marojejya insignis</emphasis>
Humbert
</taxonomicName>
, 
<taxonomicName authority="Jum. &amp; H. Perrier" authorityName="Jum. &amp; H. Perrier" class="Liliopsida" family="Arecaceae" genus="Ravenea" kingdom="Plantae" order="Arecales" pageId="4" pageNumber="45" phylum="Tracheophyta" rank="species" species="sambiranensis">
<emphasis italics="true" pageId="4" pageNumber="45">Ravenea sambiranensis</emphasis>
Jum. &amp; H. Perrier
</taxonomicName>
, 
<taxonomicName authority="Mart., Weinmanniadecora Tul." authorityName="Mart., Weinmanniadecora Tul." class="Liliopsida" family="Arecaceae" genus="Dypsis" kingdom="Plantae" order="Arecales" pageId="4" pageNumber="45" phylum="Tracheophyta" rank="species" species="nodifera">
<emphasis box="[464,629,1291,1315]" italics="true" pageId="4" pageNumber="45">Dypsis nodifera</emphasis>
Mart., 
<emphasis italics="true" pageId="4" pageNumber="45">Weinmanniadecora</emphasis>
Tul.
</taxonomicName>
, 
