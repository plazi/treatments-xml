<quantity id="BF279B36FF90D276FD75FEAFFC9C8D29" box="[690,813,333,359]" metricMagnitude="1" metricUnit="m" metricValue="5.5" metricValueMax="9.0" metricValueMin="2.0" pageId="15" pageNumber="316" unit="m" value="55.0" valueMax="90.0" valueMin="20.0">20 to 90 m</quantity>
