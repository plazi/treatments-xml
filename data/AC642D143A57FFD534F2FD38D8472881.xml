<quantity id="E33531E73A57FFD536EFFC92DF942814" box="[650,719,798,823]" metricMagnitude="-1" metricUnit="m" metricValue="1.5" pageId="30" pageNumber="323" unit="cm" value="15.0">15 cm</quantity>
