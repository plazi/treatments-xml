<taxonomicName id="4C2901BE04571504FB664812FAC8F9A0" authorityName="Jousseaume" authorityYear="1898" box="[1093,1315,1595,1620]" class="Gastropoda" family="Triphoridae" genus="Mastonia" kingdom="Animalia" pageId="94" pageNumber="95" phylum="Mollusca" rank="species" species="interpicta">
