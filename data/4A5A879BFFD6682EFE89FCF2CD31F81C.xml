<paragraph id="C24C368DFFD7682EFF4AF9F3CDFFF9BC" blockId="81.[189,744,1563,1590]" box="[189,744,1563,1590]" pageId="81" pageNumber="82">
<collectingCountry id="BAE4761DFFD7682EFF4AF9F3CE7DF9BC" box="[189,362,1563,1589]" name="South Georgia and the South Sandwich Islands" pageId="81" pageNumber="82">South Georgia</collectingCountry>
