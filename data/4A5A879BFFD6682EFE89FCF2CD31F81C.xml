<quantity id="050B9B68FFD7682EFEF1FA4CCE4BFA37" box="[262,348,1444,1470]" metricMagnitude="-2" metricUnit="m" metricValue="4.0" pageId="81" pageNumber="82" unit="mm" value="40.0">40 mm</quantity>
