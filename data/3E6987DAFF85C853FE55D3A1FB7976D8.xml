DISTRIBUTION: 
<taxonomicName id="71C04D4FFF85C853FEC5D744FD81764E" authorityName=": Tate" authorityYear="1935" box="[379,586,1606,1632]" class="Mammalia" family="Erethizontidae" genus="Coendou" kingdom="Animalia" order="Rodentia" pageId="24" phylum="Chordata" rank="species" species="spinosus">
