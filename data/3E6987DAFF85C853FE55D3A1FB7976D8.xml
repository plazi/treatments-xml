<quantity id="71389B29FF85C853FBDFD7D9FB6576D8" box="[1121,1198,1755,1782]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="24" unit="m" value="900.0">
<elevation id="3DEDD1FFFF85C853FBDFD7D9FB6576D8" box="[1121,1198,1755,1782]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="24" unit="m" value="900.0">900 m</elevation>
