<collectingCounty id="5F1E4E40FF85C853FF2ED76EFEF276AB" box="[144,313,1643,1670]" pageId="24">Mata Atlântica</collectingCounty>
of southeastern 
<collectingCountry id="CED7765CFF85C853FE48D769FDF376AB" box="[502,568,1643,1669]" name="Brazil" pageId="24">Brazil</collectingCountry>
(from 
<collectingRegion id="7404F82EFF85C853FD37D769FCE176AB" box="[649,810,1643,1669]" country="Brazil" name="Espirito Santo" pageId="24">Espírito Santo</collectingRegion>
