<collectingCountry id="CED7765CFF85C853FD70D793FCFF7685" box="[718,820,1681,1707]" name="Uruguay" pageId="24">Uruguay</collectingCountry>
(
<collectingRegion id="7404F82EFF85C853FCFDD792FB107684" box="[835,1243,1680,1706]" isEnumeration="true" pageId="24">Artigas, Rivera, Salto, Tacuarembó</collectingRegion>
; 
<collectingMunicipality id="561BACB6FF85C853FF2ED7B4FF3276FE" box="[144,249,1718,1744]" pageId="24">Ximénez</collectingMunicipality>
