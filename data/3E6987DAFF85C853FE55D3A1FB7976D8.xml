<collectingCountry id="CED7765CFF85C853FD8CD7B4FD6376FE" box="[562,680,1718,1744]" name="Argentina" pageId="24">Argentina</collectingCountry>
(
<location id="B31F6017FF85C853FD09D7B4FCED76FE" LSID="urn:lsid:plazi:treatment:3E6987DAFF85C853FE55D3A1FB7976D8:B31F6017FF85C853FD09D7B4FCED76FE" box="[695,806,1718,1744]" country="Brazil" county="Mata Atlantica" municipality="Ximenez" name="Misiones" pageId="24" stateProvince="Espirito Santo">Misiones</location>
