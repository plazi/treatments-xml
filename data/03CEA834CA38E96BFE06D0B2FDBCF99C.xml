<subSubSection id="C37D4AA9CA38E968FF97D162FCE6FEAD" box="[113,885,265,290]" pageId="13" pageNumber="368" type="type_taxon">
<paragraph id="8BD81922CA38E968FF97D162FCE6FEAD" blockId="13.[113,885,265,290]" box="[113,885,265,290]" pageId="13" pageNumber="368">
<emphasis id="B913C530CA38E968FF97D162FE96FEAD" bold="true" box="[113,261,266,290]" pageId="13" pageNumber="368">
<typeStatus id="54DCA780CA38E968FF97D162FF3AFEAD" box="[113,169,266,290]" pageId="13" pageNumber="368">Type</typeStatus>
