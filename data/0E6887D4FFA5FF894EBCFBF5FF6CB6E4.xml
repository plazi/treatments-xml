<taxonomicName id="41C14D41FFA4FF8A4FD3F8CFFDCFB511" box="[504,595,1796,1820]" class="Insecta" genus="Iteaphila" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
