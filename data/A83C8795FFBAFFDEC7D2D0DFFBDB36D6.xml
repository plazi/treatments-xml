<taxonomicName id="E7954D00FFBAFFDEC7D2D0DFFDCB35CB" authority="Motschulsky, 1853" authorityName="Motschulsky" authorityYear="1853" box="[151,528,439,464]" class="Insecta" family="Coleoptera" genus="Aulonogyrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="93" phylum="Arthropoda" rank="genus">
