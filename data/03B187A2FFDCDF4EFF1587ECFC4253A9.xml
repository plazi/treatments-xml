<location id="8EC7606FFFDCDF40FEF485E5FE7556E7" LSID="urn:lsid:plazi:treatment:03B187A2FFDCDF4EFF1587ECFC4253A9:8EC7606FFFDCDF40FEF485E5FE7556E7" box="[374,461,1717,1742]" county="Palazzo" municipality="Val d'Archesane" name="Brescia" pageId="71" pageNumber="72">Brescia</location>
, 
<collectingMunicipality id="6BC3ACCEFFDCDF40FE5F85E5FD2756E7" box="[477,671,1717,1742]" pageId="71" pageNumber="72">Val d’Archesane</collectingMunicipality>
, westl. 
<collectingCounty id="62C64E38FFDCDF40FD7C85E5FCE056E7" box="[766,856,1717,1742]" pageId="71" pageNumber="72">Palazzo</collectingCounty>
, 
<quantity id="4CE09B51FFDCDF40FCEB85E6FC0F56E4" box="[873,951,1718,1742]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="71" pageNumber="72" unit="m" value="900.0">
<elevation id="0035D187FFDCDF40FCEB85E6FC0F56E4" box="[873,951,1718,1742]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="71" pageNumber="72" unit="m" value="900.0">900 m</elevation>
</quantity>
, 
<collectorName id="26ED5362FFDCDF40FC4A85E5FBB056E7" box="[968,1032,1717,1742]" pageId="71" pageNumber="72">Hasel</collectorName>
wurzeln, 3. 11. ’89, leg. 
<collectorName id="26ED5362FFDCDF40FAC285E5FA2C56E7" box="[1344,1428,1717,1742]" pageId="71" pageNumber="72">Kahlen</collectorName>
