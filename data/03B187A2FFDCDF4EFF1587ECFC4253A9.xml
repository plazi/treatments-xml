<materialsCitation id="3B703CE9FFDCDF40FF4585E6FC0556DB" ID-GBIF-Occurrence="1701205395" collectionCode="TLMF" collectorName="Hasel &amp; Kahlen &amp; The" county="Palazzo" elevation="900" location="Brescia" municipality="Val d'Archesane" pageId="71" pageNumber="72" specimenCount="2" typeStatus="paratype">
