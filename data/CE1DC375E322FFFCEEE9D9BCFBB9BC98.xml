<collectionCode id="20A5EAA6E322FFFCEE86D8DBFBC8BBF8" box="[1008,1053,955,978]" pageId="29" pageNumber="230">BIN</collectionCode>
<collectionCode id="20A5EAA6E322FFFCE954D8DBFBBBBBF8" box="[1058,1134,955,978]" pageId="29" pageNumber="230">BOLD</collectionCode>
