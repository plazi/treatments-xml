<paragraph id="AB7A81E9E262215D29F9C96780A4A359" pageId="0" pageNumber="25">
<taxonomicName id="F2BEF3D8E0E3C7070550D898A7FE0067" baseAuthorityName="Luetken" baseAuthorityYear="1874" class="Actinopterygii" family="Loricariidae" genus="Hypostomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypostomus francisci" order="Siluriformes" pageId="0" pageNumber="25" phylum="Chordata" rank="species" species="francisci">
