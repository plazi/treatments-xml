<taxonomicName id="0FC8B80C6A753D922D898221CE16F006" class="Actinopterygii" family="Loricariidae" genus="Hypostomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypostomus" order="Siluriformes" pageId="0" pageNumber="25" phylum="Chordata" rank="genus">
