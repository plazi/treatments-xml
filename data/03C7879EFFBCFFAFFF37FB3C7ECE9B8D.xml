<taxonomicName id="4C6E4D0BFFBCFFAFFBD2FB3C7DC59B41" authority="Azadbakhsh &amp; Sciaky, 2018" authorityName="Azadbakhsh &amp; Sciaky" authorityYear="2018" class="Insecta" family="Carabidae" genus="Laemostenus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="596" phylum="Arthropoda" rank="species" species="hormozi" subGenus="Antisphodrus">
<emphasis id="B91AEA9AFFBCFFAFFBD2FB3C7A719AA4" bold="true" box="[1138,1298,1233,1259]" italics="true" pageId="1" pageNumber="596">Laemostenus</emphasis>
(
<emphasis id="B91AEA9AFFBCFFAFFA85FB3D7FAE9B40" bold="true" italics="true" pageId="1" pageNumber="596">Antisphodrus</emphasis>
) 
<emphasis id="B91AEA9AFFBCFFAFFF7EFB197E229B40" bold="true" box="[222,321,1268,1295]" italics="true" pageId="1" pageNumber="596">hormozi</emphasis>
