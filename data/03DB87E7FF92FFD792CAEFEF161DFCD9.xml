<taxonomicName id="4C724D72FF92FFD792F0EFEF137DFE70" authority="(Martius ex Bentham) Queiroz &amp; Snak (2020: 164)" authorityName="L. P. Queiroz &amp; Snak" authorityPageNumber="164" authorityYear="2020" baseAuthorityName="Martius ex Bentham" box="[178,1088,410,437]" class="Magnoliopsida" family="Fabaceae" genus="Macropsychanthus" kingdom="Plantae" order="Fabales" pageId="20" pageNumber="265" phylum="Tracheophyta" rank="species" species="violaceus">
