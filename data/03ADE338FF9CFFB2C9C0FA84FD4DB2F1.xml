<collectingRegion id="49C09CCCFF9CFFB2C855FA0EFE4CB18C" box="[285,498,1485,1512]" country="Brazil" name="Rio Grande do Sul" pageId="9" pageNumber="202">Rio Grande do Sul</collectingRegion>
