<quantity id="4CB59B187140FFC5FBF60610FF16FBF4" metricMagnitude="3" metricUnit="m" metricValue="1.95" metricValueMax="2.1" metricValueMin="1.8" pageId="50" pageNumber="187" unit="m" value="1950.0" valueMax="2100.0" valueMin="1800.0">1800- 2100 m</quantity>
, 6.- 
<date id="FFF3103D7140FFC5FEE9063DFE82FBF4" box="[279,365,1061,1081]" pageId="50" pageNumber="187" value="1991-03-07">7.3.1991</date>
