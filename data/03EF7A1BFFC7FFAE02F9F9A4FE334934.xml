<subSubSection id="C35C9886FFC4FFA00380FCE1FBC94A16" pageId="39" pageNumber="40" type="materials_examined">
<paragraph id="8BF9CB0DFFC4FFA00380FCE1FDD94DBB" blockId="39.[120,1225,772,1807]" pageId="39" pageNumber="40">
<materialsCitation id="3B2EC150FFC4FFA00380FCE1FDDD4DBB" country="Lebanon" county="Lower Cretaceous" location="Central" municipality="Caza Baabda" pageId="39" pageNumber="40" specimenCount="1" typeStatus="holotype">
<typeStatus id="54FD75AFFFC4FFA00380FCE1FF3E4D5C" box="[168,226,847,873]" pageId="39" pageNumber="40">TYPE</typeStatus>
LOCALITY: 
