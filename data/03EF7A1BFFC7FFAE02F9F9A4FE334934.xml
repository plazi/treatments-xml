<paragraph id="8BF9CB0DFFC5FFA10380FBC9FC354AB4" blockId="38.[120,1225,1086,1788]" box="[168,1001,1127,1153]" pageId="38" pageNumber="39">
<typeStatus id="54FD75AFFFC5FFA10380FBC9FF3E4AB4" box="[168,226,1127,1153]" pageId="38" pageNumber="39">TYPE</typeStatus>
SPECIES: 
<emphasis id="B932171FFFC5FFA1027AFBC9FE014AB4" box="[338,477,1127,1153]" italics="true" pageId="38" pageNumber="39">
<taxonomicName id="4C46B08EFFC5FFA1027AFBC9FE054AB4" authority="Vea &amp; Grimaldi, 2015" authorityName="Vea &amp; Grimaldi" authorityYear="2015" box="[338,473,1127,1153]" class="Insecta" family="Hemiptera incertae sedis" genus="Xiphos" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="vani" status="n. sp.">Xiphos vani</taxonomicName>
