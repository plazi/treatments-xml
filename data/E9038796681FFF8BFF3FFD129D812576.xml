<emphasis id="53DEEA92681FFF8BFF3FF9999D062472" box="[151,348,1628,1653]" italics="true" pageId="1" pageNumber="140">
<taxonomicName id="A6AA4D03681FFF8BFF3FF9999CA62473" box="[151,252,1628,1653]" class="Arachnida" family="Syringophilidae" genus="Picobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="galerida">Galerida</taxonomicName>
cristata
</emphasis>
(L.) (
<taxonomicName id="A6AA4D03681FFF8BFE09F9999E1F2473" box="[417,581,1628,1653]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="1" pageNumber="140" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
: 
<taxonomicName id="A6AA4D03681FFF8BFDFAF9999E9D2473" box="[594,711,1628,1653]" class="Aves" family="Alaudidae" kingdom="Animalia" order="Passeriformes" pageId="1" pageNumber="140" phylum="Chordata" rank="family">Alaudidae</taxonomicName>
