<collectingCountry id="F3166FC099015548FEF9FBEDFE7EFB67" box="[359,429,1129,1154]" name="China" pageId="2" pageNumber="387">China</collectingCountry>
, Province 
<collectingRegion id="49C5E1B299015548FDBDFBEEFDAFFB64" box="[547,636,1130,1153]" country="China" name="Yunnan" pageId="2" pageNumber="387">Yunnan</collectingRegion>
