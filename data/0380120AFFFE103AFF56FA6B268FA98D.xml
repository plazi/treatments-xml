<collectingCountry id="F33EE38CFFFE103AFE81F9E525F7A9A8" box="[320,407,1647,1672]" name="Canada" pageId="12" pageNumber="213">Canada</collectingCountry>
, and 
<collectingCountry id="F33EE38CFFFE103AFE13F9E52625A9A8" box="[466,581,1647,1672]" name="Venezuela" pageId="12" pageNumber="213">Venezuela</collectingCountry>
