<collectingCountry id="F33EE38CFFFE103AFB9DF9C6215AA945" box="[1116,1338,1612,1637]" name="Trinidad and Tobago" pageId="12" pageNumber="213">Trinidad &amp; Tobago</collectingCountry>
, Gulf of 
<collectingCountry id="F33EE38CFFFE103AFF56F9E5248FA9A8" box="[151,239,1647,1672]" name="Mexico" pageId="12" pageNumber="213">Mexico</collectingCountry>
, 
<collectingCountry id="F33EE38CFFFE103AFF3BF9FA2555A9A8" box="[250,309,1648,1672]" name="United States of America" pageId="12" pageNumber="213">USA</collectingCountry>
