<paragraph id="8BAB4E466D145073B7A4FA99FC5FFA64" blockId="13.[151,945,1403,1426]" box="[151,945,1403,1426]" pageId="13" pageNumber="234">
<taxonomicName id="4C1435C56D145073B7A4FA99FDF1FA67" authority="Say, 1825: 177" authorityName="Say" authorityPageNumber="177" authorityYear="1825" box="[151,543,1403,1426]" class="Insecta" family="Silphidae" genus="Necrophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="234" phylum="Arthropoda" rank="species" species="orbicollis">
