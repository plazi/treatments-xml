<taxonomicName id="BFFDEEE0BB58FFC3FD5DFCAC6D84FCF7" authority="Reid &amp; Ozawa 2016" authorityName="Reid &amp; Ozawa" authorityYear="2016" box="[738,1212,802,827]" class="Gastropoda" family="Potamididae" genus="Pirenella" higherTaxonomySource="CoL" kingdom="Animalia" pageId="14" pageNumber="415" phylum="Mollusca" rank="species" species="cingulata">
