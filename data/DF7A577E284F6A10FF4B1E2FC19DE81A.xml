</taxonomicName>
<taxonomicName id="90D39DEB284F6A10FF051E2EC1EEEBD8" box="[217,385,1384,1410]" class="Reptilia" family="Elapidae" genus="Naja" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="32" phylum="Chordata" rank="subSpecies" species="incertae" subSpecies="sedis">incertae sedis</taxonomicName>
