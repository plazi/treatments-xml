lenta, 
<quantity id="DA7C18CAFF92FF83FCBBE8D2FC66FE63" box="[863,924,458,478]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="24" pageNumber="255" unit="m" value="500.0">500 m</quantity>
