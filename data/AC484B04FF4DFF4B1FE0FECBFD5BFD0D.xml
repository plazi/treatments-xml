(
<materialsCitation id="9489F04FFF42FF4B1EECFF91FBDEFDAB" collectingDate="2002-01-29" collectionCode="SANC" collectorName="Greg Anderson &amp; Steve Johnson" county="Sani Pass" location="Natal" municipality="Southern Drakensberg" pageId="168" pageNumber="169" specimenCount="1" typeStatus="holotype">
<specimenCount id="32E7319BFF42FF4B1EECFF91FDA5FE42" box="[411,556,461,487]" pageId="168" pageNumber="169" type="generic">1 specimen</specimenCount>
