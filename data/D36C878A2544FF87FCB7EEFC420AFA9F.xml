SPECIES: 
<emphasis id="69B1EA8E2544FF99FCFDED2A44CFFD86" box="[874,1161,556,580]" italics="true" pageId="78" pageNumber="78">
<taxonomicName id="9CC54D1F2544FF99FCFDED2A44C3FD86" authority="Schwartz &amp; Weirauch &amp; Schuh, 2018" authorityName="Schwartz &amp; Weirauch &amp; Schuh" authorityYear="2018" box="[874,1157,556,580]" class="Magnoliopsida" family="Myrtaceae" genus="Melaleucacoris" kingdom="Plantae" order="Myrtales" pageId="78" pageNumber="78" phylum="Tracheophyta" rank="species" species="glomeratae" status="new species">Melaleucacoris glomeratae</taxonomicName>
