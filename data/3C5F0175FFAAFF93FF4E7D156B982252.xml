<subSubSection id="FCECE3E8FFA4FF93FF1E790A6B982252" box="[199,1160,332,357]" pageId="9" pageNumber="90" type="etymology">
<paragraph id="B449B063FFA4FF93FF1E790A6B982252" blockId="9.[151,1436,151,357]" box="[199,1160,332,357]" pageId="9" pageNumber="90">
<emphasis id="86826C71FFA4FF93FF1E790A6E562252" bold="true" box="[199,326,332,357]" pageId="9" pageNumber="90">Etymology</emphasis>
