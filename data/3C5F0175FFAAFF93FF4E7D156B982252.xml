<taxonomicName id="73F6CBE0FFA4FF93FE44790B6ECD2253" box="[413,477,333,356]" class="Insecta" family="Cerambycidae" genus="Wappesellus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="90" phylum="Arthropoda" rank="species" species="cavus">
