(
<collectingCountry id="F36D760CB65AFEEF65917CF4DD75FE7B" box="[197,261,454,480]" name="United States of America" pageId="267" pageNumber="268">USA</collectingCountry>
), 
<emphasis id="B90EEA8EB65AFEEF644D7CF4DD20FE7B" bold="true" box="[281,336,454,480]" pageId="267" pageNumber="268">Asia</emphasis>
(
<collectingCountry id="F36D760CB65AFEEF640B7CF4DDD1FE7A" box="[351,417,454,481]" name="India" pageId="267" pageNumber="268">India</collectingCountry>
), 
