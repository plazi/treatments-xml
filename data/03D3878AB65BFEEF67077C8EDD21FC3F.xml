(
<collectingCountry id="F36D760CB65AFEEF67077C93DEEAFE20" box="[595,666,417,443]" name="Spain" pageId="267" pageNumber="268">Spain</collectingCountry>
, 
<collectingCountry id="F36D760CB65AFEEF67F07C90DE87FE27" box="[676,759,418,444]" name="France" pageId="267" pageNumber="268">France</collectingCountry>
, 
<collectingCountry id="F36D760CB65AFEEF66557C93DFB9FE20" box="[769,969,417,443]" name="United Kingdom" pageId="267" pageNumber="268">United Kingdom</collectingCountry>
, 
<collectingCountry id="F36D760CB65AFEEF66877C93D87AFE20" box="[979,1034,417,443]" name="Italy" pageId="267" pageNumber="268">Italy</collectingCountry>
, 
<collectingCountry id="F36D760CB65AFEEF61477C93D814FE20" box="[1043,1124,417,443]" name="Poland" pageId="267" pageNumber="268">Poland</collectingCountry>
and 
<collectingCountry id="F36D760CB65AFEEF61CF7C93D889FE27" box="[1179,1273,417,444]" name="Finland" pageId="267" pageNumber="268">Finland</collectingCountry>
), 
