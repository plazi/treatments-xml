<taxonomicName id="C6863C90FFDCE109208BEA4FFDB066E0" authorityName="Fusu &amp; Ebrahimi &amp; Siebold &amp; Villemant" authorityYear="2015" baseAuthorityName="Risbec" baseAuthorityYear="1958" box="[505,600,623,646]" class="Insecta" family="Eupelmidae" genus="Eupelmus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="460" phylum="Arthropoda" rank="species" species="pennisetae">
