<taxonomicName id="C6863C90FFDCE1092000EA4FFE0266E1" authorityName="Gahan" authorityYear="1927" box="[370,490,623,647]" class="Insecta" family="Eupelmidae" genus="Anastatoidea" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="460" phylum="Arthropoda" rank="genus">
