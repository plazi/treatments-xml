<paragraph id="C4B2332916C862EFD8ACED99F43913B9" pageId="0" pageNumber="47">
<taxonomicName id="5550EA26FD90AE999AAC7DBA201879F3" class="Insecta" family="Pteromalidae" genus="Oodera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oodera" order="Hymenoptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="genus">Oodera</taxonomicName>
