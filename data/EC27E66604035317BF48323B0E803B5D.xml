<taxonomicName id="94244B77A6F63592211B0357B929A749" authority="Risbec, 1951" authorityName="Risbec" authorityYear="1951" class="Insecta" family="Pteromalidae" genus="Stellophora" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Stellophora magnifica" order="Hymenoptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="species" species="magnifica">Stellophora magnifica</taxonomicName>
