female. 
<taxonomicName id="4D8D4D2AFFD6FF94FF41FF3EFEECFF1B" authorityName="Ausserer" authorityYear="1871" box="[242,346,223,249]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
