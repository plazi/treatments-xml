<quantity id="4D759B4CFFD6FF94FE1BFF3EFE59FF1B" box="[424,495,223,249]" metricMagnitude="0" metricUnit="m" metricValue="6.2992" pageId="11" pageNumber="212" unit="in" value="248.0">248 in</quantity>
female. 
<taxonomicName id="4D8D4D2AFFD6FF94FDE2FF3EFD69FF1B" authorityName="Krefft" authorityYear="1871" box="[593,735,223,249]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="11" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
length 
<quantity id="4D759B4CFFD6FF94FC80FF3FFCCCFF1B" box="[819,890,222,249]" metricMagnitude="0" metricUnit="m" metricValue="6.477" pageId="11" pageNumber="212" unit="in" value="255.0">255 in</quantity>
female. 
<taxonomicName id="4D8D4D2AFFD6FF94FC68FF3EFBDFFF1B" authorityName="Krefft" authorityYear="1871" box="[987,1129,223,249]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="11" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
width 
