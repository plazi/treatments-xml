<taxonomicName id="64AE4D2AFF98292CFEFEF803FD02F84F" authority="Shi &amp; Gao &amp; Shen, 2017" authorityName="Shi &amp; Gao &amp; Shen" authorityYear="2017" box="[353,707,1946,1968]" class="Insecta" family="Lauxaniidae" genus="Homoneura" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="372" phylum="Arthropoda" rank="species" species="pangae" status="sp. nov." subGenus="Homoneura">
