<taxonomicName id="A4D54D475B0CFFE0FF57F8BD3146F89F" authority="(Green, 1896)" authorityName="Green" authorityYear="1896" baseAuthorityName="Green" baseAuthorityYear="1896" box="[151,625,1793,1819]" class="Insecta" family="Diaspididae" genus="Cryptophyllaspis" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="100" phylum="Arthropoda" rank="species" species="occulta">
<emphasis id="51A1EAD65B0CFFE0FF57F8BD3146F89F" bold="true" box="[151,625,1793,1819]" pageId="1" pageNumber="100">
<emphasis id="51A1EAD65B0CFFE0FF57F8BD32F7F89F" bold="true" box="[151,448,1793,1819]" italics="true" pageId="1" pageNumber="100">Cryptophyllaspis occulta</emphasis>
(
<bibRefCitation id="07444B355B0CFFE0FE0FF8BD315EF89F" author="Green" box="[463,617,1793,1819]" pageId="1" pageNumber="100" refString="Green, E. E. (1896) The Coccidae of Ceylon. Part I. Dulau, London, 103 pp." type="book" year="1896">Green, 1896</bibRefCitation>
)
</emphasis>
</taxonomicName>
