<taxonomicName id="21C746CF1518FFAAFD81FF74FCDA22DE" authorityName="Schedl" authorityYear="1965" box="[587,808,151,177]" class="Insecta" family="Curculionidae" genus="Lanurgus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="594" phylum="Arthropoda" rank="species" species="cribrellus">
