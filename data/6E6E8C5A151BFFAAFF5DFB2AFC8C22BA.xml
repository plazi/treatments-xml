<taxonomicName id="21C746CF1518FFAAFD1EFF58FC8522BA" baseAuthorityName="Schedl" baseAuthorityYear="1961" box="[724,887,187,213]" class="Insecta" family="Curculionidae" genus="Pseudolanurgus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="2" pageNumber="594" phylum="Arthropoda" rank="species" species="harunganae">
