<caption id="DF4B99A6FFF5FFE23F8ADCA9FD54FC3C" ID-DOI="http://doi.org/10.5281/zenodo.5435344" ID-Zenodo-Dep="5435344" httpUri="https://zenodo.org/record/5435344/files/figure.png" pageId="45" pageNumber="384" startId="45.[162,240,919,938]" targetBox="[172,769,203,868]" targetPageId="45">
