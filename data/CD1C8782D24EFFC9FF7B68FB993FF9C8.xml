<bibRefCitation id="21244B65D24EFFC9FB1068B39DC6F980" author="Karny" box="[1276,1436,1515,1540]" pageId="5" pageNumber="578" refString="Karny, H. H. (1926) On Malaysian katydids. Journal of the Federated Malay States Museums, 13, 69 - 157." type="journal article" year="1926">
Karny (
<number id="F37D24B3D24EFFC9FAB668B49DCEF980" box="[1370,1428,1515,1540]" pageId="5" pageNumber="578" value="1926.0">1926</number>
)
</bibRefCitation>
synonymised 
<taxonomicName id="82B54D17D24EFFC9FED86B509A58F9A3" box="[308,514,1551,1575]" class="Insecta" family="Cerambycidae" genus="Phaula" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="578" phylum="Arthropoda" rank="species" species="chlorotica">
<emphasis id="77C1EA86D24EFFC9FED86B509A58F9A3" box="[308,514,1551,1575]" italics="true" pageId="5" pageNumber="578">Phaula chlorotica</emphasis>
</taxonomicName>
under 
<taxonomicName id="82B54D17D24EFFC9FDB96B509B7FF9A3" box="[597,805,1551,1575]" class="Insecta" family="Phaneropteridae" genus="Arnobia" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="578" phylum="Arthropoda" rank="species" species="trichopus">
<emphasis id="77C1EA86D24EFFC9FDB96B509B7FF9A3" box="[597,805,1551,1575]" italics="true" pageId="5" pageNumber="578">Arnobia trichopus</emphasis>
</taxonomicName>
. Most recent record of this species was from Sumatra (
<bibRefCitation id="21244B65D24EFFC9FF726B6C9902F9C8" author="Gorochov" box="[158,344,1587,1612]" pageId="5" pageNumber="578" refString="Gorochov, A. V. (1998) New and little known katydids of the genera Stictophaula, Arnobia, and Mirollia (Orthoptera: Tettigoniidae: Phaneropterinae) from South-East Asia. Russian Entomological Journal, 7 (1 - 2), 1 - 14." type="journal article" year="1998">
Gorochov, 
<number id="F37D24B3D24EFFC9FEF06B6B9902F9C8" box="[284,344,1588,1612]" pageId="5" pageNumber="578" value="1998.0">1998</number>
