<taxonomicName id="4C4F4D7EFFD4855978A9F941FA2DF932" box="[1261,1429,1670,1695]" class="Insecta" family="Lycaenidae" genus="Rhamma" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="325" phylum="Arthropoda" rank="species" species="arria">
