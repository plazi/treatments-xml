<taxonomicName id="4C4F4D7EFFD485597D77F96BFE7AF96F" box="[307,450,1708,1731]" class="Insecta" family="Lycaenidae" genus="Paralustrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="325" phylum="Arthropoda" rank="species" species="orosiensis">
