</taxonomicName>
[
<taxonomicName id="4C2D4D1D46756634485C9E21FC20D4CD" authorityName="Latreille" authorityYear="1802" box="[777,949,623,650]" class="Insecta" family="Tenebrionidae" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="123" phylum="Arthropoda" rank="family">Tenebrionidae</taxonomicName>
