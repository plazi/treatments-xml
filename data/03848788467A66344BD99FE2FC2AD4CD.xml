<emphasis id="B959EA8C46756634492F9E00FCCCD420" box="[634,857,589,615]" italics="true" pageId="8" pageNumber="123">Monotoma producta</emphasis>
LeConte, 1855
</taxonomicName>
[
<taxonomicName id="4C2D4D1D467566344F4A9E03FE99D4CD" authorityName="Laporte de Castelnau" authorityYear="1840" class="Insecta" family="Monotomidae" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="123" phylum="Arthropoda" rank="family">Monotomidae</taxonomicName>
], and 
<taxonomicName id="4C2D4D1D467566344A1A9E3EFD6ED4CD" authority="(Fabricius, 1801)" baseAuthorityName="Fabricius" baseAuthorityYear="1801" box="[335,763,624,650]" class="Insecta" family="Tenebrionidae" genus="Blapstinus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="123" phylum="Arthropoda" rank="species" species="metallicus">
