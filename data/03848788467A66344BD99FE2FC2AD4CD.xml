</taxonomicName>
, 
<taxonomicName id="4C2D4D1D4675663449729E64FD77D403" baseAuthorityName="Erichson" baseAuthorityYear="1839" box="[551,738,554,580]" class="Insecta" family="Staphylinidae" genus="Strigota" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="123" phylum="Arthropoda" rank="species" species="ambigua">
<emphasis id="B959EA8C4675663449729E64FD77D403" box="[551,738,554,580]" italics="true" pageId="8" pageNumber="123">Strigota ambigua</emphasis>
</taxonomicName>
(see below) [
<taxonomicName id="4C2D4D1D4675663448209E67FB8DD403" baseAuthorityName="Herman" baseAuthorityYear="2001" box="[885,1048,553,580]" class="Insecta" family="Staphylinidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="123" phylum="Arthropoda" rank="family">Staphylinidae</taxonomicName>
], 
