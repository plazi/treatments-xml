<typeStatus id="5D20F6F4FFB9FC3E64108E9FFD92F5C3" box="[574,624,1198,1219]" pageId="2" pageNumber="55">Type</typeStatus>
data: 
<collectingCountry id="FA8C08C6FFB9FC3E649C8E9CFCF1F5C3" box="[690,787,1197,1219]" name="Australia" pageId="2" pageNumber="55">Australia</collectingCountry>
