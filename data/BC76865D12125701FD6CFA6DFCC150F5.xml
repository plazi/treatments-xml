<taxonomicName id="F3DF4CC812125701FD01FA40FC9A53FB" authorityName="GN" authorityYear="1058" box="[667,876,1469,1489]" class="Elasmobranchii" family="Cetorhinidae" genus="Cetorhinus" kingdom="Animalia" order="Lamniformes" pageId="50" pageNumber="50" phylum="Chordata" rank="species" species="maximus">
