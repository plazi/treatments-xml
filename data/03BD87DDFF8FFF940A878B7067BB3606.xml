<paragraph id="8BAB36CBFF8FFF970A878C65602E326A" blockId="5.[178,1085,1579,1601]" box="[178,1085,1579,1601]" pageId="5" pageNumber="570">
<taxonomicName id="4C144D48FF8FFF970A878C656617326A" authority="PASCOE 1869" authorityName="PASCOE" authorityYear="1869" box="[178,516,1579,1601]" class="Insecta" family="Cerambycidae" genus="Demonax" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="570" phylum="Arthropoda" rank="species" species="planatus">
