<taxonomicName id="A9838A83EEDD3D67BB3466EC99FDC30F" class="Rosopsida" family="Nothofagaceae" genus="Nothofagus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Nothofagus menziesii" order="Fagales" pageId="9" pageNumber="52" phylum="Magnoliophyta" rank="species" species="menziesii">
<emphasis id="04146688D9BE74FEA9647BBD91FCAC17" italics="true" pageId="9" pageNumber="52">Nothofagus menziesii</emphasis>
