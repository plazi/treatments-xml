<taxonomicName id="856D0BB9A817CB2846120FF6AB736CAB" class="Pinopsida" family="Podocarpaceae" genus="Manoao" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Manoao colensoi" order="Pinales" pageId="9" pageNumber="52" phylum="Tracheophyta" rank="species" species="colensoi">
<emphasis id="EE02FB1AC28A797C16C6EECA859AEF76" italics="true" pageId="9" pageNumber="52">Manoao colensoi</emphasis>
</taxonomicName>
