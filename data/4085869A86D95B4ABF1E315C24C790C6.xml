<taxonomicName id="0A4D84E10E2222F05FA3DB81A5915EFF" authorityName="B.P.J.Molloy" authorityYear="1995" class="Pinopsida" family="Podocarpaceae" genus="Manoao" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Manoao" order="Pinales" pageId="9" pageNumber="52" phylum="Tracheophyta" rank="genus">
<emphasis id="6FC63EF9C5BDAC77C9227148C3433539" italics="true" pageId="9" pageNumber="52">Manoao</emphasis>
</taxonomicName>
trunks, occasional on 
<taxonomicName id="2F53142F2410F3B6BC12F18F174378C6" class="Rosopsida" family="Nothofagaceae" genus="Nothofagus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Nothofagus solandri subsp. var. var. cliffortioides" order="Fagales" pageId="9" pageNumber="52" phylum="Magnoliophyta" rank="variety" species="solandri" subSpecies="var." variety="cliffortioides">Nothofagus solandri var. cliffortioides</taxonomicName>
, but apparently absent from trunks of 
<taxonomicName id="224A83931C81190015FE3CAE9089AC5C" class="Rosopsida" family="Nothofagaceae" genus="Nothofagus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Nothofagus menziesii" order="Fagales" pageId="9" pageNumber="52" phylum="Magnoliophyta" rank="species" species="menziesii">
<emphasis id="5F7AF7D316103B25284A20560DA3F19E" italics="true" pageId="9" pageNumber="52">Nothofagus menziesii</emphasis>
