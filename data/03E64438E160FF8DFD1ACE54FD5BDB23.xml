<taxonomicName id="4C4F8EADE161FF8EFF21CC70FDFEDC76" authority="Pierre ex Breteler" authorityName="Pierre ex Breteler" authorityYear="2014" box="[224,547,1610,1636]" class="Insecta" family="Noctuidae" genus="Cola" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="118" phylum="Arthropoda" rank="species" species="elegans" status="sp. nov.">
