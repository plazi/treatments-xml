<taxonomicName id="4C4048B1324E072777EF419AFECFA7E7" authority="Hoshina, 2012" authorityName="Hoshina" authorityYear="2012" box="[220,362,937,955]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="78" pageNumber="75" phylum="Arthropoda" rank="species" species="hijikatai" status="sp. nov.">
