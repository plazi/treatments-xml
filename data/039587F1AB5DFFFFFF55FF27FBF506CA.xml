Sharp 
<number id="3DF424C0AB5DFFFFFD34FEFBFCFA040A" box="[758,818,331,355]" pageId="10" pageNumber="205" value="1882.0">1882</number>
</bibRefCitation>
). 
<collectingCountry id="F32B7677AB5DFFFFFC84FEFAFC55040A" box="[838,925,330,355]" name="Mexico" pageId="10" pageNumber="205">
<emphasis id="B948EAF5AB5DFFFFFC84FEFAFC55040A" bold="true" box="[838,925,330,355]" pageId="10" pageNumber="205">Mexico</emphasis>
</collectingCountry>
: Temascaltepec (
<bibRefCitation id="EFAD4B16AB5DFFFFFBA2FEFAFB3A040A" author="Hinton" box="[1120,1266,330,355]" pageId="10" pageNumber="205" refString="Hinton, H. E. (1934) Miscellaneous studies in the Helminae (Dryopidae, Coleoptera). Revista de Entomologia, 4 (2), 192 - 199." type="journal article" year="1934">
Hinton 
<number id="3DF424C0AB5DFFFFFB75FEFBFB3A040A" box="[1207,1266,331,355]" pageId="10" pageNumber="205" value="1934.0">1934</number>
</bibRefCitation>
; 
<bibRefCitation id="EFAD4B16AB5DFFFFFB3CFEFAFA58040A" author="Hinton" box="[1278,1424,330,355]" pageId="10" pageNumber="205" refString="Hinton, H. E. (1940) A monographic revision of the Mexican water beetles of the family Elmidae. Novitates Zoologicae, 42 (2), 217 - 396." type="journal article" year="1940">
Hinton 
<number id="3DF424C0AB5DFFFFFA96FEFBFA58040A" box="[1364,1424,331,355]" pageId="10" pageNumber="205" value="1940.0">1940</number>
</bibRefCitation>
), Coahuila (
<bibRefCitation id="EFAD4B16AB5DFFFFFEC9FEDFFE5304EE" author="Brown" box="[267,411,367,391]" pageId="10" pageNumber="205" refString="Brown, H. P. (1971) A New Species of Elsianus from Texas and Mexico, with Records of Other Species in the United States (Coleoptera: Dryopoidea: Elmidae). The Coleopterists Bulletin, 25 (2), 55 - 58." type="journal article" year="1971">
Brown 
<number id="3DF424C0AB5DFFFFFE9DFEDFFE5304EE" box="[351,411,367,391]" pageId="10" pageNumber="205" value="1971.0">1971</number>
</bibRefCitation>
