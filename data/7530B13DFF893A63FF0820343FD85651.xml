<taxonomicName id="3A997BA8FF893A64FEDA223F3DCF5521" authority="Chim &amp; Tong, 2020" authorityName="Chim &amp; Tong" authorityYear="2020" box="[325,463,1810,1835]" class="Malacostraca" family="Typhlotanaidae" genus="Unispinosus" kingdom="Animalia" order="Tanaidacea" pageId="3" pageNumber="464" phylum="Arthropoda" rank="genus" status="n. gen.">
