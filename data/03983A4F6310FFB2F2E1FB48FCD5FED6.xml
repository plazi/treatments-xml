<taxonomicName id="4C31F0DA6310FFB2F333FA55FD83FA2D" box="[324,514,1488,1512]" class="Aves" family="Columbidae" genus="Carpophaga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Columbiformes" pageId="3" pageNumber="776" phylum="Chordata" rank="species" species="radiata">Carpophaga radiata</taxonomicName>
