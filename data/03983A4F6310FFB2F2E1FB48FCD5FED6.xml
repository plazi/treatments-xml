<taxonomicName id="4C31F0DA6310FFB2F285FA30FE34FA08" box="[242,437,1461,1485]" class="Aves" family="Columbidae" genus="Carpophaga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Columbiformes" pageId="3" pageNumber="776" phylum="Chordata" rank="species" species="radiata">Carpophaga radiata</taxonomicName>
