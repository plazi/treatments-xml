DIAGNOSIS: 
<taxonomicName id="F08A4D08D126FFCFFCFCAFF44BFFD193" box="[860,1193,403,426]" class="Reptilia" family="Anguidae" genus="Helodermoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="117" pageNumber="118" phylum="Chordata" rank="species" species="tuberculatus">
<emphasis id="05FEEA99D126FFCFFCFCAFF44BFFD193" box="[860,1193,403,426]" italics="true" pageId="117" pageNumber="118">Helodermoides tuberculatus</emphasis>
</taxonomicName>
and 
<taxonomicName id="F08A4D08D126FFCFFD73AFD74B54D1FE" baseAuthorityName="Sullivan" baseAuthorityYear="1979" box="[723,1026,432,455]" class="Reptilia" family="Anguidae" genus="Paraglyptosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="117" pageNumber="118" phylum="Chordata" rank="species" species="princeps">
