<taxonomicName id="4C0E4D2FFFEAE519513B81C762A4F95B" authority="sensu" authorityName="SenSU" box="[286,431,1743,1775]" class="Insecta" family="Staphylinidae" genus="Zyras" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="186" phylum="Arthropoda" rank="genus">
