<typeStatus id="9D2D41C981010B0AFF78C4DBFF7FD4B3" box="[151,202,691,712]" pageId="25" pageNumber="176">Type</typeStatus>
genus 
<taxonomicName id="859684E881010B0AFEFEC4DBFDD4D4B3" authority="Baldwin &amp; Johnson 1999" authorityName="Baldwin &amp; Johnson" authorityYear="1999" box="[273,609,690,712]" class="Actinopterygii" family="Apogonidae" genus="Paxton" kingdom="Animalia" order="Perciformes" pageId="25" pageNumber="176" phylum="Chordata" rank="genus">
