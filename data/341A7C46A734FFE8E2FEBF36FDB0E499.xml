, 
<date id="C80DEB90A732FFE8E39FBB5BFD05E4D1" box="[502,641,620,644]" pageId="12" pageNumber="13" value="1994-07-13">
<number id="0A7BDF77A732FFE8E39FBB5BFD95E4D1" box="[502,529,620,644]" pageId="12" pageNumber="13" value="13.0">13</number>
