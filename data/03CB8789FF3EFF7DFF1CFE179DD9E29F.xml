<taxonomicName id="4C624D1CFF3EFF7DFCCAFC589AE1E29C" box="[833,1000,936,961]" class="Insecta" family="Tachinidae" genus="Nomina" kingdom="Animalia" order="Diptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="dubia">
