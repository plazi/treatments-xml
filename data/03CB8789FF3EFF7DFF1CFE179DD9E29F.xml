<taxonomicName id="4C624D1CFF3EFF7DFEC7FCAD9BC4E22A" authority="Meigen, 1824" authorityName="Meigen" authorityYear="1824" box="[332,717,861,887]" class="Insecta" family="Tachinidae" genus="Tachina" kingdom="Animalia" order="Diptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="gagatina">
