, width 
<quantity id="4C9AD4D6FFDDFFC3FCA18DFAFC41FA25" box="[801,984,1504,1525]" metricMagnitude="-3" metricUnit="m" metricValue="1.6749999999999998" metricValueMax="1.7" metricValueMin="1.65" pageId="26" pageNumber="179" unit="mm" value="1.6749999999999998" valueMax="1.7" valueMin="1.65">1.65 to 1.70 mm</quantity>
