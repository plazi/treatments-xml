<subSubSection id="C9866549FFBDFF8AFF2E5EF5FAACFE0C" box="[199,1311,439,464]" pageId="93" pageNumber="94" type="etymology">
<paragraph id="812336C2FFBDFF8AFF2E5EF5FAACFE0C" blockId="93.[151,1436,151,608]" box="[199,1311,439,464]" pageId="93" pageNumber="94">
<emphasis id="B3E8EAD0FFBDFF8AFF2E5EF5FEFEFE13" bold="true" box="[199,333,440,464]" inLineHeading="true" pageId="93" pageNumber="94" reason="1">Etymology.</emphasis>
