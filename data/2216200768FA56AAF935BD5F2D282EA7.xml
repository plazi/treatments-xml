<paragraph id="03B1E7017DEF13D30A00C65B33B87E02" pageId="39" pageNumber="56">
<pageBreakToken id="B1A609815D72A87317CA6DC508C42628" pageId="39" pageNumber="56" start="start">Natural</pageBreakToken>
history.
</paragraph>
<paragraph id="77D2316FA7CE7A1A0458BCF1F88BB050" pageId="39" pageNumber="56">
<taxonomicName id="EB3EF169BE0961C161235ACC6258AD0A" class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Strumigenys nepalensis" order="Hymenoptera" pageId="39" pageNumber="56" phylum="Arthropoda" rank="species" species="nepalensis">Strumigenys nepalensis</taxonomicName>
