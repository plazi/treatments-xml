. 50, 51 
<taxonomicName id="04CAD7409D9C64578D8F7FD8B1109462" class="Insecta" family="Staphylinidae" genus="Scaphidium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Scaphidium vernicatum" order="Coleoptera" pageId="22" pageNumber="69" phylum="Ascomycota" rank="species" species="vernicatum">Scaphidium vernicatum</taxonomicName>
52, 53 
<taxonomicName id="9BFACB38B8F237D7285369220D3AF602" class="Insecta" family="Staphylinidae" genus="Scaphidium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Scaphidium perpulchrum" order="Coleoptera" pageId="22" pageNumber="69" phylum="Ascomycota" rank="species" species="perpulchrum">Scaphidium perpulchrum</taxonomicName>
