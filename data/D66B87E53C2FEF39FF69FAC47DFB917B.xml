<taxonomicName id="99C24D703C2FEF3AFE4FF8B07D73973E" box="[433,561,1801,1825]" class="Arachnida" family="Salticidae" genus="Scopocira" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="164" phylum="Arthropoda" rank="species" species="crotalica">
