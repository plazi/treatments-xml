<collectingCountry id="A5BD9D861414FFB58C8F4FC3E934842F" box="[404,532,500,526]" name="Argentina" pageId="6" pageNumber="63">Argentina</collectingCountry>
: Misiones: Parque Provincial Salto Encantado, Salto 
<taxonomicName id="1AAAA6951414FFB589B74FC3EFD4842F" box="[1196,1268,500,526]" class="Insecta" family="Scenopinidae" genus="Paratrichia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="66" phylum="Arthropoda" rank="species" species="acuti" status="n. sp.">Acutí</taxonomicName>
, 
<date id="A914FBD61414FFB5881A4FC3EEB7842F" box="[1281,1431,500,526]" pageId="6" pageNumber="63" value="2006-01-08" valueMax="2006-01-14" valueMin="2006-01-08">
<number id="6B62CF311414FFB5881A4FC3EE60842F" box="[1281,1344,500,526]" pageId="6" pageNumber="63" value="11.0" valueMax="14.0" valueMin="8.0">8–14</number>
