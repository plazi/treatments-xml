<number id="C67424C1FFBE7674FB65FBCFFB1C695F" box="[1202,1213,1124,1148]" pageId="27" pageNumber="28" value="1.0">1</number>
<number id="C67424C1FFBE7674FB11FBCFFB52695F" box="[1222,1267,1124,1148]" pageId="27" pageNumber="28" value="165.0">165</number>
. Antennal segments III–VIII length 
<number id="C67424C1FFBE7674FE58FB23FE796983" box="[399,472,1160,1184]" pageId="27" pageNumber="28" value="37.32">37, 32</number>
, 
<number id="C67424C1FFBE7674FE34FB23FD8A6983" box="[483,555,1160,1184]" pageId="27" pageNumber="28" value="35.5">35, 50</number>
, 
<number id="C67424C1FFBE7674FDE0FB23FD216983" box="[567,640,1160,1184]" pageId="27" pageNumber="28" value="10.13">10, 13</number>
