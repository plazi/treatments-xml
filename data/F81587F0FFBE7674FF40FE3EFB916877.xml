<number id="C67424C1FFBE7674FEE9FBEBFECA697B" box="[318,363,1088,1112]" pageId="27" pageNumber="28" value="135.0">135</number>
; maximum width 
<number id="C67424C1FFBE7674FDEAFBEBFDCB697B" box="[573,618,1088,1112]" pageId="27" pageNumber="28" value="160.0">160</number>
; pa inner setae 
<number id="C67424C1FFBE7674FCF6FBEBFCE1697B" box="[801,832,1088,1112]" pageId="27" pageNumber="28" value="60.0">60</number>
; pa outer setae 
<number id="C67424C1FFBE7674FC21FBEBFBB4697B" box="[1014,1045,1088,1112]" pageId="27" pageNumber="28" value="35.0">35</number>
. Fore wing length 
<number id="C67424C1FFBE7674FB27FBEBFABF697B" box="[1264,1310,1088,1112]" pageId="27" pageNumber="28" value="550.0">550</number>
. Metanotal median setae 
<number id="C67424C1FFBE7674FEE6FBCFFEF1695F" box="[305,336,1124,1148]" pageId="27" pageNumber="28" value="25.0">25</number>
. Tergite IX, median dorsal setae 
<number id="C67424C1FFBE7674FD12FBCFFD45695F" box="[709,740,1124,1148]" pageId="27" pageNumber="28" value="20.0">20</number>
; setae S 
<number id="C67424C1FFBE7674FC94FBCFFCEF695F" box="[835,846,1124,1148]" pageId="27" pageNumber="28" value="1.0">1</number>
<number id="C67424C1FFBE7674FC81FBCFFC22695F" box="[854,899,1124,1148]" pageId="27" pageNumber="28" value="120.0">120</number>
; S 
<number id="C67424C1FFBE7674FC48FBCFFC0F695F" box="[927,942,1124,1148]" pageId="27" pageNumber="28" value="2.0">2</number>
<number id="C67424C1FFBE7674FC62FBCFFC43695F" box="[949,994,1124,1148]" pageId="27" pageNumber="28" value="175.0">175</number>
