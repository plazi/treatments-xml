<number id="C67424C1FFBE7674FC81FBB7FC336917" box="[854,914,1052,1076]" pageId="27" pageNumber="28" value="1400.0">1400</number>
. Head length 
<number id="C67424C1FFBE7674FBFBFBB7FBF86917" box="[1068,1113,1052,1076]" pageId="27" pageNumber="28" value="105.0">105</number>
; width across eyes 
<number id="C67424C1FFBE7674FAE4FBB7FAC16917" box="[1331,1376,1052,1076]" pageId="27" pageNumber="28" value="125.0">125</number>
