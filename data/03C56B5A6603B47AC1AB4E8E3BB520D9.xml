</taxonomicName>
(
<taxonomicName id="4C6CA1CF6602B47AC1A24A1F392120D9" authorityName="E.P.Ventenat" authorityYear="1799" box="[158,315,367,392]" class="Liliopsida" family="Alismataceae" kingdom="Plantae" order="Alismatales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="family">Alismataceae</taxonomicName>
), 
<taxonomicName id="4C6CA1CF6602B47AC0724A1F39D720D9" box="[334,461,367,392]" class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Polygonales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E6602B47AC0724A1F39D720D9" box="[334,461,367,392]" italics="true" pageId="87" pageNumber="88">Polygonum</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C6CA1CF6602B47AC3384A013A4A20D9" box="[516,592,369,392]" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Polygonales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E6602B47AC3384A013A4A20D9" box="[516,592,369,392]" italics="true" pageId="87" pageNumber="88">Rumex</emphasis>
</taxonomicName>
(
<taxonomicName id="4C6CA1CF6602B47AC3634A1F3AE420D9" box="[607,766,367,392]" class="Magnoliopsida" family="Polygonaceae" kingdom="Plantae" order="Polygonales" pageId="87" pageNumber="88" phylum="Tracheophyta" rank="family">Polygonaceae</taxonomicName>
