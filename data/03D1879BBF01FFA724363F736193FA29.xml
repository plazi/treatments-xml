<taxonomicName id="4C784D0EBF01FFA725A63F2160D3FBA9" authority="Warncke, 1975" authorityName="Warncke" authorityYear="1975" box="[218,710,1115,1138]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="313" phylum="Arthropoda" rank="species" species="astrella" subGenus="Aciandrena">
