<taxonomicName id="40B18E30EBB024952EDAA0A330B59F81" LSID="urn:lsid:zoobank.org:act:C4B02C42-9193-4281-AB2D-13194B38A444" authority="Reemer" class="Insecta" family="Syrphidae" genus="Paramixogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paramixogaster piptotus" order="Diptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="piptotus">Paramixogaster piptotus Reemer</taxonomicName>
