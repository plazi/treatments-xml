<taxonomicName id="D164573F0D977732D9AA58F0F5A16CE5" class="Insecta" family="Staphylinidae" genus="Tachinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tachinus nanus" order="Coleoptera" pageId="10" pageNumber="65" phylum="Arthropoda" rank="species" species="nanus">Tachinus nanus</taxonomicName>
