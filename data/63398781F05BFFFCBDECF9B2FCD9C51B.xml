<quantity id="2C689B72F054FFFCBEC0FF53FBE0C7D4" box="[955,1058,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="3.32" pageId="8" pageNumber="315" unit="mm" value="3.32">3.32 mm</quantity>
