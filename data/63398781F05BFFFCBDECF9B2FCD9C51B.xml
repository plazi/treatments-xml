<quantity id="2C689B72F054FFFCBE0AFF77FC16C7AF" box="[881,980,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="1.85" pageId="8" pageNumber="315" unit="mm" value="1.85">1.85 mm</quantity>
