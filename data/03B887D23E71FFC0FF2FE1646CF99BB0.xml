<collectingCountry id="F30676543E70FFC0FBA4E4DD69079AC0" box="[1052,1123,223,248]" name="Iceland" pageId="15" pageNumber="190">Island</collectingCountry>
, North and South Carolina, eastern and western Florida, Texas; 
<collectingCountry id="F30676543E70FFC0FD9EE5076FE59B24" box="[550,641,261,284]" name="Panama" pageId="15" pageNumber="190">Panama</collectingCountry>
, 
<collectingCountry id="F30676543E70FFC0FD34E5066F9A9B25" box="[652,766,260,285]" name="United Kingdom" pageId="15" pageNumber="190">Caledonia</collectingCountry>
Bay; 
<collectingCountry id="F30676543E70FFC0FC87E5066E199B25" box="[831,893,260,285]" name="Cuba" pageId="15" pageNumber="190">Cuba</collectingCountry>
; 
<collectingCountry id="F30676543E70FFC0FC30E507696B9B25" box="[904,1039,260,285]" name="Puerto Rico" pageId="15" pageNumber="190">Puerto Rico</collectingCountry>
