<taxonomicName id="75401A4EAA0A3328D4EFF76B10D7CD51" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Cucujidae" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="family">Cucujidae</taxonomicName>
and 
<taxonomicName id="19023E891B5C24B5EFA03F2F2514165D" class="Insecta" family="Scolytidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="family">Scolytidae</taxonomicName>
