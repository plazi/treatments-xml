<collectingRegion id="8A658FC5FFC05773FEF0E3B7FE6FCCB8" box="[372,477,898,922]" country="Mexico" name="Hidalgo" pageId="7" pageNumber="6">Hidalgo</collectingRegion>
</emphasis>
, Tlanchinol carretera Tlanchinol-Aplantlazol, 
<quantity id="8F59ECC2FFC05773FB84E3B7FB89CCB8" box="[1024,1083,898,922]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="7" pageNumber="6" unit="km" value="4.0">4 km</quantity>
, 
<date id="3C1F67E7FFC05773FBC1E3B7FAB7CCB8" box="[1093,1285,898,923]" pageId="7" pageNumber="6" value="1997-08-02" valueMax="1997-08-06" valueMin="1997-08-02">2-6 August 1997</date>
, J. Blackaller, S. B. Salceda and G. A. Pérez, Malaise trap, 
<specimenCount id="5EA78AAEFFC05773FD6FE397FCEACC98" box="[747,856,930,954]" pageId="7" pageNumber="6" type="female">1 female</specimenCount>
