<subSubSection id="F69A6575DC4BFE2865B70530FB9CFEDF" box="[140,1111,351,375]" pageId="4" pageNumber="75" type="etymology">
<paragraph id="BE3F36FEDC4BFE2865B70530FB9CFEDF" blockId="4.[100,1121,197,375]" box="[140,1111,351,375]" pageId="4" pageNumber="75">
<emphasis id="8CF4EAECDC4BFE2865B70530FECCFEDF" bold="true" box="[140,263,351,375]" pageId="4" pageNumber="75">Etymology</emphasis>
