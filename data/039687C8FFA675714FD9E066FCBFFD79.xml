<emphasis id="B94BEACCFFA675734F89E0E8FEE2FB36" bold="true" box="[199,354,1230,1255]" pageId="4" pageNumber="190">Distribution:</emphasis>
<collectingCountry id="F328764EFFA675734E27E0E8FE3EFB36" box="[361,446,1230,1255]" name="Bhutan" pageId="4" pageNumber="190">Bhutan</collectingCountry>
