26.­­­ 
<taxonomicName id="67C743E27B6BFD7E1093FC80CF2C441A" authority="(B. Øllg.) B. Øllgaard" box="[178,769,770,799]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Phlegmariurus" kingdom="Plantae" order="Lycopodiales" pageId="37" pageNumber="38" phylum="Tracheophyta" rank="species" species="talamancanus">
