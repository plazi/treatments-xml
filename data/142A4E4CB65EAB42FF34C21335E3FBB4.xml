<quantity id="5B7B52BFB65FAB42FDCAC49234BCFF04" box="[617,723,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="1.66" pageId="9" pageNumber="110" unit="mm" value="1.66">1.66 mm</quantity>
