<subSubSection id="04B954E8AD7AFFCF3685FC62FF05F9C3" pageId="1" pageNumber="54" type="description">
<paragraph id="4C1C0763AD7AFFCF3685FC62FE7AFC26" blockId="1.[151,1437,821,2010]" pageId="1" pageNumber="54">
Female: Body length 
<quantity id="8B5BAA86AD7AFFCF378FFC62FDCEFC7E" box="[463,541,901,927]" metricMagnitude="-2" metricUnit="m" metricValue="2.7" pageId="1" pageNumber="54" unit="mm" value="27.0">27mm</quantity>
