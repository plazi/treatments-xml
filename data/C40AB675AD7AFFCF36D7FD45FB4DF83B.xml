<quantity id="8B5BAA86AD7AFFCF34B7FC62FC96FC7E" box="[759,837,901,927]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="1" pageNumber="54" unit="mm" value="19.0">19mm</quantity>
