<collectingCountry id="2BD57614FFB57108FECD8F5CFE04ED1C" box="[312,394,1359,1385]" name="Yemen" pageId="16" pageNumber="17">Yemen</collectingCountry>
(new record), 
<collectingCountry id="2BD57614FFB57108FDC38F5CFD07ED1C" box="[566,649,1359,1385]" name="Eritrea" pageId="16" pageNumber="17">Eritrea</collectingCountry>
