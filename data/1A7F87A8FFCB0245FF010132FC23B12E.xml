Group 
<taxonomicName id="55D64D3DFFCB0245FF670132FEEAB020" authorityName="Gaud" authorityYear="1957" box="[241,352,1585,1611]" class="Arachnida" family="Trouessartiidae" genus="Trouessartia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="crucifera">
