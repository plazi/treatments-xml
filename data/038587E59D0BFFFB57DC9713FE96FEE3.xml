<collectingCountry id="F33B76639D09FFFB55F49590FCC1FF53" box="[703,791,189,215]" name="Mexico" pageId="18" pageNumber="19">Mexico</collectingCountry>
, Ocampo (state 
<collectingRegion id="49E8F8119D09FFFB54879590FB84FF5C" box="[972,1106,189,216]" country="Mexico" name="Tamaulipas" pageId="18" pageNumber="19">Tamaulipas</collectingRegion>
