<taxonomicName id="4C0FFDC2DB24FFF17341F8876BFD5E04" authorityName="Bollino &amp; Bordoni" authorityYear="2021" box="[1029,1156,1847,1870]" class="Insecta" family="Curculionidae" genus="Metapocyrtus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="367" phylum="Arthropoda" rank="species" species="waoensis">
