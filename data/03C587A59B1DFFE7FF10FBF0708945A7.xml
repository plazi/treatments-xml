<subSubSection id="C37665389B1DFFE6FF42F909750243AB" box="[197,1079,1728,1752]" pageId="4" pageNumber="24" type="etymology">
<paragraph id="8BD336B39B1DFFE6FF42F909750243AB" blockId="4.[151,1437,1187,2004]" box="[197,1079,1728,1752]" pageId="4" pageNumber="24">
<emphasis id="B918EAA19B1DFFE6FF42F909707E43AB" bold="true" box="[197,331,1728,1752]" pageId="4" pageNumber="24">Etymology.</emphasis>
Latin, 
<taxonomicName id="4C6C4D309B1DFFE6FE1BF909733243AB" box="[412,519,1728,1752]" class="Insecta" family="Cerambycidae" genus="Oxathres" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="24" phylum="Arthropoda" rank="species" species="maculosa">
