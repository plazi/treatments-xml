<subSubSection id="75C19BACFF8F7939FF6C261C61E18FD8" pageId="62" pageNumber="63" type="discussion">
<paragraph id="3D64C827FF8F7939FF6C261C61E18FD8" blockId="62.[151,1435,681,922]" pageId="62" pageNumber="63">
<materialsCitation id="8DB3C27AFF8F7939FF6C261C61E58FD8" ID-GBIF-Occurrence="1288043013" location="northern Vietnam" municipality="southern Vietnam" pageId="62" pageNumber="63" specimenCount="1">
<emphasis id="0FAF1435FF8F7939FF6C261C633C8F34" bold="true" box="[199,315,861,886]" pageId="62" pageNumber="63">Remarks.</emphasis>
