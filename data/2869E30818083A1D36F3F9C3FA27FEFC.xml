Brundin
</taxonomicName>
[
<taxonomicName id="67C0299D18083A1E368EF8C7FE44F877" authority="Brundin, Aust" authorityName="Brundin, Aust" authorityYear="1966" box="[234,510,1911,1933]" class="Insecta" family="Chironomidae" genus="Paraphrotenia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="80" phylum="Arthropoda" rank="species" species="fascipennis">
