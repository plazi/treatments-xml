<paragraph id="8BE9721FEE04255CFDA0FB72FC3DFB14" blockId="73.[582,998,1197,1253]" box="[582,998,1197,1221]" pageId="73" pageNumber="769">
<heading id="D0A1C573EE04255CFDA0FB72FC3DFB14" box="[582,998,1197,1221]" centered="true" fontSize="10" level="2" pageId="73" pageNumber="769" reason="3">
<taxonomicName id="4C56099CEE04255CFDA0FB72FC44FB14" authority="HANSSON &amp; LASALLE, 2003" authorityName="HANSSON &amp; LASALLE" authorityYear="2003" box="[582,927,1197,1221]" class="Insecta" family="Eulophidae" genus="Entedononecremnus" kingdom="Animalia" order="Hymenoptera" pageId="73" pageNumber="769" phylum="Arthropoda" rank="species" species="reticulatus" status="sp. n.">
