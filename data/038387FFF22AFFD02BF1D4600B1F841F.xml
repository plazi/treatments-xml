(female, INPA). 
<emphasis id="B95EEAFBF229FFDF2F56D60A0C71820A" bold="true" box="[1072,1120,1855,1879]" pageId="7" pageNumber="110">F–J.</emphasis>
<taxonomicName id="4C2A4D6AF229FFDF2F00D60B0D54820B" authority="Calhau &amp; Lima &amp; Rafael &amp; Lamas, 2019" authorityName="Calhau &amp; Lima &amp; Rafael &amp; Lamas" authorityYear="2019" box="[1126,1349,1854,1878]" class="Insecta" family="Mydidae" genus="Chrysomydas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="110" phylum="Arthropoda" rank="species" species="phoenix" status="sp. nov.">
<emphasis id="B95EEAFBF229FFDF2F00D60B0D54820B" box="[1126,1349,1854,1878]" italics="true" pageId="7" pageNumber="110">Chrysomydas phoenix</emphasis>
</taxonomicName>
<emphasis id="B95EEAFBF229FFDF2E2AD60A0D8D820A" bold="true" box="[1356,1436,1855,1879]" pageId="7" pageNumber="110">
<taxonomicNameLabel id="A26D5780F229FFDF2E2AD60A0D8D820A" box="[1356,1436,1855,1879]" pageId="7" pageNumber="110" rank="species">sp. nov.</taxonomicNameLabel>
