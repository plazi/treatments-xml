<taxonomicName id="BDB84D01FFDB8B42FF1AFD3DB0F7C88B" box="[151,422,650,676]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="planitarsis">
<emphasis id="48CCEA90FFDB8B42FF1AFD3DB0F7C88B" bold="true" box="[151,422,650,676]" italics="true" pageId="1" pageNumber="60">Dolichopus planitarsis</emphasis>
