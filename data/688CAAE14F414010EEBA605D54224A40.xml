<taxonomicName id="C99BB451BCA22BCFB11EE8A093378D48" authority="Schalkwyk, 1966" authorityName="Schalkwyk" authorityYear="1966" class="Insecta" family="Cryptolaryngidae" genus="Cryptolarynx" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptolarynx" order="Coleoptera" pageId="251" pageNumber="252" phylum="Arthropoda" rank="genus">Cryptolarynx</taxonomicName>
