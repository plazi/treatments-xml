<taxonomicName id="89FD254849134C8F9627EECDFB41B8AB" authority="Schalkwyk, 1966" authorityName="Schalkwyk" authorityYear="1966" class="Insecta" family="Cryptolaryngidae" genus="Cryptolarynx" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptolarynx" order="Coleoptera" pageId="251" pageNumber="252" phylum="Arthropoda" rank="genus">Cryptolarynx</taxonomicName>
