<heading id="D23D81D10B12491AA3D6C4CF9BC0ED88" bold="true" box="[151,1159,1434,1461]" fontSize="11" level="1" pageId="12" pageNumber="386" reason="1">
<emphasis id="BBBEEAAF0B12491AA3D6C4CF9BC0ED88" bold="true" box="[151,1159,1434,1461]" pageId="12" pageNumber="386">
Key to 
<taxonomicName id="4ECA4D3E0B12491AA3B1C4CE9C55ED88" authority="Souza-Dias &amp; Desutter-Grandcolas" authorityName="Souza-Dias &amp; Desutter-Grandcolas" box="[240,786,1434,1461]" class="Insecta" family="Phalangopsidae" genus="Pizacris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="386" phylum="Arthropoda" rank="genus">
