<collectionCode id="8C9EFEFE5004FF9BFBB5009CFB1AFF0C" box="[1122,1183,151,175]" httpUri="http://grbio.org/cool/5icv-29es" name="Zoological Survey of India" pageId="8" pageNumber="45">ZSIC</collectionCode>
<number id="5C47741C5004FF9BFB72009CFB6BFF0C" box="[1189,1262,151,175]" pageId="8" pageNumber="45" value="25945.0">25945</number>
; male collected by M. S. Chaitra, on 
<date id="9E3140FB5004FF9BFE5000B1FDE1FF71" box="[391,612,186,210]" pageId="8" pageNumber="45" value="2005-11-19">
<number id="5C47741C5004FF9BFE5000B1FE26FF71" box="[391,419,186,210]" pageId="8" pageNumber="45" value="19.0">19</number>
November 
<number id="5C47741C5004FF9BFDFE00B1FDE1FF71" box="[553,612,186,210]" pageId="8" pageNumber="45" value="2005.0">2005</number>
</date>
, from Singsar game road (elevation 
<quantity id="2D77CBDE5004FF9BFBD600B1FBCCFF71" box="[1025,1097,186,210]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="8" pageNumber="45" unit="m" value="750.0">750 m</quantity>
asl), Kudremukh Range (latitude 
<geoCoordinate id="8FBB00FC5004FF9BFF1900D5FEE5FF56" box="[206,352,221,245]" direction="north" orientation="latitude" pageId="8" pageNumber="45" precision="1" value="13.21339">
N 
<number id="5C47741C5004FF9BFF3E00D6FED2FF56" box="[233,343,221,245]" pageId="8" pageNumber="45" value="13.21339">13.21339</number>
Â°
</geoCoordinate>
, longitude 
<geoCoordinate id="8FBB00FC5004FF9BFE0B00D5FDE9FF56" box="[476,620,221,245]" direction="east" orientation="longitude" pageId="8" pageNumber="45" precision="1" value="75.18583">
E 
<number id="5C47741C5004FF9BFE2300D6FDE6FF56" box="[500,611,221,245]" pageId="8" pageNumber="45" value="75.18583">75.18583</number>
Â°
</geoCoordinate>
; WGS 
<number id="5C47741C5004FF9BFD6B00D6FD5DFF56" box="[700,728,221,245]" pageId="8" pageNumber="45" value="84.0">84</number>
