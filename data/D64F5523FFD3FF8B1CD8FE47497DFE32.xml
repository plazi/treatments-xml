<emphasis id="6C923827FFD3FF881CD8FE474DEAFD66" bold="true" box="[151,252,619,645]" italics="true" pageId="9" pageNumber="103">Veturius</emphasis>
(
<emphasis id="6C923827FFD3FF881D42FE474C7CFD66" bold="true" box="[269,362,619,645]" italics="true" pageId="9" pageNumber="103">Publius</emphasis>
) 
<emphasis id="6C923827FFD3FF881D34FE474CE5FD66" bold="true" box="[379,499,619,645]" italics="true" pageId="9" pageNumber="103">impressus</emphasis>
Hincks, 
<number id="E82EF612FFD3FF881E2EFE404F8AFD65" box="[609,668,620,646]" pageId="9" pageNumber="103" value="1934.0">1934</number>
</emphasis>
