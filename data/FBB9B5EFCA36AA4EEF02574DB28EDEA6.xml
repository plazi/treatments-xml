<paragraph id="14E462CC57FA4CCF9840DC8727476786" pageId="4" pageNumber="5">
<taxonomicName id="D5D93CE174334A3D5637F0F599019C0E" LSID="http://zoobank.org/1B7B13C0-FA56-466E-9FFE-AB985EB582BA" authority="Fernandez Marchan" authorityName="Rosa" authorityYear="1887" class="Clitellata" family="Hormogastridae" genus="Hormogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hormogaster joseantonioi" order="Crassiclitellata" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="joseantonioi">
