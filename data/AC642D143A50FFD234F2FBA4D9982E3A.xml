<quantity id="E33531E73A50FFD2359BFB50DFDF2FD1" box="[510,644,1244,1268]" metricMagnitude="2" metricUnit="m" metricValue="5.89" metricValueMax="9.78" metricValueMin="2.0" pageId="25" pageNumber="318" unit="m" value="589.0" valueMax="978.0" valueMin="200.0">
