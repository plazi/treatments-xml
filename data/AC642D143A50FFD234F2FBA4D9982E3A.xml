<quantity id="E33531E73A50FFD23551FB3BDCE42FED" box="[308,447,1207,1231]" metricMagnitude="-1" metricUnit="m" metricValue="2.65" metricValueMax="4.1" metricValueMin="1.2" pageId="25" pageNumber="318" unit="cm" value="26.5" valueMax="41.0" valueMin="12.0">12 to 41 cm</quantity>
