<heading id="D0E881F6FFEA5B16EEE0F6DF572ECCF2" bold="true" box="[151,579,1437,1463]" fontSize="11" level="1" pageId="0" pageNumber="269" reason="1">
<taxonomicName id="4C1F4D19FFEA5B16EEE0F6DF572ECCF2" authority="Chopard, 1916" authorityName="Chopard" authorityYear="1916" box="[151,579,1437,1463]" class="Insecta" family="Mantidae" genus="Thespoides" kingdom="Animalia" order="Mantodea" pageId="0" pageNumber="269" phylum="Arthropoda" rank="species" species="bolivari">
