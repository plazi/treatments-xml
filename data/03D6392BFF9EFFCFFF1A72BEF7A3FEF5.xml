<taxonomicName id="4C7FF3BEFF9CFFCEFEA876F0F41BF9B9" authorityName="Löcker &amp; Holzinger" authorityYear="2019" box="[293,533,1527,1551]" class="Insecta" family="Cixiidae" genus="Chidaea" kingdom="Animalia" order="Hemiptera" pageId="34" pageNumber="435" phylum="Arthropoda" rank="species" species="bobadeenensis">
<emphasis id="B90B542FFF9CFFCEFEA876F0F41BF9B9" box="[293,533,1527,1551]" italics="true" pageId="34" pageNumber="435">
