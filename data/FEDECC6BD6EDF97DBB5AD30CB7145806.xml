<taxonomicName id="223F3543202B99679D7468F33FCEFC70" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180372" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole voeltzkowi Forel" lsidName-HNS="Pheidole voeltzkowi Forel" order="Hymenoptera" pageNumber="207" phylum="Arthropoda" rank="species" species="voeltzkowi">Pheidole voeltzkowi Forel</taxonomicName>
