<quantity id="4CA59B109D6CFFDBFC64F90E3521F926" box="[974,1050,1760,1784]" metricMagnitude="2" metricUnit="m" metricValue="1.64" pageId="16" pageNumber="667" unit="m" value="164.0">164 m</quantity>
