<subSubSection id="6DCF65061C13FFB05C8AFE05FCDDFCCD" pageId="18" pageNumber="60" type="materials_examined">
<paragraph id="256A368D1C13FFB05C8AFE05FB31FD05" blockId="18.[151,1436,326,782]" pageId="18" pageNumber="60">
