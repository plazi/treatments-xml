<taxonomicName id="7F940ECFFFCC3321FEDF6B0D3814FAAB" authority="KUHARA, 1999" authorityName="KUHARA" authorityYear="1999" box="[275,549,1292,1314]" class="Insecta" family="Philopotamidae" genus="Kisaura" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="1612" phylum="Arthropoda" rank="species" species="borealis">
<emphasis id="8AE0A95EFFCC3321FEDF6B0D3BB4FAAB" box="[275,389,1292,1314]" italics="true" pageId="5" pageNumber="1612">K. borealis</emphasis>
