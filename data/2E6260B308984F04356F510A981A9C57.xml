<taxonomicName id="D1D1FD2A652F875DF64C444A7DF3E9DA" class="Insecta" family="Hydrophilidae" genus="Helochares" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Helochares" order="Coleoptera" pageId="26" pageNumber="77" phylum="Arthropoda" rank="genus">Helochares</taxonomicName>
