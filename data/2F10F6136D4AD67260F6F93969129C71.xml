species: 
<taxonomicName id="60B93C866D4AD672615BF8FC6F999D6F" authority="Koch, 1836" authorityName="Koch" authorityYear="1836" box="[336,701,1843,1867]" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="50" phylum="Arthropoda" rank="species" species="cruentus">
