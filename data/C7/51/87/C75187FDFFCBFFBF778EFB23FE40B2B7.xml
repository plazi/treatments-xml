<taxonomicName baseAuthorityName="Thunberg" baseAuthorityYear="1822" box="[874,1063,1366,1393]" class="Insecta" family="Ichneumonidae" genus="Grotea" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="61" phylum="Arthropoda" rank="species" species="delicator">
