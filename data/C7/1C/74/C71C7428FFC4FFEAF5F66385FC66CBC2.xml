<quantity box="[247,417,1282,1306]" metricMagnitude="3" metricUnit="m" metricValue="1.3" metricValueMax="1.6" metricValueMin="1.0" pageId="32" pageNumber="267" unit="m" value="1300.0" valueMax="1600.0" valueMin="1000.0">1000 to 1600 m</quantity>
