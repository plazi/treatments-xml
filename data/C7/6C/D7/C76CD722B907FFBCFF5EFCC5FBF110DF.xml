<quantity box="[766,874,1376,1402]" metricMagnitude="-3" metricUnit="m" metricValue="1.24" pageId="11" pageNumber="39" unit="mm" value="1.24">1.24 mm</quantity>
, HW 
<quantity box="[940,1047,1376,1402]" metricMagnitude="-3" metricUnit="m" metricValue="1.62" pageId="11" pageNumber="39" unit="mm" value="1.62">1.62 mm</quantity>
, Edist 
<quantity box="[1124,1231,1376,1402]" metricMagnitude="-4" metricUnit="m" metricValue="5.4" pageId="11" pageNumber="39" unit="mm" value="0.54">0.54 mm</quantity>
, Ediam 
<quantity box="[1325,1432,1376,1402]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="11" pageNumber="39" unit="mm" value="0.35">0.35 mm</quantity>
