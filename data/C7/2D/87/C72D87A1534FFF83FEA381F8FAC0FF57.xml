<quantity box="[321,468,1576,1603]" metricMagnitude="3" metricUnit="m" metricValue="1.0775" metricValueMax="1.275" metricValueMin="0.88" pageId="17" pageNumber="18" unit="m" value="1077.5" valueMax="1275.0" valueMin="880.0">880–1275 m</quantity>
