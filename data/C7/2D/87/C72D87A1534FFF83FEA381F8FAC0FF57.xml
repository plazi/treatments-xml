<paragraph blockId="17.[151,1437,1468,2035]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[199,345,1540,1566]" pageId="17" pageNumber="18">Distribution</emphasis>
. Arctic (
<quantity box="[447,594,1540,1567]" metricMagnitude="3" metricUnit="m" metricValue="1.143" metricValueMax="1.505" metricValueMin="0.7809999999999999" pageId="17" pageNumber="18" unit="m" value="1143.0" valueMax="1505.0" valueMin="781.0">781–1505 m</quantity>
), NE Atlantic (
<quantity box="[768,930,1540,1567]" metricMagnitude="3" metricUnit="m" metricValue="1.9919999999999998" metricValueMax="2.6689999999999996" metricValueMin="1.315" pageId="17" pageNumber="18" unit="m" value="1992.0" valueMax="2669.0" valueMin="1315.0">1315–2669 m</quantity>
), W Atlantic (274– 
<quantity box="[1149,1225,1540,1567]" metricMagnitude="2" metricUnit="m" metricValue="2.7399999999999998" pageId="17" pageNumber="18" unit="m" value="274.0">274 m</quantity>
), E Atlantic (
<quantity metricMagnitude="3" metricUnit="m" metricValue="1.0085" metricValueMax="1.236" metricValueMin="0.7809999999999999" pageId="17" pageNumber="18" unit="m" value="1008.5" valueMax="1236.0" valueMin="781.0">781– 1236 m</quantity>
