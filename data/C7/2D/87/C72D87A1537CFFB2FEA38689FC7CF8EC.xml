<emphasis bold="true" box="[151,297,1076,1102]" pageId="34" pageNumber="35">Distribution</emphasis>
. SPA (
<quantity box="[374,450,1076,1103]" metricMagnitude="0" metricUnit="m" metricValue="1.0" metricValueMax="2.0" metricValueMin="0.0" pageId="34" pageNumber="35" unit="m" value="1.0" valueMax="2.0" valueMin="0.0">0–2 m</quantity>
