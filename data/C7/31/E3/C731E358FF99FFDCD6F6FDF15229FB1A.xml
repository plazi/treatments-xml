<paragraph blockId="75.[189,363,1778,1804]" box="[189,363,1778,1804]" pageId="75" pageNumber="76">
<heading bold="true" box="[189,363,1778,1804]" fontSize="11" level="3" pageId="75" pageNumber="76" reason="3">
<emphasis bold="true" box="[189,363,1778,1804]" pageId="75" pageNumber="76">Redescription</emphasis>
