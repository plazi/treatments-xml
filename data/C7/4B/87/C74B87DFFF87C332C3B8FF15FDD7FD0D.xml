<subSubSection pageId="7" pageNumber="271" type="description">
<paragraph blockId="7.[151,650,258,280]" box="[151,650,258,280]" pageId="7" pageNumber="271">
<taxonomicName authority="Viraktamath &amp; wesley" authorityName="Viraktamath &amp; Wesley" authorityYear="1988" box="[151,538,258,280]" class="Insecta" family="Cicadellidae" genus="Nirvana" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="271" phylum="Arthropoda" rank="species" species="striata">
