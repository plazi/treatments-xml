<subSubSection pageId="44" pageNumber="45" type="etymology">
<paragraph blockId="44.[151,1437,1786,2027]" pageId="44" pageNumber="45">
<emphasis bold="true" box="[199,326,1858,1883]" pageId="44" pageNumber="45">Etymology</emphasis>
