<subSubSection type="reference_group">
<paragraph pageNumber="39">
Limnomyrmex 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:183221" authority="Wheeler, 1910" authorityName="Wheeler" authorityYear="1910" class="Insecta" family="Formicidae" genus="Nesomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Nesomyrmex stramineus (Arnold)" order="Hymenoptera" pageNumber="39" phylum="Arthropoda" rank="species" species="stramineus">stramineus Arnold</taxonomicName>
