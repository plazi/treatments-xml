<elevation box="[891,964,225,251]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="45" pageNumber="46" unit="m" value="800.0">800 m</elevation>
</quantity>
, (13/15/0) in liquid, 
<collectorName box="[1191,1224,225,251]" pageId="45" pageNumber="46">Ac</collectorName>
1442; (2/2/0) slide mounted, 
<collectorName box="[262,295,261,287]" pageId="45" pageNumber="46">Ac</collectorName>
<date box="[301,426,261,287]" pageId="45" pageNumber="46" value="1360" valueMax="1361">1360-1361</date>
