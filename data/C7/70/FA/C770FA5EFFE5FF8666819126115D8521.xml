<taxonomicName authorityName="Gustafsson &amp; Zou" authorityYear="2020" box="[451,604,475,500]" class="Insecta" family="Philopteridae" genus="Valimia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="14" pageNumber="502" phylum="Arthropoda" rank="species" species="necopinata">
