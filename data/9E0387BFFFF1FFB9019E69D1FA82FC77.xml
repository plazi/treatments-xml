<subSubSection id="5EB06522FFF1FFB901CE6AD4FB91FC31" box="[199,1074,914,938]" pageId="2" pageNumber="441" type="etymology">
<paragraph id="161536A9FFF1FFB901CE6AD4FB91FC31" blockId="2.[151,1436,151,1007]" box="[199,1074,914,938]" pageId="2" pageNumber="441">
<emphasis id="24DEEABBFFF1FFB901CE6AD4FEE4FC32" bold="true" box="[199,327,914,938]" pageId="2" pageNumber="441">Etymology.</emphasis>
