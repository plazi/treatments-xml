<taxonomicName id="8CA575CAFF8EFB4F2D03F99CDD161E2C" authority="Li &amp; Chen &amp; Yang, 2020" authorityName="Li &amp; Chen &amp; Yang" authorityYear="2020" box="[346,585,1627,1651]" class="Insecta" family="Lauxaniidae" genus="Noeetomima" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="511" phylum="Arthropoda" rank="species" species="trisurstyla" status="sp. nov.">
