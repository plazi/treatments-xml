<treatmentCitationGroup id="4CA711C6550A562381DCD49AF2D11086" pageId="4" pageNumber="5">
<taxonomicName id="ABB74D6B550A562381DCD49AF08110E9" authority="Pascoe, 1876:54" authorityName="Pascoe" authorityPageNumber="54" authorityYear="1876" box="[151,415,418,440]" class="Insecta" family="Curculionidae" genus="Irenimus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
