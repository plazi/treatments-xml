<paragraph id="8BCD94236F27FFCA0BFCFE29FECDFDFF" blockId="89.[189,381,488,515]" box="[189,381,488,515]" pageId="89" pageNumber="90">
<emphasis id="B90648316F27FFCA0BFCFE29FEABFDFF" allCaps="true" box="[189,283,489,515]" inLineHeading="true" pageId="89" pageNumber="90">LENGTH.</emphasis>
<quantity id="4C8A39C66F27FFCA0A62FE28FEC9FDFF" box="[291,377,488,515]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="89" pageNumber="90" unit="mm" value="13.0">13 mm</quantity>
