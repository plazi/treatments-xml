<taxonomicName id="4C4E17FCFFC3864E242C7FB5FC4138D2" authorityName="Gray" authorityYear="1864" box="[875,986,976,1002]" class="Mammalia" family="Balaenidae" genus="Eubalaena" kingdom="Animalia" order="Cetacea" pageId="19" pageNumber="834" phylum="Chordata" rank="genus">
