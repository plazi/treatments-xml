<taxonomicName id="0BAD79D2FFDAF442FB5FF3AEFA293620" box="[1274,1436,1260,1284]" class="Rhabditophora" family="Haplopharyngidae" genus="Haplopharynx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplopharyngida" pageId="9" pageNumber="586" phylum="Platyhelminthes" rank="genus">
