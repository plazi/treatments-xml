<collectingCountry id="B4BA42C1FFDAF442FE79F252FD8B3603" box="[476,574,1296,1319]" name="Taiwan" pageId="9" pageNumber="586">Formosa</collectingCountry>
at 
<number id="7A651076FFDAF442FDC1F252FDCA360C" box="[612,639,1296,1320]" pageId="9" pageNumber="586" value="37.0">37</number>
° 
<geoCoordinate id="A9996496FFDAF442FD2DF252FD5F3603" box="[648,746,1296,1320]" direction="north" orientation="latitude" pageId="9" pageNumber="586" precision="55" value="0.317">
<number id="7A651076FFDAF442FD2DF252FD7F360C" box="[648,714,1296,1320]" pageId="9" pageNumber="586" value="0.317">0.317</number>
N
</geoCoordinate>
, 
<number id="7A651076FFDAF442FD52F24DFCB1360C" box="[759,772,1295,1320]" pageId="9" pageNumber="586" value="7.0">7</number>
° 
<geoCoordinate id="A9996496FFDAF442FCA8F24DFC373603" box="[781,898,1295,1320]" direction="west" orientation="longitude" pageId="9" pageNumber="586" precision="55" value="-58.08">
<number id="7A651076FFDAF442FCA8F24DFCEB360C" box="[781,862,1295,1320]" pageId="9" pageNumber="586" value="58.08">58.080</number>
W
</geoCoordinate>
, between the roots of 
<taxonomicName id="0BAD79D2FFDAF442FB24F252FB573603" box="[1153,1250,1296,1319]" class="Liliopsida" family="Poaceae" genus="Spartina" kingdom="Plantae" order="Poales" pageId="9" pageNumber="586" phylum="Tracheophyta" rank="genus">
