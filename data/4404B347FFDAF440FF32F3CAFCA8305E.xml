<date id="B8132491FFDAF442FAA8F239FF663693" pageId="9" pageNumber="586" value="2011-09-08">
<number id="7A651076FFDAF442FAA8F239FAAE36B7" box="[1293,1307,1403,1427]" pageId="9" pageNumber="586" value="8.0">8</number>
