<date id="B8132491FFDAF442FD47F3AFFC2C3621" box="[738,921,1260,1285]" pageId="9" pageNumber="586" value="2013-10-07">
<number id="7A651076FFDAF442FD47F3AFFD453620" box="[738,752,1261,1284]" pageId="9" pageNumber="586" value="7.0">7</number>
