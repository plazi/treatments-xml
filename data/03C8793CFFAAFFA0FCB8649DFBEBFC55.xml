<taxonomicName id="4C61B3A9FFAAFFA2FD49678FFC19F949" box="[765,898,1686,1704]" class="Insecta" family="Staphylinidae" genus="Neoprocirrus" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
