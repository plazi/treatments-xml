<taxonomicName id="0986BC92091FFFD6318EFEEA678B5573" authority="Sasa et Suzuki, 2001b: 76" authorityName="Sasa et Suzuki" authorityPageNumber="76" authorityYear="2001" box="[151,702,330,353]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="61" phylum="Arthropoda" rank="species" species="daitoneoum">
<emphasis id="FCF21B03091FFFD6318EFEEA64975573" box="[151,418,330,353]" italics="true" pageId="12" pageNumber="61">Polypedilum daitoneoum</emphasis>
