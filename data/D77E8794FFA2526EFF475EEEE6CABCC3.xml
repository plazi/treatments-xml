<collectingCountry id="27C07612FFAD526EFEB75C73E6EBBC7B" box="[359,426,1125,1151]" name="China" pageId="8" pageNumber="294">China</collectingCountry>
(
<collectingRegion id="9D13F860FFAD526EFE695C73E551BC84" box="[441,528,1125,1152]" country="China" name="Hainan" pageId="8" pageNumber="294">Hainan</collectingRegion>
