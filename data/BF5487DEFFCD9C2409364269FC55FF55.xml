<taxonomicName id="F0FD4D4BFFC29C270ABE40BEFC5DF912" baseAuthorityName="Hagen" baseAuthorityYear="1861" box="[667,905,1665,1689]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="2222" phylum="Arthropoda" rank="species" species="borealis">
