<paragraph id="8B91BC26FF986307731303CCFDB446F5" blockId="15.[264,593,1764,1790]" box="[264,593,1764,1790]" pageId="15" pageNumber="16">
<heading id="D0D90B4AFF986307731303CCFDB446F5" bold="true" box="[264,593,1764,1790]" fontSize="11" level="1" pageId="15" pageNumber="16" reason="1">
<emphasis id="B95A6034FF986307731303CCFDB446F5" bold="true" box="[264,593,1764,1790]" pageId="15" pageNumber="16">
<taxonomicName id="4C2EC7A5FF986307731303CCFDE546F5" authorityName="Borgmeier" authorityYear="1959" box="[264,512,1764,1790]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="digitalis">
<emphasis id="B95A6034FF986307731303CCFDE546F5" bold="true" box="[264,512,1764,1790]" italics="true" pageId="15" pageNumber="16">Melaloncha digitalis</emphasis>
