<collectingRegion id="49F3A084FFAFA171FD01DE94131BFA63" box="[752,804,1445,1470]" country="Papua New Guinea" name="Gulf" pageId="17" pageNumber="168">Gulf</collectingRegion>
of 
<collectingCountry id="F3202EF6FFAFA171FCB9DE9413A2FA63" box="[840,925,1445,1470]" name="Mexico" pageId="17" pageNumber="168">
<collectingRegion id="49F3A084FFAFA171FCB9DE9413A2FA63" box="[840,925,1445,1470]" country="Mexico" name="Mexico" pageId="17" pageNumber="168">Mexico</collectingRegion>
</collectingCountry>
