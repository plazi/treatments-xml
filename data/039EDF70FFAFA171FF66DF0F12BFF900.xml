<quantity id="4CCFC383FFAFA171FED2DD071161F990" box="[291,350,1590,1614]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="17" pageNumber="168" unit="m" value="10.0">10 m</quantity>
