<collectingCountry id="F3202EF6FFAFA171FC25DE941404FA63" box="[980,1083,1445,1470]" name="Bahamas" pageId="17" pageNumber="168">Bahamas</collectingCountry>
to 
<collectingCountry id="F3202EF6FFAFA171FBAFDE9414B5FA63" box="[1118,1162,1445,1470]" name="Iceland" pageId="17" pageNumber="168">Ilha</collectingCountry>
