<quantity id="4CCFC383FFAFA171FDD6DD201297F9F4" box="[551,680,1553,1578]" metricMagnitude="2" metricUnit="m" metricValue="2.75" metricValueMax="4.0" metricValueMin="1.5" pageId="17" pageNumber="168" unit="m" value="275.0" valueMax="400.0" valueMin="150.0">150–400 m</quantity>
