<taxonomicName id="4C4F4D0CFF86FFF0F21DFDB52FE1765F" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,489,584,608]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="miles">
