<heading id="883481F6FFF6FFE0E7F8D1CEFE4CF9BF" bold="true" box="[151,398,1516,1542]" fontSize="11" level="1" pageId="1" pageNumber="175" reason="1">
<taxonomicName id="14C34D19FFF6FFE0E7F8D1CEFE4CF9BF" authority="Watts, 1985" authorityName="Watts" authorityYear="1985" box="[151,398,1516,1542]" class="Insecta" family="Dytiscidae" genus="Tiporus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="175" phylum="Arthropoda" rank="genus">
