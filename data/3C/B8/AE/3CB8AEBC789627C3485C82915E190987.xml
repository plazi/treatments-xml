<subSubSection pageId="78" pageNumber="79" type="names">
<paragraph pageId="78" pageNumber="79">Names.</paragraph>
<paragraph pageId="78" pageNumber="79">
<emphasis bold="true" pageId="78" pageNumber="79">Myanmar</emphasis>
: 
<emphasis italics="true" pageId="78" pageNumber="79">thawka</emphasis>
, 
<emphasis italics="true" pageId="78" pageNumber="79">thawka-gyi</emphasis>
. 
<emphasis bold="true" pageId="78" pageNumber="79">English</emphasis>
: pride of Burma, queen of flowering trees.
</paragraph>
</subSubSection>
<subSubSection pageId="78" pageNumber="79" type="range">
<paragraph pageId="78" pageNumber="79">Range.</paragraph>
<paragraph pageId="78" pageNumber="79">Endemic to Myanmar (temperate southeastern Asia). Found in southern Myanmar, in Kayin and Taninthayi.</paragraph>
</subSubSection>
<subSubSection pageId="78" pageNumber="79" type="uses">
