<taxonomicName authorityName="Thompson" authorityYear="1815" box="[2227,2351,1035,1068]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="anomalus">anomalus</taxonomicName>
species group: 
<taxonomicName authorityName="Thompson" authorityYear="1815" box="[1974,2157,1083,1110]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="anomalus">H. anomalus</taxonomicName>
, 
<taxonomicName authorityName="Anderson &amp; Gutierrez" authorityYear="2009" box="[2188,2380,1083,1110]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="catopterius">H. catopterius</taxonomicName>
, 
<taxonomicName authorityName="Anderson" authorityYear="2003" box="[2411,2559,1083,1110]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="oasicus">H. oasicus</taxonomicName>
, 
<taxonomicName authorityName="Thomas" authorityYear="1901" box="[1975,2128,1122,1147]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="australis">H. australis</taxonomicName>
, and 
<taxonomicName authorityName="R. P. Anderson &amp; Jarrin-Valladares" authorityYear="2002" box="[2207,2320,1122,1147]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="teleus">H. teleus</taxonomicName>
. Only 
<taxonomicName authorityName="Thomas" authorityYear="1901" box="[2414,2567,1122,1147]" class="Mammalia" family="Heteromyidae" genus="Heteromys" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="197" phylum="Chordata" rank="species" species="australis">H. australis</taxonomicName>
