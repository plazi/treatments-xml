<taxonomicName LSID="3CC43913-BDB5-5263-9A83-0958F7ED2379" authority="Den Heyer, 1978" authorityName="Den Heyer" authorityYear="1979" class="Arachnida" family="Cunaxidae" genus="Rubroscirus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Trombidiformes" pageId="64" pageNumber="65" phylum="Arthropoda" rank="subFamily" subFamily="Coleoscirinae">
