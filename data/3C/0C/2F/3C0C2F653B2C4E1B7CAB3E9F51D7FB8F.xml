(
<number box="[939,976,618,643]" pageId="8" pageNumber="329" value="2.9">2.9</number>
–5.0); CW = 
<number box="[1125,1174,619,643]" pageId="8" pageNumber="329" value="3.09">3.09</number>
(
<number box="[1189,1279,618,643]" pageId="8" pageNumber="329" value="2.9" valueMax="3.5" valueMin="2.3">2.3–3.5</number>
); OAL = 
<number box="[1387,1436,618,643]" pageId="8" pageNumber="329" value="0.62">0.62</number>
