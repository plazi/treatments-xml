<collectionCode LSID="urn:lsid:biocol.org:col:12716" box="[486,545,656,680]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:12716" name="Bristol, Clifton and West of England Zoological Society's Gardens" pageId="3" pageNumber="324">CON</collectionCode>
