<number box="[289,378,577,601]" pageId="11" pageNumber="332" value="1.5499999999999998" valueMax="1.7" valueMin="1.4">1.4–1.7</number>
); OAL = 
<number box="[484,533,577,601]" pageId="11" pageNumber="332" value="0.33">0.33</number>
