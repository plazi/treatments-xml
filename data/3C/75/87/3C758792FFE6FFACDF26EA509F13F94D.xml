, 
<geoCoordinate box="[1106,1242,197,220]" degrees="56" direction="west" minutes="25" orientation="longitude" pageId="14" pageNumber="204" precision="15" seconds="02" value="-56.417225">56°25′02′′W</geoCoordinate>
