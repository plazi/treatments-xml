</taxonomicName>
<taxonomicName box="[396,611,1953,1975]" pageId="9" pageNumber="576" rank="species" species="varius" status="sp. nov." subGenus="Microsaurus">
(
<taxonomicName authority="Dejean, 1833" authorityName="Dejean" authorityYear="1833" box="[404,532,1954,1975]" class="Insecta" family="Staphylinidae" genus="Microsaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="576" phylum="Arthropoda" rank="genus">
