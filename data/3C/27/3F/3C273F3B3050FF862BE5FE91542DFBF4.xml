<emphasis bold="true" box="[199,355,582,608]" pageId="48" pageNumber="199">Distribution:</emphasis>
<collectingCountry box="[362,448,583,609]" name="Mexico" pageId="48" pageNumber="199">Mexico</collectingCountry>
(
<collectingRegion box="[462,540,583,609]" country="Mexico" name="Jalisco" pageId="48" pageNumber="199">Jalisco</collectingRegion>
