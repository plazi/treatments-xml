<emphasis box="[571,792,942,969]" italics="true" pageId="2" pageNumber="573">S. damingshanensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[799,889,942,968]" pageId="2" pageNumber="573">
<taxonomicNameLabel box="[799,889,942,968]" pageId="2" pageNumber="573" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName authority="Wang &amp; Framenau &amp; Zhang, 2021" authorityName="Wang &amp; Framenau &amp; Zhang" authorityYear="2021" box="[943,1085,942,969]" class="Arachnida" family="Lycosidae" genus="Sinartoria" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="573" phylum="Arthropoda" rank="species" species="zhuangius" status="sp. nov.">
<emphasis box="[943,1085,942,969]" italics="true" pageId="2" pageNumber="573">S. zhuangius</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1092,1181,942,968]" pageId="2" pageNumber="573">
<taxonomicNameLabel box="[1092,1181,942,968]" pageId="2" pageNumber="573" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</paragraph>
