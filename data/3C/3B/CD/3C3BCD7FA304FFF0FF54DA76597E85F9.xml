<date box="[531,567,1796,1820]" pageId="20" pageNumber="121">
<number box="[531,545,1796,1820]" pageId="20" pageNumber="121" value="2.0">2</number>
x
</date>
longer processes with a hair on inner 
<number box="[999,1014,1796,1820]" pageId="20" pageNumber="121" value="2.0">2</number>
/ 
<number box="[1025,1038,1796,1820]" pageId="20" pageNumber="121" value="3.0">3</number>
. Dorsum of A 
<number box="[1200,1213,1796,1820]" pageId="20" pageNumber="121" value="2.0">2</number>
