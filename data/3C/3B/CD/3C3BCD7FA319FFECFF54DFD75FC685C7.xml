<date box="[718,839,1548,1572]" pageId="9" pageNumber="110" value="2012-03-11">
<number box="[718,743,1548,1572]" pageId="9" pageNumber="110" value="11.0">11</number>
.iii. 
<number box="[776,839,1548,1572]" pageId="9" pageNumber="110" value="2012.0">2012</number>
</date>
(ex pupa); 
<number box="[970,984,1548,1572]" pageId="9" pageNumber="110" value="2.0">2</number>
exs, Mikata, Mitsushima, Tsushima Is., Nagasaki, S. Kobayashi, 
<date box="[433,554,1584,1608]" pageId="9" pageNumber="110" value="2012-03-27">
<number box="[433,462,1584,1608]" pageId="9" pageNumber="110" value="27.0">27</number>
.iii. 
<number box="[493,554,1584,1608]" pageId="9" pageNumber="110" value="2012.0">2012</number>
</date>
