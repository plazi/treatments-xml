<date box="[755,871,1700,1724]" pageId="22" pageNumber="123" value="2012-05-19">
<number box="[755,783,1700,1724]" pageId="22" pageNumber="123" value="19.0">19</number>
.v. 
<number box="[806,871,1700,1724]" pageId="22" pageNumber="123" value="2012.0">2012</number>
</date>
., S. Kobayashi, K. Nakatsuka, T. Yoshida &amp; T. Hirowatari, 8, 
<date box="[314,421,1736,1760]" pageId="22" pageNumber="123" value="2012-05-11">
