3. 
<taxonomicName authorityName="Dirkse &amp; Bouman" authorityYear="1995" baseAuthorityName="Mont." box="[1327,1452,1918,1940]" class="Bryopsida" family="Brachytheciaceae" genus="Rhynchostegiella" kingdom="Plantae" order="Hypnales" pageId="1" pageNumber="194" phylum="Bryophyta" rank="species" species="teneriffae">
