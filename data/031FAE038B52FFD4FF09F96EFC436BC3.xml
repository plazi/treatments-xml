<collectingCountry id="F3A15F858B53FFD4FC30FEA8FB8B6B0B" box="[942,1029,297,323]" name="Nigeria" pageId="39" pageNumber="40">Nigeria</collectingCountry>
, 
<collectingCountry id="F3A15F858B53FFD4FB8FFEA8FBFE6B0B" box="[1041,1136,297,323]" name="Rwanda" pageId="39" pageNumber="40">Rwanda</collectingCountry>
, 
<collectingCountry id="F3A15F858B53FFD4FBE3FEA8FA9E6B0B" box="[1149,1296,297,323]" name="South Africa" pageId="39" pageNumber="40">South Africa</collectingCountry>
, 
<collectingCountry id="F3A15F858B53FFD4FA83FEA8FA166B0B" box="[1309,1432,297,323]" name="Zimbabwe" pageId="39" pageNumber="40">Zimbabwe</collectingCountry>
, 
<collectingCountry id="F3A15F858B53FFD4FF09FECCFF606B2F" box="[151,238,333,359]" name="Uganda" pageId="39" pageNumber="40">Uganda</collectingCountry>
and 
<collectingCountry id="F3A15F858B53FFD4FEBAFECCFEFC6B2F" box="[292,370,333,359]" name="Yemen" pageId="39" pageNumber="40">Yemen</collectingCountry>
(
<bibRefCitation id="EF2762E48B53FFD4FE1EFECCFDA26B2F" author="Sabrosky, C. W." box="[384,556,333,359]" pageId="39" pageNumber="40" pagination="695 - 712" refId="ref29948" refString="Sabrosky, C. W. (1980) Family Chloropidae. In: Crosskey, R. W. (editor). Catalogue of the Diptera of the Afrotropical Region, pp. 695 - 712. British Museum (Natural History), London. 1437 pp." type="book chapter" year="1980">Sabrosky 1980</bibRefCitation>
; 
<bibRefCitation id="EF2762E48B53FFD4FDA6FECCFCF66B2F" author="Deeming, J. C. &amp; Al-Dhafer, H. M." box="[568,888,333,359]" pageId="39" pageNumber="40" pagination="1 - 88" refId="ref28013" refString="Deeming, J. C. &amp; Al-Dhafer, H. M. (2012) Chloropidae from the Arabian Peninsula (Diptera: Cyclorrhapha). Zoology in the Middle East. 58, 1 - 88." type="journal article" year="2012">Deeming &amp; Al-Dhafer 2012</bibRefCitation>
).
</paragraph>
