<taxonomicName id="F18D4D20FF89FFCE6DAC7FEE4ED6FD7C" authority="Motschulsky, 1844" authorityName="Motschulsky" authorityYear="1844" box="[672,998,700,727]" class="Insecta" family="Ptiliidae" genus="Ptinella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
