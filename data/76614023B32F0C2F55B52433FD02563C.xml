<taxonomicName id="39C88AB6B32E0C2155D722B5FEA155F5" box="[245,370,476,501]" class="Arachnida" family="Lycosidae" genus="Dzhungarocosa" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="346" phylum="Arthropoda" rank="species" species="omelkoi" status="sp. n.">
