<taxonomicName id="4C4F4D0CFF77FF01F21DFD732C007699" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,520,654,678]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="148" pageNumber="149" phylum="Arthropoda" rank="species" species="bos">
