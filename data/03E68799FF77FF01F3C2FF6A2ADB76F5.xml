<taxonomicName id="4C4F4D0CFF77FF01F01FFD732DC57699" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[842,973,654,678]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="148" pageNumber="149" phylum="Arthropoda" rank="species" species="plurima" status="sp. nov.">
