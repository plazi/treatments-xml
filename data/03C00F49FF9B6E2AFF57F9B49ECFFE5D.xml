<subSubSection id="C373EDD4FF996E2AFF07FF2B997BFF3D" box="[199,1071,151,177]" pageId="18" pageNumber="19" type="etymology">
<paragraph id="8BD6BE5FFF996E2AFF07FF2B997BFF3D" blockId="18.[151,1437,151,465]" box="[199,1071,151,177]" pageId="18" pageNumber="19">
<emphasis id="B91D624DFF996E2AFF07FF2B9C1AFF3D" bold="true" box="[199,334,151,177]" pageId="18" pageNumber="19">Etymology.</emphasis>
