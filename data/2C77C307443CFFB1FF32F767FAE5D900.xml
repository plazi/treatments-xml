<taxonomicName id="63DE0992443CFFB2FCC3F7ADFB2DDDE2" authority="Montagu, 1803" authorityName="Montagu" authorityYear="1803" box="[870,1250,1417,1442]" class="Polychaeta" family="Serpulidae" genus="Serpula" kingdom="Animalia" order="Sabellida" pageId="29" pageNumber="304" phylum="Annelida" rank="species" species="tubularia">
<emphasis id="96AAAE03443CFFB2FCC3F7ADFBE3DDE2" box="[870,1068,1417,1442]" italics="true" pageId="29" pageNumber="304">Serpula tubularia</emphasis>
