<taxonomicName id="9C4A8F5DF3CE7ED819B99315C2E50C3C" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" lsidName="Quercus" order="Fagales" pageId="0" pageNumber="141" phylum="Tracheophyta" rank="genus">
