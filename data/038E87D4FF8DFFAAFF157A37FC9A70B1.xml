<typeStatus id="549C8860FF8DFFA9FB0F790FFB577603" box="[1165,1263,1611,1637]" pageId="12" pageNumber="370">holotype</typeStatus>
of 
<taxonomicName id="4C274D41FF8DFFA9FA917908FA2C7603" authorityName="Makhan" authorityYear="1992" box="[1299,1428,1611,1637]" class="Insecta" family="Hydrochidae" genus="Hydrochus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="370" phylum="Arthropoda" rank="species" species="choennii">
<emphasis id="B953EAD0FF8DFFA9FA917908FA2C7603" box="[1299,1428,1611,1637]" italics="true" pageId="12" pageNumber="370">H. choennii</emphasis>
</taxonomicName>
: “Coll. R. I. Sc. N. B. // 
<collectingCountry id="F3307652FF8DFFA9FE1A792BFDBB76EF" box="[408,515,1647,1673]" name="Suriname" pageId="12" pageNumber="370">Suriname</collectingCountry>
