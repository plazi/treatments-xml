<typeStatus id="549C8860FF8DFFA9FF74790FFEE07603" box="[246,344,1611,1637]" pageId="12" pageNumber="370">holotype</typeStatus>
of 
<taxonomicName id="4C274D41FF8DFFA9FEF97908FD187603" authority="Makhan 1992" authorityName="Makhan" authorityYear="1992" box="[379,672,1611,1637]" class="Insecta" family="Hydrochidae" genus="Hydrochus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="370" phylum="Arthropoda" rank="species" species="choennii">
