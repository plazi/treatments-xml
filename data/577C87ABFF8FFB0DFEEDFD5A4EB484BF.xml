(n = 
<number id="691D249AFF8FFB0CFE31F9C14CAC8390" box="[468,481,1639,1665]" pageId="6" pageNumber="55" value="1.0">1</number>
) 
<number id="691D249AFF8FFB0CFE1EF9C14F6A8390" box="[507,551,1639,1665]" pageId="6" pageNumber="55" value="160.0">160</number>
long, 
<number id="691D249AFF8FFB0CFD9AF9C14FD18390" box="[639,668,1639,1665]" pageId="6" pageNumber="55" value="60.0">60</number>
wide; genitalia 
<number id="691D249AFF8FFB0CFC89F9C14E368390" box="[876,891,1639,1665]" pageId="6" pageNumber="55" value="4.0">4</number>
long, 
<number id="691D249AFF8FFB0CFC36F9C14EBD8390" box="[979,1008,1639,1665]" pageId="6" pageNumber="55" value="24.0">24</number>
wide, proximal setae on coxisternum III (
<emphasis id="EDA1EAAFFF8FFB0CFE2AF9294CA183B9" box="[463,492,1679,1704]" italics="true" pageId="6" pageNumber="55">
