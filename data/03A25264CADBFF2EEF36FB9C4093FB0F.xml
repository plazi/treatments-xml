<emphasis id="B97F3F60CADBFF28EC95FB02424DFAB8" box="[695,840,1263,1290]" italics="true" pageId="194" pageNumber="639">
<taxonomicName id="4C0B98F1CADBFF28EC95FB02424DFAB8" box="[695,840,1263,1290]" class="Insecta" family="Libellulidae" genus="Orthetrum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="194" pageNumber="639" phylum="Arthropoda" rank="species" species="agaricum" status="sp. nov.">O. agaricum</taxonomicName>
