<taxonomicName id="4C5CE69BFF80AB7FFE1EFBF5FDB9FBD3" authorityName="Hutchinson &amp; Allsopp" authorityYear="2020" box="[411,564,1032,1058]" class="Insecta" family="Dynastidae" genus="Cavonus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="459" phylum="Arthropoda" rank="species" species="fovealacus">
