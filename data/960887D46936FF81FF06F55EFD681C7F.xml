(
<emphasis id="2CD5EAD06934FF80FE70F53AFDF618E4" bold="true" box="[481,613,1909,1932]" italics="true" pageId="6" pageNumber="408">Eutanycerus</emphasis>
) 
<emphasis id="2CD5EAD06934FF80FDE5F53BFC8918E5" bold="true" box="[628,794,1908,1933]" italics="true" pageId="6" pageNumber="408">quangbinhensis</emphasis>
Long
</taxonomicName>
, 
<taxonomicNameLabel id="37E657AB6934FF80FCF7F53AFC0C18E5" box="[870,927,1909,1933]" pageId="6" pageNumber="408" rank="species">sp. n.</taxonomicNameLabel>
