<taxonomicName id="7CC5D5FFD74FE73A4E6FFC60FA8FFC01" box="[1070,1283,951,976]" class="Anthozoa" family="Agariciidae" genus="Agaricia" kingdom="Animalia" order="Scleractinia" pageId="19" pageNumber="420" phylum="Cnidaria" rank="species" species="tenuifolia">
