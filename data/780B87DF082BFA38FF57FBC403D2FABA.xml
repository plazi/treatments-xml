<taxonomicName id="37A24D4A082BFA38FEF3FBF30514FBC1" authority="FROST, 2001" authorityName="FROST" authorityYear="2001" box="[330,603,1073,1096]" class="Mammalia" family="Cercopithecidae" genus="Kuseracolobus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="aramisi" status="new species">Kuseracolobus aramisi</taxonomicName>
