<taxonomicName id="523E0A551E69A0A7E67DD7294E6E83CF" class="Insecta" family="Chrysomelidae" genus="Leptispa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptispa" order="Coleoptera" pageId="221" pageNumber="222" phylum="Arthropoda" rank="genus">Leptispa</taxonomicName>
