<normalizedToken id="784123A7F2A6894326C32F55740F2417" originalValue="Würmli">Wuermli</normalizedToken>
(1975: 18, as 
<taxonomicName id="94A28104C2DD786F841896FEAFBD53BD" lsidName="" pageId="221" pageNumber="222" rank="tribe" tribe="Leptispini">Leptispini</taxonomicName>
