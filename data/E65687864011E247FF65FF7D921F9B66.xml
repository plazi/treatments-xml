<taxonomicName id="A9FF4D134011E247FC15FE10921F9888" authorityName="A. C. Ley" authorityYear="2012" box="[911,1133,437,461]" class="Liliopsida" family="Marantaceae" genus="Thaumatococcus" kingdom="Plantae" order="Zingiberales" pageId="13" pageNumber="50" phylum="Tracheophyta" rank="species" species="flavus">
<emphasis id="5C8BEA824011E247FC15FE10921F9888" box="[911,1133,437,461]" italics="true" pageId="13" pageNumber="50">Thaumatococcus flavus</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="6E4036904011E247FF27FE77921F9B66" blockId="13.[189,1135,379,547]" pageId="13" pageNumber="50">
<keyLead id="D50E95A54011E247FF27FE77921F9B66" pageId="13" pageNumber="50">
