<taxonomicName id="202242BA460FD3C448EC044626728800" LSID="http://zoobank.org/BCDCFB42-0882-471F-AB0E-7C53B7FC1CE1" class="Arachnida" family="Salticidae" genus="Jotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Jotus albimanus" order="Araneae" pageId="0" pageNumber="53" phylum="Arthropoda" rank="species" species="albimanus">Jotus albimanus</taxonomicName>
