<taxonomicName id="4C4F4D0CFFC2FFB4F7C0FE682B3B7593" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1173,1331,405,428]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="straatmani" status="sp. nov.">
