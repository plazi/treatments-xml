<collectingDate id="EF895140FFD4FFAC0CA6B223FEFDC616" box="[231,362,311,332]" pageId="29" pageNumber="616" value="1998-08-07" valueMax="1998-08-08" valueMin="1998-08-07">7-8-VIII-1998</collectingDate>
</date>
, 
