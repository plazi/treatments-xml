<taxonomicName id="4C301A83831A3201A490FD246718FD24" authority="Robineau-Desvoidy, 1830" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[410,780,693,718]" class="Insecta" family="Sepsidae" genus="Saltella" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="242" phylum="Arthropoda" rank="genus">
