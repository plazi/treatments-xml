<subSubSection id="53D3108708687720FF05FF6BFE90FF37" pageId="11" pageNumber="12" type="reference_group">
<paragraph id="1B76430C08687720FF05FF6BFC82FF1A" blockId="11.[151,822,190,248]" box="[151,822,190,213]" pageId="11" pageNumber="12">
<treatmentCitationGroup id="3BD9642208687720FF05FF6BFC82FF1A" box="[151,822,190,213]" pageId="11" pageNumber="12">
<emphasis id="29BD9F1E08687720FF05FF6BFF1DFF1A" box="[151,169,190,213]" italics="true" pageId="11" pageNumber="12">=</emphasis>
<treatmentCitation id="9A68651D08687720FF3DFF6BFDA8FF1A" author="Ahti, T." box="[175,540,190,213]" page=")" pageId="11" pageNumber="12" year="1961">
<taxonomicName id="DCC9388F08687720FF3DFF6BFDA8FF1A" authority="Ahti (1961: 46)" authorityName="Ahti" authorityPageNumber="46" authorityYear="1961" box="[175,540,190,213]" class="Lecanoromycetes" family="Cladoniaceae" genus="Cladonia" kingdom="Fungi" order="Lecanorales" pageId="11" pageNumber="12" phylum="Ascomycota" rank="species" species="galapagosensis">
<emphasis id="29BD9F1E08687720FF3DFF6BFEC7FF1A" box="[175,371,190,213]" italics="true" pageId="11" pageNumber="12">C. galapagosensis</emphasis>
<bibRefCitation id="7F583EFD08687720FEE8FF6BFDA8FF1A" author="Ahti, T." box="[378,540,190,213]" pageId="11" pageNumber="12" pagination="1 - 160" refId="ref24323" refString="Ahti, T. (1961) Taxonomic studies on reindeer lichens (Cladonia, subgenus Cladina). Annales Botanici Societatis Zoologicae Botanicae Fennicae ' Vanamo' 32 (1): 1 - 160." type="journal article" year="1961">Ahti (1961:46)</bibRefCitation>
