<quantity id="2A271FE19910FF97EDC798D4DB9EFC43" box="[1242,1279,985,1014]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="35" pageNumber="36" unit="in" value="1.0">1 in</quantity>
