<taxonomicName id="4C6DBEA5FFE2FF88FF161D34FD22FEE6" authority="(Olivier, 1795)" baseAuthorityName="Olivier" baseAuthorityYear="1795" box="[194,659,336,357]" class="Insecta" family="Cerambycidae" genus="Callipogon" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="355" phylum="Arthropoda" rank="species" species="sericeum" subGenus="Callomegas">
