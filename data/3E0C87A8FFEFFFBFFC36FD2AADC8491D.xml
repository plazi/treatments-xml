, 
<location id="B37A6065FFE8FFBFFA99FE06AAE94ABA" LSID="urn:lsid:plazi:treatment:3E0C87A8FFEFFFBFFC36FD2AADC8491D:B37A6065FFE8FFBFFA99FE06AAE94ABA" country="Costa Rica" municipality="Rincon de la Vieja National Park" name="Estacion Las Pailas" pageId="20" pageNumber="317" stateProvince="Guanacaste">Estacion Las Pailas</location>
, 
<quantity id="715D9B5BFFE8FFBFFC62FE65AD404ABB" box="[976,1048,457,478]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="20" pageNumber="317" unit="m" value="800.0">
