<taxonomicName id="2F0B6049CDD2B616AAAF1789816E91FD" authority="Nupponen 1 &amp; Sihvonen, 2022" authorityName="Nupponen 1 &amp; Sihvonen" authorityYear="2022" class="Insecta" family="Scythrididae" genus="Scythris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scythris" order="Lepidoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="genus" status="incertae sedis">Scythris</taxonomicName>
