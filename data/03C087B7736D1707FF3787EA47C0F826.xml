<taxonomicName id="4C694D22736D1707FF3F805F46E4FBD8" authority="Zetterstedt" authorityName="Zetterstedt" box="[159,456,1050,1072]" class="Insecta" family="Anthomyiidae" genus="Delia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="flavifrons">
