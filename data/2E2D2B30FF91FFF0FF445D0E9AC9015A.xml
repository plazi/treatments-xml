<number id="104C8801FF91FFFEFF605EA69E9404D2" box="[179,193,1941,1967]" pageId="23" pageNumber="24" value="2.0">2</number>
<number id="104C8801FF91FFFEFF1B5EA69EBD04D2" box="[200,232,1941,1967]" pageId="23" pageNumber="24" value="30.0">30</number>
, IV-L- 
