<subSubSection id="17AD6561FFFDFFDEFF10FD0CFC8C526D" pageId="27" pageNumber="26" type="etymology">
<paragraph id="5F0836EAFFFDFFDEFF10FD0CFC8C526D" blockId="27.[192,1440,739,795]" pageId="27" pageNumber="26">
<emphasis id="6DC3EAF8FFFDFFDEFF10FD0CFED5538D" bold="true" box="[192,339,739,763]" pageId="27" pageNumber="26">Etymology</emphasis>
. The epithet 
<taxonomicName id="98B74D69FFFDFFDEFE2DFD0BFD1E538D" authorityName="Peck and Cook" authorityYear="2014" box="[509,664,740,763]" class="Insecta" family="Leiodidae" genus="Zeadolopus" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="26" phylum="Arthropoda" rank="species" species="caymanensis">
<emphasis id="6DC3EAF8FFFDFFDEFE2DFD0BFD1E538D" box="[509,664,740,763]" italics="true" pageId="27" pageNumber="26">caymanensis</emphasis>
</taxonomicName>
