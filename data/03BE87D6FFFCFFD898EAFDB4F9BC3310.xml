<geoCoordinate id="EE235007FFFCFFD89B4EFD40F9D732C8" box="[819,918,690,717]" degrees="30" direction="north" minutes="53" orientation="latitude" pageId="109" pageNumber="110" precision="925" value="30.883333">30°53’N</geoCoordinate>
