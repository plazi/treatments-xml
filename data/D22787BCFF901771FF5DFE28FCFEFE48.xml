<taxonomicName id="9D8E4D29FF90177EFDDCFC1EFD1EFC11" authorityName="Wiesner and Constant" authorityYear="2019" box="[577,670,908,931]" class="Insecta" family="Carabidae" genus="Naviauxella" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="varians">
