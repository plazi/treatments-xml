<subSubSection id="12946521FF90177EFF5DFC1EFA8EFC16" box="[192,1294,908,932]" pageId="7" pageNumber="6" type="etymology">
<paragraph id="5A3136AAFF90177EFF5DFC1EFA8EFC16" blockId="7.[192,1294,908,932]" box="[192,1294,908,932]" pageId="7" pageNumber="6">
<emphasis id="68FAEAB8FF90177EFF5DFC1EFEDBFC16" bold="true" box="[192,347,908,932]" pageId="7" pageNumber="6">Etymology.</emphasis>
