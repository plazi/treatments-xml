<taxonomicName id="2B8862CCFFDF4F33E1C00B1EB7C3FAD0" authority="Sowerby" authorityName="Sowerby" box="[230,462,1299,1323]" class="Bivalvia" family="Corbulidae" genus="Corbula" kingdom="Animalia" order="Myida" pageId="48" pageNumber="65" phylum="Mollusca" rank="species" species="costata">
