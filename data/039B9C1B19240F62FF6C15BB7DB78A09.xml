<taxonomicName id="4C32568E19240F62FBE6147B7EDD8A2C" authority="(Savigny, 1826)" authorityName="Savigny" authorityYear="1826" baseAuthorityName="Savigny" baseAuthorityYear="1826" class="Clitellata" family="Lumbricidae" genus="Allolobophora" kingdom="Animalia" order="Crassiclitellata" pageId="2" pageNumber="289" phylum="Annelida" rank="species" species="chlorotica">
