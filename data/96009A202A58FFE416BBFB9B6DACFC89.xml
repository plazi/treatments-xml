Nitsch, 
<number id="A86139112A59FFE41419FE1A6C51FED5" box="[565,624,332,356]" pageId="13" pageNumber="14" value="1818.0">1818</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="D9A950B52A59FFE414A8FE1B6A44FED5" authority="Bochkov and Mertins, 2010" authorityName="Bochkov and Mertins" authorityYear="2010" box="[644,1125,332,357]" class="Arachnida" family="Harpirhynchidae" genus="Harpirhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="quasimodo">
