<taxonomicName id="4C284D0DFFA1B55EFC0F02BAFF733CE0" box="[957,1147,1162,1186]" pageId="36" pageNumber="432" rank="subSpecies" subSpecies="pauciovulata">
ssp. 
<emphasis id="B95CEA9CFFA1B55EFC4202BAFF733CE0" box="[1008,1147,1162,1186]" italics="true" pageId="36" pageNumber="432">pauciovulata</emphasis>
