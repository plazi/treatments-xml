<taxonomicName id="4C0E27CDF8636C366AD1FDE8B92CAC32" box="[478,580,583,608]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="bifurcata">
