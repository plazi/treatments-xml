<taxonomicName id="248B1539D166812E41C73DE33D8ED9F3" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="gerrardii">
