<taxonomicName id="F3E980D589B9678F1D22354F4D7B2792" class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="nummularia">
