<taxonomicName id="936A3139E5AD8897E7F2F3BD16B4508C" authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="44" pageNumber="45" phylum="Angiospermae" rank="species" species="stricta">
