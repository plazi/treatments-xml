<taxonomicName id="8FEF7D8ACB2E0D37FF2558BAFAA189D5" authority="Malm &amp; Johanson, 2008" authorityName="Malm &amp; Johanson" authorityYear="2008" box="[197,488,198,219]" class="Insecta" family="Leptoceridae" genus="Gracilipsodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="451" phylum="Arthropoda" rank="species" species="aureus" status="sp. nov.">GRACILIPSODES AUREUS</taxonomicName>
