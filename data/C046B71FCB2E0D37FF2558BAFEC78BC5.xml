<taxonomicName id="8FEF7D8ACB2E0D37FBAF5AEAFFD58BA4" authorityName="Malm &amp; Johanson" authorityYear="2008" box="[1103,1180,662,683]" class="Insecta" family="Leptoceridae" genus="Gracilipsodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="432" phylum="Arthropoda" rank="species" species="aureus">
