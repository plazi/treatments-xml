<emphasis id="B93BEA9DFF59FF2FF392FE922F4975B7" bold="true" box="[199,321,367,392]" inLineHeading="true" pageId="186" pageNumber="187" reason="1">Diagnosis.</emphasis>
<taxonomicName id="4C4F4D0CFF59FF2FF21CFE8D2C6575B8" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Muir" baseAuthorityYear="1917" box="[329,621,368,392]" class="Insecta" family="Derbidae" genus="Sumangala" kingdom="Animalia" order="Hemiptera" pageId="186" pageNumber="187" phylum="Arthropoda" rank="species" species="nigropunctata">
