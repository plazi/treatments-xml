<collectionCode id="ED5E00B2853CB769DA792E22F2196218" box="[816,873,1616,1640]" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="6" pageNumber="119">ZSM</collectionCode>
