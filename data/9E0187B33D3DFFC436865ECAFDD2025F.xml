<taxonomicName id="D1A84D263D3DFFC7372C5992FDA40D92" authority="Men &amp; Dong &amp; Yue &amp; Cao &amp; Xu, 2018" authorityName="Men &amp; Dong &amp; Yue &amp; Cao &amp; Xu" authorityYear="2018" box="[257,591,2021,2040]" class="Insecta" family="Tipulidae" genus="Dolichopeza" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="598" phylum="Arthropoda" rank="species" species="setilobatoides" status="sp. nov." subGenus="Nesopeza">
<emphasis id="24DCEAB73D3DFFC7372C5992FE860D92" box="[257,365,2021,2040]" italics="true" pageId="9" pageNumber="598">Dolichopeza</emphasis>
(
<emphasis id="24DCEAB73D3DFFC737575992FE270D92" box="[378,460,2021,2040]" italics="true" pageId="9" pageNumber="598">Nesopeza</emphasis>
) 
<emphasis id="24DCEAB73D3DFFC737F55992FDA40D92" box="[472,591,2021,2040]" italics="true" pageId="9" pageNumber="598">setilobatoides</emphasis>
