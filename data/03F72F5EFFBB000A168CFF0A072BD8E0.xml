<taxonomicName id="4C5EE5CBFFBB000A1533FE780491D8A2" authority="BRULLE 1832" authorityName="BRULLE" authorityYear="1832" class="Insecta" family="Cicadellidae" genus="Euscelis" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2152" phylum="Arthropoda" rank="species" species="lineolatus">
