<taxonomicName id="4C5EE5CBFFBB000A1501FE3F0493D863" authority="LINNAEUS 1758" authorityName="LINNAEUS" authorityYear="1758" class="Insecta" family="Cicadidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2152" phylum="Arthropoda" rank="species" species="orni">
