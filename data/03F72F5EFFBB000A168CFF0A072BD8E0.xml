<taxonomicName id="4C5EE5CBFFBB000A15F4FE1E05A8D844" authority="SIGNORET 1865" authorityName="SIGNORET" authorityYear="1865" class="Insecta" family="Cixiidae" genus="Hyalesthes" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2152" phylum="Arthropoda" rank="species" species="obsoletus">
<emphasis id="B92A425AFFBB000A15F4FE1E056ED844" italics="true" pageId="3" pageNumber="2152">Hyalesthes obsoletus</emphasis>
