(KIRSCHBAUM 1868)
</taxonomicName>
, 
<taxonomicName id="4C5EE5CBFFBB000A17C2FFFD0738D806" authority="LINNAEUS 1758" authorityName="LINNAEUS" authorityYear="1758" box="[508,833,1278,1302]" class="Insecta" family="Cicadidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2152" phylum="Arthropoda" rank="species" species="orni">
