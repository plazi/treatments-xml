<taxonomicName id="4C5EE5CBFFBB000A15BCFFFD0527D825" authority="(SCOPOLI 1763)" baseAuthorityName="SCOPOLI" baseAuthorityYear="1763" class="Insecta" family="Cicadidae" genus="Lyristes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2152" phylum="Arthropoda" rank="species" species="plebejus">
