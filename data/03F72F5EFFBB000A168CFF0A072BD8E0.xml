<taxonomicName id="4C5EE5CBFFBB000A16C6FE58061BD863" authority="(LINDBERG 1936)" baseAuthorityName="LINDBERG" baseAuthorityYear="1936" box="[248,610,1371,1395]" class="Insecta" family="Cicadellidae" genus="Hecalus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2152" phylum="Arthropoda" rank="species" species="storai">
