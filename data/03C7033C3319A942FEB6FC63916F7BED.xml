' N
</geoCoordinate>
, 
<geoCoordinate id="EE5AD4ED3319A94DFE15FBFB97597941" box="[427,560,1076,1102]" direction="west" orientation="longitude" pageId="7" pageNumber="8" precision="925" value="-83.46667">
