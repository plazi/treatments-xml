<taxonomicName id="A6B14D147E57FF9DFE05FA50FC93F9EB" authority="Fabricius, 1801" authorityName="Fabricius" authorityYear="1801" box="[452,808,1523,1544]" class="Insecta" family="Lycidae" genus="Lycus" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="146" phylum="Arthropoda" rank="species" species="marginellus">
<emphasis id="53C5EA857E57FF9DFE05FA50FDC0F9EB" box="[452,635,1523,1544]" italics="true" pageId="0" pageNumber="146">Lycus marginellus</emphasis>
