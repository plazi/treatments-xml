(VL 
<number id="3DCD249B405AFFC1E70AB1C0FBA25967" box="[1068,1115,749,775]" pageId="22" pageNumber="23" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
, VM 
<number id="3DCD249B405AFFC1E7BBB1C0FB545967" box="[1181,1197,749,775]" pageId="22" pageNumber="23" value="3.0">3</number>
, DM 
<number id="3DCD249B405AFFC1E7D7B1C0FAF85967" box="[1265,1281,749,775]" pageId="22" pageNumber="23" value="3.0">3</number>
