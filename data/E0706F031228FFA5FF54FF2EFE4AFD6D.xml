<taxonomicName id="AFD9A5961228FFA5FAF9FD9DFE4EFD6D" authority="(Gaglini, 1992)" authorityName="Gaglini" authorityYear="1992" baseAuthorityName="Gaglini" baseAuthorityYear="1992" class="Bivalvia" family="Thyasiridae" genus="Axinulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="82" pageNumber="83" phylum="Mollusca" rank="species" species="exintermedius">
<emphasis id="5AAD02071228FFA5FAF9FD9DFE91FD6D" italics="true" pageId="82" pageNumber="83">Axinulus exintermedius</emphasis>
