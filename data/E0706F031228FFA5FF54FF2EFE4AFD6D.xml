<taxonomicName id="AFD9A5961228FFA5FF01FD9CFD38FD41" authority="Gaglini, 1992" authorityName="Gaglini" authorityYear="1992" box="[194,669,620,645]" class="Bivalvia" family="Thyasiridae" genus="Leptaxinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="82" pageNumber="83" phylum="Mollusca" rank="species" species="exintermedius">
