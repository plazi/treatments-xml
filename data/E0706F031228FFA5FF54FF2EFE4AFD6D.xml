<taxonomicName id="AFD9A5961228FFA5FCF3FDB9FAA4FDA5" authority="van den Hecke, 1858" authorityName="van den Hecke" authorityYear="1858" box="[816,1281,583,608]" class="Bivalvia" family="Thyasiridae" genus="Axinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="82" pageNumber="83" phylum="Mollusca" rank="species" species="intermedius">
