<paragraph id="9A079958CDB7CB511945E7E6C68EDF9A" pageId="0" pageNumber="77">
<taxonomicName id="88FCFB9BFF7E67003F47B1DF84BAC8A3" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Aleiodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleiodes rocioecheverri" order="Hymenoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="rocioecheverri">
<emphasis id="6B11D34A29798F076B963548BEC8644C" italics="true" pageId="0" pageNumber="77">Aleiodes rocioecheverri</emphasis>
</taxonomicName>
