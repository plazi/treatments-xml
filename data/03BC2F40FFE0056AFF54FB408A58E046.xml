<taxonomicName id="4C15E5D5FFE1056AFAC8FD9B8CE7E066" box="[1291,1436,554,577]" class="Monogenea" family="Gyrodactylidae" genus="Gyrodactylus" kingdom="Animalia" order="Gyrodactylidea" pageId="21" pageNumber="22" phylum="Platyhelminthes" rank="genus">Gyrodactylus</taxonomicName>
