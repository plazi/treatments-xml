<quantity id="4CEE9B2E3C04A805FD9C2FA9113CFA60" box="[628,777,1427,1454]" metricMagnitude="-1" metricUnit="m" metricValue="1.77" metricValueMax="1.92" metricValueMin="1.6199999999999999" pageId="13" pageNumber="144" unit="mm" value="177.0" valueMax="192.0" valueMin="162.0">162–192 mm</quantity>
