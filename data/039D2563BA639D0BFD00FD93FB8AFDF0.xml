<taxonomicName id="4C34EFF6BA629D0BFE45FC7AFD8AFC09" authorityName="Vigors" authorityYear="1825" box="[426,565,938,961]" class="Aves" family="Muscicapidae" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="24" phylum="Chordata" rank="subFamily" subFamily="Saxicolinae">Saxicolinae</taxonomicName>
