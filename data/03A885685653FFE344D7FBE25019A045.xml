<taxonomicName id="4C014FFD5653FFE34419FADF5100A044" box="[354,539,1366,1389]" class="Liliopsida" family="Araceae" genus="Arum" kingdom="Plantae" order="Alismatales" pageId="13" pageNumber="518" phylum="Tracheophyta" rank="species" species="maculatum">
