<caption id="DF6EB0A86642FFA2FF6C129DE481EA92" ID-DOI="http://doi.org/10.5281/zenodo.4668385" ID-Zenodo-Dep="4668385" httpUri="https://zenodo.org/record/4668385/files/figure.png" pageId="7" pageNumber="518" startId="7.[151,250,1964,1988]" targetBox="[151,1437,1497,1941]" targetPageId="7">
<paragraph id="8BAEE0206642FFA2FF6C129DE481EA92" blockId="7.[151,1436,1964,2025]" pageId="7" pageNumber="518">
