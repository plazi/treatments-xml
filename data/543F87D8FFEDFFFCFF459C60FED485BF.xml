<taxonomicName id="1B964D4DFFEFFFFCFB3199E2FAC4849F" box="[1251,1389,188,212]" class="Insecta" family="Noctuidae" genus="Helicoverpa" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="439" phylum="Arthropoda" rank="genus">
