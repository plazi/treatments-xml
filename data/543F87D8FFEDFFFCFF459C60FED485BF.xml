) and 
<taxonomicName id="1B964D4DFFEFFFFCFBE699C6FB1684FB" box="[1076,1215,152,176]" class="Insecta" family="Noctuidae" genus="Helicoverpa" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="439" phylum="Arthropoda" rank="genus">
<emphasis id="EEE2EADCFFEFFFFCFBE699C6FB1684FB" box="[1076,1215,152,176]" italics="true" pageId="10" pageNumber="439">Helicoverpa</emphasis>
