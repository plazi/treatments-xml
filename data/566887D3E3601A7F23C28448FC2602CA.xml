, 
<taxonomicName id="19C14D46E3611A7F23AD83F3FEA404E5" authorityName="Wieser" authorityYear="1959" box="[248,375,944,968]" class="Chromadorea" family="Xyalidae" genus="Cobbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monhysterida" pageId="3" pageNumber="182" phylum="Nematoda" rank="species" species="truncata">
<emphasis id="ECB5EAD7E3611A7F23AD83F3FEA404E5" box="[248,375,944,968]" italics="true" pageId="3" pageNumber="182">C. truncata</emphasis>
</taxonomicName>
and 
<taxonomicName id="19C14D46E3611A7F22F983F3FDDC04EA" authorityName="Huang &amp; Zhang" authorityYear="2010" box="[428,527,944,967]" class="Chromadorea" family="Xyalidae" genus="Cobbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monhysterida" pageId="3" pageNumber="182" phylum="Nematoda" rank="species" species="sinica">
