<taxonomicName id="7D4E667F45FEB2E0A7CF82F27F9DDC8E" authority="Choi, Chandler &amp; Park, 2019" authorityName="Choi, Chandler &amp; Park" authorityYear="2019" class="Insecta" family="Staphylinidae" genus="Porongurup" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Porongurup" order="Coleoptera" pageId="4" pageNumber="17" phylum="Arthropoda" rank="genus">
<emphasis id="A8632AD57F5BC274430325E672BF3E44" italics="true" pageId="4" pageNumber="17">Porongurup</emphasis>
</taxonomicName>
gen. nov. Key: 
<taxonomicName id="9B94BABF23B56CD4D63969326FFF9753" genus="P." lsidName="P. angulatus" pageId="4" pageNumber="17" rank="species" species="angulatus">
<emphasis id="7BFE2FC8251A204F122D7D9B65A9BC66" italics="true" pageId="4" pageNumber="17">P. angulatus</emphasis>
</taxonomicName>
sp. nov., circles; 
<taxonomicName id="AA2EB85B1837630C1BC7EB076F5B2024" genus="P." lsidName="P. clarkei" pageId="4" pageNumber="17" rank="species" species="clarkei">
