<taxonomicName id="91EE09C4FFE8C148FEC0D9CFFDCF7CFE" box="[304,569,1938,1962]" class="Insecta" family="Cicadellidae" genus="Tenucephalus" kingdom="Animalia" order="Hemiptera" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="marginellus">
