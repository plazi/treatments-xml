<quantity id="4CC89B356552FF8C7719C50FFD1FFCC2" box="[544,652,844,870]" metricMagnitude="-3" metricUnit="m" metricValue="5.25" pageId="21" pageNumber="22" unit="mm" value="5.25">5.25 mm</quantity>
