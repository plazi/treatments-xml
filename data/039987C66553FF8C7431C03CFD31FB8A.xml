, PL 
<quantity id="4CC89B356552FF8C7640C50FFC75FCC2" box="[889,998,844,870]" metricMagnitude="-4" metricUnit="m" metricValue="6.6" pageId="21" pageNumber="22" unit="mm" value="0.66">0.66 mm</quantity>
, PW 
<quantity id="4CC89B356552FF8C7111C50FFB06FCC2" box="[1064,1173,844,870]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" pageId="21" pageNumber="22" unit="mm" value="1.15">1.15 mm</quantity>
, Ediam 
<quantity id="4CC89B356552FF8C71C1C50FFEABFC2A" metricMagnitude="-4" metricUnit="m" metricValue="4.4" pageId="21" pageNumber="22" unit="mm" value="0.44">0.44 mm</quantity>
