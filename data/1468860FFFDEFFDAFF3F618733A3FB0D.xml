<subSubSection id="D4DB6492FFDEFFDAFF3F607E3398FD30" box="[151,997,292,317]" pageId="1" pageNumber="118" type="materials_examined">
<paragraph id="9C7E3719FFDEFFDAFF3F607E3398FD30" blockId="1.[151,1437,292,1792]" box="[151,997,292,317]" pageId="1" pageNumber="118">
<emphasis id="AEB5EB0BFFDEFFDAFF3F607E314FFD30" bold="true" box="[151,306,292,317]" pageId="1" pageNumber="118">
<typeStatus id="437A89BBFFDEFFDAFF3F607E30AFFD30" box="[151,210,292,317]" pageId="1" pageNumber="118">Type</typeStatus>
