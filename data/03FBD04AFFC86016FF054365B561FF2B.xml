<treatmentCitationGroup id="AB424672FFC86017FF0543F0B4B8FA44" box="[151,903,1410,1432]" pageId="12" pageNumber="13">
<taxonomicName id="4C521ADFFFC86017FF0543F0B57AFA44" authority="Bomans, 1989: 16" authorityName="Bomans" authorityPageNumber="16" authorityYear="1989" box="[151,581,1410,1432]" class="Insecta" family="Lucanidae" genus="Prismognathus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="sinensis">
<emphasis id="B926BD4EFFC86017FF0543F0B6B9FA44" box="[151,390,1410,1432]" italics="true" pageId="12" pageNumber="13">Prismognathus sinensis</emphasis>
<treatmentCitation id="0AF3474DFFC86017FE1F43F1B57AFA44" author="Bomans" box="[397,581,1410,1432]" page="16" pageId="12" pageNumber="13" year="1989">
<bibRefCitation id="EFC31CADFFC86017FE1F43F1B51DFA44" author="Bomans" box="[397,546,1411,1432]" pageId="12" pageNumber="13" refString="Bomans, H. E. (1989) Inventaire d'une collection de lucanides recoltes en Chine continentale, avec descriptions d'especes nouvelles (Coleoptera, Lucanidae). Nouvelle Revue d'Entomologie (Nouvelle Serie), 6 (1), 3 - 23." type="journal article" year="1989">
