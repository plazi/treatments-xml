</taxonomicName>
and 
<taxonomicName id="BEA84D20F1513267C162CC2A9D981E4C" box="[1110,1229,304,330]" class="Liliopsida" family="Orchidaceae" genus="Koellensteinia" kingdom="Plantae" order="Asparagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="tricolor">
