<subSubSection id="31B26528F1513267C589CC2B9AB81E68" pageId="20" pageNumber="21" type="diagnosis">
<paragraph id="791736A3F1513267C589CC2B9AB81E68" blockId="20.[136,1452,160,366]" pageId="20" pageNumber="21">
<emphasis id="4BDCEAB1F1513267C589CC2B98B01E4D" bold="true" box="[189,485,304,331]" pageId="20" pageNumber="21">Diagnostic characters:—</emphasis>
<taxonomicName id="BEA84D20F1513267C4D2CC2B9A5A1E4C" box="[486,783,304,330]" class="Liliopsida" family="Orchidaceae" genus="Koellensteinia" kingdom="Plantae" order="Asparagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="kellneriana">
<emphasis id="4BDCEAB1F1513267C4D2CC2B9A5A1E4C" box="[486,783,304,330]" italics="true" pageId="20" pageNumber="21">Koellensteinia kellneriana</emphasis>
</taxonomicName>
differs from 
<taxonomicName id="BEA84D20F1513267C697CC2A9D4A1E4C" box="[931,1055,304,330]" class="Liliopsida" family="Orchidaceae" genus="Koellensteinia" kingdom="Plantae" order="Asparagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="eburnea">
