<quantity id="4CE24F9BB65B0235FB75FDFEFACDB13D" box="[1242,1406,607,631]" metricMagnitude="-2" metricUnit="m" metricValue="1.955" metricValueMax="2.05" metricValueMin="1.86" pageId="9" pageNumber="10" unit="mm" value="19.55" valueMax="20.5" valueMin="18.6">18.6–20.5 mm</quantity>
