<quantity id="4CE24F9BB65B0235FC39FDFEFB88B13D" box="[918,1083,607,631]" metricMagnitude="-2" metricUnit="m" metricValue="1.575" metricValueMax="1.7899999999999998" metricValueMin="1.3599999999999999" pageId="9" pageNumber="10" unit="mm" value="15.75" valueMax="17.9" valueMin="13.6">13.6–17.9 mm</quantity>
