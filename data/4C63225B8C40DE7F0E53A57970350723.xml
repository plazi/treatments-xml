<taxonomicName id="D73B3F652B468EE68BF8F0E69EE53D32" class="Magnoliopsida" family="Sapindaceae" genus="Acer" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acer saccharinum" order="Sapindales" pageId="3" pageNumber="260" phylum="Tracheophyta" rank="species" species="saccharinum">Acer saccharinum</taxonomicName>
