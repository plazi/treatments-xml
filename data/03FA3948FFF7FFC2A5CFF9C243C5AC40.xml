<taxonomicName id="4C53F3DDFFF7FFC0A409F9C243DCAB50" authorityName="Herrich-Schaffer" authorityYear="1855" box="[337,472,1593,1620]" class="Insecta" family="Hepialidae" genus="Abantiades" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="116" phylum="Arthropoda" rank="genus">
