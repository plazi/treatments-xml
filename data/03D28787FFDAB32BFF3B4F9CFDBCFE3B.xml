<location id="8EA4604AFFDAB32AFBDA4FD0FB23F935" LSID="urn:lsid:plazi:treatment:03D28787FFDAB32BFF3B4F9CFDBCFE3B:8EA4604AFFDAB32AFBDA4FD0FB23F935" box="[1142,1266,1768,1790]" country="India" name="Nilgiri Hills" pageId="56" pageNumber="57">Nilgiri Hills</location>
