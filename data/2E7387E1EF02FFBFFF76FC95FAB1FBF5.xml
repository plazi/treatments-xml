<taxonomicName id="61DA4D74EF02FFBFFC20FBE1FB6FFBF4" box="[961,1155,1100,1125]" class="Amphibia" family="Phrynobatrachidae" genus="Phrynobatrachus" kingdom="Animalia" order="Anura" pageId="8" phylum="Chordata" rank="genus">
