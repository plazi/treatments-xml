<emphasis id="B94CEAC3FF2A9841CDB34AD1FF31FD26" box="[160,225,643,668]" italics="true" pageId="6" pageNumber="1605">Thrips</emphasis>
<treatmentCitation id="0A9910C0FF2A9841CDFA4AD1FD7BFD26" author="Haliday AH" box="[233,683,643,668]" page="445" pageId="6" pageNumber="1605" year="1836">
