<paragraph id="7772EABF2B9DC5445010113E2494E6FD" pageId="21" pageNumber="22">
<taxonomicName id="13110B9E240B1DAED93D446E9B58493F" authorityName="Rao" authorityYear="1922" class="Clitellata" family="Almidae" genus="Glyphidrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Haplotaxida" pageId="21" pageNumber="22" phylum="Annelida" rank="species" species="elegans">Glyphidrilus elegans</taxonomicName>
