<emphasis id="B909DD00FFEAD9002B21FC9CFEB2FC83" bold="true" box="[336,358,828,846]" pageId="4" pageNumber="155">Vc</emphasis>
, ventral cirrus; 
<emphasis id="B909DD00FFEAD9002B99FC9CFDC7FC83" bold="true" box="[488,531,828,846]" pageId="4" pageNumber="155">VeLa</emphasis>
, ventral lamella. 
<emphasis id="B909DD00FFEAD90028D0FC9CFD7AFC83" bold="true" box="[673,686,828,846]" pageId="4" pageNumber="155">A</emphasis>
, 
<emphasis id="B909DD00FFEAD90028C9FC9CFD12FC83" bold="true" box="[696,710,828,846]" pageId="4" pageNumber="155">D</emphasis>
, 
<emphasis id="B909DD00FFEAD90028A0FC9CFD09FC83" bold="true" box="[721,733,828,846]" pageId="4" pageNumber="155">E</emphasis>
