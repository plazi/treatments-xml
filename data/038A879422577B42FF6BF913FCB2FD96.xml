<quantity id="4CDB9B6722547B45FC2BFBCEFB8CFB93" box="[983,1075,1040,1066]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="19" pageNumber="20" unit="mm" value="1.6">1.6 mm</quantity>
