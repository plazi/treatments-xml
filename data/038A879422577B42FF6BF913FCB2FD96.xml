<quantity id="4CDB9B6722547B45FD01FBFEFC99FB93" box="[765,806,1040,1070]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="19" pageNumber="20" unit="in" value="1.0">1 in</quantity>
