<taxonomicName authority="Sallé, 1855" authorityName="Sallé" authorityYear="1855" class="Insecta" family="Elateridae" genus="Eudactylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eudactylus" order="Coleoptera" pageId="149" pageNumber="150" phylum="Arthropoda" rank="genus">Eudactylus</taxonomicName>
