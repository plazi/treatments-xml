<emphasis bold="true" box="[1033,1440,1059,1083]" pageId="6" pageNumber="5">
<emphasis bold="true" box="[1033,1061,1059,1082]" italics="true" pageId="6" pageNumber="5">E.</emphasis>
(
<emphasis bold="true" box="[1080,1109,1059,1082]" italics="true" pageId="6" pageNumber="5">E.</emphasis>
) 
<emphasis bold="true" box="[1128,1254,1059,1083]" italics="true" pageId="6" pageNumber="5">fuliginea</emphasis>
