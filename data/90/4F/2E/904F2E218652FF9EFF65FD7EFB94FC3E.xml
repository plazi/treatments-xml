<paragraph blockId="15.[151,1437,654,1005]" pageId="15" pageNumber="73">
<emphasis bold="true" box="[151,207,835,861]" pageId="15" pageNumber="73">Note</emphasis>
. Names 
<taxonomicName authorityName="Obenberger" authorityYear="1935" box="[303,434,835,861]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="73" phylum="Arthropoda" rank="species" species="achilleus">
