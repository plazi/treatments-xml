</paragraph>
<paragraph pageId="0" pageNumber="1168">
<taxonomicName genus="Tenthredo" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tenthredo flaveola" pageId="0" pageNumber="1168" rank="species" species="flaveola">Tenthredo flaveola</taxonomicName>
