<paragraph blockId="27.[701,1228,211,1759]" pageId="27" pageNumber="27">
CONTENTS: 
<taxonomicName box="[873,1130,212,235]" class="Mammalia" family="Muridae" genus="Bunomys" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="chrysocomus">
<emphasis box="[873,1130,212,235]" italics="true" pageId="27" pageNumber="27">Bunomys chrysocomus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Thomas" authorityYear="1896" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="coelestis">
<emphasis italics="true" pageId="27" pageNumber="27">B. coelestis</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Musser" authorityYear="1991" box="[785,922,241,264]" class="Mammalia" family="Muridae" genus="Bunomys" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="prolatus">
<emphasis box="[785,922,241,264]" italics="true" pageId="27" pageNumber="27">B. prolatus</emphasis>
</taxonomicName>
, 
<emphasis box="[949,1081,243,264]" italics="true" pageId="27" pageNumber="27">
<taxonomicName authority="Musser, 2014" authorityName="Musser" authorityYear="2014" box="[949,1077,243,264]" class="Mammalia" family="Muridae" genus="Bunomys" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="torajae" status="n. sp.">B. torajae</taxonomicName>
,
</emphasis>
<taxonomicNameLabel box="[1100,1177,242,263]" pageId="27" pageNumber="27" rank="species">n. sp.</taxonomicNameLabel>
, 
<taxonomicName class="Mammalia" family="Muridae" genus="Bunomys" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="fratrorum">
<emphasis italics="true" pageId="27" pageNumber="27">B. fratrorum</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Allen" authorityYear="1911" box="[836,976,271,294]" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="andrewsi">
<emphasis box="[836,976,271,294]" italics="true" pageId="27" pageNumber="27">B. andrewsi</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName=", Sody" authorityYear="1941" box="[999,1120,272,293]" class="Mammalia" family="Muridae" genus="Rattus" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="penitus">
<emphasis box="[999,1120,272,293]" italics="true" pageId="27" pageNumber="27">B. penitus</emphasis>
</taxonomicName>
, and 
<emphasis italics="true" pageId="27" pageNumber="27">
<taxonomicName authority="Musser, 2014" authorityName="Musser" authorityYear="2014" class="Mammalia" family="Muridae" genus="Bunomys" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="karokophilus" status="n. sp.">B. karokophilus</taxonomicName>
,
</emphasis>
<taxonomicNameLabel box="[862,921,301,322]" pageId="27" pageNumber="27" rank="species">n. sp.</taxonomicNameLabel>
; all are endemic to 
