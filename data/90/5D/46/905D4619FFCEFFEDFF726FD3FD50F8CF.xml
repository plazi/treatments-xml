<taxonomicName authorityName="Li &amp; Metzeltin" authorityYear="2013" box="[537,672,309,334]" class="Bacillariophyceae" family="Cymbellaceae" genus="Cymbella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Cymbellales" pageId="4" pageNumber="33" phylum="Ochrophyta" rank="species" species="yangtzensis">
