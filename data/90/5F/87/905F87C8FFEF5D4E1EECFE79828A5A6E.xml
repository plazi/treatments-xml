× 
<quantity box="[845,928,1940,1962]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="2" pageNumber="581" unit="mm" value="3.7">3.7 mm</quantity>
, 
<accessionNumber box="[940,1066,1940,1962]" httpUri="http://www.uniprot.org/uniprot/SMF47040" pageId="2" pageNumber="581">
<collectionCode LSID="urn:lsid:biocol.org:col:34838" box="[940,991,1940,1962]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="2" pageNumber="581">SMF</collectionCode>
<number box="[999,1066,1940,1962]" pageId="2" pageNumber="581" value="47040.0">47040</number>
