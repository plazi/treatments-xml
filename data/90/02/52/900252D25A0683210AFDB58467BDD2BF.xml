<taxonomicName class="Insecta" family="Histeridae" genus="Reichardtiolus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Reichardtiolus" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">Reichardtiolus</taxonomicName>
