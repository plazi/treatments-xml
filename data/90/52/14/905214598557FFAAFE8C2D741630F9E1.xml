<subSubSection pageId="6" pageNumber="7" type="description">
<paragraph blockId="6.[269,1315,444,1654]" box="[269,860,444,467]" pageId="6" pageNumber="7">Species Flight Status Geographic Range</paragraph>
<paragraph blockId="6.[269,1315,444,1654]" box="[269,804,486,510]" pageId="6" pageNumber="7">
<taxonomicName box="[269,393,487,510]" class="Aves" family="Rallidae" genus="Gallirallus" kingdom="Animalia" order="Gruiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="australis">
<emphasis box="[269,393,487,510]" italics="true" pageId="6" pageNumber="7">G. australis</emphasis>
</taxonomicName>
F E 
<collectingCountry box="[661,804,486,509]" name="New Zealand" pageId="6" pageNumber="7">New Zealand</collectingCountry>
</paragraph>
