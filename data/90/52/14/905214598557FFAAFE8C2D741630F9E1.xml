<taxonomicName box="[264,383,319,342]" class="Aves" family="Rallidae" genus="Gallirallus" kingdom="Animalia" order="Gruiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">Gallirallus</taxonomicName>
,
</emphasis>
