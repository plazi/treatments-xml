<specimenCount box="[808,936,486,507]" pageId="7" pageNumber="706" type="generic">21 specimens</specimenCount>
(
<specimenCount box="[949,1025,486,507]" pageId="7" pageNumber="706" type="male">10 male</specimenCount>
and 
<specimenCount box="[1071,1165,486,507]" pageId="7" pageNumber="706" type="female">11 female</specimenCount>
) collected from the 
<location LSID="urn:lsid:plazi:treatment:902587FBFFF49213FF2D6A2A8F8F179E:1D536036FFF69213FCD36E2A8F911711" box="[870,961,512,533]" country="Turkey" county="September" municipality="June" name="Piedmont" pageId="7" pageNumber="706">Piedmont</location>
(
<emphasis box="[977,989,513,534]" italics="true" pageId="7" pageNumber="706">n</emphasis>
= 20) and 
<location LSID="urn:lsid:plazi:treatment:902587FBFFF49213FF2D6A2A8F8F179E:1D536036FFF69213FBF86E2A8E8E1734" country="Turkey" county="September" municipality="June" name="Coastal Plain" pageId="7" pageNumber="706">Coastal Plain</location>
