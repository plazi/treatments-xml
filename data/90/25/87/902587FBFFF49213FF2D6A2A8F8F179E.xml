and 
<location LSID="urn:lsid:plazi:treatment:902587FBFFF49213FF2D6A2A8F8F179E:1D536036FFF69213FB996E1C88C4174F" box="[1068,1172,566,587]" country="Turkey" county="September" municipality="June" name="Great Falls" pageId="7" pageNumber="706">Great Falls</location>
parks within the 
<location LSID="urn:lsid:plazi:treatment:902587FBFFF49213FF2D6A2A8F8F179E:1D536036FFF69213FCFC6E7A8F8C1761" box="[841,988,592,613]" country="Turkey" county="September" municipality="June" name="Potomac Gorge" pageId="7" pageNumber="706">Potomac Gorge</location>
