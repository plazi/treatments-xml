<taxonomicName class="Insecta" family="Eulophidae" genus="Eulophus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eulophus" order="Hymenoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">Eulophus</taxonomicName>
by 
