<taxonomicName authorityName="Meregalli" authorityYear="2020" box="[668,823,261,287]" class="Insecta" family="Curculionidae" genus="Microplinthus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="karnalicus">
