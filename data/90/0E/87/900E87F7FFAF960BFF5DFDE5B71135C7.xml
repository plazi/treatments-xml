<subSubSection pageId="12" pageNumber="785" type="materials_examined">
<paragraph blockId="12.[168,774,668,727]" pageId="12" pageNumber="785">
<emphasis bold="true" box="[168,401,669,693]" pageId="12" pageNumber="785">Material examined.</emphasis>
Field observations; (
<geoCoordinate box="[648,771,668,693]" degrees="13" direction="south" minutes="25" orientation="latitude" pageId="12" pageNumber="785" precision="15" seconds="20" value="-13.422222">13°25′20″S</geoCoordinate>
, 
<geoCoordinate box="[168,317,701,726]" degrees="069" direction="west" minutes="36" orientation="longitude" pageId="12" pageNumber="785" precision="15" seconds="36" value="-69.61">069°36′36″W</geoCoordinate>
), 
<date box="[334,496,703,727]" pageId="12" pageNumber="785" value="2013-09-27" valueMax="2013-09-28" valueMin="2013-09-27">27–28.IX.2013</date>
