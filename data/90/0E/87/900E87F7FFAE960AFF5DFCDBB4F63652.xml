<collectingCountry box="[168,280,1543,1567]" name="Venezuela" pageId="13" pageNumber="786">Venezuela</collectingCountry>
up to 
<quantity box="[347,427,1543,1567]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="13" pageNumber="786" unit="m" value="1800.0">1800 m</quantity>
