, 
<collectingCountry box="[483,552,1475,1499]" name="Brazil" pageId="13" pageNumber="786">Brazil</collectingCountry>
, 
<collectingCountry box="[563,671,1475,1499]" name="Colombia" pageId="13" pageNumber="786">Colombia</collectingCountry>
, 
<collectingCountry box="[680,772,1475,1499]" name="Ecuador" pageId="13" pageNumber="786">Ecuador</collectingCountry>
, French Guiana, 
<collectingCountry box="[345,432,1509,1533]" name="Guyana" pageId="13" pageNumber="786">Guyana</collectingCountry>
, 
<collectingCountry box="[446,535,1509,1532]" name="Panama" pageId="13" pageNumber="786">Panama</collectingCountry>
