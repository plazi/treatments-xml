<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:151432" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole nemoralis petax Forel" lsidName-HNS="Pheidole nemoralis petax Forel" order="Hymenoptera" pageNumber="488" phylum="Arthropoda" rank="variety" species="nemoralis" variety="petax">Pheidole nemoralis Forel var. petax</taxonomicName>
<taxonomicNameLabel rank="variety">n. var.</taxonomicNameLabel>
