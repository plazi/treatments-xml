.iii. 
<number box="[1258,1323,1306,1332]" pageId="20" pageNumber="21" value="1999.0">1999</number>
</date>
, on 
<taxonomicName class="Magnoliopsida" family="Burseraceae" genus="Bursera" kingdom="Plantae" order="Sapindales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="genus">
