<paragraph id="21FC542337E8DE8E3218A1868A699493" pageId="0" pageNumber="1">
<taxonomicName id="9B3E6FA44615044D75DC0A4407B1D92F" authorityName="Benth" authorityYear="1853" class="Magnoliopsida" family="Myristicaceae" genus="Myristica" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Myristica elongata" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="elongata">Myristica elongata</taxonomicName>
