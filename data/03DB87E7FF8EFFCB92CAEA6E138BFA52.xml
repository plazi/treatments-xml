6. 
<taxonomicName id="4C724D72FF8EFFCB92E6EA6E15BEFBF3" authority="Bentham (1844: 110)" authorityName="Bentham" authorityPageNumber="110" authorityYear="1844" box="[164,643,1051,1078]" class="Magnoliopsida" family="Fabaceae" genus="Calliandra" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="253" phylum="Tracheophyta" rank="species" species="foliolosa">
