<subSubSection id="07CF654F4C15220470DE6BC2FBC8DBB8" box="[199,1148,1228,1254]" pageId="1" pageNumber="337" type="etymology">
<paragraph id="4F6A36C44C15220470DE6BC2FBC8DBB8" blockId="1.[151,1437,1119,2010]" box="[199,1148,1228,1254]" pageId="1" pageNumber="337">
<emphasis id="7DA1EAD64C15220470DE6BC2FEFADBB8" bold="true" box="[199,334,1228,1254]" pageId="1" pageNumber="337">Etymology.</emphasis>
