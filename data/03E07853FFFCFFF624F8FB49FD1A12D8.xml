<quantity id="4CB164A0FFFCFFF62704F9DEFC8E12FA" box="[816,887,1654,1677]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="12" pageNumber="593" unit="m" value="400.0">400 m</quantity>
