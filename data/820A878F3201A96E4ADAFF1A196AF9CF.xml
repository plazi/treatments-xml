<taxonomicName id="CDA34D1A3201A96E4E43FA4B192CFA37" box="[1038,1134,1477,1502]" class="Trematoda" family="Diplostomidae" genus="Tylodelphys" kingdom="Animalia" order="Diplostomida" pageId="8" pageNumber="266" phylum="Platyhelminthes" rank="species" species="brevis">
<emphasis id="38D7EA8B3201A96E4E43FA4B195EFA34" box="[1038,1052,1478,1501]" italics="true" pageId="8" pageNumber="266">T</emphasis>
