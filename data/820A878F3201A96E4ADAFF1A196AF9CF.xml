<emphasis id="38D7EA8B3201A96E4F0FFA2C18DEFA53" box="[1346,1436,1441,1466]" italics="true" pageId="8" pageNumber="266">littorale</emphasis>
</taxonomicName>
and 
<taxonomicName id="CDA34D1A3201A96E4A8AFA481F96FA37" authority="Spix &amp; Agassiz" authorityName="Spix &amp; Agassiz" box="[199,724,1477,1502]" class="Actinopterygii" family="Erythrinidae" genus="Hoplerythrinus" kingdom="Animalia" order="Characiformes" pageId="8" pageNumber="266" phylum="Chordata" rank="species" species="unitaeniatus">
