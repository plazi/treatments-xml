<taxonomicName id="E6BD3242F8CE6FC498065928B8359FE1" class="Insecta" family="Ichneumonidae" genus="Ctenochira" kingdom="Animalia" lsidName="Ctenochira grossa" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="grossa">Ctenochira grossa</taxonomicName>
