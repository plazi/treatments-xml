<paragraph id="387F38E20934F11DF59366707BD705CB" pageId="null" pageNumber="85">
<pageBreakToken id="CB68FF98D5DDEC4D41E3DA8973454AA5" pageId="null" pageNumber="85">Artengruppe</pageBreakToken>
des 
<taxonomicName id="9AA26AF2E144C6C349C2478D010DD098" authority="L." class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="85" phylum="Tracheophyta" rank="species" species="auricomus">
Ranunculus 
<normalizedToken id="4C0FD3CD6443F8F44E4C87B8ECD6FE85" originalValue="aurícomus" pageId="null" pageNumber="85">auricomus</normalizedToken>
<authorityName id="764FF0547AD86B716A4CE24A890AB765" pageId="null" pageNumber="85">L.</authorityName>
</taxonomicName>
</paragraph>
