<emphasis id="E28177076418FFDB11ACC940547EFBF3" bold="true" box="[982,1323,1084,1108]" pageId="2" pageNumber="167">
Genus 
<taxonomicName id="17F5D0966418FFDB1653C940547EFBF3" authority="Brisson, 1760" authorityName="Brisson" authorityYear="1760" box="[1065,1323,1084,1108]" class="Aves" family="Ciconiidae" genus="Ciconia" kingdom="Animalia" order="Ciconiiformes" pageId="2" pageNumber="167" phylum="Chordata" rank="genus">
<emphasis id="E28177076418FFDB1653C94055D6FBF3" bold="true" box="[1065,1155,1084,1108]" italics="true" pageId="2" pageNumber="167">Ciconia</emphasis>
<bibRefCitation id="B464D6E46418FFDB16F1C940547EFBF3" author="Brisson" box="[1163,1323,1084,1108]" firstAuthor="Brisson" pageId="2" pageNumber="167" refId="ref13599" refString="Brisson, M. J., 1760. Ornithologia sive Synopsis Methodica. Paris: C. J. B. Bauche." type="book" year="1760">Brisson, 1760</bibRefCitation>
