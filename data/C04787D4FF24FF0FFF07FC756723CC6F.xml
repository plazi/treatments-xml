<collectingRegion id="8A2AF820FF24FF0FFCC5F94D66F2CC43" box="[853,945,1770,1796]" country="Saudi Arabia" name="Ash Sharqiyah" pageId="205" pageNumber="206">Oriental</collectingRegion>
