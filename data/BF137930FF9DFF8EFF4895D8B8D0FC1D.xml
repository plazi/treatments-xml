<taxonomicName id="F0BAB3A5FF9DFF8EFB2694D2BD44FE09" box="[1273,1376,468,492]" class="Insecta" family="Pterophoridae" genus="Hellinsia" kingdom="Animalia" order="Lepidoptera" pageId="20" pageNumber="471" phylum="Arthropoda" rank="genus">
<emphasis id="05CE1434FF9DFF8EFB2694D2BD44FE09" box="[1273,1376,468,492]" italics="true" pageId="20" pageNumber="471">Hellinsia</emphasis>
</taxonomicName>
