<treatmentCitationGroup id="62828674FF9CFF92FF3DF887FA9DE656" box="[151,1323,1830,1852]" pageId="9" pageNumber="10">
<taxonomicName id="8592DAD9FF9CFF92FF3DF887FEA9E656" authorityName="Simon" authorityYear="1893" box="[151,287,1830,1852]" class="Arachnida" family="Pholcidae" genus="Leptopholcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis id="70E67D48FF9CFF92FF3DF887FEA9E656" box="[151,287,1830,1852]" italics="true" pageId="9" pageNumber="10">Leptopholcus</emphasis>
</taxonomicName>
— 
<treatmentCitation id="C333874BFF9CFF92FE93F887FE4AE656" author="Brignoli, P. M." box="[313,508,1830,1852]" page="651" pageId="9" pageNumber="10" year="1980">
