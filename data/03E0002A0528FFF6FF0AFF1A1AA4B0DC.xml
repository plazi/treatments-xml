<taxonomicName id="4C49CABF0528FFF6FAF1FE4D1AA3B3D4" box="[1388,1436,463,485]" class="Insecta" family="Cicadellidae" genus="Naevus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="400" phylum="Arthropoda" rank="species" species="pelta">
