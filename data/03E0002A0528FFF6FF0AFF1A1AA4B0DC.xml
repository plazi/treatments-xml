<taxonomicName id="4C49CABF0528FFF6FAACFE161AA4B39B" box="[1329,1435,404,426]" class="Insecta" family="Cicadellidae" genus="Naevus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="400" phylum="Arthropoda" rank="species" species="mandonius">
