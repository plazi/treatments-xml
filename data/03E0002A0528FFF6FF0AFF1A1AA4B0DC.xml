<taxonomicName id="4C49CABF0528FFF6FA8EFEDB1AA9B35E" box="[1299,1430,345,367]" class="Insecta" family="Cicadellidae" genus="Naevus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="400" phylum="Arthropoda" rank="species" species="subparallelus">
