<number id="3DE3762D9F71FFD2FDA12342FD42640E" box="[620,649,1306,1330]" pageId="44" pageNumber="63" value="14.0">14</number>
(
<quantity id="4CD3C9EF9F71FFD2FD552342FD1D640E" box="[664,726,1306,1330]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="44" pageNumber="63" unit="m" value="20.0">20 m</quantity>
