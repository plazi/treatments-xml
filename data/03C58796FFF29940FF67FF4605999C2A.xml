<taxonomicName id="4C6C4D03FFF39940FBB4FD3805939A87" authority="(Herrich-Schaeffer 1835)" authorityName="Herrich-Schaeffer" authorityYear="1835" baseAuthorityName="Herrich-Schaeffer" baseAuthorityYear="1835" box="[1092,1436,741,763]" class="Insecta" family="Miridae" genus="Isometopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="5" phylum="Arthropoda" rank="species" species="intrusus">
<emphasis id="B918EA92FFF39940FBB4FD3804999A87" box="[1092,1174,742,763]" italics="true" pageId="5">intrusus</emphasis>
