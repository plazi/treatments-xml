<taxonomicName id="7E85F3C18968D1B63ED6576D4AA58C34" class="Insecta" family="Tettigoniidae" genus="Anterastes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anterastes tolunayi" order="Orthoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="tolunayi">Anterastes tolunayi</taxonomicName>
