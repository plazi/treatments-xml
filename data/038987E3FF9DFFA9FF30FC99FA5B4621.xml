<taxonomicName id="4C204D76FF9CFFAAFE8CF936FD91471D" authority="Sirenko, 2021" authorityName="Sirenko" authorityYear="2021" box="[299,587,1782,1806]" class="Polyplacophora" family="Ischnochitonidae" genus="Connexochiton" kingdom="Animalia" order="Chitonida" pageId="13" pageNumber="96" phylum="Mollusca" rank="species" species="solomonicus" status="n. sp.">
