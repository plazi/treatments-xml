<number id="2E4224ACFFD3CB66A16944E2FDF566FD" box="[517,569,1415,1439]" pageId="12" pageNumber="413" value="0.2">0.20</number>
, 
<number id="2E4224ACFFD3CB66A12944E3FDB566FD" box="[581,633,1414,1439]" pageId="12" pageNumber="413" value="0.5">0.50</number>
