<taxonomicName id="4C2E30B11B4D2217FE8FEC0844E7F98C" authority="Bouvier, 1922" authorityName="Bouvier" authorityYear="1922" box="[297,742,1592,1614]" class="Malacostraca" family="Paguridae" genus="Michelopagurus" kingdom="Animalia" order="Decapoda" pageId="36" pageNumber="487" phylum="Arthropoda" rank="species" species="atlanticus">
