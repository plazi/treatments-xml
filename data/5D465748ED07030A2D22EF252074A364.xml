<taxonomicName id="3FB81332612DC27B8B7D0F70A2D86B58" LSID-ZBK="urn:lsid:zoobank.org:act:BD08FA7B-788B-4285-9548-0F120F97465D" class="Actinopterygii" family="Rivulidae" genus="Simpsonichthys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Simpsonichthys Carvalho 1959:2" order="Cyprinodontiformes" pageNumber="2" phylum="Chordata" rank="genus">Simpsonichthys</taxonomicName>
