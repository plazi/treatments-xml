21. 
<taxonomicName id="C65C99728FEF5F4BF49DE502174E0822" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26016" class="Insecta" family="Formicidae" genus="Azteca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Azteca pittieri Forel" lsidName-HNS="Azteca pittieri Forel" order="Hymenoptera" pageNumber="120" phylum="Arthropoda" rank="species" species="pittieri">Azteca pittieri</taxonomicName>
