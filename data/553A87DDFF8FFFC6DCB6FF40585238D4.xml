<typeStatus id="02288869FF8FFFC6DFDBFED4590F3B47" box="[1018,1066,259,283]" pageId="9" pageNumber="557">type</typeStatus>
series (
<collectingRegion id="1F57F829FF8FFFC6D8A4FED559D63B47" box="[1157,1267,258,283]" country="Germany" name="Hamburg" pageId="9" pageNumber="557">Hamburg</collectingRegion>
