<taxonomicName id="4A77A1D3E9ED1A6C33ED1960EB5785B4" class="Insecta" family="Curculionidae" genus="Miarus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Miarus" order="Coleoptera" pageId="256" pageNumber="257" phylum="Arthropoda" rank="genus">Miarus</taxonomicName>
