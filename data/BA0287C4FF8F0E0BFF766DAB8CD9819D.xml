BL: 3.0– 
<quantity id="F5539B37FF8C0E0BFE2E6A3E8E35877C" box="[463,555,352,378]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="2" pageNumber="53" unit="mm" value="3.7">3.7 mm</quantity>
; FL: 
<number id="846324F5FF8C0E0BFD886A3E8E95877C" box="[617,651,352,378]" pageId="2" pageNumber="53" value="1.5">1.5</number>
