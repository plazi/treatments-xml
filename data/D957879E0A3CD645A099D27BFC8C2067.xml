<emphasis id="638AEA9A0A3FD644A4ABD0D7FADE24F5" box="[1189,1343,1935,1957]" italics="true" pageId="14" pageNumber="415">P. longisaccata</emphasis>
</taxonomicName>
<emphasis id="638AEA9A0A3FD644A54BD0C8FA7424F4" bold="true" box="[1349,1429,1935,1956]" pageId="14" pageNumber="415">
<taxonomicNameLabel id="78B957E10A3FD644A54BD0C8FA7424F4" box="[1349,1429,1935,1956]" pageId="14" pageNumber="415" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype; 15, 
<taxonomicName id="96FE4D0B0A3FD644A12CD0E9FE5D2494" authority="Wang &amp; Du &amp; Li, 2013" authorityName="Wang &amp; Du &amp; Li" authorityYear="2013" box="[290,444,1966,1988]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="415" phylum="Arthropoda" rank="species" species="longisaccata" status="sp. nov.">
<emphasis id="638AEA9A0A3FD644A12CD0E9FE5D2494" box="[290,444,1966,1988]" italics="true" pageId="14" pageNumber="415">P. longisaccata</emphasis>
</taxonomicName>
<emphasis id="638AEA9A0A3FD644A1CDD0E9FDF22493" bold="true" box="[451,531,1966,1987]" pageId="14" pageNumber="415">
<taxonomicNameLabel id="78B957E10A3FD644A1CDD0E9FDF22493" box="[451,531,1966,1987]" pageId="14" pageNumber="415" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, paratype, showing the variation of dorsal spot; 16, 
<taxonomicName id="96FE4D0B0A3FD644A42FD0E9FB592494" authority="Wang &amp; Du &amp; Li, 2013" authorityName="Wang &amp; Du &amp; Li" authorityYear="2013" box="[1057,1208,1966,1988]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="415" phylum="Arthropoda" rank="species" species="lophacantha" status="sp. nov.">
<emphasis id="638AEA9A0A3FD644A42FD0E9FB592494" box="[1057,1208,1966,1988]" italics="true" pageId="14" pageNumber="415">P. lophacantha</emphasis>
</taxonomicName>
