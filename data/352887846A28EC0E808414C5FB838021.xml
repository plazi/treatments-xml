<taxonomicName id="7A814D116A2DEC0F82FF1558FAB38614" authority="D. M. Williams &amp; Round 1986: 316" authorityName="D. M. Williams &amp; Round" authorityPageNumber="316" authorityYear="1986" box="[755,1318,1819,1845]" class="Bacillariophyceae" family="Fragilariaceae" genus="Hyalosynedra" higherTaxonomySource="GBIF" kingdom="Chromista" order="Fragilariales" pageId="6" pageNumber="241" phylum="Ochrophyta" rank="genus">
