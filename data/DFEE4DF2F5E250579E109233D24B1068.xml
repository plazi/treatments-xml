<taxonomicName id="398E5CC60824A449CE4E62B53D442901" authorityName="Smith" authorityYear="1853" class="Insecta" family="Halictidae" genus="Sphecodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphecodes fumipennis" order="Hymenoptera" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="fumipennis">Sphecodes fumipennis</taxonomicName>
