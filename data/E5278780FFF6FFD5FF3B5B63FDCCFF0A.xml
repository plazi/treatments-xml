<taxonomicName id="AA8E4D15FFF1FFD5FBFB5CB4FB5AFF61" box="[1111,1252,152,177]" class="Mammalia" family="Muridae" genus="Peromyscus" kingdom="Animalia" order="Rodentia" pageId="28" pageNumber="59" phylum="Chordata" rank="genus">
