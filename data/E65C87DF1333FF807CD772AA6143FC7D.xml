<taxonomicName id="A9F54D4A1333FF807EE570D260F4FC7C" authority="Nischk and Otte, 2000" authorityName="Nischk and Otte" authorityYear="2000" box="[677,1187,932,957]" class="Insecta" family="Haglotettigoniidae" genus="Neoacla" kingdom="Animalia" order="Orthoptera" pageId="16" pageNumber="414" phylum="Arthropoda" rank="species" species="clandestina" status="n. comb.">
<emphasis id="5C81EADB1333FF807EE570D267DDFC7D" box="[677,906,932,957]" italics="true" pageId="16" pageNumber="414">Neoacla clandestina</emphasis>
(Nischk and Otte, 
<number id="D83D24EE1333FF80781F70D260CCFC7C" box="[1119,1179,932,956]" pageId="16" pageNumber="414" value="2000.0">2000</number>
)
</taxonomicName>
, 
<taxonomicNameLabel id="47B257A01333FF8078EE70D26143FC7D" box="[1198,1300,932,957]" pageId="16" pageNumber="414">
<emphasis id="5C81EADB1333FF8078EE70D26143FC7D" bold="true" box="[1198,1300,932,957]" pageId="16" pageNumber="414">n. comb.</emphasis>
