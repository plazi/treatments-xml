<taxonomicName id="18B74D645407FFABFCA5C61CF7FFFE9A" authority="(Zeller, 1867)" baseAuthorityName="Zeller" baseAuthorityYear="1867" box="[806,1157,261,288]" class="Insecta" family="Pyralidae" genus="Ancylosis" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="109" phylum="Arthropoda" rank="species" species="samaritanella">
