<taxonomicName id="18B74D645407FFABFE30C61CF1FBFEA5" box="[435,641,261,287]" class="Insecta" family="Pyralidae" genus="Ancylosis" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="109" phylum="Arthropoda" rank="species" species="monella">
