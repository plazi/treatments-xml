<taxonomicName id="4C5B4D086C1CFF85B6B9FA97E862FAE0" baseAuthorityName="Praz" baseAuthorityYear="2017" box="[737,864,1339,1365]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
