<taxonomicName id="4C5B4D086C1CFF85B0A8FAF3EE97FACF" box="[1264,1429,1375,1402]" class="Magnoliopsida" family="Caprifoliaceae" kingdom="Plantae" order="Dipsacales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="family">Caprifoliaceae</taxonomicName>
) and 
<taxonomicName id="4C5B4D086C1CFF85B49FFA2FEA46FA28" baseAuthorityName="Muller" baseAuthorityYear="2021" box="[199,324,1411,1437]" class="Magnoliopsida" family="Lamiaceae" kingdom="Plantae" order="Lamiales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
