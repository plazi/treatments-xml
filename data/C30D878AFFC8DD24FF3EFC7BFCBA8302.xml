<emphasis id="79D0EA8EFFC8DD27FEF4F99EFE208425" box="[349,392,1536,1560]" italics="true" pageId="5" pageNumber="44">
ad 
<number id="FD6C24BBFFC8DD27FED2F99EFE208425" box="[379,392,1536,1560]" pageId="5" pageNumber="44" value="2.0">2</number>
