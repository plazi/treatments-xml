<quantity id="95387F9DFFF7FFF7FE23FA7D89F18584" box="[385,427,1456,1481]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="24" pageNumber="26" unit="in" value="5.0">5 in</quantity>
