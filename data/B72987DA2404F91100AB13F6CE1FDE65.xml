<taxonomicName id="F8804D4F2404F91103FC1089CA1CDE19" authority="Demange, 1989" authorityName="Demange" authorityYear="1989" box="[960,1302,1772,1798]" class="Diplopoda" family="Harpagophoridae" genus="Thyropygus" kingdom="Animalia" order="Spirostreptida" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="enghoffi">
