<taxonomicName id="423390FA598F755481B45B791E1D42BC" authority="L." authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Asteraceae" genus="Prenanthes" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="altissima">
