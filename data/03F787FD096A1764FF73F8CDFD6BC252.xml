<taxonomicName id="4C5E4D68096D1764FE83F8A8FE21C279" box="[359,442,1832,1857]" class="Phyllopharyngea" family="Thecacinetidae" genus="Thecacineta" higherTaxonomySource="GBIF" kingdom="Protista" order="Exogenida" pageId="4" pageNumber="64" phylum="Ciliophora" rank="species" species="calix">
