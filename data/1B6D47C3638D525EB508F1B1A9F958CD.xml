<subSubSection id="C466ABD8E20DFF965205EF0BC3B4320C" pageId="0" pageNumber="57889" type="nomenclature">
<paragraph id="799BB78B79718E09FCC1F55467CCBFD6" pageId="0" pageNumber="57889">
<taxonomicName id="69D0264C47566F11FD86736BD6343C31" LSID="1B6D47C3-638D-525E-B508-F1B1A9F958CD" authority="Simon, 1876" authorityName="Simon" authorityYear="1876" class="Arachnida" family="Lycosidae" genus="Pardosa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pardosa cribrata" order="Araneae" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="cribrata">Pardosa cribrata Simon, 1876</taxonomicName>
