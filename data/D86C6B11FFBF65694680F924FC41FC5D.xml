. 
<collectingCountry id="28D29A97FFBE65694770FC02FE53FC5D" box="[359,470,944,968]" name="Myanmar" pageId="13" pageNumber="414">Myanmar</collectingCountry>
, 
<collectingCountry id="28D29A97FFBE656947F6FC1DFDC2FC5D" box="[481,583,943,968]" name="Thailand" pageId="13" pageNumber="414">Thailand</collectingCountry>
, 
<collectingCountry id="28D29A97FFBE65694445FC02FD0EFC52" box="[594,651,944,967]" name="Laos" pageId="13" pageNumber="414">Laos</collectingCountry>
, North-East 
<collectingCountry id="28D29A97FFBE6569450EFC1DFCD9FC5D" box="[793,860,943,968]" name="China" pageId="13" pageNumber="414">China</collectingCountry>
