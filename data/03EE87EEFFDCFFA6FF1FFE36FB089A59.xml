<quantity id="4CBF9B1DFFDCFFA4FD6AFDA1FCAA9BCC" box="[738,818,617,641]" metricMagnitude="2" metricUnit="m" metricValue="8.2" pageId="20" pageNumber="21" unit="m" value="820.0">820 m</quantity>
