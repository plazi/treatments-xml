<taxonomicName id="E9CE50B48D7AFFCFFC009E5F33CBF8B0" box="[959,1163,1864,1889]" class="Arachnida" family="Amaurobiidae" genus="Draconarius" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="48" phylum="Arthropoda" rank="species" species="parabrunneus">
