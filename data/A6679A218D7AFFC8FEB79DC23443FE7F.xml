<typeStatus id="F17595958D7DFFC8FDB4980B3531FEE7" box="[523,625,284,310]" pageId="4" pageNumber="49">holotype</typeStatus>
of 
<taxonomicName id="E9CE50B48D7DFFC8FD24980B345EFEE4" box="[667,798,284,309]" class="Arachnida" family="Amaurobiidae" genus="Draconarius" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="incertus">
