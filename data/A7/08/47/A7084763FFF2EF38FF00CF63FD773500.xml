<paragraph blockId="11.[151,1437,930,2034]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[199,374,1866,1891]" pageId="11" pageNumber="12">
<typeStatus box="[199,256,1867,1891]" pageId="11" pageNumber="12">Type</typeStatus>
