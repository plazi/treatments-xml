<taxonomicName authorityName="Casey" authorityYear="1920" class="Hexapoda" family="Curculionidae" genus="Sibariops" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="13" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="13" pageNumber="26">Sibariops</emphasis>
</taxonomicName>
