</taxonomicName>
and 
<taxonomicName authority="Bouzan &amp; Iniesta &amp; Brescovit, 2018" authorityName="Bouzan &amp; Iniesta &amp; Brescovit" authorityYear="2018" box="[801,890,1891,1915]" class="Diplopoda" family="Chelodesmidae" genus="Rupidesmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="2" pageNumber="591" phylum="Arthropoda" rank="species" species="aclys" status="n.sp.">
