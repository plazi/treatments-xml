<taxonomicName authorityName="Desmarest" authorityYear="1817" class="Malacostraca" family="Eryonidae" genus="Eryon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="868" phylum="Arthropoda" rank="species" species="cuvieri">
<emphasis italics="true" pageId="11" pageNumber="868">E.cuvieri</emphasis>
</taxonomicName>
