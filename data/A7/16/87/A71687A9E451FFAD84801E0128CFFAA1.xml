<taxonomicName baseAuthorityName="Semenov" baseAuthorityYear="1895" box="[756,855,796,818]" class="Insecta" family="Carabidae" genus="Antisphodrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="132" phylum="Arthropoda" rank="species" species="glasunowi">
