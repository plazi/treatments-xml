<collectingCountry box="[1354,1431,422,447]" name="Russia" pageId="11" pageNumber="234">Russia</collectingCountry>
, Northern 
<collectingCountry box="[259,393,458,483]" name="Kazakhstan" pageId="11" pageNumber="234">Kazakhstan</collectingCountry>
