<taxonomicName baseAuthorityName="Melvill" baseAuthorityYear="1907" box="[998,1064,853,873]" class="Bivalvia" family="Propeamussiidae" genus="Similipecten" kingdom="Animalia" order="Pectinida" pageId="24" pageNumber="101" phylum="Mollusca" rank="species" species="eous">S. eous</taxonomicName>
<taxonomicName authorityName="Dijkstra &amp; Maestrati" authorityYear="2008" box="[1272,1366,853,873]" class="Bivalvia" family="Propeamussiidae" genus="Similipecten" kingdom="Animalia" order="Pectinida" pageId="24" pageNumber="101" phylum="Mollusca" rank="species" species="herosae">S. herosae</taxonomicName>
</emphasis>
