<typeStatus box="[574,620,1018,1041]" pageId="1" pageNumber="66">type</typeStatus>
material from Lesser Antilles (
<collectingCountry box="[964,1087,1018,1041]" name="Martinique" pageId="1" pageNumber="66">Martinique</collectingCountry>
, 
<collectingCountry box="[1099,1231,1018,1041]" name="Guadeloupe" pageId="1" pageNumber="66">Guadeloupe</collectingCountry>
, 
<collectingCountry box="[1243,1381,1018,1041]" name="Sint Martin" pageId="1" pageNumber="66">Saint-Martin</collectingCountry>
