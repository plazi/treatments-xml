<paragraph pageId="0" pageNumber="109">Type species.</paragraph>
<paragraph pageId="0" pageNumber="109">
<taxonomicName class="Arachnida" family="Agelenidae" genus="Mistaria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mistaria leucopyga" order="Araneae" pageId="0" pageNumber="109" phylum="Arthropoda" rank="species" species="leucopyga">Mistaria leucopyga</taxonomicName>
