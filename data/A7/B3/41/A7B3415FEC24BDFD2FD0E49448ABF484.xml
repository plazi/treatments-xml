<subSubSection pageId="null" pageNumber="350" type="reference_group">
<paragraph pageId="null" pageNumber="350">
(
<taxonomicName authority="L." authorityName="L." class="Liliopsida" family="Poaceae" genus="Festuca" kingdom="Plantae" order="Poales" pageId="null" pageNumber="350" phylum="Tracheophyta" rank="species" species="elatior">
