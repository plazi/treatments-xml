<quantity id="4CEF9B25FFC6FFE29CAEFE9DFF58318C" box="[1235,1305,367,393]" metricMagnitude="-1" metricUnit="m" metricValue="1.8" pageId="87" pageNumber="88" unit="cm" value="18.0">18 cm</quantity>
(
<quantity id="4CEF9B25FFC6FFE29D54FE9DFF2D318F" box="[1321,1388,367,394]" metricMagnitude="-1" metricUnit="m" metricValue="1.8034" pageId="87" pageNumber="88" unit="in" value="7.1">7.1 in</quantity>
) TL (
<bibRefCitation id="EF864B31FFC6FFE298B0FE61FB2231AB" author="Pietsch" box="[205,355,403,430]" firstAuthor="Pietsch" pageId="87" pageNumber="88" pagination="479 - 493" refId="ref312362" refString="Pietsch, T. W. (1986) Systematics and distribution of bathypelagic anglerfishes of the family Ceratiidae (order: Lophiiformes). Copeia, 1986, 479 - 493." type="journal article" year="1986">Pietsch 1986</bibRefCitation>
