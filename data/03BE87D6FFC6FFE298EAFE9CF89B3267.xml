<quantity id="4CEF9B25FFC6FFE29985FE61F87C31A8" box="[504,573,403,429]" metricMagnitude="-1" metricUnit="m" metricValue="6.4" pageId="87" pageNumber="88" unit="cm" value="64.0">64 cm</quantity>
