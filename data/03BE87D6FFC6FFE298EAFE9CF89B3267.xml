<quantity id="4CEF9B25FFC6FFE29A36FE60F8DA31AB" box="[587,667,402,430]" metricMagnitude="-1" metricUnit="m" metricValue="6.400799999999999" pageId="87" pageNumber="88" unit="in" value="25.2">25.2 in</quantity>
