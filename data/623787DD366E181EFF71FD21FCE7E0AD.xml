<bibRefCitation id="8E0F4B3A366E181EFEB5FD48FDC0E68B" author="JOSEPH G" box="[373,537,730,750]" pageId="33" pageNumber="1270" pagination="365 - 366" refId="ref35503" refString="JOSEPH G. (1868): Lathrobium (Centrocnemis) Krniense n. sp. - Berliner Entomologische Zeitschrift 12: 365 - 366." type="journal article" year="1868">
