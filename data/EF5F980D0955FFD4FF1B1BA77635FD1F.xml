<taxonomicName id="A0F652980955FFD4FE5018387437FDC7" authority="REITT." authorityName="REITTER" authorityYear="1862" box="[506,768,560,582]" class="Insecta" family="Carabidae" genus="Harpalus" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="918" phylum="Arthropoda" rank="subSpecies" species="pulvinatus" subSpecies="lubricus">
