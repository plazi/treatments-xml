<subSubSection id="13B4B3D5FFE0FFC0FF378F06B0C7C8D7" pageId="59" pageNumber="60" type="discussion">
<paragraph id="5B11E05EFFE0FFC0FF378F06B4DFC94B" blockId="59.[151,1437,379,801]" pageId="59" pageNumber="60">
<emphasis id="69DA3C4CFFE0FFC0FF378F06B063CA97" bold="true" box="[199,315,379,405]" pageId="59" pageNumber="60">Remarks.</emphasis>
<taxonomicName id="9CAE9BDDFFE0FFC0FEB38F06B0E8CA97" authorityName="Gray" authorityYear="1867" box="[323,432,379,405]" class="Calcarea" family="Clathrinidae" genus="Clathrina" kingdom="Animalia" order="Clathrinida" pageId="59" pageNumber="60" phylum="Porifera" rank="genus">
