<td id="42615DB3FFFB0008FBB3F9B6D5F31B39" box="[439,498,1585,1611]" gridcol="1" gridrow="2" pageId="30" pageNumber="31">0-0</td>
<td id="42615DB3FFFB0008F813F9B6D6551B39" box="[535,596,1585,1611]" gridcol="2" gridrow="2" pageId="30" pageNumber="31">1-0</td>
