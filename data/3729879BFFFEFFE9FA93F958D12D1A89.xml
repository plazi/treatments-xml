<th id="42615DB3FFFB0008FAC3F98AD52B1B55" box="[199,298,1549,1575]" gridcol="0" gridrow="1" pageId="30" pageNumber="31">Leg 1</th>
<td id="42615DB3FFFB0008FBB3F98AD5F31B55" box="[439,498,1549,1575]" gridcol="1" gridrow="1" pageId="30" pageNumber="31">0-0</td>
<td id="42615DB3FFFB0008F813F98AD6551B55" box="[535,596,1549,1575]" gridcol="2" gridrow="1" pageId="30" pageNumber="31">1-I</td>
<td id="42615DB3FFFB0008F873F98AD7051B55" box="[631,772,1549,1575]" gridcol="3" gridrow="1" pageId="30" pageNumber="31">I-0; III, I, III</td>
<td id="42615DB3FFFB0008F933F98AD7AD1B55" box="[823,940,1549,1575]" gridcol="4" gridrow="1" pageId="30" pageNumber="31">0-0; I, II, I</td>
</tr>
