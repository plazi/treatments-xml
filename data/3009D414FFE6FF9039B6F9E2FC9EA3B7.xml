<taxonomicName id="7FA01E81FFE6FFAD39E2F9E3FC4AA46F" authority="Moron and Solis, 2000" authorityName="Moron and Solis" authorityYear="2000" box="[195,987,1554,1581]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="132" phylum="Arthropoda" rank="species" species="guanacasteca" subGenus="Phyllophaga">
