<taxonomicName id="5AAC59A2FFB1FF0B9B00FBD4FCDAFBB6" authority="Gunther, 1887" authorityName="Gunther" authorityYear="1887" box="[329,815,1042,1068]" class="Actinopterygii" family="Pleuronectidae" genus="Macrurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pleuronectiformes" pageId="8" pageNumber="33" phylum="Chordata" rank="species" species="liocephalus">
