<taxonomicName id="4C3801B40B498675FF95FD170D9B93D2" box="[113,172,733,756]" class="Liliopsida" family="Musaceae" genus="Musa" kingdom="Plantae" order="Zingiberales" pageId="17" pageNumber="65" phylum="Tracheophyta" rank="genus">
<emphasis id="B94CA6250B498675FF95FD170D9B93D2" box="[113,172,733,756]" italics="true" pageId="17" pageNumber="65">Musa</emphasis>
