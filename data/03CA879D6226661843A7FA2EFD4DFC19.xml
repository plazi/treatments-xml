<taxonomicName id="4C634D086226661941BFFA2EFC9FFA40" authorityName="Mulsant" authorityYear="1842" box="[655,780,1441,1467]" class="Insecta" family="Scarabaeidae" genus="Oxythyrea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="423" phylum="Arthropoda" rank="genus">
