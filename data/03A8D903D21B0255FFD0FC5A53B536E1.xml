</taxonomicName>
(mel’li.fer. L. masc. adj. 
<taxonomicName id="4C011396D21B0255FD0AFC0953F0370B" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[675,754,1021,1045]" class="Bacilli" family="Lactobacillaceae" genus="Bombilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="20" pageNumber="2802" phylum="Firmicutes" rank="species" species="mellifer">
<emphasis id="B975B407D21B0255FD0AFC0953F0370B" box="[675,754,1021,1045]" italics="true" pageId="20" pageNumber="2802">mellifer</emphasis>
</taxonomicName>
