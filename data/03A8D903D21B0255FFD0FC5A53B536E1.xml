<taxonomicName id="4C011396D21B0255FE72FBCF536A374D" authorityName="Linnaeus" authorityYear="1758" box="[475,616,1083,1107]" class="Insecta" family="Apidae" genus="Apis" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="2802" phylum="Arthropoda" rank="species" species="mellifera">
