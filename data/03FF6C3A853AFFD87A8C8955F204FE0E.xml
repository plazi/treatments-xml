<taxonomicName id="4C56A6AF853CFFD87AE78A26F725FEDF" box="[252,363,306,331]" class="Entognatha" family="Neanuridae" genus="Pseudachorutes" kingdom="Animalia" order="Collembola" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="dubius">
