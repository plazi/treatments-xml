<taxonomicName id="4C56A6AF853CFFD878728A4DF4B6FEE6" box="[617,760,345,370]" class="Entognatha" family="Neanuridae" genus="Pseudachorutes" kingdom="Animalia" order="Collembola" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="scythicus">
