<taxonomicName id="4C56A6AF853CFFD878A58A94F567FE0D" box="[702,809,384,409]" class="Entognatha" family="Neanuridae" genus="Pseudachorutes" kingdom="Animalia" order="Collembola" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="dubius">
