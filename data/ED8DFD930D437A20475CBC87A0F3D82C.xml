<subSubSection id="2357C3B6269446EE2B23A123D960A0F6" pageId="19" pageNumber="20" type="reference_group">
<paragraph id="96FE96CDA447DABCD02480005BFD4AEE" pageId="19" pageNumber="20">
<taxonomicName id="CD413B09EC84687498A163C8C6DA6A07" authorityName="Rao" authorityYear="1922" class="Clitellata" family="Almidae" genus="Glyphidrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Haplotaxida" pageId="19" pageNumber="20" phylum="Annelida" rank="species" species="fluviatilis">Glyphidrilus fluviatilis</taxonomicName>
