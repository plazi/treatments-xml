<number id="3DD845465C346D7BFE98F8B69316F8ED" box="[264,316,1866,1891]" pageId="46" pageNumber="47" value="14.6">14.6</number>
, 
<number id="3DD845465C346D7BFED7F8B69351F8ED" box="[327,379,1866,1891]" pageId="46" pageNumber="47" value="20.5">20.5</number>
