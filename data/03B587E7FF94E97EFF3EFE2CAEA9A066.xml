<taxonomicName id="4C1C4D72FF94E97EFD9EFC37AE18A68E" box="[567,826,937,962]" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="199" phylum="Arthropoda" rank="species" species="charesi">
