<taxonomicName id="4C1C4D72FF94E97EFF3EFC6CAF1FA146" authority="Emery, 1908" authorityName="Emery" authorityYear="1908" box="[151,573,1009,1034]" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="199" phylum="Arthropoda" rank="species" species="baldaccii">
