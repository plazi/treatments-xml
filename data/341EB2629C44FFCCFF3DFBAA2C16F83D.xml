<taxonomicName id="7BB778F79C44FFCCFD28F9492B85F979" authority="Signoret" authorityName="Signoret" box="[642,990,1687,1712]" class="Insecta" family="Diaspididae" genus="Aspidiotus" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="311" phylum="Arthropoda" rank="species" species="destructor">
<emphasis id="8EC3DF669C44FFCCFD28F9492B28F979" box="[642,883,1687,1712]" italics="true" pageId="10" pageNumber="311">Aspidiotus destructor</emphasis>
Signoret
</taxonomicName>
