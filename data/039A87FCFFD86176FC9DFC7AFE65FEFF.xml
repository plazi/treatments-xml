<taxonomicName id="4C334D69FFD86174FD2CFA3EFCC4FA29" authorityName="De Jersey &amp; Hamilton" authorityYear="1967" box="[677,892,1451,1477]" genus="Alisporites" higherTaxonomySource="GBIF" kingdom="Plantae" pageId="25" pageNumber="112" phylum="Tracheophyta" rank="species" species="australis">
