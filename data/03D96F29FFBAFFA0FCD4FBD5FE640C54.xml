<location id="8EAF88E4FFBAFF9FFAABFB25FCA80AD0" LSID="urn:lsid:plazi:treatment:03D96F29FFBAFFA0FCD4FBD5FE640C54:8EAF88E4FFBAFF9FFAABFB25FCA80AD0" country="United States of America" county="Phare Morro" latitude="23.866667" longLatPrecision="1254" longitude="-88.96667" municipality="Caribbean Sea" name="Steamer Blake" pageId="31" pageNumber="476">
Steamer 
<emphasis id="B904022DFFBAFF9FFCD0FB04FCA80AD0" box="[826,891,1208,1229]" italics="true" pageId="31" pageNumber="476">Blake</emphasis>
</location>
