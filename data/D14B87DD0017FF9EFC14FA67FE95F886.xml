<caption id="0D9D66430016FF91FFB5FB1CFC41FAD1" ID-DOI="http://doi.org/10.5281/zenodo.5534509" ID-Zenodo-Dep="5534509" httpUri="https://zenodo.org/record/5534509/files/figure.png" pageId="23" pageNumber="23" startId="23.[112,167,1258,1278]" targetBox="[414,1212,145,1227]" targetPageId="23">
<paragraph id="595D36CB0016FF91FFB5FB1CFC41FAD1" blockId="23.[112,1514,1258,1350]" pageId="23" pageNumber="23">
