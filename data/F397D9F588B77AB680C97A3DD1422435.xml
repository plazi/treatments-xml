<taxonomicName id="CB55B03F35FD9B1D98815DDCB3C550A1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:190756" class="Insecta" family="Halictidae" genus="Nomia" kingdom="Animalia" lsidName-HNS="Nomia apicalis Smith" order="Hymenoptera" pageNumber="43" phylum="Arthropoda" rank="species" species="apicalis">Nomia apicalis</taxonomicName>
