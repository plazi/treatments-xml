<taxonomicName id="5FB593C1FFA0FF99EE5DFF2075FFFF48" authority="Uvarov (1931)" authorityName="Uvarov" authorityYear="1931" box="[366,663,192,218]" class="Insecta" family="Mantidae" genus="Microthespis" kingdom="Animalia" order="Mantodea" pageId="11" pageNumber="54" phylum="Arthropoda" rank="species" species="evansi">
<emphasis id="AAC13450FFA0FF99EE5DFF207689FF4B" box="[366,481,192,217]" italics="true" pageId="11" pageNumber="54">M. evansi</emphasis>
<bibRefCitation id="FC2495B3FFA0FF99EEDBFF2075FFFF48" author="Uvarov" box="[488,663,192,218]" pageId="11" pageNumber="56" refString="Uvarov, B. P. (1931) Notes on the Genus Iris, Saussure (Orthoptera, Mantidae). Annals and Magazine of Natural History, London. 8, 234 - 239." type="journal article" year="1931">
Uvarov (
<number id="2E7DFA65FFA0FF99ED63FF2075E7FF48" box="[592,655,192,218]" pageId="11" pageNumber="54" value="1931.0">1931</number>
)
</bibRefCitation>
</taxonomicName>
