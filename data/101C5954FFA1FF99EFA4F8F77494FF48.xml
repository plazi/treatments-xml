<taxonomicName id="5FB593C1FFA1FF98EE75F83B753EF866" box="[326,598,2011,2036]" class="Insecta" family="Mantidae" genus="Microthespis" kingdom="Animalia" order="Mantodea" pageId="10" pageNumber="53" phylum="Arthropoda" rank="species" species="dmitriewi">
