<collectingCountry id="F37DF790FFBF5559B16AFF3EFE6438EB" box="[294,407,151,176]" name="Tajikistan" pageId="12" pageNumber="113">Tajikistan</collectingCountry>
, 
<collectingCountry id="F37DF790FFBF5559B1EEFF3EFDF238EB" box="[418,513,151,176]" name="Pakistan" pageId="12" pageNumber="113">Pakistan</collectingCountry>
and 
<collectingCountry id="F37DF790FFBF5559B274FF3EFD4838EB" box="[568,699,151,176]" name="Kazakhstan" pageId="12" pageNumber="113">Kazakhstan</collectingCountry>
