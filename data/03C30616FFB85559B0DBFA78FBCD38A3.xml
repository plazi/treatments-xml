<collectingCountry id="F37DF790FFBF5559B0DBFF15FEF7388E" box="[151,260,188,213]" name="Mongolia" pageId="12" pageNumber="113">Mongolia</collectingCountry>
by 
<bibRefCitation id="EFFBCAF1FFBF5559B17AFF15FE26388F" author="Dietze" box="[310,469,188,213]" pageId="12" pageNumber="113" refString="Dietze, K. (1913) Biologie der Eupithecien. II. Berlin, 173 S., 86 Taf." type="journal article" year="1913">
Dietze (
<number id="3DA2A527FFBF5559B1DFFF15FE3E388F" box="[403,461,188,212]" pageId="12" pageNumber="113" value="1913.0">1913</number>
)
</bibRefCitation>
