<collectingCountry id="F37DF790FFB8555EB55BF862FA6B3FBF" box="[1303,1432,1995,2020]" name="Uzbekistan" pageId="11" pageNumber="112">Uzbekistan</collectingCountry>
, 
<collectingCountry id="F37DF790FFBF5559B0DBFF31FEE838EB" box="[151,283,152,176]" name="Kyrgyzstan" pageId="12" pageNumber="113">Kyrgyzstan</collectingCountry>
