<taxonomicName id="21858EE3AEC2304109F1BF1D8B237EC7" class="Insecta" family="Dilaridae" genus="Dilar" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dilar" order="Neuroptera" pageId="5" pageNumber="128" phylum="Arthropoda" rank="genus">Dilar</taxonomicName>
