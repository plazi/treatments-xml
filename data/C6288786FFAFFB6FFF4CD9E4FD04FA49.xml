<taxonomicName id="89814D13FFAFFB6FFEA0D992FBFAFAED" authority="Deb &amp; Raychaudhuri, 1965: 122" authorityName="Deb &amp; Raychaudhuri" authorityPageNumber="122" authorityYear="1965" box="[379,1029,1259,1284]" class="Arachnida" family="Uropodidae" genus="Indotrachytes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="116" phylum="Arthropoda" rank="species" species="longisetus">
<emphasis id="7CF5EA82FFAFFB6FFEA0D992FD72FAED" box="[379,653,1259,1284]" italics="true" pageId="15" pageNumber="116">Indotrachytes longisetus</emphasis>
<bibRefCitation id="2A104B61FFAFFB6FFD4FD992FC31FAED" author="Deb" box="[660,974,1259,1284]" pageId="15" pageNumber="116" refString="Deb, D. C. &amp; Raychaudhuri, D. N. (1965) A new trachytid mite [Acarina: Mesostigmata] from West Bengal, India. Proceedings of the Zoological Society of Calcutta, 18, 121 - 124." type="journal article" year="1965">
