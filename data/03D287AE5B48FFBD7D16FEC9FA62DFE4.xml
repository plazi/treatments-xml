<taxonomicName id="4C7B4D3B5B48FFBD7E6FFA35FCE6DBF7" authorityName="Kirchner &amp; Kondratieff" authorityYear="1984" box="[605,794,1415,1439]" class="Insecta" family="Perlodidae" genus="Diploperla" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="133" phylum="Arthropoda" rank="species" species="kanawholensis">
