<paragraph id="72C1256EC63E2E76AC9F92547601AA20" pageId="0" pageNumber="89">
<materialsCitation id="F2B51B75BDC672A5B4A7F3CC78F98D0B" ID-GBIF-Occurrence="3069667303" accessionNumber="SRC 00866" collectingDate="2021-01-01" collectingDateMax="2021-12-31" collectingDateMin="2021-01-01" elevation="1280" latitude="4.038" longLatPrecision="13" longitude="114.886" specimenCount="1">
