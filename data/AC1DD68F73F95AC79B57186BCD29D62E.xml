<elevation id="A50359F7191CAB9F3E2CE56C4A52BB39" metricMagnitude="3" metricUnit="m" metricValue="1.28" unit="m" value="1280.0">
<quantity id="2380AE35906464BA0589A575ADCD1BF4" metricMagnitude="3" metricUnit="m" metricValue="1.28" unit="m" value="1280.0">1,280 m</quantity>
a.s.l.
</elevation>
