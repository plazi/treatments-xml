<taxonomicName id="4C314D2ADE197A6CFD8A0FFE3987F93C" authorityName="Mugrabi &amp; Azevedo" authorityYear="2016" box="[618,715,1761,1785]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="508" phylum="Arthropoda" rank="species" species="kukamba">
