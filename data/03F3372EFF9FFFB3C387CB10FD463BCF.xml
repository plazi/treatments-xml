<collectingCountry id="F34DC6A8FF98FFB3C1A6CD63FD623B7A" box="[694,761,332,357]" name="China" pageId="12" pageNumber="357">China</collectingCountry>
(
<collectingRegion id="499E48DAFF98FFB3C019CD63FCFC3B7A" box="[777,871,332,357]" country="China" name="Sichuan" pageId="12" pageNumber="357">Sichuan</collectingRegion>
