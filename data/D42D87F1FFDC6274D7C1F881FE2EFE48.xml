43. 
<taxonomicName id="9B844D64FFDD6274D797FF63FD07FF70" authority="(Linnaeus, 1758)" authorityName="Linnaeus" authorityYear="1758" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[193,652,151,176]" class="Insecta" family="Tettigoniidae" genus="Decticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="verrucivorus">
