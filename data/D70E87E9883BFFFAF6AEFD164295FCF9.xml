: 
<taxonomicName id="98A74D7C883DFFFAF728FE994145FEBB" authority="Ito, 1983" authorityName="Ito" authorityYear="1983" box="[273,585,352,375]" class="Maxillopoda" family="Argestidae" genus="Argestes" kingdom="Animalia" order="Harpacticoida" pageId="31" pageNumber="254" phylum="Arthropoda" rank="species" species="reductus">
