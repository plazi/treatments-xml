[[ Genus 
<taxonomicName id="74E69B5CA10C8D19D2DD5F256A13940F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2461" class="Insecta" family="Formicidae" genus="Bothriomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Bothriomyrmex Emery" order="Hymenoptera" pageNumber="2" phylum="Arthropoda" rank="genus">Bothriomyrmex</taxonomicName>
