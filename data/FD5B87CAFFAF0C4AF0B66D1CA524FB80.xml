<taxonomicName id="B2F24D5FFFAF0C49F6FB69FCA7B7FA76" authority="de Pinna &amp; Kirovsky, 2011" authorityName="de Pinna &amp; Kirovsky" authorityYear="2011" class="Actinopterygii" family="Trichomycteridae" genus="Pygidianops" kingdom="Animalia" order="Siluriformes" pageId="9" pageNumber="493" phylum="Chordata" rank="species" species="amphioxus" status="new species">
