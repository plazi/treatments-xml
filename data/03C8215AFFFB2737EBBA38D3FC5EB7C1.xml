<paragraph id="8BDE904CFFFB2737EBE43A3AFC04B2D3" blockId="2.[151,1437,386,1924]" box="[201,902,638,663]" pageId="2" pageNumber="31">
<emphasis id="B9154C5EFFFB2737EBE43A3AFE30B2D2" bold="true" box="[201,434,638,663]" pageId="2" pageNumber="31">GenBank numbers.</emphasis>
ssrDNA 
<accessionNumber id="94320DAFFFFB2737E9313A3AFD1BB2D3" box="[540,665,638,662]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY157184" pageId="2" pageNumber="31">
AY 
<number id="3DA9826BFFFB2737E9683A3AFD1BB2D3" box="[581,665,638,662]" pageId="2" pageNumber="31" value="157184.0">157184</number>
</accessionNumber>
; lsrDNA 
<accessionNumber id="94320DAFFFFB2737E8293A3AFC00B2D3" box="[772,898,638,662]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY157174" pageId="2" pageNumber="31">
AY 
<number id="3DA9826BFFFB2737E8033A3AFC00B2D3" box="[814,898,638,662]" pageId="2" pageNumber="31" value="157174.0">157174</number>
