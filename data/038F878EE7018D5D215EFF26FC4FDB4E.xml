</taxonomicName>
, 
<taxonomicName id="4C264D1BE7018D5C2088F7CEFCF4D130" authorityName="Assing" authorityYear="2013" box="[691,780,2058,2079]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="sectum">
<emphasis id="B952EA8AE7018D5C2088F7CEFCF4D130" box="[691,780,2058,2079]" italics="true" pageId="62" pageNumber="115">L. sectum</emphasis>
</taxonomicName>
