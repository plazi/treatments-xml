<taxonomicName id="4C264D1BE7018D5C2072F829FD19D12E" authorityName="Assing" authorityYear="2013" box="[585,737,2028,2050]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="bihamulatum">
