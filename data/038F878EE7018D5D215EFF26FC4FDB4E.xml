<taxonomicName id="4C264D1BE7018D5C2002F7CEFD50D131" authorityName="Assing" authorityYear="2013" box="[569,680,2057,2079]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="desectum">
