<taxonomicName id="4C264D1BE7018D5C212CF7CEFC66D130" authorityName="Assing" authorityYear="2013" box="[791,926,2058,2079]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="amputatum">
<emphasis id="B952EA8AE7018D5C212CF7CEFC66D130" box="[791,926,2058,2079]" italics="true" pageId="62" pageNumber="115">L. amputatum</emphasis>
