<taxonomicName id="4C264D1BE7008D5D2121FD8EFC49DB4E" authorityName="Assing" authorityYear="2013" box="[794,945,586,609]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="116" phylum="Arthropoda" rank="species" species="squamosum">
<emphasis id="B952EA8AE7008D5D2121FD8EFC49DB4E" box="[794,945,586,609]" italics="true" pageId="63" pageNumber="116">L. squamosum</emphasis>
