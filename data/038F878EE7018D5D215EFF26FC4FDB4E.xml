</taxonomicName>
(filled star) and the 
<taxonomicName id="4C264D1BE7018D5C219CF829FBEED12E" authorityName="Assing" authorityYear="2013" box="[935,1046,2028,2050]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="desectum">
<emphasis id="B952EA8AE7018D5C219CF829FBEED12E" box="[935,1046,2028,2050]" italics="true" pageId="62" pageNumber="115">L. desectum</emphasis>
</taxonomicName>
(open symbols) groups: 
<taxonomicName id="4C264D1BE7018D5C273EF829FA64D12E" authorityName="Assing" authorityYear="2013" box="[1285,1436,2028,2050]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="bihamulatum">
<emphasis id="B952EA8AE7018D5C273EF829FA64D12E" box="[1285,1436,2028,2050]" italics="true" pageId="62" pageNumber="115">L. bihamulatum</emphasis>
</taxonomicName>
(black star); 
<taxonomicName id="4C264D1BE7018D5C230FF7CEFE5AD131" authorityName="Assing" authorityYear="2013" box="[308,418,2057,2079]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="115" phylum="Arthropoda" rank="species" species="abscisum">
