<taxonomicName id="C7B34D08FF93FFDAFB28FB891A19FBBC" authority="Gertsch Embolus" authorityName="Gertsch Embolus" class="Arachnida" family="Leptonetidae" genus="Archoleptoneta" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="schusteri">
