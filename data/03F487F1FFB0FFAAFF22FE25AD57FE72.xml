<taxonomicName id="4C5D4D64FFB0FFA9FEABF80BAF39F828" baseAuthorityName="Goetghebuer" baseAuthorityYear="1913" box="[286,368,1980,2003]" class="Insecta" family="Chironomidae" genus="Chaetocladius" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="229" phylum="Arthropoda" rank="species" species="piger">
