<quantity id="4CA788CCE079FF91C5D7FAFDAAC11559" box="[647,736,1303,1327]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="12" pageNumber="514" unit="mm" value="4.2">4.2 mm</quantity>
