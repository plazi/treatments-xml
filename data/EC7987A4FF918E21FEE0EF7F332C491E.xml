<taxonomicName id="A3D04D31FF918E2BFEE0EF7F309A48EC" authority="Gledhill &amp; Vidrine, 2002" authorityName="Gledhill &amp; Vidrine" authorityYear="2002" box="[378,716,624,648]" class="Arachnida" family="Unionicolidae" genus="Unionicola" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="1355" phylum="Arthropoda" rank="species" species="blayi" status="new species" subGenus="Mutelicola">
