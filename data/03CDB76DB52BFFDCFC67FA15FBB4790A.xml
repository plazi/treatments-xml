<taxonomicName id="4C647DF8B52BFFDCFCF4F913FB8878DA" authority="(Westring, 1861)" baseAuthorityName="Westring" baseAuthorityYear="1861" box="[813,1083,1713,1737]" class="Arachnida" family="Lycosidae" genus="Pardosa" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="42" phylum="Arthropoda" rank="species" species="agrestis">
