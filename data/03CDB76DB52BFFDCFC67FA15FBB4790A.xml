<taxonomicName id="4C647DF8B52BFFDCFAB3F96DFA1C78F7" box="[1386,1455,1740,1764]" class="Arachnida" family="Species" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="53" phylum="Arthropoda" rank="family">species</taxonomicName>
