</treatmentCitation>
</taxonomicName>
, ex 
<taxonomicName id="6EF84D5D4C02B362FD9EF976F585D3DC" baseAuthorityName="Tottenham" baseAuthorityYear="1955" box="[610,720,1698,1720]" class="Insecta" family="Staphylinidae" genus="Philonthus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">
