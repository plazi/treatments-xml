<quantity id="4CD39B19FFB3FF98FCD48A8C47CAF80A" box="[838,927,2013,2041]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" pageId="3" pageNumber="394" unit="mm" value="6.5">6.5 mm</quantity>
