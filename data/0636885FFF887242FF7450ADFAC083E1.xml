<taxonomicName id="499F42CAFF887242FB8752B2FB2583E1" authorityName="Bolívar" authorityYear="1887" box="[1124,1192,1721,1744]" class="Insecta" family="Tetrigidae" genus="Xistra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
