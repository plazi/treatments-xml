<date id="250410153E5DFFE457D0FF12C0BFFF57" box="[966,995,181,202]" pageId="3" pageNumber="4">
<number id="E77224F23E5DFFE457D0FF12C08EFF57" box="[966,978,181,202]" pageId="3" pageNumber="4" value="2.0">2</number>
X
