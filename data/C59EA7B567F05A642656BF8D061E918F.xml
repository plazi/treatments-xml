<paragraph id="BEC8202F45FF5693FCA4B5B8374C9966" pageId="6" pageNumber="7">
<taxonomicName id="102F5E82C8A0111325C76F90ADD8A949" class="Gastropoda" family="Onchidiidae" genus="Onchidium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Onchidium typhae" order="Systellommatophora" pageId="6" pageNumber="7" phylum="Mollusca" rank="species" species="typhae">Onchidium typhae</taxonomicName>
