<taxonomicName id="6367841FFA56AF69AC2D907E4826482C" class="Actinopterygii" family="Loricariidae" genus="Kronichthys" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="8" phylum="Chordata" rank="species" species="heylandi">Kronichthys heylandi</taxonomicName>
