<taxonomicName id="B0FB5BE53D57FF8CFDBF5C7C51DDF820" box="[517,667,1915,1938]" class="Insecta" family="Phoridae" genus="Coniceromyia" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="43" phylum="Arthropoda" rank="genus">Coniceromyia</taxonomicName>
franciscana
</taxonomicName>
,
