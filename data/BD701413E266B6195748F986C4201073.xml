<collectionCode id="53C83DC0E266B61957E1F969C506100E" box="[1008,1043,1789,1810]" pageId="91" pageNumber="814">CL</collectionCode>
2.83, 
<collectionCode id="53C83DC0E266B6195048F969C596100E" box="[1113,1155,1789,1810]" pageId="91" pageNumber="814">CW</collectionCode>
2.16) to 
<taxonomicName id="F2D9DE86E266B61950F9F96AC48F100E" authorityName="BRIGNOLI" authorityYear="1976" box="[1256,1434,1789,1811]" class="Arachnida" family="Agelenidae" genus="Tegenaria" kingdom="Animalia" order="Araneae" pageId="91" pageNumber="814" phylum="Arthropoda" rank="species" species="regispyrrhi">
