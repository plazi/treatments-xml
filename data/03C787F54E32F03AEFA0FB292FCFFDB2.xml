<number id="3DA624C44E32F034EEA8F96F2E98F915" box="[415,429,1760,1784]" pageId="7" pageNumber="53" value="2.0">2</number>
<number id="3DA624C44E32F034EE84F96F2EF4F915" box="[435,449,1760,1784]" pageId="7" pageNumber="53" value="1.0">1</number>
<number id="3DA624C44E32F034EEFFF96F2EE3F915" box="[456,470,1760,1784]" pageId="7" pageNumber="53" value="4.0">4</number>
<number id="3DA624C44E32F034EEEBF96F2ED9F915" box="[476,492,1760,1784]" pageId="7" pageNumber="53" value="3.0">3</number>
. Measurements of palp and legs: Palp 
<number id="3DA624C44E32F034ECAFF96F2CFCF915" box="[920,969,1760,1784]" pageId="7" pageNumber="53" value="16.1">16.1</number>
[
<number id="3DA624C44E32F034ECE0F96F2CCAF915" box="[983,1023,1760,1784]" pageId="7" pageNumber="53" value="4.8">4.8</number>
, 
<number id="3DA624C44E32F034EB3DF96F2B05F915" box="[1034,1072,1760,1784]" pageId="7" pageNumber="53" value="2.6">2.6</number>
, 
<number id="3DA624C44E32F034EB0DF96F2B55F915" box="[1082,1120,1760,1784]" pageId="7" pageNumber="53" value="3.5">3.5</number>
