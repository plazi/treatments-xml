<taxonomicName id="C9C14D66FFCAFF84FB1BF917A9344024" authority="Chim &amp; Tong, 2019" authorityName="Chim &amp; Tong" authorityYear="2019" box="[1243,1406,1773,1799]" class="Malacostraca" family="Tanaididae" genus="Xenosinelobus" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="414" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="3CB5EAF7FFCAFF84FB1BF917A9344024" box="[1243,1406,1773,1799]" italics="true" pageId="1" pageNumber="414">Xenosinelobus</emphasis>
</taxonomicName>
