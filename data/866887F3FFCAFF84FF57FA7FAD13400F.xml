<taxonomicName id="C9C14D66FFCAFF84FD32F917AFDE4024" authorityName="Sieg" authorityYear="1980" box="[754,916,1773,1799]" class="Malacostraca" family="Tanaididae" genus="Parasinelobus" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="414" phylum="Arthropoda" rank="genus">
<emphasis id="3CB5EAF7FFCAFF84FD32F917AFDE4024" box="[754,916,1773,1799]" italics="true" pageId="1" pageNumber="414">Parasinelobus</emphasis>
</taxonomicName>
, 
<taxonomicName id="C9C14D66FFCAFF84FC62F917A8454024" authorityName="Sieg" authorityYear="1980" box="[930,1039,1773,1799]" class="Malacostraca" family="Tanaididae" genus="Sinelobus" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="414" phylum="Arthropoda" rank="genus">
<emphasis id="3CB5EAF7FFCAFF84FC62F917A8454024" box="[930,1039,1773,1799]" italics="true" pageId="1" pageNumber="414">Sinelobus</emphasis>
