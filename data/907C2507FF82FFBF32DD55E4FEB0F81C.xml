<taxonomicName id="DFD5EF92FF82FFBF366B54E6FBD1F836" box="[1057,1144,1965,1990]" class="Insecta" family="Ulidiidae" genus="Herina" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="42" phylum="Arthropoda" rank="genus">
