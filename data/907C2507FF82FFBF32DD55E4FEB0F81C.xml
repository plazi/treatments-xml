<taxonomicName id="DFD5EF92FF82FFBF33A25428FD92F88C" box="[488,571,1891,1916]" class="Insecta" family="Ulidiidae" genus="Herina" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="42" phylum="Arthropoda" rank="genus">
