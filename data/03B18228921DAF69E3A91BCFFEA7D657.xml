<taxonomicName id="4C1848BD9219AF68E2641A0AFD35D021" box="[346,671,1877,1899]" class="Insecta" family="Gomphidae" genus="Onychogomphus" kingdom="Animalia" order="Odonata" pageId="4" pageNumber="185" phylum="Arthropoda" rank="species" species="marijanmatoki">
