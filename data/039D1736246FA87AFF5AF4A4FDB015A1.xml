<quantity id="4CCC0BC5246FA87CFACDF3A3FA1E117A" box="[1280,1429,1261,1287]" metricMagnitude="2" metricUnit="m" metricValue="9.655" metricValueMax="11.809999999999999" metricValueMin="7.5" pageId="18" pageNumber="61" unit="m" value="965.5" valueMax="1181.0" valueMin="750.0">750–1181 m</quantity>
