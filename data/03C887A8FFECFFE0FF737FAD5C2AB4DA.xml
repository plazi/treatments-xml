D. Known from southwestern 
<collectingRegion id="49A5F85CFFECFFE0FCD1786D5B6FB427" box="[821,1024,1878,1903]" country="Australia" name="Western Australia" pageId="39" pageNumber="240">
<collectingRegion id="49A5F85CFFECFFE0FCD1786D5CFFB426" box="[821,912,1879,1902]" country="American Samoa" name="Western" pageId="39" pageNumber="240">Western</collectingRegion>
