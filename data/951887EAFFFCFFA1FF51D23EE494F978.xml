<taxonomicName id="DAB14D7FFFFCFFA1FE12D0D0E62BFC39" box="[468,619,968,994]" class="Hexanauplia" family="Parastenheliidae" genus="Penicillicaris" kingdom="Animalia" order="Harpacticoida" pageId="63" pageNumber="299" phylum="Arthropoda" rank="genus">
