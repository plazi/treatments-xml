<taxonomicName id="5FA1F5AACA73FFB77D1BB21010F04CB9" box="[288,430,1648,1673]" class="Maxillopoda" family="Tetraclitidae" genus="Tetraclitella" kingdom="Animalia" order="Sessilia" pageId="2" pageNumber="45" phylum="Arthropoda" rank="genus">
