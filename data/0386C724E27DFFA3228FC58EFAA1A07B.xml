<taxonomicName id="4C2F0DB1E27DFFA3228FC61BFD10A187" authority="Meiklejohn, Wallman and Pape, 2013" authorityName="Meiklejohn, Wallman and Pape" authorityYear="2013" box="[151,756,618,640]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="170" phylum="Arthropoda" rank="species" species="collessi">
