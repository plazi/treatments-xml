<subSubSection id="C354D8B9FF85FFC8D38A58738CD6FCB4" box="[151,709,793,817]" pageId="46" pageNumber="47" type="materials_examined">
<paragraph id="8BF18B32FF85FFC8D38A58738CD6FCB4" blockId="46.[151,1436,793,1645]" box="[151,709,793,817]" pageId="46" pageNumber="47">
<emphasis id="B93A5720FF85FFC8D38A58738F19FCB4" bold="true" box="[151,266,793,817]" pageId="46" pageNumber="47">Type host</emphasis>
