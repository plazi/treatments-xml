<taxonomicName id="91598B65C0DFC41AB9F17D5DB612B31A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139989" class="Insecta" family="Formicidae" genus="Rogeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Rogeria (Irogera) rugosa Mann" order="Hymenoptera" pageNumber="23" phylum="Arthropoda" rank="species" species="rugosa">Rogeria rugosa Mann</taxonomicName>
