<taxonomicName id="31F1A298C135FE146F2AF9071F582701" authorityName="Vega-Badillo, Morrone &amp; Zaragoza-Caballero" authorityYear="2021" class="Insecta" family="Phengodidae" genus="Cenophengus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cenophengus tsiik" order="Coleoptera" pageId="0" pageNumber="73" phylum="Arthropoda" rank="species" species="tsiik">Cenophengus tsiik</taxonomicName>
