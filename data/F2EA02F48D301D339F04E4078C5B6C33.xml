<subSubSection id="03060E80C86B136FAD1872C2B5784B22" pageId="null" pageNumber="180" type="nomenclature">
<paragraph id="30B5661BDC135FF2081484E72AF39DC8" pageId="null" pageNumber="180">
<taxonomicName id="C056EE0CC9C60609E5C1E905D41E9314" authority="L." class="Magnoliopsida" family="Brassicaceae" genus="Raphanus" kingdom="Plantae" order="Brassicales" pageId="null" pageNumber="180" phylum="Tracheophyta" rank="species" species="sativus">
Raphanus 
<normalizedToken id="68CDA93394FF0E0AF25AE00493F31B95" originalValue="satívus" pageId="null" pageNumber="180">sativus</normalizedToken>
<authorityName id="00EDB5045027AAB7BDD030A82DC162FB" pageId="null" pageNumber="180">L.</authorityName>
</taxonomicName>
</paragraph>
