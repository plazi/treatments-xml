<taxonomicName id="21AD0AAEFFEEFFD2FC15848E6617F9A8" box="[1009,1119,1519,1544]" class="Insecta" family="Libellulidae" genus="Erythrodiplax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="142" phylum="Arthropoda" rank="species" species="laselva">E. laselva</taxonomicName>
</emphasis>
