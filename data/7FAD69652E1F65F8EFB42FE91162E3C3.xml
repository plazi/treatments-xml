<taxonomicName id="DC3EA6434AE47ECA3AC458A530B32644" authority="Vitzthum, 1942" authorityName="Vitzthum" authorityYear="1942" class="Arachnida" family="Ameroseiidae" genus="Neocypholaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neocypholaelaps ewae" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="ewae">Neocypholaelaps ewae</taxonomicName>
