<taxonomicName id="4C574D65FF9AFFE1FD00F920FCFCF93D" box="[704,821,1719,1742]" class="Echinoidea" family="Spatangidae" genus="Spatangus" kingdom="Animalia" order="Spatangoida" pageId="61" pageNumber="62" phylum="Echinodermata" rank="genus">
