<date id="FF8FC3FC1919FFF568E793EA1C19FC7E" box="[667,824,995,1017]" pageId="38" pageNumber="38" value="1937-06-07">
<collectingDate id="EFCB3A141919FFF568E793EA1C19FC7E" box="[667,824,995,1017]" pageId="38" pageNumber="38" value="1937-06-07">7 June 1937</collectingDate>
</date>
