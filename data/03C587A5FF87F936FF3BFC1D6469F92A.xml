<taxonomicName id="4C6C4D30FF87F936FECBFA2E64A0FA2E" authority="Reeve, 1844" authorityName="Reeve" authorityYear="1844" box="[359,725,1486,1511]" class="Gastropoda" family="Turridae" genus="Pleurotoma" higherTaxonomySource="CoL" kingdom="Animalia" order="Neogastropoda" pageId="67" pageNumber="68" phylum="Mollusca" rank="species" species="lyciaca">
