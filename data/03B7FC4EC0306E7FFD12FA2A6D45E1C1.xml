<quantity id="4CE6E0BDC0306E7CFC5EF9BD6B32E61B" box="[947,1112,1596,1620]" metricMagnitude="-1" metricUnit="m" metricValue="9.05" metricValueMax="13.0" metricValueMin="5.1" pageId="13" pageNumber="338" unit="mm" value="905.0" valueMax="1300.0" valueMin="510.0">510 to 1300 mm</quantity>
