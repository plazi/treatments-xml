<quantity id="4CEF9B25FF0AFF2E9B69FA1DF91D360C" box="[788,860,1519,1545]" metricMagnitude="2" metricUnit="m" metricValue="2.62" pageId="155" pageNumber="156" unit="m" value="262.0">262 m</quantity>
