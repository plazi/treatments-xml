<number id="5D4F8E0DFFDAFFCEFDEBF982D631F98E" box="[559,575,1544,1570]" pageId="7" pageNumber="8" value="6.0">6</number>
(
<number id="5D4F8E0DFFDAFFCEFD8CF982D656F98E" box="[584,600,1544,1570]" pageId="7" pageNumber="8" value="4.0">4</number>
