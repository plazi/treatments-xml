<quantity box="[284,417,1020,1046]" metricMagnitude="2" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="19" pageNumber="20" unit="m" value="850.0" valueMax="900.0" valueMin="800.0">800–900 m</quantity>
