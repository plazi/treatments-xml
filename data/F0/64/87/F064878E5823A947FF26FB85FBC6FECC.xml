<quantity box="[339,472,1181,1207]" metricMagnitude="2" metricUnit="m" metricValue="2.0" metricValueMax="2.5" metricValueMin="1.5" pageId="17" pageNumber="18" unit="m" value="200.0" valueMax="250.0" valueMin="150.0">
