<taxonomicName authorityName="Wiegmann" authorityYear="1834" box="[1219,1335,1590,1615]" class="Reptilia" family="Liolaemidae" genus="Liolaemus" kingdom="Animalia" order="Squamata" pageId="21" pageNumber="422" phylum="Chordata" rank="genus">
