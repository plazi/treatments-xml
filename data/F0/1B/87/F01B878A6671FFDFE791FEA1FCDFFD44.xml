(Sauria: 
<taxonomicName box="[151,293,1626,1651]" class="Reptilia" family="Liolaemidae" kingdom="Animalia" order="Squamata" pageId="21" pageNumber="422" phylum="Chordata" rank="family">Liolaemidae</taxonomicName>
