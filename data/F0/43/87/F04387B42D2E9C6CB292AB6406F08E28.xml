<subSubSection pageId="1" pageNumber="62" type="etymology">
<paragraph blockId="1.[151,1436,1689,1909]" pageId="1" pageNumber="62">
<emphasis bold="true" box="[198,388,1689,1715]" pageId="1" pageNumber="62">Ethymology:—</emphasis>
