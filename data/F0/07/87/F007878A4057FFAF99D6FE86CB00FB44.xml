<taxonomicName authority="Ng, 2002" authorityName="Ng" authorityYear="2002" box="[297,630,361,383]" class="Malacostraca" family="Sesarmidae" genus="Sesarmoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="356" phylum="Arthropoda" rank="species" species="boholano">
