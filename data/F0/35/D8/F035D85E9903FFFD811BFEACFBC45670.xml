<quantity box="[551,659,930,957]" metricMagnitude="-4" metricUnit="m" metricValue="8.7" pageId="8" pageNumber="176" unit="mm" value="0.87">0.87 mm</quantity>
, PL: 2.16‒ 
<quantity box="[788,894,930,957]" metricMagnitude="-3" metricUnit="m" metricValue="1.97" pageId="8" pageNumber="176" unit="mm" value="1.97">1.97 mm</quantity>
