<subSubSection pageId="3" pageNumber="171" type="diagnosis">
<paragraph blockId="3.[189,310,1224,1250]" box="[189,310,1224,1250]" pageId="3" pageNumber="171">
<heading bold="true" box="[189,310,1224,1250]" fontSize="11" level="3" pageId="3" pageNumber="171" reason="3">
