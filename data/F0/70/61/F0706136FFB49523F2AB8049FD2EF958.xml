<subSubSection box="[151,713,656,679]" pageId="1" pageNumber="62" type="materials_examined">
<paragraph blockId="1.[151,713,656,679]" box="[151,713,656,679]" pageId="1" pageNumber="62">
<typeStatus box="[151,204,656,679]" pageId="1" pageNumber="62">Type</typeStatus>
species: 
<taxonomicName authority="Clark, 1964" authorityName="Clark" authorityYear="1964" box="[301,713,656,679]" class="Arachnida" family="Syringophilidae" genus="Syringophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="62" phylum="Arthropoda" rank="species" species="zenadourae">
<emphasis box="[301,580,656,679]" italics="true" pageId="1" pageNumber="62">Syringophilus zenadourae</emphasis>
<bibRefCitation author="Clark G. M." box="[587,713,656,679]" pageId="1" pageNumber="62" pagination="77 - 92" refId="ref2837" refString="Clark G. M. (1964) The acarine genus Syringophilus in North American birds. Acarologia, 6, 77 - 92." type="journal article" year="1964">Clark, 1964</bibRefCitation>
</taxonomicName>
</paragraph>
