(
<taxonomicName box="[384,584,1330,1356]" class="Aves" kingdom="Animalia" order="Columbiformes" pageId="4" pageNumber="65" phylum="Chordata" rank="order">Columbiformes</taxonomicName>
: 
<taxonomicName box="[598,754,1330,1356]" class="Aves" family="Columbidae" kingdom="Animalia" order="Columbiformes" pageId="4" pageNumber="65" phylum="Chordata" rank="family">Columbidae</taxonomicName>
), 
<collectingCountry box="[776,941,1330,1356]" name="Colombia" pageId="4" pageNumber="65">COLOMBIA</collectingCountry>
, 
<collectingRegion box="[955,1038,1330,1356]" country="Colombia" name="Choco" pageId="4" pageNumber="65">Choco</collectingRegion>
, 
<date box="[1052,1297,1330,1356]" pageId="4" pageNumber="65" value="1909-09-27">27 September 1909</date>
, coll. M.G. Palmer. All material is deposited at 
<collectionCode box="[570,642,1365,1391]" pageId="4" pageNumber="65">AMU</collectionCode>
