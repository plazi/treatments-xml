<collectorName box="[331,453,1188,1214]" pageId="13" pageNumber="14">M. Hauser</collectorName>
(
<collectionCode box="[469,554,1188,1214]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34264" name="University of Kansas - Biodiversity Institute" pageId="13" pageNumber="14" type="Museum">SEMC</collectionCode>
)
