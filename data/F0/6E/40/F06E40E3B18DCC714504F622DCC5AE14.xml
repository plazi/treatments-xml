<taxonomicName class="Pinopsida" family="Cupressaceae" genus="Papilionoideae" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Pinales" pageId="6" pageNumber="89" phylum="Tracheophyta" rank="genus">Papilionoideae</taxonomicName>
