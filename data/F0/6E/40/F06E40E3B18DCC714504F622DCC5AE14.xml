<subSubSection pageId="6" pageNumber="89" type="floral associations">
<paragraph pageId="6" pageNumber="89">Floral associations.</paragraph>
<paragraph pageId="6" pageNumber="89">
<taxonomicName genus="Campanulaceae" lsidName="" pageId="6" pageNumber="89" rank="genus">Campanulaceae</taxonomicName>
(
<taxonomicName class="Dicotyledoneae" family="Campanulaceae" genus="Wahlenbergia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Campanulales" pageId="6" pageNumber="89" phylum="Angiospermae" rank="genus">
<emphasis italics="true" pageId="6" pageNumber="89">Wahlenbergia</emphasis>
</taxonomicName>
