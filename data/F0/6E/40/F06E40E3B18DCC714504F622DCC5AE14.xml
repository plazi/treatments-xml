<taxonomicName class="Dicotyledoneae" family="Campanulaceae" genus="Wahlenbergia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Campanulales" pageId="6" pageNumber="89" phylum="Angiospermae" rank="genus">
