<number box="[669,699,260,284]" pageId="10" pageNumber="115" value="49.0">49</number>
, 
<emphasis box="[711,798,260,284]" italics="true" pageId="10" pageNumber="115">
ps 
<number box="[739,752,260,284]" pageId="10" pageNumber="115" value="2.0">2</number>
