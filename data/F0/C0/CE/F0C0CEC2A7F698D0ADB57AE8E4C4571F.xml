<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia" order="Fabales" pageId="25" pageNumber="118" phylum="Tracheophyta" rank="genus">Acacia</taxonomicName>
