<paragraph pageId="null" pageNumber="44">
<taxonomicName authority="L." class="Magnoliopsida" family="Ranunculaceae" genus="Thalictrum" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="44" phylum="Tracheophyta" rank="species" species="minus">
