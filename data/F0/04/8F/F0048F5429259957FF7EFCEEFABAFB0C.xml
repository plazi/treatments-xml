<emphasis box="[1137,1154,1181,1207]" italics="true" pageId="1" pageNumber="489">A</emphasis>
. 
<emphasis box="[1169,1265,1181,1207]" italics="true" pageId="1" pageNumber="489">bayoumi</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Metwally &amp; Ibrahim" authorityYear="1985" box="[1280,1356,1181,1207]" class="Arachnida" family="Laelapidae" genus="Androlaelaps" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="489" phylum="Arthropoda" rank="species" species="kifli">
<emphasis box="[1280,1297,1181,1207]" italics="true" pageId="1" pageNumber="489">A</emphasis>
. 
<emphasis box="[1312,1356,1181,1207]" italics="true" pageId="1" pageNumber="489">kifli</emphasis>
</taxonomicName>
