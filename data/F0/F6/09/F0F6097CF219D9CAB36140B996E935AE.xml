<subSubSection pageId="75" pageNumber="76" type="type locality">
<paragraph pageId="75" pageNumber="76">Type locality.</paragraph>
<paragraph pageId="75" pageNumber="76">[Ecuador] &quot;insulis Gallapagos&quot;.</paragraph>
</subSubSection>
<subSubSection pageId="75" pageNumber="76" type="label">
<paragraph pageId="75" pageNumber="76">Label.</paragraph>
<paragraph pageId="75" pageNumber="76">
<normalizedToken originalValue="”Brazils”">&quot;Brazils&quot;</normalizedToken>
, 
<normalizedToken originalValue="”Galapagos”">&quot;Galapagos&quot;</normalizedToken>
