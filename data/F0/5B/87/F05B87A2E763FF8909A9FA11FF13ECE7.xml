<taxonomicName box="[538,680,1916,1942]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName box="[692,820,1916,1943]" class="Insecta" family="Crambidae" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="family">Crambidae</taxonomicName>
), 
<taxonomicName authority="(Boddie, 1850)" authorityName="Boddie" authorityYear="1850" baseAuthorityName="Boddie" baseAuthorityYear="1850" box="[840,1208,1916,1943]" class="Insecta" family="Noctuidae" genus="Helicoverpa" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="zea">
<emphasis box="[840,1024,1916,1942]" italics="true" pageId="8" pageNumber="209">Helicoverpa zea</emphasis>
(Boddie, 1850)
</taxonomicName>
, 
<taxonomicName authority="(Fabricius, 1777)" authorityName="Fabricius" authorityYear="1777" baseAuthorityName="Fabricius" baseAuthorityYear="1777" class="Insecta" family="Crambidae" genus="Heliothis" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="virescens">
<emphasis box="[1221,1436,1916,1942]" italics="true" pageId="8" pageNumber="209">Heliothis virescens</emphasis>
(Fabricius, 1777)
</taxonomicName>
, and 
<taxonomicName authority="Riley, 1885" authorityName="Riley" authorityYear="1885" box="[405,707,1952,1979]" class="Insecta" family="Noctuidae" genus="Anomis" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="texana">
<emphasis box="[405,571,1953,1979]" italics="true" pageId="8" pageNumber="209">Anomis texana</emphasis>
Riley, 1885
</taxonomicName>
(
<taxonomicName box="[721,863,1952,1978]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName box="[873,990,1952,1979]" class="Insecta" family="Noctuidae" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="family">Noctuidae</taxonomicName>
) (
<bibRefCitation author="Whu, M. &amp; Valdivieso, L." box="[1012,1289,1952,1979]" pageId="8" pageNumber="209" pagination="61 - 68" refId="ref19828" refString="Whu, M. &amp; Valdivieso, L. (1999) Distribucion y comportamiento de ocho especies de Trichogramma y Trichogrammatoidea (Hymenoptera: Trichogrammatidae) en el Peru. Revista Peruana de Entomologia, 41, 61 - 68." type="journal article" year="1999">Whu &amp; Valdivieso 1999</bibRefCitation>
).
</paragraph>
