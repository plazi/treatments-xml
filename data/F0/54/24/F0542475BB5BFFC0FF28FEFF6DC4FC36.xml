S 
<collectingCountry box="[480,578,993,1018]" name="Vietnam" pageId="13" pageNumber="414">Vietnam</collectingCountry>
, E 
<collectingCountry box="[613,715,993,1018]" name="Thailand" pageId="13" pageNumber="414">Thailand</collectingCountry>
, 
<collectingCountry box="[726,832,993,1018]" name="Malaysia" pageId="13" pageNumber="414">Malaysia</collectingCountry>
, Borneo, 
<collectingRegion box="[940,992,994,1017]" country="Indonesia" name="Jawa Timur" pageId="13" pageNumber="414">Java</collectingRegion>
, S 
<collectingRegion box="[1026,1127,993,1018]" country="Indonesia" name="Sulawesi Utara" pageId="13" pageNumber="414">Sulawesi</collectingRegion>
(
<bibRefCitation author="Reid" box="[1142,1264,993,1018]" pageId="13" pageNumber="414" refString="Reid, D. G. (2014) The genus Cerithidea Swainson, 1840 (Gastropoda: Potamididae) in the Indo-West Pacific region. Zootaxa, 3775 (1), 1 - 65. http: // dx. doi. org / 10.11646 / zootaxa. 3775.1.1" type="journal article" year="2014">Reid 2014</bibRefCitation>
