<subSubSection pageId="29" pageNumber="430" type="description">
<paragraph blockId="29.[151,1436,1418,1731]" box="[199,861,1490,1515]" pageId="29" pageNumber="430">
<emphasis bold="true" box="[199,381,1490,1515]" pageId="29" pageNumber="430">Measurements.</emphasis>
