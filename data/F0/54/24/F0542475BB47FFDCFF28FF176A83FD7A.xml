<subSubSection pageId="17" pageNumber="418" type="description">
<paragraph blockId="17.[151,1242,417,694]" box="[199,723,489,514]" pageId="17" pageNumber="418">
<emphasis bold="true" box="[199,381,489,514]" pageId="17" pageNumber="418">Measurements.</emphasis>
