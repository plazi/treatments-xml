<paragraph pageId="null" pageNumber="383">
<taxonomicName authority="Host" class="Liliopsida" family="Poaceae" genus="Aegilops" kingdom="Plantae" order="Poales" pageId="null" pageNumber="383" phylum="Tracheophyta" rank="species" species="cylindrica">
