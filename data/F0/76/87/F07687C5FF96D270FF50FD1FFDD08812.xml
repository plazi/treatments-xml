<collectingCountry box="[943,1011,945,971]" name="Brazil" pageId="9" pageNumber="310">Brazil</collectingCountry>
(from 
<collectingRegion box="[1088,1153,945,971]" country="Brazil" name="Ceara" pageId="9" pageNumber="310">Ceará</collectingRegion>
to 
<collectingRegion box="[1188,1354,945,972]" country="Brazil" name="Santa Catarina" pageId="9" pageNumber="310">Santa Catarina</collectingRegion>
) (
<bibRefCitation author="Melo, G. A. S." pageId="9" pageNumber="310" refId="ref27684" refString="Melo, G. A. S. (1999) Manual de Identificacao dos Crustacea Decapoda do Litoral Brasileiro: Anomura; Thalassinidea; Palinuridea e Astacidea. Pleiade / FAPESP, Sao Paulo, 551 pp." type="book" year="1999">Melo 1999</bibRefCitation>
; 
<bibRefCitation author="Nucci, P. R. &amp; Melo, G. A. S." box="[222,452,981,1007]" pageId="9" pageNumber="310" pagination="141 - 147" refId="ref28491" refString="Nucci, P. R. &amp; Melo, G. A. S. (2000) Range extensions for eight species of western Atlantic hermit crabs (Crustacea, Paguroidea). Nauplius, 8 (1), 141 - 147." type="journal article" year="2000">Nucci &amp; Melo 2000</bibRefCitation>
).
</paragraph>
<paragraph blockId="9.[151,1437,837,1116]" pageId="9" pageNumber="310">
<emphasis bold="true" box="[199,398,1017,1044]" pageId="9" pageNumber="310">Ecological notes.</emphasis>
