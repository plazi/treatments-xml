<quantity box="[994,1111,693,719]" metricMagnitude="-2" metricUnit="m" metricValue="2.001" pageId="21" pageNumber="322" unit="mm" value="20.01">20.01 mm</quantity>
; average: CW = 14.77 ± 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="7.4" pageId="21" pageNumber="322" unit="mm" value="7.4">7.40 mm</quantity>
; 
<collectionCode box="[210,297,729,755]" pageId="21" pageNumber="322">CZUFS</collectionCode>
<collectionCode box="[304,373,729,755]" pageId="21" pageNumber="322">CRU-</collectionCode>
