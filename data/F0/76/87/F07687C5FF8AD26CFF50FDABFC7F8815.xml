<quantity box="[993,1110,657,683]" metricMagnitude="-2" metricUnit="m" metricValue="2.882" pageId="21" pageNumber="322" unit="mm" value="28.82">28.82 mm</quantity>
; average: CW = 28.36 ± 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="6.5" pageId="21" pageNumber="322" unit="mm" value="0.65">0.65 mm</quantity>
; 
<collectionCode box="[209,296,693,719]" pageId="21" pageNumber="322">CZUFS</collectionCode>
<collectionCode box="[303,372,693,719]" pageId="21" pageNumber="322">CRU-</collectionCode>
