<collectingCountry box="[1172,1240,1772,1798]" name="Brazil" pageId="5" pageNumber="306">Brazil</collectingCountry>
(from 
<collectingRegion box="[1323,1404,1773,1799]" country="Brazil" name="Amapa" pageId="5" pageNumber="306">Amapá</collectingRegion>
to 
<collectingRegion box="[151,317,1808,1835]" country="Brazil" name="Santa Catarina" pageId="5" pageNumber="306">Santa Catarina</collectingRegion>
) (
<bibRefCitation author="D'Incao, F." box="[339,499,1808,1835]" pageId="5" pageNumber="306" pagination="101 - 125" refId="ref23445" refString="D'Incao, F. (1995) Brazillian rock shrimps of the genus Sicyonia (Decapoda: Sicyoniidae). Nauplius, 3, 101 - 125." type="journal article" year="1995">D’Incao 1995</bibRefCitation>
, 
<bibRefCitation author="D'Incao, F." box="[510,569,1809,1835]" pageId="5" pageNumber="306" pagination="311 - 321" refId="ref23473" refString="D'Incao, F. (1998) Malacostraca-Eucarida-Dendrobranchiata. In: Young, P. S. (Ed.), Catalogue of Crustacea of Brazil. Serie Livros 6. Museu nacional do Rio de Janeiro, Rio de Janeiro, pp. 311 - 321." type="book chapter" year="1998">1998</bibRefCitation>
; 
<bibRefCitation author="Costa, R. C. &amp; Fransozo, A. &amp; Melo, G. A. &amp; Freire, F. A. M." box="[580,775,1808,1835]" pageId="5" pageNumber="306" pagination="1 - 12" refId="ref23245" refString="Costa, R. C., Fransozo, A., Melo, G. A. &amp; Freire, F. A. M. (2003) Chave ilustrada para identificacao de camaroes Dendrobranchiata do litoral norte do estado de Sao Paulo, Brasil. Biota Neotropica, 3 (1), 1 - 12. https: // doi. org / 10.1590 / S 1676 - 06032003000100011" type="journal article" year="2003">
