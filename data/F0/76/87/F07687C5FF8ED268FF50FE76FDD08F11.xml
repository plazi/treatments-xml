<taxonomicName authority="Rathbun" authorityName="Rathbun" authorityYear="1897" box="[907,1135,764,790]" class="Malacostraca" family="Inachidae" genus="Ericerodes" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="318" phylum="Arthropoda" rank="genus">
<emphasis box="[907,1029,764,790]" italics="true" pageId="17" pageNumber="318">Ericerodes</emphasis>
