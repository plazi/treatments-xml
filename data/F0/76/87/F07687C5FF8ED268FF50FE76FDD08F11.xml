<collectingRegion box="[727,792,621,647]" country="Brazil" name="Ceara" pageId="17" pageNumber="318">Ceará</collectingRegion>
to 
<collectingRegion box="[832,900,620,646]" country="Brazil" name="Bahia" pageId="17" pageNumber="318">Bahia</collectingRegion>
