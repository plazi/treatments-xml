<quantity box="[988,1105,909,936]" metricMagnitude="-2" metricUnit="m" metricValue="2.5469999999999997" pageId="14" pageNumber="315" unit="mm" value="25.47">25.47 mm</quantity>
; average: CW = 23.09 ± 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.37" pageId="14" pageNumber="315" unit="mm" value="3.37">3.37 mm</quantity>
; 
<collectionCode box="[211,298,945,971]" pageId="14" pageNumber="315">CZUFS</collectionCode>
<collectionCode box="[306,375,945,971]" pageId="14" pageNumber="315">CRU-</collectionCode>
00081. Penaeid—24 (10 M, 12 F, 2 
<collectionCode box="[785,825,945,971]" pageId="14" pageNumber="315">OF</collectionCode>
); size range: 4.59 ≤ CW ≤ 
<quantity box="[1133,1252,945,972]" metricMagnitude="-2" metricUnit="m" metricValue="6.659" pageId="14" pageNumber="315" unit="mm" value="66.59">66.59 mm</quantity>
; average: CW = 
<quantity box="[151,361,981,1008]" metricMagnitude="-2" metricUnit="m" metricValue="1.731" metricValueMax="1.742" metricValueMin="1.72" pageId="14" pageNumber="315" unit="mm" value="17.31" valueMax="17.42" valueMin="17.2">17.20 ± 17.42 mm</quantity>
