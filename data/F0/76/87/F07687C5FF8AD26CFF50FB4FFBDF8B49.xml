<quantity box="[970,1100,1304,1331]" metricMagnitude="-1" metricUnit="m" metricValue="1.0065" pageId="21" pageNumber="322" unit="mm" value="100.65">100.65 mm</quantity>
; average: CW = 88.99 ± 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="1.88" pageId="21" pageNumber="322" unit="mm" value="18.8">18.80 mm</quantity>
; 
<collectionCode box="[211,298,1341,1367]" pageId="21" pageNumber="322">CZUFS</collectionCode>
<collectionCode box="[307,375,1341,1367]" pageId="21" pageNumber="322">CRU-</collectionCode>
00019. Penaeid—4 (2 M, 2 F); size range: 16.71 ≤ CW ≤ 
<quantity box="[1037,1156,1340,1367]" metricMagnitude="-2" metricUnit="m" metricValue="5.543" pageId="21" pageNumber="322" unit="mm" value="55.43">55.43 mm</quantity>
; average: CW = 34.99 ± 
<quantity box="[151,268,1377,1403]" metricMagnitude="-2" metricUnit="m" metricValue="1.9989999999999999" pageId="21" pageNumber="322" unit="mm" value="19.99">19.99 mm</quantity>
; 
<collectionCode box="[280,367,1377,1403]" pageId="21" pageNumber="322">CZUFS</collectionCode>
<collectionCode box="[374,442,1377,1403]" pageId="21" pageNumber="322">CRU-</collectionCode>
