<collectingCountry box="[1175,1243,1017,1043]" name="Brazil" pageId="20" pageNumber="321">Brazil</collectingCountry>
(from 
<collectingRegion box="[1324,1405,1017,1043]" country="Brazil" name="Amapa" pageId="20" pageNumber="321">Amapá</collectingRegion>
