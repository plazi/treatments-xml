<collectingCountry box="[969,1057,436,462]" name="Mexico" pageId="30" pageNumber="331">Mexico</collectingCountry>
, 
<collectingCountry box="[1069,1161,436,462]" name="Panama" pageId="30" pageNumber="331">Panamá</collectingCountry>
, West Indies, 
<collectingCountry box="[1319,1432,436,462]" name="Colombia" pageId="30" pageNumber="331">Colombia</collectingCountry>
, 
<collectingCountry box="[151,269,471,497]" name="Venezuela" pageId="30" pageNumber="331">Venezuela</collectingCountry>
, and 
<collectingCountry box="[333,401,471,497]" name="Brazil" pageId="30" pageNumber="331">Brazil</collectingCountry>
(Atol das Rocas, seamounts of North Brazilian Chain, and from 
<collectingRegion box="[1156,1237,471,497]" country="Brazil" name="Amapa" pageId="30" pageNumber="331">Amapá</collectingRegion>
to 
<collectingRegion box="[1278,1396,471,498]" country="Brazil" name="Sao Paulo" pageId="30" pageNumber="331">São Paulo</collectingRegion>
) as 
<taxonomicName authority="Kingsley" authorityName="Kingsley" box="[151,549,505,531]" class="Malacostraca" family="Palaemonidae" genus="Periclimenes" kingdom="Animalia" order="Decapoda" pageId="30" pageNumber="331" phylum="Arthropoda" rank="species" species="americanus">
