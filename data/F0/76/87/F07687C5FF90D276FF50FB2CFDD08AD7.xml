<collectingCountry box="[477,545,1446,1472]" name="Brazil" pageId="15" pageNumber="316">Brazil</collectingCountry>
(
<collectingRegion box="[561,650,1446,1472]" country="Brazil" name="Paraiba" pageId="15" pageNumber="316">Paraíba</collectingRegion>
