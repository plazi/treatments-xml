<collectingCountry box="[965,1053,1410,1436]" name="Mexico" pageId="15" pageNumber="316">Mexico</collectingCountry>
, West Indies, 
<collectingCountry box="[1209,1322,1410,1436]" name="Colombia" pageId="15" pageNumber="316">Colombia</collectingCountry>
