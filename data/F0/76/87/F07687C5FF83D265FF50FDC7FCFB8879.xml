<emphasis box="[151,233,873,899]" italics="true" pageId="28" pageNumber="329">Hytella</emphasis>
Férussac
</taxonomicName>
, 
<taxonomicName authority="Topsent" authorityName="Topsent" authorityYear="1892" box="[359,623,873,899]" class="Demospongiae" family="Coelosphaeridae" genus="Lissodendoryx" kingdom="Animalia" order="Poecilosclerida" pageId="28" pageNumber="329" phylum="Porifera" rank="genus">
