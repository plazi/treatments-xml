<collectingRegion box="[904,985,225,251]" country="Brazil" name="Amapa" pageId="15" pageNumber="316">Amapá</collectingRegion>
to 
<collectingRegion box="[1024,1119,225,251]" country="Brazil" name="Alagoas" pageId="15" pageNumber="316">Alagoas</collectingRegion>
