<taxonomicName baseAuthorityName="Stimpson" baseAuthorityYear="1871" box="[1073,1231,298,323]" class="Malacostraca" family="Portunidae" genus="Achelous" kingdom="Animalia" order="Decapoda" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="spinicarpus">
