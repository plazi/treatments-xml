<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24915" class="Insecta" family="Formicidae" genus="Rogeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Rogeria Emery" order="Hymenoptera" pageNumber="91" phylum="Arthropoda" rank="genus">Rogeria</taxonomicName>
