<subSubSection id="1FCEB2D18C48FFD65CDBB645FB135D22" box="[199,1182,1338,1363]" pageId="7" pageNumber="386" type="etymology">
<paragraph id="576BE15A8C48FFD65CDBB645FB135D22" blockId="7.[151,1437,1086,2011]" box="[199,1182,1338,1363]" pageId="7" pageNumber="386">
<emphasis id="65A03D488C48FFD65CDBB645FEC35D22" bold="true" box="[199,334,1338,1363]" pageId="7" pageNumber="386">Etymology.</emphasis>
