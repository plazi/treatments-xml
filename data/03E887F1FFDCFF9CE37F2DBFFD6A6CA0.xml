<taxonomicName id="4C414D64FFDDFF9BE0FC2B23FD83684F" authority="(Willd.) M. Kato. A. Petiole" baseAuthorityName="Willd." box="[184,561,1863,1882]" class="Polypodiopsida" family="Athyriaceae" genus="Deparia" kingdom="Plantae" order="Polypodiales" pageId="3" pageNumber="360" phylum="Tracheophyta" rank="species" species="boryana">
