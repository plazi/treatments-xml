<taxonomicName id="4C364D1DFFC0FFE8AB7DF89213FC4722" authorityName="Aiton" authorityYear="2010" box="[617,713,1889,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="pinaster">
<emphasis id="B942EA8CFFC0FFE8AB7DF89213FC4722" box="[617,713,1889,1914]" italics="true" pageId="32" pageNumber="33">pinaster</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C364D1DFFC0FFE8ABC3F892122D4722" baseAuthorityName="Mordvilko" baseAuthorityYear="1895" box="[727,792,1889,1914]" class="Insecta" family="Aphididae" genus="Cinara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="pinea">
<emphasis id="B942EA8CFFC0FFE8ABC3F892122D4722" box="[727,792,1889,1914]" italics="true" pageId="32" pageNumber="33">pinea</emphasis>
</taxonomicName>
, 
<emphasis id="B942EA8CFFC0FFE8AA33F89312A94722" box="[807,924,1888,1914]" italics="true" pageId="32" pageNumber="33">
<taxonomicName id="4C364D1DFFC0FFE8AA33F89312AD4722" box="[807,920,1888,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="sylvestris">sylvestris</taxonomicName>
.
</emphasis>
</paragraph>
