: 
<taxonomicName id="4C364D1DFFC0FFE8A821F892108A4722" box="[309,447,1889,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="nigra">
<emphasis id="B942EA8CFFC0FFE8A821F892108A4722" box="[309,447,1889,1914]" italics="true" pageId="32" pageNumber="33">Pinus nigra</emphasis>
</taxonomicName>
, rarely 
<taxonomicName id="4C364D1DFFC0FFE8AB0DF892136F4722" box="[537,602,1889,1914]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="mugo">
<emphasis id="B942EA8CFFC0FFE8AB0DF892136F4722" box="[537,602,1889,1914]" italics="true" pageId="32" pageNumber="33">mugo</emphasis>
</taxonomicName>
