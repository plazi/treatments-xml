<taxonomicName id="7ED84D58FFB87746BDD3A1CE9DE18E2E" box="[1012,1167,1006,1031]" class="Aves" family="Scolopacidae" kingdom="Animalia" order="Charadriiformes" pageId="84" pageNumber="85" phylum="Chordata" rank="family">Scolopacidae</taxonomicName>
