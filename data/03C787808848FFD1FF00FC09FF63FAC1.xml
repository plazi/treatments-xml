<taxonomicName id="4C6E4D158848FFD1FDEAFA91FD6CFB55" box="[637,742,1212,1236]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="ovatus">
