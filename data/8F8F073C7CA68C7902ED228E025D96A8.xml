<taxonomicName id="8C4E4E5034CA70D6285BEA8BEA29DB8B" LSID="urn:lsid:zoobank.org:act:FBC72B91-DE34-49D0-9918-A486421C9F48" class="Insecta" family="Staphylinidae" genus="Lathrobium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lathrobium uncum" order="Coleoptera" pageId="3" pageNumber="24" phylum="Arthropoda" rank="species" species="uncum">Lathrobium uncum</taxonomicName>
