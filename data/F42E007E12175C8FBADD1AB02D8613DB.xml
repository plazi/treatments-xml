<geoCoordinate id="2397DFCF79D9575E1F02F1AA7EA1539E" degrees="105" direction="east" minutes="42" orientation="longitude" precision="1" seconds="52.3" value="105.71452">105°42'52.3&quot;E</geoCoordinate>
