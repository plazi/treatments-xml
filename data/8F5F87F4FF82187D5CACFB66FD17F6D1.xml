e, 
<number id="B13E24C5FF82187F5D47FB5DFE5DF032" box="[380,394,1268,1292]" pageId="9" pageNumber="49" value="5.0">5</number>
a, 
<number id="B13E24C5FF82187F5D9FFB5DFE64F032" box="[420,435,1268,1292]" pageId="9" pageNumber="49" value="6.0">6</number>
