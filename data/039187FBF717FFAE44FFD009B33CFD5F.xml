<quantity id="4CC09B08F717FFAE4733D352B2E9FDCF" box="[859,959,535,559]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" pageId="9" pageNumber="102" unit="mm" value="12.5">12.5 mm</quantity>
