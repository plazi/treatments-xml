<paragraph id="8B8736EDF717FFAE44FFD0B1B0F0FDB4" blockId="9.[151,1437,500,703]" pageId="9" pageNumber="102">
<taxonomicName id="4C384D6EF717FFAE44FFD0B1B0CFFDEC" box="[151,409,500,524]" class="Insecta" family="Reduviidae" genus="Rhyparoclopius" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="9" pageNumber="102" phylum="Arthropoda" rank="species" species="dubius">
