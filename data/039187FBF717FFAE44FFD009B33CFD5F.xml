<quantity id="4CC09B08F717FFAE4150D352B4CBFDCF" box="[1336,1437,535,559]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" pageId="9" pageNumber="102" unit="mm" value="13.5">13.5 mm</quantity>
in length (
<bibRefCitation id="EFA94B1CF717FFAE4560D379B0CFFDB4" author="Barber" box="[264,409,572,596]" pageId="9" pageNumber="102" refString="Barber, H. G. (1930) Essay on the subfamily Stenopodinae of the New World. Entomologica Americana, n. s., 10, 149 - 238." type="journal article" year="1930">
Barber 
<number id="3DF024CAF717FFAE4535D379B0CFFDB4" box="[349,409,572,596]" pageId="9" pageNumber="102" value="1930.0">1930</number>
</bibRefCitation>
).
</paragraph>
