</quantity>
between 
<location id="8E813FBCAF795D50FE45AACD74FFDE92" LSID="urn:lsid:plazi:treatment:03F7D871AF785D50FF10AD8177AFDFA7:8E813FBCAF795D50FE45AACD74FFDE92" box="[477,659,1166,1189]" country="Costa Rica" name="Volcanoes Barba" pageId="7" pageNumber="114">Volcanoes Barba</location>
