<taxonomicName id="4C5E12E4AF795D50FE83AAED7707DEF2" box="[283,363,1198,1221]" class="Sordariomycetes" family="Xylariaceae" genus="Xylaria" kingdom="Fungi" order="Xylariales" pageId="7" pageNumber="113" phylum="Ascomycota" rank="genus">Xylaria</taxonomicName>
