<quantity id="4CA6C482AF795D50FDEDAA0D7560DE52" box="[629,780,1102,1125]" metricMagnitude="3" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="7" pageNumber="114" unit="m" value="1650.0" valueMax="1800.0" valueMin="1500.0">
