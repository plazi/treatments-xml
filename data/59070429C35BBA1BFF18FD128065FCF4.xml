</taxonomicName>
<taxonomicName id="16AECEBCC35BBA1BFF71FD12832FFD07" authority="Goeze, 1788: 345" authorityName="Goeze" authorityPageNumber="345" authorityYear="1788" box="[254,563,687,713]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="lanatus">
