<quantity id="4CF79B33FFBEFF97C05CFCE6FDCAFCBF" box="[549,604,831,851]" metricMagnitude="2" metricUnit="m" metricValue="2.2" pageId="81" pageNumber="236" unit="m" value="220.0">220m</quantity>
, 7757/4, 
<date id="FFB11016FFBEFF97C0C2FCE6FCB2FCBF" box="[699,804,831,851]" pageId="81" pageNumber="236" value="2006-10-27">27.10.2006</date>
