<heading id="D0D30E33FFCE2E5CFF5EFAC7FDE0FAE3" bold="true" box="[151,562,1357,1384]" fontSize="11" level="1" pageId="4" pageNumber="371" reason="1">
<emphasis id="B950654DFFCE2E5CFF5EFAC7FDE0FAE3" bold="true" box="[151,562,1357,1384]" pageId="4" pageNumber="371">
Genus 
<taxonomicName id="4C24C2DCFFCE2E5CFF27FAC4FE6FFAEC" box="[238,445,1357,1383]" class="Arachnida" family="Torrenticolidae" genus="Stygotorrenticola" kingdom="Animalia" order="Trombidiformes" pageId="4" pageNumber="371" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B950654DFFCE2E5CFF27FAC4FE6FFAEC" bold="true" box="[238,445,1357,1383]" italics="true" pageId="4" pageNumber="371">Stygotorrenticola</emphasis>
