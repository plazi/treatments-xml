<taxonomicName id="A6A8ED32ABC827F8D50A20E1EBDCA81E" authority="Hill" authorityName="Hill" class="Magnoliopsida" family="Fabaceae" genus="Cracca" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="species" species="villosa">Cracca villosa</taxonomicName>
