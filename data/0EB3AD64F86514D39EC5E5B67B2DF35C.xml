<paragraph id="F0FFE8FCF2B4166348CDAF48D5024A07" pageId="43" pageNumber="66">
<taxonomicName id="8A3F462A93245A7959A39CB5CE8F5995" class="Insecta" family="Carabidae" genus="Graphipterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Graphipterus valdanii" order="Coleoptera" pageId="43" pageNumber="66" phylum="Arthropoda" rank="species" species="valdanii">Graphipterus valdanii</taxonomicName>
