<taxonomicName id="4C4D4D6AE45FEE54FF4EFF6FFA4952F1" authority="(Thomson 1862)" baseAuthorityName="Thomson" baseAuthorityYear="1862" box="[144,603,151,178]" class="Insecta" family="Leiodidae" genus="Agathidium" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="118" phylum="Arthropoda" rank="species" species="arcticum">
<emphasis id="B939EAFBE45FEE54FF4EFF6FF99D52F1" bold="true" box="[144,399,151,178]" italics="true" pageId="7" pageNumber="118">Agathidium arcticum</emphasis>
(Thomson 1862)
</taxonomicName>
(
<taxonomicName id="4C4D4D6AE45FEE54FDB4FF60FAF552F1" authorityName="Fleming" authorityYear="1821" box="[618,743,152,178]" class="Insecta" family="Leiodidae" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="118" phylum="Arthropoda" rank="family">Leiodidae</taxonomicName>
