<taxonomicName id="8FEE4D41FFADFF86FDB8F8F16781CC37" authorityName="Filippova" authorityYear="1957" box="[552,706,1878,1904]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="stromi">
