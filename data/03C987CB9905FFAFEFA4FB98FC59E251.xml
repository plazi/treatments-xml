, 
<collectingCountry id="F377764D9905FFA8EEE2F89BFE79E451" box="[390,468,1890,1914]" name="Brazil" pageId="3" pageNumber="2">Brazil</collectingCountry>
, 
<collectingCountry id="F377764D9905FFA8EE86F89BFD8AE451" box="[482,551,1890,1914]" name="Chile" pageId="3" pageNumber="2">Chile</collectingCountry>
, 
<collectingCountry id="F377764D9905FFA8ED51F89BFD10E451" box="[565,701,1890,1914]" name="Costa Rica" pageId="3" pageNumber="2">Costa Rica</collectingCountry>
, 
<collectingCountry id="F377764D9905FFA8EDA8F89BFCBDE451" box="[716,784,1890,1914]" name="Cuba" pageId="3" pageNumber="2">Cuba</collectingCountry>
, 
<collectingCountry id="F377764D9905FFA8EC7AF89BFC25E451" box="[798,904,1890,1914]" name="Ecuador" pageId="3" pageNumber="2">Ecuador</collectingCountry>
, 
<collectingCountry id="F377764D9905FFA8ECF1F89BFB8CE451" box="[917,1057,1890,1914]" name="Guatemala" pageId="3" pageNumber="2">Guatemala</collectingCountry>
, 
<collectingCountry id="F377764D9905FFA8EB4BF89BFB01E451" box="[1071,1196,1890,1914]" name="Honduras" pageId="3" pageNumber="2">Honduras</collectingCountry>
