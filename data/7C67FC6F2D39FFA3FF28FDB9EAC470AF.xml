</taxonomicName>
, 
<taxonomicName id="33CE36FA2D39FFA2FD93FCB5EDF672D0" authorityName="Fibiger" authorityYear="2010" box="[556,686,1481,1506]" class="Insecta" family="Erebidae" genus="Asylemissa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
