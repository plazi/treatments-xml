<taxonomicName id="33CE36FA2D39FFA2FC9DFCB5ECDF72D0" authorityName="Fibiger" authorityYear="2010" box="[802,903,1481,1506]" class="Insecta" family="Erebidae" genus="Obscura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
