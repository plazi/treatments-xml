<quantity id="4CE39B1AFF9EFF83FC786409BA92FEBB" box="[957,1018,258,282]" metricMagnitude="1" metricUnit="m" metricValue="1.3" pageId="9" pageNumber="412" unit="m" value="13.0">13 m</quantity>
