<paragraph id="E544903D21352F6DFF6F4D686E4EFE67" blockId="30.[189,1401,311,408]" pageId="30" pageNumber="31">
MEASUREMENTS. 
<typeStatus id="3A402E9F21352F6DFEAD4D676BB5FEA1" box="[383,495,311,337]" pageId="30" pageNumber="31">Holotype</typeStatus>
, total body length 
<quantity id="22033DD821352F6DFD034D676965FEA2" box="[721,831,311,338]" metricMagnitude="-3" metricUnit="m" metricValue="3.55" pageId="30" pageNumber="31" unit="mm" value="3.55">3.55 mm</quantity>
; head 
<quantity id="22033DD821352F6DFC5C4D6769B0FEA2" box="[910,1002,311,338]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="30" pageNumber="31" unit="mm" value="0.7">0.7 mm</quantity>
long, across eyes 
<quantity id="22033DD821352F6DFB154D676F79FEA2" box="[1223,1315,311,338]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="30" pageNumber="31" unit="mm" value="0.8">0.8 mm</quantity>
broad, pronotum 
<quantity id="22033DD821352F6DFEEA4D0A6BFBFE85" box="[312,417,346,373]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="30" pageNumber="31" unit="mm" value="0.85">0.85 mm</quantity>
long, maximum width 
<quantity id="22033DD821352F6DFD614D0A694AFE85" box="[691,784,346,373]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="30" pageNumber="31" unit="mm" value="0.7">0.7 mm</quantity>
, minimum width 
<quantity id="22033DD821352F6DFC0D4D0A6E66FE85" box="[991,1084,346,373]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="30" pageNumber="31" unit="mm" value="0.3">0.3 mm</quantity>
, elytra 
<quantity id="22033DD821352F6DFB434D0A6E8FFE85" box="[1169,1237,346,373]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="30" pageNumber="31" unit="mm" value="2.0">2 mm</quantity>
long, 
<quantity id="22033DD821352F6DFACD4D0A6F23FE85" box="[1311,1401,346,373]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="30" pageNumber="31" unit="mm" value="1.5">1.5 mm</quantity>
