<materialsCitation id="3B6C5873FF9BFFB5C9F5F987FA24B23A" box="[189,1434,1604,1631]" collectingDate="2012-10-11" collectorName="O. L. M. Silva &amp; M. Pastore" country="Brazil" location="Eldorado" pageId="14" pageNumber="207" specimenCount="1" stateProvince="Sao Paulo">
<emphasis id="B9708E3CFF9BFFB5C9F5F987FDBFB23A" bold="true" box="[189,513,1604,1630]" pageId="14" pageNumber="207">Representative Specimen:—</emphasis>
<collectingCountry id="F31312BEFF9BFFB5CB49F986FDD8B23B" box="[513,614,1605,1631]" name="Brazil" pageId="14" pageNumber="207">BRAZIL</collectingCountry>
. 
<emphasis id="B9708E3CFF9BFFB5CB38F987FD55B23A" bold="true" box="[624,747,1604,1630]" pageId="14" pageNumber="207">
<collectingRegion id="49C09CCCFF9BFFB5CB38F987FD58B23A" box="[624,742,1604,1630]" country="Brazil" name="Sao Paulo" pageId="14" pageNumber="207">São Paulo</collectingRegion>
:
</emphasis>
<location id="8EDB04F5FF9BFFB5CBB9F987FCE8B23A" LSID="urn:lsid:plazi:treatment:03ADE338FF9BFFB5C9C0FA5AFCA2B357:8EDB04F5FF9BFFB5CBB9F987FCE8B23A" box="[753,854,1604,1630]" country="Brazil" name="Eldorado" pageId="14" pageNumber="207" stateProvince="Sao Paulo">Eldorado</location>
, 
<date id="FFBA74EEFF9BFFB5CA28F987FBABB23A" box="[864,1045,1604,1630]" pageId="14" pageNumber="207" value="2012-10-11">
<collectingDate id="EFFE8D06FF9BFFB5CA28F987FBABB23A" box="[864,1045,1604,1630]" pageId="14" pageNumber="207" value="2012-10-11">11 October 2012</collectingDate>
</date>
, 
