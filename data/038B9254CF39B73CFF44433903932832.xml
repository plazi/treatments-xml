10359´26´´E, 
<quantity id="4CDA8EA7CF39B73DFECC405B03F6287F" box="[299,423,557,579]" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="1.9" metricValueMin="1.35" pageId="22" pageNumber="1297" unit="m" value="1625.0" valueMax="1900.0" valueMin="1350.0">1350-1900 m</quantity>
, 
<date id="FF9C0582CF39B73DFE57405B0047287E" box="[432,534,557,579]" pageId="22" pageNumber="1297" value="2013-06-16">16.06.2013</date>
