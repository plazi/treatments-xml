<collectionCode id="ED3200E42C57FFBBFC98B0F4FCEEFD70" box="[772,844,710,729]" country="Austria" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="22" pageNumber="649" type="Museum">NHMW</collectionCode>
, 
<collectionCode id="ED3200E42C57FFBBFCC8B0F4FC30FD70" box="[852,914,710,729]" country="Germany" httpUri="http://grbio.org/cool/a6ms-py1z" name="Zoologische Staatssammlung" pageId="22" pageNumber="649">ZSMC</collectionCode>
