</emphasis>
<taxonomicName id="E68E4D7EFFDDFFD87338FCB2A4E09A13" authority="Smith &amp; Mondaca, 2016" authorityName="Smith &amp; Mondaca" authorityYear="2016" box="[383,710,866,890]" class="Insecta" family="Melolonthidae" genus="Chilenopilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="coquimbensis" status="new species">
