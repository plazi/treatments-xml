<taxonomicName id="E68E4D7EFFDDFFD87287F932A7E19F93" authority="Martinez" authorityName="Martinez" authorityYear="1972" box="[192,455,1762,1786]" class="Insecta" family="Melolonthidae" genus="Luispenaia" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus">
