<quantity id="E6769B18FFDDFFD87711F8F1A6C99E30" metricMagnitude="-2" metricUnit="m" metricValue="1.55" metricValueMax="1.6" metricValueMin="1.5" pageId="3" pageNumber="2" unit="mm" value="15.5" valueMax="16.0" valueMin="15.0">15–16 mm</quantity>
