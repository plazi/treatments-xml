Erichson
</taxonomicName>
, 
<taxonomicName id="E68E4D7EFFDDFFD8705FF912A5F09FB3" authority="Neita and Ocampo, Lichniop" authorityName="Neita and Ocampo, Lichniop" box="[536,982,1730,1754]" class="Insecta" family="Melolonthidae" genus="Eideria" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus">
