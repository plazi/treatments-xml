<taxonomicName id="4C37BAB697067C07FCAF9D8B3580FD73" box="[846,981,703,727]" class="Lobosa" family="Hyalospheniidae" genus="Longinebela" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Arcellinida" pageId="15" pageNumber="621" phylum="Protozoa" rank="genus" status="gen. nov.">
<emphasis id="B9431D2797067C07FCAF9D8B3580FD73" box="[846,981,703,727]" italics="true" pageId="15" pageNumber="621">Longinebela</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A270A05C97067C07FC0A9D8B3237FD72" box="[1003,1122,703,727]" pageId="15" pageNumber="621" rank="genus">gen. nov.</taxonomicNameLabel>
