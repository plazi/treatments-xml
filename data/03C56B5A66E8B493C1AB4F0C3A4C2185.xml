<subSubSection id="C37689C766EBB493C1FB4BE73A4C2185" pageId="190" pageNumber="191" type="biology_ecology">
<paragraph id="8BD3DA4C66EBB493C1FB4BE73D8621E1" blockId="190.[151,1436,151,213]" box="[199,1436,151,176]" pageId="190" pageNumber="191">
<emphasis id="B918065E66EBB493C1FB4BE7393D21E1" bold="true" box="[199,295,151,176]" pageId="190" pageNumber="191">Biology.</emphasis>
