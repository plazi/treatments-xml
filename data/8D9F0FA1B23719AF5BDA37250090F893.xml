<taxonomicName id="7DA83A988C40AB171591E6297386178B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33982" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole sagana Wheeler" lsidName-HNS="Pheidole sagana Wheeler" order="Hymenoptera" pageNumber="371" phylum="Arthropoda" rank="species" species="sagana">sagana</taxonomicName>
