<taxonomicName id="D5C6D645F9D4E1BD58805B1A634EEE88" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181857" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole chalcoides Wilson" lsidName-HNS="Pheidole chalcoides Wilson" order="Hymenoptera" pageNumber="371" phylum="Arthropoda" rank="species" species="chalcoides">chalcoides</taxonomicName>
, 
<taxonomicName id="E7843E2B6140611CD75D9396DE75FD40" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:228995" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole crinata" lsidName-HNS="Pheidole crinata Wilson" order="Hymenoptera" pageNumber="371" phylum="Arthropoda" rank="species" species="crinata">crinata</taxonomicName>
