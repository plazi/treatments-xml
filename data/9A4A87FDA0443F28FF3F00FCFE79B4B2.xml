<quantity id="D51B9B0EA0443F28FAFE018DFA2DB491" box="[1366,1435,429,452]" metricMagnitude="4" metricUnit="m" metricValue="5.0" pageId="1" pageNumber="494" unit="km" value="50.0">50 km</quantity>
