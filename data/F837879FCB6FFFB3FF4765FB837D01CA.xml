<collectingCountry id="08897619CB6FFFB3FEBF66F6878B01CA" box="[367,437,976,1002]" name="Japan" pageId="1" pageNumber="53">Japan</collectingCountry>
, 
<collectingCountry id="08897619CB6FFFB3FE1066F6843401CA" box="[448,522,976,1002]" name="South Korea" pageId="1" pageNumber="53">Korea</collectingCountry>
, 
<collectingCountry id="08897619CB6FFFB3FDC666F6845901CA" box="[534,615,976,1002]" name="Russia" pageId="1" pageNumber="53">Russia</collectingCountry>
, 
<collectingCountry id="08897619CB6FFFB3FDA366F6848701CA" box="[627,697,976,1002]" name="China" pageId="1" pageNumber="53">China</collectingCountry>
(
<collectingCountry id="08897619CB6FFFB3FD1966F6851D01CA" box="[713,803,976,1002]" name="Taiwan" pageId="1" pageNumber="53">Taiwan</collectingCountry>
