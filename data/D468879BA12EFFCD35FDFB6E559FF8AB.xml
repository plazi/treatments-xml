<taxonomicName id="9BC14D0EA12EFFCD3633F960552AF95B" box="[857,1144,1698,1723]" class="Reptilia" family="Phyllodactylidae" genus="Thecadactylus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="rapicauda">
<emphasis id="6EB5EA9FA12EFFCD3633F960552AF95B" box="[857,1144,1698,1723]" italics="true" pageId="7" pageNumber="8">Thecadactylus rapicauda</emphasis>
</taxonomicName>
or western 
<taxonomicName id="9BC14D0EA12EFFCD3069F96654C6F95B" box="[1283,1428,1698,1723]" class="Reptilia" family="Phyllodactylidae" genus="Thecadactylus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="rapicauda">
<emphasis id="6EB5EA9FA12EFFCD3069F96654C6F95B" box="[1283,1428,1698,1723]" italics="true" pageId="7" pageNumber="8">T. rapicauda</emphasis>
</taxonomicName>
; 
<bibRefCitation id="38504B7CA12EFFCD35FDF90450F3F93F" author="Turci" box="[151,417,1734,1759]" pageId="7" pageNumber="8" refString="Turci, L. C. B. &amp; Bernarde, P. S. (2008) Levantamento herpetofaunistico em uma localidade no municipio de Cacoal, Rondonia, Brasil. Bioikos, 22, 101 - 108." type="journal article" year="2008">
Turci &amp; Bernarde 
<number id="EA0924AAA12EFFCD340CF90550F3F93F" box="[358,417,1735,1759]" pageId="7" pageNumber="8" value="2008.0">2008</number>
