<subSubSection id="1A96A2C036603128FF5ED4E1A1F2FE11" box="[199,911,438,463]" pageId="10" pageNumber="102" type="etymology">
<paragraph id="5233F14B36603128FF5ED4E1A1F2FE11" blockId="10.[151,1437,258,1867]" box="[199,911,438,463]" pageId="10" pageNumber="102">
<emphasis id="60F82D5936603128FF5ED4E1A333FE11" bold="true" box="[199,334,438,463]" pageId="10" pageNumber="102">Etymology.</emphasis>
