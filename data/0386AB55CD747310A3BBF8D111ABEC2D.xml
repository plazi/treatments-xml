. 
<taxonomicName id="4C2F61C0CD757310A0E5FD5B161FEBFB" authorityName="Krefft" authorityYear="1871" box="[969,1119,712,737]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="5" pageNumber="6" phylum="Nematoda" rank="genus">
