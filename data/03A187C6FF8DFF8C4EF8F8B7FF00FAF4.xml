<taxonomicName id="4C084D53FF8EFF8C4C6AFA86FD8FFAAC" baseAuthorityName="McAlpine" baseAuthorityYear="1981" box="[517,608,1314,1340]" kingdom="Plantae" order="Diptera" pageId="10" pageNumber="11" rank="order">Diptera</taxonomicName>
