<taxonomicName id="039C4D78FFBB0A1B75BDC2D4EBC6FC28" authority="Parent, 1934" authorityName="Parent" authorityYear="1934" box="[227,698,940,967]" class="Insecta" family="Dolichopodidae" genus="Dactylonotus" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="grandicornis">
<emphasis id="F6E8EAE9FFBB0A1B75BDC2D4EB63FC28" box="[227,543,940,967]" italics="true" pageId="1" pageNumber="2">Dactylonotus grandicornis</emphasis>
Parent, 
<number id="725424DCFFBB0A1B7721C2D5EBC6FC28" box="[639,698,941,967]" pageId="1" pageNumber="2" value="1934.0">1934</number>
</taxonomicName>
