<number id="0E687725FFF0FFBB39B6FD9BFF36A0C6" box="[151,167,618,644]" pageId="9" pageNumber="110" value="3.0">3</number>
. 
<taxonomicName id="7FA01E81FFF0FFBB3995FD9BFCBEA0C6" authority="Bates, 1888" authorityName="Bates" authorityYear="1888" box="[180,815,618,644]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="110" phylum="Arthropoda" rank="species" species="chiriquina" subGenus="Phyllophaga">
<emphasis id="8AD4B910FFF0FFBB3995FD9BFEDCA0C6" bold="true" box="[180,333,618,644]" italics="true" pageId="9" pageNumber="110">Phyllophaga</emphasis>
