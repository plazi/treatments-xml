<taxonomicName id="52D5EFF1F429FFF160E8711D4D06F8B1" authority="Jaume &amp; Brehier, 2005" authorityName="Jaume &amp; Brehier" authorityYear="2005" class="Malacostraca" family="Atyidae" genus="Typhlatya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="791" phylum="Arthropoda" rank="species" species="arfeae">
