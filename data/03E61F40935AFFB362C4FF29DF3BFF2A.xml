; C. 
<taxonomicName id="4C4FD5D5935BFFB06674F9A1DF9BF9C4" box="[1063,1167,1567,1590]" class="Entognatha" family="Isotomidae" genus="Agrenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="26" phylum="Arthropoda" rank="species" species="falcula" status="n. sp.">
<emphasis id="B93B7244935BFFB06674F9A1DF9BF9C4" box="[1063,1167,1567,1590]" italics="true" pageId="9" pageNumber="26">A. falcula</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A208CF3F935BFFB066C5F9A0DFC6F9C4" box="[1174,1234,1566,1590]" pageId="9" pageNumber="26" rank="species">
<emphasis id="B93B7244935BFFB066C5F9A0DFC6F9C4" bold="true" box="[1174,1234,1566,1590]" pageId="9" pageNumber="26">n. sp.</emphasis>
</taxonomicNameLabel>
; D-E. Tips of Ant. IV. D. 
<taxonomicName id="4C4FD5D5935BFFB0628EF9FCDA5DF9AB" box="[221,329,1602,1625]" class="Entognatha" family="Isotomidae" genus="Agrenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="26" phylum="Arthropoda" rank="species" species="extrema" status="n. sp.">
<emphasis id="B93B7244935BFFB0628EF9FCDA5DF9AB" box="[221,329,1602,1625]" italics="true" pageId="9" pageNumber="26">A.extrema</emphasis>
