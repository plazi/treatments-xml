<taxonomicName id="4C4FD5D5935BFFB06373F9A1DA61F9C4" box="[288,373,1567,1590]" class="Entognatha" family="Isotomidae" genus="Agrenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis id="B93B7244935BFFB06373F9A1DA61F9C4" box="[288,373,1567,1590]" italics="true" pageId="9" pageNumber="26">Agrenia</emphasis>
</taxonomicName>
spp. A-C, mucrones. A. 
<taxonomicName id="4C4FD5D5935BFFB060D2F9A1D83AF9C4" box="[641,814,1567,1590]" class="Entognatha" family="Isotomidae" genus="Agrenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="26" phylum="Arthropoda" rank="species" species="tarashchukae" status="n. sp.">
<emphasis id="B93B7244935BFFB060D2F9A1D83AF9C4" box="[641,814,1567,1590]" italics="true" pageId="9" pageNumber="26">A. tarashchukae</emphasis>
