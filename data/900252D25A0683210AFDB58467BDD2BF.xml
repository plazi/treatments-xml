<subSubSection id="BC1E577528AF56BC231C2AAD37C08217" pageId="1" pageNumber="2" type="reference_group">
<paragraph id="41B30C147A37F3E1E670550A4408741C" pageId="1" pageNumber="2">
<taxonomicName id="C158268036F0606BD4EEE33012FC5DD1" class="Insecta" family="Histeridae" genus="Reichardtiolus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Reichardtiolus" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">Reichardtiolus</taxonomicName>
