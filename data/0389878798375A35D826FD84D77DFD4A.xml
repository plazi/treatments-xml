<taxonomicName id="4C204D1298375A35D8C0FD84D1F8FDC9" authority="Gray, 1866" authorityName="Gray" authorityYear="1866" box="[673,1005,527,553]" class="Mammalia" family="Rhinonycteridae" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Chiroptera" pageId="43" pageNumber="44" phylum="Chordata" rank="family">Rhinonycteridae Gray, 1866</taxonomicName>
