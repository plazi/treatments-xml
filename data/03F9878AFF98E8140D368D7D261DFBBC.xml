<taxonomicName id="4C504D1FFF98E8140E1A8EFB21C2FD37" authorityName="Liew &amp; Schilthuizen &amp; Vermeulen" authorityYear="2009" box="[569,679,744,765]" class="Gastropoda" family="Dyakiidae" genus="Everettia" kingdom="Animalia" order="Stylommatophora" pageId="23" pageNumber="538" phylum="Mollusca" rank="species" species="jucundior">
