<materialsCitation id="92F1CF24FF89854CFED0F9C3FE1DF922" ID-GBIF-Occurrence="2578498297" collectionCode="MCZ" collectorName="Specimen No." pageId="4" pageNumber="574" specimenCount="1" typeStatus="holotype">
