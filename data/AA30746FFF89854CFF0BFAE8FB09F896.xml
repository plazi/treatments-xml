<collectionCode id="44885DBCFF89854CFCA3F9C3FC92F995" box="[831,895,1631,1655]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="4" pageNumber="574" type="Museum">MCZ</collectionCode>
