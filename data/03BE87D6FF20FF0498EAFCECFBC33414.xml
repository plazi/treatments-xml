<quantity id="4CEF9B25FF20FF049C3AFC5CFEC233CC" box="[1095,1155,942,969]" metricMagnitude="1" metricUnit="m" metricValue="6.5" pageId="177" pageNumber="178" unit="m" value="65.0">65 m</quantity>
