<quantity id="4CEF9B25FF20FF049CE9FC5DFE9C33CF" box="[1172,1245,943,970]" metricMagnitude="1" metricUnit="m" metricValue="6.49224" pageId="177" pageNumber="178" unit="ft" value="213.0">213 ft</quantity>
