<quantity id="4CEF9B25FF20FF049C95FCEDFF6E333C" box="[1256,1327,799,825]" metricMagnitude="-1" metricUnit="m" metricValue="3.2" pageId="177" pageNumber="178" unit="cm" value="32.0">32 cm</quantity>
