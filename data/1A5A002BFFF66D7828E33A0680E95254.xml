<taxonomicName id="55F3CABEFFF66D772CB43B3581725456" authority="Setchell &amp; Gardner." authorityName="Setchell &amp; Gardner." class="Florideophyceae" family="Gracilariaceae" genus="Gracilaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Gigartinales" pageId="7" pageNumber="8" phylum="Rhodophyta" rank="species" species="crispata">
