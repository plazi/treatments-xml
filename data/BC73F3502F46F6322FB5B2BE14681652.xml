<taxonomicName id="3344FAE6EA53E1800539E9E31126D4C0" LSID="http://zoobank.org/CF35CCD0-1196-4672-A46B-7B805D804554" authority="Jin &amp; Li" class="Insecta" family="Mymaridae" genus="Eubroncus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eubroncus tibetanus" order="Hymenoptera" pageId="5" pageNumber="34" phylum="Arthropoda" rank="species" species="tibetanus">Eubroncus tibetanus Jin &amp; Li</taxonomicName>
