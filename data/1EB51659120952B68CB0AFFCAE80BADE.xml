<paragraph id="F3762E2CEECB88DD188301FA2BEF2334" pageId="0" pageNumber="73">
<emphasis id="2B936E29B4FF67D423A3CF48C8A7B007" bold="true" pageId="0" pageNumber="73">Figure 22.</emphasis>
<taxonomicName id="DA689A2E1470583D387E956434BE0A90" authorityName="Yu &amp; Li" authorityYear="2021" family="Clubionidae" genus="Sinostidia" kingdom="Animalia" lsidName="Sinostidia shuangjiao" order="Araneae" pageId="0" pageNumber="73" rank="species" species="shuangjiao">
<emphasis id="1190FBE3E21ACA816E0FF57A3454F145" italics="true" pageId="0" pageNumber="73">Sinostidia shuangjiao</emphasis>
</taxonomicName>
