<emphasis id="B95F3D289008FFEBFBA5FC41E36322C4" box="[1046,1082,951,977]" italics="true" pageId="6" pageNumber="671">
ad 
<subScript id="17AFE37F9008FFEBFB82FC36E36322C4" attach="left" box="[1073,1082,960,977]" fontSize="7" pageId="6" pageNumber="671">1</subScript>
