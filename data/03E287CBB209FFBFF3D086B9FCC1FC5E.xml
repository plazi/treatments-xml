<taxonomicName id="4C4B4D5EB20FFFBFF0ED8791FBF2FCC5" box="[938,1123,765,792]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="36" pageNumber="56" phylum="Tracheophyta" rank="species" species="section">
<emphasis id="B93FEACFB20FFFBFF0ED8791FB99FCCA" box="[938,1032,765,791]" italics="true" pageId="36" pageNumber="37">Quercus</emphasis>
section
</taxonomicName>
