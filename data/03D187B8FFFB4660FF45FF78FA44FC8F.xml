<taxonomicName id="4C784D2DFFFB4660FEBFFF77FE0CFF3D" box="[365,467,151,177]" class="Insecta" family="Sciomyzidae" genus="Sepedon" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
