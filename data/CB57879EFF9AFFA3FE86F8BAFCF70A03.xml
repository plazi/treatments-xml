<typeStatus id="9C45882AFF99FFA3FE86FCB8FEC60F3B" box="[264,325,780,806]" pageId="6" pageNumber="7">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="84FE4D0BFF99FFA3FE3FFCB8FC0D0F3B" authority="Coifmann, 1937" authorityName="Coifmann" authorityYear="1937" box="[433,910,780,806]" class="Malacostraca" family="Mysidae" genus="Gastrosaccus" kingdom="Animalia" order="Mysida" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="dissimilis">
<emphasis id="718AEA9AFF99FFA3FE3FFCB8FD450F38" box="[433,710,780,805]" italics="true" pageId="6" pageNumber="7">Gastrosaccus dissimilis</emphasis>
<bibRefCitation id="276F4B79FF99FFA3FD43FCB8FC0D0F3B" author="Coifmann" box="[717,910,780,806]" pageId="6" pageNumber="26" refString="Coifmann, I. (1937) Misidacei raccolti dalla R. Corvetta Vettor Pisani negli anni 1882 - 85. Annuario del Museo Zoollogico della R. Universita Napoli, N. S., 7, 1 - 14." type="journal article" year="1937">
Coifmann, 
<number id="F53624AFFF99FFA3FCDFFCB8FC0D0F3B" box="[849,910,780,806]" pageId="6" pageNumber="7" value="1937.0">1937</number>
</bibRefCitation>
</taxonomicName>
