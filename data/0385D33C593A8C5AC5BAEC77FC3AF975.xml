<quantity id="4CD4CFCF593A8C58C62EED18FCA5FAA9" box="[771,881,1365,1389]" metricMagnitude="-3" metricUnit="m" metricValue="3.52" pageId="4" pageNumber="173" unit="mm" value="3.52">3.52 mm</quantity>
, HL= 
<quantity id="4CD4CFCF593A8C58C697ED18FBF3FAA9" box="[954,1063,1365,1389]" metricMagnitude="-3" metricUnit="m" metricValue="1.16" pageId="4" pageNumber="173" unit="mm" value="1.16">1.16 mm</quantity>
