2. 
<taxonomicName id="B6FE01949E5527EA624DCC5986E2189C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27358" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Cardiocondyla" kingdom="Animalia" lsidName-HNS="Cardiocondyla shuckardi Forel" order="Hymenoptera" pageNumber="161" phylum="Arthropoda" rank="species" species="shuckardi" status="nov. sp.">CARDIOCONDYLA SHUCKARDI</taxonomicName>
, 
<taxonomicNameLabel id="CB3CED61D822D65779072253C632CCB6" rank="species">nov. sp.</taxonomicNameLabel>
</paragraph>
