<taxonomicName id="4C3A4D74FFDBCE21B19EFE80C4ABFEDD" baseAuthorityName="Hampson" baseAuthorityYear="1900" box="[973,1176,354,379]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="279" phylum="Arthropoda" rank="species" species="perpallida">
<emphasis id="B94EEAE5FFDBCE21B19EFE80C4ABFEDD" bold="true" box="[973,1176,354,379]" italics="true" pageId="6" pageNumber="279">Barsine perpallida</emphasis>
