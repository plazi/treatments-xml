RSa/ 
<number id="AD35512442415E5FFCA3FD42FCE2FC91" box="[789,798,764,788]" pageId="5" pageNumber="506" value="1.0">1</number>
RS = 
<number id="AD35512442415E5FFCD6FD42FC28FC91" box="[864,980,764,788]" pageId="5" pageNumber="506" value="3.61" valueMax="3.8" valueMin="3.42">
3.42 
<emphasis id="29899F1142415E5FFC27FD43FC63FC91" box="[913,927,765,788]" italics="true" pageId="5" pageNumber="506">–</emphasis>
3.80
</number>
; pterostigma length/width = 2.00 
<emphasis id="29899F1142415E5FFAF1FD43FAA9FC91" box="[1351,1365,765,788]" italics="true" pageId="5" pageNumber="506">–</emphasis>
<number id="AD35512442415E5FFAE3FD42FA7AFC91" box="[1365,1414,764,788]" pageId="5" pageNumber="506" value="2.43">2.43</number>
<emphasis id="29899F1142415E5FFA30FD43FA68FC91" box="[1414,1428,765,788]" italics="true" pageId="5" pageNumber="506">–</emphasis>
; r = 
<quantity id="DC05EEE642415E5FFF7DFC9EFE84FCB2" box="[203,376,799,824]" metricMagnitude="-4" metricUnit="m" metricValue="2.45" metricValueMax="2.6" metricValueMin="2.3" pageId="5" pageNumber="506" unit="mm" value="0.245" valueMax="0.26" valueMin="0.23">
0.23 
