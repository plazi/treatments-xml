<taxonomicName id="4C7E3589FF75FF53FF48FF1A3032FB7C" authorityName="Burmeister" authorityYear="1847" box="[192,334,1140,1164]" class="Insecta" family="Dynastidae" genus="Harposcelis" kingdom="Animalia" order="Coleoptera" pageId="190" pageNumber="189" phylum="Arthropoda" rank="genus">
