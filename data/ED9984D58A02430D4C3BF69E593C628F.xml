<taxonomicName id="3C6BBA17DA943E623099AE20D2C78DB6" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia" order="Fabales" pageId="16" pageNumber="109" phylum="Tracheophyta" rank="genus">Acacia</taxonomicName>
