<taxonomicName id="E7539D4E1D861EA89D263DDB785AA4CE" class="Insecta" family="Eulophidae" genus="Omphale" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Omphale breviventris" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="breviventris">Omphale breviventris</taxonomicName>
