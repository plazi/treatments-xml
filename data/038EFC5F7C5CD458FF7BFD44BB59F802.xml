A– 
<number id="3DEF5F6E7C5CD458FEDFFD21B83EFD0B" box="[307,334,716,740]" pageId="29" pageNumber="30" value="14.0">14</number>
E; 
<number id="3DEF5F6E7C5CD458FE80FD21B8F7FD0B" box="[364,391,716,740]" pageId="29" pageNumber="30" value="21.0">21</number>
