<paragraph id="A2EAC2DEA24B178D9D778E5821C8168F" pageId="0" pageNumber="3">
<emphasis id="36782026325165D55129AD7E739D4408" bold="true" pageId="0" pageNumber="3">Figures 56, 57.</emphasis>
<taxonomicName id="6D667926DC6CF875884D85BA8847F655" authority="Ortega-Blanco, McKellar &amp; Engel" authorityName="Ortega-Blanco, McKellar &amp; Engel" class="Hexapoda" family="Proterosceliopsidae" genus="Proterosceliopsis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Proterosceliopsis plurima" order="Hymenoptera" pageId="0" pageNumber="3" phylum="Arthropoda" rank="species" species="plurima">
<emphasis id="032486D579C967F0EE4DA1E305DF1AEF" italics="true" pageId="0" pageNumber="3">Proterosceliopsis plurima</emphasis>
</taxonomicName>
