species: 
<taxonomicName id="4C6806BF5F5FFFA4010BF958FC83F929" authority="Germain, 1855" authorityName="Germain" authorityYear="1855" box="[316,820,1736,1762]" class="Insecta" family="Cleridae" genus="Neogyponyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="punctipennis">
