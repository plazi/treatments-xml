<taxonomicName id="06B3F47C55DF34E5FA105001E2773E2C" class="Magnoliopsida" family="Phyllanthaceae" genus="Uapaca" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Uapaca togoensis" order="Malpighiales" pageId="0" pageNumber="115" phylum="Tracheophyta" rank="species" species="togoensis">Uapaca togoensis</taxonomicName>
and 
<taxonomicName id="A10B106CF155CAE0C50DADD68230E618" class="Magnoliopsida" family="Dipterocarpaceae" genus="Monotes" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Monotes kerstingii" order="Malvales" pageId="0" pageNumber="115" phylum="Tracheophyta" rank="species" species="kerstingii">Monotes kerstingii</taxonomicName>
