<taxonomicName id="5A9F59B46455FF9C3185FC81FCBAFCBB" box="[700,870,787,811]" class="Insecta" family="Mycetophilidae" genus="Docosia" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="39" phylum="Arthropoda" rank="species" species="blagoderovi">
