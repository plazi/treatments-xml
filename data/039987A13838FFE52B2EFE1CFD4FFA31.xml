<quantity id="4CC89B523839FFE52B07FD99FD25FD09" box="[590,697,631,657]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="43" pageNumber="44" unit="mm" value="0.13">0.13 mm</quantity>
