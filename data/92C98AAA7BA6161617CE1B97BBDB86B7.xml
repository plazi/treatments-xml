</taxonomicName>
<emphasis id="0D5DD309C9336265CFCB82D19EFDC4E1" bold="true" pageId="1" pageNumber="10">16.</emphasis>
<taxonomicName id="CAD7BDCF2ED75DB370037D76BC53D1E1" authority="Kirby, 1882" authorityName="Kirby" authorityYear="1882" baseAuthorityName="Konow" baseAuthorityYear="1896" class="Insecta" family="Poaceae" genus="Xenapates" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Xenapates gaullei" order="Hymenoptera" pageId="1" pageNumber="10" phylum="Arthropoda" rank="species" species="gaullei">
