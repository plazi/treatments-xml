<taxonomicName id="4C4F12F32144FFFEE3CB466CFEF9FACD" authorityName="Zorn" authorityYear="2007" box="[222,379,1384,1405]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="86" phylum="Arthropoda" rank="species" species="aequalisaequalis">
<emphasis id="B93BB5622144FFFEE3CB466CFEF9FACD" box="[222,379,1384,1405]" italics="true" pageId="5" pageNumber="86">Anomala aequalisaequalis</emphasis>
