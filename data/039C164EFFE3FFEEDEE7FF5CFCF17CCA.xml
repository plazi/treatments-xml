<subSubSection id="C32FF4D3FFE3FFEFDEE7FF5CFB39787E" box="[725,1187,587,610]" pageId="98" pageNumber="99" type="nomenclature">
<paragraph id="8B8AA758FFE3FFEFDEE7FF5CFB39787E" blockId="98.[725,1187,587,610]" box="[725,1187,587,610]" pageId="98" pageNumber="99">
<heading id="D0C21034FFE3FFEFDEE7FF5CFB39787E" box="[725,1187,587,610]" centered="true" fontSize="9" level="2" pageId="98" pageNumber="99" reason="2">
<taxonomicName id="4C35DCDBFFE3FFEFDEE7FF5CFB91787E" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[725,1035,587,610]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="hainanensis" status="new species">
<emphasis id="B9417B4AFFE3FFEFDEE7FF5CFB91787E" bold="true" box="[725,1035,587,610]" italics="true" pageId="98" pageNumber="99">Paramblynotus hainanensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFE3FFEFD82EFF5AFB39787E" box="[1052,1187,588,610]" pageId="98" pageNumber="99" rank="species">new species</taxonomicNameLabel>
