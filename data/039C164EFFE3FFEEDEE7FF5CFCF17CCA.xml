<paragraph id="8B8AA758FFE2FFEEDEAFFDDCFC7D7B1C" blockId="99.[669,1175,203,256]" pageId="99" pageNumber="100">
<taxonomicName id="4C35DCDBFFE2FFEEDEAFFDDCFB0D7AFD" authority="Cameron, 1910" authorityName="Cameron" authorityYear="1910" box="[669,1175,203,226]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="99" pageNumber="100" phylum="Arthropoda" rank="species" species="annulicornis">
