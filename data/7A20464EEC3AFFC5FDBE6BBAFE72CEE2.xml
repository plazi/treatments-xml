<taxonomicName id="35898CDBEC3BFFCBFE93690DFE7CC974" authorityName="Allen" authorityYear="2010" box="[303,472,865,886]" class="Bivalvia" family="Limidae" genus="Limatula" kingdom="Animalia" order="Limida" pageId="23" pageNumber="2612" phylum="Mollusca" rank="species" species="celtica">Limatula celtica</taxonomicName>
