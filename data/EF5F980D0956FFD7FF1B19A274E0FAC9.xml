<quantity id="A00E84FE0956FFD7FC5419DC730EFC68" box="[1022,1081,981,1001]" metricMagnitude="1" metricUnit="m" metricValue="7.3" pageId="18" pageNumber="919" unit="m" value="73.0">73 m</quantity>
