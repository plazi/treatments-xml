<geoCoordinate id="02C24FDC0956FFD7FF7419E47678FB80" box="[222,335,1005,1025]" degrees="33" direction="north" minutes="05.137" orientation="latitude" pageId="18" pageNumber="919" precision="1" value="33.085617">33°05.137’N</geoCoordinate>
/ 
<geoCoordinate id="02C24FDC0956FFD7FEFF19E476F8FB80" box="[341,463,1005,1025]" degrees="035" direction="east" minutes="34.730" orientation="longitude" pageId="18" pageNumber="919" precision="1" value="35.578835">035°34.730’E</geoCoordinate>
