<emphasis id="D713311BFC9F7B442E596CD161BE6203" italics="true" pageId="0" pageNumber="5">Nauclea</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="CD3E560BE08AAF2C907C1665C793FE8B" class="Magnoliopsida" family="Rubiaceae" genus="Nauclea" kingdom="Plantae" lsidName="" order="Gentianales" pageId="0" pageNumber="5" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
