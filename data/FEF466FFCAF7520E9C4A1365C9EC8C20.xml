<paragraph id="D15E6E15B4AA7DAD16E172ABC50A803B" pageId="0" pageNumber="5">
<taxonomicName id="7861F4A09AEF96D3069F9A9C86905DA7" LSID="http://zoobank.org/0364A60A-39EB-4402-9B16-EFADE3D120E4" authority="Awad" authorityName="Awad" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Synopeas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synopeas kiki" order="Hymenoptera" pageId="0" pageNumber="5" phylum="Arthropoda" rank="species" species="kiki" status="sp. nov.">Synopeas kiki Awad</taxonomicName>
