<taxonomicName id="1F8D71EFFF906D37FF0F39AFD357F81C" authority="Gmel." authorityName="Gmel." box="[228,547,1958,1983]" class="Magnoliopsida" family="Rhamnaceae" genus="Colletia" kingdom="Plantae" order="Rosales" pageId="13" pageNumber="314" phylum="Tracheophyta" rank="species" species="spinosissima">
