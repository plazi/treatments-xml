</taxonomicName>
, 
<taxonomicName id="1F8D71EFFF906D37FDC539AED3A3F863" authority="Brongn." authorityName="Brongn." box="[558,727,1959,1984]" class="Magnoliopsida" family="Rhamnaceae" genus="Scutia" kingdom="Plantae" order="Rosales" pageId="13" pageNumber="314" phylum="Tracheophyta" rank="genus">
