<quantity id="5A7A9B2AFFB09009FEC7FEC1FEAA5208" box="[264,349,275,299]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="8" pageNumber="199" unit="mm" value="3.2">3,2 mm</quantity>
; Breite: 
<quantity id="5A7A9B2AFFB09009FE7EFEC0FDC75208" box="[433,560,274,298]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="8" pageNumber="199" unit="mm" value="1.4" valueMax="1.5" valueMin="1.3">1,3-1,5 mm</quantity>
