<taxonomicName id="37FE4D285A1DFF8FFCC3235AFD61FE79" authority="Meyrick, 1930" authorityName="Meyrick" authorityYear="1930" class="Insecta" family="Tortricidae" genus="Cacoecia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="biforata">
