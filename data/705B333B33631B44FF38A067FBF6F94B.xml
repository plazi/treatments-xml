<paragraph id="F84D822D33631B44FF38A0DEFBC2FA3B" blockId="10.[151,1436,1509,1677]" box="[151,1024,1509,1534]" pageId="10" pageNumber="55">
<emphasis id="CA865E3F33631B44FF38A0DEFF1AFA3B" bold="true" box="[151,216,1509,1534]" pageId="10" pageNumber="55">NOW.</emphasis>
<taxonomicName id="3FF2F9AE33631B44FF70A0DEFD30FA3B" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[223,754,1509,1533]" class="Aves" family="Fringillidae" genus="Carduelis" kingdom="Animalia" order="Passeriformes" pageId="10" pageNumber="55" phylum="Chordata" rank="subSpecies" species="carduelis" subSpecies="carduelis">
<emphasis id="CA865E3F33631B44FF70A0DEFDEAFA3B" box="[223,552,1509,1533]" italics="true" pageId="10" pageNumber="55">Carduelis carduelis carduelis</emphasis>
(Linnaeus, 
<number id="4E3A900A33631B44FD01A0DEFD28FA3B" box="[686,746,1509,1533]" pageId="10" pageNumber="55" value="1758.0">1758</number>
)
</taxonomicName>
