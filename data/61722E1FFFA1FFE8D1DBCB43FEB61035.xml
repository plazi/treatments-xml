<taxonomicName id="2EDBE48AFFA1FFEFD1CFCA1BFE06151F" authority="Rafinesque (1838: 75)" authorityName="Rafinesque" authorityPageNumber="75" authorityYear="1838" box="[156,463,1836,1858]" class="Magnoliopsida" family="Rosaceae" genus="Basilima" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="genus">
