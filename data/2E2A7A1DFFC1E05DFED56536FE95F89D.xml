<taxonomicName id="6183B088FFC2E05DFDE3651EFCE1FB92" box="[574,808,1084,1109]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="nubis">
