<emphasis id="654B5130ACC75EA98CA26CA56B2AF3CC" bold="true" pageId="0" pageNumber="129">C, D</emphasis>
<taxonomicName id="217075C81676830235F57767362B13D4" authority="Z.M.He &amp; Zhu L.Yang, 2021" authorityName="Z. M. He &amp; Zhu L. Yang" authorityYear="2021" class="Agaricomycetes" family="Hygrophoraceae" genus="Spodocybe" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Spodocybe bispora" order="Agaricales" pageId="0" pageNumber="129" phylum="Basidiomycota" rank="species" species="bispora">
