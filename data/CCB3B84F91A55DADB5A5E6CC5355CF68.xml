<taxonomicName id="01E44B2DAD60EEB007E62BF526C2775E" authority="Z.M.He &amp; Zhu L.Yang, 2021" authorityName="Z. M. He &amp; Zhu L. Yang" authorityYear="2021" class="Agaricomycetes" family="Hygrophoraceae" genus="Spodocybe" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Spodocybe rugosiceps" order="Agaricales" pageId="0" pageNumber="129" phylum="Basidiomycota" rank="species" species="rugosiceps">
