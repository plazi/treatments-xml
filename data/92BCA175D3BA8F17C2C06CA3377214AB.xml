<paragraph id="C1ADB4E3EF4AFED8980982C914E6D6EE" pageId="19" pageNumber="236">
<taxonomicName id="D6374FD75A6F43D9C3EFE4BBA355F21C" LSID="http://zoobank.org/D8CBE451-DB4D-47DA-8B69-59A39C2D381C" authority="Klimaszewski &amp; Webster" class="Insecta" family="Staphylinidae" genus="Liogluta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liogluta pseudocastoris" order="Coleoptera" pageId="19" pageNumber="236" phylum="Arthropoda" rank="species" species="pseudocastoris">Liogluta pseudocastoris Klimaszewski &amp; Webster</taxonomicName>
