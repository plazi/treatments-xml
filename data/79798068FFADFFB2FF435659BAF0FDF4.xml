<taxonomicName id="36D04AFDFFADFFB0FE1457A6BC62FC13" authorityName=", Shear" authorityYear="1972" box="[448,589,909,934]" class="Liliopsida" family="Asparagaceae" genus="Trichopetalum" kingdom="Plantae" order="Asparagales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="stannardi">
