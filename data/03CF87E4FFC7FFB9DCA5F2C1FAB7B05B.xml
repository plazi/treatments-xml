<emphasis id="B912EAE0FFC4FFB8D8C4F6F9FA52B698" box="[1270,1437,1254,1278]" italics="true" pageId="2" pageNumber="446">
<taxonomicName id="4C664D71FFC4FFB8D8C4F6F9FA56B698" authorityName="Lin &amp; Peng" authorityYear="2021" box="[1270,1433,1254,1278]" class="Insecta" family="Staphylinidae" genus="Domene" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="446" phylum="Arthropoda" rank="species" species="leigongensis">D. leigongensis</taxonomicName>
