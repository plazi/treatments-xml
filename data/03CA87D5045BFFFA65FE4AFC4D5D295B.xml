<taxonomicName id="4C634D40045BFFFA64894B6E4C6C2958" authorityName="Paulian" authorityYear="1977" box="[507,628,307,333]" class="Insecta" family="Hybosoridae" genus="Besuchetostes" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="83" phylum="Arthropoda" rank="species" species="jaccoudi">
