<taxonomicName id="62E66791FF88FFB7FB9EFD29638DE79F" authorityName="De Deckker" authorityYear="1981" box="[1061,1174,712,738]" class="Ostracoda" family="Cyprididae" genus="Bennelongia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="nimala">
