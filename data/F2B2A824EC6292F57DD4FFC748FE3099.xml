<taxonomicName id="9CE0B54FF585D0D0446B45872926C82A" class="Polychaeta" family="Sigalionidae" genus="Pisione" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pisione remota" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="remota">Pisione remota</taxonomicName>
(Southern, 1914) | 
<taxonomicName id="91149D6CFB46AC4B37E809329FEB7DD9" genus="Praegeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Praegeria remota" pageId="0" pageNumber="20997" rank="species" species="remota">Praegeria remota</taxonomicName>
