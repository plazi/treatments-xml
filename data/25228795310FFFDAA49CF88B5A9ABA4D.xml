<taxonomicName id="6A8B4D00310FFFDBA49CF88B5B3BBC5B" authority="Duges, 1886" authorityName="Duges" authorityYear="1886" box="[151,563,1852,1878]" class="Reptilia" family="Colubridae" genus="Rhinocheilus" kingdom="Animalia" order="Squamata" pageId="10" pageNumber="43" phylum="Chordata" rank="species" species="antonii">
