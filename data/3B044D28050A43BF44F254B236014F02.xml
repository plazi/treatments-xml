<taxonomicName id="9A7ADCEF345DA299DAA4BAA966D993BF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:135596" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica ligniperda Latreille" order="Hymenoptera" pageNumber="86" phylum="Arthropoda" rank="species" species="ligniperda">Formica ligniperda Latreille</taxonomicName>
