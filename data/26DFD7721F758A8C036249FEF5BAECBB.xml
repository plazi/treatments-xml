<geoCoordinate id="E43D0CE702EB18476E839FE472969105" direction="north" orientation="latitude" precision="925" value="24.966667">24°58'N</geoCoordinate>
, 
<geoCoordinate id="86B26B275BAEB98A88EC5B5A91959AC5" direction="east" orientation="longitude" precision="925" value="105.6">105°36'E</geoCoordinate>
