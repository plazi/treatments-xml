<subSubSection id="328D69FBFFC2FFAFFEBC00E3FB99B801" pageId="7" pageNumber="8" type="etymology">
<paragraph id="7A283A70FFC2FFAFFEBC00E3FE69BBB1" blockId="7.[264,1322,438,585]" pageId="7" pageNumber="8">
<emphasis id="48E3E662FFC2FFAFFEBC00E3FE16BB98" bold="true" box="[264,405,438,464]" pageId="7" pageNumber="8">Etymology.</emphasis>
<emphasis id="48E3E662FFC2FFAFFE2A00E2FE2EBB98" box="[414,429,439,464]" italics="true" pageId="7" pageNumber="8">a</emphasis>
(Greek) = not, 
<taxonomicName id="BD9741F3FFC2FFAFFDDF00E2FD71BB98" authorityName="Irwin &amp; Lyneborg" authorityYear="1981" box="[619,754,439,464]" class="Insecta" family="Therevidae" genus="Penniverpa" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
