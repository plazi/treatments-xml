<paragraph id="8BBD6450AA5CAE76FF54A591FE55FF4F" blockId="7.[151,493,151,212]" box="[151,493,151,176]" pageId="7" pageNumber="54">
<heading id="D0F5D33CAA5CAE76FF54A591FE55FF4F" bold="true" box="[151,493,151,176]" fontSize="10" level="2" pageId="7" pageNumber="54" reason="2">
<emphasis id="B976B842AA5CAE76FF54A591FE55FF4F" bold="true" box="[151,493,151,176]" pageId="7" pageNumber="54">
<taxonomicName id="4C021FD3AA5CAE76FF54A591FE1FFF4F" box="[151,423,151,176]" class="Insecta" family="Phlaeothripidae" genus="Bactrothrips" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="species" species="furvescrus" status="n. sp.">
<emphasis id="B976B842AA5CAE76FF54A591FE1FFF4F" bold="true" box="[151,423,151,176]" italics="true" pageId="7" pageNumber="54">Bactrothrips furvescrus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2450539AA5CAE76FE6CA59EFE55FF4F" box="[431,493,152,176]" pageId="7" pageNumber="54" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
