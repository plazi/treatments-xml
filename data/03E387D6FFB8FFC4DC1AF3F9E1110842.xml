, 
<taxonomicName id="4C4A4D43FFB8FFC4D85CF7DEE12D0842" authority="(Morelet, 1860)" baseAuthorityName="Morelet" baseAuthorityYear="1860" class="Gastropoda" family="Bulimulidae" genus="Bostryx" kingdom="Animalia" order="Stylommatophora" pageId="20" pageNumber="77" phylum="Mollusca" rank="species" species="papillatus">
