<paragraph id="5119A97FF2AF3A0369E8DA24073FE702" pageId="0" pageNumber="3">
<taxonomicName id="E4A63681D31CD1F27CEA0A18FAFFBD03" authority="Ortega-Blanco, McKellar &amp; Engel, 2014" authorityName="Ortega-Blanco, McKellar &amp; Engel" authorityYear="2014" class="Insecta" family="Scelionidae" genus="Proterosceliopsis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Proterosceliopsis masneri" order="Hymenoptera" pageId="0" pageNumber="3" phylum="Arthropoda" rank="species" species="masneri">
