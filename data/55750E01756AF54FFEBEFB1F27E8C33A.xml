<number id="6B14AD30756AF541FEE5FB7C2773C2D5" box="[339,349,1239,1265]" pageId="7" pageNumber="8" value="1.0">1</number>
</figureCitation>
C–D, 
<number id="6B14AD30756AF541FE11FB7C2799C2D5" box="[423,439,1239,1265]" pageId="7" pageNumber="8" value="2.0">2</number>
B, 
<number id="6B14AD30756AF541FE6EFB7C27CBC2D5" box="[472,485,1239,1265]" pageId="7" pageNumber="8" value="3.0">3</number>
C–D, 
<number id="6B14AD30756AF541FD9BFB7C2414C2D5" box="[557,570,1239,1265]" pageId="7" pageNumber="8" value="3.0">3</number>
F)
