<paragraph id="8BEB36ACFF8A2E4D64AFFB23D69ECEE0" blockId="102.[151,1437,1142,1671]" pageId="102" pageNumber="103">
<emphasis id="B920EABEFF8A2E4D64AFFB23D375CE84" bold="true" box="[199,314,1178,1203]" pageId="102" pageNumber="103">Diagnosis</emphasis>
