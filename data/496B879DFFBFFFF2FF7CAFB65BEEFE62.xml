, 
<collectingCountry id="B9D5761BFFBFFFF2FD8CAC385ABEF89B" box="[600,681,1815,1839]" name="Bolivia" pageId="8" pageNumber="613">Bolivia</collectingCountry>
, 
<collectingCountry id="B9D5761BFFBFFFF2FD6BAC385B14F89B" box="[703,771,1815,1839]" name="Brazil" pageId="8" pageNumber="613">Brazil</collectingCountry>
, 
