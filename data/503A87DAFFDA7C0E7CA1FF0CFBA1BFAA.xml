<taxonomicName id="1F934D4FFFDA7C0F7D21F973FE2BB982" authority="Shen &amp; Yin, 2020" authorityName="Shen &amp; Yin" authorityYear="2020" box="[279,456,1766,1790]" class="Insecta" family="Staphylinidae" genus="Sathytes" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="392" phylum="Arthropoda" rank="species" species="obliquus" status="sp. nov.">
<emphasis id="EAE7EADEFFDA7C0F7D21F973FE2BB982" box="[279,456,1766,1790]" italics="true" pageId="6" pageNumber="392">Sathytes obliquus</emphasis>
</taxonomicName>
<emphasis id="EAE7EADEFFDA7C0F7DF8F973FDFEB982" bold="true" box="[462,541,1766,1790]" pageId="6" pageNumber="392">
