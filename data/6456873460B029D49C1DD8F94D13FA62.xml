<taxonomicName id="8C00DE0E438C74890D64EEA53B59B7BD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:236922" class="Insecta" family="Formicidae" genus="Axinidris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Axinidris namib" order="Hymenoptera" pageNumber="567" phylum="Arthropoda" rank="species" species="namib">Axinidris namib</taxonomicName>
