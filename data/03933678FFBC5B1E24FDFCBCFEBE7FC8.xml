<taxonomicName id="4C3AFCEDFFBC5B1B25BCFA3BFDCC7DE9" box="[470,590,1488,1514]" class="Actinopterygii" family="Pimelodidae" genus="Pimelodus" kingdom="Animalia" order="Siluriformes" pageId="1" pageNumber="58" phylum="Chordata" rank="species" species="ornatus">
<emphasis id="B94E5B7CFFBC5B1B25BCFA3BFE677DE9" box="[470,485,1488,1513]" italics="true" pageId="1" pageNumber="58">P</emphasis>
