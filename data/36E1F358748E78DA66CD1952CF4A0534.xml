<taxonomicName id="B5A95CF4087E76DBF2787814FD26E879" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24890" class="Insecta" family="Formicidae" genus="Oligomyrmex" kingdom="Animalia" lsidName-HNS="Oligomyrmex Mayr" order="Hymenoptera" pageNumber="22" phylum="Arthropoda" rank="genus">Oligomyrmex Mayr</taxonomicName>
