<taxonomicName id="D29EE6ED452BFFBCFF72D498D491FF5A" authority="Yoshida &amp; Hirowatari, 2014" authorityName="Yoshida &amp; Hirowatari" authorityYear="2014" box="[151,742,151,178]" class="Insecta" family="Silvanidae" genus="Psammoecus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="92" phylum="Arthropoda" rank="species" species="scitus">
