<collectingCountry id="7EF376036B37FF9DFCD8FF3CFCFC95FB" box="[782,834,184,207]" name="Peru" pageId="4" pageNumber="57">Peru</collectingCountry>
, 
<collectingRegion id="C420F8716B37FF9DFC9AFF3CFC3E95FB" box="[844,896,184,207]" country="Brazil" name="Acre" pageId="4" pageNumber="57">Acre</collectingRegion>
