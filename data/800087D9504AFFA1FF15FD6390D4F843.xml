, Fujian, Cong’an, 
<quantity id="CF519B2A504AFFA1FC0AFA199502FA21" box="[904,1033,1496,1522]" metricMagnitude="2" metricUnit="m" metricValue="2.4" metricValueMax="2.5" metricValueMin="2.3" pageId="10" pageNumber="65" unit="m" value="240.0" valueMax="250.0" valueMin="230.0">230–250m</quantity>
, 
<date id="7C17100F504AFFA1FB91FA1995BAFA21" box="[1043,1201,1496,1522]" pageId="10" pageNumber="65" value="1960-05-26">
<number id="BE6124E8504AFFA1FB91FA19953BFA21" box="[1043,1072,1496,1522]" pageId="10" pageNumber="65" value="26.0">26</number>
