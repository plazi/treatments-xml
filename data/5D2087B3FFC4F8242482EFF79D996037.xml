<taxonomicName id="12894D26FFC4F824264DEEF29EB9638B" authority="Hennig, 1959" authorityName="Hennig" authorityYear="1959" box="[600,1022,402,427]" class="Insecta" family="Muscidae" genus="Spilogona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="246" phylum="Arthropoda" rank="species" species="bomynensis">
