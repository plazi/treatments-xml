<number id="7B4FA847FF866344FEB1FF6EFEE0FF08" box="[261,315,226,252]" pageId="4" pageNumber="5" value="16.7">16.7</number>
%, 
<number id="7B4FA847FF866344FEEBFF6EFE5BFF08" box="[351,384,226,252]" pageId="4" pageNumber="5" value="22.0">22</number>
