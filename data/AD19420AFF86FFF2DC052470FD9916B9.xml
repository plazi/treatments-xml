<taxonomicName id="E2B0889FFF86FFF2DD3E2570FDDD1639" authorityName="Prado" authorityYear="1969" box="[357,620,1338,1363]" class="Insecta" family="Syringogastridae" genus="Syringogaster" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="papaveroi">Syringogaster papaveroi</taxonomicName>
