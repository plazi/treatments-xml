<taxonomicName id="6183B088FFCFE050FD9E603EFCA2FEF2" box="[579,875,284,309]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="corniculae">
