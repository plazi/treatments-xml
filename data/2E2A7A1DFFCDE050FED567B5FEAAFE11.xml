<quantity id="617B66EEFFCEE051FCCE6598FCBDFB13" box="[787,884,1210,1236]" metricMagnitude="3" metricUnit="m" metricValue="1.52" pageId="30" pageNumber="31" unit="m" value="1520.0">1520 m</quantity>
, Res[erva] Biol[ógica] Monteverde, Prov[incia] Puntarenas, 
<collectingCountry id="DE948B9BFFCEE051FDF065C0FD29FB3B" box="[557,736,1250,1276]" name="Costa Rica" pageId="30" pageNumber="31">
<collectionCode id="C09253CEFFCEE051FDF065C0FD42FB3B" box="[557,651,1250,1276]" pageId="30" pageNumber="31">COSTA</collectionCode>
