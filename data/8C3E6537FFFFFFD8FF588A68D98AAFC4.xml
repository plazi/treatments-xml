<taxonomicName id="C397AFA2FFFFFFDEFDB88BBBD944A8D7" box="[631,810,1939,1963]" class="Insecta" family="Cicadellidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="26" phylum="Arthropoda" rank="species" species="muscaeformis">
