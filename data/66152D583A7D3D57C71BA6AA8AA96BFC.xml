<normalizedToken id="A8393819C83E54D0673E1F00B245F901" originalValue="Oñate">Onate</normalizedToken>
,Valle de Araotz, Cueva de la Ermita de Sandaili, UTM 30TWN4580260906, 
<geoCoordinate id="134A4A0C664409DA59309A311D1EF6B7" direction="north" orientation="latitude" precision="1" value="42.999443">N42.999442</geoCoordinate>
, 
<geoCoordinate id="D2482A9A25FDF9569C23979362F351DD" direction="east" orientation="longitude" precision="1" value="2.438076">E-2.438076</geoCoordinate>
