<taxonomicName id="4C01A3DDFFF9A06AFD89F995860F5C99" authority="(Dujardin, 1841)" authorityName="Dujardin" authorityYear="1841" baseAuthorityName="Dujardin" baseAuthorityYear="1841" box="[539,1049,1652,1679]" class="Oligohymenophorea" family="Radiophryidae" genus="Metaradiophrya" kingdom="Protozoa" order="Astomatida" pageId="6" pageNumber="7" phylum="Ciliophora" rank="species" species="lumbrici">
