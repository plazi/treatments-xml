<paragraph id="363E36F6FFABFF8E2FFDFB3395C088CD" blockId="16.[138,1185,1053,1270]" box="[170,429,1246,1270]" pageId="16" pageNumber="1527">
<taxonomicName id="F1814D75FFABFF8E2FFDFB3395C888CD" authorityName="Young" authorityYear="1967" box="[170,421,1246,1270]" class="Monogenea" family="Ancyrocephalidae" genus="Tetrancistrum" kingdom="Animalia" order="Dactylogyridea" pageId="16" pageNumber="1527" phylum="Platyhelminthes" rank="species" species="oraminii">
<emphasis id="04F5EAE4FFABFF8E2FFDFB3395C888CD" box="[170,421,1246,1270]" italics="true" pageId="16" pageNumber="1527">Tetrancistrum oraminii</emphasis>
