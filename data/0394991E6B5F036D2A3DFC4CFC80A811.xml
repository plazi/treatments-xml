: 
<collectingCountry id="F32A68986B5F036D2AE3F8A1FD96A811" box="[470,561,1852,1878]" name="Algeria" pageId="1" pageNumber="2">Algeria</collectingCountry>
, Southern 
<collectingCountry id="F32A68986B5F036D299BF8A1FD42A811" box="[686,741,1852,1878]" name="Italy" pageId="1" pageNumber="2">Italy</collectingCountry>
, 
<collectingCountry id="F32A68986B5F036D29C5F8A1FC84A811" box="[752,803,1852,1878]" name="Iran" pageId="1" pageNumber="2">Iran</collectingCountry>
