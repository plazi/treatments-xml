<collectingCountry id="F34A76659D63FFD4FBF5FC7C35F0FC7B" box="[1119,1227,914,938]" name="Colombia" pageId="31" pageNumber="682">Colombia</collectingCountry>
, 
<collectingCountry id="F34A76659D63FFD4FB73FC7D346DFC7B" box="[1241,1366,914,939]" name="Costa Rica" pageId="31" pageNumber="682">Costa Rica</collectingCountry>
