<paragraph id="E37360EB45144D4FBE9CB4E87F5EA22B" pageId="31" pageNumber="32">
<emphasis id="8294813789F778DC9A49E60FF1ACC23C" bold="true" pageId="31" pageNumber="32">Myanmar</emphasis>
: 
<emphasis id="DD4964A1254BF01412A4323FDED06502" italics="true" pageId="31" pageNumber="32">hyang</emphasis>
, 
<emphasis id="9CC6D78A11B569FE58509EE12BA7917B" italics="true" pageId="31" pageNumber="32">mai-bau</emphasis>
, 
<emphasis id="FDEFC20E636BD01DBDF25ACF72FE5AE4" italics="true" pageId="31" pageNumber="32">nbau</emphasis>
, 
<emphasis id="E0920B5F7406B3B1075C8EE0D2EDB450" italics="true" pageId="31" pageNumber="32">ning-bau</emphasis>
, 
<emphasis id="0FC7C4AAFB8DEF12642AE880B4B447B2" italics="true" pageId="31" pageNumber="32">yang-bau</emphasis>
. 
<emphasis id="DC280B5F2BC5311FC7B340CAB22CD1C7" bold="true" pageId="31" pageNumber="32">English</emphasis>
