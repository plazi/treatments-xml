<taxonomicName id="BAA4F5B5FFA0D34AE969F879FD12F84D" authority="Zwick" authorityName="Zwick" authorityYear="2012" box="[536,747,1934,1956]" class="Insecta" family="Scirtidae" genus="Petrocyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="163" phylum="Arthropoda" rank="species" species="cataractae">
