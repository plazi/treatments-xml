<caption id="6887FF9AFFB7FFBC25B5B81EC84651F8" pageId="101" pageNumber="101" startId="101.[150,198,1633,1652]" targetBox="[208,1142,211,1601]" targetPageId="101">
<paragraph id="3C47AF12FFB7FFBC25B5B81EC84651F8" blockId="101.[128,1228,1633,1732]" pageId="101" pageNumber="101">
