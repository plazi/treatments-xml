</taxonomicName>
(
<taxonomicName id="4C6B4D6DFB53B30768979920FEB78E69" box="[158,300,1614,1639]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
