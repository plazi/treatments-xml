<mcCollectionCode id="5E465361FB53B30769B69921FE778E68" box="[447,492,1615,1638]" country="0" name="Museum of the Earth, Polish Academy of Sciences" pageId="7" pageNumber="54">MZ</mcCollectionCode>
, 
<mcCollectionCode id="5E465361FB53B30769FE9921FDA88E69" box="[503,563,1615,1639]" country="United Kingdom" httpUri="http://grbio.org/cool/h8hj-4rxx" name="National Museum of Scotland - Natural Sciences" pageId="7" pageNumber="54" type="Museum">NMS</mcCollectionCode>
