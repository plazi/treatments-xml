<specimenCode id="DB974D471951FFBD6B7196A51EE5F946" box="[269,452,1707,1728]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="110" pageNumber="110">AMNH 679113</specimenCode>
