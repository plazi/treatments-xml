<taxonomicName id="4C319EBF1951FFBD6A2195A91D3AFA3D" authority="Rothschild, 1901: 34" authorityName="Rothschild" authorityPageNumber="34" authorityYear="1901" box="[93,539,1447,1467]" class="Aves" family="Ptilonorhynchidae" genus="Loborhamphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="110" pageNumber="110" phylum="Chordata" rank="species" species="nobilis">
<emphasis id="B945392E1951FFBD6A2195A91E1CFA3D" box="[93,317,1447,1467]" italics="true" pageId="110" pageNumber="110">Loborhamphus nobilis</emphasis>
