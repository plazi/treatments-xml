<number id="3D85248DE57EFFBA4284FF13FB126EFF" box="[1171,1187,216,239]" pageId="11" pageNumber="54" value="2.0">2</number>
; hypotrichous opisthogaster ............................................................................................................................................................................. 
<number id="3D85248DE57EFFBA4399FF33FA2A6F1F" box="[1422,1435,248,271]" pageId="11" pageNumber="54" value="4.0">4</number>
