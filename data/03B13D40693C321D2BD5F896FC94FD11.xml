<taxonomicName id="4C18F7D5693D321D2821FE22FB2FFE29" authority="Haworth" authorityName="Haworth" box="[867,1181,476,500]" class="Insecta" family="Noctuidae" genus="Archanara" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="80" phylum="Arthropoda" rank="species" species="geminipuncta">
<emphasis id="B96C5044693D321D2821FE22FBAFFE29" box="[867,1053,476,500]" italics="true" pageId="1" pageNumber="80">A. geminipuncta</emphasis>
(Haworth)
</taxonomicName>
, 
<taxonomicName id="4C18F7D5693D321D2FECFE22FF5EFDCA" authority="Esper" authorityName="Esper" class="Insecta" family="Noctuidae" genus="Arenostola" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="80" phylum="Arthropoda" rank="species" species="semicana">
<emphasis id="B96C5044693D321D2FECFE22FA2EFE29" box="[1198,1436,476,500]" italics="true" pageId="1" pageNumber="80">Arenostola semicana</emphasis>
