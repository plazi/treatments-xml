<taxonomicName id="4C18F7D5693D321D29D5FDFEFC72FDCA" authority="Hubner" authorityName="Hubner" box="[663,960,511,536]" class="Insecta" family="Noctuidae" genus="Rhizedra" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="80" phylum="Arthropoda" rank="species" species="lutosa">
<emphasis id="B96C5044693D321D29D5FDFEFCFEFDC5" box="[663,844,512,536]" italics="true" pageId="1" pageNumber="80">Rhizedra lutosa</emphasis>
