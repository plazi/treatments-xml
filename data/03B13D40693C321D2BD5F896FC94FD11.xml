</taxonomicName>
, 
<taxonomicName id="4C18F7D5693D321D2BBEFDFEFDE7FDCA" authority="Tauscher" authorityName="Tauscher" box="[252,597,511,536]" class="Insecta" family="Noctuidae" genus="Chilodes" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="80" phylum="Arthropoda" rank="species" species="maritima">
<emphasis id="B96C5044693D321D2BBEFDFEFE61FDC5" box="[252,467,512,536]" italics="true" pageId="1" pageNumber="80">Chilodes maritima</emphasis>
