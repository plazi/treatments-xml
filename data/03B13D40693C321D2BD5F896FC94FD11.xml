<taxonomicName id="4C18F7D5693D321D2F32FE01FB57FDCA" box="[1136,1253,511,535]" class="Insecta" family="Noctuidae" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="80" phylum="Arthropoda" rank="family">Noctuidae</taxonomicName>
