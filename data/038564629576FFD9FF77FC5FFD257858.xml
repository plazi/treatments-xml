<collectionCode id="ED3D4DB19576FFD9FB1AFA95FABE787B" box="[1274,1380,1341,1364]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="18" pageNumber="219">NHMUK</collectionCode>
