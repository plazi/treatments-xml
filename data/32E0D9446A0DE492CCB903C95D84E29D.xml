<paragraph id="CE5DD3D2F5854912AEF1AF6739DBCFD9" pageId="0" pageNumber="43">
<taxonomicName id="093C167AE11C9F4E0D418D683F59537D" authorityName="Brusina in Westerlund" authorityYear="1902" class="Gastropoda" family="Hydrobiidae" genus="Andrusovia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Andrusovia dybowskii" order="Littorinimorpha" pageId="0" pageNumber="43" phylum="Ciliophora" rank="species" species="dybowskii">
