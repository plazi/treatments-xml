<taxonomicName id="03E44D172B49FF8C38C92C53CC8A842A" authority="Delile" authorityName="Delile" box="[731,1006,331,357]" class="Magnoliopsida" family="Amaranthaceae" genus="Salsola" kingdom="Plantae" order="Caryophyllales" pageId="50" pageNumber="51" phylum="Tracheophyta" rank="species" species="tetragona">
<emphasis id="F690EA862B49FF8C38C92C53CCC6842A" box="[731,930,331,357]" italics="true" pageId="50" pageNumber="51">Salsola tetragona</emphasis>
