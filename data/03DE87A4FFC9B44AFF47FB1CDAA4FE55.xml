<date id="FFC91072FFC9B448FB4BF9D9DB30F9EC" box="[1179,1329,1588,1613]" pageId="1" pageNumber="552" value="2007-09-08" valueMax="2007-09-15" valueMin="2007-09-08">8–15.ix.2007</date>
