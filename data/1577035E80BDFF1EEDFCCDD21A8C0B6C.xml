<paragraph id="9D61B24880BDFF1EEDACCA861A8C0B6C" blockId="185.[151,1437,1019,1296]" pageId="185" pageNumber="186">
<emphasis id="AFAA6E5A80BDFF1EEDACCA8618A70AB4" bold="true" box="[199,506,1199,1224]" pageId="185" pageNumber="186">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA80BDFF1EEF6ECA991BC40AB4" box="[517,665,1199,1224]" country="Papua New Guinea" name="New Ireland" pageId="185" pageNumber="186">New Ireland</collectingRegion>
