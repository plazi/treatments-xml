<taxonomicName id="E2C13500BE1919DF26FFE2B186C0EC61" class="Agaricomycetes" family="Pleurotaceae" genus="Pleurotus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Pleurotus" order="Agaricales" pageId="15" pageNumber="70" phylum="Basidiomycota" rank="genus">Pleurotus</taxonomicName>
