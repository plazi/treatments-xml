<taxonomicName id="4C164D3C0E41FFFF54666283FD33FD77" authority="Cai &amp; Taylor" authorityName="Cai &amp; Taylor" box="[264,747,724,750]" class="Insecta" family="Reduviidae" genus="Lentireduvius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="brasiliensis" status="new species">
<emphasis id="B962EAAD0E41FFFF54666283FD98FD77" bold="true" box="[264,576,724,750]" italics="true" pageId="3" pageNumber="54">Lentireduvius brasiliensis</emphasis>
Cai &amp; Taylor
</taxonomicName>
, 
<taxonomicNameLabel id="A25157D60E41FFFF57966283FC50FD77" box="[760,904,724,750]" pageId="3" pageNumber="54" rank="species">new species</taxonomicNameLabel>
</emphasis>
