<taxonomicName id="9DE8138D6AB9E071792CA933A2A69F9A" class="Liliopsida" family="Posidoniaceae" genus="Posidonia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Posidonia oceanica" order="Alismatales" pageId="0" pageNumber="20997" phylum="Tracheophyta" rank="species" species="oceanica">Posidonia oceanica</taxonomicName>
