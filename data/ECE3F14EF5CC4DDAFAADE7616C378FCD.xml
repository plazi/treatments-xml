<taxonomicName id="EB0B3A718A8975BA33F1AD3C1A6CAFFE" class="Insecta" family="Cicadellidae" genus="Molopopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Molopopterus alfa" order="Hemiptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="species" species="alfa">Molopopterus alfa</taxonomicName>
