<taxonomicName id="2B753CB5F8BC58F11DABBEA91E2D341B" class="Insecta" family="Cicadellidae" genus="Empoasca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Empoasca" order="Hemiptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="genus">Empoasca</taxonomicName>
