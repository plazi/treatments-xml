<pageBreakToken id="966DCA74BA9063AB8E8BDE195D2F13FE" pageId="115" pageNumber="116" start="start">Johansen</pageBreakToken>
(1914: 533, as 
<taxonomicName id="2015640A5A72BD4FB95D2F954B09B0CE" genus="Trogophloeina" lsidName="Trogophloeina" pageId="115" pageNumber="116" rank="genus">Trogophloeina</taxonomicName>
