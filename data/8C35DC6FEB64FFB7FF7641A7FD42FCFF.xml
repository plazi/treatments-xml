<taxonomicName id="C39C16FAEB65FFB7FCC24020FC38FD21" box="[803,929,638,664]" class="Magnoliopsida" family="Hydrangeaceae" genus="Hydrangea" kingdom="Plantae" order="Cornales" pageId="9" pageNumber="460" phylum="Tracheophyta" rank="genus">
<emphasis id="36E8B16BEB65FFB7FCC24020FC38FD21" box="[803,929,638,664]" italics="true" pageId="9" pageNumber="460">Hydrangea</emphasis>
</taxonomicName>
, 
<taxonomicName id="C39C16FAEB65FFB7FC4E4020FC7EFD21" box="[943,999,638,664]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="9" pageNumber="460" phylum="Chordata" rank="genus">
<emphasis id="36E8B16BEB65FFB7FC4E4020FC7EFD21" box="[943,999,638,664]" italics="true" pageId="9" pageNumber="460">Salix</emphasis>
</taxonomicName>
, 
<taxonomicName id="C39C16FAEB65FFB7FC144021FBDFFD21" box="[1013,1094,639,664]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="9" pageNumber="460" phylum="Tracheophyta" rank="genus">
<emphasis id="36E8B16BEB65FFB7FC144021FBDFFD21" box="[1013,1094,639,664]" italics="true" pageId="9" pageNumber="460">Prunus</emphasis>
</taxonomicName>
, 
<taxonomicName id="C39C16FAEB65FFB7FBB24021FB2AFD20" box="[1107,1203,639,665]" class="Magnoliopsida" family="Hamamelidaceae" genus="Sycopsis" kingdom="Plantae" order="Saxifragales" pageId="9" pageNumber="460" phylum="Tracheophyta" rank="genus">
<emphasis id="36E8B16BEB65FFB7FBB24021FB2AFD20" box="[1107,1203,639,665]" italics="true" pageId="9" pageNumber="460">Sycopsis</emphasis>
</taxonomicName>
, 
<taxonomicName id="C39C16FAEB65FFB7FB234021FAACFD21" box="[1218,1333,639,664]" class="Magnoliopsida" family="Oleaceae" genus="Ligustrum" kingdom="Plantae" order="Lamiales" pageId="9" pageNumber="460" phylum="Tracheophyta" rank="genus">
