<taxonomicName id="C39C16FAEB65FFB7FAA24021FAE1FD20" box="[1347,1400,639,665]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="9" pageNumber="460" phylum="Tracheophyta" rank="genus">
<emphasis id="36E8B16BEB65FFB7FAA24021FAE1FD20" box="[1347,1400,639,665]" italics="true" pageId="9" pageNumber="460">Acer</emphasis>
