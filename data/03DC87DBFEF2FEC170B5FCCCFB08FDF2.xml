</emphasis>
, 
<taxonomicName id="4C754D4EFEF2FEC17261F8E4FD77FDF2" authorityName="Skoracki" authorityYear="2011" baseAuthorityName="Skoracki and Magowski" baseAuthorityYear="2001" box="[579,726,1916,1940]" class="Magnoliopsida" family="Lamiaceae" genus="Prunella" kingdom="Plantae" order="Lamiales" pageId="382" pageNumber="383" phylum="Tracheophyta" rank="species" species="modularis" status="comb. nov.">
