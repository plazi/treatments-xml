<subSubSection id="C34F0EE2FFC6FF952CC7F8B78F57A233" box="[192,1128,1793,1818]" pageId="5" pageNumber="4" type="type_taxon">
<paragraph id="8BEA5D69FFC6FF952CC7F8B78F57A233" blockId="5.[192,1128,1793,1818]" box="[192,1128,1793,1818]" pageId="5" pageNumber="4">
