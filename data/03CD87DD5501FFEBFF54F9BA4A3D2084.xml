<taxonomicName id="4C644D485500FFE8FDFCF9BE4CBE24AC" box="[575,686,1660,1682]" class="Insecta" family="Lachesillidae" genus="Hemicaecilius" kingdom="Animalia" order="Psocodea" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="nicolasi" status="n. sp.">
