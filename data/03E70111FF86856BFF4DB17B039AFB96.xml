</taxonomicName>
and 
<taxonomicName id="4C4ECB84FF86856BFC72B1CF017AFBF1" authority="Weise . Distribution." authorityName="Weise . Distribution." class="Insecta" family="Chrysomelidae" genus="Gallerucida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="359" phylum="Arthropoda" rank="species" species="gebieni">
<emphasis id="B93A6C15FF86856BFC72B1CF0400FBCE" box="[936,1050,1013,1037]" italics="true" pageId="1" pageNumber="359">G. gebieni</emphasis>
