<taxonomicName id="4C4ECB84FF86856BFD6DB1CF0368FBCE" authority="Weise" authorityName="Weise" box="[695,882,1013,1037]" class="Insecta" family="Chrysomelidae" genus="Gallerucida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="359" phylum="Arthropoda" rank="species" species="haroldi">
<emphasis id="B93A6C15FF86856BFD6DB1CF033DFBCE" box="[695,807,1013,1037]" italics="true" pageId="1" pageNumber="359">G. haroldi</emphasis>
