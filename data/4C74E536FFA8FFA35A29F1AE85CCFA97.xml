<taxonomicName id="03DD2FA3FFA8FFA35D78F77585EEFBF7" authorityName="Grassi" authorityYear="1888" box="[1036,1158,1105,1129]" class="Insecta" family="Machilidae" kingdom="Animalia" order="Archaeognatha" pageId="1" pageNumber="314" phylum="Arthropoda" rank="subFamily" subFamily="Machilinae">Machilinae</taxonomicName>
