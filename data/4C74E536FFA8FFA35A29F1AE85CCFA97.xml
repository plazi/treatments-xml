</taxonomicName>
, described (
<bibRefCitation id="A04C29D1FFA8FFA35D69F73585A2FBB7" author="Silvestri, F" box="[1053,1226,1041,1065]" pageId="1" pageNumber="314" pagination="325 - 335" refId="ref7708" refString="Silvestri, F., 1906. Notes sui Machilidae. III. Descrizione di un nuovo genere di sei nuove specie. Redia, Firenze, 3: 325 - 335." type="journal article" year="1906">Silvestri, 1906</bibRefCitation>
) from the 
<collectingCountry id="BCCA14B0FFA8FFA35C39F73584C3FBB7" box="[1357,1451,1041,1065]" name="Vietnam" pageId="1" pageNumber="314">Vietnam</collectingCountry>
(
<collectingCountry id="BCCA14B0FFA8FFA35A47F71582EDFBD7" box="[819,901,1073,1097]" name="Vietnam" pageId="1" pageNumber="314">Tonkin</collectingCountry>
