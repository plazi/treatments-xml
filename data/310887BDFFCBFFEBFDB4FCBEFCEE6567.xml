<superScript id="4ED49BE3FFD4FFEBFEB1FB92FEBE6434" attach="left" box="[324,335,1071,1085]" fontSize="6" pageId="16" pageNumber="17">st</superScript>
tergite (4.0 and 1.5 times in 
<taxonomicName id="7EA14D28FFD4FFEBFD6CFB8DFCD46443" authority="Peris-Felipo &amp; Achterberg &amp; Belokobylskij, 2019" authorityName="Peris-Felipo &amp; Achterberg &amp; Belokobylskij" authorityYear="2019" box="[665,805,1072,1098]" class="Insecta" family="Braconidae" genus="Asobara" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="cracentis" status="sp. nov.">
<emphasis id="8BD5EAB9FFD4FFEBFD6CFB8DFCD46443" box="[665,805,1072,1098]" italics="true" pageId="16" pageNumber="17">A. cracentis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="90E657C2FFD4FFEBFCDEFB8DFC756443" box="[811,900,1072,1098]" pageId="16" pageNumber="17" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="7EA14D28FFD4FFEBFC4EFB8DFBCE6443" authority="Peris-Felipo &amp; Achterberg &amp; Belokobylskij, 2019" authorityName="Peris-Felipo &amp; Achterberg &amp; Belokobylskij" authorityYear="2019" box="[955,1087,1072,1098]" class="Insecta" family="Braconidae" genus="Asobara" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="zululana" status="sp. nov.">
