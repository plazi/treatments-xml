<taxonomicName id="7EA14D28FFD4FFEBFD21FBE9FCAE6464" authority="Peris-Felipo &amp; Achterberg &amp; Belokobylskij, 2019" authorityName="Peris-Felipo &amp; Achterberg &amp; Belokobylskij" authorityYear="2019" box="[724,863,1108,1134]" class="Insecta" family="Braconidae" genus="Asobara" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="cracentis" status="sp. nov.">
<emphasis id="8BD5EAB9FFD4FFEBFD21FBE9FCAE6464" box="[724,863,1108,1134]" italics="true" pageId="16" pageNumber="17">A. cracentis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="90E657C2FFD4FFEBFC91FBE9FC4D6467" box="[868,956,1108,1134]" pageId="16" pageNumber="17" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="7EA14D28FFD4FFEBFC04FBE9FB846467" authority="Peris-Felipo &amp; Achterberg &amp; Belokobylskij, 2019" authorityName="Peris-Felipo &amp; Achterberg &amp; Belokobylskij" authorityYear="2019" box="[1009,1141,1108,1134]" class="Insecta" family="Braconidae" genus="Asobara" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="zululana" status="sp. nov.">
<emphasis id="8BD5EAB9FFD4FFEBFC04FBE9FB846467" box="[1009,1141,1108,1134]" italics="true" pageId="16" pageNumber="17">A. zululana</emphasis>
