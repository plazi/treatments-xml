<emphasis id="B94E5D1CFFCEFFB5C18BFF0EFEED1AD5" bold="true" box="[151,324,151,177]" pageId="30" pageNumber="31">
Group 
<taxonomicName id="4C3AFA8DFFCEFFB5C1EDFF0EFEED1AD5" box="[241,324,151,177]" class="Insecta" family="Lauxaniidae" genus="Dacus" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="33" phylum="Arthropoda" rank="species" species="seguyi">Seguyi</taxonomicName>
