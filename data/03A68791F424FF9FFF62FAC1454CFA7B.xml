<quantity id="4CF79B62F425FF9FFC7DFA2143C2FA3C" box="[904,976,1439,1464]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="4" unit="m" value="200.0">200 m</quantity>
