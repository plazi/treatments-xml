<taxonomicName id="4C0F4D04F425FF9FFC13FA7A44BFFA58" box="[998,1197,1476,1500]" class="Magnoliopsida" family="Dipterocarpaceae" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="family">Dipterocarpaceae</taxonomicName>
