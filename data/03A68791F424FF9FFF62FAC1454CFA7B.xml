<taxonomicName id="4C0F4D04F425FF9FFB42FA7A450DFA58" box="[1207,1311,1476,1500]" class="Magnoliopsida" family="Tiliaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="family">Tiliaceae</taxonomicName>
, 
<taxonomicName id="4C0F4D04F425FF9FFADFFA7A40CAF984" class="Magnoliopsida" family="Euphorbiaceae" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
, 
<taxonomicName id="4C0F4D04F425FF9FFF16FA59414EFA7B" box="[227,348,1511,1535]" class="Magnoliopsida" family="Clusiaceae" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="family">Clusiaceae</taxonomicName>
and Guttiferae (
