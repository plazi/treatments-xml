<quantity id="8811CAFA2A3BBB1CFF74E16AE555280F" box="[214,313,1044,1068]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="8.0" metricValueMin="7.0" pageId="89" pageNumber="2319" unit="mm" value="7.5" valueMax="8.0" valueMin="7.0">
