<materialsCitation id="1781083A444BFFC1FDDE7EC1FE241D4E" ID-GBIF-Occurrence="3097576303" collectingDate="2012-05-23" location="Hubei Province" pageId="3" pageNumber="78" specimenCount="1" stateProvince="Hubei">
<emphasis id="959DDE75444BFFC1FDDE7EC1FD0212AF" box="[639,767,2020,2044]" italics="true" pageId="3" pageNumber="78">adenophorus</emphasis>
in Guanmenshan, Shennongjia
</materialsCitation>
</taxonomicName>
, 
<collectingRegion id="652DCC85444BFFC1FE447120FD791D4E" box="[485,644,2053,2077]" country="China" name="Hubei" pageId="3" pageNumber="78">Hubei Province</collectingRegion>
