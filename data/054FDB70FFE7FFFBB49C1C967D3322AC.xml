<taxonomicName id="4AE611E5FFE6FFFBB01619A67CD92498" box="[1400,1515,954,976]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="arboreus">
