<taxonomicName id="4AE611E5FFE6FFFBB75519A67BF32498" box="[571,705,954,976]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="furunculus">
<emphasis id="BF92B674FFE6FFFBB75519A67BF32498" box="[571,705,954,976]" italics="true" pageId="33" pageNumber="34">T. furunculus</emphasis>
