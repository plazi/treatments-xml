<taxonomicName id="4C0A4D77EB08FFE0FE1DFF1F46463407" baseAuthorityName="Casey" baseAuthorityYear="1893" box="[435,542,237,263]" class="Insecta" family="Staphylinidae" genus="Pella" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="loricata">
