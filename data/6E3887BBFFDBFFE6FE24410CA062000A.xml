<taxonomicName id="21914D2EFFDBFFE6FCB44154A3D8063B" authority="(Latreille, 1807)" baseAuthorityName="Latreille" baseAuthorityYear="1807" class="Insecta" family="Kateretidae" genus="Kateretes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="227" phylum="Arthropoda" rank="species" species="rufilabris">
