<typeStatus id="392A880FFFDBFFE6FE7B4155A2E50605" box="[428,470,832,851]" pageId="2" pageNumber="227">Type</typeStatus>
species: 
<taxonomicName id="21914D2EFFDBFFE6FDFE4154A0710605" authority="Latreille, 1807" authorityName="Latreille" authorityYear="1807" box="[553,834,832,851]" class="Insecta" family="Kateretidae" genus="Cercus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="227" phylum="Arthropoda" rank="species" species="rufilabris">
