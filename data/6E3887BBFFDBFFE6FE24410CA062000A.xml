<taxonomicName id="21914D2EFFDBFFE6FF404154A2900605" authority="Gistel, 1856: 362" authorityName="Gistel" authorityPageNumber="362" authorityYear="1856" box="[151,419,831,851]" class="Insecta" family="Kateretidae" genus="Ammalaptus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="227" phylum="Arthropoda" rank="genus">
<emphasis id="D4E5EABFFFDBFFE6FF404154A2310605" box="[151,258,833,851]" italics="true" pageId="2" pageNumber="227">Ammalaptus</emphasis>
<bibRefCitation id="82004B5CFFDBFFE6FEDE4155A2900605" author="GISTEL J. N. F. X." box="[265,419,831,851]" pageId="2" pageNumber="227" pagination="2" refId="ref6501" refString="GISTEL J. N. F. X. 1856: Die Mysterien der europaischen Insectenwelt. Tobias Dannheimer, Kempten, xii + 530 + [2] pp." type="journal article" year="1856">Gistel, 1856: 362</bibRefCitation>
</taxonomicName>
</treatmentCitation>
