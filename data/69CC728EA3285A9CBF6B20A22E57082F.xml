<taxonomicName id="F6CD311F29822A259D4C8AAA4CA17393" authority="dorycnii" genus="Z." lsidName="Z. dorycnii subsp. dorycnii" pageId="0" pageNumber="39" rank="subSpecies" species="dorycnii" subSpecies="dorycnii">
<emphasis id="180C366DE5480EF1F794E0F405F8BE9E" italics="true" pageId="0" pageNumber="39">Z. dorycnii dorycnii</emphasis>
</taxonomicName>
