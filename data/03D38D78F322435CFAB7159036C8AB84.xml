<subSubSection id="C3606FE5F322435CFAE7148636C8AB84" lastPageId="6" lastPageNumber="438" pageId="5" pageNumber="437" type="discussion">
<paragraph id="8BC53C6EF322435CFAE7148636C8AB84" blockId="5.[151,1436,1887,2020]" lastBlockId="6.[151,1436,151,213]" lastPageId="6" lastPageNumber="438" pageId="5" pageNumber="437">
<emphasis id="B90EE07CF322435FFAE714863630AC91" bold="true" box="[199,315,1959,1984]" pageId="5" pageNumber="437">Remarks.</emphasis>
<bibRefCitation id="EFEB419FF322435FFB6A148636FFACEE" author="Hecher" box="[330,500,1959,1984]" pageId="5" pageNumber="437" refString="Hecher, C. (1997) Two new species of Pseudovelia (Insecta: Heteroptera: Veliidae) from Vietnam. Annalen des Naturhistorischen Museums in Wien, Series B, 99, 41 - 49." type="journal article" year="1997">
