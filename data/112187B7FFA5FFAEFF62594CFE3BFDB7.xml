. 
<taxonomicName id="5E884D22FFA7FFA1FEDD5A53FE26F9DD" box="[296,483,1601,1622]" class="Malacostraca" family="Ischyroceridae" genus="Neoecetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="conipes" status="gen. nov.">
<emphasis id="ABFCEAB3FFA7FFA1FEDD5A53FE26F9DD" box="[296,483,1601,1622]" italics="true" pageId="39" pageNumber="40">Neoecetes conipes</emphasis>
</taxonomicName>
<emphasis id="ABFCEAB3FFA7FFA1FE1E5A53FD61F9DD" bold="true" box="[491,676,1601,1622]" pageId="39" pageNumber="40">
<taxonomicNameLabel id="B0CF57C8FFA7FFA1FE1E5A53FD88F9DD" box="[491,589,1601,1622]" pageId="39" pageNumber="40" rank="genus">gen. nov.</taxonomicNameLabel>
, sp. nov
</emphasis>
. Holotype, 3. 
<emphasis id="ABFCEAB3FFA7FFA1FCCA5A53FC09F9DE" bold="true" box="[831,972,1600,1622]" pageId="39" pageNumber="40">gp 1 and gp2</emphasis>
, gnathopods 1 and 2; 
<emphasis id="ABFCEAB3FFA7FFA1FB465A52FAD7F9DE" bold="true" box="[1203,1298,1600,1622]" pageId="39" pageNumber="40">p4 and 6</emphasis>
, pereopods 4 and 6; 
<emphasis id="ABFCEAB3FFA7FFA1FF295A4DFF33F9FF" bold="true" box="[220,246,1631,1652]" pageId="39" pageNumber="40">vp</emphasis>
