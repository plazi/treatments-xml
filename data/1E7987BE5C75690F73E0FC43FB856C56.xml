, Province 
<quantity id="51289B4D5C75691071ECFA7CFC3669F6" box="[762,906,1481,1502]" metricMagnitude="2" metricUnit="m" metricValue="7.25" metricValueMax="12.1" metricValueMin="2.4" pageId="15" pageNumber="-1" unit="m" value="725.0" valueMax="1210.0" valueMin="240.0">240–1,210 m</quantity>
