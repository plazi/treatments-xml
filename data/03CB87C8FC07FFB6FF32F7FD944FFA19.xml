<quantity id="4C9A9B3BFC06FFB6FD24F3C69381FB58" box="[641,720,1196,1222]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="33" pageNumber="34" unit="m" value="900.0">900 m</quantity>
