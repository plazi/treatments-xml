9. 
<taxonomicName id="05F808757BD6FB610561A39A80BF076E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:134304" class="Insecta" family="Formicidae" genus="Oecodoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Oecodoma mexicana Smith" order="Hymenoptera" pageId="185" pageNumber="185" phylum="Arthropoda" rank="species" species="mexicana">Oecodoma Mexicana</taxonomicName>
