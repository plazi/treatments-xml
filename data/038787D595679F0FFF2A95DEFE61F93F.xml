<quantity id="4CD69B2695679F0EFE00972FFD95F88E" box="[445,538,1814,1838]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="8" pageNumber="516" unit="mm" value="1.8">1.8 mm</quantity>
