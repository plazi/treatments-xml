<quantity id="4CD69B2695679F0EFC5F972FFBB3F88E" box="[994,1084,1814,1838]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="8" pageNumber="516" unit="mm" value="0.1">0.1 mm</quantity>
