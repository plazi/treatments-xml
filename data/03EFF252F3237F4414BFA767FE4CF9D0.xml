<collectingRegion id="49828DA6F3237F4410DEA7B4FACAFA60" box="[1270,1337,1479,1503]" country="Tanzania" name="Pwani" pageId="39" pageNumber="240">Coast</collectingRegion>
</collectingCountry>
, 
<collectingCountry id="F35103D4F3237F441162A7B5FEAEF9BC" name="Central African Republic" pageId="39" pageNumber="240">
<collectingRegion id="49828DA6F3237F441162A7B5FA6FFA60" box="[1354,1436,1478,1503]" country="Zambia" name="Central" pageId="39" pageNumber="240">Central</collectingRegion>
