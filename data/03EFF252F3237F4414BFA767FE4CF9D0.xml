<collectingCountry id="F35103D4F3237F441061A7B5FB6EFA60" box="[1097,1181,1478,1503]" name="Guinea-Bissau" pageId="39" pageNumber="240">Guinea</collectingCountry>
, 
<collectingCountry id="F35103D4F3237F441085A7B4FACAFA60" box="[1197,1337,1479,1503]" name="Ivory Coast" pageId="39" pageNumber="240">
