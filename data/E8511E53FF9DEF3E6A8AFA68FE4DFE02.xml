, and 
<taxonomicName id="A7F8D4C6FF9EEF3E6986FE4BFB03FE46" authority="Haworth" authorityName="Haworth" box="[972,1231,392,418]" class="Magnoliopsida" family="Cactaceae" genus="Mammillaria" kingdom="Plantae" order="Caryophyllales" pageId="66" pageNumber="65" phylum="Tracheophyta" rank="genus">
<emphasis id="528C7357FF9EEF3E6986FE4BFBAEFE46" box="[972,1122,392,418]" italics="true" pageId="66" pageNumber="65">Mammillaria</emphasis>
