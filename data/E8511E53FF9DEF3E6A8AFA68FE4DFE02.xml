, 
<taxonomicName id="A7F8D4C6FF9EEF3E6832FE4BFC58FE46" authority="Engelmann" authorityName="Engelmann" box="[632,916,392,418]" class="Magnoliopsida" family="Cactaceae" genus="Echinocereus" kingdom="Plantae" order="Caryophyllales" pageId="66" pageNumber="65" phylum="Tracheophyta" rank="genus">
<emphasis id="528C7357FF9EEF3E6832FE4BFCC6FE46" box="[632,778,392,418]" italics="true" pageId="66" pageNumber="65">Echinocereus</emphasis>
