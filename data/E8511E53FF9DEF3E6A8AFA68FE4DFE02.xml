(
<taxonomicName id="A7F8D4C6FF9EEF3E6E91FE4BFA9CFE46" box="[1243,1360,392,418]" class="Magnoliopsida" family="Cactaceae" kingdom="Plantae" order="Caryophyllales" pageId="66" pageNumber="65" phylum="Tracheophyta" rank="family">Cactaceae</taxonomicName>
