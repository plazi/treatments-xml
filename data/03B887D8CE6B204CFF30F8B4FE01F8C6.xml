<number id="3DD924E9CE69204CFE9EF93CFE56F956" box="[313,432,1664,1690]" pageId="10" pageNumber="55" value="1.92" valueMax="2.07" valueMin="1.77">1.77–2.07</number>
, 
<number id="3DD924E9CE69204CFE1BF93CFE15F956" box="[444,499,1664,1690]" pageId="10" pageNumber="55" value="1.94">1.94</number>
. HV 
<number id="3DD924E9CE69204CFD97F93CFD4EF956" box="[560,680,1664,1690]" pageId="10" pageNumber="55" value="2.8" valueMax="3.14" valueMin="2.46">2.46–3.14</number>
, 
<number id="3DD924E9CE69204CFD13F93CFD0DF956" box="[692,747,1664,1690]" pageId="10" pageNumber="55" value="2.88">2.88</number>
.
