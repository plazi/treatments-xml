<subSubSection pageId="5" pageNumber="67" type="etymology">
<paragraph blockId="5.[151,1437,152,1376]" pageId="5" pageNumber="67">
<emphasis bold="true" box="[197,331,1002,1028]" pageId="5" pageNumber="67">Etymology</emphasis>
