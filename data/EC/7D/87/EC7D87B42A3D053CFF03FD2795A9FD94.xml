<typeStatus box="[287,389,476,500]" pageId="22" pageNumber="23">Holotype</typeStatus>
</emphasis>
. Male, 
<collectingCountry box="[485,615,476,501]" name="Costa Rica" pageId="22" pageNumber="23">Costa Rica</collectingCountry>
, Puntarenas province, Coto Brus, La Amistad International Park, Las Tablas Protected Zone, Biological Station Las Alturas, 
<quantity box="[798,959,511,536]" metricMagnitude="3" metricUnit="m" metricValue="1.55" metricValueMax="1.6" metricValueMin="1.5" pageId="22" pageNumber="23" unit="m" value="1550.0" valueMax="1600.0" valueMin="1500.0">1500–1600 m</quantity>
, 
<date box="[975,1174,511,536]" pageId="22" pageNumber="23" value="2012-10-14" valueMax="2012-10-15" valueMin="2012-10-14">
<number box="[975,1045,511,536]" pageId="22" pageNumber="23" value="14.5" valueMax="15.0" valueMin="14.0">14–15</number>
Oct. 
<number box="[1115,1174,512,536]" pageId="22" pageNumber="23" value="2012.0">2012</number>
