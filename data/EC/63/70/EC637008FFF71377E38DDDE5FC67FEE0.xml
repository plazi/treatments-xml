<paragraph blockId="6.[151,1437,152,2022]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[198,445,1435,1461]" pageId="6" pageNumber="7">Material examined.</emphasis>
<typeStatus box="[455,564,1436,1462]" pageId="6" pageNumber="7">Holotype</typeStatus>
female, two 
<typeStatus box="[726,827,1436,1462]" pageId="6" pageNumber="7">paratype</typeStatus>
females and one 
