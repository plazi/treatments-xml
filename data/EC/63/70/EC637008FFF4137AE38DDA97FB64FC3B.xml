<paragraph blockId="7.[151,1438,591,2005]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[151,303,591,617]" pageId="7" pageNumber="8">Dimensions.</emphasis>
<typeStatus box="[311,420,592,618]" pageId="7" pageNumber="8">Holotype</typeStatus>
female: length 
<number box="[606,653,592,618]" pageId="7" pageNumber="8" value="551.0">551</number>
, breadth 
<number box="[761,808,592,618]" pageId="7" pageNumber="8" value="367.0">367</number>
; 
