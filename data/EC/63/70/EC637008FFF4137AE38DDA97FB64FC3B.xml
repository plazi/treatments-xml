female: length 
<number box="[1108,1155,592,618]" pageId="7" pageNumber="8" value="580.0">580</number>
, breadths 
<number box="[1275,1322,592,618]" pageId="7" pageNumber="8" value="343.0">343</number>
; 
<typeStatus box="[1335,1436,592,618]" pageId="7" pageNumber="8">paratype</typeStatus>
males: length 
<number box="[315,421,629,655]" pageId="7" pageNumber="8" value="530.535">530, 535</number>
, breadth 
