<typeStatus box="[164,216,1134,1155]" pageId="7" pageNumber="90">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Hogg, 1905" authorityName="Hogg" authorityYear="1905" box="[324,631,1133,1155]" class="Arachnida" family="Lycosidae" genus="Lycosa" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="90" phylum="Arthropoda" rank="species" species="errans">
