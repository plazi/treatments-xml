<taxonomicName authority="hort. ex G. Nicholson, The Garden" authorityName="G. Nicholson, The Garden" box="[189,900,1929,1956]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="variety" species="betulus" variety="fastigiata">
