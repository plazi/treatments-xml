, 
<collectingCountry box="[1073,1130,1930,1956]" name="Iceland" pageId="10" pageNumber="11">Insel</collectingCountry>
Yesso, Mororan, 
<date pageId="10" pageNumber="11" value="1904-09">Sep. 1904</date>
, 
<emphasis box="[312,524,1966,1992]" italics="true" pageId="10" pageNumber="11">U.J. Faurie 5775</emphasis>
