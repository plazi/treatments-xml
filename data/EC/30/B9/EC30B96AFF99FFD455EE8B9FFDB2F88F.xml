<collectingCountry box="[594,666,1291,1317]" name="China" pageId="15" pageNumber="16">China</collectingCountry>
</emphasis>
(
<bibRefCitation author="Li P. C. &amp; Skvortsov A. K." box="[688,959,1291,1317]" pageId="15" pageNumber="16" pagination="286 - 301" refId="ref41365" refString="Li P. C. &amp; Skvortsov A. K. 1999. Betulaceae. In: Wu Z. Y. &amp; Raven P. H. (eds) Flora of China. Vol. 4: 286 - 301. Science Press, Beijing, and Missouri Botanical Garden Press, St. Louis." type="journal article" year="1999">Li &amp; Skvortsov 1999</bibRefCitation>
) with a distinct 
<taxonomicName authority="(Hayata) S. S. Ying" authorityName="S. S. Ying" authorityYear="1913" baseAuthorityName="Hayata" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="variety" species="kawakamii" variety="minutiserrata">
