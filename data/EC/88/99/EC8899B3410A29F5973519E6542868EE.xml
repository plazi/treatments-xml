<taxonomicName class="Arachnida" family="Anapidae" genus="Comaroma" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="85" phylum="Arthropoda" rank="genus">Comaroma</taxonomicName>
. 
<taxonomicName class="Arachnida" family="Anapidae" genus="Comaroma" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="85" phylum="Arthropoda" rank="species" species="simoni">simoni</taxonomicName>
