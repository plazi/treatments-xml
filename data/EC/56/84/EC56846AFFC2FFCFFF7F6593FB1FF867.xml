<collectingCountry name="Brazil" pageId="4" pageNumber="43">Brazil</collectingCountry>
, 
<collectingCountry box="[193,282,2011,2035]" name="Mexico" pageId="4" pageNumber="43">Mexico</collectingCountry>
, 
<collectingCountry box="[292,407,2011,2035]" name="Venezuela" pageId="4" pageNumber="43">Venezuela</collectingCountry>
and 
<collectingCountry box="[462,539,2011,2035]" name="United States of America" pageId="4" pageNumber="43">U.S.A.</collectingCountry>
(
