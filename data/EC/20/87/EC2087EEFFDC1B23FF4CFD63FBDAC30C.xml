<number box="[200,228,1563,1588]" pageId="6" pageNumber="351" value="75.0">75</number>
(
<number box="[246,321,1564,1588]" pageId="6" pageNumber="351" value="75.5" valueMax="77.0" valueMin="74.0">74–77</number>
