<keyStep pageId="15" pageNumber="264">
<paragraph blockId="15.[151,1437,565,617]" box="[151,1437,565,587]" pageId="15" pageNumber="264">
<keyLead box="[151,1437,565,587]" pageId="15" pageNumber="264">
5. Cephalothorax length&gt; 
<quantity box="[453,522,565,586]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="15" pageNumber="264" unit="mm" value="2.1">2.1mm</quantity>
