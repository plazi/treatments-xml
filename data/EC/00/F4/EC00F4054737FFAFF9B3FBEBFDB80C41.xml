<subSubSection pageId="7" pageNumber="8" type="materials_examined">
<paragraph blockId="7.[151,1436,151,294]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[198,513,151,177]" pageId="7" pageNumber="8">Type material examined:</emphasis>
<taxonomicName authorityName="Brusca" authorityYear="1971" box="[522,745,152,177]" class="Insecta" family="Leptohyphidae" genus="Leptohyphes" kingdom="Animalia" order="Ephemeroptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="alleni">
<emphasis box="[522,745,152,177]" italics="true" pageId="7" pageNumber="8">Leptohyphes alleni</emphasis>
</taxonomicName>
<materialsCitation collectingDate="1968-11-07" collectionCode="RKA" collectorName="N. Huajuapan de Leon" country="Mexico" elevation="2" location="Huajuapan de Leon" pageId="7" pageNumber="8" specimenCode="CAS 13600" specimenCount="1" stateProvince="Oaxaca" typeStatus="holotype">
