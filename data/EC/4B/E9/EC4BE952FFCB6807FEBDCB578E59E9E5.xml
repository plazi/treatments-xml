<heading bold="true" box="[264,510,361,387]" fontSize="11" level="1" pageId="9" pageNumber="10" reason="1">
<emphasis bold="true" box="[264,510,361,387]" pageId="9" pageNumber="10">
<taxonomicName authority="Blahnik Table Of Contents, 2005" authorityName="Blahnik Table Of Contents" authorityYear="2005" box="[264,365,361,387]" class="Insecta" family="Philopotamidae" genus="Alterosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus" status="new genus">
