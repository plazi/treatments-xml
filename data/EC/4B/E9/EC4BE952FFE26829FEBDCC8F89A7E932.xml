<taxonomicName authorityName="Blahnik Table Of Contents" authorityYear="2005" box="[725,840,1279,1304]" class="Insecta" family="Philopotamidae" genus="Alterosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="intervales">
