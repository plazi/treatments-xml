<number box="[917,989,151,176]" pageId="9" pageNumber="591" value="56.5" valueMax="61.0" valueMin="52.0">52–61</number>
vs 
<quantity box="[1034,1136,151,176]" metricMagnitude="-1" metricUnit="m" metricValue="5.715" metricValueMax="6.096" metricValueMin="5.334" pageId="9" pageNumber="591" unit="in" value="22.5" valueMax="24.0" valueMin="21.0">21–24 in</quantity>
<taxonomicName box="[1147,1284,151,176]" class="Adenophorea" family="Mononchidae" genus="Paramononchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="9" pageNumber="591" phylum="Nematoda" rank="species" species="orientalis">
<emphasis box="[1147,1284,151,176]" italics="true" pageId="9" pageNumber="591">P. orientalis</emphasis>
</taxonomicName>
) (
<bibRefCitation author="Gagarin" pageId="9" pageNumber="591" refString="Gagarin, V. G. &amp; Naumova, T. V. (2012 c) Paramononchus orientalis sp. n. and Ethmolaimus maximus sp. n. (Nematoda) from Lake Baikal, Russia. Zootaxa, 3513, 51 - 60." type="journal article" year="2012" yearSuffix="c">
Gagarin &amp; Naumova 
<number box="[267,328,188,212]" pageId="9" pageNumber="591" value="2012.0">2012</number>
c
</bibRefCitation>
).
</paragraph>
