female, and 
<specimenCount box="[1140,1300,1283,1323]" pageId="67" pageNumber="78" type="male">one male</specimenCount>
<typeStatus box="[1317,1472,1283,1323]" pageId="67" pageNumber="78">paratype</typeStatus>
