<taxonomicName authority=", Usinger. A" authorityName="Usinger. A" box="[465,1027,1334,1374]" class="Magnoliopsida" family="Piperaceae" genus="Piper" kingdom="Plantae" order="Piperales" pageId="67" pageNumber="78" phylum="Tracheophyta" rank="species" species="guahamense">
