<collectingCountry box="[1081,1174,2141,2185]" name="Micronesia" pageId="18" pageNumber="29">Truk</collectingCountry>
, 
<collectingCountry box="[1195,1483,2141,2185]" name="Micronesia" pageId="18" pageNumber="29">Caroline Islands</collectingCountry>
