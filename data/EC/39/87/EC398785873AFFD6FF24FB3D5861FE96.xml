<quantity box="[416,503,1652,1676]" metricMagnitude="0" metricUnit="m" metricValue="5.5" metricValueMax="10.0" metricValueMin="1.0" pageId="21" pageNumber="422" unit="m" value="5.5" valueMax="10.0" valueMin="1.0">1–10 m</quantity>
