</taxonomicName>
, 
<taxonomicName authority="Hanley" authorityName="Hanley" box="[841,1109,1687,1712]" class="Bivalvia" family="Ostreidae" genus="Ostrea" kingdom="Animalia" order="Ostreoida" pageId="21" pageNumber="422" phylum="Mollusca" rank="species" species="columbiensis">
<emphasis box="[841,1021,1688,1712]" italics="true" pageId="21" pageNumber="422">O. columbiensis</emphasis>
