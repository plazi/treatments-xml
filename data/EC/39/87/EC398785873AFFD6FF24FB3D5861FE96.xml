<subSubSection pageId="21" pageNumber="422" type="description">
<paragraph blockId="21.[151,1437,1543,2037]" pageId="21" pageNumber="422">
<emphasis bold="true" box="[199,298,1652,1677]" pageId="21" pageNumber="422">Habitat.</emphasis>
