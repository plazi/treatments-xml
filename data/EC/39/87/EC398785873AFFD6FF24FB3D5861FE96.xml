</taxonomicName>
and 
<taxonomicName box="[1164,1371,1688,1712]" class="Bivalvia" family="Spondylidae" genus="Spondylus" kingdom="Animalia" order="Pectinoida" pageId="21" pageNumber="422" phylum="Mollusca" rank="species" species="calcifer">
