<quantity box="[369,462,291,315]" metricMagnitude="1" metricUnit="m" metricValue="4.95" metricValueMax="9.0" metricValueMin="0.9" pageId="30" pageNumber="431" unit="m" value="49.5" valueMax="90.0" valueMin="9.0">9–90 m</quantity>
