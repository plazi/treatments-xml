<paragraph pageId="0" pageNumber="73">
Two specimens 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" unit="mm" value="45.0" valueMax="50.0" valueMin="40.0">40-50 mm</quantity>
