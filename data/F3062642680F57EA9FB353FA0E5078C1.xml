<taxonomicName id="AB07F80C1409161856CE6C10CA5CE248" class="Insecta" family="Halictidae" genus="Sphecodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphecodes atlanticus" order="Hymenoptera" pageId="3" pageNumber="16" phylum="Arthropoda" rank="species" species="atlanticus">Sphecodes atlanticus</taxonomicName>
