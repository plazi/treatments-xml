<taxonomicName id="19F34D1E933B6212FC93FDE1FB10FD10" authority="Evenhuis et al. (2015: 72)" authorityName="Evenhuis" authorityPageNumber="72" authorityYear="2015" box="[822,1177,620,645]" class="Insecta" family="Anthomyiidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="teste">
