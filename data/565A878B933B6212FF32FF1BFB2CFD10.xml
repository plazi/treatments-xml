<taxonomicName id="19F34D1E933B6212FE3AFDCBFD33FDF4" authority="Rondani, 1856" authorityName="Rondani" authorityYear="1856" box="[415,698,583,608]" class="Insecta" family="Tachinidae" genus="Chetoliga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
