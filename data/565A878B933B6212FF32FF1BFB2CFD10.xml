<taxonomicName id="19F34D1E933B6212FF60FDE0FE57FD10" authority="Brauer, 1880" authorityName="Brauer" authorityYear="1880" box="[197,478,620,645]" class="Insecta" family="Tachinidae" genus="Chaetolyga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
