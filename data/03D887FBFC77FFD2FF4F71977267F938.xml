<taxonomicName id="4C714D6EFC74FFD2FDEE71E27001F970" authority="Dejean." authorityName="Dejean." box="[566,871,1689,1714]" class="Insecta" family="Carabidae" genus="Cicindela" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="435" phylum="Arthropoda" rank="species" species="nilotica">
<emphasis id="B905EAFFFC74FFD2FDEE71E27062F970" box="[566,772,1689,1714]" italics="true" pageId="34" pageNumber="435">Cicindela nilotica</emphasis>
Dejean.
</taxonomicName>
<taxonomicName id="4C714D6EFC74FFD2FCAA71E27736F970" box="[882,1104,1689,1714]" class="Insecta" family="Carabidae" genus="Habrodera" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="435" phylum="Arthropoda" rank="species" species="nitidula">
