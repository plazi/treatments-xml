(Durango fir) and 
<taxonomicName id="6E4698DF039EDCF4D6C00FACE61012AB" class="Pinopsida" family="Pinaceae" genus="Pseudotsuga" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pseudotsuga menziesii" order="Pinales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="menziesii">Pseudotsuga menziesii</taxonomicName>
