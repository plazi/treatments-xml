<taxonomicName id="1E04C6945685E8A8EC18E715770D2E29" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Abies" order="Pinales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="genus">Abies</taxonomicName>
