<taxonomicName id="4C4051BCFF83FFFCFECAFC25801F39E2" authorityName="FAUVEL" authorityYear="1895" box="[357,563,962,984]" class="Insecta" family="Staphylinidae" genus="Stilicoderus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="485" phylum="Arthropoda" rank="species" species="discalis">
