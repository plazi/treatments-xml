<collectingCountry id="F3576AAFFF83FFFCFE9AFC3983BE39CE" box="[309,402,990,1012]" name="Thailand" pageId="4" pageNumber="485">Thailand</collectingCountry>
, 
<collectingCountry id="F3576AAFFF83FFFCFE34FC3883E339CE" box="[411,463,991,1012]" name="Laos" pageId="4" pageNumber="485">Laos</collectingCountry>
, and 
<collectingCountry id="F3576AAFFF83FFFCFDABFC39807739CE" box="[516,603,990,1012]" name="Vietnam" pageId="4" pageNumber="485">Vietnam</collectingCountry>
