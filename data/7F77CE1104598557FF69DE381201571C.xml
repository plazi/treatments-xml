<emphasis id="C5AAA31504588558FF69DC6E106F5351" bold="true" box="[151,287,1856,1880]" pageId="71" pageNumber="72">FIGURE 33.</emphasis>
<taxonomicName id="30DE048404588558FEDBDC6E135C5351" authority="Carvalho-Filho &amp; Esposito &amp; Mello-Patiu, 2021" authorityName="Carvalho-Filho &amp; Esposito &amp; Mello-Patiu" authorityYear="2021" box="[293,556,1856,1880]" class="Insecta" family="Sarcophagidae" genus="Nephochaetopteryx" kingdom="Animalia" order="Diptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="tembe" status="sp. nov.">
<emphasis id="C5AAA31504588558FEDBDC6E135C5351" box="[293,556,1856,1880]" italics="true" pageId="71" pageNumber="72">Nephochaetopteryx tembe</emphasis>
</taxonomicName>
<emphasis id="C5AAA31504588558FDCCDC6E13F25351" bold="true" box="[562,642,1856,1880]" pageId="71" pageNumber="72">
