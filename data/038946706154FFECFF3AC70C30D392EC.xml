<taxonomicName id="4C208CE56154FFEDFF3AC7553602959E" authority="Priesner, 1932: 172" authorityName="Priesner" authorityPageNumber="172" authorityYear="1932" box="[151,564,1908,1930]" class="Insecta" family="Thripidae" genus="Sericothrips" kingdom="Animalia" order="Thysanoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="signifer">
<emphasis id="B9542B746154FFEDFF3AC7553550959E" box="[151,358,1908,1930]" italics="true" pageId="38" pageNumber="39">Sericothrips signifer</emphasis>
<treatmentCitation id="0A81D1776154FFEDFEC1C7553602959E" author="Priesner" box="[364,564,1908,1930]" page="172" pageId="38" pageNumber="39" year="1932">
<bibRefCitation id="EFB18A976154FFEDFEC1C7553637959E" author="Priesner" box="[364,513,1908,1930]" pageId="38" pageNumber="39" refString="Priesner, H. (1932) Neue Thysanopteren aus Mexiko, gesammelt von Prof. Dr. A. Dampf. Teil 1. Wiener Entomologischer Zeitung, 49, 170 - 185." type="journal article" year="1932">Priesner, 1932</bibRefCitation>
