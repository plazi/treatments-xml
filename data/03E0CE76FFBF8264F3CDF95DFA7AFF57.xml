<taxonomicName id="4C4904E3FFBF8262F2D3F95BFD49F930" authorityName="Brown" authorityYear="1927" box="[478,680,1669,1692]" class="Insecta" family="Aphodiidae" genus="Pseudataenius" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="10" phylum="Arthropoda" rank="genus">
