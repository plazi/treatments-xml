<taxonomicName id="4C24E3F7FF931B195F77F9F8FE4BF977" box="[283,385,1645,1671]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="45" phylum="Arthropoda" rank="genus">Catocala</taxonomicName>
