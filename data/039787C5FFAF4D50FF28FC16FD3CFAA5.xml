<taxonomicName id="4C3E4D50FFAF4D50FF28FC5EFD56FBE0" authority=": Ahmad et al., 2014: 21" authorityName="Ahmad" authorityPageNumber="21" authorityYear="2014" box="[151,726,1002,1027]" class="Actinopterygii" family="Mastacembelidae" genus="Macrognathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Synbranchiformes" pageId="18" pageNumber="533" phylum="Chordata" rank="species" species="maculatus">
