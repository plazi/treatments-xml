<taxonomicName id="4C4638C7F3147F731754A3E1FBEDFE14" authorityName="Cassola" authorityYear="2003" box="[892,1054,402,427]" class="Insecta" family="Carabidae" genus="Foveodromica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="217" phylum="Arthropoda" rank="genus">
