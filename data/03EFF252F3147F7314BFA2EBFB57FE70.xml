<taxonomicName id="4C4638C7F3147F731049A3E7FB1BFE14" baseAuthorityName="W. Horn" baseAuthorityYear="1907" box="[1121,1256,402,427]" class="Insecta" family="Carabidae" genus="Lophyra" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="217" phylum="Arthropoda" rank="species" species="wellmani">
