<taxonomicName id="4C4638C7F3147F731634A3C4FD52FE70" authorityName="Burgeon" authorityYear="1928" box="[540,673,438,463]" class="Insecta" family="Carabidae" genus="Graphipterus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="217" phylum="Arthropoda" rank="species" species="ellipticus">
