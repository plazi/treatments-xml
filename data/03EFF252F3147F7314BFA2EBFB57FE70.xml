<taxonomicName id="4C4638C7F3147F73172FA3E7FC99FE14" authorityName="Dejean" authorityYear="1826" box="[775,874,404,427]" class="Insecta" family="Carabidae" genus="Dromica" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="217" phylum="Arthropoda" rank="genus">
