<taxonomicName id="BBA74282B32EFFE3FF7DA03A9916317D" box="[244,380,1301,1325]" class="Malacostraca" family="Inachidae" genus="Dorhynchus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="thomsoni">
