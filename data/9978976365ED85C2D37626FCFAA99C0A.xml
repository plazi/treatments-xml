<subSubSection id="59CB75FE65ED85C3D18D2131FC4C9B19" pageId="184" pageNumber="185" type="type_taxon">
<paragraph id="116E267565ED85C3D18D2131FEA09B49" blockId="184.[189,344,1047,1074]" box="[189,344,1047,1074]" pageId="184" pageNumber="185">
<heading id="4A26911965ED85C3D18D2131FEA09B49" bold="true" box="[189,344,1047,1074]" fontSize="11" level="3" pageId="184" pageNumber="185" reason="3">
