<taxonomicName id="4C429DEEFFA7F07BFE16FD53FDD9FCF6" authorityName="Linnaeus" authorityYear="1758" box="[433,568,762,788]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="16" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
