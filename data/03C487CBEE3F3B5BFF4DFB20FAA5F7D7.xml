<subSubSection id="C3776556EE3F3B5BFF4DFA9DFB3BF9F8" pageId="366" pageNumber="367" type="materials_examined">
<paragraph id="8BD236DDEE3F3B5BFF4DFA9DFE5EFA70" blockId="366.[136,778,1363,2070]" pageId="366" pageNumber="367">
<emphasis id="B919EACFEE3F3B5BFF4DFA9DFEB5FAAC" bold="true" box="[136,368,1363,1389]" pageId="366" pageNumber="367">Material examined</emphasis>
