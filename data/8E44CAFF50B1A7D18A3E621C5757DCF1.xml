<normalizedToken id="2C4588E3AD73057D70AB80ECAE5EBF06" originalValue="“Europe”">&quot;Europe&quot;</normalizedToken>
. 
<location id="6DE78708B5D00CDAA228EA3076B5CDA7" LSID="urn:lsid:plazi:treatment:8E44CAFF50B1A7D18A3E621C5757DCF1:6DE78708B5D00CDAA228EA3076B5CDA7" country="Armenia" county="Caspian Sea" municipality="Black Sea" name="Other">Other</location>
