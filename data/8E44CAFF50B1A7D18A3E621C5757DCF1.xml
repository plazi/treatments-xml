<taxonomicName id="FA4DA129FB2C52CC1FC8EAE0C53283EF" authority="Le Peletier &amp; Audinet-Serville, 1828" authorityName="Dovnar-Zapolskij" authorityYear="1925" class="Insecta" family="Tenthredinidae" genus="Empria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Empria pravei" order="Hymenoptera" pageId="6" pageNumber="91" phylum="Arthropoda" rank="species" species="pravei">
