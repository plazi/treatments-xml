<taxonomicName id="4C1E4D0CFF896906FC87F986FC240669" authorityName="BROUN AND" authorityYear="1903" box="[843,939,1639,1657]" class="Insecta" family="Hydrophilidae" genus="Cylomissus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="700" phylum="Arthropoda" rank="genus">
