<taxonomicName id="C1BFE6FAFFD0FF8199F3FE64FEB2289F" authority="Sp." authorityName="Sp." box="[162,298,469,491]" class="Anthozoa" family="Chrysogorgiidae" genus="Radicipes" kingdom="Animalia" order="Alcyonacea" pageId="22" pageNumber="23" phylum="Cnidaria" rank="genus">
