<taxonomicName id="8CC84D31FF8AFFB1EBB80772FD8B6A03" box="[463,565,272,297]" class="Insecta" family="Eriococcidae" genus="Eriococcus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="56" phylum="Arthropoda" rank="species" species="pumiliae">
