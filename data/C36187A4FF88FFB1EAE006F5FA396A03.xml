<emphasis id="79BCEAA0FF8AFFB1EEE00772FA396A03" box="[1175,1415,272,297]" italics="true" pageId="6" pageNumber="56">
<taxonomicName id="8CC84D31FF8AFFB1EEE00772FA3D6A03" box="[1175,1411,272,297]" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="56" phylum="Tracheophyta" rank="species" species="pumilio">Nothofagus pumilio</taxonomicName>
