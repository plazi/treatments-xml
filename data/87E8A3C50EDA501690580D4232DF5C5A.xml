<geoCoordinate id="9C13E2A7D2194FEA202D15D4633F19A0" degrees="12.9207" direction="north" orientation="latitude" precision="5" value="12.9207">12.9207</geoCoordinate>
<geoCoordinate id="880AFAC0B51A29751071AE1A47A6BF2A" degrees="85.9172" direction="west" orientation="longitude" precision="5" value="-85.9172">-85.9172</geoCoordinate>
