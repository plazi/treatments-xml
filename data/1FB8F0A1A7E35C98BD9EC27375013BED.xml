, 
<taxonomicName id="212118D2CB288496E18182E5860AC066" baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Bulimulidae" genus="Thaumastus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thaumastus inca" order="Stylommatophora" pageId="13" pageNumber="30" phylum="Mollusca" rank="species" species="inca">
