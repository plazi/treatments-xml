<emphasis id="B969EA84FFBAFFFB08F1FEDCFEEBFE9A" bold="true" box="[136,297,353,377]" italics="true" pageId="6" pageNumber="7">Distribution. –</emphasis>
<collectingCountry id="F30A7606FFBAFFFB0948FEDCFEB3FE9B" box="[305,369,353,376]" name="China" pageId="6" pageNumber="7">China</collectingCountry>
(
<collectingRegion id="49D9F874FFBAFFFB0906FEDCFE1AFE9B" box="[383,472,353,376]" country="China" name="Yunnan" pageId="6" pageNumber="7">Yunnan</collectingRegion>
, 
<collectingRegion id="49D9F874FFBAFFFB099DFEDCFDF3FE9B" box="[484,561,353,376]" country="China" name="Hunan" pageId="6" pageNumber="7">Hunan</collectingRegion>
