<number id="7500BC6DE17EFFB6B3CB439D3821EF21" box="[1245,1297,1278,1302]" pageId="16" pageNumber="417" value="0.31">0.31</number>
, width 
<number id="7500BC6DE17EFFB6B272439D38A7EF21" box="[1380,1431,1278,1302]" pageId="16" pageNumber="417" value="0.7">0.70</number>
