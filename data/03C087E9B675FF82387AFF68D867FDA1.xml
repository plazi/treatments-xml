<taxonomicName id="4C694D7CB675FF81387BFE0CDA1BF6C9" box="[126,322,2323,2355]" class="Insecta" family="Lestidae" genus="Lestes" higherTaxonomySource="CoL" kingdom="Animalia" order="Odonata" pageId="1" pageNumber="77" phylum="Arthropoda" rank="species" species="nympha">
