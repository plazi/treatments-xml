<subSubSection id="C3436575FFA2FFDA56CC4455A8A3FDC4" lastPageId="18" lastPageNumber="1017" pageId="17" pageNumber="1016" type="description">
<paragraph id="8BE636FEFFA2FFD956CC4455AA90F929" blockId="17.[144,1104,1563,1691]" box="[144,504,1563,1587]" pageId="17" pageNumber="1016">
<heading id="D0AE8192FFA2FFD956CC4455AA90F929" box="[144,504,1563,1587]" fontSize="10" level="3" pageId="17" pageNumber="1016" reason="5">
<emphasis id="B92DEAECFFA2FFD956CC4455AA67F929" box="[144,271,1564,1587]" italics="true" pageId="17" pageNumber="1016">Description</emphasis>
(based on 
<typeStatus id="54E2885CFFA2FFD957D24452AA99F929" box="[398,497,1563,1587]" pageId="17" pageNumber="1016">holotype</typeStatus>
)
