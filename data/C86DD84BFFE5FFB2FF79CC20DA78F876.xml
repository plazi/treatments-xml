<subSubSection id="08DE3AD6FFE5FFB2FF79CCB2DCEDFAA8" pageId="2" pageNumber="28" type="diagnosis">
<paragraph id="407B695DFFE5FFB2FF79CCB2DCEDFAA8" blockId="2.[151,1436,1261,2034]" pageId="2" pageNumber="28">
<emphasis id="72B0B54FFFE5FFB2FF79CCB2DE39FA83" bold="true" box="[151,282,1261,1287]" pageId="2" pageNumber="28">Diagnosis:</emphasis>
