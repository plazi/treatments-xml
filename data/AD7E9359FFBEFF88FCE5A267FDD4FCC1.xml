<taxonomicName id="E2D759CCFFBFFF88FE00A034FD5FFE9F" authority="(Pall. ex Pursh) G. L. Nesom &amp; G. I. Baird" authorityName="G. L. Nesom &amp; G. I. Baird" baseAuthorityName="Pursh" class="Magnoliopsida" family="Asteraceae" genus="Ericameria" kingdom="Plantae" order="Asterales" pageId="3" pageNumber="1019" phylum="Tracheophyta" rank="species" species="nauseosa">
