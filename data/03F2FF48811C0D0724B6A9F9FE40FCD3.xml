<typeStatus id="54E0F0FC811C0D092796A849FB97FCDE" box="[951,1049,823,848]" pageId="55" pageNumber="56">holotype</typeStatus>
(BMNH): 
<number id="3D935C79811C0D0920B4A849FB11FCDE" box="[1173,1183,823,848]" pageId="55" pageNumber="56" value="1.0">1</number>
/ 
<number id="3D935C79811C0D092088A849FB37FCDE" box="[1193,1209,823,848]" pageId="55" pageNumber="56" value="2.0">2</number>
