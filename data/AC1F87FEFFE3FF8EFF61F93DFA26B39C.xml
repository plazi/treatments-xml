<taxonomicName id="E3B64D6BFFECFF8EFCE3FEC2FB81B60C" authority="Pujade-Villar" authorityName="Pujade-Villar" box="[789,1083,298,319]" class="Insecta" family="Cynipidae" genus="Andricus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="459" phylum="Arthropoda" rank="species" species="melikai">
