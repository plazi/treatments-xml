<taxonomicName id="E3B64D6BFFE3FF81FCD8F879FBF5B095" authority="Barbotin" authorityName="Barbotin" authorityYear="1985" box="[814,1103,1937,1958]" class="Insecta" family="Cynipidae" genus="Plagiotrochus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="458" phylum="Arthropoda" rank="species" species="razeti">
