<materialsCitation id="3B633C87D65CC77543D0F913FB994FC9" ID-GBIF-Occurrence="3412504302" country="France" location="Sisteron" municipality="Cretaceous" pageId="5" pageNumber="6" specimenCount="1" specimenCount-male="1" typeStatus="holotype">
