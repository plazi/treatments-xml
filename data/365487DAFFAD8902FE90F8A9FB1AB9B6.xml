<taxonomicName id="79FD4D4FFFAF8902FE28FBD0FE09B85B" authority="(G. F. Frolich, 1793)" baseAuthorityName="G. F. Frolich" baseAuthorityYear="1793" box="[421,494,1100,1123]" pageId="2" pageNumber="3" rank="species" species="argus">
