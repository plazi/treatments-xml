<taxonomicName id="4C19C9C9182FFFC890B4CA043116C6BE" authority="Schmalfuss &amp; Ferrara, 1983" authorityName="Schmalfuss &amp; Ferrara" authorityYear="1983" class="Malacostraca" family="Armadillidae" genus="Anchicubaris" kingdom="Animalia" order="Isopoda" pageId="3" pageNumber="74" phylum="Arthropoda" rank="species" species="annobonensis">
