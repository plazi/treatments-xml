<taxonomicName id="6AF062D8A85DFFFAD6FFFA708CC6FA5C" authority="ANDREWES, 1931" authorityName="ANDREWES" authorityYear="1931" box="[159,387,1515,1533]" class="Insecta" family="Carabidae" genus="Orictites" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="minotaur">
