<taxonomicName id="6AF062D8A85DFFFAD5FCFA4A8EA2FA42" authorityName="ANDREWES" authorityYear="1931" box="[924,999,1489,1507]" class="Insecta" family="Carabidae" genus="Orictites" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
<emphasis id="9F84C549A85DFFFAD5FCFA4A8EA2FA42" bold="true" box="[924,999,1489,1507]" italics="true" pageId="14" pageNumber="15">Orictites</emphasis>
