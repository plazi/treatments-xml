<number id="3DE924EFFFC29F08F886A84655B6FD95" box="[682,698,554,580]" pageId="2" pageNumber="42" value="4.0">4</number>
antennae with 
<number id="3DE924EFFFC29F08F946A846548BFD95" box="[874,903,554,580]" pageId="2" pageNumber="42" value="19.0">19</number>
articles, 
<number id="3DE924EFFFC29F08F9DCA8465301FD95" box="[1008,1037,554,580]" pageId="2" pageNumber="42" value="14.0">14</number>
with 
<number id="3DE924EFFFC29F08FE63A8465385FD95" box="[1103,1161,554,580]" pageId="2" pageNumber="42" value="20.9">20, 9</number>
with 
<number id="3DE924EFFFC29F08FEE7A8465209FD95" box="[1227,1285,554,580]" pageId="2" pageNumber="42" value="21.2">21, 2</number>
with 
<number id="3DE924EFFFC29F08FF6AA846526AFD95" box="[1350,1382,554,580]" pageId="2" pageNumber="42" value="22.0">22</number>
, and 
<number id="3DE924EFFFC29F08FABBA83C57AAFDBB" box="[151,166,592,618]" pageId="2" pageNumber="42" value="2.0">2</number>
with 
<number id="3DE924EFFFC29F08FAC7A83C5600FDBB" box="[235,268,592,618]" pageId="2" pageNumber="42" value="23.0">23</number>
