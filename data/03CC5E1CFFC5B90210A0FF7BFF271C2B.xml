<typeStatus id="54DE51A8FFC5B9031113FEE6FE571C75" box="[292,394,266,292]" pageId="6" pageNumber="59">holotype</typeStatus>
(
<collectionCode id="ED7477CFFFC5B90311AEFEE6FE2B1C75" box="[409,502,266,292]" httpUri="http://grbio.org/cool/zxpe-c0as" name="Virginia Museum of Natural History" pageId="6" pageNumber="59">VMNH</collectionCode>
) from Bowden Cave, 
<quantity id="4C9D42EFFFC5B90312CFFEE6FC911C75" box="[760,844,266,292]" metricMagnitude="4" metricUnit="m" metricValue="1.1265408" pageId="6" pageNumber="59" unit="mi" value="7.0">7 miles</quantity>
