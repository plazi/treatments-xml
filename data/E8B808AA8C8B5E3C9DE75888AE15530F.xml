<taxonomicName id="324F749448F01244454E34E8BA4A36AC" authorityName="Damadi &amp; Moghaddam &amp; Ghassemzadeh &amp; Ghanbarifardi" authorityYear="2020" class="Actinopterygii" family="Haemulidae" genus="Plectorhinchus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Plectorhinchus makranensis" order="Perciformes" pageId="0" pageNumber="141" phylum="Chordata" rank="species" species="makranensis">
