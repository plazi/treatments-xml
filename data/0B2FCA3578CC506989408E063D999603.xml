<paragraph id="9A91D6CCA9129627D0B7EF0F58AAF89B" pageId="0" pageNumber="127">
<materialsCitation id="1DD677EAFAFC67B9C278755C7A5F1446" collectingDate="1975-06-15" collectingDateMax="2000-08-10" collectingDateMin="1975-06-15" collectorName="D. K. Smith, W. B. Schofield, S. S. Talbot, G. Argus" country="U. S. A." county="Adak Island" latitude="54.916668" location="Attu Island" longLatPrecision="1067" longitude="-159.25" municipality="Finger Bay" specimenCount="1" stateProvince="Alaska">
<collectingCountry id="6B057F05A0F389CBE7DF2D5A91A85A4C" name="United States of America">U.S.A.</collectingCountry>
<collectingRegion id="4893E27FABD89AED44483A53513CF661" country="United States of America" name="Alaska">Alaska</collectingRegion>
: 
<collectingCounty id="994EB49B1E0E74105212218BB16E52B6">Adak Island</collectingCounty>
, 
