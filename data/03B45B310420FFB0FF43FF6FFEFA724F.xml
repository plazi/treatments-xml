<number id="3DD5F8000420FFB0FAF2FCC8F86F7618" box="[1318,1326,816,830]" pageId="7" pageNumber="8" value="4.0">4</number>
+ 
<number id="3DD5F8000420FFB0FAE3FCC8F87F7618" box="[1335,1342,816,830]" pageId="7" pageNumber="8" value="5.0">5</number>
and M 
<number id="3DD5F8000420FFB0FA58FCC8F8D57618" box="[1420,1428,816,830]" pageId="7" pageNumber="8" value="1.0">1</number>
; basal extremity of brown subapical spot on R 
<number id="3DD5F8000420FFB0FD77FCAFFFEA7643" box="[675,683,855,869]" pageId="7" pageNumber="8" value="4.0">4</number>
+ 
<number id="3DD5F8000420FFB0FD60FCAFFFFA7643" box="[692,699,855,869]" pageId="7" pageNumber="8" value="5.0">5</number>
before vertical level of spots on R 
<number id="3DD5F8000420FFB0FB99FCAFF9147643" box="[1101,1109,855,869]" pageId="7" pageNumber="8" value="2.0">2</number>
