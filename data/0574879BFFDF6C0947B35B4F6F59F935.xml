<emphasis id="BFA9EA9FFFDF6C0945565A926982FA50" bold="true" box="[626,719,1507,1533]" pageId="1" pageNumber="60">sp. nov.</emphasis>
</taxonomicNameLabel>
from the Ypresian of North 
<collectingCountry id="F5CA761DFFDF6C09433F5A956FCDFA53" box="[1051,1152,1508,1534]" name="United States of America" pageId="1" pageNumber="60">America</collectingCountry>
(Republic, Washington, 
<collectingCountry id="F5CA761DFFDF6C0947B3597B6BA2F989" box="[151,239,1546,1572]" name="United States of America" pageId="1" pageNumber="60">U.S.A.</collectingCountry>
); five extant species from 
<collectingCountry id="F5CA761DFFDF6C09451F597B69E1F989" box="[571,684,1546,1572]" name="Australia" pageId="1" pageNumber="60">Australia</collectingCountry>
and Tasmania: 
<taxonomicName id="4ADD4D0EFFDF6C094454597A6F13F989" box="[880,1118,1547,1572]" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="myrmeleonoides">
<emphasis id="BFA9EA9FFFDF6C094454597A6F13F989" box="[880,1118,1547,1572]" italics="true" pageId="1" pageNumber="60">N. myrmeleonoides</emphasis>
</taxonomicName>
, 
<taxonomicName id="4ADD4D0EFFDF6C09434B597A6B99F9E6" authority="Gerstaecker, 1885" authorityName="Gerstaecker" authorityYear="1885" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="modesta">
<emphasis id="BFA9EA9FFFDF6C09434B597A6FB7F989" box="[1135,1274,1547,1572]" italics="true" pageId="1" pageNumber="60">N. modesta</emphasis>
<bibRefCitation id="E94C4B7CFFDF6C094227597B6B99F9E6" author="Gerstaecker" pageId="1" pageNumber="67" refString="Gerstaecker, C. E. A. (1885 b) Zwei fernere decaden Australischer Neuroptera Megaloptera. Mittheilungen aus dem Naturwissenschaftlichen Verein fur Neu-Vorpommern und Rugen, 16 (for 1884), 84 - 116." type="journal article" year="1885" yearSuffix="b">
Gerstaecker, 
<number id="3B1524AAFFDF6C0947B359406B99F9E6" box="[151,212,1585,1611]" pageId="1" pageNumber="60" value="1885.0">1885</number>
</bibRefCitation>
</taxonomicName>
b; 
<taxonomicName id="4ADD4D0EFFDF6C0947CB59406AB6F9E7" box="[239,507,1585,1610]" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="paramyrmeleonides">
<emphasis id="BFA9EA9FFFDF6C0947CB59406AB6F9E7" box="[239,507,1585,1610]" italics="true" pageId="1" pageNumber="60">N. paramyrmeleonides</emphasis>
</taxonomicName>
<bibRefCitation id="E94C4B7CFFDF6C094526594069C9F9E6" author="New" box="[514,644,1585,1611]" pageId="1" pageNumber="68" refString="New, T. R. (1982) A revision of the Australian Nymphidae (Insecta: Neuroptera). Australian Journal of Zoology, 29 (for 1981), 707 - 750." type="journal article" year="1982">
New, 
<number id="3B1524AAFFDF6C094562594069C9F9E6" box="[582,644,1585,1611]" pageId="1" pageNumber="60" value="1982.0">1982</number>
</bibRefCitation>
; 
<taxonomicName id="4ADD4D0EFFDF6C0945B459406863F9E7" box="[656,814,1585,1610]" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="nigrescens">
<emphasis id="BFA9EA9FFFDF6C0945B459406863F9E7" box="[656,814,1585,1610]" italics="true" pageId="1" pageNumber="60">N. nigrescens</emphasis>
</taxonomicName>
<bibRefCitation id="E94C4B7CFFDF6C094411594068F5F9E6" author="New" box="[821,952,1585,1611]" pageId="1" pageNumber="68" refString="New, T. R. (1982) A revision of the Australian Nymphidae (Insecta: Neuroptera). Australian Journal of Zoology, 29 (for 1981), 707 - 750." type="journal article" year="1982">
New, 
<number id="3B1524AAFFDF6C09445E594068F5F9E6" box="[890,952,1585,1611]" pageId="1" pageNumber="60" value="1982.0">1982</number>
</bibRefCitation>
; 
<taxonomicName id="4ADD4D0EFFDF6C0944E059406F7EF9E7" box="[964,1075,1585,1610]" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="aperta">
