<taxonomicName id="4C4CCBEDBA170D2F6936FB3C07FE9F27" authority="Navas (1932)" authorityName="Navas" authorityYear="1932" box="[330,732,272,298]" class="Insecta" family="Stenopsychidae" genus="Stenopsyche" kingdom="Animalia" order="Trichoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="ulmeri">
