<taxonomicName id="4C4CCBEDBA170D2F6B60FB3C01BE9F27" authority="Hwang (1963)" authorityName="Hwang" authorityYear="1963" box="[796,1180,272,298]" class="Insecta" family="Stenopsychidae" genus="Stenopsyche" kingdom="Animalia" order="Trichoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="appendiculata">
