<materialsCitation id="3B253CF3FEA0CB5AFE64FAADFB8DFE95" collectorName="Warren &amp; Banos. In &amp; Banos &amp; Balzapamba &amp; On &amp; The &amp; However &amp; Richard Haensch" country="Peru" elevation="750" location="Peru" pageId="320" pageNumber="457" specimenCount="2" specimenCount-female="1" specimenCount-male="1" typeStatus="syntype">
