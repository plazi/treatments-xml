<quantity id="4CB59B4BFEA0CB5AFF3AFA8DFF61FAC0" box="[176,241,1325,1351]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="320" pageNumber="457" unit="m" value="750.0">
