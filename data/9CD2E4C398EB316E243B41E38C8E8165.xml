<taxonomicName id="B0D8E56C0076D67BD70761CE69C54A55" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181908" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole morelosana Wilson" lsidName-HNS="Pheidole morelosana Wilson" order="Hymenoptera" pageNumber="465" phylum="Arthropoda" rank="species" species="morelosana">Pheidole morelosana</taxonomicName>
