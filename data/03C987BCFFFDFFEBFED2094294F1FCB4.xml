<typeStatus id="54DB8808FFFDFFEBFEE20A599625FDF5" box="[312,367,519,544]" pageId="1" pageNumber="2">Type</typeStatus>
species: 
<taxonomicName id="4C604D29FFFDFFEBFE060A5995ACFDF5" authority="Beddome 1870" authorityName="Beddome" authorityYear="1870" box="[476,742,519,544]" class="Amphibia" family="Caeciliidae" genus="Gegeneophis" kingdom="Animalia" order="Gymnophiona" pageId="1" pageNumber="2" phylum="Chordata" rank="species" species="carnosus">Gegeneophis carnosus</taxonomicName>
</emphasis>
(
<bibRefCitation id="EFF14B5BFFFDFFEBFD2F0A5994E0FDF4" author="Beddome" box="[757,938,519,545]" pageId="1" pageNumber="7" refString="Beddome, R. H. (1870) Descriptions of new reptiles from the Madras Presidency. Madras Monthly Journal of Medicine and Science, 2, 169 - 176." type="journal article" year="1870">
