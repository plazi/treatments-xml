<geoCoordinate id="01964EAAFFB3FFBBFEB4130312FFFBCF" box="[294,429,1053,1077]" degrees="095. 5679" direction="west" orientation="longitude" pageId="7" pageNumber="34" precision="5" value="-95.5679">095. 5679°W</geoCoordinate>
