<collectingCountry box="[605,701,1207,1228]" name="Argentina" pageId="24" pageNumber="475">Argentina</collectingCountry>
............................................... 
<taxonomicName box="[1281,1435,1207,1228]" class="Actinopterygii" family="Gobiidae" genus="Gobiosoma" kingdom="Animalia" order="Perciformes" pageId="24" pageNumber="475" phylum="Chordata" rank="species" species="hemigymnum">
<emphasis box="[1281,1435,1207,1228]" italics="true" pageId="24" pageNumber="475">G. hemigymnum</emphasis>
