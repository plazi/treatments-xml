<paragraph blockId="13.[490,824,1507,1532]" box="[490,824,1507,1532]" pageId="13" pageNumber="1116">
<heading box="[490,824,1507,1532]" centered="true" fontSize="11" level="2" pageId="13" pageNumber="1116" reason="2">
