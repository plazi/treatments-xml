<paragraph blockId="28.[160,1155,1425,1727]" lastBlockId="29.[160,1156,142,1345]" lastPageId="29" lastPageNumber="1132" pageId="28" pageNumber="1131">
<emphasis bold="true" box="[160,360,1460,1485]" inLineHeading="true" italics="true" pageId="28" pageNumber="1131" reason="3">
Male (
<typeStatus box="[242,344,1460,1485]" pageId="28" pageNumber="1131">holotype</typeStatus>
