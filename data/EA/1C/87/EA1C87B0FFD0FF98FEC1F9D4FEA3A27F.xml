</taxonomicName>
<taxonomicNameLabel box="[309,361,1985,2003]" pageId="28" pageNumber="425" rank="species">n. sp.</taxonomicNameLabel>
, mâle holotype et femelle allotype; 
<emphasis bold="true" box="[663,675,1986,2004]" pageId="28" pageNumber="425">E</emphasis>
, 
<taxonomicName authority="Roy, 2020" authorityName="Roy" authorityYear="2020" box="[687,756,1985,2003]" class="Insecta" family="Mantidae" genus="Danuriella" kingdom="Animalia" order="Mantodea" pageId="28" pageNumber="425" phylum="Arthropoda" rank="species" species="sogai" status="n. sp.">
<emphasis box="[687,756,1985,2003]" italics="true" pageId="28" pageNumber="425">D. sogai</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[763,814,1985,2003]" pageId="28" pageNumber="425" rank="species">n. sp.</taxonomicNameLabel>
, mâle holotype; 
<emphasis bold="true" box="[955,966,1986,2004]" pageId="28" pageNumber="425">F</emphasis>
, 
<taxonomicName authority="Roy, 2020" authorityName="Roy" authorityYear="2020" box="[978,1054,1985,2003]" class="Insecta" family="Mantidae" genus="Danuriella" kingdom="Animalia" order="Mantodea" pageId="28" pageNumber="425" phylum="Arthropoda" rank="species" species="tigrina" status="n. sp.">
<emphasis box="[978,1054,1985,2003]" italics="true" pageId="28" pageNumber="425">D. tigrina</emphasis>
</taxonomicName>
