<paragraph blockId="2.[132,1266,1826,1871]" pageId="2">
<emphasis bold="true" box="[132,233,1826,1845]" pageId="2">FIGURE 2:</emphasis>
<taxonomicName authorityName="Rosa" authorityYear="2010" box="[238,392,1826,1845]" class="Insecta" family="Elateridae" genus="Ignelater" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" phylum="Arthropoda" rank="species" species="inaguensis" status="sp. nov.">
<emphasis box="[238,392,1826,1845]" italics="true" pageId="2">Ignelater inaguensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[397,460,1826,1845]" pageId="2" rank="species">sp. nov.</taxonomicNameLabel>
<emphasis bold="true" box="[463,482,1826,1845]" pageId="2">A,</emphasis>
antenna; 
<emphasis bold="true" box="[563,608,1826,1845]" pageId="2">B, C,</emphasis>
aedeagus (ventral, dorsal); 
<emphasis bold="true" box="[829,852,1826,1845]" pageId="2">D,</emphasis>
ventral sclerite of penis (lateroventral); 
<emphasis bold="true" box="[1175,1213,1826,1845]" pageId="2">E, F,</emphasis>
dorsal sclerite of penis (dorsal, ventral); 
<emphasis bold="true" box="[405,426,1851,1871]" pageId="2">G,</emphasis>
sternite IX; 
<emphasis bold="true" box="[529,552,1852,1871]" pageId="2">H,</emphasis>
sternite VIII; 
