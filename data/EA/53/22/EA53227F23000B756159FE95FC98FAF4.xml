, 
<collectingCountry box="[1079,1183,1324,1348]" name="Mauritius" pageId="9" pageNumber="232">Mauritius</collectingCountry>
and 
<collectingCountry box="[1234,1360,1324,1348]" name="Madagascar" pageId="9" pageNumber="232">Madagascar</collectingCountry>
