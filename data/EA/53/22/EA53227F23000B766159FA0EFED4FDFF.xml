<collectingCountry box="[360,442,535,559]" name="Zambia" pageId="10" pageNumber="233">Zambia</collectingCountry>
and in 
<collectingCountry box="[520,632,535,559]" name="Cameroon" pageId="10" pageNumber="233">Cameroun</collectingCountry>
and 
<collectingCountry name="Sao Tome and Principe" pageId="10" pageNumber="233">São Tomé and Príncipe</collectingCountry>
, the 
<collectingCountry box="[325,495,567,591]" name="Comoros" pageId="10" pageNumber="233">Comoro islands</collectingCountry>
, 
<collectingCountry box="[507,600,567,591]" name="Réunion" pageId="10" pageNumber="233">Réunion</collectingCountry>
, and 
<collectingCountry box="[658,786,567,591]" name="Madagascar" pageId="10" pageNumber="233">Madagascar</collectingCountry>
(
<bibRefCitation author="BADRE, F." box="[142,301,594,623]" editor="BOSSER, J." firstAuthor="BADRE" journalOrPublisher="Pteridophytes" pageId="10" pageNumber="233" pagination="19 - 33" part="3" refId="ref5526" refString="BADRE, F. (2002 b). Lycopodiacees. In: BOSSER, J. et al. (ed.), Fl. Mascareignes, Pteridophytes 3: 19 - 33." title="Lycopodiacees" type="book chapter" volumeTitle="Fl. Mascareignes" year="2002" yearSuffix="b">BADRÉ, 2002b</bibRefCitation>
).
</paragraph>
