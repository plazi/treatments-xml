<taxonomicName LSID="urn:lsid:zoobank.org:act:D484B71E-5CE6-48DF-AB4E-01F5D673495E" authority="Sharkey" authorityName="Foerster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Lytopylus bradzlotnicki" order="Hymenoptera" pageId="9" pageNumber="388" phylum="Arthropoda" rank="species" species="bradzlotnicki">Lytopylus bradzlotnicki Sharkey</taxonomicName>
