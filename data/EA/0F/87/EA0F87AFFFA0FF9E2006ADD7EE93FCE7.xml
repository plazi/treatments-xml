<location LSID="urn:lsid:plazi:treatment:EA0F87AFFFA0FF9E2006ADD7EE93FCE7:67796062FFA0FF99257CACA2EB9BFC29" box="[1137,1193,991,1012]" country="Colombia" county="We" municipality="The" name="Three" pageId="3" stateProvince="Boyaca">Three</location>
