<collectingCountry box="[784,895,1627,1652]" name="Argentina" pageId="10" pageNumber="359">Argentina</collectingCountry>
or 
<collectingCountry box="[936,996,1627,1652]" name="Chile" pageId="10" pageNumber="359">Chile</collectingCountry>
