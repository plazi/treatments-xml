<quantity id="A9009B34FFF1FF83FD86FEA9F4DB535D" box="[603,677,326,350]" metricMagnitude="4" metricUnit="m" metricValue="1.1" pageId="1" pageNumber="34" unit="km" value="11.0">11 km</quantity>
