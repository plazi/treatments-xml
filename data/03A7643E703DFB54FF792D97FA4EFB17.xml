<number id="3DC6C70F703DFB54FDDD2B0EFDE8FBEE" box="[563,570,1092,1104]" pageId="1" pageNumber="2" value="5.0">5</number>
and fl 
<number id="3DC6C70F703DFB54FD992B0EFDACFBEE" box="[631,638,1092,1104]" pageId="1" pageNumber="2" value="6.0">6</number>
(Fig. 
<number id="3DC6C70F703DFB54FD562B7CFD03FBF2" box="[696,721,1078,1100]" pageId="1" pageNumber="2" value="11.0">11</number>
); clava at least 
<number id="3DC6C70F703DFB54FC882B7CFC58FBF2" box="[870,906,1078,1100]" pageId="1" pageNumber="2" value="244.0">244</number>
