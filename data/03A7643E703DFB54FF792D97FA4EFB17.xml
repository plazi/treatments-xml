<number id="3DC6C70F703DFB54FF792CF6FF77FC6C" box="[151,165,956,978]" pageId="1" pageNumber="2" value="2.0">2</number>
(
<number id="3DC6C70F703DFB54FF432CF6FF64FC6C" box="[173,182,956,978]" pageId="1" pageNumber="2" value="1.0">1</number>
) Flagellum with fl 
