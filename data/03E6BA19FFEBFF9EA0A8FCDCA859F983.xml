<taxonomicName id="4C4F708CFFEBFF9EA39CFBE4AD3DFBD2" authorityName="Grote" authorityYear="1873" box="[931,1099,1086,1110]" class="Insecta" family="Noctuidae" genus="Pleonectopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="268" phylum="Arthropoda" rank="genus">
