<quantity id="CA02C9DE2D4AFFFFFE53FC20AC76FB90" box="[482,569,1003,1030]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="17" pageNumber="64" unit="mm" value="2.7">2.7 mm</quantity>
