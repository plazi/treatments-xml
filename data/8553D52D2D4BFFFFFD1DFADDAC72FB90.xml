<quantity id="CA02C9DE2D4AFFFFFFDEFC20AE8BFB90" box="[111,196,1003,1030]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="17" pageNumber="64" unit="mm" value="4.8">4.8 mm</quantity>
