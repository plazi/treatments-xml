<taxonomicName id="4C2117BAFFB9237F7F7A7538B6EDBCB0" authority="Herzenstein, 1890" authorityName="Herzenstein" authorityYear="1890" class="Actinopterygii" family="Pleuronectidae" genus="Pleuronectes" kingdom="Animalia" order="Pleuronectiformes" pageId="15" pageNumber="268" phylum="Chordata" rank="species" species="japonicus">
