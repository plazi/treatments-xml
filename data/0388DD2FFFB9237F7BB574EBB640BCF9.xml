<taxonomicName id="4C2117BAFFB9237F7996751CB310BCB0" authority="Houttuyn, 1782" authorityName="Houttuyn" authorityYear="1782" box="[692,1140,1901,1927]" class="Actinopterygii" family="Pleuronectidae" genus="Pleuronectes" kingdom="Animalia" order="Pleuronectiformes" pageId="15" pageNumber="268" phylum="Chordata" rank="species" species="japonicus">
