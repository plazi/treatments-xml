<quantity id="4CD9C1DCFFB9237F7F1E751CB310BCB0" box="[1084,1140,1901,1927]" metricMagnitude="1" metricUnit="m" metricValue="4.52628" pageId="15" pageNumber="268" unit="in" value="1782.0">1782</quantity>
