<taxonomicName id="4C6C03C3707EFFCFFF6FFB4A0472FB27" authority="Barrientos" authorityName="Barrientos" authorityYear="2020" box="[212,520,1255,1273]" class="Arachnida" family="Linyphiidae" genus="Lepthyphantes" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="11" phylum="Arthropoda" rank="species" species="imazigheni" status="n. sp.">
