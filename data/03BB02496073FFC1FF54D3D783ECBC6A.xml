<collectingCountry id="F305F3CF6073FFC0FA81D01B8493BE5C" box="[1346,1436,785,810]" name="Jamaica" pageId="22" pageNumber="173">Jamaica</collectingCountry>
<quantity id="4CEA1EBA6073FFC0FF54D03F81DEBE3B" box="[151,209,821,846]" metricMagnitude="1" metricUnit="m" metricValue="7.6" pageId="22" pageNumber="173" unit="m" value="76.0">76 m</quantity>
