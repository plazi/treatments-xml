<quantity id="4CEA1EBA6073FFC0FAC0D03C8456BE3B" box="[1283,1369,822,846]" metricMagnitude="1" metricUnit="m" metricValue="1.0" metricValueMax="1.2" metricValueMin="0.8" pageId="22" pageNumber="173" unit="m" value="10.0" valueMax="12.0" valueMin="8.0">8–12 m</quantity>
