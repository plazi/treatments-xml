<taxonomicName id="1DFB83C3FF94FFD5AAF83ECAFE50FDEC" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="30" phylum="Chordata" rank="species" species="semilarvatus">
<emphasis id="E88F2452FF94FFD5AAF83ECAFE50FDEC" italics="true" pageId="23" pageNumber="30">Parus semilarvatus</emphasis>
