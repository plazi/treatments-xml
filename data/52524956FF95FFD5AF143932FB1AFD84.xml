<quantity id="1D0355A5FF94FFD5AFFF3ECAFDB6FE37" box="[483,566,484,510]" metricMagnitude="-2" metricUnit="m" metricValue="8.4" pageId="23" pageNumber="30" unit="mm" value="84.0">84 mm</quantity>
