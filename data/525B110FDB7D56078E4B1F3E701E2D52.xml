<taxonomicName id="EFE0E821621113F9A3C6C00DABAA17B5" authority="Walsingham &amp; Durrant, 1909" authorityName="Walsingham &amp; Durrant" authorityYear="1909" class="Insecta" family="Heliozelidae" genus="Holocacista" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Holocacista" order="Lepidoptera" pageId="0" pageNumber="105" phylum="Arthropoda" rank="genus">
