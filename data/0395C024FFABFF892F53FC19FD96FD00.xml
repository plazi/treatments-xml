, 
<taxonomicName id="4C3C0AB1FFAAFF8A2DC9F9DCFBBAF985" box="[914,1107,1538,1563]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="5" pageNumber="56" phylum="Tracheophyta" rank="species" species="densiflora">
