<taxonomicName id="4C3C0AB1FFAAFF8A2C61FB12FCE8FB7D" box="[570,769,1228,1251]" class="Insecta" family="Geometridae" genus="Idaea" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="56" phylum="Arthropoda" rank="species" species="khankaensis" status="sp. n.">
<emphasis id="B948AD20FFAAFF8A2C61FB12FCE8FB7D" box="[570,769,1228,1251]" italics="true" pageId="5" pageNumber="56">Idaea khankaensis</emphasis>
