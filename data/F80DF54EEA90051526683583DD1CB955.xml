<subSubSection id="8A2337C179A68225216117F20A65167F" pageId="26" pageNumber="27" type="reference_group">
<paragraph id="3AEEC8F8EB01261171B131798DA73B38" pageId="26" pageNumber="27">
<taxonomicName id="01A367E68A05D5C2AB293C8D2BF48E73" class="Demospongiae" family="Irciniidae" genus="Sarcotragus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sarcotragus foetidus" order="Dictyoceratida" pageId="26" pageNumber="27" phylum="Porifera" rank="species" species="foetidus">Sarcotragus foetidus</taxonomicName>
