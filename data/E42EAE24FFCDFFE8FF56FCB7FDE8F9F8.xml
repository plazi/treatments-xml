<taxonomicName id="AB8764B1FFCCFFE8FBDFFA43FB4EFA20" authorityName="Saussure" authorityYear="1878" box="[1147,1245,1422,1439]" class="Insecta" family="Gryllidae" genus="Podoscirtus" kingdom="Animalia" order="Orthoptera" pageId="13" pageNumber="286" phylum="Arthropoda" rank="species" species="rufidulus">
