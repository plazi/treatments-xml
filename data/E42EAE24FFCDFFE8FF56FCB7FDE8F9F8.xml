<emphasis id="5EF3C320FFCCFFE8FCDBFA43FC28FA20" box="[895,955,1422,1439]" italics="true" pageId="13" pageNumber="286">elegans</emphasis>
</taxonomicName>
; B, 
<taxonomicName id="AB8764B1FFCCFFE8FC79FA43FBC4FA20" baseAuthorityName="Desutter-Grandcolas" baseAuthorityYear="1997" box="[989,1111,1422,1439]" class="Insecta" family="Gryllidae" genus="Matuanus" kingdom="Animalia" order="Orthoptera" pageId="13" pageNumber="286" phylum="Arthropoda" rank="species" species="caledonicus">
<emphasis id="5EF3C320FFCCFFE8FC79FA43FBC4FA20" box="[989,1111,1422,1439]" italics="true" pageId="13" pageNumber="286">M. caledonicus</emphasis>
</taxonomicName>
