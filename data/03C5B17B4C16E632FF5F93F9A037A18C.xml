<subSubSection id="C37653E64C15E632FF0F93D2A78CA499" box="[199,681,188,213]" pageId="6" pageNumber="319" type="etymology">
<paragraph id="8BD3006D4C15E632FF0F93D2A78CA499" blockId="6.[151,1437,151,1149]" box="[199,681,188,213]" pageId="6" pageNumber="319">
<emphasis id="B918DC7F4C15E632FF0F93D2A46BA499" bold="true" box="[199,334,188,213]" pageId="6" pageNumber="319">Etymology.</emphasis>
