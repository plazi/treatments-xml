<location id="8EA36035FFE04332FAD3FBCFFA23FE31" LSID="urn:lsid:plazi:treatment:03D587F8FFE64332FC94FD63FA94F975:8EA36035FFE04332FAD3FBCFFA23FE31" box="[1340,1424,425,448]" country="China" county="Material" municipality="Yabian" name="Tumen" pageId="4" pageNumber="400" stateProvince="Jilin">Tumen</location>
