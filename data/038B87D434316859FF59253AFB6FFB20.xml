), 
<quantity id="4CDA9B2734316859FADB26B7FA73FD28" box="[1301,1431,705,729]" metricMagnitude="-1" metricUnit="m" metricValue="8.0" metricValueMax="12.0" metricValueMin="4.0" pageId="5" pageNumber="44" unit="m" value="0.8" valueMax="1.2" valueMin="0.4">0.4—1.2 m</quantity>
, coral rubble, 
<taxonomicName id="4C224D4134316859FEE52690FE6FFD0C" box="[299,395,742,765]" class="Phaeophyceae" family="Dictyotaceae" genus="Dictyota" higherTaxonomySource="CoL" kingdom="Animalia" order="Dictyotales" pageId="5" pageNumber="44" phylum="Ochrophyta" rank="genus">
<emphasis id="B956EAD034316859FEE52690FE6FFD0C" box="[299,395,742,765]" italics="true" pageId="5" pageNumber="44">Dictyota</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C224D4134316859FE572693FDE2FD0C" box="[409,518,741,765]" class="Liliopsida" family="Hydrocharitaceae" genus="Thalassia" kingdom="Animalia" order="Alismatales" pageId="5" pageNumber="44" phylum="Tracheophyta" rank="genus">
<emphasis id="B956EAD034316859FE572693FDE2FD0C" box="[409,518,741,765]" italics="true" pageId="5" pageNumber="44">Thalassia</emphasis>
</taxonomicName>
in part live 
<taxonomicName id="4C224D4134316859FD432690FD3AFD0C" box="[653,734,742,765]" class="Anthozoa" family="Poritidae" genus="Porites" kingdom="Animalia" order="Scleractinia" pageId="5" pageNumber="44" phylum="Cnidaria" rank="genus">
<emphasis id="B956EAD034316859FD432690FD3AFD0C" box="[653,734,742,765]" italics="true" pageId="5" pageNumber="44">Porites</emphasis>
</taxonomicName>
, sand, reef crest rubble, encrusting red algae of 
<taxonomicName id="4C224D4134316859FB392693FA78FD0C" box="[1271,1436,741,765]" class="Florideophyceae" family="Corallinaceae" genus="Lithothamnion" higherTaxonomySource="GBIF" kingdom="Plantae" order="Corallinales" pageId="5" pageNumber="44" phylum="Rhodophyta" rank="genus">
<emphasis id="B956EAD034316859FB392693FA78FD0C" box="[1271,1436,741,765]" italics="true" pageId="5" pageNumber="44">Lithothamnion</emphasis>
</taxonomicName>
<typeStatus id="5499886034316859FF59277CFF2EFCD3" box="[151,202,778,802]" pageId="5" pageNumber="44">type</typeStatus>
