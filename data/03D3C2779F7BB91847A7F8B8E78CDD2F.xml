<emphasis id="B90EAF739F7BB91F464EF8D4E1D8DE8E" box="[382,703,1916,1940]" italics="true" pageId="2" pageNumber="45">
<taxonomicName id="4C7A08E29F7BB91F464EF8D4E173DE8E" authorityName="Garcia Aldrete" authorityYear="1998" box="[382,532,1916,1940]" class="Insecta" family="Ptiloneuridae" genus="Euplocania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="45" phylum="Arthropoda" rank="species" species="pictaoides">E. pictaoides</taxonomicName>
, 
<taxonomicName id="4C7A08E29F7BB91F4510F8D4E1D8DE8E" authority="Vinasco-Mondragón &amp; González-Obando &amp; García-Aldrete, 2018" authorityName="Vinasco-Mondragón &amp; González-Obando &amp; García-Aldrete" authorityYear="2018" box="[544,703,1917,1940]" class="Insecta" family="Ptiloneuridae" genus="Euplocania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="45" phylum="Arthropoda" rank="species" species="vaupesiana" status="n. sp.">E. vaupesiana</taxonomicName>
</emphasis>
<taxonomicNameLabel id="A23D12089F7BB91F45F7F8D5E061DE8F" box="[711,774,1916,1941]" pageId="2" pageNumber="45" rank="species">
<emphasis id="B90EAF739F7BB91F45F7F8D5E061DE8F" bold="true" box="[711,774,1916,1941]" pageId="2" pageNumber="45">n. sp.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="4C7A08E29F7BB91F4425F8D4E0DBDE8E" authority="Vinasco-Mondragón &amp; González-Obando &amp; García-Aldrete, 2018" authorityName="Vinasco-Mondragón &amp; González-Obando &amp; García-Aldrete" authorityYear="2018" box="[789,956,1917,1940]" class="Insecta" family="Ptiloneuridae" genus="Euplocania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="45" phylum="Arthropoda" rank="species" species="ecuatoriana" status="n. sp.">
