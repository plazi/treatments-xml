<taxonomicName id="44A9BF87968E6929ACE573A8C92DBEE7" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax multipunctatus" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="multipunctatus">Mecyclothorax multipunctatus</taxonomicName>
