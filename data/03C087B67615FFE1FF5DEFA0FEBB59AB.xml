<number id="3DA124877614FFE4FB0FEE32FB4B5869" box="[1221,1236,1592,1616]" pageId="1" pageNumber="596" value="2.0">2</number>
-CU 
<number id="3DA124877614FFE4FAC2EE32FA8E5869" box="[1288,1297,1592,1616]" pageId="1" pageNumber="596" value="1.0">1</number>
(incorrectly depicted in original description of 
<taxonomicName id="4C694D237614FFE4FDD3EE57FD06584D" box="[537,665,1628,1652]" class="Insecta" family="Braconidae" genus="Phanerotoma" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="596" phylum="Arthropoda" rank="species" species="agarwali">
<emphasis id="B91DEAB27614FFE4FDD3EE57FD06584D" box="[537,665,1628,1652]" italics="true" pageId="1" pageNumber="596">P. agarwali</emphasis>
</taxonomicName>
; 
<number id="3DA124877614FFE4FD62EE56FD54584D" box="[680,715,1628,1652]" pageId="1" pageNumber="596" value="0.7">0.7</number>
times in 
<taxonomicName id="4C694D237614FFE4FCF9EE57FC71584D" box="[819,1006,1628,1652]" class="Insecta" family="Braconidae" genus="Phanerotoma" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="596" phylum="Arthropoda" rank="species" species="andamanensis">
<emphasis id="B91DEAB27614FFE4FCF9EE57FC71584D" box="[819,1006,1628,1652]" italics="true" pageId="1" pageNumber="596">P. andamanensis</emphasis>
</taxonomicName>
