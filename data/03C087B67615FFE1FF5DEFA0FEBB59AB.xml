<number id="3DA124877614FFE4FCCEEE32FC915869" box="[772,782,1592,1616]" pageId="1" pageNumber="596" value="1.0">1</number>
-CU 
<number id="3DA124877614FFE4FC8CEE32FCCF5869" box="[838,848,1592,1616]" pageId="1" pageNumber="596" value="1.0">1</number>
about 
<number id="3DA124877614FFE4FC6AEE3DFC5C5869" box="[928,963,1591,1616]" pageId="1" pageNumber="596" value="0.5">0.5</number>
