<taxonomicName id="77C50C83DCE1A5A2FC2C899EFBE1FE32" authorityName="Haeckel" authorityYear="1872" box="[988,1071,404,427]" class="Calcarea" family="Clathrinidae" genus="Ascetta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clathrinida" pageId="147" pageNumber="148" phylum="Porifera" rank="genus">
<emphasis id="82B1AB12DCE1A5A2FC2C899EFBE1FE32" box="[988,1071,404,427]" italics="true" pageId="147" pageNumber="148">Ascetta</emphasis>
</taxonomicName>
