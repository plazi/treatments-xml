<quantity id="4CCA8D91FF84FFF9FB81D6C91A0064F5" box="[1054,1149,1246,1272]" metricMagnitude="2" metricUnit="m" metricValue="1.22" pageId="9" pageNumber="38" unit="m" value="122.0">122 m</quantity>
) with 
<number id="3DFA3253FF84FFF9FB50D6C91AA364F5" box="[1231,1246,1246,1272]" pageId="9" pageNumber="38" value="5.0">5</number>
