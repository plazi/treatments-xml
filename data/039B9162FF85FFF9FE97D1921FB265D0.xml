<quantity id="4CCA8D91FF84FFF9FD13D6C91CA164F5" box="[652,732,1246,1272]" metricMagnitude="1" metricUnit="m" metricValue="8.1" pageId="9" pageNumber="38" unit="m" value="81.0">81 m</quantity>
