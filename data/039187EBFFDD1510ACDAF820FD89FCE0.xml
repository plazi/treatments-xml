<quantity id="4CC09B18FFDE1510AFC9FE94FC69FEEF" box="[900,1011,306,332]" metricMagnitude="-3" metricUnit="m" metricValue="3.93" pageId="42" pageNumber="43" unit="mm" value="3.93">3.93 mm</quantity>
