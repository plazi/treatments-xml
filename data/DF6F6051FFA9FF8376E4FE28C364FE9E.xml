<paragraph id="5779D147FFA9FF8176E0FE63C7BBFD9A" blockId="73.[144,758,418,541]" pageId="73" pageNumber="773">
<emphasis id="65B20D55FFA9FF8176E0FE63C65DFE79" box="[144,296,489,510]" italics="true" pageId="73" pageNumber="773">Type species:</emphasis>
<taxonomicName id="90C6AAC4FFA9FF817749FE62C7BFFD9A" authority="Marks" authorityName="Marks" authorityYear="1963" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="73" pageNumber="773" phylum="Arthropoda" rank="species" species="spilotus" subGenus="Ochlerotatus">
<taxonomicName id="90C6AAC4FFA9FF817749FE62C5E0FE7A" box="[313,661,488,510]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="73" pageNumber="773" phylum="Arthropoda" rank="species" species="spilotus" subGenus="Ochlerotatus">
<emphasis id="65B20D55FFA9FF817749FE62C60FFE7A" box="[313,378,488,509]" italics="true" pageId="73" pageNumber="773">Aedes</emphasis>
(
<emphasis id="65B20D55FFA9FF8177E3FE62C551FE7A" box="[403,548,488,509]" italics="true" pageId="73" pageNumber="773">Ochlerotatus</emphasis>
) 
<emphasis id="65B20D55FFA9FF81744DFE62C5E0FE7A" box="[573,661,488,509]" italics="true" pageId="73" pageNumber="773">spilotus</emphasis>
</taxonomicName>
<bibRefCitation id="3357ACB6FFA9FF8174D6FE62C7BFFD9A" author="Marks EN." journalOrPublisher="Journal of the Entomological Society of Queensland" pageId="73" pageNumber="773" pagination="31 - 47" part="2" refString="Marks EN. 1963. The subgenus Ochlerotatus in the Australian Region (Diptera: Culicidae) V. The Stricklandi Section. Journal of the Entomological Society of Queensland 2: 31 - 47." title="The subgenus Ochlerotatus in the Australian Region (Diptera: Culicidae) V. The Stricklandi Section" type="journal article" year="1963">Marks, 1963</bibRefCitation>
</taxonomicName>
