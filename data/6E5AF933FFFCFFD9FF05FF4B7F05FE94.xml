<taxonomicName id="21F333A6FFFDFFDEFB3DF947797FF941" authority="Ruiz, 2017" authorityName="Ruiz" authorityYear="2017" box="[1199,1318,1691,1712]" class="Arachnida" family="Salticidae" genus="Mago" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="587" phylum="Arthropoda" rank="species" species="mimicus" status="sp. nov.">
<emphasis id="D4879437FFFDFFDEFB3DF947797FF941" box="[1199,1318,1691,1712]" italics="true" pageId="3" pageNumber="587">M. mimicus</emphasis>
</taxonomicName>
<emphasis id="D4879437FFFDFFDEFABCF94679D9F941" bold="true" box="[1326,1408,1690,1712]" pageId="3" pageNumber="587">
<taxonomicNameLabel id="CFB4294CFFFDFFDEFABCF94679D9F941" box="[1326,1408,1690,1712]" pageId="3" pageNumber="587" rank="species">sp. nov.</taxonomicNameLabel>
