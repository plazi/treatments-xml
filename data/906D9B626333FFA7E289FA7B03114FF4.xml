<collectingCountry id="60D36AE46333FFA7E65DF89A05AB4F09" box="[1091,1155,1808,1831]" name="Japan" pageId="1" pageNumber="36">Japan</collectingCountry>
and 
<collectingCountry id="60D36AE46333FFA7E6A0F88504074F09" box="[1214,1327,1807,1831]" name="Indonesia" pageId="1" pageNumber="36">Indonesia</collectingCountry>
