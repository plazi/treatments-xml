<taxonomicName id="61EC1C01B04F0EA0634DE37997DBE095" LSID="B3AFA186-614C-5B3D-8BF6-CBA95E939638" authority="Li &amp; He &amp; Chen, 2020" authorityName="Li &amp; He &amp; Chen" authorityYear="2020" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon (Lucobracon) coarctatus" order="Hymenoptera" pageId="0" pageNumber="209" phylum="Arthropoda" rank="species" species="coarctatus" status="sp. nov." subGenus="Lucobracon">Bracon (Lucobracon) coarctatus</taxonomicName>
