<taxonomicName id="08B74FEA1550E429FE0A6086FD09F590" authority="E. Sim." authorityName="E. Sim." box="[414,753,434,459]" class="Arachnida" family="Galeodidae" genus="Galeodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="blanchardi">
