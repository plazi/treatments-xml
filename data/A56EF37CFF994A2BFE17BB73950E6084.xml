<taxonomicName id="EAC739E9FF994A2BFDEFBC7C92E3623F" box="[531,698,1056,1080]" class="Arachnida" family="Phytoseiidae" genus="Neoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="3" pageNumber="96" phylum="Arthropoda" rank="species" species="teke">
