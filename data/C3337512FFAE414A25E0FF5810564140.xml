<taxonomicName id="8C9ABF87FFAD414B2455F851133E47D1" authorityName="Potapova" authorityYear="2014" box="[317,587,2023,2044]" class="Bacillariophyceae" family="Cymbellaceae" genus="Encyonema" higherTaxonomySource="GBIF" kingdom="Chromista" order="Cymbellales" pageId="2" pageNumber="117" phylum="Ochrophyta" rank="species" species="appalachianum">Encyonema appalachianum</taxonomicName>
