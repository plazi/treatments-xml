<location id="8ED76073CD0CFFF8FB6CFC9A7FBBFCE3" LSID="urn:lsid:plazi:treatment:03A187BECD0CFFF8FF35FE4D7A98FC53:8ED76073CD0CFFF8FB6CFC9A7FBBFCE3" box="[1230,1274,770,795]" name="The" pageId="8" pageNumber="253">The</location>
other, cat. no
</materialsCitation>
. 
<materialsCitation id="3B603CF5CD0CFFF8FF35FCBF7A00FC7F" ID-GBIF-Occurrence="1915274036" collectionCode="ZMB" collectorName="System der Polydesmiden" country="Ecuador" location="Richard Haensch" municipality="Attems" pageId="8" pageNumber="253" specimenCode="ZMB 5173" specimenCount="1" typeStatus="holotype">
<specimenCode id="DBAE9ED3CD0CFFF8FF35FCBF7A58FCC7" box="[151,281,806,831]" collectionCode="ZMB" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum f�r Naturkunde Berlin (Zoological Collections)" pageId="8" pageNumber="253">ZMB 5173</specimenCode>
