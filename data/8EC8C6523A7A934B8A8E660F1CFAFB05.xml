<pageBreakToken id="09EA7358D8DFDA3658066D00CDF19505" pageId="null" pageNumber="590" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="18070FA565F50B1BF92121C2AD3D882E" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="auricula">
