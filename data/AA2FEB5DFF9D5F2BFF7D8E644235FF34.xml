del Estero
</collectingRegion>
, 
<collectingRegion id="E04294A9FF9D5F2AFD608F2141EFF849" box="[650,722,1900,1925]" country="Argentina" name="Chaco" pageId="0" pageNumber="195">Chaco</collectingRegion>
and 
<collectingRegion id="E04294A9FF9D5F2AFCE28F204053F849" box="[776,878,1901,1925]" country="Argentina" name="Santa Fe" pageId="0" pageNumber="195">Santa Fe</collectingRegion>
