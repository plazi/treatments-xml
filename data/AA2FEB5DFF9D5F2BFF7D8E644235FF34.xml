<subSubSection id="6A9C09C0FF9D5F2BFF2D8F054235FF34" lastPageId="1" lastPageNumber="196" pageId="0" pageNumber="195" type="distribution">
<paragraph id="22395A4BFF9D5F2AFF2D8F054047F849" blockId="0.[151,1437,1684,2033]" pageId="0" pageNumber="195">
<emphasis id="10F28659FF9D5F2AFF2D8F05425DF8AD" bold="true" box="[199,352,1864,1889]" pageId="0" pageNumber="195">Distribution.</emphasis>
From eastern 
<collectingCountry id="5A911ADBFF9D5F2AFDE48F05415DF8AD" box="[526,608,1864,1889]" name="Bolivia" pageId="0" pageNumber="195">Bolivia</collectingCountry>
and southern 
<collectingCountry id="5A911ADBFF9D5F2AFCE18F054072F8AD" box="[779,847,1864,1889]" name="Brazil" pageId="0" pageNumber="195">Brazil</collectingCountry>
(records from 
<collectingRegion id="E04294A9FF9D5F2AFBE98F0447A5F8AD" box="[1027,1176,1865,1889]" country="Brazil" name="Mato Grosso" pageId="0" pageNumber="195">Mato Grosso</collectingRegion>
and 
<collectingRegion id="E04294A9FF9D5F2AFB3D8F054649F8AD" box="[1239,1396,1864,1889]" country="Brazil" name="Minas Gerais" pageId="0" pageNumber="195">Minas Gerais</collectingRegion>
) to northeast of 
<collectingCountry id="5A911ADBFF9D5F2AFEC98F2142AFF849" box="[291,402,1900,1925]" name="Argentina" pageId="0" pageNumber="195">Argentina</collectingCountry>
(
<collectingRegion id="E04294A9FF9D5F2AFE4B8F214142F848" box="[417,639,1900,1925]" country="Argentina" name="Santiago del Estero" pageId="0" pageNumber="195">
