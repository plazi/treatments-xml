<emphasis id="EC02844C4C06166984D30DE05333EE4E" italics="true" pageId="44" pageNumber="97">Chrysocoma ciliate</emphasis>
</taxonomicName>
L.); 
<taxonomicName id="F518C119DD01226C4C342F084A2C8189" genus="Boraginaceae" lsidName="" pageId="44" pageNumber="97" rank="genus">Boraginaceae</taxonomicName>
(
<taxonomicName id="4BEFCDA63576585D60FCA8BF5E8A9BA8" authorityName="Thunb" authorityYear="1794" class="Dicotyledoneae" family="Boraginaceae" genus="Anchusa" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Tubiflorae" pageId="44" pageNumber="97" phylum="Angiospermae" rank="species" species="capensis">
<emphasis id="9AFA71DDE069741615657579C9910E79" italics="true" pageId="44" pageNumber="97">Anchusa capensis</emphasis>
