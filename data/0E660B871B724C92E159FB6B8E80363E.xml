Mill.); 
<taxonomicName id="E6DEFA7B3CC543B1E650A2AE624CC23E" genus="Asteraceae" lsidName="" pageId="44" pageNumber="97" rank="genus">Asteraceae</taxonomicName>
(
<taxonomicName id="54F23DC7BFC0866FF2756AE276329A90" class="Magnoliopsida" family="Asteraceae" genus="Berkheya" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="44" pageNumber="97" phylum="Tracheophyta" rank="species" species="heterophylla">
<emphasis id="0D61639EF66440F57CD26DC73D675B89" italics="true" pageId="44" pageNumber="97">Berkheya heterophylla</emphasis>
