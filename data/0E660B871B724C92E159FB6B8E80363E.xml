<taxonomicName id="28A6E9A620265923D92EC7AACEC753CA" authorityName="Latreille" authorityYear="1802" class="Hexapoda" family="Tabanidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Tabanidae</taxonomicName>
, 
<taxonomicName id="3681E2638BF249B90944F2E82E8380C6" authorityName="Macquart" authorityYear="1834" class="Hexapoda" family="Nemestrinidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Nemestrinidae</taxonomicName>
, 
<taxonomicName id="E751F7342BA06AA5020E5B8502344EF8" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Bombyliidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Bombyliidae</taxonomicName>
, 
<taxonomicName id="FEE9FFD33354F88844C004B4BA854925" class="Hexapoda" family="Calliphoridae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Calliphoridae</taxonomicName>
, 
<taxonomicName id="E7CED540A9722C5BB857411BAE25EB08" class="Hexapoda" family="Sarcophagidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Sarcophagidae</taxonomicName>
, and 
<taxonomicName id="0DA00C18C942C4AA786F54CC7F8145CF" class="Insecta" family="Tachinidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Tachinidae</taxonomicName>
.
</paragraph>
