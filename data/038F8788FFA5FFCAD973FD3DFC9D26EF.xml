<taxonomicName id="4C264D1DFFA5FFCDD973FD3DFB7D21EE" authority="(Cooper 1861: 120)" baseAuthorityName="Cooper" baseAuthorityPageNumber="120" baseAuthorityYear="1861" box="[810,1256,1642,1669]" class="Reptilia" family="Testudinidae" genus="Gopherus" kingdom="Animalia" order="Testudines" pageId="51" pageNumber="52" phylum="Chordata" rank="species" species="agassizii">
