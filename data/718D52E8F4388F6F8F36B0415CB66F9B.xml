<taxonomicName id="63E4219D191AE6EDCF25566D95CDE783" class="Cephalopoda" family="Dactylioceratidae" genus="Collina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Collina glabicira" order="Ammonoidea" pageId="16" pageNumber="17" phylum="Mollusca" rank="species" species="glabicira">Collina glabicira</taxonomicName>
