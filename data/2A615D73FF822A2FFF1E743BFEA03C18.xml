<taxonomicName id="65C897E6FF822A2FFED17134FE5B3C74" box="[344,488,1806,1831]" class="Insecta" family="Gelechiidae" genus="Istrianis" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="435" phylum="Arthropoda" rank="species" species="falkovitshi">
