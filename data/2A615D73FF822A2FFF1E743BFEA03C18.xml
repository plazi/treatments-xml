<collectionCode id="C4D974A0FF822A2FFEB87450FED639D0" box="[305,357,619,643]" httpUri="http://grbio.org/cool/z2y8-2jfr" name="Russian Academy of Sciences, Zoological Institute, Zoological Museum" pageId="34" pageNumber="435">ZIN</collectionCode>
