<heading id="D0A0967DFFB38303FE21FA1A7FA4D4D4" bold="true" box="[498,700,1451,1479]" fontSize="11" level="4" pageId="9" pageNumber="10" reason="6">
<taxonomicName id="4C575A92FFB38303FE21FA1A7FA4D4D4" box="[498,700,1451,1479]" class="Reptilia" kingdom="Animalia" order="Ornithischia" pageId="9" pageNumber="10" phylum="Chordata" rank="order">
<emphasis id="B923FD03FFB38303FE21FA1A7FA4D4D4" bold="true" box="[498,700,1451,1479]" italics="true" pageId="9" pageNumber="10">Ornithischians</emphasis>
