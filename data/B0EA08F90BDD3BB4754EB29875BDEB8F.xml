<paragraph id="B093D253D2073DBEAF5296D193955537" pageId="4" pageNumber="19">
<taxonomicName id="AB5F2206E9F033BE3998D92F10A75EDE" authority="Waterhouse, 1879" authorityName="Waterhouse" authorityYear="1879" class="Insecta" family="Lycidae" genus="Eniclases" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eniclases pseudoapertus" order="Coleoptera" pageId="4" pageNumber="19" phylum="Arthropoda" rank="species" species="pseudoapertus">Eniclases pseudoapertus</taxonomicName>
