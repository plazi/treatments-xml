<taxonomicName id="98A04D0CFF91FFA86CF1FA7CFDC4D7FD" authority="Loof, 1964" authorityName="Loof" authorityYear="1964" box="[213,524,1411,1435]" class="Adenophorea" family="Dorylaimidae" genus="Dorylaimellus" kingdom="Animalia" order="Dorylaimida" pageId="32" pageNumber="2818" phylum="Nematoda" rank="species" species="spicatus" subGenus="Clavidorylaimellus">
<emphasis id="6DD4EA9DFF91FFA86CF1FA7CFF38D7FD" box="[213,240,1412,1435]" italics="true" pageId="32" pageNumber="2818">D.</emphasis>
(
<emphasis id="6DD4EA9DFF91FFA86D26FA7BFED4D7FD" box="[258,284,1411,1435]" italics="true" pageId="32" pageNumber="2818">C.</emphasis>
) 
<emphasis id="6DD4EA9DFF91FFA86D0AFA7CFE4ED7FD" box="[302,390,1412,1435]" italics="true" pageId="32" pageNumber="2818">spicatus</emphasis>
<bibRefCitation id="3B314B7EFF91FFA86DABFA7BFDC4D7FD" author="Loof PAA" box="[399,524,1411,1435]" pageId="32" pageNumber="2818" pagination="201 - 300" refId="ref13863" refString="Loof PAA. 1964. Free living and plant parasitic nematodes from Venezuela. Nematologica 10: 201 - 300." type="journal article" year="1964">Loof, 1964</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="98A04D0CFF91FFA86E6EFA7CFC63D7FD" authority="Cobb, 1913" authorityName="Cobb" authorityYear="1913" box="[586,939,1411,1435]" class="Adenophorea" family="Dorylaimidae" genus="Dorylaimellus" kingdom="Animalia" order="Dorylaimida" pageId="32" pageNumber="2818" phylum="Nematoda" rank="species" species="virginianus" subGenus="Dorylaimellus">
