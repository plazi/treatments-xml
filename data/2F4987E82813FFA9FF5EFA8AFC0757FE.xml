<paragraph id="A75F36FE2813FFA8FF5EFAA0FD1352AB" blockId="0.[151,1437,1347,2022]" box="[151,674,1385,1409]" pageId="0" pageNumber="599">
<emphasis id="9594EAEC2813FFA8FF5EFAA0FE9552AB" bold="true" box="[151,292,1385,1409]" pageId="0" pageNumber="599">Type­species</emphasis>
: 
<taxonomicName id="60E04D7D2813FFA8FEFBFAA3FD2E52AB" authority="Muller, 1836" authorityName="Muller" authorityYear="1836" box="[306,671,1385,1409]" class="Aves" family="Muscicapidae" genus="Muscicapa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="0" pageNumber="599" phylum="Chordata" rank="species" species="concreta">
<emphasis id="9594EAEC2813FFA8FEFBFAA3FDBB52AB" box="[306,522,1386,1409]" italics="true" pageId="0" pageNumber="599">Muscicapa concreta</emphasis>
