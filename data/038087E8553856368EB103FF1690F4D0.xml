<emphasis id="B95DEAEC553856368C6903BE1315F2A6" box="[530,621,3316,3337]" pageId="3" pageNumber="276">German:</emphasis>
<vernacularName id="052A46D0553856368C0E03BE1252F2A6" box="[629,810,3316,3337]" pageId="3" pageNumber="276">Togian-Hirscheber</vernacularName>
