<taxonomicName id="75C408AEFF88FF8EC91C99F27988C5B8" authority="Schuh &amp; Štys &amp; Cassis &amp; Lehnert &amp; Swanson &amp; Bruce, 2015" authorityName="Schuh &amp; Štys &amp; Cassis &amp; Lehnert &amp; Swanson &amp; Bruce" authorityYear="2015" box="[366,760,1204,1230]" class="Insecta" family="Plokiophilidae" genus="Paraplokiophiloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" phylum="Arthropoda" rank="species" species="schwendingeri" status="new species">
