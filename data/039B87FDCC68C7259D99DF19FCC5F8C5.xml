<taxonomicName id="4C324D68CC68C72599EEDE56FABBF897" authorityName="Diels" authorityYear="1925" box="[1248,1366,1813,1838]" class="Magnoliopsida" family="Annonaceae" genus="Polyalthia" kingdom="Plantae" order="Magnoliales" pageId="16" pageNumber="49" phylum="Tracheophyta" rank="species" species="decora">
