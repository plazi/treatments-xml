: Sigwart &amp; 
<bibRefCitation id="74694B3DFFBAFFF8FCFB2AE2FC1CF903" box="[856,1011,1733,1758]" pageId="12" pageNumber="313" refId="ref17200" refString="Sirenko, V. I. (2012) Chitons (Мollusca, Polyplacophora) of Nha Trang Vay, South Vietnam. In: Vritayev, T. A. &amp; Pavlov, D. S. (Eds.), Benthiс fauna of the Bay of Nha Trang, South Vietnam. Vol. 2. KМK Scientific Press Ltd, Мoscow, pp. 56 - 122." type="book chapter">Sirenko 2012</bibRefCitation>
) and 
<collectingCountry id="68EF765CFFBAFFF8FB962AE2FB68F903" box="[1077,1159,1733,1758]" name="Taiwan" pageId="12" pageNumber="313">Taiwan</collectingCountry>
(
<quantity id="D7009B29FFBAFFF8FB3B2AE2FB1AF903" box="[1176,1269,1733,1758]" metricMagnitude="3" metricUnit="m" metricValue="2.62" pageId="12" pageNumber="313" unit="m" value="2620.0">2620 m</quantity>
