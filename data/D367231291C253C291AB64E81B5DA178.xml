<taxonomicName id="6104F64F1EB243417203D29D66BAE7F8" baseAuthorityName="Hubner" baseAuthorityYear="1823" class="Hexapoda" family="Erebidae" genus="Alabama" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Alabama argillacea" order="Lepidoptera" pageId="0" pageNumber="23" phylum="Arthropoda" rank="species" species="argillacea">
<emphasis id="6816D63F09782EBD6CAA920D5D411B70" italics="true" pageId="0" pageNumber="23">Alabama argillacea</emphasis>
</taxonomicName>
(
<normalizedToken id="565060F846407DEBD6BFC81AE76044A0" originalValue="Hübner">Huebner</normalizedToken>
, 1818) (
<taxonomicName id="9843F88B48D046C4F7E9EA96A4510004" class="Insecta" family="Noctuidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Lepidoptera" pageId="0" pageNumber="23" phylum="Arthropoda" rank="family">Noctuidae</taxonomicName>
