<taxonomicName id="4C5B4D086C69FFF0B6B0FB92E8C4FBED" box="[744,966,1086,1112]" class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" pageId="79" pageNumber="80" phylum="Tracheophyta" rank="species" species="ehrenbergii">
