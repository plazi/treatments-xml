</taxonomicName>
, 
<taxonomicName id="4C07C9F6FF96EA530844A9920626FB2B" authority="Roman, 1965" authorityName="Roman" authorityYear="1965" box="[837,1130,1159,1184]" class="Secernentea" family="Hoplolaimidae" genus="Helicotylenchus" kingdom="Animalia" order="Tylenchida" pageId="5" pageNumber="580" phylum="Nematoda" rank="species" species="curvatus">
