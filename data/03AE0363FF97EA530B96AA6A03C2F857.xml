Román, 
<number id="3DCFA052FF96EA530F31A99C0626FB2B" box="[1072,1130,1159,1184]" pageId="5" pageNumber="580" value="1965.0">1965</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C07C9F6FF96EA530F77A9920054FB4F" authority="Elbadri, Moon, Wani, Bukhari, Lee &amp; Choo, 2009" authorityName="Elbadri, Moon, Wani, Bukhari, Lee &amp; Choo" authorityYear="2009" class="Secernentea" family="Hoplolaimidae" genus="Helicotylenchus" kingdom="Animalia" order="Tylenchida" pageId="5" pageNumber="580" phylum="Nematoda" rank="species" species="zeidani">
