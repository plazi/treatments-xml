and 
<taxonomicName id="4C07C9F6FF96EA53094FA9B601EBFB4F" authority="Siddiqi, 1972" authorityName="Siddiqi" authorityYear="1972" box="[590,935,1196,1221]" class="Secernentea" family="Hoplolaimidae" genus="Helicotylenchus" kingdom="Animalia" order="Tylenchida" pageId="5" pageNumber="580" phylum="Nematoda" rank="species" species="dihysteroides">
