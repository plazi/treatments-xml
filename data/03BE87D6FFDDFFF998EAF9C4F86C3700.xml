<quantity id="4CEF9B25FFDDFFF99C5EF98CFEE8369C" box="[1059,1193,1662,1689]" metricMagnitude="2" metricUnit="m" metricValue="6.25" metricValueMax="12.0" metricValueMin="0.5" pageId="76" pageNumber="77" unit="m" value="625.0" valueMax="1200.0" valueMin="50.0">50–1,200 m</quantity>
(
<quantity id="4CEF9B25FFDDFFF99CCAF98DFF0B369F" box="[1207,1354,1663,1690]" metricMagnitude="2" metricUnit="m" metricValue="6.248399999999999" metricValueMax="11.996928" metricValueMin="0.499872" pageId="76" pageNumber="77" unit="ft" value="2050.0" valueMax="3936.0" valueMin="164.0">164–3,936 ft</quantity>
