<taxonomicName id="4C3AFA8DFFFAFF81C18BFA3DFD851FDA" box="[151,556,1444,1470]" class="Insecta" family="Lauxaniidae" genus="Dacus" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="vestigivittatus" status="sp.n." subGenus="Psilodacus">
