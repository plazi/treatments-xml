<taxonomicName id="27CA4D35FE8EFEE85D28F9D6FC1F67E0" authority="(Macquart, 1851) (Herting &amp; Dely-Draskovits 1993: 155)" authorityName="Herting &amp; Dely-Draskovits" authorityYear="1993" baseAuthorityName="Macquart" baseAuthorityYear="1851" class="Insecta" family="Tachinidae" genus="Istocheta" kingdom="Animalia" order="Diptera" pageId="323" pageNumber="324" phylum="Arthropoda" rank="species" species="cinerea">
