<taxonomicName id="27CA4D35FE8EFEE85AAAF9D6FB99670C" authority="Rondani, 1859" authorityName="Rondani" authorityYear="1859" box="[692,1066,1626,1653]" class="Insecta" family="Tachinidae" genus="Istocheta" kingdom="Animalia" order="Diptera" pageId="323" pageNumber="324" phylum="Arthropoda" rank="species" species="frontosa">
