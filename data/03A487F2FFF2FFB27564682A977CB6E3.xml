<quantity id="4CF59B01FFF5FFB2756C6A3697CEB129" box="[563,726,791,818]" metricMagnitude="3" metricUnit="m" metricValue="1.4" metricValueMax="1.41" metricValueMin="1.39" pageId="12" pageNumber="13" unit="m" value="1400.0" valueMax="1410.0" valueMin="1390.0">1390–1410 m</quantity>
