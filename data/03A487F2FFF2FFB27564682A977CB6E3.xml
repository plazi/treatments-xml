<geoCoordinate id="EE395023FFF5FFB273986BD59069B115" box="[1223,1393,756,783]" degrees="07" direction="south" minutes="50" orientation="latitude" pageId="12" pageNumber="13" precision="1" seconds="38.4" value="-7.8440003">07°50’38.4” S</geoCoordinate>
