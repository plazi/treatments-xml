<taxonomicName id="AFD9A596124EFFC3FD57FC24FBCBFC29" authority="(Gravenhorst, 1831)" authorityName="Gravenhorst" authorityYear="1831" baseAuthorityName="Gravenhorst" baseAuthorityYear="1831" box="[660,1134,980,1005]" class="Gastropoda" family="Vermetidae" genus="Vermetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="granulatus">
