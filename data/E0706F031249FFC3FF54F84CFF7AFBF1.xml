<taxonomicName id="AFD9A596124EFFC3FDAAFC40FC77FC0D" authority="Biondi, 1858" authorityName="Biondi" authorityYear="1858" box="[617,978,943,968]" class="Gastropoda" family="Vermetidae" genus="Vermetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="cristatus">
