<taxonomicName id="4C505DB7FFEDFFABFD75254AFCC4FEFA" box="[711,885,261,284]" class="Phaeophyceae" family="Lessoniaceae" genus="Eisenia" higherTaxonomySource="CoL" kingdom="Animalia" order="Laminariales" pageId="3" pageNumber="285" phylum="Ochrophyta" rank="species" species="muganiensis">
