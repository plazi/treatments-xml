<caption id="65E710AF5C79FFCEFF36FAF0DBD6A7D9" ID-DOI="http://doi.org/10.5281/zenodo.4523640" ID-Zenodo-Dep="4523640" httpUri="https://zenodo.org/record/4523640/files/figure.png" pageId="16" pageNumber="267" startId="16.[178,218,1331,1351]" targetBox="[182,1083,212,1314]" targetPageId="16">
<paragraph id="312740275C79FFCEFF36FAF0DBD6A7D9" blockId="16.[178,1086,1331,1419]" pageId="16" pageNumber="267">
<emphasis id="03EC9C355C79FFCEFF36FAF0D871A715" bold="true" box="[178,278,1331,1351]" pageId="16" pageNumber="267">Figs 49-59</emphasis>
: 
<taxonomicName id="F6983BA45C79FFCEFEA0FAF0D884A715" authority="Assing, 2008" authorityName="Assing" authorityYear="2008" box="[292,483,1331,1351]" class="Insecta" family="Staphylinidae" genus="Leptusa" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="267" phylum="Arthropoda" rank="species" species="armatissima" status="nov.sp.">
<emphasis id="03EC9C355C79FFCEFEA0FAF0D884A715" box="[292,483,1331,1351]" italics="true" pageId="16" pageNumber="267">Leptusa armatissima</emphasis>
</taxonomicName>
<taxonomicNameLabel id="18DF214E5C79FFCEFE69FAF0DB55A715" box="[493,562,1331,1351]" pageId="16" pageNumber="267" rank="species">nov.sp.</taxonomicNameLabel>
