<taxonomicName id="14FF22E7FFBB4A70FD72893EFCDFED33" authorityName="Forel" authorityYear="1908" box="[727,811,591,616]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="chloe">
