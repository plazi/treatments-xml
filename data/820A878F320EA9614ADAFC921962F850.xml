<number id="BC6B24BE320EA96148ABFA411E7BFA0C" box="[742,825,1484,1509]" pageId="7" pageNumber="265" value="102.5" valueMax="119.0" valueMin="86.0">86–119</number>
(
<number id="BC6B24BE320EA9614900FA411E29FA0D" box="[845,875,1484,1508]" pageId="7" pageNumber="265" value="98.0">98</number>
) long by 
<number id="BC6B24BE320EA96149AEFA411904FA0D" box="[995,1094,1484,1508]" pageId="7" pageNumber="265" value="240.5" valueMax="243.0" valueMin="238.0">238–243</number>
(
<number id="BC6B24BE320EA9614E14FA4119C7FA0D" box="[1113,1157,1484,1508]" pageId="7" pageNumber="265" value="241.0">241</number>
