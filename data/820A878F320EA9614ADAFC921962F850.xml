(
<number id="BC6B24BE320EA9614B98FA251CBFFA29" box="[469,509,1448,1472]" pageId="7" pageNumber="265" value="4.7">4.7</number>
). Testes tandem; anterior testis asymmetrical, cuneiform, 
<number id="BC6B24BE320EA9614ED3FA2A19B3FA29" box="[1182,1265,1447,1472]" pageId="7" pageNumber="265" value="107.0" valueMax="119.0" valueMin="95.0">95–119</number>
(
<number id="BC6B24BE320EA9614F49FA251872FA29" box="[1284,1328,1448,1472]" pageId="7" pageNumber="265" value="107.0">107</number>
) long by 
<number id="BC6B24BE320EA9614ADAFA411DBAFA0C" box="[151,248,1484,1509]" pageId="7" pageNumber="265" value="264.0" valueMax="268.0" valueMin="260.0">260–268</number>
(
<number id="BC6B24BE320EA9614B41FA411C7BFA0C" box="[268,313,1484,1509]" pageId="7" pageNumber="265" value="264.0">264</number>
