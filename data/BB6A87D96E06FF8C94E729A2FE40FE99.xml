<taxonomicName id="F4C34D4C6E07FF8B95202A50FD88F8F4" authority="Hossain &amp; Kwon &amp; Suh &amp; Kwon, 2019" authorityName="Hossain &amp; Kwon &amp; Suh &amp; Kwon" authorityYear="2019" box="[336,575,1883,1907]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="343" phylum="Arthropoda" rank="species" species="giusana" status="sp. nov." subGenus="Empoasca">
