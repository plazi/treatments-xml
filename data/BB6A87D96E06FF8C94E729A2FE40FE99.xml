<caption id="67BC66476E07FF8B94E72A56FC9AF83C" httpUri="https://zenodo.org/record/3363838/files/figure.png" pageId="3" pageNumber="343" startId="3.[151,264,1882,1907]" targetBox="[153,1429,251,1854]" targetPageId="3">
