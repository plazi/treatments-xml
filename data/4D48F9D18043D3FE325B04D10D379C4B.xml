<taxonomicName id="BD40EA32F651D48F84FAB25C8F9202EB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26216" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus atlantis Forel" order="Hymenoptera" pageNumber="63" phylum="Arthropoda" rank="subSpecies" species="rubripes" subSpecies="atlantis">r. C. Atlantis</taxonomicName>
<taxonomicNameLabel id="C67287F0F3C3E135038EDA3F9137C74B" rank="subSpecies">n. st.</taxonomicNameLabel>
</paragraph>
