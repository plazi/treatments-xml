<taxonomicName id="66D94D1DFFC2FFD1FF4AFD06FE05FD4D" authority="R.T. Lowe, 1852: 276" authorityName="R.T. Lowe" authorityPageNumber="276" authorityYear="1852" box="[151,427,679,698]" class="Gastropoda" family="Lauriidae" genus="Leiostyla" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="4" pageNumber="7" phylum="Mollusca" rank="genus">
<emphasis id="93ADEA8CFFC2FFD1FF4AFD06FF4AFD4D" box="[151,228,679,698]" italics="true" pageId="4" pageNumber="7">Leiostyla</emphasis>
<bibRefCitation id="C5484B6FFFC2FFD1FF34FD06FED1FD4D" author="LOWE, R. T." box="[233,383,679,698]" journalOrPublisher="The Annals and Magazine of natural History" pageId="4" pageNumber="7" pagination="275 - 279" part="2" refString="LOWE, R. T., 1852. Brief diagnostic notices of new Maderan land shells. - The Annals and Magazine of natural History (2) 9 (50): 112 - 120; (2) 9 (52): 275 - 279." title="Brief diagnostic notices of new Maderan land shells" type="journal article" year="1852">R.T. Lowe, 1852</bibRefCitation>
