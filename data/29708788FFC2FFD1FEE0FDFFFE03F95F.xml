<emphasis id="93ADEA8CFFC2FFD1FE0FFD6BFE51FD2B" box="[466,511,714,732]" italics="true" pageId="4" pageNumber="7">Pupa</emphasis>
[
<taxonomicName id="66D94D1DFFC2FFD1FDD1FD6BFDF7FD2A" box="[524,601,714,733]" class="Gastropoda" family="Lauriidae" genus="Leiostyla" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="4" pageNumber="7" phylum="Mollusca" rank="genus">
