<treatmentCitationGroup id="B1B611BBFF9BFF9AD4C5E06555E11A4E" box="[151,675,1586,1608]" pageId="71" pageNumber="72">
<taxonomicName id="56A64D16FF9BFF9AD4C5E06555DD1A4E" authority="Mound &amp; Marullo, 1996: 299" authorityName="Mound &amp; Marullo" authorityPageNumber="299" authorityYear="1996" box="[151,671,1586,1608]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="paulus">
<emphasis id="A3D2EA87FF9BFF9AD4C5E065562A1A4E" box="[151,360,1586,1608]" italics="true" pageId="71" pageNumber="72">Holopothrips paulus</emphasis>
<treatmentCitation id="10071084FF9BFF9AD53CE06555DD1A4E" author="" box="[366,671,1586,1608]" page="299" pageId="71" pageNumber="72">
<bibRefCitation id="F5374B64FF9BFF9AD53CE065552E1A4E" box="[366,620,1586,1608]" pageId="71" pageNumber="72" refId="ref59437" refString="Mound, L. A. &amp; Marullo, R. (1996) The thrips of Central and South America: an introduction (Insecta: Thysanoptera). Memoirs on Entomology, International, 6, 1 - 488." type="journal article">Mound &amp; Marullo, 1996</bibRefCitation>
