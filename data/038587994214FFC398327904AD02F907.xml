(
<bibRefCitation id="EFBD4B7E4214FFC29F857C32AC19F9D6" author="Pearse AS" box="[1264,1399,1613,1632]" pageId="40" pageNumber="41" pagination="531 - 556" refId="ref82411" refString="Pearse AS. 1915. An account of the Crustacea collected by the Walker expedition to Santa Marta, Colombia. Proceedings of the United States National Museum 49 (2123): 531 - 556 + plates 70 - 73." type="journal article" year="1915">Pearse, 1915</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8B93368F4214FFC29BEB7C16AC10F92E" blockId="40.[158,1407,1208,1884]" pageId="40" pageNumber="41">
<keyLead id="30DD95BA4214FFC29BEB7C16AC10F92E" pageId="40" pageNumber="41">
