<emphasis id="B932EABAFFB4FFA3FE01FCEAFC3EFCCF" box="[385,940,870,894]" italics="true" pageId="7" pageNumber="256">
<taxonomicName id="4C464D2BFFB4FFA3FE01FCEAFE23FCCF" box="[385,433,871,894]" class="Reptilia" family="Boidae" genus="Boa" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="256" phylum="Chordata" rank="genus">Boa</taxonomicName>
