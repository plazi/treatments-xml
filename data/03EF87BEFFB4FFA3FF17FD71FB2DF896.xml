<taxonomicName id="4C464D2BFFB4FFA3FE3CFCEBFDC0FCCF" box="[444,594,870,894]" class="Reptilia" family="Boidae" genus="Chilabothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="256" phylum="Chordata" rank="genus">Chilabothrus</taxonomicName>
, 
