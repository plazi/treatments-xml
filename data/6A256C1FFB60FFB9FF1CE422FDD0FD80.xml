<taxonomicName id="258CA68AFB61FFB9FC82E3FFFC18FF15" authority="Hübner, 1821" authorityName="Hübner" authorityYear="1821" box="[777,1015,152,175]" class="Insecta" family="Noctuidae" genus="Periphanes" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="treitschkei">
