<taxonomicName id="4C144D2DFF9AFF81FD85FEFBCC8B1E59" box="[577,715,295,320]" class="Insecta" family="Carabidae" genus="Calathus" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="jakupicensis">
