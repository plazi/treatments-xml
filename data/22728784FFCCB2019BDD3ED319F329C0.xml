</taxonomicName>
and 
<taxonomicName id="6DDB4D11FFCEB2079AEC3EB419852DE7" authority="(Kobayashi &amp; Kishida, 2004) (Kobayashi, Kishida &amp; Min, 2008)" authorityName="Kobayashi, Kishida &amp; Min" authorityYear="2008" baseAuthorityName="Kobayashi &amp; Kishida" baseAuthorityYear="2004" box="[422,1214,1449,1472]" class="Insecta" family="Notodontidae" genus="Bireta" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="species" species="fortis">
<emphasis id="98AFEA80FFCEB2079AEC3EB41CC72DE7" box="[422,508,1449,1472]" italics="true" pageId="2" pageNumber="59">B. fortis</emphasis>
