<taxonomicName id="42ACC1F90AE2661EBF679C3E877F956D" class="Insecta" family="Coreidae" genus="Melucha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Melucha lineatella" order="Hemiptera" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="lineatella">Melucha lineatella</taxonomicName>
