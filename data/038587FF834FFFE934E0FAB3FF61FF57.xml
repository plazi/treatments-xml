</taxonomicName>
<emphasis id="B958EAFB834EFFEA3592FA55F830FA5B" bold="true" box="[485,587,1501,1525]" pageId="9" pageNumber="542">
<taxonomicNameLabel id="A26B5780834EFFEA3592FA55F830FA5B" box="[485,587,1501,1525]" pageId="9" pageNumber="542" rank="species">spec. nov.</taxonomicNameLabel>
</emphasis>
, scale = 0.5 mm: 
<emphasis id="B958EAFB834EFFEA368BFA54F961FA5A" bold="true" box="[764,794,1500,1524]" pageId="9" pageNumber="542">7A</emphasis>
, male genitalia, left lateral; 
<emphasis id="B958EAFB834EFFEA3044FA55FE2AFA5B" bold="true" box="[1075,1105,1501,1525]" pageId="9" pageNumber="542">7B</emphasis>
, male genitalia, dorsal; 
<emphasis id="B958EAFB834EFFEA3137FA54FF24FA5A" bold="true" box="[1344,1375,1500,1524]" pageId="9" pageNumber="542">7C</emphasis>
, male genitalia, ventral; 
<emphasis id="B958EAFB834EFFEA3538F989FB16F9B7" bold="true" box="[335,365,1537,1561]" pageId="9" pageNumber="542">7D</emphasis>
, female genitalia, left lateral; 
<emphasis id="B958EAFB834EFFEA36E9F989F8C7F9B7" bold="true" box="[670,700,1537,1561]" pageId="9" pageNumber="542">7E</emphasis>
, female genitalia, dorsal; 
<emphasis id="B958EAFB834EFFEA37B4F989F99BF9B7" bold="true" box="[963,992,1537,1561]" pageId="9" pageNumber="542">7F</emphasis>
