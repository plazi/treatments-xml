<taxonomicName id="4C32F4912D67FFE6B1261F972FCF3F86" authorityName="Udayan &amp; Robi" authorityYear="2017" box="[875,1005,161,184]" class="Magnoliopsida" family="Lauraceae" genus="Litsea" kingdom="Plantae" order="Laurales" pageId="1" pageNumber="198" phylum="Tracheophyta" rank="species" species="gorayana">
<emphasis id="B94653002D67FFE6B1261F972FCF3F86" box="[875,1005,161,184]" italics="true" pageId="1" pageNumber="198">L. gorayana</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C32F4912D67FFE6B66C1F9728B83F86" box="[1057,1178,160,184]" class="Magnoliopsida" family="Lauraceae" genus="Litsea" kingdom="Plantae" order="Laurales" pageId="1" pageNumber="198" phylum="Tracheophyta" rank="species" species="udayanii">
<emphasis id="B94653002D67FFE6B66C1F9728B83F86" box="[1057,1178,160,184]" italics="true" pageId="1" pageNumber="198">L. udayanii</emphasis>
</taxonomicName>
.
</paragraph>
</caption>
