<taxonomicName id="4C394D52FFA5FF8CE0FA4933366D6D27" box="[737,836,581,605]" class="Insecta" family="Carabidae" genus="Blennidus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="jelskii">
