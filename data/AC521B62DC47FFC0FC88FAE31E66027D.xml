<taxonomicName id="E3FBD1F7DC42FFC1FF22FCE91C4B0018" authority="Tishechkin, 2009" authorityName="Tishechkin" authorityYear="2009" box="[188,366,838,855]" class="Insecta" family="Histeridae" genus="Eucurtiopsis" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="673" phylum="Arthropoda" rank="species" species="degallieri" status="n. sp.">
