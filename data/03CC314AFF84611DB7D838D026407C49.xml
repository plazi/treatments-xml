<number id="3DAD927BFF85611DB6623FEB246D7992" box="[301,332,464,490]" pageId="11" pageNumber="12" value="78.0">78</number>
) as “Rondo Forest Reserve, Lindi District, Lindi Region, 
<collectingCountry id="F372C0CCFF85611DB4BD3FEB217A7992" box="[1010,1115,464,490]" name="Tanzania" pageId="11" pageNumber="12">Tanzania</collectingCountry>
(
<geoCoordinate id="EE51E69BFF85611DB3233FEB21E67992" box="[1132,1223,461,490]" direction="south" orientation="latitude" pageId="11" pageNumber="12" precision="925" value="-10.116667">
<number id="3DAD927BFF85611DB3233FEB21AB7992" box="[1132,1162,464,490]" pageId="11" pageNumber="12" value="10.0">10</number>
o07'S
</geoCoordinate>
, 
<geoCoordinate id="EE51E69BFF85611DB39B3FEB20137992" box="[1236,1330,461,490]" direction="east" orientation="longitude" pageId="11" pageNumber="12" precision="925" value="39.383335">
