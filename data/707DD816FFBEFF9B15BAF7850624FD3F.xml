<subSubSection id="B0CE3A8BFFBEFF9A15BAF785029DFA2A" pageId="48" pageNumber="49" type="nomenclature">
<paragraph id="F86B6900FFBEFF9A15BAF785012DFA09" blockId="48.[151,554,1466,1527]" box="[151,554,1466,1492]" pageId="48" pageNumber="49">
<heading id="A323DE6CFFBEFF9A15BAF785012DFA09" bold="true" box="[151,554,1466,1492]" fontSize="11" level="1" pageId="48" pageNumber="49" reason="1">
<emphasis id="CAA0B512FFBEFF9A15BAF785012DFA09" bold="true" box="[151,554,1466,1492]" pageId="48" pageNumber="49">
<taxonomicName id="3FD41283FFBEFF9A15BAF78502E7FA09" authority="Troubridge" authorityName="Troubridge" authorityYear="2008" box="[151,480,1466,1492]" class="Insecta" family="Noctuidae" genus="Sympistis" kingdom="Animalia" order="Lepidoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="shait" status="sp. n.">
<emphasis id="CAA0B512FFBEFF9A15BAF785024CFA09" bold="true" box="[151,331,1466,1492]" italics="true" pageId="48" pageNumber="49">Sympistis shait</emphasis>
Troubridge
</taxonomicName>
