<subSubSection id="B0CE3A8BFFBFFF9B15E8F0A00627FD67" box="[197,1312,671,698]" pageId="49" pageNumber="50" type="etymology">
<paragraph id="F86B6900FFBFFF9B15E8F0A00627FD67" blockId="49.[151,1436,152,738]" box="[197,1312,671,698]" pageId="49" pageNumber="50">
<emphasis id="CAA0B512FFBFFF9B15E8F0A0024DFD64" bold="true" box="[197,330,671,697]" pageId="49" pageNumber="50">Etymology</emphasis>
