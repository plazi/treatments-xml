<number id="2E12D544FFD68F51BF7B2271673CAB5B" box="[604,618,258,283]" pageId="14" pageNumber="115" value="5.0">5</number>
<typeStatus id="476179C1FFD68F51BF5322706789AB5B" box="[628,735,259,283]" pageId="14" pageNumber="115">paratypes</typeStatus>
