<specimenCount id="8EDC0CEAFFD68F51BFD722706635AB5B" box="[752,867,258,283]" pageId="14" pageNumber="115" type="female">
<number id="2E12D544FFD68F51BFD7227067A9AB5B" box="[752,767,259,283]" pageId="14" pageNumber="115" value="4.0">4</number>
females
</specimenCount>
, 
<specimenCount id="8EDC0CEAFFD68F51BE57227066E2AB5B" box="[880,948,258,283]" pageId="14" pageNumber="115" type="generic">
