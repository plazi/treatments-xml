<subSubSection id="D0C094E8FFD68F51BDB022716708ABC7" pageId="14" pageNumber="115" type="materials_examined">
<paragraph id="9865C763FFD68F51BDB022716708ABC7" blockId="14.[151,1437,258,1399]" pageId="14" pageNumber="115">
<emphasis id="AAAE1B71FFD68F51BDB022716411AB5B" bold="true" box="[151,327,258,283]" pageId="14" pageNumber="115">
<typeStatus id="476179C1FFD68F51BDB022716584AB5B" box="[151,210,258,283]" pageId="14" pageNumber="115">Type</typeStatus>
material.
</emphasis>
<typeStatus id="476179C1FFD68F51BC77227164EEAB5B" box="[336,440,258,283]" pageId="14" pageNumber="115">Holotype</typeStatus>
