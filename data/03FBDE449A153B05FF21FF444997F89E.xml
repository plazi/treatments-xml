<quantity id="4CAAC2B79A153B05FB3CF97E4F26F95E" box="[1184,1333,1737,1761]" metricMagnitude="3" metricUnit="m" metricValue="3.9" metricValueMax="4.06" metricValueMin="3.7399999999999998" pageId="13" pageNumber="100" unit="m" value="3900.0" valueMax="4060.0" valueMin="3740.0">3740–4060 m</quantity>
