<taxonomicName id="4C0826EFFF8BF90EF85CFA47B8AB6C4A" authorityName="Chang &amp; Li &amp; Yin &amp; Schülke" authorityYear="2019" box="[525,726,1522,1544]" class="Insecta" family="Staphylinidae" genus="Tachinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="492" phylum="Arthropoda" rank="genus" species="tangliangi">
