<collectionCode id="ED22AAC7FFC7FFE9E8BDFB4DFD8A469A" LSID="urn:lsid:biocol.org:col:34871" box="[481,568,1220,1246]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="34" pageNumber="43">USNM</collectionCode>
