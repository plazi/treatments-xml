<taxonomicName id="4C714D2FFF94167AFF7BFEA8FE88FEB6" authorityName="Strand" authorityYear="1906" box="[219,341,303,329]" class="Arachnida" family="Zodariidae" genus="Mallinella" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
