<quantity id="4CE4EAD8470EFF9CE1B3FA88A5FE9A13" box="[934,1008,1373,1397]" metricMagnitude="-2" metricUnit="m" metricValue="2.5" pageId="26" pageNumber="856" unit="mm" value="25.0">25 mm</quantity>
; Three Brothers, Great Chagos Bank; 
<date id="FFA261FD470EFF9CE3E4FAABA49E9AF0" box="[497,656,1406,1430]" pageId="26" pageNumber="856" value="1996-03-03">3 March 1996</date>
; at 
<quantity id="4CE4EAD8470EFF9CE0AAFAABA4E99AF3" box="[703,743,1406,1430]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="26" pageNumber="856" unit="m" value="5.0">5 m</quantity>
depth. Chag96/92a, b: 
<quantity id="4CE4EAD8470EFF9CE1E1FAABA2319AF3" box="[1012,1087,1406,1430]" metricMagnitude="-2" metricUnit="m" metricValue="3.7" pageId="26" pageNumber="856" unit="mm" value="37.0">37 mm</quantity>
, 
<quantity id="4CE4EAD8470EFF9CE299FA4BA6D89AD3" box="[140,214,1438,1462]" metricMagnitude="-2" metricUnit="m" metricValue="3.5" pageId="26" pageNumber="856" unit="mm" value="35.0">35 mm</quantity>
; Victory Bank; 
<date id="FFA261FD470EFF9CE385FA4BA43E9AD0" box="[400,560,1438,1462]" pageId="26" pageNumber="856" value="1996-03-03">3 March 1996</date>
; at 
<quantity id="4CE4EAD8470EFF9CE04AFA4BA4899AD3" box="[607,647,1438,1462]" metricMagnitude="0" metricUnit="m" metricValue="8.0" pageId="26" pageNumber="856" unit="m" value="8.0">8 m</quantity>
