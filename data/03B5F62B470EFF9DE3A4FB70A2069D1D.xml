<quantity id="4CE4EAD8470EFF9CE3D7FAE8A4029A33" box="[450,524,1341,1365]" metricMagnitude="-2" metricUnit="m" metricValue="2.7" pageId="26" pageNumber="856" unit="mm" value="27.0">27 mm</quantity>
; on rubble near bommie east of Ile Poule, inside Peros Banhos Atoll; 
<date id="FFA261FD470EFF9CE392FA88A4569A13" box="[391,600,1373,1397]" pageId="26" pageNumber="856" value="1996-02-23">23 February 1996</date>
; at 
<quantity id="4CE4EAD8470EFF9CE085FA8BA4B99A13" box="[656,695,1374,1397]" metricMagnitude="0" metricUnit="m" metricValue="7.0" pageId="26" pageNumber="856" unit="m" value="7.0">7 m</quantity>
