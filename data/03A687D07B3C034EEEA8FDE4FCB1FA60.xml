<taxonomicName id="4C0F4D457B3C0342EC0CFC4AFC9AFC02" baseAuthorityName="Green" baseAuthorityYear="1922" box="[563,860,998,1023]" class="Insecta" family="Coccidae" genus="Paralecanium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="trifasciatum">Paralecanium trifasciatum</taxonomicName>
