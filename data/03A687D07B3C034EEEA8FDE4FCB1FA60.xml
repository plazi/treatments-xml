<taxonomicName id="4C0F4D457B3C0342ED28FC32FCBDFC4A" box="[791,891,926,951]" class="Magnoliopsida" family="Magnoliaceae" genus="Michelia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Magnoliales" pageId="68" pageNumber="69" phylum="Tracheophyta" rank="genus">
