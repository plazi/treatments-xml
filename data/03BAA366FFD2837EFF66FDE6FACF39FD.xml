, 
<taxonomicName id="4C1369F3FFD2837EFE31FD2AFC243C0D" authority="Brdt, Brandt" authorityName="Brdt, Brandt" box="[448,919,750,775]" class="Insecta" family="Noctuidae" genus="Agrotis" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="cacumena" subGenus="Ogygia">
