<collectingCountry id="6EA2A1B6FFD44846FC417225FBCEFC83" box="[950,1053,849,874]" name="Malaysia" pageId="17" pageNumber="498">Malaysia</collectingCountry>
and 
<collectingCountry id="6EA2A1B6FFD44846FBA57225FB14FC83" box="[1106,1223,849,874]" name="Singapore" pageId="17" pageNumber="498">Singapore</collectingCountry>
