(
<taxonomicName id="D1B59AA5FFD44846FCF272EDFC57FC5B" box="[773,900,921,946]" class="Magnoliopsida" family="Clusiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="17" pageNumber="498" phylum="Magnoliopsida" rank="family">Clusiaceae</taxonomicName>
