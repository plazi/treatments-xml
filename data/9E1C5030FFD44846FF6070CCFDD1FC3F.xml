Recorded only from East 
<collectingCountry id="6EA2A1B6FFD44846FD7E7225FD23FC83" box="[649,752,849,874]" name="Malaysia" pageId="17" pageNumber="498">Malaysia</collectingCountry>
(
<collectingCountry id="6EA2A1B6FFD44846FD097225FCB7FC83" box="[766,868,849,874]" name="Malaysia" pageId="17" pageNumber="498">
<collectingRegion id="D4712FC4FFD44846FD097225FCB7FC83" box="[766,868,849,874]" country="Malaysia" name="Sarawak" pageId="17" pageNumber="498">Sarawak</collectingRegion>
</collectingCountry>
