<collectingCountry id="6EA2A1B6FFD44846FAAB7226FA4FFC80" box="[1372,1436,850,873]" name="Japan" pageId="17" pageNumber="498">Japan</collectingCountry>
in timber from 
<collectingCountry id="6EA2A1B6FFD44846FEB67201FE71FC67" box="[321,418,885,910]" name="Malaysia" pageId="17" pageNumber="498">
<collectingRegion id="D4712FC4FFD44846FEB67201FE71FC67" box="[321,418,885,910]" country="Malaysia" name="Sarawak" pageId="17" pageNumber="498">Sarawak</collectingRegion>
</collectingCountry>
(
<bibRefCitation id="72249CD7FFD44846FE477201FDE0FC67" box="[432,563,885,910]" pageId="17" pageNumber="498" refId="ref15201" refString="Ohno, S. (1990) The Scolytidae and Platypodidae (Coleoptera) from Borneo found in logs at Nagoya port. 2. Research Bulletin of the Plant Protection Service of Japan, 26, 95 - 103." type="journal article">Ohno 1990</bibRefCitation>
).
</paragraph>
</subSubSection>
