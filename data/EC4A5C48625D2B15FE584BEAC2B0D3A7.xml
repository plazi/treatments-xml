<taxonomicName id="A3E396DD625E2B14FF7B4E0AC29BD04B" authorityName="Fitzhugh" authorityYear="2002" box="[231,464,1381,1403]" class="Polychaeta" family="Fabriciidae" genus="Pseudofabriciola" kingdom="Animalia" order="Sabellida" pageId="14" pageNumber="905" phylum="Annelida" rank="species" species="rousei">
