<taxonomicName id="069B7BF89266FFD3CB4D7CC19EC2FA4D" box="[525,644,1487,1512]" class="Diplopoda" family="Dalodesmidae" genus="Paredrodesmus" kingdom="Animalia" order="Polydesmida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="purpureus">
<emphasis id="F3EFDC699266FFD3CB4D7CC19EC2FA4D" box="[525,644,1487,1512]" italics="true" pageId="18" pageNumber="19">purpureus</emphasis>
