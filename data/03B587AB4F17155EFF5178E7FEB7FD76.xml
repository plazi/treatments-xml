<collectionCode id="ED0DAE784F17155EFA8B7957FAA4FEC6" box="[1357,1391,297,323]" country="Russia" name="Laboratory of Palaeontology" pageId="32" pageNumber="33">LP</collectionCode>
90: height = 6.5; width = 5.9; apical angle = 82 
<superScript id="7C699BF54F17155EFDBA7932FD4AFEDF" attach="none" box="[636,641,332,346]" fontSize="6" pageId="32" pageNumber="33">º</superScript>
