<typeStatus id="E9368845FFE2FF82FF0F5695BC2EBE89" box="[199,258,1717,1742]" pageId="1" pageNumber="122">Type</typeStatus>
material.
</emphasis>
<typeStatus id="E9368845FFE2FF82FEB45695BCC5BE89" box="[380,489,1717,1742]" pageId="1" pageNumber="122">Holotype</typeStatus>
: male, Gulinjing, Maguan, Yunnan, 
<date id="42331027FFE2FF82FCB65696BEFDBE89" box="[894,977,1717,1742]" pageId="1" pageNumber="122" value="1920-07">
<number id="804524C0FFE2FF82FCB65696BEB6BE89" box="[894,922,1718,1742]" pageId="1" pageNumber="122" value="20.0">20</number>
