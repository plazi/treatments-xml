<taxonomicName id="4C184D08FFA5DE58FE40FA747D08FA08" box="[464,623,1444,1469]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" kingdom="Animalia" order="Trichoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="venezuelensis">
