<taxonomicName id="4C02C1F6FFA0FFB595B8FA97FC64FA8D" authorityName="TRAUTMANN. Die" authorityYear="1926" box="[900,1023,1354,1374]" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="50" pageNumber="451" phylum="Arthropoda" rank="species" species="caucasium">
