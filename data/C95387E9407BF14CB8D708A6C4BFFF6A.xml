<number id="F73224D8407BF153B95C0E5EC61EFB30" box="[284,298,1143,1167]" pageId="15" pageNumber="403" value="3.0">3</number>
&amp; 
<number id="F73224D8407BF153B90E0E5EC669FB30" box="[334,349,1143,1167]" pageId="15" pageNumber="403" value="4.0">4</number>
) 
<quantity id="86029B1A407BF153B92C0E5EC688FB31" box="[364,444,1143,1167]" metricMagnitude="-2" metricUnit="m" metricValue="4.1" pageId="15" pageNumber="403" unit="mm" value="41.0">41 mm</quantity>
