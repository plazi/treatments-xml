<taxonomicName id="4C638F8FCE5CDD4AF0DB6E18FD8E589B" box="[492,627,1651,1676]" class="Arachnida" family="Linyphiidae" genus="Ceraticelus" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="52" phylum="Arthropoda" rank="genus">
