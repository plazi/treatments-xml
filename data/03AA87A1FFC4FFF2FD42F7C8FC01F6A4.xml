<materialsCitation id="3B6B3CEAFFC4FFF2FBC3F76EFA50F732" ID-GBIF-Occurrence="1434647838" box="[1053,1475,2230,2268]" location="Iwo Jima" municipality="Bonin-Inseln" pageId="1" pageNumber="174" specimenCount="1">
