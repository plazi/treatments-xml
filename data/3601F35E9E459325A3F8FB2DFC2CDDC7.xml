<collectingCountry id="C6BF02D89E459325A105F876FD21DDC7" box="[618,739,1938,1962]" name="Costa Rica" pageId="28" pageNumber="29">Costa Rica</collectingCountry>
and 
<collectingCountry id="C6BF02D89E459325A075F876FCADDDC7" box="[794,879,1938,1962]" name="Mexico" pageId="28" pageNumber="29">Mexico</collectingCountry>
