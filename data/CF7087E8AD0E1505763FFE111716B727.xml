<taxonomicName id="80D94D7DAD0E150574F2F9DB126DB724" authority="Blainv." authorityName="Blainv." box="[499,837,1564,1596]" class="Polyplacophora" family="Cryptoplacidae" genus="Cryptoplax" kingdom="Animalia" order="Chitonida" pageId="1" pageNumber="396" phylum="Mollusca" rank="species" species="laevis">
