<normalizedToken id="B955C275F5B3657575D49F58DB78B012" originalValue="enfoncés">enfonces</normalizedToken>
<normalizedToken id="65F176E9242B2EED85C7311D3224EFED" originalValue="épars">epars</normalizedToken>
