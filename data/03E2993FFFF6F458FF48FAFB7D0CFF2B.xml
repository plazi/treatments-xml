<taxonomicName id="4C4B53AAFFF6F45BFF48FAFB7EACFADB" authority="Wiegmann" authorityName="Wiegmann" box="[151,501,1293,1319]" class="Reptilia" family="Teiidae" genus="Ameiva" kingdom="Animalia" order="Squamata" pageId="21" pageNumber="22" phylum="Chordata" rank="species" species="undulata">
