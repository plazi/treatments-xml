), 
<collectingCountry id="97E63F8D4E579925FC0AFABBCD84DD51" box="[957,1206,1309,1335]" name="Dominican Republic" pageId="15" pageNumber="16">Dominican Republic</collectingCountry>
(
<collectionCode id="89E0E7D84E579925FB70FABBCC15DD51" box="[1223,1319,1309,1335]" httpUri="http://grbio.org/cool/daw7-8h44" name="The Cleveland Museum of Natural History" pageId="15" pageNumber="16">CMNH</collectionCode>
), 
<collectingCountry id="97E63F8D4E579925FA8BFABBCCAFDD51" box="[1340,1437,1309,1335]" name="Jamaica" pageId="15" pageNumber="16">Jamaica</collectingCountry>
(
<collectionCode id="89E0E7D84E579925FF28FAE4C9CEDD3A" LSID="urn:lsid:biocol.org:col:34871" box="[159,252,1346,1372]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="15" pageNumber="16">USNM</collectionCode>
), and 
<collectingCountry id="97E63F8D4E579925FEF1FAE4C8E5DD3A" box="[326,471,1346,1372]" name="Puerto Rico" pageId="15" pageNumber="16">Puerto Rico</collectingCountry>
(
<collectionCode id="89E0E7D84E579925FE5FFAE4CB2BDD3A" box="[488,537,1346,1372]" pageId="15" pageNumber="16">TW</collectionCode>
, 
<collectionCode id="89E0E7D84E579925FD91FAE4CB56DD3A" box="[550,612,1346,1372]" pageId="15" pageNumber="16">UPR</collectionCode>
, 
<collectionCode id="89E0E7D84E579925FDC7FAE4CBE2DD3A" box="[624,720,1346,1372]" httpUri="http://grbio.org/cool/daw7-8h44" name="The Cleveland Museum of Natural History" pageId="15" pageNumber="16">CMNH</collectionCode>
). 
<bibRefCitation id="8B6002EC4E579925FD52FAE4CA4BDD3A" author="Dear" box="[741,889,1346,1372]" pageId="15" pageNumber="16" refString="Dear, J. P. (1985) A revision of the New World Chrysomyini (Diptera: Calliphoridae). Revista Brasileira de Zoologia, 3, 109 - 169." type="journal article" year="1985">
Dear (
<number id="59396D3A4E579925FC86FAE4CA42DD3A" box="[817,880,1346,1372]" pageId="15" pageNumber="16" value="1985.0">1985</number>
)
</bibRefCitation>
