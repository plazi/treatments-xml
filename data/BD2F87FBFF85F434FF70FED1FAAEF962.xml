. 
<materialsCitation id="85EE3CB0FF85F434FE63FA3EFDCEF9C4" collectingDate="1986-07-07" collectionCode="EME" collectorName="P. Jump" country="Mexico" location="Sierra Alta Tarahumara" municipality="Male" pageId="25" pageNumber="26" specimenCount="1" stateProvince="Chihuahua" typeStatus="holotype">
