<taxonomicName id="4C0687F8AE6AFF93FC52F9D6FB421D05" box="[972,1242,1590,1615]" class="Mammalia" family="Echimyidae" genus="Proechimys" kingdom="Animalia" order="Rodentia" pageId="8" pageNumber="247" phylum="Chordata" rank="species" species="decumanus">
