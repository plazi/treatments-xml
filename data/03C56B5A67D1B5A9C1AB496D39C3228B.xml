</taxonomicName>
(= 
<taxonomicName id="4C6CA1CF67D1B5A9C5B9480A38C922E7" authority="Olivier, 1807" authorityName="Olivier" authorityYear="1807" class="Insecta" family="Curculionidae" genus="Larinus" kingdom="Animalia" order="Coleoptera" pageId="388" pageNumber="389" phylum="Arthropoda" rank="species" species="carlinae">
