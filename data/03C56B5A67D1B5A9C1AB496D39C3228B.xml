<taxonomicName id="4C6CA1CF67D1B5A9C0D448ED3B4622E7" authority="Fabricius, 1792" authorityName="Fabricius" authorityYear="1792" box="[488,860,925,950]" class="Insecta" family="Curculionidae" genus="Curculio" kingdom="Animalia" order="Coleoptera" pageId="388" pageNumber="389" phylum="Arthropoda" rank="species" species="planus">
