species: 
<taxonomicName id="F18F4D5FFFFEFF9A0647FF4E562FFF1B" authority="Pascoe, 1857" authorityName="Pascoe" authorityYear="1857" box="[294,629,152,174]" class="Insecta" family="Cerambycidae" genus="Golsinda" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="corallina">
<emphasis id="04FBEACEFFFEFF9A0647FF4E55BCFF1B" box="[294,486,152,174]" italics="true" pageId="1" pageNumber="64">Golsinda corallina</emphasis>
Pascoe, 
<number id="804724FBFFFEFF9A055EFF4E562FFF1B" box="[575,629,152,174]" pageId="1" pageNumber="64" value="1857.0">1857</number>
</taxonomicName>
