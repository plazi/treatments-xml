<taxonomicName id="4C304D536555FF8B74E0C44FFD62FD81" box="[473,753,524,549]" class="Insecta" family="Lycidae" genus="Dilophotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="anthracinus" status="sp. n.">
