<taxonomicName id="0A67005D971AD73270C5CEB311ADBCC7" class="Anthozoa" family="Alcyoniidae" genus="Sinularia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinularia australiensis" order="Alcyonacea" pageId="3" pageNumber="32" phylum="Cnidaria" rank="species" species="australiensis">Sinularia australiensis</taxonomicName>
