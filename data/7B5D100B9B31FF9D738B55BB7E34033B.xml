, palpal organ, ventrolateral view; 
<emphasis id="C1807D0F9B32FF9C725956287C3706DB" bold="true" box="[267,281,1657,1675]" pageId="22" pageNumber="617">C</emphasis>
, palpal organ,lateral view; 
<emphasis id="C1807D0F9B32FF9C72AA56287F2806DB" bold="true" box="[504,518,1657,1675]" pageId="22" pageNumber="617">D</emphasis>
, 
<emphasis id="C1807D0F9B32FF9C715D56287F3506DB" bold="true" box="[527,539,1657,1675]" pageId="22" pageNumber="617">E</emphasis>
, palpal tibia; 
<emphasis id="C1807D0F9B32FF9C71D556287FBC06DB" bold="true" box="[647,658,1657,1675]" pageId="22" pageNumber="617">F</emphasis>
