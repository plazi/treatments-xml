<taxonomicName id="36AB6FF1C08CE1A1AE636734AC136617" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142247" class="Insecta" family="Formicidae" genus="Atta" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Atta diligens Smith" order="Hymenoptera" pageNumber="235" phylum="Arthropoda" rank="species" species="diligens">diligens</taxonomicName>
