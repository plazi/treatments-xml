<date id="547AB0B33D57FFB0FC9FFB3BFC53FA9B" box="[789,952,1266,1293]" pageId="36" pageNumber="37" value="2016-10-15" valueMax="2016-10-17" valueMin="2016-10-15">
<collectingDate id="443E495B3D57FFB0FC9FFB3BFC53FA9B" box="[789,952,1266,1293]" pageId="36" pageNumber="37" value="2016-10-15" valueMax="2016-10-17" valueMin="2016-10-15">15–17.X.2016</collectingDate>
</date>
, 
<location id="251BC0A83D57FFB0FC4FFB3AFE78FAA7" LSID="urn:lsid:plazi:treatment:A86D27653D57FFB2FF1DFC66FC3BFF6F:251BC0A83D57FFB0FC4FFB3AFE78FAA7" country="Colombia" county="San Antonio del Tequendama" latitude="4.606353" longLatPrecision="1" longitude="-74.30583" municipality="Parque Natural de Chicaque" name="Estudiantes Curso de Aranas Universidad Nacional de Colombia" pageId="36" pageNumber="37" stateProvince="Cundinamarca">Estudiantes Curso de Arañas Universidad Nacional de Colombia</location>
leg. (ICN-Ar-10603); ♁1, same data (ICN-Ar-10610)
