<taxonomicName id="979768AC2319F34DFE36FEE5E715AC9D" authorityName="Hagen in Selys" authorityYear="1865" box="[472,572,367,392]" class="Insecta" family="Coenagrionidae" genus="Argia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="140" phylum="Arthropoda" rank="species" species="mollis">
