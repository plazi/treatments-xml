<taxonomicName id="ECC44D6E9672FFF8FF4BFC83798FC833" baseAuthorityName="Pallas" baseAuthorityYear="1814" box="[151,179,802,826]" class="Actinopterygii" family="Gobiidae" genus="Neogobius" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="398" phylum="Chordata" rank="species" species="melanostomus">16</taxonomicName>
