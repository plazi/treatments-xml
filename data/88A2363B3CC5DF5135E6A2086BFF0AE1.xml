<subSubSection id="4DB5C3A11D45E8C69C27F1C00D4C123E" pageId="0" pageNumber="1" type="etymology">
<paragraph id="AED2C3B472345612ADD4B1A18EFDFCCA" pageId="0" pageNumber="1">Etymology.</paragraph>
<paragraph id="CD353681AE126E4B9EF88CFE070E302F" pageId="0" pageNumber="1">
<taxonomicName id="DF0F8A7D4D5071D4FF14F6290324E633" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Chelonus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chelonus sergioriosi" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="sergioriosi">
