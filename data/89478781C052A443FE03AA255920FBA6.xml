<collectingCountry id="79F97607C057A443FF78AB255A15FBA6" box="[138,323,1078,1102]" name="New Caledonia" pageId="14" pageNumber="831">New Caledonia</collectingCountry>
, between 190 and 
<specimenCount id="17E8FD1EC057A443FDD0AB255920FBA6" box="[546,630,1078,1102]" pageId="14" pageNumber="831" type="generic">
<quantity id="C6169B72C057A443FDD0AB255924FBA6" box="[546,626,1078,1102]" metricMagnitude="2" metricUnit="m" metricValue="2.4" pageId="14" pageNumber="831" unit="m" value="240.0">
<elevation id="8AC3D1A4C057A443FDD0AB255924FBA6" box="[546,626,1078,1102]" metricMagnitude="2" metricUnit="m" metricValue="2.4" pageId="14" pageNumber="831" unit="m" value="240.0">240 m</elevation>
</quantity>
.
</specimenCount>
</materialsCitation>
</paragraph>
