<subSubSection id="C373278EFFE4FFD2D9C703A29036378D" box="[199,1398,405,432]" pageId="32" pageNumber="233" type="etymology">
<paragraph id="8BD67405FFE4FFD2D9C703A29036378D" blockId="32.[151,1436,153,432]" box="[199,1398,405,432]" pageId="32" pageNumber="233">
<emphasis id="B91DA817FFE4FFD2D9C703A2940E378D" bold="true" box="[199,334,405,431]" pageId="32" pageNumber="233">Etymology.</emphasis>
