<taxonomicName id="4C690F86FFE4FFD2DB3503A19733378D" authorityName="Johanson &amp; Mary &amp; Sjöberg &amp; Malm" authorityYear="2020" box="[565,627,406,431]" class="Insecta" family="Leptoceridae" genus="Oecetis" kingdom="Animalia" order="Trichoptera" pageId="32" pageNumber="233" phylum="Arthropoda" rank="species" species="ovata">
