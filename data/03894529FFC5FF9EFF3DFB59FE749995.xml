<collectingCountry id="F337B4AFFFC5FF9EFEBCF984FE78994A" box="[278,418,1634,1660]" name="Uzbekistan" pageId="2" pageNumber="17">Uzbekistan</collectingCountry>
[
<geoCoordinate id="EE1492F8FFC5FF9EFE18F984FDFA994A" box="[434,544,1634,1660]" direction="north" orientation="latitude" pageId="2" pageNumber="17" precision="555" value="40.96">
<number id="3DE8E618FFC5FF9EFE18F984FE25994A" box="[434,511,1634,1660]" pageId="2" pageNumber="17" value="40.96">40.96</number>
°N
</geoCoordinate>
, 
<geoCoordinate id="EE1492F8FFC5FF9EFD84F984FD40994A" box="[558,666,1634,1660]" direction="east" orientation="longitude" pageId="2" pageNumber="17" precision="555" value="64.35">
<number id="3DE8E618FFC5FF9EFD84F984FDA1994A" box="[558,635,1634,1660]" pageId="2" pageNumber="17" value="64.35">64.35</number>
°E
</geoCoordinate>
]. We thus restrict the 
<typeStatus id="549B4A9DFFC5FF9EFC01F984FC3A994A" box="[939,992,1634,1660]" pageId="2" pageNumber="17">type</typeStatus>
