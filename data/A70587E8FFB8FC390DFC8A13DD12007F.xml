<taxonomicName id="E8AC4D7DFFB9FC390D678E6CDFD101FE" authorityName="Desmarest" authorityYear="1817" class="Malacostraca" family="Eryonidae" genus="Eryon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="868" phylum="Arthropoda" rank="species" species="cuvieri">
