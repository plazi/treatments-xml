<taxonomicName id="253277F0105815BE60C2F530DBBE61C1" genus="P." lsidName="P. areolaris" pageId="0" pageNumber="1" rank="species" species="areolaris">
<emphasis id="65E88CF373A860F3AF7ACE3966297880" italics="true" pageId="0" pageNumber="1">P. areolaris</emphasis>
</taxonomicName>
