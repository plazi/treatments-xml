<quantity id="4CA49B2C907CFFC33848FA49BA8CC6D5" box="[258,415,1435,1460]" metricMagnitude="-3" metricUnit="m" metricValue="7.904999999999999" metricValueMax="8.35" metricValueMin="7.46" pageId="6" pageNumber="299" unit="mm" value="7.904999999999999" valueMax="8.35" valueMin="7.46">7.46–8.35 mm</quantity>
