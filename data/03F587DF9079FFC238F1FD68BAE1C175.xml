<quantity id="4CA49B2C907CFFC339DDFAA9BBE7C6F2" box="[151,244,1403,1427]" metricMagnitude="-3" metricUnit="m" metricValue="1.76" pageId="6" pageNumber="299" unit="mm" value="1.76">1.76 mm</quantity>
