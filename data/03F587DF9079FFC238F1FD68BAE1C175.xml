<quantity id="4CA49B2C907CFFC339D5FA89BA2FC615" box="[159,316,1371,1396]" metricMagnitude="-3" metricUnit="m" metricValue="1.49" metricValueMax="1.55" metricValueMin="1.43" pageId="6" pageNumber="299" unit="mm" value="1.49" valueMax="1.55" valueMin="1.43">1.43–1.55 mm</quantity>
