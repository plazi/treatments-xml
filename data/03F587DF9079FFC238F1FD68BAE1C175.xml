<quantity id="4CA49B2C907CFFC338A6FA89B999C615" box="[492,650,1371,1396]" metricMagnitude="-3" metricUnit="m" metricValue="1.62" metricValueMax="1.7" metricValueMin="1.54" pageId="6" pageNumber="299" unit="mm" value="1.62" valueMax="1.7" valueMin="1.54">1.54–1.70 mm</quantity>
