<typeStatus id="7E2F887AFFEDFFFF38D8FA84AE379805" box="[197,258,1293,1319]" pageId="6" pageNumber="7">Type</typeStatus>
material. 
<typeStatus id="7E2F887AFFEDFFFF3994FA84AEC89805" box="[393,509,1293,1319]" pageId="6" pageNumber="7">Holotype</typeStatus>
.
</emphasis>
ɗ (
<collectionCode id="C785AE1DFFEDFFFF3A2EFA84AD979805" box="[563,674,1293,1319]" pageId="6" pageNumber="7">NWAFU</collectionCode>
), 
<collectingCountry id="D9837648FFEDFFFF3AA5FA84AC2C9805" box="[696,793,1293,1319]" name="China" pageId="6" pageNumber="7">CHINA</collectingCountry>
: Shasnxi Prov., Feng Co., Tiantaishan, 
<quantity id="666C9B3DFFEDFFFF3CE5FA84AA799805" box="[1272,1356,1293,1319]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="6" pageNumber="7" unit="m" value="2000.0">2000m</quantity>
, 
<date id="D52A1018FFEDFFFF3D46FA84AFE1986D" pageId="6" pageNumber="7" value="1998-06-09">
<number id="175C24FFFFEDFFFF3D46FA84AA5C9805" box="[1371,1385,1293,1319]" pageId="6" pageNumber="7" value="9.0">9</number>
-VI- 
<number id="175C24FFFFEDFFFF388AFABCAFE1986D" box="[151,212,1333,1359]" pageId="6" pageNumber="7" value="1998.0">1998</number>
