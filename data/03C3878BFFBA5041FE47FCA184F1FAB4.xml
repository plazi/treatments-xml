<taxonomicName id="4C6A4D1EFFBA5041FFC9FC6981ACFC01" authority="Forster-Cooper, 1915" authorityName="Forster-Cooper" authorityYear="1915" box="[108,489,902,929]" class="Mammalia" family="Hyracodontidae" genus="Aprotodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">
