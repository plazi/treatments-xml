<taxonomicName id="4C334D50FFB65B664F60CC5FCB82251F" authorityName="Cockerell" authorityYear="1916" box="[317,446,1596,1619]" class="Insecta" family="Halictidae" genus="Halictus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="723" phylum="Arthropoda" rank="species" species="tannaensis">
