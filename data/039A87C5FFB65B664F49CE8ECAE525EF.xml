<quantity id="4CCB9B36FFB65B664FD6CC42CBF92575" box="[395,453,1569,1593]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="4" pageNumber="723" unit="mm" value="7.0">7 mm</quantity>
