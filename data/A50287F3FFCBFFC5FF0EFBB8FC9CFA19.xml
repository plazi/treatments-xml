<emphasis id="1FDFEAF7FFCBFFC5FF0EFBB8FF22FBF1" bold="true" box="[151,199,1086,1111]" pageId="38" pageNumber="39">145.</emphasis>
<taxonomicName id="EAAB4D66FFCBFFC5FF56FBB8FD6AFBF1" authority="Linnaeus, 1764" authorityName="Linnaeus" authorityYear="1764" box="[207,655,1086,1111]" class="Insecta" family="Nymphalidae" genus="Hypolimnas" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="misippus">
<emphasis id="1FDFEAF7FFCBFFC5FF56FBB8FE2DFBF1" bold="true" box="[207,456,1086,1111]" italics="true" pageId="38" pageNumber="39">Hypolimnas misippus</emphasis>
(
<bibRefCitation id="493A4B14FFCBFFC5FE4EFBB8FD63FBF1" author="Linnaeus" box="[471,646,1086,1111]" pageId="38" pageNumber="39" refString="Linnaeus, C. (1764) Museum Ludovicae Ulricae Reginae. Salvius, Holmiae, [viii] + 720 pp." type="book" year="1764">Linnaeus, 1764</bibRefCitation>
)
</taxonomicName>
