<geoCoordinate id="EE3A5073F202F164EC33130AFC38580F" box="[873,953,1382,1404]" degrees="39.50" direction="south" orientation="latitude" pageId="54" pageNumber="54" precision="555" value="-39.5">39.50S</geoCoordinate>
