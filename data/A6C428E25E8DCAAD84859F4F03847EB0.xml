<taxonomicName id="E67650C991B738C95C1681CD8CE63976" class="Arachnida" family="Protoplophoridae" genus="Arthrhoplophora" kingdom="Animalia" order="Sarcoptiformes" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">Arthrhoplophora</taxonomicName>
