<heading id="D08E81E5A71AFFE9FF275056FD1C2B38" bold="true" box="[151,713,1621,1647]" fontSize="11" level="1" pageId="13" pageNumber="38" reason="1">
<emphasis id="B90DEA9BA71AFFE9FF275056FD1C2B38" bold="true" box="[151,713,1621,1647]" pageId="13" pageNumber="38">
<taxonomicName id="4C794D0AA71AFFE9FF275056FDF92B38" authority="Evenhuis" authorityName="Evenhuis" box="[151,556,1621,1647]" class="Insecta" family="Dolichopodidae" genus="Campsicnemus" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="38" phylum="Arthropoda" rank="species" species="plautus" status="new species">
<emphasis id="B90DEA9BA71AFFE9FF275056FE652B38" bold="true" box="[151,432,1621,1647]" italics="true" pageId="13" pageNumber="38">Campsicnemus plautus</emphasis>
Evenhuis
</taxonomicName>
, 
<taxonomicNameLabel id="A23E57E0A71AFFE9FD895056FD1C2B38" box="[569,713,1621,1647]" pageId="13" pageNumber="38" rank="species">new species</taxonomicNameLabel>
</emphasis>
