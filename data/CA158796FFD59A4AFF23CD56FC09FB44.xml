<specimenCount id="54BAFD09FFDA9A4AFE06CA37FDAEFC94" box="[485,521,811,838]" pageId="8" pageNumber="7" type="male">1♂</specimenCount>
(
<collectionCode id="24ADAE45FFDA9A4AFDF4CA37FDCAFC97" box="[535,621,811,837]" pageId="8" pageNumber="7">MGCL</collectionCode>
