<paragraph id="8B8AA758FFE1FFEDDEDEFB63FB167C97" blockId="96.[748,1164,1652,1675]" box="[748,1164,1652,1675]" pageId="96" pageNumber="97">
<heading id="D0C21034FFE1FFEDDEDEFB63FB167C97" box="[748,1164,1652,1675]" centered="true" fontSize="9" level="2" pageId="96" pageNumber="97" reason="2">
<taxonomicName id="4C35DCDBFFE1FFEDDEDEFB63FC6F7C96" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[748,1013,1652,1675]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="ornatus" status="new species">
<emphasis id="B9417B4AFFE1FFEDDEDEFB63FC6F7C96" bold="true" box="[748,1013,1652,1675]" italics="true" pageId="96" pageNumber="97">Paramblynotus ornatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFE1FFEDD837FB62FB167C97" box="[1029,1164,1653,1675]" pageId="96" pageNumber="97" rank="species">new species</taxonomicNameLabel>
