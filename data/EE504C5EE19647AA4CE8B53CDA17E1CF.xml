<taxonomicName id="8BCC09E56411523ED6DED9B2A2ED7BD6" class="Insecta" family="Acrididae" genus="Acteana" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acteana brachyptera" order="Orthoptera" pageId="8" pageNumber="45" phylum="Arthropoda" rank="species" species="brachyptera">Acteana brachyptera</taxonomicName>
