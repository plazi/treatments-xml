<quantity id="4C8F9B32FFE7F96F6E4FF8DA1DCDA716" box="[1345,1436,1878,1902]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="12" pageNumber="13" unit="mm" value="0.1">0.1 mm</quantity>
(distance between successive apices 
<quantity id="4C8F9B32FFE7F96F693FF8F61AF5A7EA" box="[561,676,1914,1938]" metricMagnitude="-5" metricUnit="m" metricValue="2.7" pageId="12" pageNumber="13" unit="mm" value="0.027">0.027 mm</quantity>
) in 
<taxonomicName id="4C774D54FFE7F96F69DEF8F71B6EA7EB" box="[720,831,1915,1939]" class="Arachnida" family="Chthoniidae" genus="Chthonius" kingdom="Animalia" order="Pseudoscorpiones" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="elymus">
<emphasis id="B903EAC5FFE7F96F69DEF8F71B6EA7EB" box="[720,831,1915,1939]" italics="true" pageId="12" pageNumber="13">C. elymus</emphasis>
</taxonomicName>
.
