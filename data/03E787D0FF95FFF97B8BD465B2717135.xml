<quantity id="4CB69B23FF92FFF97E75D24DB78D7135" metricMagnitude="-3" metricUnit="m" metricValue="2.8499999999999996" metricValueMax="3.3" metricValueMin="2.4" pageId="20" pageNumber="21" unit="mm" value="2.8499999999999996" valueMax="3.3" valueMin="2.4">2.4– 3.3 mm</quantity>
