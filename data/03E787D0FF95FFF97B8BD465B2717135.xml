<quantity id="4CB69B23FF92FFF979F8D26EB45E7135" box="[740,828,295,319]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="20" pageNumber="21" unit="mm" value="1.6">1.6 mm</quantity>
