<quantity id="4CB69B23FF92FFF97AE1D26EB5017135" box="[509,611,295,319]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="20" pageNumber="21" unit="mm" value="1.05">1.05 mm</quantity>
