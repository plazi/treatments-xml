<taxonomicName id="4C4E4D45FF92FFF97E2ED3A9B57C7116" authority="(C. Agardh) Endlicher. The" authorityName="Endlicher. The" baseAuthorityName="C. Agardh" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" kingdom="Plantae" order="Bryopsidales" pageId="20" pageNumber="21" phylum="Chlorophyta" rank="species" species="browni">
