<quantity id="4CB69B23FF92FFF97FAAD26EB26D7135" box="[1206,1295,295,319]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="20" pageNumber="21" unit="mm" value="1.9">1.9 mm</quantity>
