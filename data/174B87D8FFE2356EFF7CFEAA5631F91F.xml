(
<collectionCode id="F9F3AE0BFFE2356EFDB6FCBD50DCFCF7" LSID="urn:lsid:biocol.org:col:34070" box="[605,685,871,895]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="11" pageNumber="312">NZAC</collectionCode>
) and 
<number id="292A24E9FFE2356EFD06FCBC508AFCF7" box="[749,763,870,895]" pageId="11" pageNumber="312" value="5.0">5</number>
<typeStatus id="4059886CFFE2356EFCE8FCBC51D4FCF7" box="[771,933,870,895]" pageId="11" pageNumber="312">paralectotypes</typeStatus>
(
<collectionCode id="F9F3AE0BFFE2356EFC5EFCBD567CFCF7" box="[949,1037,871,895]" pageId="11" pageNumber="312">BMNH</collectionCode>
, 
