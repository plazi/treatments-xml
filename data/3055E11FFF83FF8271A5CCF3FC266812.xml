<paragraph id="B8435009FF83FF8271A5CD07FDFF6A21" blockId="19.[151,1436,355,847]" box="[151,577,355,380]" pageId="19" pageNumber="20">
<emphasis id="8A888C1BFF83FF8271A5CD07FF426A21" bold="true" box="[151,252,355,380]" pageId="19" pageNumber="20">Stations.</emphasis>
TA 
<number id="0E34422EFF83FF82701CCD00FEF46A21" box="[302,330,356,380]" pageId="19" pageNumber="20" value="23.0">23</number>
(
<collectionCode id="DEEDC8CCFF83FF82706BCD00FE0B6A21" LSID="urn:lsid:biocol.org:col:34988" box="[345,437,356,380]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="19" pageNumber="20">MNHN</collectionCode>
