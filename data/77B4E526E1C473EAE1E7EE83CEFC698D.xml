<paragraph id="0594F366060AB2AAD1EDB64F39CD6DC6" pageId="6" pageNumber="41">
<taxonomicName id="517446A9518FABFC75D6AC3E99DC47E2" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agrilus biakanus" order="Coleoptera" pageId="6" pageNumber="41" phylum="Arthropoda" rank="species" species="biakanus">Agrilus biakanus</taxonomicName>
