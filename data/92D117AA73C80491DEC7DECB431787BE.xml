<taxonomicName id="7E26E983BEF7FA6067AEDC4031852D8B" genus="Sternorrhyncha" lsidName="Sternorrhyncha" pageId="0" pageNumber="5216" rank="suborder" suborder="Sternorrhyncha">Sternorrhyncha</taxonomicName>
