<paragraph id="5D3A1F66356AFFB86CFE7EBF0BDCCDC0" blockId="1.[151,1437,1763,1941]" pageId="1" pageNumber="114">
<emphasis id="6FF1C374356AFFB86CFE7EBF081ECCAD" bold="true" box="[151,277,1763,1786]" pageId="1" pageNumber="114">
FIGURE 
<number id="EB4D0D41356AFFB86D6B7EBF0804CCAD" box="[258,271,1763,1784]" pageId="1" pageNumber="114" value="1.0">1</number>
