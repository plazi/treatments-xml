<taxonomicName id="4C10EAEDFFECDD7254EC747DE5577577" box="[402,634,1842,1864]" class="Agaricomycetes" family="Omphalotaceae" genus="Neonothopanus" kingdom="Fungi" order="Agaricales" pageId="4" pageNumber="61" phylum="Basidiomycota" rank="species" species="undetermined">
<emphasis id="B9644D7CFFECDD7254EC747DE5047578" box="[402,553,1842,1863]" italics="true" pageId="4" pageNumber="61">Neonothopanus</emphasis>
