<taxonomicName id="4C10EAEDFFECDD7256BF747CE3747578" box="[961,1113,1842,1864]" class="Agaricomycetes" family="Omphalotaceae" genus="Neonothopanus" kingdom="Fungi" order="Agaricales" pageId="4" pageNumber="61" phylum="Basidiomycota" rank="species" species="hygrophanus">
<emphasis id="B9644D7CFFECDD7256BF747CE3747578" box="[961,1113,1842,1864]" italics="true" pageId="4" pageNumber="61">N. hygrophanus</emphasis>
