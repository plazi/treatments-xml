<emphasis id="B9644D7CFFECDD72565C747DE46A7578" bold="true" box="[802,839,1842,1863]" pageId="4" pageNumber="61">b, d</emphasis>
<taxonomicName id="4C10EAEDFFECDD725633747CE4887578" authorityName="R. H. Petersen &amp; Krisai" authorityYear="1999" baseAuthorityName="Speg." box="[845,933,1842,1864]" class="Agaricomycetes" family="Omphalotaceae" genus="Neonothopanus" kingdom="Fungi" order="Agaricales" pageId="4" pageNumber="61" phylum="Basidiomycota" rank="species" species="nambi">
