<taxonomicName id="4C69B6D9FFA08A04FCDCA3301F90FD59" authority="Svihla, 2004" authorityName="Svihla" authorityYear="2004" box="[866,1237,620,644]" class="Insecta" family="Cantharidae" genus="Lycocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="jelineki">
