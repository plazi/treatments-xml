<paragraph id="F12D36EFFFCDFFF838FCFF4AFE3AFF74" blockId="18.[178,437,216,238]" box="[178,437,216,238]" pageId="18" pageNumber="851">
<heading id="AA658183FFCDFFF838FCFF4AFE3AFF74" box="[178,437,216,238]" fontSize="8" level="2" pageId="18" pageNumber="851" reason="2">
<emphasis id="C3E6EAFDFFCDFFF838FCFF4AFE3AFF74" bold="true" box="[178,437,216,238]" pageId="18" pageNumber="851">
<taxonomicName id="36924D6CFFCDFFF838FCFF4AFEEDFF74" authority="Malicky, 2008" authorityName="Malicky" authorityYear="2008" box="[178,354,217,238]" class="Insecta" family="Ecnomidae" genus="Ecnomus" kingdom="Animalia" order="Trichoptera" pageId="18" pageNumber="851" phylum="Arthropoda" rank="species" species="perseis" status="nov.sp.">
<emphasis id="C3E6EAFDFFCDFFF838FCFF4AFEEDFF74" bold="true" box="[178,354,217,238]" italics="true" pageId="18" pageNumber="851">Ecnomus perseis</emphasis>
