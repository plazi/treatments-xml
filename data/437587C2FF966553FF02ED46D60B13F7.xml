<materialsCitation id="7BB43C89FF966553FEDBEDFBD7DD1187" ID-GBIF-Occurrence="2451575340" collectionCode="NHMUK" pageId="82" pageNumber="83" specimenCode="NHMUK 20160223" specimenCount="1" typeStatus="syntype">
