<specimenCode id="9B7A9EAFFF966553FE4AEDFBD6EA11DB" box="[479,704,1443,1470]" collectionCode="NHMUK" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="82" pageNumber="83" type="Museum">NHMUK 20160223</specimenCode>
