<quantity id="4CDBA1B6236EFFE6FD25FADF21A6FAAB" box="[767,912,1364,1390]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" metricValueMax="9.0" metricValueMin="7.0" pageId="3" pageNumber="32" unit="mm" value="0.8" valueMax="0.9" valueMin="0.7">0.7–0.9 mm</quantity>
