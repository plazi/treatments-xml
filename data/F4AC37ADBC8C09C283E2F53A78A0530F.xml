<paragraph id="EA947867806295221D958EFD3E7EF0B4" pageId="0" pageNumber="350" type="mainText">
<pageBreakToken id="2602346E851903265FC5BF543B75FE83" pageId="0" pageNumber="350" start="start">
<taxonomicName id="9FB1B00F3FBEAC3CF53891A49491115B" class="Insecta" family="Formicidae" genus="Aphaenogaster" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="350" phylum="Arthropoda" rank="subGenus" subGenus="Deromyrma">Deromyrma</taxonomicName>
