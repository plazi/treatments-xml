<collectionCode id="ED0AE78AFFAFFFC6FF10FF7F24C3CE18" box="[138,233,232,256]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="39" pageNumber="2039">MNHN</collectionCode>
