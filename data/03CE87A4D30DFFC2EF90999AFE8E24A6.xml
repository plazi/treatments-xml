<taxonomicName id="4C674D31D30DFFC2ECAA9B91FAFA25CD" authority="carolinensis (Dikmans)" authorityName="Dikmans" box="[941,1367,1717,1742]" class="Chromadorea" family="Heligmonellidae" genus="Carolinensis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="7" pageNumber="502" phylum="Nematoda" rank="species" species="carolinensis">
