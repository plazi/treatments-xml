<taxonomicName id="4C011396D22F0261FF4BF87F538134BD" authority="Weiss et al. 1981" authorityName="Weiss" authorityYear="1981" box="[226,643,1931,1955]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="32" pageNumber="2814" phylum="Firmicutes" rank="species" species="sharpeae">
