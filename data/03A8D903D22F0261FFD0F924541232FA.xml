<taxonomicName id="4C011396D22F0261FD35F8D453FA3426" box="[668,760,1824,1848]" class="Bacilli" family="Lactobacillaceae" genus="Lacticaseibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="32" pageNumber="2814" phylum="Firmicutes" rank="species" species="sharpeae">
