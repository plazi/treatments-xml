<quantity id="4C889B385E2EFFDA18A353DAFD45FB7D" box="[676,741,1217,1242]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="22" pageNumber="177" unit="mm" value="1.0">1 mm</quantity>
