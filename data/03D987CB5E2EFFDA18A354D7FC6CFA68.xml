<quantity id="4C889B385E2EFFDA199653FBFBB4FB5E" box="[913,1044,1248,1273]" metricMagnitude="-3" metricUnit="m" metricValue="5.05" metricValueMax="5.6" metricValueMin="4.5" pageId="22" pageNumber="177" unit="mm" value="5.05" valueMax="5.6" valueMin="4.5">4.5-5.6 mm</quantity>
