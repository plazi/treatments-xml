<quantity id="4C889B385E2EFFDA18E153E5FCE3FAB0" box="[742,835,1278,1303]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="22" pageNumber="177" unit="mm" value="2.5">2.5 mm</quantity>
