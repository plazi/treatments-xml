<taxonomicName id="08B74FEA156AE413FD8B6746FD68F2D0" authorityName="Kraepelin" authorityYear="1899" box="[543,656,1650,1675]" class="Arachnida" family="Daesiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="24" pageNumber="25" phylum="Arthropoda" rank="family">Daesiidae</taxonomicName>
