<collectorName id="243C4CD6AC533B34FDC8DEEA5E2BF962" box="[537,616,1711,1731]" pageId="31" pageNumber="424">Ziéla, U.</collectorName>
<collectionCode id="EFD8B1C5AC533B34FDB6DEEA5E39F962" box="[615,634,1711,1731]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="31" pageNumber="424" type="Museum">V</collectionCode>
., 
<date id="FD770FC0AC533B34FD56DEEA5E9DF962" box="[647,734,1711,1731]" pageId="31" pageNumber="424" value="1957-05-04">
