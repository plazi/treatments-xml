<materialsCitation id="39A1235DAC533B34FE18DEEA5E9DF962" box="[457,734,1711,1731]" collectingDate="1957-05-04" collectionCode="V" collectorName="Ziela, U." location="Nimba" pageId="31" pageNumber="424" specimenCount="1" stateProvince="Nimba">
