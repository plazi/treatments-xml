<taxonomicName id="8ECE4D24FFEAFFEAFE20F370FC8A2B52" authority="Ashmead, 1900" authorityName="Ashmead" authorityYear="1900" box="[510,847,1059,1082]" class="Insecta" family="Encyrtidae" genus="Psyllaephagus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="385" phylum="Arthropoda" rank="genus">
