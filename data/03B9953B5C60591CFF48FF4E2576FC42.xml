(
<bibRefCitation id="EF8159DC5C60591CFD7AFC982201FCF2" author="Kopelke" box="[677,844,833,858]" pageId="71" pageNumber="72" refString="Kopelke, J. - P. (1994) Die Arten der Pontania dolichura - Gruppe in Mittel- und Nordeuropa (Insecta: Hymenoptera: Tenthredinidae: Nematinae). Senckenbergiana Biologica, 74 (1 - 2), 127 - 145." type="journal article" year="1994">Kopelke 1994</bibRefCitation>
)
</taxonomicName>
, 
<emphasis id="B964F83F5C60591CFCBCFC9B254BFCF1" box="[867,1030,833,858]" italics="true" pageId="71" pageNumber="72">
<taxonomicName id="4C105FAE5C60591CFCBCFC9B22A5FCF2" baseAuthorityName="Zinovjev" baseAuthorityYear="1999" box="[867,1000,833,858]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="71" pageNumber="72" phylum="Chordata" rank="species" species="viminalis">S. viminalis</taxonomicName>
×
