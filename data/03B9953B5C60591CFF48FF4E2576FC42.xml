plants: 
<taxonomicName id="4C105FAE5C60591CFE36FC982219FCF2" authority="(Kopelke 1994)" authorityName="Kopelke" authorityYear="1994" baseAuthorityName="Kopelke" baseAuthorityYear="1994" box="[489,852,833,858]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="71" pageNumber="72" phylum="Chordata" rank="species" species="purpurea">
