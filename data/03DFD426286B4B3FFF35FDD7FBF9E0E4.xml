<emphasis id="B902B922286B4B3FFF35FDD7FDB2E526" bold="true" box="[151,613,511,538]" pageId="34" pageNumber="235">
Genus 
<taxonomicName id="4C761EB3286B4B3FFF4CFE28FE27E526" authority="Jaschhof" authorityName="Jaschhof" box="[238,496,511,538]" class="Insecta" family="Cecidomyiidae" genus="Mikrostyles" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="235" phylum="Arthropoda" rank="genus" status="gen. nov.">
