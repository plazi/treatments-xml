<collectionCode id="E88EA18CFF887242FB5A5493FB708581" box="[1209,1277,152,176]" country="Germany" httpUri="http://grbio.org/cool/4j3t-w40g" name="Museum of Zoology Senckenberg Dresden" pageId="57" pageNumber="58">MTD</collectionCode>
