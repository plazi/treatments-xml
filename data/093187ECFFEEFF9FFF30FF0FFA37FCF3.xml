<taxonomicName id="46984D79FFEEFF9FFC72FEEDFACBFE3B" authority="Triapitsyn" authorityName="Sankararaman &amp; Manickavasagam &amp; Triapitsyn &amp; Huber &amp; Kharbisnop" authorityYear="2020" box="[981,1376,373,395]" class="Insecta" family="Mymaridae" genus="Camptopteroides" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="245" phylum="Arthropoda" rank="species" species="reducta" status="sp. n." subGenus="Camptopteroides">
<emphasis id="B3ECEAE8FFEEFF9FFC72FEEDFB52FE3B" bold="true" box="[981,1273,373,395]" pageId="2" pageNumber="245">
<emphasis id="B3ECEAE8FFEEFF9FFC72FEEDFC40FE3A" bold="true" box="[981,1003,373,394]" italics="true" pageId="2" pageNumber="245">C.</emphasis>
(
