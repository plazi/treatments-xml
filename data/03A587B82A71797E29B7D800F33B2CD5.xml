<typeStatus id="54B7880C2A6F797E2BDEDFE8F5742CD4" box="[766,864,1196,1221]" pageId="48" pageNumber="49">holotype</typeStatus>
collected in 
<collectingRegion id="49C8F84C2A6F797E2AD0DFE9F2D02CD4" box="[1008,1220,1196,1221]" country="Panama" name="Veraguas" pageId="48" pageNumber="49">Veraguas Province</collectingRegion>
, 
