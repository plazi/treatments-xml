<taxonomicName id="3EAE92ACFFF8FF98B6A0FCB44DC7FCAB" box="[993,1209,837,863]" class="Magnoliopsida" family="Plantaginaceae" genus="Kickxia" kingdom="Plantae" order="Lamiales" pageId="11" pageNumber="329" phylum="Tracheophyta" rank="species" species="aegyptiaca">
