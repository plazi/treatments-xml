, HW 
<quantity id="4CD4E9ECC7751815FB664ECDFAEEF968" box="[1232,1394,1677,1702]" metricMagnitude="-4" metricUnit="m" metricValue="6.05" metricValueMax="6.2" metricValueMin="5.8999999999999995" pageId="15" pageNumber="512" unit="mm" value="0.605" valueMax="0.62" valueMin="0.59">0.59–0.62 mm</quantity>
