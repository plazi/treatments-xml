, EW 
<quantity id="4CD4E9ECC7751815FC914EF1FC55F904" box="[807,969,1713,1738]" metricMagnitude="-3" metricUnit="m" metricValue="1.04" metricValueMax="1.05" metricValueMin="1.03" pageId="15" pageNumber="512" unit="mm" value="1.04" valueMax="1.05" valueMin="1.03">1.03–1.05 mm</quantity>
