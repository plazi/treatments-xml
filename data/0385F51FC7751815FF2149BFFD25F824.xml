<collectingCountry id="F33B0499C7751815FED14E95FE32F923" box="[359,430,1749,1774]" name="China" pageId="15" pageNumber="512">China</collectingCountry>
: 
<collectingRegion id="49E88AEBC7751815FE0F4E96FD98F923" box="[441,516,1750,1774]" country="China" name="Gansu" pageId="15" pageNumber="512">Gansu</collectingRegion>
