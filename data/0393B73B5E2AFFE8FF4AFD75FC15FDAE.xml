<paragraph id="8B85062D5E2AFFE9FF4AFD75FDCDFD39" blockId="4.[151,536,655,715]" box="[151,536,655,682]" pageId="4" pageNumber="296">
<heading id="D0CDB1415E2AFFE9FF4AFD75FDCDFD39" bold="true" box="[151,536,655,682]" fontSize="11" level="1" pageId="4" pageNumber="296" reason="1">
<emphasis id="B94EDA3F5E2AFFE9FF4AFD75FDCDFD39" bold="true" box="[151,536,655,682]" pageId="4" pageNumber="296">
<taxonomicName id="4C3A7DAE5E2AFFE9FF4AFD75FE66FD3A" authority="Nascimento &amp; Heffern, 2018" authorityName="Nascimento &amp; Heffern" authorityYear="2018" box="[151,435,655,681]" class="Insecta" family="Cerambycidae" genus="Nyssodrysina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="296" phylum="Arthropoda" rank="species" species="maculosa" status="sp. nov.">
<emphasis id="B94EDA3F5E2AFFE9FF4AFD75FE66FD3A" bold="true" box="[151,435,655,681]" italics="true" pageId="4" pageNumber="296">Nyssodrysina maculosa</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27D67445E2AFFE9FE67FD6AFDCDFD39" box="[442,536,656,682]" pageId="4" pageNumber="296" rank="species">sp. nov.</taxonomicNameLabel>
