</taxonomicName>
, 
<taxonomicName id="453480456E45C481253838953CDD12B2" genus="G." lsidName="G. kamakusensis" pageId="2" pageNumber="27" rank="species" species="kamakusensis">
<emphasis id="458A6E562CF109E4DBFF6DC5AD5ABC04" italics="true" pageId="2" pageNumber="27">G. kamakusensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="87FBDB123F8E0FDC58106579809F02C8" genus="G." lsidName="G. pseudoferruginea" pageId="2" pageNumber="27" rank="species" species="pseudoferruginea">
<emphasis id="BF2043D6F6228F0F25B6D5FA3BC55B86" italics="true" pageId="2" pageNumber="27">G. pseudoferruginea</emphasis>
