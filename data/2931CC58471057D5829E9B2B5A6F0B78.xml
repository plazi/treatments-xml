<taxonomicName id="7BA00221B555BFFA055D5888EF3B3FD7" authorityName="Maas &amp; Westra" authorityYear="2019" class="Magnoliopsida" family="Annonaceae" genus="Guatteria" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Guatteria attenuata" order="Magnoliales" pageId="2" pageNumber="27" phylum="Tracheophyta" rank="species" species="attenuata">
