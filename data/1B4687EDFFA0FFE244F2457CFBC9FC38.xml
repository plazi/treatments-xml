<taxonomicName id="54EF4D78FFA3FFE24429429BFE25FD49" authorityName="Schmitz" authorityYear="1929" box="[261,444,706,727]" class="Insecta" family="Phoridae" genus="Megaselia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="1618" phylum="Arthropoda" rank="species" species="speiseri">
