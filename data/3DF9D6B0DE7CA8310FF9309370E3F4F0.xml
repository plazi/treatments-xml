<taxonomicName id="E76954F3AE90C71646D7BD108AC07752" authority="Weber, 1801" authorityName="Weber" authorityYear="1801" class="Insecta" family="Endomychidae" genus="Eumorphus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eumorphus" order="Coleoptera" pageId="1" pageNumber="140" phylum="Arthropoda" rank="genus">Eumorphus Weber, 1801</taxonomicName>
