<taxonomicName id="4C7C4D67FFCB4C00F53C39F7F3F9F34E" authorityName="Dobson" authorityYear="1872" box="[2713,3073,3200,3239]" class="Mammalia" family="Emballonuridae" genus="Taphozous" kingdom="Animalia" order="Chiroptera" pageId="4" pageNumber="353" phylum="Chordata" rank="species" species="theobaldi">
