<materialsCitation id="F9BA3CB43C35123DC12BF9F0FBE4F3E0" ID-GBIF-Occurrence="2556252008" collectionCode="RBINS" collectorName="Coll. R. I. Sc. N. B. Bras. Corumba &amp; Coll. A. Knisch &amp; Ex Coll. A. d'Orchymont &amp; P. D. Perkins." determinerName="Kniz &amp; J. L. Hellman" location="Hydrochus secretus Knisch" longitude="9.13" municipality="The" pageId="11" pageNumber="160" specimenCount="2030" specimenCount-female="2" specimenCount-male="5" specimenCount-pupa="2019" specimenCount-worker="1" typeStatus="syntype">
