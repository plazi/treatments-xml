<emphasis id="AB9D286A3625FFFE9CF288FC963C6E8C" bold="true" box="[199,308,151,176]" pageId="23" pageNumber="374">Remarks</emphasis>
. 
<taxonomicName id="5EE98FFB3625FFFE9D7288F3955E6E8C" box="[327,598,152,176]" class="Insecta" family="Hybotidae" genus="Platypalpus" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="374" phylum="Arthropoda" rank="species" species="farabiensis">
