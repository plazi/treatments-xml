<quantity id="4C9638B0FFF0FFB61DD963032DA4BD58" box="[681,767,292,316]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="12" pageNumber="48" unit="m" value="1100.0">1100 m</quantity>
