<taxonomicName id="084ADF46AC7699F3DE5EDC8FF0A33969" authority="L." class="Magnoliopsida" family="Cucurbitaceae" genus="Cucumis" kingdom="Plantae" order="Cucurbitales" pageId="null" pageNumber="362" phylum="Tracheophyta" rank="species" species="sativus">
Cucumis 
<normalizedToken id="967822C111CFC06D50B00582022E7668" originalValue="satívus" pageId="null" pageNumber="362">sativus</normalizedToken>
<authorityName id="F7D6A9FAE2ACB2D83167E731824254D6" pageId="null" pageNumber="362">L.</authorityName>
