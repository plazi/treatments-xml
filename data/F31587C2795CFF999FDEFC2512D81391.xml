<bibRefCitation id="1F2D4B25795EFF999B4FFEF1159F102B" author="Vaney" box="[1030,1185,306,332]" pageId="7" pageNumber="16" refString="Vaney, C. (1908) Les Holothuries de l'Expedition Antarctique Nationale Ecossaise. Transactions of the Royal Society of Edinburgh, 46, 405 - 441, 5 pls." type="journal article" year="1908">
Vaney, 
<number id="CD7424F3795EFF999B28FEF1159F102B" box="[1121,1185,306,332]" pageId="7" pageNumber="8" value="1908.0">1908</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="BCBC4D57795EFF999BE7FEF1107F1014" authority="Ekman, 1927" authorityName="Ekman" authorityYear="1927" class="Holothuroidea" family="Psolidae" genus="Psolidium" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="navicula">
