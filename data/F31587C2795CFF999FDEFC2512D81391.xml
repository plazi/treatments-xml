<number id="CD7424F3795EFF999E4BFE9A107F1014" box="[258,321,345,371]" pageId="7" pageNumber="8" value="1927.0">1927</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="BCBC4D57795EFF999EC8FE9A12EB1014" authority="Ludwig &amp; Heding, 1935" authorityName="Ludwig &amp; Heding" authorityYear="1935" box="[385,981,345,371]" class="Holothuroidea" family="Psolidae" genus="Psolidium" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="bistriatum">
<emphasis id="49C8EAC6795EFF999EC8FE9A13AF1015" box="[385,657,345,370]" italics="true" pageId="7" pageNumber="8">Psolidium bistriatum</emphasis>
<bibRefCitation id="1F2D4B25795EFF999DD3FE9A12EB1014" author="Ludwig" box="[666,981,345,371]" pageId="7" pageNumber="15" refString="Ludwig, H. &amp; Heding, S. G. (1935) Die holothurien der Deutschen Tiefsee-Expedition. 1. Fusslose und dendrochirote formen. Wissenschaftliche Ergeb-nisse der Deutschen Tiefsee-Expedition auf dem dampfer Valdivia 1898 - 1899, 24, 123 - 214." type="journal article" year="1935">
Ludwig &amp; Heding, 
<number id="CD7424F3795EFF999CDFFE9A12EB1014" box="[918,981,345,371]" pageId="7" pageNumber="8" value="1935.0">1935</number>
</bibRefCitation>
</taxonomicName>
