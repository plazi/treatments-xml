; lengths of antennal segments I −IV 
<number id="D03424962B0C183806F4FEBACEA6FEBD" box="[971,1023,332,356]" pageId="10" pageNumber="203" value="0.18">0.18</number>
, 
<number id="D03424962B0C18380134FEBAC966FEBD" box="[1035,1087,332,356]" pageId="10" pageNumber="203" value="0.74">0.74</number>
