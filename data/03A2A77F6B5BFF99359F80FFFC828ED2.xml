<collectingRegion id="49CFD88B6B5AFF9935C48415FDEA8E32" box="[444,563,349,373]" country="Japan" name="Hokkaido" pageId="11" pageNumber="936">Hokkaido</collectingRegion>
, 
<collectingCountry id="F31C56F96B5AFF9936478415FD5F8E32" box="[575,646,349,373]" name="Japan" pageId="11" pageNumber="936">Japan</collectingCountry>
. (
<bibRefCitation id="EF9A6B986B5AFF9936E28415FCA18E32" author="Bernard FR &amp; Cai YY &amp; Morton B" box="[666,888,349,373]" pageId="11" pageNumber="936" refId="ref6881" refString="Bernard FR, Cai YY, Morton B. 1993. Catalogue of the living marine bivalve molluscs of China. Hong Kong: Hong Kong University Press." type="book" year="1993">Bernard et al. 1993</bibRefCitation>
; 
<bibRefCitation id="EF9A6B986B5AFF9937FC8415FC318E32" author="Xu F" box="[900,1000,349,373]" pageId="11" pageNumber="936" refId="ref10140" refString="Xu F. 1997. Bivalve Mollusca of China Seas. Beijing: Science Press." type="book" year="1997">Xu 1997</bibRefCitation>
