<taxonomicName id="4C734D5A0119DA79FC5BFC25385929D5" authorityName="Kellogg" authorityYear="1899" box="[971,1071,905,930]" class="Insecta" family="Ricinidae" genus="Physostomum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="invadens">invadens</taxonomicName>
