<taxonomicName id="4C734D5A0119DA79FF33FC013E6729B1" box="[163,529,941,966]" class="Aves" family="Troglodytidae" genus="Heleodytes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="18" pageNumber="19" phylum="Chordata" rank="subSpecies" species="rufinucha" subSpecies="chiapensis">
