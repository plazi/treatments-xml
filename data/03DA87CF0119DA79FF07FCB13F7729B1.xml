<taxonomicName id="4C734D5A0119DA79FE07FC263E5529D5" authorityName="Kellogg" authorityYear="1899" box="[407,547,905,930]" class="Insecta" family="Ricinidae" genus="Physostomum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="invadens">
