</emphasis>
<taxonomicName id="4C584D42FF9B846DFEADFDCAFD16BB98" authority="Blanchard, 1852" authorityName="Blanchard" authorityYear="1852" box="[299,719,618,640]" class="Insecta" family="Nymphalidae" genus="Argyrophorus" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="argenteus">
