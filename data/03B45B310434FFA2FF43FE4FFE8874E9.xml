<quantity id="4CE547C20434FFA4FD7FFAD5FE457063" box="[683,772,1325,1349]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="19" pageNumber="20" unit="mm" value="3.2">3.2 mm</quantity>
