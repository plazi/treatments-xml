<paragraph id="E635CACE9D73100BCD8455334DFB2985" pageId="6" pageNumber="15">
<taxonomicName id="0867D2F231E4646FB63BDE5CB6E731B6" class="Insecta" family="Hydraenidae" genus="Hydraena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydraena scabra" order="Coleoptera" pageId="6" pageNumber="15" phylum="Arthropoda" rank="species" species="scabra">Hydraena scabra</taxonomicName>
