<taxonomicName id="AEC44D507E7CFFE987B3F891FC77F8C9" baseAuthorityName="Schmutz" baseAuthorityYear="1909" box="[654,987,1873,1897]" class="Insecta" family="Phlaeothripidae" genus="Docessissophothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="444" phylum="Arthropoda" rank="species" species="brasiliensis">
