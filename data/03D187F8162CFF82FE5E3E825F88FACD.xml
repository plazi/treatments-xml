<quantity id="4C809B0B162CFF82FEE3393F5F53FBA8" box="[262,317,1106,1125]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="8" pageNumber="267" unit="m" value="600.0">600 m</quantity>
a.s.l., grassy steppe, rocks, 
