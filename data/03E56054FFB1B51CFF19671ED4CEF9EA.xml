<heading id="D0BB662EFFB1B51CFF19671ED4ECFAF9" bold="true" box="[151,641,1383,1408]" fontSize="10" level="2" pageId="49" pageNumber="50" reason="2">
<taxonomicName id="4C4CAAC1FFB1B51CFF19671ED4ECFAF9" authority="(Chen &amp; He, 1994)" baseAuthorityName="Chen &amp; He" baseAuthorityYear="1994" box="[151,641,1383,1408]" class="Insecta" family="Phasmatidae" genus="Ramulus" kingdom="Animalia" order="Phasmida" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="minutidentatus">
