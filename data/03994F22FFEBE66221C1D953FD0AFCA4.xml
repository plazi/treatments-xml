<taxonomicName id="4C3085B7FFEBE662240FD9EAFE94F883" authority="Cobb, 1920" authorityName="Cobb" authorityYear="1920" class="Adenophorea" family="Belondiridae" genus="Axonchium" kingdom="Animalia" order="Dorylaimida" pageId="34" pageNumber="35" phylum="Nematoda" rank="genus">
