<taxonomicName id="4C3085B7FFEBE6622058D814FD59F9AE" authority="Siddiqi, 1995" authorityName="Siddiqi" authorityYear="1995" box="[270,683,890,914]" class="Adenophorea" family="Belondiridae" genus="Axonchium" kingdom="Animalia" order="Dorylaimida" pageId="34" pageNumber="35" phylum="Nematoda" rank="species" species="perplexans">
