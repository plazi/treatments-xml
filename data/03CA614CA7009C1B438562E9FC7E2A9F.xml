<paragraph id="8BDCD05AA7009C1A438562E9FD7B2AC1" blockId="4.[151,649,474,499]" box="[151,649,474,499]" pageId="4" pageNumber="595">
<heading id="D0946736A7009C1A438562E9FD7B2AC1" bold="true" box="[151,649,474,499]" fontSize="10" level="2" pageId="4" pageNumber="595" reason="2">
<emphasis id="B9170C48A7009C1A438562E9FD7B2AC1" bold="true" box="[151,649,474,499]" pageId="4" pageNumber="595">
Two misplaced 
<taxonomicName id="4C63ABD9A7009C1A425D62E9FE3E2AC1" box="[335,460,474,499]" class="Insecta" family="Veliidae" genus="Microvelia" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="595" phylum="Arthropoda" rank="genus">
<emphasis id="B9170C48A7009C1A425D62E9FE3E2AC1" bold="true" box="[335,460,474,499]" italics="true" pageId="4" pageNumber="595">Microvelia</emphasis>
</taxonomicName>
from 
<collectingCountry id="F37490CAA7009C1A410662E9FD7B2AC1" box="[532,649,474,499]" name="Colombia" pageId="4" pageNumber="595">Colombia</collectingCountry>
</emphasis>
</heading>
</paragraph>
