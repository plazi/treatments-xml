<taxonomicName id="7414ECF8B9F4D6B94A4CF036437E6C94" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia ehrenbergiana" order="Fabales" pageId="40" pageNumber="41" phylum="Tracheophyta" rank="species" species="ehrenbergiana">
