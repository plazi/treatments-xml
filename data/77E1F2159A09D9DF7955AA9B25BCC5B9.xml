<taxonomicName id="4E6BCF0656FA14408564B9527BA47AA8" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="40" pageNumber="41" phylum="Tracheophyta" rank="species" species="gerrardii">
