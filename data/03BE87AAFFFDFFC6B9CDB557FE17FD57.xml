(
<taxonomicName id="4C174D3FFFFAFFC7B886B440FBA0FCFC" box="[918,1074,856,878]" class="Magnoliopsida" family="Rhamnaceae" kingdom="Plantae" order="Rosales" pageId="84" pageNumber="85" phylum="Tracheophyta" rank="family">Rhamnaceae</taxonomicName>
