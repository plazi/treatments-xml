<quantity id="4CC89B28FF9BC8563BA1FA47DD74FA38" box="[668,809,1410,1434]" metricMagnitude="-3" metricUnit="m" metricValue="6.95" metricValueMax="7.2" metricValueMin="6.7" pageId="8" pageNumber="534" unit="mm" value="6.95" valueMax="7.2" valueMin="6.7">6.7–7.2 mm</quantity>
