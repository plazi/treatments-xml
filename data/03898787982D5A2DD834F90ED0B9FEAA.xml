<emphasis id="B954EA83982E5A2CDB37F826D3ADF826" box="[342,440,1965,1990]" italics="true" pageId="50" pageNumber="51">Nycteris</emphasis>
<taxonomicName id="4C204D12982E5A2CDBA0F826D0EEF826" authority="(K. Andersen, 1912)" baseAuthorityName="K. Andersen" baseAuthorityYear="1912" box="[449,763,1964,1990]" class="Mammalia" family="Nycteridae" genus="Petalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="50" pageNumber="51" phylum="Chordata" rank="species" species="nana">
