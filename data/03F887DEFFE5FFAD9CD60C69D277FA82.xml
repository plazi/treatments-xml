</taxonomicName>
, 
<number id="3D9924EFFFE2FFAD9EB80BE5D408FB12" box="[761,817,1196,1220]" pageId="20" pageNumber="21" value="1923.0">1923</number>
as 
<taxonomicName id="4C514D4BFFE2FFAD9F1B0BE5D314FB12" authority="Haeckel, 1889" authorityName="Haeckel" authorityYear="1889" box="[858,1069,1196,1221]" class="Hydrozoa" family="Cytaeididae" genus="Stylactella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecata" pageId="20" pageNumber="21" phylum="Cnidaria" rank="species" species="inermis">
<emphasis id="B925EADAFFE2FFAD9F1B0BE5D314FB12" box="[858,1069,1196,1221]" italics="true" pageId="20" pageNumber="21">Stylactella inermis</emphasis>
