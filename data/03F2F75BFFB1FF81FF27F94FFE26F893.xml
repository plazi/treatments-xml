<collectingCountry id="F34C06DDFFAEFF81FED7F980FE78F98F" box="[359,427,1591,1616]" name="Brazil" pageId="16" pageNumber="143">Brazil</collectingCountry>
(
<collectingRegion id="499F88AFFFAEFF81FE0AF980FD8CF98F" box="[442,607,1591,1616]" country="Brazil" name="Rio de Janeiro" pageId="16" pageNumber="143">Rio de Janeiro</collectingRegion>
) (
<figureCitation id="13605AC8FFAEFF81FDC6F980FD18F98F" box="[630,715,1591,1616]" captionStart="FIGURE 76" captionStartId="20.[151,250,1345,1367]" captionTargetBox="[152,1435,695,1233]" captionTargetId="figure@20.[151,1435,694,1262]" captionTargetPageId="20" captionText="FIGURE 76. Distribution of Diplosynapsis halterata Enderlein, 1914, Diplosynapsis remus Tomasovic, 2002, Diplosynapsis spina sp. nov. and Diplosynapsis uncinata sp. nov." httpUri="https://zenodo.org/record/203838/files/figure.png" pageId="16" pageNumber="143">Fig. 76</figureCitation>
