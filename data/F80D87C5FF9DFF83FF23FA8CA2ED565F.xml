<taxonomicName id="B7A44D50FF9CFF81FDA6FB23A49E558C" authority="Parenti." authorityName="Parenti." box="[530,759,1221,1243]" genus="E." pageId="25" pageNumber="426" rank="species" species="bimaculata">
<emphasis id="42D0EAC1FF9CFF81FDA6FB23A441558C" box="[530,552,1222,1243]" italics="true" pageId="25" pageNumber="426">E.</emphasis>
<taxonomicName id="B7A44D50FF9CFF81FD9BFB20A49E558C" authority="Parenti." authorityName="Parenti." box="[559,759,1221,1243]" pageId="25" pageNumber="426" rank="species" species="bimaculata">
<taxonomicName id="B7A44D50FF9CFF81FD9BFB20A49A558C" authority="Parenti" authorityName="Parenti" box="[559,755,1221,1243]" pageId="25" pageNumber="426" rank="species" species="bimaculata">
<taxonomicName id="B7A44D50FF9CFF81FD9BFB20A4C8558C" box="[559,673,1221,1243]" pageId="25" pageNumber="426" rank="species" species="bimaculata">
<emphasis id="42D0EAC1FF9CFF81FD9BFB20A4C8558C" box="[559,673,1221,1243]" italics="true" pageId="25" pageNumber="426">bimaculata</emphasis>
