<treatmentCitationGroup id="AB5911ACFFC7FFFAFF5B7D39FB9EFE94" box="[151,1151,313,338]" pageId="6" pageNumber="201">
<taxonomicName id="4C494D01FFC7FFFAFF5B7D39FF1AFE94" authorityName="GuenÃ©e" authorityYear="1845" box="[151,251,313,338]" class="Insecta" family="Tortricidae" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="201" phylum="Arthropoda" rank="tribe" tribe="Cochylini">Cochylini</taxonomicName>
New Genus 2 [
<taxonomicName id="4C494D01FFC7FFFAFE567D3AFDD0FE94" authority="" authorityName="Heinrich" authorityYear="1921" box="[410,561,314,338]" class="Insecta" family="Tortricidae" genus="Phalonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="201" phylum="Arthropoda" rank="species" species="cephalanthana">
<emphasis id="B93DEA90FFC7FFFAFE567D3AFDD0FE94" box="[410,561,314,338]" italics="true" pageId="6" pageNumber="201">cephalanthana</emphasis>
</taxonomicName>
group]: 
<treatmentCitation id="0AE81093FFC7FFFAFD447D3AFCDDFE94" author="Brown, J. W." box="[648,828,313,338]" page="208" pageId="6" pageNumber="201" year="2005">
