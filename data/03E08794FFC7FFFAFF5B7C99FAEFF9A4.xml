<taxonomicName id="4C494D01FFC7FFFAFEEA7CDEFD20FF30" authority="Heinrich, 1921" authorityName="Heinrich" authorityYear="1921" box="[294,705,221,246]" class="Insecta" family="Tortricidae" genus="Phalonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="201" phylum="Arthropoda" rank="species" species="cephalanthana">
