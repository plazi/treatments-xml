<taxonomicName id="1CD3D1EAFFE58262FCF0FBFBFC54FB9D" box="[842,908,1095,1119]" class="Entognatha" family="Entomobryidae" genus="Sinella" kingdom="Animalia" order="Collembola" pageId="12" pageNumber="486" phylum="Arthropoda" rank="species" species="gei">
