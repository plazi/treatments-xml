, 
<taxonomicName id="318D4D31A54F0E6F5A0CFC11B1E21BC0" authorityName="Thomson" authorityYear="1869" box="[821,939,949,974]" class="Insecta" family="Halictidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Halictidae</taxonomicName>
, 
<taxonomicName id="318D4D31A54F0E6F5A8FFC11B6181BC0" box="[950,1105,949,974]" class="Insecta" family="Megachilidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Megachilidae</taxonomicName>
, 
<taxonomicName id="318D4D31A54F0E6F5D65FC11B69D1BC0" authorityName="Latreille" authorityYear="1802" box="[1116,1236,949,974]" class="Insecta" family="Mutillidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Mutillidae</taxonomicName>
