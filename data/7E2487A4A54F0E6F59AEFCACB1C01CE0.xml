<taxonomicName id="318D4D31A54F0E6F5B7EFC11B0841BC0" authorityName="Latreille" authorityYear="1802" box="[583,717,949,974]" class="Insecta" family="Andrenidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Andrenidae</taxonomicName>
