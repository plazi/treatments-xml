<taxonomicName id="318D4D31A54F0E6F5C34FC11B7C91BC0" authorityName="Latreille" authorityYear="1802" box="[1293,1408,949,974]" class="Insecta" family="Sphecidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Sphecidae</taxonomicName>
