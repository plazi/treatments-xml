</taxonomicName>
(
<taxonomicName id="318D4D31A54F0E6F5A1CFC7DB1F71BFC" box="[805,958,985,1010]" class="Insecta" family="Cicindellidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Cicindellidae</taxonomicName>
