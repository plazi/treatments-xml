<taxonomicName id="318D4D31A54F0E6F580FFC59B3841C18" authorityName="Latreille" authorityYear="1802" box="[310,461,1021,1046]" class="Insecta" family="Scarabaeidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="family">Scarabaeidae</taxonomicName>
