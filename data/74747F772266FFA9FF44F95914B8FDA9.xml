and 
<taxonomicName id="3BDDB5E22258FFA9FE72FE18173CFE6E" box="[417,546,440,464]" class="Trematoda" family="Transversotrematidae" genus="Transversotrema" kingdom="Animalia" order="Plagiorchiida" pageId="32" pageNumber="33" phylum="Platyhelminthes" rank="species" species="espanola" status="n. sp.">
