: 
<collectingCountry id="F31DE695FFE1FFAA6679B4DB62B2FE21" box="[360,424,475,501]" name="Japan" pageId="38" pageNumber="539">Japan</collectingCountry>
(Honshu and Shikoku) and 
<collectingCountry id="F31DE695FFE1FFAA65F2B4DB603DFE21" box="[739,807,475,501]" name="South Korea" pageId="38" pageNumber="539">Korea</collectingCountry>
(
<bibRefCitation id="EF9BDBF4FFE1FFAA6427B4DB60D6FE21" author="Uchida, T." box="[822,972,474,501]" pageId="38" pageNumber="539" pagination="6 - 33" refId="ref47927" refString="Uchida, T. (1935) Zur Ichneumonidenfauna von Tosa (I.) Subfam. Ichneumoninae. Insecta Matsumurana, 10, 6 - 33." type="journal article" year="1935">Uchida 1935</bibRefCitation>
