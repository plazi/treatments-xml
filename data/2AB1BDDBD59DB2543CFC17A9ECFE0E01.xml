<subSubSection id="CD7CDB81978B6B3C11577587DDDE113C" pageId="22" pageNumber="67" type="reference_group">
<paragraph id="973CD6C97057576D71BB0C0F7A391F86" pageId="22" pageNumber="67">
<taxonomicName id="D1F7B039597CFCA00994D3B5E35395EB" authority="Ashmead, 1893" authorityName="Ashmead" authorityYear="1893" class="Insecta" family="Scelionidae" genus="Trissolcus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trissolcus erugatus" order="Hymenoptera" pageId="22" pageNumber="67" phylum="Arthropoda" rank="species" species="erugatus">Trissolcus erugatus</taxonomicName>
