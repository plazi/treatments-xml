<paragraph id="8B9F36F5B82AFF9CFFC2D275FEC0F926" blockId="19.[140,1108,1398,1672]" pageId="19" pageNumber="376">
<taxonomicName id="4C204D76B82AFF9CFFC2D275FE01F9B3" authorityName="Fall" authorityYear="1934" box="[185,430,1539,1565]" class="Insecta" family="Leiodidae" genus="Agathidium" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="376" phylum="Arthropoda" rank="species" species="atronitens">
<emphasis id="B954EAE7B82AFF9CFFC2D275FE01F9B3" box="[185,430,1539,1565]" italics="true" pageId="19" pageNumber="376">Agathidium atronitens</emphasis>
</taxonomicName>
is newly recorded in 
<collectingRegion id="49E4F817B82AFF9CFDDAD272FCF6F9B3" box="[673,857,1539,1566]" country="Canada" name="New Brunswick" pageId="19" pageNumber="376">New Brunswick</collectingRegion>
