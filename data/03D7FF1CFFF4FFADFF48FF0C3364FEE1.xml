<taxonomicName id="4C7E3589FFF4FFD2FD00FF9E35F7FAF8" authority="ornatus Redtenbacher, 1868" authorityName="Redtenbacher" authorityYear="1868" box="[648,1163,1264,1288]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="62" phylum="Arthropoda" rank="subSpecies" species="ornatus" subGenus="Canthon" subSpecies="ornatus">
