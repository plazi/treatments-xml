<collectingCountry id="F3690E9AFFF4FFD2FEFDFF9E30BEFAF8" box="[373,450,1264,1288]" name="Brazil" pageId="63" pageNumber="62">Brazil</collectingCountry>
, 
<collectingCountry id="F3690E9AFFF4FFD2FE43FF9E333BFAF8" box="[459,583,1264,1288]" name="Argentina" pageId="63" pageNumber="62">Argentina</collectingCountry>
