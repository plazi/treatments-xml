<collectorName id="447B5359FFE1FF80DEEBF818FB4F42A9" box="[1022,1175,1942,1979]" pageId="14" pageNumber="11">Schlinger</collectorName>
and 
<collectorName id="447B5359FFE1FF80DC9DF84AFE0C42FB" box="[392,468,1988,2025]" pageId="14" pageNumber="11">Ross</collectorName>
