<materialsCitation id="3B6A3CA77737FFC1FB01FE638A9E8F1E" collectionCode="CHUNB" pageId="9" pageNumber="50" specimenCode="CHUNB 1198, 1218" specimenCount="1" specimenCount-male="1">
<specimenCount id="9D04FD737737FFC1FB01FE638E0E8F3D" box="[1179,1308,505,529]" pageId="9" pageNumber="50" type="male">three males</specimenCount>
– 
<specimenCode id="DBA49E817737FFC1FAA3FE638BC38F1E" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="9" pageNumber="50">CHUNB 1198</specimenCode>
