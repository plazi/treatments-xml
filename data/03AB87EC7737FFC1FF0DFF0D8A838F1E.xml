<collectingCountry id="F315766A7737FFC1FB1DFF4C8FE28DC2" box="[1159,1264,214,238]" name="Bahamas" pageId="9" pageNumber="50">Bahamas</collectingCountry>
, 
<collectingCountry id="F315766A7737FFC1FB64FF4C8E418DC2" box="[1278,1363,214,238]" name="Bolivia" pageId="9" pageNumber="50">Bolivia</collectingCountry>
