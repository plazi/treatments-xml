, 
<collectingRegion id="49C6F8187737FFC1FDDDFE8588248C1A" box="[583,822,286,310]" country="Brazil" name="Mato Grosso do Sul" pageId="9" pageNumber="50">Mato Grosso do Sul</collectingRegion>
, Pará, 
<collectingRegion id="49C6F8187737FFC1FC13FE8488F28C1A" box="[905,992,286,310]" country="Brazil" name="Paraiba" pageId="9" pageNumber="50">Paraíba</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FC75FE848F6C8C1A" box="[1007,1150,286,310]" country="Brazil" name="Pernambuco" pageId="9" pageNumber="50">Pernambuco</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FB17FE848FCE8C1A" box="[1165,1244,286,310]" country="Brazil" name="Parana" pageId="9" pageNumber="50">Paraná</collectingRegion>
