<collectingCountry id="F315766A7737FFC1FF0DFF608BCC8C3E" box="[151,222,250,274]" name="Brazil" pageId="9" pageNumber="50">Brazil</collectingCountry>
, northern 
<collectingCountry id="F315766A7737FFC1FECCFF608ADA8C3E" box="[342,456,250,274]" name="Argentina" pageId="9" pageNumber="50">Argentina</collectingCountry>
, and 
<collectingCountry id="F315766A7737FFC1FD90FF6189668C3F" box="[522,628,251,275]" name="Paraguay" pageId="9" pageNumber="50">Paraguay</collectingCountry>
