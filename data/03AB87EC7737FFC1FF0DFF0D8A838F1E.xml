<collectingRegion id="49C6F8187737FFC1FB71FE848E8A8C1A" box="[1259,1432,286,310]" country="Brazil" name="Rio de Janeiro" pageId="9" pageNumber="50">Rio de Janeiro</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FF0DFED88BE88C76" box="[151,250,322,346]" country="Brazil" name="Roraima" pageId="9" pageNumber="50">Roraima</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FE9FFED88ACB8C76" box="[261,473,322,346]" country="Brazil" name="Rio Grande do Sul" pageId="9" pageNumber="50">Rio Grande do Sul</collectingRegion>
