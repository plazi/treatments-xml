<collectingRegion id="FE0EF9EAC7072D58FF4550EC4397F7FE" box="[222,511,1181,1205]" country="Ecuador" name="Morona-Santiago" pageId="12" pageNumber="741">Morona Santiago Province</collectingRegion>
