<taxonomicName id="118EE0A0FF96B427FECAFD39FE3AAA97" box="[320,492,643,665]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="2" pageNumber="483" phylum="Arthropoda" rank="species" species="suoitia">
