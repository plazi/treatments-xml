<treatmentCitationGroup id="B1B611BBFFFFFFFED4C5E6AA55651D15" box="[151,551,253,275]" pageId="35" pageNumber="36">
<taxonomicName id="56A64D16FFFFFFFED4C5E6AA55611D15" authority="Hood, 1954: 52" authorityName="Hood" authorityPageNumber="52" authorityYear="1954" box="[151,547,253,275]" class="Insecta" family="Thysanoptera (awaiting allocation)" genus="Homorothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="erianthi">
<emphasis id="A3D2EA87FFFFFFFED4C5E6AA563E1D15" box="[151,380,253,275]" italics="true" pageId="35" pageNumber="36">Homorothrips erianthi</emphasis>
<treatmentCitation id="10071084FFFFFFFED5D0E6AA55611D15" author="" box="[386,547,253,275]" page="52" pageId="35" pageNumber="36">
<bibRefCitation id="F5374B64FFFFFFFED5D0E6AA56BF1D15" box="[386,509,253,275]" pageId="35" pageNumber="36" refId="ref58788" refString="Hood, J. D. (1954) Brasilian Thysanoptera IV. Proceedings of the Biological Society of Washington, 67, 17 - 54." type="journal article">Hood, 1954</bibRefCitation>
