<collectorName id="8D31F3A53D55FFB2FC1DF855FBEAF823" box="[919,1025,1947,1974]" pageId="38" pageNumber="39">B. Malkn</collectorName>
leg. (
<collectionCode id="46D50EB63D55FFB2FBCBF855FB76F820" box="[1089,1181,1948,1974]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="38" pageNumber="39">AMNH</collectionCode>
); ♁2, 
<date id="547AB0B33D55FFB2FB68F852FA73F823" box="[1250,1432,1947,1973]" pageId="38" pageNumber="39" value="1968-07-27" valueMax="1968-07-31" valueMin="1968-07-27">
<collectingDate id="443E495B3D55FFB2FB68F852FA73F823" box="[1250,1432,1947,1973]" pageId="38" pageNumber="39" value="1968-07-27" valueMax="1968-07-31" valueMin="1968-07-27">27–31.VII.1968</collectingDate>
</date>
, 
<collectorName id="8D31F3A53D55FFB2FF1DF809FEE2F84F" box="[151,265,1983,2010]" pageId="38" pageNumber="39">B. Malkin</collectorName>
leg. (
<collectionCode id="46D50EB63D55FFB2FEC2F809FE4FF84C" box="[328,420,1984,2010]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="38" pageNumber="39">AMNH</collectionCode>
)
</materialsCitation>
.
</paragraph>
