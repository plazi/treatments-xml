<caption id="74BBC6FB3D54FFB3FF1DF8F7FB1FF808" ID-DOI="http://doi.org/10.5281/zenodo.4468427" ID-Zenodo-Dep="4468427" httpUri="https://zenodo.org/record/4468427/files/figure.png" pageId="39" pageNumber="40" startId="39.[151,250,1854,1878]" targetBox="[151,1421,181,1830]" targetPageId="39">
