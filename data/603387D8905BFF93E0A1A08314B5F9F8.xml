<heading id="B36D81A2905BFF93E0A1A08314AFFB76" bold="true" box="[151,422,1193,1219]" fontSize="11" level="1" pageId="12" pageNumber="587" reason="1">
<taxonomicName id="2F9A4D4D905BFF93E0A1A08314AFFB76" authority="Chace, 1985" authorityName="Chace" authorityYear="1985" box="[151,422,1193,1219]" class="Malacostraca" family="Pandalidae" genus="Parapandalus" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="587" phylum="Arthropoda" rank="species" species="reflexa">
