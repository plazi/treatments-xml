<quantity id="4CD18884FF83BF1FA92CEFC24F9B9AFE" box="[1354,1432,432,458]" metricMagnitude="2" metricUnit="m" metricValue="1.28" pageId="13" pageNumber="14" unit="m" value="128.0">128 m</quantity>
, in association with 
<taxonomicName id="4C295EE2FF83BF1FAD1CEFAA48079AC6" box="[378,516,472,498]" class="Anthozoa" kingdom="Animalia" order="Alcyonacea" pageId="13" pageNumber="14" phylum="Cnidaria" rank="order">Alcyonacea</taxonomicName>
(
<bibRefCitation id="EFB85890FF83BF1FAE75EFAA48A19AC6" author="Haig" box="[531,674,472,498]" pageId="13" pageNumber="46" refString="Haig, J. (1981 b) Porcellanid crabs from the Indo-West Pacific, Part II. Steenstrupia, 7, 269 - 291." type="journal article" year="1981" yearSuffix="b">
Haig 
<number id="3DE13746FF83BF1FAE33EFAA48919AC6" box="[597,658,472,498]" pageId="13" pageNumber="14" value="1981.0">1981</number>
b
</bibRefCitation>
; 
<bibRefCitation id="EFB85890FF83BF1FAEC8EFAA49A09AC6" author="Kato" box="[686,931,472,498]" pageId="13" pageNumber="46" refString="Kato, S. &amp; Okuno, J. (2001) Shrimps and crabs of Hachijo Island. TBS-Britannica Co., Ltd., Tokyo, 160 pp. (In Japanese)" type="book" year="2001">
Kato &amp; Okuno 
<number id="3DE13746FF83BF1FAF02EFAA49A09AC6" box="[868,931,472,498]" pageId="13" pageNumber="14" value="2001.0">2001</number>
</bibRefCitation>
).
</paragraph>
