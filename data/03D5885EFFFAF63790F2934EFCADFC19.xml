<taxonomicName id="4C7C42CBFFFAF63791E492A1FCA1FC19" authority="(Fabricius)" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[470,822,994,1021]" class="Insecta" family="Scarabaeidae" genus="Catharsius" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="pithecius">
