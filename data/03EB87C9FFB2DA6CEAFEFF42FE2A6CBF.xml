<specimenCode id="DBE49EA4FFB2DA6CEB7EFF37FE286F8F" box="[279,451,226,252]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="79" pageNumber="80">AMNH 26411</specimenCode>
