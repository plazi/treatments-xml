<taxonomicName id="49C04D6BFFADF76D86D3A5E3FEE3FC2A" box="[199,319,980,1003]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="22" pageNumber="23" phylum="Arthropoda" rank="genus">
