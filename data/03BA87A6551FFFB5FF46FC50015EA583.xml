, 
<collectingCountry id="F3047620551FFFB5FB51FC870031A27E" box="[1215,1284,805,829]" name="Brazil" pageId="2" pageNumber="351">Brazil</collectingCountry>
, 
<collectingCountry id="F3047620551FFFB5FAF2FC8700BDA27E" box="[1308,1416,805,829]" name="Colombia" pageId="2" pageNumber="351">Colombia</collectingCountry>
, 
<collectingCountry id="F3047620551FFFB5FCC3FCEB06A6A222" box="[813,915,841,865]" name="Paraguay" pageId="2" pageNumber="351">Paraguay</collectingCountry>
, 
<collectingCountry id="F3047620551FFFB5FC73FCEB06FAA223" box="[925,975,841,864]" name="Peru" pageId="2" pageNumber="351">Peru</collectingCountry>
(
<bibRefCitation id="EF824B41551FFFB5FC33FCEA019EA223" author="Molano F &amp; Moreira FFF &amp; Morales I" box="[989,1195,840,864]" pageId="2" pageNumber="351" pagination="575 - 585" refId="ref6977" refString="Molano F, Moreira FFF, Morales I (2016) New species and records of Microveliinae and Veliinae (Hemiptera: Heteroptera: Veli- idae) from Colombia. Zootaxa 4171 (3): 575 - 585. https: // doi. org / 10.11646 / zootaxa. 4171.3.11" type="journal article" year="2016">Molano et al. 2016</bibRefCitation>
