<collectingCountry id="FBD4762AFF978D019E5978ACFC73FF00" box="[829,938,152,176]" name="Myanmar" pageId="7" pageNumber="588">Myanmar</collectingCountry>
(
<collectingCountry id="FBD4762AFF978D019EDF78ACFBD4FF00" box="[955,1037,152,176]" name="Myanmar" pageId="7" pageNumber="588">Burma</collectingCountry>
), 
<collectingCountry id="FBD4762AFF978D01994578A3FB5CFF00" box="[1057,1157,151,176]" name="Thailand" pageId="7" pageNumber="588">Thailand</collectingCountry>
