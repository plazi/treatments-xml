<emphasis id="B97D7B2EE948FFA6D9ABFD0578E71CC2" bold="true" box="[949,962,706,723]" pageId="15" pageNumber="88">A</emphasis>
, 
<emphasis id="B97D7B2EE948FFA6D9D2FD0578FD1CC2" bold="true" box="[972,984,706,723]" pageId="15" pageNumber="88">E</emphasis>
, ventral view; 
<emphasis id="B97D7B2EE948FFA6DE51FD057F791CC2" bold="true" box="[1103,1116,706,723]" pageId="15" pageNumber="88">B</emphasis>
, 
<emphasis id="B97D7B2EE948FFA6DE78FD057F541CC2" bold="true" box="[1126,1137,706,723]" pageId="15" pageNumber="88">F</emphasis>
, dorsal view; 
<emphasis id="B97D7B2EE948FFA6D8D4FD1F79FD1CF8" bold="true" box="[714,728,728,745]" pageId="15" pageNumber="88">C</emphasis>
, 
<emphasis id="B97D7B2EE948FFA6D8FCFD1F79D51CF8" bold="true" box="[738,752,728,745]" pageId="15" pageNumber="88">G</emphasis>
, lateral view; 
<emphasis id="B97D7B2EE948FFA6D97CFD1F78551CF8" bold="true" box="[866,880,728,745]" pageId="15" pageNumber="88">D</emphasis>
, 
<emphasis id="B97D7B2EE948FFA6D964FD1F78AD1CF8" bold="true" box="[890,904,728,745]" pageId="15" pageNumber="88">H</emphasis>
