<emphasis id="52B9EAF66F0D6160FEF0FC490FC450EA" bold="true" box="[264,646,953,979]" pageId="1" pageNumber="16">
<taxonomicName id="A7CD4D676F0D6160FEF0FC490CAA50EA" authority="Malm &amp; Johanson, 2005" authorityName="Malm &amp; Johanson" authorityYear="2005" box="[264,488,953,979]" class="Insecta" family="Petrothrincidae" genus="Gyrocarisa" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="16" phylum="Arthropoda" rank="species" species="scottae" status="new species">
<emphasis id="52B9EAF66F0D6160FEF0FC490CAA50EA" bold="true" box="[264,488,953,979]" italics="true" pageId="1" pageNumber="16">Gyrocarisa scottae</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="498A578D6F0D6160FE0EFC490FC450EA" box="[502,646,953,979]" pageId="1" pageNumber="16" rank="species">new species</taxonomicNameLabel>
</emphasis>
