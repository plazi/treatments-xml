<taxonomicName id="E5FF219BFFEA9F51BCA2FA63FED2FA43" box="[229,379,1486,1511]" class="Insecta" family="Ephydridae" genus="Mimapsilopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="502" phylum="Arthropoda" rank="genus">Mimapsilopa</taxonomicName>
