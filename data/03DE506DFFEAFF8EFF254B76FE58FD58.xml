<taxonomicName id="4C779AF8FFEAFF8DFD1F484BFCE6F9C9" authorityName=", StOrr" authorityYear="1980" box="[685,803,1610,1633]" class="Reptilia" family="Elapidae" genus="Paroplocephalus" kingdom="Animalia" order="Squamata" pageId="1" pageNumber="334" phylum="Chordata" rank="species" species="atriceps">
