gen. n. species: (a) 
<taxonomicName id="66226943EC5A63AA2CCD0D2B9F648362" genus="S." lsidName="S. crassiovis" pageId="6" pageNumber="169" rank="species" species="crassiovis">S. crassiovis</taxonomicName>
comb. n., ZMH.A14197, male, Provinsi Sumatera Barat; (b) 
<taxonomicName id="C285545FCA51B163122A9D1B33EC30BB" genus="S." lsidName="S. dabulescens" pageId="6" pageNumber="169" rank="species" species="dabulescens">S. dabulescens</taxonomicName>
sp. n., MZB.AMPH.29396, male, holotype, Provinsi Aceh; (c) 
<taxonomicName id="79845A64B0EAFDC3BC6DFE055C5E10C8" genus="S." lsidName="S. montana" pageId="6" pageNumber="169" rank="species" species="montana">S. montana</taxonomicName>
