<taxonomicName id="9611725380FF4A9D1317327F5B8E4D84" authorityName="Richards" authorityYear="1962" class="Insecta" family="Masaridae" genus="Masarina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="4" pageNumber="87" phylum="Arthropoda" rank="genus">
