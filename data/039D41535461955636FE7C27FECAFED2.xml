<location id="8EEBA69E5460955134987CF0FC7FFB42" LSID="urn:lsid:plazi:treatment:039D41535461955636FE7C27FECAFED2:8EEBA69E5460955134987CF0FC7FFB42" box="[753,932,1232,1258]" municipality="Kashima-shi" name="Mt. Kyogatake" pageId="19" pageNumber="20">Mt. Kyogatake</location>
, 
<collectingMunicipality id="6BEF6A3F5460955135D97CF0FB93FB42" box="[944,1096,1232,1258]" pageId="19" pageNumber="20">Kashima­shi</collectingMunicipality>
</materialsCitation>
