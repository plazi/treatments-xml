<taxonomicName id="BA94A089A339655958DAF8A968255E7D" box="[727,1067,1836,1862]" class="Magnoliopsida" family="Fabaceae" genus="Elephanthorrhiza" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="503" phylum="Tracheophyta" rank="species" species="elephantina">
