<taxonomicName id="D96E08EA09C5AB81C0F9F7B9A4985464" authority="Linsenmaier.Later, 1951" authorityName="Linsenmaier.Later" authorityYear="1951" class="Insecta" family="Chrysididae" genus="Pseudospinolia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pseudospinolia" order="Hymenoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="genus">Pseudospinolia</taxonomicName>
