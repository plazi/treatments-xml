species: 
<taxonomicName id="4C1B20F10AA346DE2D4DF9DAFDA4D12B" authority="Cockerell" authorityName="Cockerell. Apterous" authorityYear="1902" box="[294,623,1619,1644]" class="Insecta" family="Halimococcidae" genus="Halimococcus" kingdom="Animalia" order="Hemiptera" pageId="140" pageNumber="141" phylum="Arthropoda" rank="species" species="lampas">
<emphasis id="B96F87600AA346DE2D4DF9DAFDCCD12B" box="[294,519,1620,1644]" italics="true" pageId="140" pageNumber="141">Halimococcus lampas</emphasis>
Cockerell
</taxonomicName>
