<collectionCode id="ED1CAE0195730866E21AF9E5EAEDF98E" box="[636,697,1584,1610]" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="32" pageNumber="33" type="Museum">ZMB</collectionCode>
