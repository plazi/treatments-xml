<location id="8ED2601F95730866E10AF986E9B3F9A9" LSID="urn:lsid:plazi:treatment:03A487D295730867E0F1FB53E96FFF10:8ED2601F95730866E10AF986E9B3F9A9" box="[364,487,1619,1645]" country="Japan" name="Yokohama" pageId="32" pageNumber="33">Yokohama</location>
, 
<collectingCountry id="F31A765495730866E194F981EA61F9AA" box="[498,565,1620,1646]" name="Japan" pageId="32" pageNumber="33">Japan</collectingCountry>
