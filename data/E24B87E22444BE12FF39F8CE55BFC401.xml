<taxonomicName id="ADE24D772445BE12FE8FFEED542AC401" authority="J.F. Arnold" authorityName="J.F. Arnold" box="[289,555,295,320]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="11" pageNumber="54" phylum="Tracheophyta" rank="species" species="nigra">
