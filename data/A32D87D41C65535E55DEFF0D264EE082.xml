species: 
<taxonomicName id="EC844D411C65535F546FFE84212DE14D" authority="Felt" authorityName="Felt" box="[294,574,286,310]" class="Insecta" family="Cecidomyiidae" genus="Oligoxenomyia" kingdom="Animalia" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="radicis">
