104. - 
<taxonomicName id="5B78E02E1D8FEA9B92C127877225D73D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:37208" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" lsidName="Tetramorium schoutedeni Santschi" lsidName-HNS="Tetramorium schoutedeni Santschi" order="Hymenoptera" pageNumber="213" phylum="Arthropoda" rank="species" species="schoutedeni">Tetramorium schoutedeni</taxonomicName>
