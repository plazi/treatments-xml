<quantity id="5D0A14BFFFCFFFF4929924BEDE97F4D6" box="[496,644,1459,1485]" metricMagnitude="2" metricUnit="m" metricValue="2.945" metricValueMax="2.96" metricValueMin="2.93" pageId="19" pageNumber="20" unit="m" value="294.5" valueMax="296.0" valueMin="293.0">293–296 m</quantity>
