<quantity id="5D0A14BFFFCFFFF490982486D89DF4BE" box="[1009,1166,1419,1445]" metricMagnitude="2" metricUnit="m" metricValue="1.03" metricValueMax="1.05" metricValueMin="1.01" pageId="19" pageNumber="20" unit="m" value="103.0" valueMax="105.0" valueMin="101.0">101–105 m</quantity>
