<quantity id="5D0A14BFFFCFFFF492D72486DE48F4BE" box="[446,603,1419,1445]" metricMagnitude="2" metricUnit="m" metricValue="1.22" metricValueMax="1.31" metricValueMin="1.13" pageId="19" pageNumber="20" unit="m" value="122.0" valueMax="131.0" valueMin="113.0">113–131 m</quantity>
