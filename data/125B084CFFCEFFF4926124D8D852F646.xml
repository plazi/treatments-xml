<quantity id="5D0A14BFFFCFFFF4926124D6DD70F4EE" box="[264,355,1499,1525]" metricMagnitude="2" metricUnit="m" metricValue="1.35" pageId="19" pageNumber="20" unit="m" value="135.0">135 m</quantity>
