<quantity id="5D0A14BFFFCFFFF491ED246EDEEEF466" box="[644,765,1379,1405]" metricMagnitude="1" metricUnit="m" metricValue="8.65" metricValueMax="9.8" metricValueMin="7.5" pageId="19" pageNumber="20" unit="m" value="86.5" valueMax="98.0" valueMin="75.0">75–98 m</quantity>
