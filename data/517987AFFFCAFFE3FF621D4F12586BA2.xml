<taxonomicName id="1ED04D3AFFCAFFE0FF621D4F155C6F62" authority="Dejean 1821: 56" authorityName="Dejean. Nikolajev" authorityPageNumber="56" authorityYear="1995" box="[192,550,1283,1308]" class="Insecta" family="Ochodaeidae" genus="Ochodaeus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="genus">
