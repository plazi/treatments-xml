<quantity id="4CF79B33FFB7FF9EC3C2FB24FE67FAFD" box="[443,497,1277,1297]" metricMagnitude="2" metricUnit="m" metricValue="3.8" pageId="88" pageNumber="243" unit="m" value="380.0">380m</quantity>
