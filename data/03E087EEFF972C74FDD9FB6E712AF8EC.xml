<paragraph id="8BF636F8FF902C74FF41FEE57259FE9D" blockId="12.[189,1399,268,365]" pageId="12" pageNumber="13">
<emphasis id="B93DEAEAFF902C74FF41FEE5727FFED6" bold="true" box="[234,325,268,294]" pageId="12" pageNumber="13">Female</emphasis>
SIZE. Body length 
<quantity id="4CB19B1DFF902C74FE3AFEC77106FEB9" box="[401,572,302,329]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" metricValueMax="1.45" metricValueMin="1.05" pageId="12" pageNumber="13" unit="mm" value="12.5" valueMax="14.5" valueMin="10.5">10.5–14.5 mm</quantity>
