<taxonomicName id="4C494D7BFF902C74FEF8FE7D7288FE5D" authorityName="Enghoff &amp; Reboleira" authorityYear="2019" baseAuthorityName="Mauries" baseAuthorityYear="1985" box="[339,434,403,429]" class="Diplopoda" family="Paradoxosomatidae" genus="Jeekelosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="abadi">
