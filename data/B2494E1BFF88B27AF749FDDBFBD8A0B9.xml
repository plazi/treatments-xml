<taxonomicName id="FDE0848EFF88B27AF708FD41FC3BA7AF" authorityName="Borojevic &amp; Boury-Esnault &amp; Vacelet" authorityYear="2000" box="[833,991,653,677]" class="Calcarea" family="Trichogypsiidae" kingdom="Animalia" order="Baerida" pageId="51" pageNumber="254" phylum="Porifera" rank="family">Trichogypsiidae</taxonomicName>
