<taxonomicName id="08A884ABF42BC638113E38B212ED58A1" authority="Latzel, 1880" authorityName="Latzel" authorityYear="1880" class="Chilopoda" family="Lithobiidae" genus="Polybothrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lithobiida" pageId="0" pageNumber="13" phylum="Arthropoda" rank="species" species="mesopotamius" subGenus="Polybothrus">
<pageBreakToken id="B4F6A565B2DB3BD995F8F94D7EB1259B" pageId="0" pageNumber="13" start="start">Polybothrus</pageBreakToken>
(Polybothrus) mesopotamius
</taxonomicName>
