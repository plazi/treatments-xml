<taxonomicName id="4B7156B9CBC41C9175D1BCE934AC7462" LSID-ZBK="urn:lsid:zoobank.org:act:3328B736-21B0-4CE3-91AE-B13EA2536DA7" class="Elasmobranchii" family="Scyliorhinidae" genus="Atelomycterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Atelomycterus Garman 1913:100" order="Carcharhiniformes" pageNumber="24" phylum="Chordata" rank="genus">Atelomycterus</taxonomicName>
