<collectingCountry id="9BDE76741B76FFC4FD66FA71FD3E9481" box="[709,761,1447,1471]" name="Italy" pageId="18" pageNumber="19">Italy</collectingCountry>
, 
<collectingCountry id="9BDE76741B76FFC4FCA7FA71FCAE9481" box="[772,873,1447,1471]" name="Slovenia" pageId="18" pageNumber="19">Slovenia</collectingCountry>
, 
<collectingCountry id="9BDE76741B76FFC4FCD7FA71FC0D9481" box="[884,970,1447,1471]" name="Croatia" pageId="18" pageNumber="19">Croatia</collectingCountry>
