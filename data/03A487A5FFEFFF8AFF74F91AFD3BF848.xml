<emphasis id="B979EAA1FFE8FF8AFF74FAC5FE88FAD4" bold="true" box="[151,287,1333,1355]" pageId="20" pageNumber="223">FIGURE 14.</emphasis>
<taxonomicName id="4C0D4D30FFE8FF8AFEC6FAC5FE6BFAD5" authority="Mohrig &amp; Kauschke &amp; Broadley, 2018" authorityName="Mohrig &amp; Kauschke &amp; Broadley" authorityYear="2018" box="[293,508,1333,1355]" class="Insecta" family="Sciaridae" genus="Bradysia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="223" phylum="Arthropoda" rank="species" species="parareflexa" status="sp. n.">
