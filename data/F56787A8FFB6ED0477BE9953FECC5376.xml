<treatmentCitationGroup id="5DDE1190FFB6ED0477BE990BFD6D512B" box="[178,734,1124,1146]" pageId="2" pageNumber="165">
<treatmentCitation id="FC6F10AFFFB6ED0477BE990BFD375128" author="ALEKSANDROV A" box="[178,644,1124,1146]" page="151" pageId="2" pageNumber="165" year="1934">
<taxonomicName id="BACE4D3DFFB6ED0477BE990BFD375128" authority="ALEKSANDROV, 1934: 151" authorityName="ALEKSANDROV" authorityPageNumber="151" authorityYear="1934" box="[178,644,1124,1146]" class="Insecta" family="Staphylinidae" genus="Paederus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="165" phylum="Arthropoda" rank="species" species="pseudobaudii">
<emphasis id="4FBAEAACFFB6ED0477BE990BFE34512B" box="[178,391,1124,1145]" italics="true" pageId="2" pageNumber="165">Paederus pseudobaudii</emphasis>
<bibRefCitation id="195F4B4FFFB6ED047682990BFD375128" author="ALEKSANDROV A" box="[398,644,1124,1146]" pageId="2" pageNumber="165" pagination="150 - 155" refId="ref12219" refString="ALEKSANDROV A. I. (1934): K spisku zhukov semeistva Staphilinidae (Coleoptera) iz Girinskoi provintsii s opisaniem novykh form. - Ezhegodnik, Kluba Estestvoznaniia i Geografii Khsml. 1 [1933]: 150 - 155." type="journal article" year="1934">ALEKSANDROV, 1934: 151</bibRefCitation>
</taxonomicName>
</treatmentCitation>
