<taxonomicName id="4C48ADF7FF82C9329D0E65BCFD9AAD43" authority="(Zeller, 1878)" baseAuthorityName="Zeller" baseAuthorityYear="1878" box="[286,576,488,514]" class="Insecta" family="Gelechiidae" genus="Teleiodes" kingdom="Animalia" order="Lepidoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="saltuum">
