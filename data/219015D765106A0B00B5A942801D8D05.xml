<taxonomicName id="4D42985F57180728D9351A776524DD6A" class="Insecta" family="Ichneumonidae" genus="Hemiteles" kingdom="Animalia" lsidName="Hemiteles" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Hemiteles</taxonomicName>
