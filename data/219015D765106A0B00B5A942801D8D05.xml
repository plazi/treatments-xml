<taxonomicName id="7A9D695EA2A691D04CDB912F2358E076" class="Insecta" family="Ichneumonidae" genus="Phygadeuon" kingdom="Animalia" lsidName="Phygadeuon" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Phygadeuon</taxonomicName>
