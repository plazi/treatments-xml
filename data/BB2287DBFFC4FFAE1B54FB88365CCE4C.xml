MATERIAL: 
<typeStatus id="EC30886FFFC6FFAE1869FC4D31D4C9D4" box="[565,674,924,950]" pageId="3" pageNumber="4">Holotype</typeStatus>
male. 
<emphasis id="01FFEADFFFC6FFAE18A5FC4D30E5C9D4" bold="true" box="[761,915,924,950]" pageId="3" pageNumber="4">
<collectingCountry id="4B9C765DFFC6FFAE18A5FC4D30E5C9D4" box="[761,915,924,950]" name="Tanzania" pageId="3" pageNumber="4">TANZANIA</collectingCountry>
</emphasis>
: East Usambara, Amani, 
<quantity id="F4739B28FFC6FFAE1E8EFC4D3650C9D4" box="[1234,1318,924,950]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="3" pageNumber="4" unit="m" value="1000.0">1000m</quantity>
, 
<date id="4735100DFFC6FFAE1B54FC15320CC9BC" box="[264,378,964,990]" pageId="3" pageNumber="4" value="1977-01-21">
<number id="854324EAFFC6FFAE1B54FC153251C9BC" box="[264,295,964,990]" pageId="3" pageNumber="4" value="21.0">21</number>
.i. 
<number id="854324EAFFC6FFAE1B64FC15320CC9BC" box="[312,378,964,990]" pageId="3" pageNumber="4" value="1977.0">1977</number>
