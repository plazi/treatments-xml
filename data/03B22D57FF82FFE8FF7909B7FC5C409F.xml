<taxonomicName id="4C1BE7C2FF9CFFE8FB460A3BFA974072" baseAuthorityName="Stebbing" baseAuthorityYear="1888" box="[1192,1296,1824,1849]" class="Malacostraca" family="Photidae" genus="Gammaropsis" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="787" phylum="Arthropoda" rank="species" species="atlantica">
