<collectingCountry id="9BEA7644BC4EFFDC5017BE1AFE0FF8BF" box="[361,434,1809,1835]" name="Brazil" pageId="8" pageNumber="31">Brazil</collectingCountry>
: 
<collectingRegion id="2139F836BC4EFFDC50C2BE1AFDD6F8BF" box="[444,619,1809,1835]" country="Brazil" name="Sao Paulo" pageId="8" pageNumber="31">São Paulo state</collectingRegion>
