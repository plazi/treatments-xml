<caption id="DF5696E8F13BFFD1FF62F8F7FDA48336" httpUri="https://zenodo.org/record/292649/files/figure.png" pageId="2" pageNumber="587" targetBox="[152,1435,263,1776]" targetPageId="2">
<paragraph id="8B96C660F13BFFD1FF62F8F7FDA48336" blockId="2.[151,1436,1800,1881]" pageId="2" pageNumber="587">
