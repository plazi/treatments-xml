<taxonomicName id="4C795DCAFFE058598A2DD406BE2DFB16" authorityName="Duchassaing &amp; Michelin" authorityYear="1846" box="[843,1033,333,359]" class="Anthozoa" family="Gorgoniidae" genus="Pterogorgia" kingdom="Animalia" order="Alcyonacea" pageId="18" pageNumber="69" phylum="Cnidaria" rank="species" species="guadalupensis">
