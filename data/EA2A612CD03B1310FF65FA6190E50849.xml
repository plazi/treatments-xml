<materialsCitation id="D2EBDA67D03B1310FF65F93C90E50991" ID-GBIF-Occurrence="1288188033" location="Costa" pageId="3" pageNumber="304" specimenCount="1" stateProvince="Costa">
<keyLead id="D972730FD03B1310FF65F93C90E50991" pageId="3" pageNumber="304">
