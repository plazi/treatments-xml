<taxonomicName id="4C784D0EBF05FFA326993D6B6673F9F3" box="[997,1126,1553,1576]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="317" phylum="Arthropoda" rank="subGenus" subGenus="Aenandrena">
