<taxonomicName id="63DDCB9AA0091478FECAE18CFE4D2BD2" authority="Gómez, 2022" authorityName="Gómez" authorityYear="2022" box="[344,477,702,730]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="popeyei" status="sp. nov.">
<emphasis id="96A96C0BA0091478FECAE18CFE4D2BD2" box="[344,477,702,730]" italics="true" pageId="42" pageNumber="43">A. popeyei</emphasis>
