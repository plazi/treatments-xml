<geoCoordinate id="EE235007FFC2FFE699C3FE9DF875318C" box="[446,564,367,393]" degrees="123" direction="west" minutes="00" orientation="longitude" pageId="83" pageNumber="84" precision="925" value="-123.0">123°00’W</geoCoordinate>
) (
<bibRefCitation id="EF864B31FFC2FFE69A32FE9DF964318C" author="Priede" box="[591,805,367,393]" etAl="et al." firstAuthor="Priede" pageId="83" pageNumber="84" pagination="173" refId="ref314484" refString="Priede, I. G., Drazen, J. C., Bailey, D. M., Kuhnz, L. A. &amp; Fabian, D. (2019) Abyssal demersal fishes recorded at station M (34 ° 50 ' N, 123 ° 00 ' W, 4100 m depth) in the northeast Pacific Ocean: an annotated check list and synthesis. Deep-Sea Research Part II, 173. https: // doi. org / 10.1016 / j. dsr 2.2019.104648" type="journal article" year="2019">
Priede 
<emphasis id="B963EAD2FFC2FFE69ADEFE9DF89E318C" box="[675,735,367,393]" italics="true" pageId="83" pageNumber="84">et al.</emphasis>
2019
</bibRefCitation>
), and to northern 
<collectingCountry id="F3007650FFC2FFE69B87FE9DFE76318C" box="[1018,1079,367,393]" name="Chile" pageId="83" pageNumber="84">Chile</collectingCountry>
(
<geoCoordinate id="EE235007FFC2FFE69C37FE9DFEE6318C" box="[1098,1191,367,393]" degrees="23" direction="south" minutes="29" orientation="latitude" pageId="83" pageNumber="84" precision="925" value="-23.483334">23°29’S</geoCoordinate>
