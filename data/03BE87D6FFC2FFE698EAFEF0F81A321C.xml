<quantity id="4CEF9B25FFC2FFE69D20FE29FA9D321C" metricMagnitude="3" metricUnit="m" metricValue="6.273" pageId="83" pageNumber="84" unit="m" value="6273.0">6,273 m</quantity>
(
<quantity id="4CEF9B25FFC2FFE69897FE0CFB12321F" box="[234,339,510,538]" metricMagnitude="3" metricUnit="m" metricValue="6.272784" pageId="83" pageNumber="84" unit="ft" value="20580.0">20,580 ft</quantity>
) (
<bibRefCitation id="EF864B31FFC2FFE69914FE0DF80E321C" author="Machida" box="[361,591,511,537]" etAl="et al." firstAuthor="Machida" pageId="83" pageNumber="84" pagination="189 - 200" refId="ref298450" refString="Machida, Y., Ohta, S. &amp; Okamura, O. (1987) Newly obtained specimens and information on a deep-sea fish Spectrunculus grandis (Gunther) (Ophidiidae, Ophidiiformes) from Japan. Reports of the Usa Marine Biological Institute, Kochi University, 9, 189 - 200." type="journal article" year="1987">
