. To 
<quantity id="4CEF9B25FFC2FFE69B1AFEF1F9FE3118" box="[871,959,259,285]" metricMagnitude="0" metricUnit="m" metricValue="1.38" pageId="83" pageNumber="84" unit="cm" value="138.0">138 cm</quantity>
