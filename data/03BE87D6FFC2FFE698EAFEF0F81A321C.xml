<bibRefCitation id="EF864B31FFC2FFE69D54FE61FB6631D4" author="Nielsen" etAl="et al." firstAuthor="Nielsen" pageId="83" pageNumber="84" refId="ref305228" refString="Nielsen, J. G., Cohen, D. M., Markle, D. F. &amp; Robins, C. R. (1999) FAO species catalogue. Volume 18. Ophidiiform fishes of the world (Order Ophidiiformes). FAO Fisheries Synopsis, No. 125. FAO, Rome." type="book" year="1999">
Nielsen 
<emphasis id="B963EAD2FFC2FFE69DF5FE61FAA231D4" italics="true" pageId="83" pageNumber="84">et al.</emphasis>
1999
</bibRefCitation>
); depth: voucher specimens from depths of about 
<quantity id="4CEF9B25FFC2FFE69B22FE45F9B531D4" box="[863,1012,439,465]" metricMagnitude="3" metricUnit="m" metricValue="2.566" metricValueMax="4.332" metricValueMin="0.8" pageId="83" pageNumber="84" unit="m" value="2566.0" valueMax="4332.0" valueMin="800.0">800–4,332 m</quantity>
(
<quantity id="4CEF9B25FFC2FFE69C79FE44FEFA31D7" box="[1028,1211,438,466]" metricMagnitude="3" metricUnit="m" metricValue="2.5655016" metricValueMax="4.3309032" metricValueMin="0.8001" pageId="83" pageNumber="84" unit="ft" value="8417.0" valueMax="14209.0" valueMin="2625.0">
<geoCoordinate id="EE235007FFC2FFE69C79FE44FE0531D4" box="[1028,1092,438,465]" degrees="2,625" direction="north" orientation="latitude" pageId="83" pageNumber="84" precision="55" value="2.625">2,625</geoCoordinate>
<geoCoordinate id="EE235007FFC2FFE69C39FE44FEDE31D4" box="[1092,1183,438,465]" degrees="14,209" direction="west" orientation="longitude" pageId="83" pageNumber="84" precision="55" value="-14.209">–14,209</geoCoordinate>
ft
</quantity>
) (min.: Nielsen and Hureau 1980; max.: 
<bibRefCitation id="EF864B31FFC2FFE699D1FE29F83631F0" author="Linley" box="[428,631,475,501]" etAl="et al." firstAuthor="Linley" pageId="83" pageNumber="84" refId="ref296329" refString="Linley, T. D., Stewart, A. L., McMillan, P. J., Clark, M. R., Gerringer, M. E., Drazen, J. C., Fujii, T. &amp; Jamieson, A. J. (2017) Bait attending fishes of the abyssal zone and hadal boundary: community structure," type="book" year="2017">
Linley 
<emphasis id="B963EAD2FFC2FFE69981FE29F87531F0" box="[508,564,475,501]" italics="true" pageId="83" pageNumber="84">et al.</emphasis>
2017
</bibRefCitation>
