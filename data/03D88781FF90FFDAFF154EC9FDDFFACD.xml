<taxonomicName id="4C714D14FF90FFDAFF714F56FD9FFEDA" authority="Song &amp; Zhu, 1999" authorityName="Song &amp; Zhu" authorityYear="1999" box="[243,529,264,287]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="taishan">
