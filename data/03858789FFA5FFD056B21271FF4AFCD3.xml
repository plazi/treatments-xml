species. 32, 
<emphasis id="B958EA8DFFA8FFD154F413B5FC38F8C2" box="[721,909,1791,1814]" italics="true" pageId="10" pageNumber="43">E. species VS307</emphasis>
, adult female. Scale bar 1 mm; 33, same, head, frontal view; 34, same, abdomen, lateral view; 35, 
<taxonomicName id="4C2C4D1CFFA8FFD154E71255FC90F8E2" box="[706,805,1823,1846]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="43" phylum="Arthropoda" rank="species" species="phiala" status="sp. n.">
<emphasis id="B958EA8DFFA8FFD154E71255FC90F8E2" box="[706,805,1823,1846]" italics="true" pageId="10" pageNumber="43">E. phiala</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26B57F6FFA8FFD155081254FCDFF8E2" box="[813,874,1822,1846]" pageId="10" pageNumber="43" rank="species">
