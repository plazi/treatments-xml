<taxonomicName id="E0F64D56DE49FFD5B06DC256FD179738" authority="Gutu, 2006: 164 - 171" authorityName="Gutu" authorityPageNumber="164 - 171" authorityYear="2006" box="[357,695,546,569]" class="Malacostraca" family="Kalliapseudidae" genus="Hemikalliapseudes" kingdom="Animalia" order="Tanaidacea" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="vasileradui">
