<taxonomicName id="69C297C0F720FFB2FED6FDA64697FDC0" authorityName="Folsom" authorityYear="1896" box="[319,432,521,546]" class="Entognatha" family="Arrhopalitidae" genus="Smynthurus" kingdom="Animalia" order="Collembola" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="benitus">
