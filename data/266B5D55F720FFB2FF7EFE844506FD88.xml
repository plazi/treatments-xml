<taxonomicName id="69C297C0F720FFB2FD64FDA64401FDC0" baseAuthorityName="Stach" baseAuthorityYear="1945" box="[653,806,521,546]" class="Entognatha" family="Arrhopalitidae" genus="Pygmarrhopalites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="principalis">
