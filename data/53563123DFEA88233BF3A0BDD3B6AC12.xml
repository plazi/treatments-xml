<geoCoordinate id="7C103D472135FC3E03A17B45A7BA924B" degrees="11.42232" direction="north" orientation="latitude" precision="1" value="11.42232">11.42232</geoCoordinate>
, 
<geoCoordinate id="4F122351F95FBBC4FC2523D0848044A9" degrees="107.42834" direction="east" orientation="longitude" precision="1" value="107.42834">107.42834</geoCoordinate>
