<quantity id="8D6B9B35FFEEFF85FC97FB471F4BFB11" box="[785,845,1239,1263]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="93" pageNumber="94" unit="m" value="20.0">20 m</quantity>
