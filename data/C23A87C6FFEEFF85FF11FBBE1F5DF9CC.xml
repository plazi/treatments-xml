<date id="3E2D1010FFEEFF85FB8BFB47189CFB10" box="[1037,1178,1239,1263]" pageId="93" pageNumber="94" value="2007-06-28">
<number id="FC5B24F7FFEEFF85FB8BFB47182FFB10" box="[1037,1065,1239,1263]" pageId="93" pageNumber="94" value="28.0">28</number>
