<paragraph id="BC0803749C42FFCAFF3DFC0029A4FC3A" blockId="12.[151,578,928,1011]" box="[151,511,989,1011]" pageId="12" pageNumber="313">
<taxonomicName id="7BB778F79C42FFCAFF3DFC0029A0FC3A" authority="Weise 1908: 13" authorityName="Weise" authorityPageNumber="13" authorityYear="1908" box="[151,507,989,1011]" class="Insecta" family="Coccinellidae" genus="Diomus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="313" phylum="Arthropoda" rank="species" species="notescens">
<emphasis id="8EC3DF669C42FFCAFF3DFC002915FC3A" box="[151,334,990,1011]" italics="true" pageId="12" pageNumber="313">Diomus notescens</emphasis>
: Weise 
<number id="0A7F11539C42FFCAFE35FC03298DFC3A" box="[415,470,989,1011]" pageId="12" pageNumber="313" value="1908.0">1908</number>
