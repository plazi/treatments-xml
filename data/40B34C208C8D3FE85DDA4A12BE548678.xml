<taxonomicName id="6C76A894DC645F4839F812691F61FB60" authorityName="Mateu" authorityYear="1961" class="Insecta" family="Carabidae" genus="Straneotia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Straneotia freyi" order="Coleoptera" pageId="16" pageNumber="73" phylum="Arthropoda" rank="species" species="freyi">
