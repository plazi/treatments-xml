<taxonomicName id="4C4DBA951B574C1DFF12264347C7FEE1" authorityName="Shoemaker" authorityYear="1942" box="[194,395,301,325]" class="Malacostraca" family="Neomegamphopidae" genus="Neomegamphopus" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="3141" phylum="Arthropoda" rank="genus">
