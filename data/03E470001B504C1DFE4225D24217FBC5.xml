<taxonomicName id="4C4DBA951B574C1DFE0A26E2448AFE00" authorityName="Myers" authorityYear="1981" box="[474,710,396,420]" class="Malacostraca" family="Neomegamphopidae" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="3141" phylum="Arthropoda" rank="family">Neomegamphopidae</taxonomicName>
