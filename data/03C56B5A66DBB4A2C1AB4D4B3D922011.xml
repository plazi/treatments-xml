<taxonomicName id="4C6CA1CF66DAB4A2C5C04A743D83204C" baseAuthorityName="Gultekin" baseAuthorityYear="2004" box="[1276,1433,260,285]" class="Magnoliopsida" family="Boraginaceae" kingdom="Plantae" order="Boraginales" pageId="143" pageNumber="144" phylum="Tracheophyta" rank="family">Boraginaceae</taxonomicName>
