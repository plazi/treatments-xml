<heading id="D0B281DFFFC3BC2FFE0E055BFCFD505A" box="[428,807,1546,1571]" centered="true" fontSize="10" level="2" pageId="3" pageNumber="1989" reason="3">
<taxonomicName id="4C454D30FFC3BC2FFE0E055BFD40505B" authority="Wiles &amp; Chatterjee &amp; Troch, 2002" authorityName="Wiles &amp; Chatterjee &amp; Troch" authorityYear="2002" box="[428,666,1546,1570]" class="Arachnida" family="Pontarachnidae" genus="Litarachna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="1989" phylum="Arthropoda" rank="species" species="cawthorni" status="new species">
<emphasis id="B931EAA1FFC3BC2FFE0E055BFD40505B" bold="true" box="[428,666,1546,1570]" italics="true" pageId="3" pageNumber="1989">Litarachna cawthorni</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20257DAFFC3BC2FFD06055DFCFD505A" box="[676,807,1547,1571]" pageId="3" pageNumber="1989" rank="species">new species</taxonomicNameLabel>
