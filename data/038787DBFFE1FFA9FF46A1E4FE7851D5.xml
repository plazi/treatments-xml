<taxonomicName id="4C2E4D4EFFE1FFA9FF46A22CFE7E5027" authority="K. Viets, 1935" authorityName="K. Viets" authorityYear="1935" box="[151,499,1549,1571]" class="Arachnida" family="Arrenuridae" genus="Arrenurus" kingdom="Animalia" order="Trombidiformes" pageId="8" pageNumber="542" phylum="Arthropoda" rank="species" species="furciger">
