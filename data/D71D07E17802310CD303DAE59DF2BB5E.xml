<taxonomicName id="F80F1EF5045FE9D278BF78FE31CEC219" class="Pinopsida" family="Cupressaceae" genus="Metasequioa" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Pinales" pageId="7" pageNumber="28" phylum="Tracheophyta" rank="genus">
