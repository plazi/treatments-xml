<taxonomicName id="4C144D37C665A217FAE0F0ACFA2F4C1B" authorityName="Dobson" authorityYear="1877" box="[1304,1472,3894,3933]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="19" pageNumber="246" phylum="Chordata" rank="species" species="calcaratus">
