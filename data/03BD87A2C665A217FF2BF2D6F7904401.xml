<paragraph id="8BAB36B4C665A217FBF1F157F8F04CBD" blockId="19.[1031,1827,3631,4198]" pageId="19" pageNumber="246">
<taxonomicName id="4C144D37C665A217FBF1F157FA904DB2" authorityName="Gould" authorityYear="1854" box="[1033,1407,3789,3828]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="19" pageNumber="246" phylum="Chordata" rank="species" species="ceruinus">
