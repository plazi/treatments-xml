<taxonomicName id="4C144D37C665A217FB33F09BFAD74C6E" authorityName="Temminck" authorityYear="1834" box="[1227,1336,3841,3880]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="19" pageNumber="246" phylum="Chordata" rank="species" species="bicolor">
