<taxonomicName id="18CA4D21FFF27919FF5CF89EFE27F868" authority="Hood, 1941" authorityName="Hood" authorityYear="1941" box="[151,394,1915,1942]" class="Insecta" family="Thripidae" genus="Frankliniella" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="bondari">
<emphasis id="EDBEEAB0FFF27919FF5CF89EFE27F868" bold="true" box="[151,394,1915,1942]" pageId="4" pageNumber="5">
<emphasis id="EDBEEAB0FFF27919FF5CF89EFF58F86B" bold="true" box="[151,245,1916,1942]" italics="true" pageId="4" pageNumber="5">bondari</emphasis>
Hood, 
<number id="69022485FFF27919FE9BF899FE27F868" box="[336,394,1915,1941]" pageId="4" pageNumber="5" value="1941.0">1941</number>
</emphasis>
</taxonomicName>
