<taxonomicName id="E3E44D758637550C36426D918633FA31" box="[369,549,1456,1481]" class="Chilopoda" family="Lithobiidae" genus="Hessebius" kingdom="Animalia" order="Lithobiomorpha" pageId="2" pageNumber="53" phylum="Arthropoda" rank="species" species="jangtseanus">
