<taxonomicName id="E3E44D758637550C37A46E648586F9A6" box="[151,400,1605,1630]" class="Chilopoda" family="Lithobiidae" genus="Paobius" kingdom="Animalia" order="Lithobiomorpha" pageId="2" pageNumber="53" phylum="Arthropoda" rank="species" species="jangtseanus">
<emphasis id="1690EAE48637550C37A46E648586F9A6" box="[151,400,1605,1630]" italics="true" pageId="2" pageNumber="53">Paobius jangtseanus</emphasis>
