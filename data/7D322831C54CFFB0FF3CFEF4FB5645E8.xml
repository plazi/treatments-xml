<paragraph id="F5249927C54CFFB0FF3CFE7EFDEF4402" blockId="2.[151,1436,366,574]" pageId="2" pageNumber="399">
<taxonomicName id="329BE2A4C54CFFB0FF3CFE7EFD5C47C3" authority="Ahmad &amp; Shadab, 1974: 181" authorityName="Ahmad &amp; Shadab" authorityPageNumber="181" authorityYear="1974" box="[151,679,427,450]" class="Insecta" family="Coreidae" genus="Paranotocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="399" phylum="Arthropoda" rank="species" species="echinus">
