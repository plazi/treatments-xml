<taxonomicName id="4C6008C73555FFDCC37FFA92FCB9FA8D" box="[558,704,1334,1359]" class="Insecta" family="Acroceridae" genus="Paracrocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="145" phylum="Arthropoda" rank="species" species="minuscula">
