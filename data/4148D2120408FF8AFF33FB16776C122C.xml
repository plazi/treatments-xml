<taxonomicName id="0EE118870408FF8BFDDCF848751317A8" box="[632,769,1925,1951]" class="Globothalamea" family="Globigerinidae" genus="Globigerina" kingdom="Chromista" order="Rotaliida" pageId="88" pageNumber="89" phylum="Foraminifera" rank="genus">
