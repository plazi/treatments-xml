<keyLead id="30A795BE3332DC5FFF39FC3431F832B6" pageId="11" pageNumber="12">
<number id="3D9E24AC3332DC5FFF39FC3434CA32D6" box="[151,167,914,937]" pageId="11" pageNumber="12" value="3.0">3</number>
