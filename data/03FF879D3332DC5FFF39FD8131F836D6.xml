<keyLead id="30A795BE3332DC5FFF39FBB431F83516" pageId="11" pageNumber="12">
<number id="3D9E24AC3332DC5FFF39FBB434CA3556" box="[151,167,1042,1065]" pageId="11" pageNumber="12" value="4.0">4</number>
