<taxonomicName id="D55B4B24A25C87D7EF97458A3ADCB94A" class="Magnoliopsida" family="Primulaceae" genus="Embelia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Embelia" order="Ericales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Embelia</taxonomicName>
