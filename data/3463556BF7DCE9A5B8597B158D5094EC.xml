<paragraph id="6DF5063E51BC63E73C75B26358616D35" pageNumber="51">
<taxonomicName id="68AA24DCF5B08F415015CB0749982DD4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141856" authority="Forel" authorityName="Forel" class="Hexapoda" family="Formicidae" genus="Euponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Euponera (Trachymesopus) suspecta Santschi" lsidName-HNS="Euponera (Trachymesopus) suspecta Santschi" order="Hymenoptera" pageNumber="51" phylum="Arthropoda" rank="species" species="suspecta" subGenus="Trachymesopus">Euponera (Trachymesopus) suspecta</taxonomicName>
