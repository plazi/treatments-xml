</taxonomicName>
, and 
<taxonomicName id="4C6CF39ED37FFF822EEB942DE8BACF64" authority="(Nees) Blume (1851: 342)" authorityName="Blume" authorityPageNumber="342" authorityYear="1851" baseAuthorityName="Nees" box="[402,993,345,371]" class="Magnoliopsida" family="Lauraceae" genus="Actinodaphne" kingdom="Plantae" order="Laurales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="obovata">
<emphasis id="B918540FD37FFF822EEB942DE9C6CF65" box="[402,669,345,370]" italics="true" pageId="4" pageNumber="5">Actinodaphne obovata</emphasis>
(Nees) Blume (1851: 342)
</taxonomicName>
