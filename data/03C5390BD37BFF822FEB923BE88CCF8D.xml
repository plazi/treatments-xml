</taxonomicName>
, 
<taxonomicName id="4C6CF39ED37FFF822FEE942DEA0ACF64" box="[151,337,345,371]" class="Magnoliopsida" family="Fagaceae" genus="Lithocarpus" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B918540FD37FFF822FEE942DEA7DCF65" box="[151,294,345,370]" italics="true" pageId="4" pageNumber="5">Lithocarpus</emphasis>
