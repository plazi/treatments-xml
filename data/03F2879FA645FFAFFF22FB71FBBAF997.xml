<caption id="DF246601A643FFAFFF22FBBAFB8DFB25" box="[151,1058,1135,1158]" httpUri="https://zenodo.org/record/252444/files/figure.png" pageId="13" pageNumber="270" targetBox="[226,1346,193,1114]" targetPageId="13">
<paragraph id="8BE43689A643FFAFFF22FBBAFB8DFB25" blockId="13.[151,1058,1135,1158]" box="[151,1058,1135,1158]" pageId="13" pageNumber="270">
