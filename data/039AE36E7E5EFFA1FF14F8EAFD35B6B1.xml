<subSubSection id="C32901F37E5CFFA1FF46FC9FFAE6B696" box="[197,1291,886,912]" pageId="10" pageNumber="67" type="etymology">
<paragraph id="8B8C52787E5CFFA1FF46FC9FFAE6B696" blockId="10.[151,1437,152,951]" box="[197,1291,886,912]" pageId="10" pageNumber="67">
<emphasis id="B9478E6A7E5CFFA1FF46FC9FFEBCB696" bold="true" box="[197,337,886,912]" pageId="10" pageNumber="67">Etymology.</emphasis>
