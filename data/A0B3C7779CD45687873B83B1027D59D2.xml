<taxonomicName id="3BB7E95D934998E17E76C317C1B6EB95" authorityName="Masner &amp; Huggert" authorityYear="1989" class="Insecta" family="Platygastridae " genus="Afrisolia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Afrisolia obesa" order="Hymenoptera" pageId="0" pageNumber="115" phylum="Arthropoda" rank="species" species="obesa">Afrisolia obesa</taxonomicName>
