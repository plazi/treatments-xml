<taxonomicName id="8639A76620A58AA42DCB698C8A96B112" authorityName="Foerster" authorityYear="1878" class="Insecta" family="Platygastridae " genus="Isolia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Isolia" order="Hymenoptera" pageId="0" pageNumber="115" phylum="Arthropoda" rank="genus">Isolia</taxonomicName>
