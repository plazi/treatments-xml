<collectionCode id="42B61DCEFFD08320FC475E25FC0EF9A7" box="[899,926,1605,1628]" country="Germany" lsid="urn:lsid:biocol.org:col:15444" name="Friedrich-Schiller-Universität Jena" pageId="109" pageNumber="109" type="Herbarium">JE</collectionCode>
