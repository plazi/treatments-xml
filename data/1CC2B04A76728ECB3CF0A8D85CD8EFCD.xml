17. 
<taxonomicName id="1245BB800B45FAE5EA33B545D9076FF2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145365" class="Insecta" family="Formicidae" genus="Eciton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eciton minutum Jerdon" lsidName-HNS="Eciton minutum Jerdon" order="Hymenoptera" pageNumber="112" phylum="Arthropoda" rank="species" species="minutum">Eciton minutum</taxonomicName>
