<collectingCountry id="2099761E2965FF9909FC0AE6FF728242" box="[151,221,1258,1282]" name="Egypt" pageId="3" pageNumber="380">Egypt</collectingCountry>
, 
<collectingCountry id="2099761E2965FF9909830AE5FEE58242" box="[232,330,1257,1282]" name="Pakistan" pageId="3" pageNumber="380">Pakistan</collectingCountry>
, Indian and Pacific ocean ilands and 
<collectingCountry id="2099761E2965FF990B820AE5FC808242" box="[745,815,1257,1282]" name="China" pageId="3" pageNumber="380">China</collectingCountry>
; further confirmation on occurrence in 
<collectingCountry id="2099761E2965FF990D8C0AE6FABB8241" box="[1255,1300,1258,1281]" name="Iran" pageId="3" pageNumber="380">Iran</collectingCountry>
