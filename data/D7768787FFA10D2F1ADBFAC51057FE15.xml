<taxonomicName id="98DF4D12FFA00D2F1E72FED710DEFE85" baseAuthorityName="Armua de Reyes &amp; Estevez" baseAuthorityYear="2016" box="[1086,1217,331,356]" class="Liliopsida" family="Pontederiaceae" genus="Pontederia" kingdom="Plantae" order="Commelinales" pageId="29" pageNumber="480" phylum="Tracheophyta" rank="species" species="crassipes">
