, 
<taxonomicName id="98DF4D12FFA00D2F1E82FED71120FE85" box="[1230,1343,330,356]" class="Liliopsida" family="Pontederiaceae" genus="Pontederia" kingdom="Plantae" order="Commelinales" pageId="29" pageNumber="480" phylum="Tracheophyta" rank="species" species="cordata">
