<paragraph id="9A705BF5C33E99455F96EE114617122A" pageId="0" pageNumber="147">
<taxonomicName id="D7F02E355B451E42F6527E4312137282" class="Insecta" family="Carabidae" genus="Merizodus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Merizodus catapileanus" order="Coleoptera" pageId="0" pageNumber="147" phylum="Arthropoda" rank="species" species="catapileanus">
