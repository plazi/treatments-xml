<taxonomicName id="EFEF4D71FF94FFF30396F92DFE40F90F" authority="Solyanikov, 2002" authorityName="Solyanikov" authorityYear="2002" box="[151,482,1687,1711]" class="Insecta" family="Psychidae" genus="Hyalopteronia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="352" phylum="Arthropoda" rank="genus">
