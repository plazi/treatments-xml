<typeStatus id="54F397AAB535FFE7FC75FA0B0559E9B0" box="[955,1045,1496,1517]" pageId="5">paratype</typeStatus>
, 
<collectingCountry id="F35F6998B535FFE7FBD1FA04050FE9B0" box="[1055,1091,1495,1517]" name="Iceland" pageId="5">Isla</collectingCountry>
Bonita, 
<collectingCountry id="F35F6998B535FFE7FB57FA040440E9B0" box="[1177,1292,1495,1517]" name="Costa Rica" pageId="5">Costa Rica</collectingCountry>
). 
<number id="3D803B2FB535FFE7FAD5FA040466E9B0" box="[1307,1322,1495,1517]" pageId="5" value="8.0">8</number>
: 
<taxonomicName id="4C48528BB535FFE7FAFBFA0B04D0E9B1" box="[1333,1436,1495,1517]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="5" phylum="Arthropoda" rank="species" species="estrella">
<emphasis id="B93CF51AB535FFE7FAFBFA0B04D0E9B1" box="[1333,1436,1495,1517]" italics="true" pageId="5">A. estrella</emphasis>
</taxonomicName>
(male: 
<typeStatus id="54F397AAB535FFE7FF2DFA250073EA51" box="[227,319,1526,1548]" pageId="5">holotype</typeStatus>
; female: 
<typeStatus id="54F397AAB535FFE7FE68FA24034CEA51" box="[422,512,1527,1548]" pageId="5">paratype</typeStatus>
, Los Almendros, 
<collectingCountry id="F35F6998B535FFE7FD0DFA250270EA51" box="[707,828,1526,1548]" name="Costa Rica" pageId="5">Costa Rica</collectingCountry>
). 
<number id="3D803B2FB535FFE7FC9FFA25022DEA51" box="[849,865,1526,1548]" pageId="5" value="9.0">9</number>
: 
<taxonomicName id="4C48528BB535FFE7FCBFFA25028AEA56" box="[881,966,1526,1547]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="5" phylum="Arthropoda" rank="species" species="inbio">
<emphasis id="B93CF51AB535FFE7FCBFFA25028AEA56" box="[881,966,1526,1547]" italics="true" pageId="5">A. inbio</emphasis>
</taxonomicName>
(male: 
<typeStatus id="54F397AAB535FFE7FBD0FA250536EA51" box="[1054,1146,1526,1548]" pageId="5">holotype</typeStatus>
; female: 
<typeStatus id="54F397AAB535FFE7FB2FFA240477EA51" box="[1249,1339,1527,1548]" pageId="5">paratype</typeStatus>
