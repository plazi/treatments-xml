<taxonomicName id="D21874181536415D61AB59C745A13C2C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181889" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole juniperae Wilson" lsidName-HNS="Pheidole juniperae Wilson" order="Hymenoptera" pageNumber="440" phylum="Arthropoda" rank="species" species="juniperae">Pheidole juniperae</taxonomicName>
