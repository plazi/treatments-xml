(
<figureCitation id="135D2A17FF87CA7EFF36F98810DEF9D5" box="[159,253,1584,1608]" captionStart="FIGURE 20" captionStartId="32.[151,250,1957,1980]" captionTargetBox="[177,1436,204,1915]" captionTargetId="figure-56@32.[151,1436,195,1936]" captionTargetPageId="32" captionText="FIGURE 20. Radfordia (M.) golenishchevi sp. nov., male holotype. A, dorsal view; B, ventral view; C, seta m; D, genital cone. Scale bars: A, B = 100 µm; C, D = 50 µm." figureDoi="http://doi.org/10.5281/zenodo.5282115" httpUri="https://zenodo.org/record/5282115/files/figure.png" pageId="29" pageNumber="30">Figs. 20</figureCitation>
, 
<figureCitation id="135D2A17FF87CA7EFEA1F9881106F9D5" box="[264,293,1584,1608]" captionStart="FIGURE 21" captionStartId="33.[151,250,1151,1174]" captionTargetBox="[160,1429,199,1125]" captionTargetId="figure-266@33.[151,1436,193,1130]" captionTargetPageId="33" captionText="FIGURE 21. Radfordia (M.) golenishchevi sp. nov., female tritonymph. A, idiosoma in dorsal view; B, same in ventral view; C, leg IV in ventral view. Scale bars: A, B = 100 µm; C = 50 µm." figureDoi="http://doi.org/10.5281/zenodo.5282117" httpUri="https://zenodo.org/record/5282117/files/figure.png" pageId="29" pageNumber="30">21</figureCitation>
