<taxonomicName id="3EFF26851A0AF4523DF14FE81D15FF06" box="[228,339,226,252]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="species" species="undetermined">
