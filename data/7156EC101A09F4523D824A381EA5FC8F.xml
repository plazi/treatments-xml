<taxonomicName id="3EFF26851A0AF4523CA34FB71E58FF2D" box="[438,542,189,215]" class="Magnoliopsida" family="Salicaceae" genus="Salix" kingdom="Plantae" order="Malpighiales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="CB8B81141A0AF4523CA34FB71DB4FF2C" box="[438,498,189,214]" italics="true" pageId="26" pageNumber="27">Salix</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A0AF4523F394FB71EEAFF2D" box="[556,684,189,215]" class="Magnoliopsida" family="Salicaceae" kingdom="Plantae" order="Malpighiales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="family">Salicaceae</taxonomicName>
