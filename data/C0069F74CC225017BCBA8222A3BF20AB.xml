<taxonomicName id="F516E0E7A223844187870CB7E4953732" authority="S.F.Gray, 1821" authorityName="S.F.Gray" authorityYear="1821" class="Magnoliopsida" family="Ranunculaceae" genus="Consolida" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Consolida" order="Ranunculales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="genus">
