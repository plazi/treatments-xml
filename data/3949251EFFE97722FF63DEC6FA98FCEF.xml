<collectingCountry id="C9F7D498FFE87722FE98DF44FE4EFD2E" box="[364,471,616,642]" name="Australia" pageId="69" pageNumber="70">Australia</collectingCountry>
(
<collectingRegion id="73245AEAFFE87722FE13DF44FD3FFD2E" box="[487,678,616,642]" country="Australia" name="South Australia" pageId="69" pageNumber="70">South Australia</collectingRegion>
