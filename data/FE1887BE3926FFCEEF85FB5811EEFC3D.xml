<taxonomicName id="B1B14D2B3925FFCEECA4FC6C11E1FC1D" authorityName="Eaton" authorityYear="1871" box="[151,249,898,921]" class="Insecta" family="Leptophlebiidae" genus="Leptophlebia" kingdom="Animalia" order="Ephemeroptera" pageId="18" pageNumber="332" phylum="Arthropoda" rank="species" species="picteti">
