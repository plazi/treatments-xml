(
<figureCitation id="132BDB94FFF9647F47DE2BB5FF64F95E" box="[159,242,1733,1759]" captionStart="FIGURES 2–5" captionStartId="3.[151,269,1179,1203]" captionTargetBox="[168,1421,876,1146]" captionTargetId="figure-497@3.[151,1436,870,1165]" captionTargetPageId="3" captionText="FIGURES 2–5. Mentum (ventral view): 2 Helluodes devagiriensis n.sp.; 3 Physocrotaphus ceylonicus; 4 Pogonoglossus giganteus; 5 Schuelea drumonti. Scale bar = 1 mm." figureDoi="http://doi.org/10.5281/zenodo.5108348" httpUri="https://zenodo.org/record/5108348/files/figure.png" pageId="6" pageNumber="36">Figs. 2</figureCitation>
, 
<figureCitation id="132BDB94FFF9647F47BE2BB5FEDFF95E" box="[255,329,1733,1759]" captionStart="FIGURES 6–14" captionStartId="4.[151,269,1435,1459]" captionTargetBox="[175,1403,493,1408]" captionTargetId="figure-287@4.[151,1436,474,1411]" captionTargetPageId="4" captionText="FIGURES 6–14. Labrum, maxillary palpi and pronotum: 6–8 Helluodes devagiriensis n.sp.; 9–11 Helluodes taprobanae; 12–14 Helluodes westwoodi. Not scaled." figureDoi="http://doi.org/10.5281/zenodo.5108350" httpUri="https://zenodo.org/record/5108350/files/figure.png" pageId="6" pageNumber="36">6, 7, 8</figureCitation>
, 
<figureCitation id="132BDB94FFF9647F46162BB5FE34F95E" box="[343,418,1733,1759]" captionStart="FIGURES 15–18" captionStartId="8.[151,269,1462,1486]" captionTargetBox="[201,1408,184,1426]" captionTargetId="figure-234@8.[151,1436,160,1438]" captionTargetPageId="8" captionText="FIGURES 15–18. Median depression and cluster of filamentous setae on 2nd and 3rd sternites of male: 15 Physocrotaphus ceylonicus; 16 Pogonoglossus giganteus; 17 Helluodes devagiriensis n.sp.; 18 2nd and 3rd sternites of female Helluodes devagiriensis n.sp. Not scaled." figureDoi="http://doi.org/10.5281/zenodo.5108354" httpUri="https://zenodo.org/record/5108354/files/figure.png" pageId="6" pageNumber="36">17, 18</figureCitation>
, 
<figureCitation id="132BDB94FFF9647F46EE2BB5FE6CF95E" box="[431,506,1733,1759]" captionStart="FIGURES 19–22" captionStartId="10.[151,269,652,676]" captionTargetBox="[181,1410,203,601]" captionTargetId="figure-261@10.[164,1424,194,629]" captionTargetPageId="10" captionText="FIGURES 19–22. Male and female genitalia (dorsal and lateral view): 19–20 Helluodes devagiriensis n.sp.; 21–22 Physocrotaphus ceylonicus. Scale bar = 1 mm." figureDoi="http://doi.org/10.5281/zenodo.5108356" httpUri="https://zenodo.org/record/5108356/files/figure.png" pageId="6" pageNumber="36">19, 20</figureCitation>
