<paragraph id="8BE936E57631FF96FF60D8C8010F7537" blockId="11.[151,1436,510,1005]" box="[151,1048,510,537]" pageId="11" pageNumber="522">
<emphasis id="B922EAF77631FF96FF60D8C8040F7537" bold="true" box="[151,280,511,537]" pageId="11" pageNumber="522">Etymology</emphasis>
: 
<taxonomicName id="4C564D667631FF96FED0D8C904DE7537" authorityName="Calvert" authorityYear="1909" box="[295,457,510,537]" class="Insecta" family="Lestidae" genus="Lestes" kingdom="Animalia" order="Odonata" pageId="11" pageNumber="522" phylum="Arthropoda" rank="species" species="quadristriatus">
<emphasis id="B922EAF77631FF96FED0D8C904DE7537" bold="true" box="[295,457,510,537]" italics="true" pageId="11" pageNumber="522">quadristriatus</emphasis>
</taxonomicName>
