<quantity id="A3229B02497FFFDFDD69FC5BFDBF36F0" box="[518,626,896,919]" metricMagnitude="-2" metricUnit="m" metricValue="1.105" pageId="2" pageNumber="67" unit="mm" value="11.05">11.05 mm</quantity>
