<quantity id="A3229B02497FFFDFDBE9FC5BFB1136F0" box="[1158,1244,896,919]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="2" pageNumber="67" unit="mm" value="2.5">2.5 mm</quantity>
