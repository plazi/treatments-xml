<taxonomicName id="4C3A4D4945128C30719DF8B3FDF2CA3A" authority="Zhiltzova, 1988" authorityName="Zhiltzova" authorityYear="1988" box="[151,621,1887,1913]" class="Insecta" family="Nemouridae" genus="Protonemura" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="61" phylum="Arthropoda" rank="species" species="brachystyla">
