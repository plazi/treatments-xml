<taxonomicName id="AA8E4D15FFF5FFD1FE065D9CFDD3FE19" box="[426,621,432,457]" class="Mammalia" family="Sciuridae" genus="Marmota" kingdom="Animalia" order="Rodentia" pageId="24" pageNumber="55" phylum="Chordata" rank="species" species="monax">
