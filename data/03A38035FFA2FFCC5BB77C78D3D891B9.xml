<geoCoordinate id="EE3E57E4FFA0FFCC58937DC4D56E91D3" box="[465,543,1513,1534]" degrees="1.81" direction="west" orientation="longitude" pageId="19" pageNumber="265" precision="555" value="-1.81">W 1.81</geoCoordinate>
