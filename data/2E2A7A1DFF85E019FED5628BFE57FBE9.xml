<number id="104BD92CFF86E019FCC06206FCBEFCF9" box="[797,887,804,830]" pageId="86" pageNumber="87" value="286000.0">286000</number>
, 
<number id="104BD92CFF86E019FC546206FC2EFCF9" box="[905,999,804,830]" pageId="86" pageNumber="87" value="567500.0">567500</number>
”, “ 
<collectionCode id="C09253CEFF86E019FBD36206FB97FCF9" box="[1038,1118,804,830]" country="Costa Rica" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="86" pageNumber="87">INBio</collectionCode>
