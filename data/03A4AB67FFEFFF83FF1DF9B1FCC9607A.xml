<paragraph id="8BB21A71FFEFFF83FF1DF9F9FC2361AF" blockId="12.[151,926,1644,1666]" box="[151,926,1644,1666]" pageId="12" pageNumber="463">
<typeStatus id="54B6A4D3FFEFFF83FF1DF9F9FF7461AF" box="[151,201,1645,1666]" pageId="12" pageNumber="463">Type</typeStatus>
species: 
<taxonomicName id="4C0D61F2FFEFFF83FEACF9F8FD1E61AF" authority="Mortensen, 1903" authorityName="Mortensen" authorityYear="1903" box="[294,675,1644,1666]" class="Echinoidea" family="Echinothuriidae" genus="Sperosoma" kingdom="Animalia" order="Echinothurioida" pageId="12" pageNumber="463" phylum="Echinodermata" rank="species" species="koehleri">
<emphasis id="B979C663FFEFFF83FEACF9F8FE5261AF" box="[294,495,1644,1666]" italics="true" pageId="12" pageNumber="463">Sperosoma koehleri</emphasis>
