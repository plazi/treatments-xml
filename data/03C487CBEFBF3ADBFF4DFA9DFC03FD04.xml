<heading id="D09A81B1EFBF3ADBFF4DFA9DFE3AFAAC" bold="true" box="[136,511,1363,1390]" fontSize="11" level="1" pageId="238" pageNumber="239" reason="1">
<emphasis id="B919EACFEFBF3ADBFF4DFA9DFE3AFAAC" bold="true" box="[136,511,1363,1390]" pageId="238" pageNumber="239">
Genus 
<taxonomicName id="4C6D4D5EEFBF3ADBFF1AFA9DFE3AFAAC" authority="Canu, 1891" authorityName="Canu" authorityYear="1891" box="[223,511,1363,1390]" class="Maxillopoda" family="Notodelphyidae" genus="Bonnierilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="238" pageNumber="239" phylum="Arthropoda" rank="genus">
<emphasis id="B919EACFEFBF3ADBFF1AFA9DFEAFFAAF" bold="true" box="[223,362,1363,1390]" italics="true" pageId="238" pageNumber="239">Bonnierilla</emphasis>
<bibRefCitation id="EFFC4B2CEFBF3ADBFEB4FA9DFE3AFAAC" author="Canu, E." box="[369,511,1363,1389]" journalOrPublisher="Bulletin Scientifique de la France et de la Belgique" pageId="238" pageNumber="239" pagination="467 - 487" part="23" refId="ref327421" refString="Canu, E. (1891) Les Copepodes marins du Boulonnais. V. Les semiparasites. Bulletin Scientifique de la France et de la Belgique, 23, 467 - 487." title="Les Copepodes marins du Boulonnais. V. Les semiparasites" type="journal article" year="1891">Canu, 1891</bibRefCitation>
