<taxonomicName id="E3E18191FFB7FFBE1B19FFFEFB7CFE1E" baseAuthorityName="Cameron" baseAuthorityYear="1905" box="[1134,1269,418,443]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="disposita">
