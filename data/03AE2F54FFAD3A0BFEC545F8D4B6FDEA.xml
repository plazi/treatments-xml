<taxonomicName id="4C07E5C1FFAD3A0BFED444CDD7EFFE00" authority="Stys, 1975" authorityName="Stys" authorityYear="1975" box="[373,679,395,414]" class="Insecta" family="Corixidae" genus="Arctocorisa" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="374" phylum="Arthropoda" rank="species" species="armeniaca">
