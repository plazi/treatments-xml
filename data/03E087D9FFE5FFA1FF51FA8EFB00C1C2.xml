<geoCoordinate id="EE7D5008FFE5FFA1FE27FA7EFDFAC1C2" box="[481,615,1482,1509]" degrees="53,66668" direction="south" orientation="latitude" pageId="16" pageNumber="217" precision="1" value="-53.66668">53,66668°S</geoCoordinate>
