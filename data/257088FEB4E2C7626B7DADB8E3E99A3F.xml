<taxonomicName id="DDA6050E46CB75773838A6D432E33DD6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:192105" class="Insecta" family="Formicidae" genus="Atta" kingdom="Animalia" lsidName="Atta (Acromyrmex) octospinosa (Reich)" lsidName-HNS="Atta (Acromyrmex) octospinosa (Reich)" order="Hymenoptera" pageNumber="42" phylum="Arthropoda" rank="species" species="octospinosa" subGenus="Acromyrmex">Atta (Acromyrmex) octospinosa Reich</taxonomicName>
