<caption id="1CA36648FFFBE031FFC8F151FCF07A19" ID-DOI="http://doi.org/10.5281/zenodo.5214422" ID-Zenodo-Dep="5214422" httpUri="https://zenodo.org/record/5214422/files/figure.png" pageId="64" pageNumber="1274" startId="64.[125,185,1361,1380]" targetBox="[174,1126,137,1321]" targetPageId="64">
<paragraph id="486336C0FFFBE031FFC8F151FCF07A19" blockId="64.[125,1172,1361,1406]" pageId="64" pageNumber="1274">
