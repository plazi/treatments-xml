, 
<taxonomicName id="4C3570A4FFB1FF83B596FAC8FB8F3BE1" authorityName="" authorityYear="1789" box="[984,1139,692,717]" class="Magnoliopsida" family="Hypericaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="7" pageNumber="187" phylum="Tracheophyta" rank="family">Hypericaceae</taxonomicName>
