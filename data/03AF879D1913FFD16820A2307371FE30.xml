<taxonomicName id="4C064D081914FFD36B19A7EE72F6F837" authority="Steenis &amp; Hippa &amp; Mutin, 2018" authorityName="Steenis &amp; Hippa &amp; Mutin" authorityYear="2018" box="[303,730,1929,1955]" class="Insecta" family="Syrphidae" genus="Sphegina" kingdom="Animalia" order="Diptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="brevipilis" status="sp. nov." subGenus="Asiosphegina">
<emphasis id="B972EA991914FFD36B19A7EE72F6F837" box="[303,730,1929,1955]" italics="true" pageId="52" pageNumber="53">Sphegina (Asiosphegina) brevipilis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24157E21914FFD368DEA7EE737FF837" box="[744,851,1929,1955]" pageId="52" pageNumber="53" rank="species">sp. nov.</taxonomicNameLabel>
, holotype, 
<emphasis id="B972EA991914FFD369EBA7ED73DFF830" bold="true" box="[989,1011,1930,1956]" italics="true" pageId="52" pageNumber="53">♂</emphasis>
