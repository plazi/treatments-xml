<emphasis id="B923EAA7FF83FF8CFEC43179FEC9E30B" box="[276,374,272,297]" italics="true" pageId="17" pageNumber="18">Quercus</emphasis>
</taxonomicName>
spp. (
<taxonomicName id="4C574D36FF83FF8CFE113179FDE6E30B" box="[449,601,272,297]" class="Insecta" family="Gelechiidae" genus="Telphusa" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="sedulitella">
<emphasis id="B923EAA7FF83FF8CFE113179FDE6E30B" box="[449,601,272,297]" italics="true" pageId="17" pageNumber="18">T. sedulitella</emphasis>
</taxonomicName>
). 
