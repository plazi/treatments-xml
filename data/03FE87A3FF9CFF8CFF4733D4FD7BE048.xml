<taxonomicName id="4C574D36FF83FF8CFDC73109FD3FE35B" box="[535,640,352,377]" class="Insecta" family="Gelechiidae" genus="Telphusa" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
