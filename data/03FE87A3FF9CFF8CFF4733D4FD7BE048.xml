<collectingCountry id="F3407625FF83FF8CFAD03109FA23E358" box="[1280,1436,352,378]" name="United States of America" pageId="17" pageNumber="18">United States</collectingCountry>
and 
<collectingCountry id="F3407625FF83FF8CFF1B31E1FE9CE380" box="[203,291,392,418]" name="Canada" pageId="17" pageNumber="18">Canada</collectingCountry>
(
