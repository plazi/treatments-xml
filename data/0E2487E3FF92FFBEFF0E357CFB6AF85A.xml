<taxonomicName id="418D4D76FF92FFBEFEBD35C2FDB2FA49" authority="Gray, 1845: 139" authorityName="Gray" authorityPageNumber="139" authorityYear="1845" box="[292,551,1413,1436]" class="Reptilia" family="Leptotyphlopidae" genus="Epictia" kingdom="Animalia" order="Squamata" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
