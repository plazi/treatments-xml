<taxonomicName id="4C46A882BF10FFEB0B08F8069DEEACD4" authorityName="Hita Garcia &amp; Fisher" authorityYear="2012" box="[351,437,1933,1955]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="olana">
