<taxonomicName id="64EF4D359E24FFECFE03708EFDE012E6" box="[432,534,1172,1195]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="32" pageNumber="32" phylum="Arthropoda" rank="genus">
