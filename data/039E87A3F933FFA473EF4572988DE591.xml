</materialsCitation>
<materialsCitation id="3B5F3CE8F933FFA4751645E999FDE222" collectionCode="AM" location="Bridge Creek" pageId="14" pageNumber="319" specimenCode="F114643" specimenCount="1" typeStatus="paratype">
