from W885, 
<location id="8EE8606EF933FFA4743145D799FDE222" LSID="urn:lsid:plazi:treatment:039E87A3F933FFA473EF4572988DE591:8EE8606EF933FFA4743145D799FDE222" box="[1080,1228,492,515]" name="Bridge Creek" pageId="14" pageNumber="319">Bridge Creek</location>
