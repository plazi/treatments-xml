<taxonomicName id="4C064D330016FFAB9D08FA80FCE4FA42" baseAuthorityName="Fabricius" baseAuthorityYear="1794" box="[570,864,1398,1422]" class="Insecta" family="Empididae" genus="Rhamphomyia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="401" phylum="Arthropoda" rank="species" species="nigripennis" subGenus="Holoclera">
