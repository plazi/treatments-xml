<subSubSection id="C31C6528A47C186DFF64FCB6D434FB0A" pageId="24" pageNumber="25" type="materials_examined">
<paragraph id="8BB936A3A47C186DFF64FCB6D362FB92" blockId="24.[151,1437,152,1340]" pageId="24" pageNumber="25">
<emphasis id="B972EAB1A47C186DFF64FCB6D7A3FCBA" bold="true" box="[197,385,834,860]" pageId="24" pageNumber="25">
<typeStatus id="54BD8801A47C186DFF64FCB6D720FCBA" box="[197,258,834,860]" pageId="24" pageNumber="25">Type</typeStatus>
material:
</emphasis>
