<typeStatus id="54EF880EFFD22E1564AFFBDCD366CE4A" box="[199,297,1125,1149]" pageId="62" pageNumber="63">Paratype</typeStatus>
ovigerous female, 
<specimenCode id="DBF29ED7FFD22E156597FBDDD0D6CE4A" box="[511,665,1124,1149]" collectionCode="NIWA" country="New Zealand" httpUri="http://grbio.org/cool/nyq3-3g6s" name="National Institute of Water and Atmospheric Research" pageId="62" pageNumber="63" type="Other institutional type">NIWA 45704</specimenCode>
