<taxonomicName id="4C7E4D106A7E0E52FEF7F8E6FE2DF888" box="[379,497,1802,1824]" class="Insecta" family="Trogossitidae" genus="Antillipeltis" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="451" phylum="Arthropoda" rank="genus" status="gen. nov.">
