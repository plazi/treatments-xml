(syn. 
<taxonomicName id="438A4D63FFAAFF81FC2C83C9FAFBFE1F" authority=", Tingidiforma" authorityName="Tingidiforma" authorityYear="1981" box="[943,1300,417,443]" class="Insecta" family="Psyllidae" genus="Hemipteripsylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="147" phylum="Arthropoda" rank="genus">
