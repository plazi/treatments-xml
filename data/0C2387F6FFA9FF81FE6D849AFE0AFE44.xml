<taxonomicName id="438A4D63FFAAFF81FD9A83C9FC83FE1F" authority="Kuwayama, 1931" authorityName="Kuwayama" authorityYear="1931" box="[537,876,417,443]" class="Insecta" family="Aphalaridae" genus="Togepsylla" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="147" phylum="Arthropoda" rank="genus">
