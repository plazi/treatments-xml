<taxonomicName id="4C139E97E438FFE7EDB6D56EFC98FDFD" box="[735,854,562,588]" class="Arachnida" family="Pholcidae" kingdom="Animalia" order="Araneae" pageId="85" pageNumber="86" phylum="Arthropoda" rank="family">Pholcidae</taxonomicName>
