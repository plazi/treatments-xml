<taxonomicName id="4C139E97E438FFE7EF8ED509FDDCFDC1" authority="Huber 2009" authorityName="Huber" authorityYear="2009" box="[231,530,597,624]" class="Arachnida" family="Filistatidae" kingdom="Animalia" order="Araneae" pageId="85" pageNumber="86" phylum="Arthropoda" rank="family">
