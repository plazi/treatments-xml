<collectingCountry id="81B966E71049FFEEA8A379D29E2D1EC9" box="[996,1095,198,219]" name="Brazil" pageId="33" pageNumber="105">BRAZIL</collectingCountry>
: 2f, 
<collectorName id="545B43A11049FFEEAFCA79D29E911ECE" box="[1165,1275,198,220]" pageId="33" pageNumber="105">S. Paulo</collectorName>
