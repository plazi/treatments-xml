<taxonomicName id="4C0C4D76FFC1FFDFFB170E63D99FF895" box="[1173,1238,1888,1913]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="3" pageNumber="37" phylum="Mollusca" rank="genus">
<emphasis id="B978EAE7FFC1FFDFFB170E63D99FF895" box="[1173,1238,1888,1913]" italics="true" pageId="3" pageNumber="37">Ficus</emphasis>
</taxonomicName>
sp., 
<date id="FFB21035FFC1FFDFFA900E63D8D5F896" box="[1298,1436,1888,1914]" pageId="3" pageNumber="37" value="1916-12-20">
<number id="3DC424D2FFC1FFDFFA900E63D879F896" box="[1298,1328,1888,1914]" pageId="3" pageNumber="37" value="20.0">20</number>
-xii- 
<number id="3DC424D2FFC1FFDFFAE30E63D8D5F896" box="[1377,1436,1888,1914]" pageId="3" pageNumber="37" value="1916.0">1916</number>
</date>
(P.v.d. Goot) (
<collectionCode id="ED1DAE30FFC1FFDFFEB80E8BDCD0F84E" box="[314,409,1928,1954]" pageId="3" pageNumber="37">BMNH</collectionCode>
).
