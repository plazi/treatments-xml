<taxonomicName id="A9F97B985C6D6A69573AD3BBC8540477" class="Arachnida" family="Nemesiidae" genus="Raveniola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Raveniola shangrila" order="Araneae" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="shangrila">Raveniola shangrila</taxonomicName>
