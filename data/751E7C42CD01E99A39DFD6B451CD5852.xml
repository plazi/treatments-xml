<taxonomicName id="4226994B03FBA753C386B5B52A4D4879" class="Arachnida" family="Nemesiidae" genus="Raveniola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Raveniola spirula" order="Araneae" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="spirula">Raveniola spirula</taxonomicName>
