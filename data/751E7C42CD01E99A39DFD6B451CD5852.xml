<taxonomicName id="B8150823F9D1D0A8565ABC03AD8A794D" class="Arachnida" family="Nemesiidae" genus="Raveniola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Raveniola xizangensis" order="Araneae" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="xizangensis">Raveniola xizangensis</taxonomicName>
