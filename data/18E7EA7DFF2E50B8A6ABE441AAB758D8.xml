<taxonomicName id="2052F26EB7F5C87DB9967355510330EC" class="Liliopsida" family="Orchidaceae" genus="Cranichis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cranichis ciliata" order="Asparagales" pageId="0" pageNumber="11" phylum="Tracheophyta" rank="species" species="ciliata">
<emphasis id="4D48D6C8A8A7885428CB7F4A2B63FAAA" italics="true" pageId="0" pageNumber="11">Cranichis ciliata</emphasis>
