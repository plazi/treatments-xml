<taxonomicName id="4C424D7BFFE9FFB13C86426ED9964223" box="[684,931,1087,1112]" class="Clitellata" family="Glossoscolecidae" genus="Glossoscolex" kingdom="Animalia" order="Haplotaxida" pageId="15" pageNumber="74" phylum="Annelida" rank="species" species="sanpedroensis" subGenus="Glossoscolex">
