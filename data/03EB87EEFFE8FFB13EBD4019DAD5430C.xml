<quantity id="4CBA9B1DFFE9FFB13CED424AD9224207" box="[711,791,1124,1148]" metricMagnitude="-2" metricUnit="m" metricValue="3.9" pageId="15" pageNumber="74" unit="mm" value="39.0">39 mm</quantity>
