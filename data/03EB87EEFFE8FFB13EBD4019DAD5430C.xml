<taxonomicName id="4C424D7BFFE9FFB13A3045D6DBE9424F" authority="Cordero, 1943" authorityName="Cordero" authorityYear="1943" class="Clitellata" family="Glossoscolecidae" genus="Glossoscolex" kingdom="Animalia" order="Haplotaxida" pageId="15" pageNumber="74" phylum="Annelida" rank="subSpecies" species="uruguayensis" subGenus="Glossoscolex" subSpecies="uruguayensis">
