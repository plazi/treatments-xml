<paragraph id="8BEE36C0FFA0FFD4FF396459305C7D0B" blockId="4.[189,1180,1312,1395]" pageId="4" pageNumber="217">
<taxonomicName id="4C514D43FFA0FFD4FF39645935D07D40" box="[189,408,1312,1336]" class="Insecta" family="Scirtidae" genus="Indiocyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="217" phylum="Arthropoda" rank="species" species="rufopacus">
