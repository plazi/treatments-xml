<taxonomicName id="CE4E5A9A20FFE8BE13C0656D6B0EE2F9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29407" class="Insecta" family="Formicidae" genus="Harpegnathos" kingdom="Animalia" lsidName-HNS="Harpegnathos saltator Jerdon" order="Hymenoptera" pageId="82" pageNumber="82" phylum="Arthropoda" rank="species" species="saltator">Harpegnathos saltator, Jerdon</taxonomicName>
