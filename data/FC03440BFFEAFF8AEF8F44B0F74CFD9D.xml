<taxonomicName id="B3AA8E9EFFEAFF8AEB134B91FB76F6DC" authorityName="Pocock" authorityYear="1908" box="[1032,1189,2294,2327]" class="Mammalia" family="Viverridae" genus="Poiana" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="215" phylum="Chordata" rank="species" species="leightoni">P. leightoni</taxonomicName>
