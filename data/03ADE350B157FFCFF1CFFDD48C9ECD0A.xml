<taxonomicName id="4C0429C5B156FFCFF40CFE8A8DD8CD4B" authorityName="De Prins" authorityYear="2012" box="[1364,1426,281,302]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="kazuri">kazuri</taxonomicName>
