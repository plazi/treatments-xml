<typeStatus id="3D6D886BFF80FF8AE797FD1B54B8FD30" box="[677,725,733,756]" pageId="7" pageNumber="356">TYPE</typeStatus>
GENUS. — 
<taxonomicName id="25D64D4AFF80FF8AE673FD1A5225FD36" authority="Pchelintsev, 1965" authorityName="Pchelintsev" authorityYear="1965" box="[833,1096,732,756]" class="Gastropoda" family="Gastropoda (awaiting allocation)" genus="Umbonea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda (awaiting allocation)" pageId="7" pageNumber="356" phylum="Mollusca" rank="genus">
<emphasis id="D0A2EADBFF80FF8AE673FD1A55F6FD36" box="[833,923,732,756]" italics="true" pageId="7" pageNumber="356">Umbonea</emphasis>
<bibRefCitation id="86474B38FF80FF8AE692FD1A5225FD36" author="PCHELINTSEV V. F." box="[928,1096,732,756]" pageId="7" pageNumber="356" refId="ref20970" refString="PCHELINTSEV V. F. 1965. - Mesozoic Murchisoniata from the Crimean Highland. Nauka, Moskwa, 216 p." type="book" year="1965">Pchelintsev, 1965</bibRefCitation>
</taxonomicName>
(
<bibRefCitation id="86474B38FF80FF8AE167FD1A52C3FD36" author="PCHELINTSEV V. F." box="[1109,1198,732,756]" pageId="7" pageNumber="356" refId="ref20970" refString="PCHELINTSEV V. F. 1965. - Mesozoic Murchisoniata from the Crimean Highland. Nauka, Moskwa, 216 p." type="book" year="1965">1965: 85</bibRefCitation>
