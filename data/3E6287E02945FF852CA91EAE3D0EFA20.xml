<number id="000324D12947FF852F091BDB3C59FC35" box="[823,872,896,921]" pageId="10" pageNumber="253" value="12.5">12.5</number>
<date id="C27510362947FF852F511BDA3CABFC35" box="[879,922,896,921]" pageId="10" pageNumber="253">
x 
