<emphasis id="84BFEAE42947FF852EE11B063C7DFCDA" box="[735,844,861,886]" italics="true" pageId="10" pageNumber="253">Internode</emphasis>
: length 
<number id="000324D12947FF852F851B063B1FFCDA" box="[955,1070,861,886]" pageId="10" pageNumber="253" value="940.0" valueMax="1240.0" valueMin="640.0">640–1240</number>
, diameter 
<number id="000324D12947FF8528881B063A2AFCDA" box="[1206,1307,861,886]" pageId="10" pageNumber="253" value="290.0" valueMax="350.0" valueMin="230.0">230–350</number>
