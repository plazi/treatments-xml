<taxonomicName id="4C011396D21B0255FD90F953537935A1" box="[569,635,1703,1727]" class="Bacilli" family="Lactobacillaceae" genus="Bombilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="20" pageNumber="2802" phylum="Firmicutes" rank="species" species="bombi">
