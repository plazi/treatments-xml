<taxonomicName id="AFD9A596122FFFA2FD42F846FCCEF80B" authority="Monts" authorityName="Monts" box="[641,875,1974,1999]" class="Gastropoda" family="Eulimidae" genus="Eulima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="85" pageNumber="86" phylum="Mollusca" rank="species" species="intermedia">A. intermedia Monts</taxonomicName>
