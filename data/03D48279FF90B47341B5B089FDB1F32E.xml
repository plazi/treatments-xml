<geoCoordinate id="EE4955A8FF91B47045DEB755FAC7F606" box="[1276,1334,258,285]" degrees="4" direction="south" minutes="25" orientation="latitude" pageId="5" pageNumber="377" precision="925" value="-4.4166665">
