<taxonomicName id="4C689BFAFFFFFFE1E37E45DC66CBE590" authority="Timm, 1964" authorityName="Timm" authorityYear="1964" box="[261,643,1423,1446]" class="Adenophorea" family="Tylencholaimidae" genus="Tantunema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="8" pageNumber="8" phylum="Nematoda" rank="species" species="pakistanense" status="comb. nov.">
<emphasis id="B91C3C6BFFFFFFE1E37E45DC65BFE590" box="[261,503,1423,1446]" italics="true" pageId="8" pageNumber="8">
Tantunema 
<taxonomicName id="4C689BFAFFFFFFE1E30045DC66CBE590" authority="Timm, 1964" authorityName="Timm" authorityYear="1964" box="[379,643,1423,1446]" pageId="8" pageNumber="8" rank="species" species="pakistanense" status="comb. nov.">
<taxonomicName id="4C689BFAFFFFFFE1E30045DC65BFE590" box="[379,503,1423,1446]" pageId="8" pageNumber="8" rank="species" species="pakistanense">pakistanense</taxonomicName>
