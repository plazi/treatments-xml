<location id="8106600DA949BE267240FD5BFADDFD5F" LSID="urn:lsid:plazi:treatment:0C7087C0A94BBE2676BEFF1BFCABFA33:8106600DA949BE267240FD5BFADDFD5F" box="[1129,1291,727,753]" country="Costa Rica" name="Playa Panama" pageId="6" pageNumber="482" stateProvince="Guanacaste">Playa Panamá</location>
, 
<date id="F0671016A949BE26733FFD5BFAA9FD5F" box="[1302,1407,727,753]" pageId="6" pageNumber="482" value="2014-12">
<collectingDate id="E023E9FEA949BE26733FFD5BFAA9FD5F" box="[1302,1407,727,753]" pageId="6" pageNumber="482" value="2014-12">XII.2014</collectingDate>
</date>
, 
<collectorName id="292C5300A949BE2673A3FD5BFF33FCBB" pageId="6" pageNumber="482">J. Vlasak</collectorName>
