<taxonomicName id="C8AF4D42FF8EFFA7142A92B6AB8294E7" authority="Zamani &amp; Marusik" authorityName="Zamani &amp; Marusik" authorityYear="2020" box="[280,666,1924,1948]" class="Arachnida" family="Scytodidae" genus="Scytodes" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="158" phylum="Arthropoda" rank="species" species="kumonga">
