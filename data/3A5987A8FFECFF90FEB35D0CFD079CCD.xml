</taxonomicName>
<taxonomicName id="75F04D3DFFEDFF93FDD55C33FD359B13" authorityName="Mound &amp; Masumoto" authorityYear="2005" box="[622,718,1323,1344]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="wellsae">
<emphasis id="8084EAACFFEDFF93FDD55C33FD359B13" box="[622,718,1323,1344]" italics="true" pageId="57" pageNumber="58">T. wellsae</emphasis>
