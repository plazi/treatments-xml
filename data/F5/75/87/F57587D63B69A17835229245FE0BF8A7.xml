<number box="[660,675,1799,1825]" pageId="4" pageNumber="323" value="8.0">8</number>
: 
<quantity box="[680,726,1799,1825]" metricMagnitude="-1" metricUnit="m" metricValue="2.286" pageId="4" pageNumber="323" unit="in" value="9.0">9 in</quantity>
female, 
