<collectorName pageId="12" pageNumber="109">N. San Gabriel Mixlepec</collectorName>
, 
<quantity box="[262,361,800,827]" metricMagnitude="3" metricUnit="m" metricValue="1.59" pageId="12" pageNumber="109" unit="m" value="1590.0">
<elevation box="[262,361,800,827]" metricMagnitude="3" metricUnit="m" metricValue="1.59" pageId="12" pageNumber="109" unit="m" value="1590.0">1590 m</elevation>
</quantity>
., 
<date box="[380,514,800,826]" pageId="12" pageNumber="109" value="1991-07-10">
<collectingDate box="[380,514,800,826]" pageId="12" pageNumber="109" value="1991-07-10">10.vii.1991</collectingDate>
</date>
, 
<collectorName box="[530,649,801,827]" pageId="12" pageNumber="109">J. Kemner</collectorName>
and 
<collectorName box="[712,852,800,827]" pageId="12" pageNumber="109">H. Romack</collectorName>
, coll. (1 ♁). [
<collectionCode box="[1015,1103,800,826]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34802" lsid="urn:lsid:biocol.org:col:34802" name="Natural History Museum of Los Angeles County" pageId="12" pageNumber="109" type="Museum">LACM</collectionCode>
; 
<collectionCode box="[140,207,836,862]" country="Canada" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="12" pageNumber="109">CNC</collectionCode>
; 
