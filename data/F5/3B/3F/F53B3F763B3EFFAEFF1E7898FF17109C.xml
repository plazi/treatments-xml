<date box="[151,278,1979,2003]" pageId="17" pageNumber="18" value="1931-04-25">
<number box="[151,180,1979,2003]" pageId="17" pageNumber="18" value="25.0">25</number>
.IV.1931
</date>
, 4Æ¤, 
<number box="[338,373,1979,2003]" pageId="17" pageNumber="18" value="33.0">33</number>
, Yasumatsu; KabiâWakayama, 
<date box="[729,856,1979,2003]" pageId="17" pageNumber="18" value="1969-04-20">
<number box="[729,759,1979,2003]" pageId="17" pageNumber="18" value="20.0">20</number>
.IV.1969
</date>
, 1Æ¤, 
<date box="[916,1049,1979,2003]" pageId="17" pageNumber="18" value="1969-04-12">
<number box="[916,947,1979,2003]" pageId="17" pageNumber="18" value="12.0">12</number>
. IV.1969
</date>
, 13, M. Matsumura (
<collectionCode box="[1292,1365,1979,2003]" pageId="17" pageNumber="18">IZAN</collectionCode>
).
</paragraph>
