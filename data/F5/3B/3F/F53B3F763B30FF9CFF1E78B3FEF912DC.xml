/ 
<number box="[1415,1431,332,356]" pageId="33" pageNumber="34" value="2.0">2</number>
, T 
<number box="[168,182,367,391]" pageId="33" pageNumber="34" value="2.0">2</number>
and T 
<number box="[257,269,367,391]" pageId="33" pageNumber="34" value="3.0">3</number>
ca 
<number box="[312,326,367,391]" pageId="33" pageNumber="34" value="4.0">4</number>
/ 
<number box="[335,347,367,391]" pageId="33" pageNumber="34" value="5.0">5</number>
