<taxonomicName box="[1001,1109,1229,1256]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="503" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
) in 
<collectingCountry box="[1152,1214,1229,1255]" name="Israel" pageId="8" pageNumber="503">Israel</collectingCountry>
