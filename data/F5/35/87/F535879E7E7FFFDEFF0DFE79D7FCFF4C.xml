<taxonomicName authorityName="Cook" authorityYear="1967" box="[506,686,1538,1563]" class="Arachnida" family="Torrenticolidae" genus="Neoatractides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="27" pageNumber="28" phylum="Arthropoda" rank="subGenus" subGenus="Allotorrenticola">
<emphasis box="[506,686,1538,1563]" italics="true" pageId="27" pageNumber="28">Allotorrenticola</emphasis>
</taxonomicName>
includes six species from Asia, i.e., 
<taxonomicName authority="(Lundblad, 1941)" baseAuthorityName="Lundblad" baseAuthorityYear="1941" class="Arachnida" family="Torrenticolidae" genus="Neoatractides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="abnormipalpis">
<emphasis box="[1107,1435,1538,1563]" italics="true" pageId="27" pageNumber="28">Neoatractides abnormipalpis</emphasis>
(
<bibRefCitation author="Lundblad, O." box="[158,341,1574,1599]" pageId="27" pageNumber="28" pagination="97 - 121" refId="ref37405" refString="Lundblad, O. (1941) Neue Wassermilben. Vorlaufige Mitteilung. Entomologisk Tidskrift, 62, 97 - 121." type="journal article" year="1941">Lundblad, 1941</bibRefCitation>
)
</taxonomicName>
(
<collectingCountry box="[367,448,1574,1599]" name="Myanmar" pageId="27" pageNumber="28">Burma</collectingCountry>
, 
<collectingCountry box="[462,568,1574,1599]" name="Malaysia" pageId="27" pageNumber="28">Malaysia</collectingCountry>
, 
<collectingCountry box="[582,662,1574,1599]" name="Brunei" pageId="27" pageNumber="28">Brunei</collectingCountry>
; 
<bibRefCitation author="Wiles, P. R." box="[676,811,1574,1599]" pageId="27" pageNumber="28" pagination="191 - 236" refId="ref38559" refString="Wiles, P. R. (1997) Asian and Oriental Torrenticolidae Piersig, 1902 (Acari: Hydrachnidia: Lebertioidea): a revision of the family and description of new species of Torrenticola Piersig and Pseudotorrenticola Walter, from Southeast Asia. Journal of Natural History, 31, 191 - 236. http: // dx. doi. org / 10.1080 / 00222939700770121" type="journal article" year="1997">Wiles 1997</bibRefCitation>
), 
<taxonomicName authority="(Wiles, 1991)" baseAuthorityName="Wiles" baseAuthorityYear="1991" box="[833,1115,1574,1599]" class="Arachnida" family="Torrenticolidae" genus="Neoatractides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="bahtilli">
<emphasis box="[833,949,1574,1599]" italics="true" pageId="27" pageNumber="28">N. bahtilli</emphasis>
(
<bibRefCitation author="Wiles, P. R." box="[967,1107,1574,1599]" pageId="27" pageNumber="28" pagination="41 - 56" refId="ref38490" refString="Wiles, P. R. (1991) Rheophilic watermites (Acari: Hydrachnidia) from mainland Malaysia. Acarologia, 32 (1), 41 - 56." type="journal article" year="1991">Wiles, 1991</bibRefCitation>
)
</taxonomicName>
(
<collectingCountry box="[1133,1240,1574,1599]" name="Malaysia" pageId="27" pageNumber="28">Malaysia</collectingCountry>
, 
