<taxonomicName authority="Pic, 1916" authorityName="Pic" authorityYear="1916" class="Insecta" family="Ciidae" genus="Falsocis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Falsocis" order="Coleoptera" pageId="14" pageNumber="73" phylum="Arthropoda" rank="genus">Falsocis</taxonomicName>
