<number box="[1224,1267,1691,1715]" pageId="14" pageNumber="15" value="325.0">325</number>
; extended ventral shield including coxae, genital field, excretory pore, all ventralia and Vgl, L/W 
<number box="[1129,1172,1727,1751]" pageId="14" pageNumber="15" value="428.0">428</number>
/ 
<number box="[1181,1224,1727,1751]" pageId="14" pageNumber="15" value="366.0">366</number>
