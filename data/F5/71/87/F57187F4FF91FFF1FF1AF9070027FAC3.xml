<number box="[1221,1264,1763,1787]" pageId="14" pageNumber="15" value="113.0">113</number>
, L Ac 
<number box="[1341,1381,1763,1787]" pageId="14" pageNumber="15" value="2.0" valueMax="3.0" valueMin="1.0">1-3</number>
