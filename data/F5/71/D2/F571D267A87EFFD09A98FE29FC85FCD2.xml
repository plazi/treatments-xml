</taxonomicName>
und 
<taxonomicName baseAuthorityName="Jordan" baseAuthorityYear="1894" box="[552,772,773,794]" class="Insecta" family="Cerambycidae" genus="Rhaphuma" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="427" phylum="Arthropoda" rank="species" species="reticulata">
