<treatmentCitation box="[398,680,1873,1895]" pageId="30" pageNumber="163">
<taxonomicName authority="MAYR, 1861" authorityName="MAYR" authorityYear="1861" box="[398,680,1873,1895]" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="163" phylum="Arthropoda" rank="genus">
