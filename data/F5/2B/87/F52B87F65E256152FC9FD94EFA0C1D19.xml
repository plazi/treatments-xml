<taxonomicName authorityName="LINNAEUS" authorityYear="1761" box="[1300,1375,977,998]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="152" phylum="Arthropoda" rank="species" species="rufa">
