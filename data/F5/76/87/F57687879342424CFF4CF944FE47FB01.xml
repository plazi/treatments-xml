<quantity box="[362,464,1765,1792]" metricMagnitude="1" metricUnit="m" metricValue="3.5" metricValueMax="4.5" metricValueMin="2.5" pageId="33" pageNumber="34" unit="m" value="35.0" valueMax="45.0" valueMin="25.0">25–45 m</quantity>
