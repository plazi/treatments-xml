<date box="[1119,1287,1394,1420]" pageId="8" pageNumber="37" value="2019-05-21">
<collectingDate box="[1119,1287,1394,1420]" pageId="8" pageNumber="37" value="2019-05-21">May 21, 2019</collectingDate>
