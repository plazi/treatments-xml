<taxonomicName authority="Kormilev, 1981" authorityName="Kormilev" authorityYear="1981" box="[370,773,1105,1129]" class="Insecta" family="Aradidae" genus="Carventus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="chilensis">
