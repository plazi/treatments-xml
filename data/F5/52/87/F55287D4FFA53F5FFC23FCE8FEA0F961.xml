<taxonomicName authority="Kovařík &amp; Lowe &amp; Elmi &amp; Šťáhlavský, 2019" authorityName="Kovařík &amp; Lowe &amp; Elmi &amp; Šťáhlavský" authorityYear="2019" box="[292,490,2020,2041]" class="Arachnida" family="Buthidae" genus="Parabuthus" kingdom="Animalia" order="Scorpiones" pageId="37" pageNumber="36" phylum="Arthropoda" rank="species" species="mazuchi" status="sp. n.">
