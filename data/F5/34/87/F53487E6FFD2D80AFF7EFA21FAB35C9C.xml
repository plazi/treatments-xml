<quantity box="[677,798,1921,1946]" metricMagnitude="-2" metricUnit="m" metricValue="2.95" metricValueMax="3.3" metricValueMin="2.6" pageId="17" pageNumber="508" unit="mm" value="29.5" valueMax="33.0" valueMin="26.0">26–33 mm</quantity>
