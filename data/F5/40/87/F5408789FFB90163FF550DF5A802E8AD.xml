i
</date>
(
<number box="[1044,1082,1581,1607]" pageId="3" pageNumber="40" value="1.5">1.5</number>
), 
<number box="[1106,1123,1581,1607]" pageId="3" pageNumber="40" value="2.0">2</number>
e(
<number box="[1149,1160,1581,1607]" pageId="3" pageNumber="40" value="1.0">1</number>
), 
<date box="[1184,1207,1581,1607]" pageId="3" pageNumber="40">
