i
</date>
(
<number box="[1217,1234,1581,1607]" pageId="3" pageNumber="40" value="2.0">2</number>
), 
<number box="[1257,1271,1581,1607]" pageId="3" pageNumber="40" value="3.0">3</number>
e(
<number box="[1295,1334,1581,1607]" pageId="3" pageNumber="40" value="1.5">1.5</number>
