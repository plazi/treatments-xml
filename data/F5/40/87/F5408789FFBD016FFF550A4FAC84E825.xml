<emphasis box="[1147,1324,1398,1421]" italics="true" pageId="8" pageNumber="45">
<taxonomicName box="[1147,1234,1398,1421]" class="Amphibia" family="Mantellidae" genus="Boophis" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="8" pageNumber="45" phylum="Chordata" rank="genus">Boophis</taxonomicName>
