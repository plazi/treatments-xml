collection data is: 
<collectingCountry box="[151,276,1694,1720]" name="Bolivia" pageId="2" pageNumber="31">BOLIVIA</collectingCountry>
: 
<collectingRegion box="[289,572,1694,1720]" country="Bolivia" name="Santa Cruz" pageId="2" pageNumber="31">Santa Cruz Department</collectingRegion>
, 
<collectingMunicipality box="[586,787,1694,1720]" pageId="2" pageNumber="31">Florida Province</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:F5088797AE4DFF86FF77A4E8FA4A04EA:787E605AAE4EFF86FCC1A0ACFBFA05DF" box="[801,1066,1694,1720]" country="Bolivia" county="Its" latitude="-18.1" longLatPrecision="1276" longitude="-63.6" municipality="Florida Province" name="Refugio Los Volcanes" pageId="2" pageNumber="31" stateProvince="Santa Cruz">Refugio Los Volcanes</location>
, 
<locationDeviation box="[1080,1321,1694,1720]" location="Bermejo" pageId="2" pageNumber="31">
<quantity box="[1080,1142,1694,1720]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="2" pageNumber="31" unit="km" value="4.0">4 km</quantity>
N of Bermejo
</locationDeviation>
, 
<geoCoordinate box="[1335,1431,1691,1720]" degrees="18" direction="south" minutes="06" orientation="latitude" pageId="2" pageNumber="31" precision="925" value="-18.1">18°06'S</geoCoordinate>
, 
<geoCoordinate box="[151,256,1734,1762]" degrees="63" direction="west" minutes="36" orientation="longitude" pageId="2" pageNumber="31" precision="925" value="-63.6">63°36'W</geoCoordinate>
, 1045 meters, 
<date box="[429,630,1736,1762]" pageId="2" pageNumber="31" value="2007-10-31">
<collectingDate box="[429,630,1736,1762]" pageId="2" pageNumber="31" value="2007-10-31">31 October 2007</collectingDate>
</date>
, 
<collectorName box="[644,819,1736,1762]" pageId="2" pageNumber="31">N. E. Woodley</collectorName>
(
<collectionCode box="[836,928,1736,1762]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="2" pageNumber="31" type="Museum">USNM</collectionCode>
