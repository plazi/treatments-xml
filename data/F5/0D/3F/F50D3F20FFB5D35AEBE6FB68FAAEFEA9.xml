<paragraph blockId="25.[151,1437,1289,1926]" pageId="25" pageNumber="176">
<emphasis bold="true" box="[199,294,1361,1386]" pageId="25" pageNumber="176">Habitus</emphasis>
. BL 
<quantity box="[365,460,1361,1386]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="25" pageNumber="176" unit="mm" value="2.6">2.6 mm</quantity>
