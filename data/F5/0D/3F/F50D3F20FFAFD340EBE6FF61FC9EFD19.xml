<taxonomicName authorityName="Klausnitzer" authorityYear="2009" box="[326,473,1800,1822]" class="Insecta" family="Scirtidae" genus="Ypsiloncyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="genus">
<emphasis box="[326,473,1800,1822]" italics="true" pageId="5" pageNumber="156">Ypsiloncyphon</emphasis>
</taxonomicName>
spp. 
<taxonomicName authority="(Klausnitzer)" authorityName="Klausnitzer" box="[532,812,1800,1823]" class="Insecta" family="Scirtidae" genus="Cyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="chlorizans">
