<taxonomicName authority="Zwick" authorityName="Zwick" authorityYear="2014" box="[1009,1185,1862,1884]" class="Insecta" family="Scirtidae" genus="Ypsiloncyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="longus">
