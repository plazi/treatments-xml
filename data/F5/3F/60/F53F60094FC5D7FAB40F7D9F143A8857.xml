<normalizedToken originalValue="Blütenstand" pageId="null" pageNumber="615">Bluetenstand</normalizedToken>
<normalizedToken originalValue="vielblütig" pageId="null" pageNumber="615">vielbluetig</normalizedToken>
(20-40 
<normalizedToken originalValue="Blüten" pageId="null" pageNumber="615">Blueten</normalizedToken>
); Lippe 
<normalizedToken originalValue="gelbgrün" pageId="null" pageNumber="615">gelbgruen</normalizedToken>
