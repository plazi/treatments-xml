<taxonomicName id="F0AB4D37FFF83A3A0BB6FC769541AB55" authorityName="Mey" authorityYear="2017" box="[713,874,1016,1043]" class="Insecta" family="Philopteridae" genus="Priceiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="22" pageNumber="274" phylum="Arthropoda" rank="species" species="sikkimensis">
