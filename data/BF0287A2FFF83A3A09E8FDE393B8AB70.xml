and 
<taxonomicName id="F0AB4D37FFF83A3A0DB2FC5A9467AB55" authority="Gustafsson et al., 2018 b" authorityName="Gustafsson" authorityYear="2018" class="Insecta" family="Philopteridae" genus="Priceiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="22" pageNumber="274" phylum="Arthropoda" rank="species" species="macrocephala" subGenus="Thescelovora">
