<accessionNumber id="94578BF2FFC01C3B3529FD27FC5EFD10" box="[860,992,729,755]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/MN982362" pageId="25" pageNumber="226">MN982362</accessionNumber>
, 
<accessionNumber id="94578BF2FFC01C3B3599FD27FBCEFD10" box="[1004,1136,729,755]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/MN982363" pageId="25" pageNumber="226">MN982363</accessionNumber>
