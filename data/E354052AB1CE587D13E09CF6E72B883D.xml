<taxonomicName id="039A08AA898F4BE545799F8D1CCE27AC" class="Choanoflagellatea" family="Codosigidae" genus="Kentia" higherTaxonomySource="CoL" kingdom="Protozoa" lsidName="Kentia" order="Choanoflagellida" pageId="2" pageNumber="3" phylum="Choanozoa" rank="genus">Kentia</taxonomicName>
