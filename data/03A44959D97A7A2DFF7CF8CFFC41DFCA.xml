<quantity id="4CF555AAD97D7A2DFCD0FE8BFC07DF92" box="[827,924,368,395]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="12" pageNumber="535" unit="mm" value="4.0" valueMax="5.0" valueMin="3.0">
