<collectionCode id="E08AB283FFFDFF95FECAB849FE963EAC" box="[292,374,871,897]" country="Brazil" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="5" pageNumber="34" type="Museum">MZSP</collectionCode>
)
