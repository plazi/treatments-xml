<subSubSection id="0FA9C4C8FFD2BD63E9A6FE881B32FD0B" pageId="11" pageNumber="36" type="materials_examined">
<paragraph id="470C9743FFD2BD63E9A6FE881C66FE14" blockId="11.[151,1437,366,895]" box="[151,1327,366,392]" pageId="11" pageNumber="36">
<emphasis id="75C74B51FFD2BD63E9A6FE881842FE14" bold="true" box="[151,267,366,392]" pageId="11" pageNumber="36">Material.</emphasis>
<collectingCountry id="3FA4D7D3FFD2BD63E822FE8818C7FE14" box="[275,398,366,392]" name="Venezuela" pageId="11" pageNumber="36">Venezuela</collectingCountry>
