<taxonomicName id="4C5C13B6FFBD1A750B6DFE32FDEA78DB" authority="Bitner &amp; Cohen, 2015" authorityName="Bitner &amp; Cohen" authorityYear="2015" box="[368,541,386,407]" class="Rhynchonellata" family="Terebratulidae" genus="Ebiscothyris" kingdom="Animalia" order="Terebratulida" pageId="8" pageNumber="494" phylum="Brachiopoda" rank="genus" status="GEN. NOV.">EBISCOTHYRIS</taxonomicName>
<emphasis id="B928B427FFBD1A75083EFE35FD6178DA" bold="true" box="[547,662,386,407]" pageId="8" pageNumber="494">
<taxonomicNameLabel id="A21B095CFFBD1A75083EFE35FD6178DA" box="[547,662,386,407]" pageId="8" pageNumber="494" rank="genus">GEN. NOV.</taxonomicNameLabel>
