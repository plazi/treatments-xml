and 
<accessionNumber id="943531C9F32EFFC684B5B0F8FCCBFDDF" box="[658,774,505,530]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/KJ887545" pageId="18" pageNumber="99">KJ887545</accessionNumber>
