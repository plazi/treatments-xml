<taxonomicName authorityName="Loew. The" authorityYear="1857" box="[370,487,1177,1201]" class="Insecta" family="Hybotidae" genus="Syndyas" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="18" phylum="Arthropoda" rank="genus">
