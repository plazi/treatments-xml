</taxonomicName>
sp. (
<taxonomicName box="[895,1010,188,212]" class="Insecta" kingdom="Animalia" order="Mantodea" pageId="6" pageNumber="299" phylum="Arthropoda" rank="order">Mantodea</taxonomicName>
) (
<figureCitation box="[1033,1097,188,212]" captionStart="FIGURE 1" captionStartId="1.[151,250,1979,2002]" captionTargetBox="[192,1413,1200,1932]" captionTargetId="figure@1.[151,1436,1167,1957]" captionTargetPageId="1" captionText="FIGURE 1. Specimens of Chordodes with their hosts. (A) C. moutoni with its host, Hierodula sp. (B) C. lasuboni n. sp. with its host, Hierodula sp." httpUri="https://zenodo.org/record/218971/files/figure.png" pageId="6" pageNumber="299">
