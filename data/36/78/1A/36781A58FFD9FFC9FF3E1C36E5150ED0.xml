and 
<taxonomicName authority="(Rudolphi, 1819)" baseAuthorityName="Rudolphi" baseAuthorityYear="1819" box="[136,472,252,274]" class="Archiacanthocephala" family="Giganthorhynchidae" genus="Mediorhynchus" kingdom="Animalia" order="Gigantorhynchida" pageId="6" pageNumber="267" phylum="Acanthocephala" rank="species" species="micracanthus">
