<subSubSection pageId="6" pageNumber="597" type="materials_examined">
<paragraph blockId="6.[151,1436,151,1040]" pageId="6" pageNumber="597">
<emphasis bold="true" box="[199,389,836,861]" pageId="6" pageNumber="597">
<typeStatus box="[199,258,836,861]" pageId="6" pageNumber="597">Type</typeStatus>
materials.
</emphasis>
<typeStatus box="[402,509,836,861]" pageId="6" pageNumber="597">Holotype</typeStatus>
, gender unknown, CNU-HYM-LB 
