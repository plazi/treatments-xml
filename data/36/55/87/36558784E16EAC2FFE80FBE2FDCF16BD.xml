<taxonomicName authority="Dominguez" authorityName="Dominguez" box="[682,914,1809,1832]" class="Insecta" family="Leptohyphidae" genus="Haplohyphes" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="baritu">
