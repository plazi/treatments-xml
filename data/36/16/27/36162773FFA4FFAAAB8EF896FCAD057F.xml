</taxonomicName>
(
<typeStatus box="[479,529,1922,1946]" pageId="7" pageNumber="218">type</typeStatus>
species), 
<taxonomicName box="[640,775,1922,1947]" class="Actinopterygii" family="Cichlidae" genus="Paraneetroplus" kingdom="Animalia" order="Perciformes" pageId="7" pageNumber="218" phylum="Chordata" rank="species" species="gibbiceps">
