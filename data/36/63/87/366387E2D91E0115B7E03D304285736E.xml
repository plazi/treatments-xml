<collectingRegion box="[470,562,223,248]" country="Colombia" name="Quindio" pageId="15" pageNumber="66">Quindío</collectingRegion>
and 
<collectingRegion box="[617,793,223,248]" country="Colombia" name="Valle del Cauca" pageId="15" pageNumber="66">
