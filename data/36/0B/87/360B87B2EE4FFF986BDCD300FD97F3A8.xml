of 
<collectingMunicipality box="[796,1000,1942,1968]" pageId="9" pageNumber="10">Rio Preto da Eva</collectingMunicipality>
, 
<date box="[1012,1158,1942,1968]" pageId="9" pageNumber="10" value="1986-03">
<collectingDate box="[1012,1158,1942,1968]" pageId="9" pageNumber="10" value="1986-03">March 1986</collectingDate>
