<subSubSection pageId="6" pageNumber="7" type="distribution">
<paragraph blockId="6.[151,1436,728,2030]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[198,503,1733,1759]" pageId="6" pageNumber="7">Distribution and habitat</emphasis>
:— 
<taxonomicName box="[540,800,1733,1758]" class="Liliopsida" family="Marantaceae" genus="Monotagma" kingdom="Plantae" order="Zingiberales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="lilacinum">
