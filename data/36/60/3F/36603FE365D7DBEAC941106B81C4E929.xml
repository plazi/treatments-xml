<subSubSection pageId="20" pageNumber="21" type="reference_group">
<paragraph pageId="20" pageNumber="21">
<taxonomicName class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristiphora astragali" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="astragali">Pristiphora astragali</taxonomicName>
