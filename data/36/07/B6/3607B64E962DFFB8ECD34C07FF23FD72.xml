<emphasis box="[1296,1318,535,562]" italics="true" pageId="9" pageNumber="263">
d 
<subScript attach="left" box="[1309,1318,545,562]" fontSize="7" pageId="9" pageNumber="263">1</subScript>
</emphasis>
; setae 
<emphasis box="[1407,1427,536,562]" italics="true" pageId="9" pageNumber="263">
c 
<subScript attach="left" box="[1418,1427,545,562]" fontSize="7" pageId="9" pageNumber="263">2</subScript>
</emphasis>
