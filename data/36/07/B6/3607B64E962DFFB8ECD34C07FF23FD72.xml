<emphasis box="[1257,1276,536,562]" italics="true" pageId="9" pageNumber="263">
c 
<subScript attach="left" box="[1267,1276,545,562]" fontSize="7" pageId="9" pageNumber="263">1</subScript>
</emphasis>
