<collectingCountry box="[299,669,809,835]" name="Democratic Republic of the Congo" pageId="4" pageNumber="38">Democratic Republic of Congo</collectingCountry>
, 
<collectingCountry box="[681,898,809,835]" name="Equatorial Guinea" pageId="4" pageNumber="38">Equatorial Guinea</collectingCountry>
, and 
<collectingCountry box="[959,1051,809,835]" name="Nigeria" pageId="4" pageNumber="38">Nigeria</collectingCountry>
