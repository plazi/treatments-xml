<paragraph blockId="57.[154,1111,173,1317]" box="[194,841,205,229]" pageId="57" pageNumber="1544">
<typeStatus box="[194,304,205,229]" pageId="57" pageNumber="1544">Holotype</typeStatus>
, „, length of body 
<quantity box="[538,602,205,229]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="57" pageNumber="1544" unit="mm" value="9.0">9 mm</quantity>
, of fore wing 
<quantity box="[772,837,205,229]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="57" pageNumber="1544" unit="mm" value="8.0">8 mm</quantity>
