<paragraph blockId="9.[151,808,151,177]" box="[151,808,151,177]" pageId="9" pageNumber="49">
<heading bold="true" box="[151,808,151,177]" fontSize="11" level="1" pageId="9" pageNumber="49" reason="1">
<emphasis bold="true" box="[151,808,151,177]" pageId="9" pageNumber="49">
Subfamily 
<taxonomicName box="[287,450,151,177]" class="Insecta" family="Phasmatidae" kingdom="Animalia" order="Phasmida" pageId="9" pageNumber="49" phylum="Arthropoda" rank="subFamily" subFamily="Lonchodinae">Lonchodinae</taxonomicName>
