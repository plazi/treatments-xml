<taxonomicName authority="" authorityName="Gray" authorityYear="1867" box="[413,619,932,959]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="85" pageNumber="86" phylum="Porifera" rank="subGenus" subGenus="Carmia">
