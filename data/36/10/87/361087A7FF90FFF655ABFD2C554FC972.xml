<taxonomicName authority="" authorityName="Gray" authorityYear="1867" box="[771,978,813,840]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="86" pageNumber="87" phylum="Porifera" rank="subGenus" subGenus="Carmia">
