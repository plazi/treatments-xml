<taxonomicName authority="" authorityName="Dendy" authorityYear="1905" box="[771,1037,431,458]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="174" pageNumber="175" phylum="Porifera" rank="subGenus" subGenus="Paresperella">
