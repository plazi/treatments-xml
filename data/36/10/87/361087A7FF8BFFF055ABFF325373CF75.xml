<taxonomicName authority="II" authorityName="II" box="[1017,1149,1914,1938]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="79" pageNumber="80" phylum="Porifera" rank="species" species="anisochela">anisochela II</taxonomicName>
, f, 
<taxonomicName authority="III" authorityName="III" box="[1180,1321,1914,1938]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="79" pageNumber="80" phylum="Porifera" rank="species" species="anisochela">anisochela III</taxonomicName>
