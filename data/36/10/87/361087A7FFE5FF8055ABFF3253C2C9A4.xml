<taxonomicName authority="Baer, 1906" authorityName="Baer" authorityYear="1906" box="[283,798,1174,1200]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="imperfecta" subGenus="Arenochalina">
