. 
<taxonomicName authority="Van &amp; Aryasari &amp; De, 2021" authorityName="Van &amp; Aryasari &amp; De" authorityYear="2021" box="[306,672,1901,1925]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="172" pageNumber="173" phylum="Porifera" rank="species" species="seychellensis" status="sp.nov." subGenus="Paresperella">
<emphasis box="[306,672,1901,1925]" italics="true" pageId="172" pageNumber="173">Mycale (Paresperella) seychellensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[678,753,1901,1925]" pageId="172" pageNumber="173">
<taxonomicNameLabel box="[678,753,1901,1925]" pageId="172" pageNumber="173" rank="species">sp.nov.</taxonomicNameLabel>
