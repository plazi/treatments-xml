<subSubSection box="[151,479,477,504]" pageId="2" pageNumber="563" type="nomenclature">
<paragraph blockId="2.[151,479,477,504]" box="[151,479,477,504]" pageId="2" pageNumber="563">
<heading bold="true" box="[151,479,477,504]" fontSize="11" level="1" pageId="2" pageNumber="563" reason="1">
<emphasis bold="true" box="[151,479,477,504]" pageId="2" pageNumber="563">
<taxonomicName authority="Heffern &amp; Santos-Silva &amp; Botero, 2019" authorityName="Heffern &amp; Santos-Silva &amp; Botero" authorityYear="2019" box="[151,361,477,504]" class="Insecta" family="Cerambycidae" genus="Vandenbergheius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="563" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,361,477,504]" italics="true" pageId="2" pageNumber="563">Vandenbergheius</emphasis>
