<taxonomicName authorityName="Draudt" authorityYear="1937" box="[773,888,1816,1839]" class="Insecta" family="Noctuidae" genus="Craniophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="jactans">
