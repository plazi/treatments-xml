</taxonomicName>
, 
<taxonomicName authorityName="Kiss" authorityYear="2017" box="[984,1089,1120,1145]" class="Insecta" family="Noctuidae" genus="Berionycta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="berioi">
