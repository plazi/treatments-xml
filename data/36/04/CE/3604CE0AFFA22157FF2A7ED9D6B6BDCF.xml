<taxonomicName authorityName="Kiss" authorityYear="2017" baseAuthorityName="Wiltshiri" baseAuthorityYear="1986" box="[1246,1376,1308,1331]" class="Insecta" family="Noctuidae" genus="Berionycta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="asirensis">
