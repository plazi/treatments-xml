<taxonomicName authorityName="Berio" authorityYear="1943" box="[596,745,1200,1225]" class="Insecta" family="Noctuidae" genus="Craniophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="hemileuca">
