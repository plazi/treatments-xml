<quantity box="[1063,1135,1267,1292]" metricMagnitude="4" metricUnit="m" metricValue="1.3" pageId="72" pageNumber="73" unit="km" value="13.0">13 km</quantity>
W, 
<quantity box="[1182,1271,1267,1292]" metricMagnitude="3" metricUnit="m" metricValue="1.95" pageId="72" pageNumber="73" unit="m" value="1950.0">1950 m</quantity>
