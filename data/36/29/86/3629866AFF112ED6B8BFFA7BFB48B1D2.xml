<collectingCountry box="[1000,1236,655,679]" name="Dominican Republic" pageId="145" pageNumber="146">Dominican Republic</collectingCountry>
) and 
<collectingCountry box="[1301,1436,655,679]" name="Puerto Rico" pageId="145" pageNumber="146">Puerto Rico</collectingCountry>
(
<figureCitation box="[159,258,692,716]" captionStart="FIGURES 337–344" captionStartId="279.[151,258,1967,1988]" captionTargetBox="[151,1398,195,1952]" captionTargetId="figure-66@279.[151,1398,193,1952]" captionTargetPageId="279" captionText="FIGURES 337–344. Distribution maps for species of Messatoporus. 337, M. longicaudis. 338, M. longitergus. 339, M. lordos. 340, M. lunatus. 341, M. maculipes. 342, M. maculistis. 343, M. matucanus. 344, M. mesonotator." figureDoi="http://doi.org/10.5281/zenodo.5262233" httpUri="https://zenodo.org/record/5262233/files/figure.png" pageId="145" pageNumber="146">Fig. 340</figureCitation>
). 
<taxonomicName authorityName="Santos &amp; Aguiar" authorityYear="2013" box="[277,523,692,716]" class="Insecta" family="Ichneumonidae" genus="Messatoporus" kingdom="Animalia" order="Hymenoptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="lunatus">
