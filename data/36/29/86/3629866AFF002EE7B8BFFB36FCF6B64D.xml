<subSubSection pageId="160" pageNumber="161" type="etymology">
<paragraph blockId="160.[151,1436,151,1364]" pageId="160" pageNumber="161">
<emphasis bold="true" box="[199,326,692,716]" pageId="160" pageNumber="161">Etymology</emphasis>
. From the Latin 
<taxonomicName authorityName="Santos &amp; Aguiar" authorityYear="2013" box="[525,660,692,716]" class="Insecta" family="Ichneumonidae" genus="Messatoporus" kingdom="Animalia" order="Hymenoptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="occidentalis">
<emphasis box="[525,660,692,716]" italics="true" pageId="160" pageNumber="161">occidentalis</emphasis>
</taxonomicName>
