<taxonomicName id="19424265AB50CAB4723F627EAE0A7947" authority="Attems" authorityName="Attems" class="Diplopoda" family="Polydesmidae" genus="Brachydesmus" higherTaxonomySource="CoL" kingdom="Animalia" order="Polydesmida" pageId="1" pageNumber="43" phylum="Arthropoda" rank="species" species="parallelus">Brachydesmus parallelus Attems</taxonomicName>
.
</paragraph>
<paragraph id="120ACAA502D46BE4428FF5322AF533F1" pageId="1" pageNumber="43" type="mainText">
<materialsCitation id="CFA72F498BF5D66B95B031FAA278FF68" ID-GBIF-Occurrence="1058480049" collectingDate="1930-01-01" collectingDateMax="1930-12-31" collectingDateMin="1930-01-01" country="Afghanistan" location="Draga-Hoehle b. Ponikve di Sesana (Ponikve bei Auber)" pageId="1" pageNumber="43" stateProvince="Draga-Hoehle b. Ponikve di Sesana (Ponikve bei Auber)">
