<specimenCount id="6B2E5DA78F2235F8370DD706B389788B" pageId="1" pageNumber="43" type="generic">1 ♀</specimenCount>
und vom 
<date id="B926A7E5AE22DCFF03B09EA0182522DE" pageId="1" pageNumber="43">
<collectingDate id="862106EA25BF17648861A9008D9DD618" pageId="1" pageNumber="43">3. II.</collectingDate>
</date>
<specimenCount id="BC973379EC3D3E0371D0A9A7C564A8E3" pageId="1" pageNumber="43" type="generic">3 ♂</specimenCount>
<specimenCount id="0358A2CF948B416A1C83698D143103E1" pageId="1" pageNumber="43" type="generic">10 ♀</specimenCount>
