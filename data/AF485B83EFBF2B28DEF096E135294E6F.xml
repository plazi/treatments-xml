<collectingRegion id="85040979DFC5568A93B26E4F75922EEF" pageId="1" pageNumber="43">
<normalizedToken id="147E3902D9C932E77F58A28493A34595" originalValue="Draga-Höhle" pageId="1" pageNumber="43">Draga-Hoehle</normalizedToken>
