<quantity id="5C52D9C28ED3AACF5D1D2D49C43C5513" metricMagnitude="-3" metricUnit="m" metricValue="9.25" metricValueMax="9.5" metricValueMin="9.0" pageId="1" pageNumber="43" unit="mm" value="9.25" valueMax="9.5" valueMin="9.0">9-9 1/2 mm</quantity>
