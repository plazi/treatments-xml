<taxonomicName id="4C2B4D3B076DFFB4FE38F7BAFAE0F786" authority="Dobson, Journ. Asiat. Soc. Bong. 1872, p. 337" box="[471,1356,2098,2134]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" higherTaxonomySource="CoL" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="115" phylum="Chordata" rank="species" species="garoensis">
