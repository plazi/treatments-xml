<taxonomicName id="D99747CDD14B936D0AA7B92B91E1F9D4" authorityName="Adanson" authorityYear="1763" box="[392,462,1571,1595]" class="Liliopsida" family="Arecaceae" genus="Sabal" kingdom="Plantae" order="Arecales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
