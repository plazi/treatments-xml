and 
<taxonomicName id="D99747CDD14B936D0E02B92C9153F9B4" authority="(Caricaeae)" baseAuthorityName="Caricaeae" class="Magnoliopsida" family="Caricaceae" genus="Jaracatia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Brassicales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
