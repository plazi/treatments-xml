<taxonomicName id="D99747CDD14B936D0821B90C9345F9F4" box="[782,874,1540,1563]" class="Liliopsida" family="Arecaceae" genus="Euterpe" kingdom="Plantae" order="Arecales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
<emphasis id="2CE3E05CD14B936D0821B90C9345F9F4" box="[782,874,1540,1563]" italics="true" pageId="60" pageNumber="59">Euterpe</emphasis>
</taxonomicName>
, 
<taxonomicName id="D99747CDD14B936D0858B90B93C1F9F4" authorityName="C.F.P.Martius" authorityYear="1824" box="[887,1006,1539,1563]" class="Liliopsida" family="Arecaceae" genus="Guilielma" kingdom="Plantae" order="Arecales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
<emphasis id="2CE3E05CD14B936D0858B90B93C1F9F4" box="[887,1006,1539,1563]" italics="true" pageId="60" pageNumber="59">Guilielma</emphasis>
</taxonomicName>
, 
<taxonomicName id="D99747CDD14B936D08D5B90C9459F9F4" authorityName="J.Gaertner" authorityYear="1791" box="[1018,1142,1540,1563]" class="Liliopsida" family="Arecaceae" genus="Manicaria" kingdom="Plantae" order="Arecales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
<emphasis id="2CE3E05CD14B936D08D5B90C9459F9F4" box="[1018,1142,1540,1563]" italics="true" pageId="60" pageNumber="59">Manicaria</emphasis>
</taxonomicName>
, 
<taxonomicName id="D99747CDD14B936D0FADB90B9533F9F4" authorityName="C.F.P.Martius" authorityYear="1824" box="[1154,1308,1539,1563]" class="Magnoliopsida" family="Arecaceae" genus="Maximiliana" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
