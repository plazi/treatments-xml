<taxonomicName id="D99747CDD14B936D0F88B92C94D8F9D4" box="[1191,1271,1572,1595]" class="Magnoliopsida" family="Caricaceae" genus="Carica" kingdom="Plantae" order="Brassicales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
