<taxonomicName id="D99747CDD14B936D084DB94C9389F9B4" box="[866,934,1604,1627]" class="Liliopsida" family="Musaceae" genus="Musa" kingdom="Plantae" order="Zingiberales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
