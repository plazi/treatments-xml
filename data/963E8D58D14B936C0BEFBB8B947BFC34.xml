</taxonomicName>
, 
<taxonomicName id="D99747CDD14B936D0BDFB92C9165F9D4" box="[240,330,1572,1595]" class="Magnoliopsida" family="Euphorbiaceae" genus="Ricinus" kingdom="Plantae" order="Malpighiales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
