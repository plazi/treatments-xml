</taxonomicName>
, 
<taxonomicName id="D99747CDD14B936D0E06B90B95B6F9F4" box="[1321,1433,1539,1563]" class="Magnoliopsida" family="Arecaceae" genus="Oreodoxa" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="genus">
