<taxonomicName id="D99747CDD14B936D0953B94B930EF9B4" box="[636,801,1603,1627]" class="Liliopsida" family="Bromeliaceae" kingdom="Plantae" order="Poales" pageId="60" pageNumber="59" phylum="Tracheophyta" rank="family">Bromeliaceae</taxonomicName>
