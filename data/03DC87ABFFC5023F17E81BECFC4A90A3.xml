<taxonomicName id="4C754D3EFFC5023F17E81A51FE32925F" authority="Jangoux, 1984" authorityName="Jangoux" authorityYear="1984" box="[151,400,1296,1320]" class="Asteroidea" family="Solasteridae" genus="Seriaster" kingdom="Animalia" order="Valvatida" pageId="26" pageNumber="93" phylum="Echinodermata" rank="genus">
