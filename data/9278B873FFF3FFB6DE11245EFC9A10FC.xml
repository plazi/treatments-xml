</emphasis>
<taxonomicName id="DDD172E6FFF3FFB6D9472227FC3B1267" authority="(Richard &amp; Bernard, 1792)" baseAuthorityName="Richard &amp; Bernard" baseAuthorityYear="1792" class="Aves" family="Columbidae" genus="Leptotila" kingdom="Animalia" order="Columbiformes" pageId="13" pageNumber="428" phylum="Chordata" rank="species" species="rufaxilla">
