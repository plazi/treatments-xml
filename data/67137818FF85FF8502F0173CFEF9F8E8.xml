<taxonomicName id="28BAB28DFF85FF8501A7178EFB82F8A0" box="[960,1105,1785,1809]" class="Malacostraca" family="Hymenosomatidae" genus="Halicarcinus" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="515" phylum="Arthropoda" rank="genus">
