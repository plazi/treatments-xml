<taxonomicName id="28BAB28DFF85FF850322178EFE04F8A0" box="[325,471,1785,1809]" class="Malacostraca" family="Hymenosomatidae" genus="Rhynchoplax" higherTaxonomySource="CoL" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="515" phylum="Arthropoda" rank="genus">
