(
<specimenCount id="8AFDFD0EFFBFF331FE3D4F03844BDF67" box="[445,576,1122,1147]" pageId="8" pageNumber="185" type="generic">1 specimen</specimenCount>
). Part (AM F. 
<date id="E8451047FFBFF331FD5A4F03853BDF67" box="[730,816,1123,1147]" pageId="8" pageNumber="185" value="88-08-14">
<collectingDate id="F801E9AFFFBFF331FD5A4F03853BDF67" box="[730,816,1123,1147]" pageId="8" pageNumber="185" value="88-08-14">140888</collectingDate>
