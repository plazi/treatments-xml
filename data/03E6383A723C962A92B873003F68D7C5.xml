<taxonomicName id="4C4FF2AF723A962A907E763138EBD7C6" authorityName="Ale-Rocha" authorityYear="2007" box="[593,663,872,897]" class="Insecta" family="Hybotidae" genus="Neohybos" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="53" phylum="Arthropoda" rank="species" species="tenuis">
