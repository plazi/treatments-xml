<number id="3DA5248B0051FF90FF76FAE7FF2CFAE3" box="[151,182,1348,1374]" pageId="11" pageNumber="464" value="14.0">14</number>
. 
<taxonomicName id="4C6D4D2F0051FF90FF22FAE7FDAFFAE3" authority="Yang" authorityName="Yang" box="[195,565,1348,1374]" class="Insecta" family="Cicadellidae" genus="Matsumurina" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="464" phylum="Arthropoda" rank="species" species="longissima">
<emphasis id="B919EABE0051FF90FF22FAE7FE6BFAE3" bold="true" box="[195,497,1348,1374]" italics="true" pageId="11" pageNumber="464">Matsumurina longissima</emphasis>
Yang
</taxonomicName>
