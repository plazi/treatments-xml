<taxonomicName id="C3541D9D38F7437F5A91A46FF7345571" class="Insecta" family="Noctuidae" genus="Azeta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Azeta ceramina" order="Lepidoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="ceramina">
