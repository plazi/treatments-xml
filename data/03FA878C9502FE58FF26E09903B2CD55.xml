</emphasis>
<taxonomicName id="4C534D199502FE58FE88E3400118CC51" authority="Dall, 1889" authorityName="Dall" authorityYear="1889" box="[313,629,1588,1613]" class="Gastropoda" family="Acteonidae" genus="Actaeon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="18" pageNumber="215" phylum="Mollusca" rank="species" species="curtulus">
