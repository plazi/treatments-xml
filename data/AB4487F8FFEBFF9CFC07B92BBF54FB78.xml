<caption id="77926666FFE9FF92FF24BD50B8AFF93F" ID-DOI="http://doi.org/10.5281/zenodo.5393734" ID-Zenodo-Dep="5393734" httpUri="https://zenodo.org/record/5393734/files/figure.png" pageId="6" pageNumber="106" startId="6.[159,171,1643,1662]" targetBox="[319,1276,619,1571]" targetPageId="6">
