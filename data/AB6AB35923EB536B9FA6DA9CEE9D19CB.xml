, et 
<taxonomicName id="2A536E498872B97623423B4C869FD470" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27830" authority=", Smith" class="Insecta" family="Formicidae" genus="Cataulacus" kingdom="Animalia" lsidName-HNS="Cataulacus reticulatus Smith" order="Hymenoptera" pageNumber="146" phylum="Arthropoda" rank="species" species="reticulatus">C. reticulatus, Smith</taxonomicName>
