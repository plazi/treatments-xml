<collectingCountry id="F33476473D49FF82FAFD9C26FAE0FEE1" box="[1282,1381,352,378]" name="Portugal" pageId="23" pageNumber="24">Portugal</collectingCountry>
(
<bibRefCitation id="EFB24B263D49FF82FA879C26FEAFFE39" author="Da" pageId="23" pageNumber="28" refString="Da Cunha, A. X. (1948) Tardigrados do Fauna Portuguesa IV. Memorias e Estudos do Museu Zoologico da Universidade da Coimbra, 188, 1 - 5." type="journal article" year="1948">
Da 
<collectingCountry id="F33476473D49FF82FF689CCEFF61FE39" box="[151,228,392,418]" name="Saint Helena, Ascension and Tristan da Cunha" pageId="23" pageNumber="24">Cunha</collectingCountry>
<number id="3DEB24F03D49FF82FF139CCEFEAFFE39" box="[236,298,392,418]" pageId="23" pageNumber="24" value="1948.0">1948</number>
</bibRefCitation>
; 
<bibRefCitation id="EFB24B263D49FF82FEC79CCEFE6DFE39" author="Fontoura" box="[312,488,392,418]" pageId="23" pageNumber="28" refString="Fontoura, A. P. (1981) Contribution pour l'etudedes tardigrades terrestres du Portugal avec la description d'une nouvelle especie du genre Macrobiotus. Publicacoes do Instituto de Zoologia ' Dr. Augusto Nobre'. Facultad de Ciencias do Porto, 160,1 - 24." type="book" year="1981">
Fontoura 
<number id="3DEB24F03D49FF82FE559CCEFE6DFE39" box="[426,488,392,418]" pageId="23" pageNumber="24" value="1981.0">1981</number>
</bibRefCitation>
; 
<bibRefCitation id="EFB24B263D49FF82FE099CCEFCDCFE39" author="Maucci" box="[502,857,392,418]" pageId="23" pageNumber="29" refString="Maucci, W. &amp; Durante Pasa, V. (1984) Macrobiotus lusitanicus sp. nov. nuova specie di eutardigrado del portogallo nord - occidentale. Bollettino del Museo Civico di Storia Naturale di Verona, 11, 319 - 326." type="journal article" year="1984">
