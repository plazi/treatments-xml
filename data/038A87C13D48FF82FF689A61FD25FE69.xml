<taxonomicName id="4C234D543D49FF82FB9A9CF6FAE0FE52" box="[1125,1381,432,457]" class="Eutardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="23" pageNumber="24" phylum="Tardigrada" rank="species" species="richtersi">
