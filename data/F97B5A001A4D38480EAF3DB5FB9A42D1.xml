<taxonomicName id="B6D290951A4D384A0F133C2FFD37415C" authority="Fabricius, 1780" authorityName="Fabricius" authorityYear="1780" box="[299,687,1334,1357]" class="Polychaeta" family="Terebellidae" genus="Amphitrite" kingdom="Animalia" order="Terebellida" pageId="12" pageNumber="252" phylum="Annelida" rank="species" species="cincinnata">
