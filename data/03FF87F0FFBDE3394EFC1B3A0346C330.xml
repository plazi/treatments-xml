<taxonomicName id="4C564D65FFBDE3384DAF1BF10703C790" authority="Herre, 1923" authorityName="Herre" authorityYear="1923" box="[964,1323,1761,1787]" class="Elasmobranchii" family="Triakidae" genus="Hemitriakis" kingdom="Animalia" order="Carcharhiniformes" pageId="4" pageNumber="45" phylum="Chordata" rank="species" species="leucoperiptera">
<emphasis id="B922EAF4FFBDE3384DAF1BF106BDC791" box="[964,1173,1761,1786]" italics="true" pageId="4" pageNumber="45">H. leucoperiptera</emphasis>
<bibRefCitation id="EFC74B17FFBDE3384AF71BF10703C790" author="Herre" box="[1180,1323,1761,1787]" pageId="4" pageNumber="57" refString="Herre, A. W. C. T. (1923) Notes on Philippine sharks. I. Philippine Journal of Science, 23, 68 - 73." type="journal article" year="1923">
Herre, 
<number id="3D9E24C1FFBDE3384A851BF10703C790" box="[1262,1323,1761,1787]" pageId="4" pageNumber="45" value="1923.0">1923</number>
</bibRefCitation>
</taxonomicName>
, from the 
<collectingCountry id="F3417676FFBDE3384EFC1A180353C649" box="[151,379,1800,1826]" name="Philippines" pageId="4" pageNumber="45">Philippine Islands</collectingCountry>
. 
<taxonomicName id="4C564D65FFBDE3384FE21A18004FC64A" box="[393,615,1800,1825]" class="Elasmobranchii" family="Triakidae" genus="Hemitriakis" kingdom="Animalia" order="Carcharhiniformes" pageId="4" pageNumber="45" phylum="Chordata" rank="species" species="leucoperiptera">
<emphasis id="B922EAF4FFBDE3384FE21A18004FC64A" box="[393,615,1800,1825]" italics="true" pageId="4" pageNumber="45">H. leucoperiptera</emphasis>
