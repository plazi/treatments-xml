<taxonomicName id="4C564D65FFBDE3384D7C1A4506D4C604" authority="Muller &amp; Henle, 1839" authorityName="Muller &amp; Henle" authorityYear="1839" box="[791,1276,1877,1903]" class="Elasmobranchii" family="Scyliorhinidae" genus="Galeus" kingdom="Animalia" order="Carcharhiniformes" pageId="4" pageNumber="45" phylum="Chordata" rank="species" species="japanicus">
