<quantity id="4CEF9B25FF47FF639C3BF919FED73703" box="[1094,1174,1771,1798]" metricMagnitude="-1" metricUnit="m" metricValue="2.9972" pageId="214" pageNumber="215" unit="in" value="11.8">11.8 in</quantity>
