<quantity id="4CEF9B25FF47FF639B8CF919FE773700" box="[1009,1078,1771,1797]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="214" pageNumber="215" unit="cm" value="30.0">30 cm</quantity>
