<taxonomicName id="19BE4D021668FFCAFD06FE21FCAE30DB" authority="Hemp &amp; París, 2021" authorityName="Hemp &amp; París" authorityYear="2021" box="[703,860,403,429]" class="Insecta" family="Tettigoniidae" genus="Apteroscirtus" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="291" phylum="Arthropoda" rank="species" species="dundoensis" status="n. sp.">
<emphasis id="ECCAEA931668FFCAFD06FE21FCAE30DB" box="[703,860,403,429]" italics="true" pageId="4" pageNumber="291">A. dundoensis</emphasis>
</taxonomicName>
