<subSubSection id="94956579E602A854FF761247FC50E53E" box="[199,1010,1086,1111]" pageId="8" pageNumber="459" type="etymology">
<paragraph id="DC3036F2E602A854FF761247FC50E53E" blockId="8.[151,1437,906,2011]" box="[199,1010,1086,1111]" pageId="8" pageNumber="459">
<emphasis id="EEFBEAE0E602A854FF761247FEECE53E" bold="true" box="[199,334,1086,1111]" pageId="8" pageNumber="459">Etymology.</emphasis>
