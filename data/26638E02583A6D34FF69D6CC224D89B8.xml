<taxonomicName id="69CA4497583B6D34FEEFD64F2471893E" box="[308,534,344,365]" class="Insecta" family="Nymphalidae" genus="Coenonympha" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="746" phylum="Arthropoda" rank="species" species="tullia">
