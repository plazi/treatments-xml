</taxonomicName>
, 
<taxonomicName id="4C034D76FFB7FF8873B188D0FDF8FCFB" box="[503,617,746,769]" class="Magnoliopsida" family="Fabaceae" genus="Ceratonia" kingdom="Plantae" order="Fabales" pageId="4" pageNumber="283" phylum="Tracheophyta" rank="genus">
<emphasis id="B977EAE7FFB7FF8873B188D0FDF8FCFB" box="[503,617,746,769]" italics="true" pageId="4" pageNumber="283">Ceratonia</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C034D76FFB7FF8870E788D0FD6CFCFB" box="[673,765,746,769]" class="Magnoliopsida" family="Anacardiaceae" genus="Pistacia" kingdom="Plantae" order="Sapindales" pageId="4" pageNumber="283" phylum="Tracheophyta" rank="genus">
<emphasis id="B977EAE7FFB7FF8870E788D0FD6CFCFB" box="[673,765,746,769]" italics="true" pageId="4" pageNumber="283">Pistacia</emphasis>
