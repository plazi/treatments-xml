<taxonomicName id="3CB429E2FFBFA24BAD98FD05F32CFD5C" authority="Lombardo, 1999" authorityName="Lombardo" authorityYear="1999" class="Insecta" family="Mantidae" genus="Rehniella" kingdom="Animalia" order="Mantodea" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="planicephala">
