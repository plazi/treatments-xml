<emphasis id="B9139A0DFFD01211741409E89C3AFB0C" box="[1071,1099,1188,1211]" italics="true" pageId="12" pageNumber="57">ag</emphasis>
<emphasis id="B9139A0DFFD01211747009FE9C22FB08" box="[1099,1107,1202,1215]" italics="true" pageId="12" pageNumber="57">
<number id="3DAF5438FFD01211747009FE9C22FB08" box="[1099,1107,1202,1215]" pageId="12" pageNumber="57" value="1.0">1</number>
</emphasis>
; three pairs of anal and three pairs of adanal setae present, setae 
<emphasis id="B9139A0DFFD01211720D09879A23FB55" box="[566,594,1227,1250]" italics="true" pageId="12" pageNumber="57">an</emphasis>
<emphasis id="B9139A0DFFD01211726809959A2AFB51" box="[595,603,1241,1254]" italics="true" pageId="12" pageNumber="57">
<number id="3DAF5438FFD01211726809959A2AFB51" box="[595,603,1241,1254]" pageId="12" pageNumber="57" value="1.0">1</number>
