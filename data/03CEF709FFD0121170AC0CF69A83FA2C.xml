<emphasis id="B9139A0DFFD01211721009E89A36FB0C" box="[555,583,1188,1211]" italics="true" pageId="12" pageNumber="57">ag</emphasis>
<emphasis id="B9139A0DFFD01211727C09FE9A3EFB08" box="[583,591,1202,1215]" italics="true" pageId="12" pageNumber="57">
<number id="3DAF5438FFD01211727C09FE9A3EFB08" box="[583,591,1202,1215]" pageId="12" pageNumber="57" value="2.0">2</number>
</emphasis>
