<collectionCode id="ED1096AAFF86FFA6DF3BF3F5CEADFA0B" box="[370,435,1477,1496]" country="Czech Republic" httpUri="http://grbio.org/cool/hj12-2xm7" name="National Museum Prague" pageId="25" pageNumber="88">NMPC</collectionCode>
