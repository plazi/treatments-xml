<specimenCount id="9D07C5E6FF86FFA6DC66F39BCD72FA6D" box="[559,620,1451,1470]" pageId="25" pageNumber="88" type="generic">1 spec.</specimenCount>
, 
<collectingRegion id="49C5C08DFF86FFA6DC3AF39BCDB3FA6D" box="[627,685,1451,1470]" country="Yemen" name="San'a'" pageId="25" pageNumber="88">Sana’a</collectingRegion>
city, Bait Baws, 
<geoCoordinate id="EE3568A8FF86FFA6DD75F39BCC8AFA6D" box="[828,916,1451,1470]" degrees="15" direction="north" minutes="16.3" orientation="latitude" pageId="25" pageNumber="88" precision="92" value="15.271667">15°16.3ʹN</geoCoordinate>
