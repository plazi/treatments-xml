<geoCoordinate id="EE3568A8FF86FFA6DDD4F39BCCEAFA6D" box="[925,1012,1451,1470]" degrees="44" direction="east" minutes="12.1" orientation="longitude" pageId="25" pageNumber="88" precision="92" value="44.201668">44°12.1ʹE</geoCoordinate>
, 
<quantity id="4CF9A38AFF86FFA6DDB2F39BCB20FA6D" box="[1019,1086,1451,1470]" metricMagnitude="3" metricUnit="m" metricValue="2.41" pageId="25" pageNumber="88" unit="m" value="2410.0">2410 m</quantity>
