<elevation id="00287E70FF88FFB7E3CE7AD86DC52EE7" box="[1191,1251,979,1003]" metricMagnitude="1" metricUnit="m" metricValue="9.0" pageId="3" pageNumber="124" unit="m" value="90.0">
