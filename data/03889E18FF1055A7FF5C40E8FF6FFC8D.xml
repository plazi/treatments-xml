<location id="8EFE79D5FF1055B9FF5C4531FE87FBBB" LSID="urn:lsid:plazi:treatment:03889E18FF1055A7FF5C40E8FF6FFC8D:8EFE79D5FF1055B9FF5C4531FE87FBBB" box="[151,257,1104,1129]" country="Portugal" municipality="Algarve region" name="Portimao" pageId="175" pageNumber="176" stateProvince="Faro district">Portimão</location>
, near 
<collectorName id="26D44AD8FF1055B9FE884531FDAEFBBB" box="[323,552,1104,1129]" pageId="175" pageNumber="176">Mexilhoeira Grande</collectorName>
, 
<collectorName id="26D44AD8FF1055B9FDF84531FD7CFBBB" box="[563,762,1104,1129]" pageId="175" pageNumber="176">Cêrro do Algarve</collectorName>
, 
<taxonomicName id="4C21548DFF1055B9FCCE4531FC49FBBA" authority="dos Mouros" authorityName="dos Mouros" box="[773,975,1104,1129]" class="Insecta" family="Aetalionidae" genus="Mina" higherTaxonomySource="CoL" kingdom="Plantae" order="Hemiptera" pageId="175" pageNumber="176" phylum="Arthropoda" rank="genus">Mina dos Mouros</taxonomicName>
, leg. A. de 
<collectorName id="26D44AD8FF1055B9FB854530FB58FBBB" box="[1102,1246,1104,1129]" pageId="175" pageNumber="176">B. Machado.</collectorName>
