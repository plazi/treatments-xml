, 
<collectingCountry id="72A2A4937813FFBFFDB1FEFE4D58FE1B" box="[539,628,376,402]" name="Mexico" pageId="12" pageNumber="27">Mexico</collectingCountry>
(
<figureCitation id="928EF8867813FFBFFD2EFEFE4DF1FE1B" box="[644,733,376,402]" captionStart="FIGURES 41–42" captionStartId="11.[151,269,1935,1959]" captionTargetBox="[156,1436,195,1905]" captionTargetId="figure-19@11.[151,1436,154,1932]" captionTargetPageId="11" captionText="FIGURES 41–42. distribution of Amphipteryx spp." figureDoi="http://doi.org/10.5281/zenodo.5302150" httpUri="https://zenodo.org/record/5302150/files/figure.png" pageId="12" pageNumber="27">Fig. 42</figureCitation>
