<taxonomicName id="4C55C7F7C465FF99A447A934DCAB257B" authorityName="Blume" authorityYear="1834" class="Liliopsida" family="Asparagaceae" genus="Aspidistra" kingdom="Plantae" order="Asparagales" pageId="14" pageNumber="513" phylum="Tracheophyta" rank="species" species="elatior">
<emphasis id="B9216066C465FF99A447A934DCAB257B" italics="true" pageId="14" pageNumber="513">Aspidistra elatior</emphasis>
