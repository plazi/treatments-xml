<taxonomicName id="4C55C7F7C465FF99A09AA934DEAA251C" box="[500,737,521,546]" class="Insecta" family="Diaspididae" genus="Chionaspis" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="513" phylum="Arthropoda" rank="species" species="evonymii">
