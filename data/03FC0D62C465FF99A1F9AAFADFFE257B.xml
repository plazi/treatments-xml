<taxonomicName id="4C55C7F7C465FF99A565A937D8A7251C" box="[1035,1260,522,545]" class="Magnoliopsida" family="Celastraceae" genus="Euonymus" kingdom="Plantae" order="Celastrales" pageId="14" pageNumber="513" phylum="Tracheophyta" rank="species" species="japonica">
