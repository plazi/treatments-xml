<taxonomicName id="4C1E3EE61410FFF30384FF3BF465FF68" authority="(Stuben, 2000)" authorityName="Stüben &amp; Astrin" authorityYear="2010" baseAuthorityName="Stuben" baseAuthorityYear="2000" box="[164,639,197,220]" class="Insecta" family="Curculionidae" genus="Sonchiacalles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="66" phylum="Arthropoda" rank="species" species="muelleri" status="comb. nov.">
<emphasis id="B96A99771410FFF30384FF3BF7DDFF68" bold="true" box="[164,455,197,219]" italics="true" pageId="26" pageNumber="66">Sonchiacalles muelleri</emphasis>
(
<emphasis id="B96A99771410FFF302F6FF38F46DFF68" bold="true" box="[470,631,198,220]" pageId="26" pageNumber="66">Stüben, 2000</emphasis>
)
</taxonomicName>
<emphasis id="B96A99771410FFF301A6FF38F511FF68" bold="true" box="[646,779,198,219]" pageId="26" pageNumber="66">
<taxonomicNameLabel id="A259240C1410FFF301A6FF38F511FF68" box="[646,779,198,219]" pageId="26" pageNumber="66" rank="species">comb. nov.</taxonomicNameLabel>
