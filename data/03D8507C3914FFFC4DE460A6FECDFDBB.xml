<taxonomicName id="4C719AE93912FFF04C6967A3FD39F977" authority="Malogolowkin." authorityName="Malogolowkin." box="[282,702,1758,1780]" class="Insecta" family="Drosophilidae" genus="Rhinoleucophenga" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="266" phylum="Arthropoda" rank="species" species="lopesi">
