<taxonomicName id="63A364B63279303FFD5BFC70FB82FC6D" authority="Jeannel, 1930" authorityName="Jeannel" authorityYear="1930" box="[705,1142,949,975]" class="Insecta" family="Carabidae" genus="Cothresia" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="48" phylum="Arthropoda" rank="species" species="humeralis">
