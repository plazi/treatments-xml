<taxonomicName id="63A364B63279303FFF22FC70FD69FC6D" authority="Jeannel, 1930" authorityName="Jeannel" authorityYear="1930" box="[184,669,949,975]" class="Insecta" family="Carabidae" genus="Plocamotrechus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="48" phylum="Arthropoda" rank="species" species="humeralis">
