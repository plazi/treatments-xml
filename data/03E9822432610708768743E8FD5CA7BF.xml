<emphasis id="B934EF2032610708774241B9FEC8A7FE" bold="true" box="[113,365,906,930]" pageId="97" pageNumber="90">Differential diagnosis.</emphasis>
<taxonomicName id="4C4048B132610708764741BFFDB9A7FF" authorityName="Nakane" authorityYear="1963" box="[372,540,908,931]" class="Insecta" family="Leiodidae" genus="Leiodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="97" pageNumber="90" phylum="Arthropoda" rank="species" species="okawai">
