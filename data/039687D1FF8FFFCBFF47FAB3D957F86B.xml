<taxonomicName id="4C3F4D44FF8EFFCBFE8BF9A7D9C7F9A2" box="[347,450,1635,1656]" class="Insecta" family="Agromyzidae" genus="Liriomyza" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="172" phylum="Arthropoda" rank="genus">
