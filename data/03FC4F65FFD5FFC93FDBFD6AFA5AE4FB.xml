<collectingCountry id="F342BEE3FFD5FFC93B4EFC73FB81E3D5" box="[1026,1095,974,995]" name="United States of America" pageId="3" pageNumber="193">U.S.A.</collectingCountry>
, 
<collectingCountry id="F342BEE3FFD5FFC93B1CFC73FB5EE3D5" box="[1104,1176,974,995]" name="Canada" pageId="3" pageNumber="193">Canada</collectingCountry>
