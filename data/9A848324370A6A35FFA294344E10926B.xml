<taxonomicName id="90D0DD47B17EFD149FB713A16AA012C9" class="Insecta" family="Cerambycidae" genus="Morimopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Morimopsis" order="Coleoptera" pageId="214" pageNumber="215" phylum="Arthropoda" rank="genus">Morimopsis</taxonomicName>
