<taxonomicName id="D80D3FFFDF36A8C735EC0E14C0A9380F" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="cerinthoides">
