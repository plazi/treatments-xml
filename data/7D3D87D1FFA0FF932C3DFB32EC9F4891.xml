<taxonomicName id="32944D44FFA2FF932F6FFEAAEBF348C1" authority="Yang, Zhu et An" authorityName="Yang, Zhu et An" box="[602,997,364,390]" class="Insecta" family="Empididae" genus="Clinocera" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="guizhouensis">
