<taxonomicName id="76B60F8AFF96D174EDBB80317432FACB" authority="Shen &amp; Gai &amp; Song &amp; Sun &amp; Scheller &amp; Zhou, 2007" authorityName="Shen &amp; Gai &amp; Song &amp; Sun &amp; Scheller &amp; Zhou" authorityYear="2007" box="[234,515,1341,1360]" class="Pauropoda" family="Pauropodidae" genus="Allopauropus" kingdom="Animalia" order="Tetramerocerata" pageId="4" pageNumber="2315" phylum="Arthropoda" rank="species" species="disphaeroides" status="sp. n." subGenus="Decapauropus">
