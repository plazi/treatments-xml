<taxonomicName id="C6E44D458673FFD5FB8EFABFFB32FA98" authorityName="Carayon" authorityYear="1972" box="[1029,1180,1303,1328]" class="Insecta" family="Anthocoridae" kingdom="Animalia" order="Hemiptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="subFamily" subFamily="Lasiochilinae">Lasiochilinae</taxonomicName>
