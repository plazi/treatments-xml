<accessionNumber id="9462AB55FFEBFFEBFC33FD11FB870239" box="[902,1043,762,787]" httpUri="http://www.uniprot.org/uniprot/MMF18750" pageId="9" pageNumber="468">
<collectionCode id="ED20AE73FFEBFFEBFC33FD11FC520238" box="[902,966,763,786]" country="Portugal" httpUri="http://grbio.org/cool/mme0-gx6r" name="Museu Municipal do Funchal" pageId="9" pageNumber="468">MMF</collectionCode>
18750
</accessionNumber>
