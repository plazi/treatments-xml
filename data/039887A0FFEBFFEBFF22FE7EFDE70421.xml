<accessionNumber id="9462AB55FFEBFFEBFD4CFCF5FC13021D" box="[761,903,798,823]" httpUri="http://www.uniprot.org/uniprot/MMF36797" pageId="9" pageNumber="468">
<collectionCode id="ED20AE73FFEBFFEBFD4CFCF5FCAD021C" box="[761,825,799,822]" country="Portugal" httpUri="http://grbio.org/cool/mme0-gx6r" name="Museu Municipal do Funchal" pageId="9" pageNumber="468">MMF</collectionCode>
36797
