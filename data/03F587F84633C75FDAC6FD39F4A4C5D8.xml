<subSubSection id="C34665654633C75FDAC6FCA9F4A4C5D8" lastPageId="40" lastPageNumber="1357" pageId="39" pageNumber="1356" type="description">
<paragraph id="8BE336EE4633C750DAC6FCA9F574C51C" blockId="39.[178,908,748,899]" box="[178,804,853,873]" pageId="39" pageNumber="1356">
<taxonomicName id="4C5C4D6D4633C750DAC6FCA9F763C51C" authorityName="ASHMEAD" authorityYear="1895" box="[178,307,853,873]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="39" pageNumber="1356" phylum="Arthropoda" rank="tribe" tribe="Listrodromini">Listrodromini</taxonomicName>
