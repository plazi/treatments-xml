<paragraph id="B69C5A61F457D48FF3B9A32DE8242DD0" pageId="0" pageNumber="73">Type material.</paragraph>
<paragraph id="F96844AD87BB36689849ADE5FF076C95" pageId="0" pageNumber="73">
<emphasis id="D2D204104B91297E02BDBF044A3A540D" italics="true" pageId="0" pageNumber="73">Holotype</emphasis>
