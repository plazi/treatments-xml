<paragraph id="5B4D0F13FF9D8064FF23FAFAFEDBFAD2" blockId="38.[151,360,1328,1354]" box="[151,360,1328,1354]" pageId="38" pageNumber="39">
<heading id="0005B87FFF9D8064FF23FAFAFEDBFAD2" bold="true" box="[151,360,1328,1354]" fontSize="11" level="1" pageId="38" pageNumber="39" reason="1">
