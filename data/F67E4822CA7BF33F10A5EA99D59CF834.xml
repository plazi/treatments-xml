<taxonomicName id="B9D782B7CA7BF33F1230EBB0D4AEF834" box="[514,640,2000,2024]" class="Magnoliopsida" family="Asteraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="12" pageNumber="333" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
