<taxonomicName id="B9D782B7CA7BF33F12CEEBCCD561F81F" box="[764,847,1964,1987]" class="Magnoliopsida" family="Fabaceae" genus="Cytisus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="12" pageNumber="333" phylum="Tracheophyta" rank="genus">
<emphasis id="4CA32526CA7BF33F12CEEBCCD561F81F" box="[764,847,1964,1987]" italics="true" pageId="12" pageNumber="333">Cytisus</emphasis>
</taxonomicName>
