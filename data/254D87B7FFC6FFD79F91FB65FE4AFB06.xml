<taxonomicName id="6AE44D22FFC6FFD59D20FAB3FB8EFACA" authorityName="Fabricius" authorityYear="1793" box="[797,1030,1287,1323]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="order">ODONATA</taxonomicName>
