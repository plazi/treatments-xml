<taxonomicName id="53E0C17483B76CDF097636BBFDFE37DC" genus="Pachyceble" lsidName="Pachyceble lanei" pageId="1" pageNumber="22" rank="species" species="lanei">Pachyceble lanei</taxonomicName>
