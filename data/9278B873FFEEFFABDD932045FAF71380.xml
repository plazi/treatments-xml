<materialsCitation id="AAB90338FFEEFFABDC6A208EFCE21F10" collectorName="La Cruz &amp; Paraje Tres Cerros" country="Argentina" latitude="-29.112501" location="ARGENTINA" longLatPrecision="20" longitude="-56.933613" pageId="16" pageNumber="431" specimenCount="1" specimenCount-female="1">
