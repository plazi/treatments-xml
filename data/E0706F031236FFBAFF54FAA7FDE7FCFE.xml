, 
<taxonomicName id="AFD9A5961237FFBAFF35FE46FECEFE15" authorityName="Monterosato" authorityYear="1883" box="[246,363,439,464]" class="Bivalvia" family="Mytilidae" genus="Mytilaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mytiloida" pageId="77" pageNumber="78" phylum="Mollusca" rank="genus">Mytilaster</taxonomicName>
