<quantity id="4CD19B51FFDEAD67ADC2FA5A6C4EBFC8" box="[938,1068,1484,1508]" metricMagnitude="2" metricUnit="m" metricValue="8.74" metricValueMax="9.27" metricValueMin="8.209999999999999" pageId="4" pageNumber="387" unit="m" value="874.0" valueMax="927.0" valueMin="821.0">
821– 
<specimenCount id="9D2FFD3DFFDEAD67AD8AFA5A6C4EBFC8" box="[994,1068,1484,1508]" pageId="4" pageNumber="387" type="generic">927 m</specimenCount>
