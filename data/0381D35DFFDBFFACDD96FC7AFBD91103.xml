<taxonomicName id="4C2819C8FFDAFFACDE9DFB10FCC210A3" authority="Mifsud &amp; Burckhardt, 2002" authorityName="Mifsud &amp; Burckhardt" authorityYear="2002" box="[703,829,1164,1188]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="1971" phylum="Arthropoda" rank="species" species="ambigua" status="sp. n.">
