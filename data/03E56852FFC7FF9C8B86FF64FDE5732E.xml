<taxonomicName id="4C4CA2C7FFC7FF9089B7FDB4FCDF734C" authority="Asenjo &amp; Zampaulo &amp; Ferreira, 2018" authorityName="Asenjo &amp; Zampaulo &amp; Ferreira" authorityYear="2018" box="[678,853,583,606]" class="Insecta" family="Staphylinidae" genus="Oxarthrius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="409" phylum="Arthropoda" rank="species" species="inexpectatus" status="sp. nov.">
<emphasis id="B9380556FFC7FF9089B7FDB4FCDF734C" box="[678,853,583,606]" italics="true" pageId="5" pageNumber="409">O. inexpectatus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20BB82DFFC7FF90884DFDB5FC3F734C" box="[860,949,582,607]" pageId="5" pageNumber="409" rank="species">
<emphasis id="B9380556FFC7FF90884DFDB5FC24734D" bold="true" box="[860,942,582,607]" pageId="5" pageNumber="409">sp. nov</emphasis>
