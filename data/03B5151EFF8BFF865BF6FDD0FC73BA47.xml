: 
<taxonomicName id="4C1CDF8BFF8BFF865A07FBD5FE37BB12" authorityName="FAIRMAIRE &amp; BRISOUT" authorityYear="1859" box="[358,484,1101,1122]" class="Insecta" family="Dytiscidae" genus="Hydroporus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="600" phylum="Arthropoda" rank="species" species="discretus">
