<heading id="D0A4DF41F32DBE40201DF92BFC1DF91E" box="[310,938,1667,1692]" centered="true" fontSize="10" level="2" pageId="7" pageNumber="1142" reason="2">
<taxonomicName id="4C5313AEF32DBE40201DF92BFD7DF91E" authority="Reis and Navia" authorityName="Reis and Navia" authorityYear="2014" box="[310,714,1667,1691]" class="Arachnida" family="Eriophyidae" genus="Auriculatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="7" pageNumber="1142" phylum="Arthropoda" rank="species" species="clitoria" status="gen. nov., sp. nov.">
<emphasis id="B927B43FF32DBE40201DF92BFDBBF91E" box="[310,524,1667,1691]" italics="true" pageId="7" pageNumber="1142">Auriculatus clitoria</emphasis>
Reis and Navia
</taxonomicName>
, 
<taxonomicNameLabel id="A2140944F32DBE4023FCF92CFC1DF91E" box="[727,938,1667,1692]" pageId="7" pageNumber="1142" rank="species">gen. nov., sp. nov.</taxonomicNameLabel>
