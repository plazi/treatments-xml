<subSubSection id="15C517CFFF8A2D35D24CFBE2763C9D10" pageId="1" pageNumber="2" type="type_taxon">
<paragraph id="5D604444FF8A2D35D24CFBE2763C9D10" blockId="1.[113,752,1025,1080]" pageId="1" pageNumber="2">
<emphasis id="6FAB9856FF8A2D35D24CFBE275429D30" box="[113,562,1025,1049]" italics="true" pageId="1" pageNumber="2">
<emphasis id="6FAB9856FF8A2D35D24CFBE2766B9D31" bold="true" box="[113,283,1025,1049]" italics="true" pageId="1" pageNumber="2">
<typeStatus id="8264FAE6FF8A2D35D24CFBE277D79D31" box="[113,167,1025,1049]" pageId="1" pageNumber="2">Type</typeStatus>
