<number id="342424BFFF9A3D3EB4F6FD5D769B30CF" box="[1194,1223,719,745]" pageId="7" pageNumber="38" value="60.0">60</number>
(
<number id="342424BFFF9A3D3EB484FD5D777930CF" box="[1240,1317,719,745]" pageId="7" pageNumber="38" value="59.5" valueMax="61.0" valueMin="58.0">58–61</number>
