<taxonomicName id="45EC4D1BFF943D30B54CFED673DB33A3" class="Arachnida" family="Eriophyidae" genus="Calepitrimerus" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="bungeanus">
