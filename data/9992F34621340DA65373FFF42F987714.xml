<taxonomicName id="BA7B79CD964D747E020DDDB3FFC8E6B6" authority="Verh." authorityName="Verh." class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="217" phylum="Arthropoda" rank="species" species="coxodentatus">
