<taxonomicName id="7FED4D32FFBBFFC10726FEE0FC977467" authorityName="Franz" authorityYear="1989" box="[614,794,330,357]" class="Insecta" family="Staphylinidae" genus="Scydmaenus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="366" phylum="Arthropoda" rank="species" species="kunmingensis">
