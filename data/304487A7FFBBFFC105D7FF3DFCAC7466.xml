<taxonomicName id="7FED4D32FFBBFFC1077AFE8CFD267443" baseAuthorityName="Sharp" baseAuthorityYear="1874" box="[570,683,295,321]" class="Insecta" family="Staphylinidae" genus="Scydmaenus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="366" phylum="Arthropoda" rank="species" species="vestitus">
