<taxonomicName id="89BF4D0F4B71C066FC49FA88176AFAD8" authority="Kornicker 1981" authorityName="Kornicker" authorityYear="1981" box="[981,1317,1343,1369]" class="Ostracoda" family="Cylindroleberididae" genus="Tetraleberis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="7" pageNumber="22" phylum="Arthropoda" rank="genus">
