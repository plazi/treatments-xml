<taxonomicName id="89BF4D0F4B71C066FEA0FAD010D7FA00" authority="Kornicker 1981" authorityName="Kornicker" authorityYear="1981" box="[316,664,1383,1409]" class="Ostracoda" family="Cylindroleberididae" genus="Amboleberis" kingdom="Animalia" order="Myodocopida" pageId="7" pageNumber="22" phylum="Arthropoda" rank="genus">
