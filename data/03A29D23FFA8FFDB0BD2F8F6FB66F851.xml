<heading id="D0FC9B59FFA8FFDA0BD2F8F6FD26F88B" bold="true" box="[151,783,1844,1870]" fontSize="11" level="1" pageId="4" pageNumber="255" reason="1">
<emphasis id="B97FF027FFA8FFDA0BD2F8F6FD26F88B" bold="true" box="[151,783,1844,1870]" pageId="4" pageNumber="255">
Genus 
<taxonomicName id="4C0B57B6FFA8FFDA0BABF8F6FCA9F88B" authority="Xu, Tang &amp; Skelley" authorityName="Xu, Tang &amp; Skelley" box="[238,640,1844,1870]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="4" pageNumber="255" phylum="Arthropoda" rank="genus">
<emphasis id="B97FF027FFA8FFDA0BABF8F6FFAFF88B" bold="true" box="[238,390,1844,1870]" italics="true" pageId="4" pageNumber="255">Cycadophila</emphasis>
