<taxonomicName id="06930FFEBB3D653126DFFE3243D3614E" class="Insecta" family="Chrysomelidae" genus="Aspidimorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aspidimorpha" order="Coleoptera" pageId="219" pageNumber="220" phylum="Arthropoda" rank="genus">Aspidimorpha</taxonomicName>
