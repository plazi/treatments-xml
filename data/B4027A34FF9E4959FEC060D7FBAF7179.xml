<quantity id="FB5366C7FF9E4959FD8861C4FD7873B1" box="[576,672,1439,1465]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="7" pageNumber="38" unit="mm" value="1.3">1.3 mm</quantity>
, width/length 
<number id="8A63D905FF9E4959FC8561C4FC5B73B1" box="[845,899,1439,1465]" pageId="7" pageNumber="38" value="0.4">0.40</number>
, c/ 
<geoCoordinate id="599FADE5FF9E4959FC6F61C4FC2573B1" box="[935,1021,1439,1465]" direction="west" orientation="longitude" pageId="7" pageNumber="38" precision="555" value="-0.65">
w 
<number id="8A63D905FF9E4959FC0F61C4FC2573B1" box="[967,1021,1439,1465]" pageId="7" pageNumber="38" value="0.65">0.65</number>
</geoCoordinate>
, R 
