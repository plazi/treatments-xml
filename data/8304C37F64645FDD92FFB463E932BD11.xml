<taxonomicName id="CA76093242AE3AA4ABA5073E61EDBF80" authorityName="Galiano" authorityYear="1987" class="Arachnida" family="Salticidae" genus="Aillutticus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aillutticus" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
