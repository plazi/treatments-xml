<geoCoordinate id="EE225027FFEDFFF0FC60FC2E1BDBFB86" box="[910,989,1019,1043]" degrees="0.53" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-0.53">w 0.53</geoCoordinate>
