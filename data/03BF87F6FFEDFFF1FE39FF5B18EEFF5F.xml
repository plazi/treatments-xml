; F2 l/ 
<geoCoordinate id="EE225027FFEDFFF0FD73FCFF1AEBFCD7" box="[669,749,810,834]" degrees="1.88" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-1.88">w 1.88</geoCoordinate>
; F3 l/ 
<geoCoordinate id="EE225027FFEDFFF0FCC8FCFF1B73FCD7" box="[806,885,810,834]" degrees="1.63" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-1.63">w 1.63</geoCoordinate>
; clava l/ 
<geoCoordinate id="EE225027FFEDFFF0FC22FCFF1C1DFCD7" box="[972,1051,810,834]" degrees="3.10" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-3.1">w 3.10</geoCoordinate>
; ped l/F1 l 0.77; F1 l/F2 l 1.73; F1 l/F3 l 2.0; F1 l/clava l 0.84; F2 l/clava l 0.48; F3 l/clava l 0.42; F1 w/ped 
<geoCoordinate id="EE225027FFEDFFF0FF4EFCA518F4FC1D" box="[160,242,880,904]" degrees="1.33" direction="west" orientation="longitude" pageId="10" pageNumber="630" precision="555" value="-1.33">w 1.33</geoCoordinate>
