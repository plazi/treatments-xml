<quantity id="4CA54AB70115FFFB6BD5FA01DCDAB1CD" box="[1169,1254,1450,1476]" metricMagnitude="-3" metricUnit="m" metricValue="6.6" pageId="46" pageNumber="47" unit="mm" value="6.6">6.6mm</quantity>
