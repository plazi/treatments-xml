<quantity id="4CA54AB70115FFFB6C36FA01DBF4B1CD" box="[882,968,1450,1476]" metricMagnitude="-3" metricUnit="m" metricValue="9.6" pageId="46" pageNumber="47" unit="mm" value="9.6">9.6mm</quantity>
