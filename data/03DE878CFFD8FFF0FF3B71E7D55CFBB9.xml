, 
<collectingCountry id="F360760AFFD8FFF0FC5F72A5D462FCC1" box="[1011,1138,757,783]" name="Costa Rica" pageId="32" pageNumber="33">Costa Rica</collectingCountry>
, 
<collectingCountry id="F360760AFFD8FFF0FB2C72A5D4CFFCC1" box="[1152,1247,757,783]" name="Ecuador" pageId="32" pageNumber="33">Ecuador</collectingCountry>
, 
<collectingCountry id="F360760AFFD8FFF0FB4172A5D588FCC1" box="[1261,1432,757,783]" name="French Guiana" pageId="32" pageNumber="33">French Guiana</collectingCountry>
, 
<collectingCountry id="F360760AFFD8FFF0FF3B7349D103FCFD" box="[151,275,793,819]" name="Guatemala" pageId="32" pageNumber="33">Guatemala</collectingCountry>
, 
<collectingCountry id="F360760AFFD8FFF0FE8C7349D180FCFD" box="[288,400,793,819]" name="Honduras" pageId="32" pageNumber="33">Honduras</collectingCountry>
, 
<collectingCountry id="F360760AFFD8FFF0FE317349D203FCFD" box="[413,531,793,819]" name="Nicaragua" pageId="32" pageNumber="33">Nicaragua</collectingCountry>
, and 
<collectingCountry id="F360760AFFD8FFF0FDFE734AD2BBFCFA" box="[594,683,794,820]" name="Panama" pageId="32" pageNumber="33">Panama</collectingCountry>
(
<bibRefCitation id="EFE64B6BFFD8FFF0FD177349D375FCFD" author="Metcalf, Z. P." box="[699,869,793,819]" pageId="32" pageNumber="33" pagination="1 - 585" refId="ref71633" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
; Sanborn 2011a; 2013; 2014a; 2018c; 
<bibRefCitation id="EFE64B6BFFD8FFF0FA8C7349D107FC99" author="Sanborn, A. F. &amp; Maes, J. - M." pageId="32" pageNumber="33" pagination="437 - 442" refId="ref74898" refString="Sanborn, A. F. &amp; Maes, J. - M. (2012) Checklist of the cicadas (Insecta: Hemiptera: Cicadidae) of Nicaragua including new records for seventeen species. Check List, 8, 437 - 442. https: // doi. org / 10.15560 / 8.3.437" type="journal article" year="2012">Sanborn &amp; Maes 2012</bibRefCitation>
; 
<bibRefCitation id="EFE64B6BFFD8FFF0FE8E736ED1F0FC99" author="Maes, J. - M. &amp; Moulds, M. &amp; Sanborn, A. F." box="[290,480,829,856]" pageId="32" pageNumber="33" pagination="1 - 138" refId="ref71321" refString="Maes, J. - M., Moulds, M. &amp; Sanborn, A. F. (2012) Cicadidae (Homoptera) de Nicaragua: catalogo ilustrado, incluyendo especies exoticas del Museo Entomologico de Leon. Revista Nicaraguense Entomologia, 72 (Suplemento 2), 1 - 138." type="journal article" year="2012">
