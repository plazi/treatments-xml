<collectingRegion id="4B4956BE3166FFE4FBEEFC1F1183FC59" box="[1137,1176,898,924]" country="Serbia" name="Sid" pageId="28" pageNumber="29">SD</collectingRegion>
=0.10); 
<specimenCount id="9F8B53D53166FFE4FB68FC1F1087FC59" box="[1271,1436,898,924]" pageId="28" pageNumber="29" type="generic">one specimen</specimenCount>
fully winged, 
<emphasis id="BBF9444E3166FFE4FED0FC3A14CBFC04" bold="true" box="[335,464,935,961]" pageId="28" pageNumber="29">TSL/WW</emphasis>
=1.17; 
<emphasis id="BBF9444E3166FFE4FDB1FC3A1773FC04" bold="true" box="[558,616,935,961]" pageId="28" pageNumber="29">m/st</emphasis>
=4.28; 
<emphasis id="BBF9444E3166FFE4FD58FC3A1676FC04" bold="true" box="[711,877,935,961]" pageId="28" pageNumber="29">HWW/HWS</emphasis>
=1.53; 
<emphasis id="BBF9444E3166FFE4FC53FC3A1193FC04" bold="true" box="[972,1160,935,961]" pageId="28" pageNumber="29">T1W/T1+T2L</emphasis>
=1.25–2.66 (m=1.88, 
<collectingRegion id="4B4956BE3166FFE4FF08FC5015A5FC22" box="[151,190,973,999]" country="Serbia" name="Sid" pageId="28" pageNumber="29">SD</collectingRegion>
=0.48); 
<emphasis id="BBF9444E3166FFE4FE86FC501482FC22" bold="true" box="[281,409,973,999]" pageId="28" pageNumber="29">T3W/TSL</emphasis>
=0.93–1.11 (m=1.03, 
<collectingRegion id="4B4956BE3166FFE4FD08FC5017A5FC22" box="[663,702,973,999]" country="Serbia" name="Sid" pageId="28" pageNumber="29">SD</collectingRegion>
=0.06)
