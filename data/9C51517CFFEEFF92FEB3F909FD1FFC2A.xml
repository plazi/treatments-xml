<emphasis id="268C3C78FFEEFF91FEB3F909FC58F94A" bold="true" box="[264,981,1724,1750]" pageId="4" pageNumber="65">
<taxonomicName id="D3F89BE9FFEEFF91FEB3F909FD6DF94A" authority="Kohlmann &amp; Solis" authorityName="Kohlmann &amp; Solis" box="[264,736,1724,1750]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="inusitatus" status="sp. nov.">
<emphasis id="268C3C78FFEEFF91FEB3F909FE62F94A" bold="true" box="[264,495,1724,1750]" italics="true" pageId="4" pageNumber="65">Canthon inusitatus</emphasis>
Kohlmann &amp; Solís
</taxonomicName>
, 
