<taxonomicName id="4C1363E56AB7F593FBFAFAAA8EDCFAE8" authorityName="Blazewicz-Paszkowycz" authorityYear="2007" box="[1131,1212,1321,1346]" class="Malacostraca" family="Typhlotanaidae" genus="Typhlotanais" kingdom="Animalia" order="Tanaidacea" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="eximus">
