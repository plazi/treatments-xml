<taxonomicName id="4C5E719DFF96FFC581CDFE7DFDDAFE13" authority="(LINNAEUS 1758)" baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[178,568,422,444]" class="Insecta" family="Ichneumonidae" genus="Rhyssa" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="442" phylum="Arthropoda" rank="species" species="persuasoria">
