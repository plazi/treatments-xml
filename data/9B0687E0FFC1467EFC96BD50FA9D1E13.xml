<taxonomicName id="D4AF4D75FFC1467EFA9ABD50FC63194B" authorityName="Frison" authorityYear="1942" class="Insecta" family="Perlodidae" genus="Isoperla" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="168" phylum="Arthropoda" rank="species" species="dicala">
