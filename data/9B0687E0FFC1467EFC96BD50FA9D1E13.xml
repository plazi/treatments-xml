<collectingRegion id="D16BF814FFC1467EFABCBDC7FBAB19DB" isEnumeration="true" pageId="6" pageNumber="168">New Brunswick and Maine</collectingRegion>
(
<collectorName id="BE5A5320FFC1467EFB8FBDABFB5419DB" box="[1114,1205,984,1008]" pageId="6" pageNumber="168">DeWalt</collectorName>
et al. 2018). 
<taxonomicName id="D4AF4D75FFC1467EFA9ABDABFC631E3F" authorityName="Frison" authorityYear="1942" class="Insecta" family="Perlodidae" genus="Isoperla" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="168" phylum="Arthropoda" rank="species" species="dicala">
<emphasis id="21DBEAE4FFC1467EFA9ABDABFC631E3F" italics="true" pageId="6" pageNumber="168">Isoperla dicala</emphasis>
