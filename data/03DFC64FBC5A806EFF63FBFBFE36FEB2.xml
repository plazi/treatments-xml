<taxonomicName id="4C760CDABC5B8069FE4BFADFFDE4FAAE" box="[447,614,1350,1374]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="512" phylum="Arthropoda" rank="species" species="undetermined">
