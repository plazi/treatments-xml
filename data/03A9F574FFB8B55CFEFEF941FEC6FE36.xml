<taxonomicName id="4C003FE1FFB9B553FE67F980FDC4F9C5" box="[401,565,1550,1573]" class="Eurotatoria" family="Notommatidae" genus="Resticula" kingdom="Animalia" order="Ploima" pageId="7" pageNumber="44" phylum="Rotifera" rank="species" species="nyssa">
