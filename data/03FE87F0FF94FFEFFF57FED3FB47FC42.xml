<taxonomicName id="4C574D65FF94FFEFFD9BFC0EFD04FC42" authorityName=": Schultz" authorityYear="2009" box="[603,717,921,945]" class="Echinoidea" family="Clypeasteridae" genus="Clypeaster" kingdom="Animalia" order="Clypeasteroida" pageId="51" pageNumber="52" phylum="Echinodermata" rank="species" species="fervens">
<emphasis id="B923EAF4FF94FFEFFD9BFC0EFD04FC42" box="[603,717,921,945]" italics="true" pageId="51" pageNumber="52">C. fervens</emphasis>
</taxonomicName>
