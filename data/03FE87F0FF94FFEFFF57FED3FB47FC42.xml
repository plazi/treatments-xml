<quantity id="4CAF9B03FF94FFEFFC1DFCE2FBEFFC7F" box="[989,1062,885,909]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="51" pageNumber="52" unit="m" value="100.0">100 m</quantity>
