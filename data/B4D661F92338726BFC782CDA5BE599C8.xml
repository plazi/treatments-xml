<taxonomicName id="8CC6EEADBFCF573BE5A25576424EEB7D" class="Insecta" family="Braconidae" genus="Sigalphus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sigalphus rufescens" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="species" species="rufescens">Sigalphus rufescens</taxonomicName>
