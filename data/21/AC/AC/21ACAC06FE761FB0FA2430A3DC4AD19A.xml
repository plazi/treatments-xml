<taxonomicName baseAuthorityName="LeConte" baseAuthorityYear="1878" class="Insecta" family="Carabidae" genus="Rhadine" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="737" pageNumber="738" phylum="Arthropoda" rank="species" species="jejuna">
