<taxonomicName authority="Hadzi, 1951" authorityName="Hadzi" authorityYear="1951" box="[610,1023,871,898]" class="Heterotrichea" family="Folliculinidae" genus="Epifolliculina" higherTaxonomySource="GBIF" kingdom="Chromista" order="Heterotrichida" pageId="9" pageNumber="556" phylum="Ciliophora" rank="species" species="diaphana">
