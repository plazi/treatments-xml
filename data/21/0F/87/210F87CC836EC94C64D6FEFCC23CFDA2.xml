<collectingCountry box="[1263,1350,366,392]" name="Canada" pageId="3" pageNumber="550">Canada</collectingCountry>
, 
<collectingRegion country="Canada" name="British Columbia" pageId="3" pageNumber="550">British Columbia</collectingRegion>
