<collectingCountry box="[453,515,945,971]" name="Syria" pageId="7" pageNumber="408">Syria</collectingCountry>
, 
<collectingCountry box="[525,605,945,971]" name="Turkey" pageId="7" pageNumber="408">Turkey</collectingCountry>
(
