<paragraph blockId="12.[640,1112,753,809]" lastBlockId="13.[136,608,176,360]" lastPageId="13" lastPageNumber="207" pageId="12" pageNumber="206">
<emphasis bold="true" box="[640,757,753,777]" pageId="12" pageNumber="206">Diagnosis.</emphasis>
