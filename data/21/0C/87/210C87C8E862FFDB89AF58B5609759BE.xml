<number box="[505,519,1652,1676]" pageId="17" pageNumber="18" value="5.0">5</number>
(
<number box="[529,539,1652,1676]" pageId="17" pageNumber="18" value="1.0">1</number>
)- 
<number box="[557,572,1652,1676]" pageId="17" pageNumber="18" value="6.0">6</number>
(
