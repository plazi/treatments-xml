), 
<taxonomicName box="[1115,1302,1698,1723]" class="Demospongiae" family="Coelosphaeridae" kingdom="Animalia" order="Poecilosclerida" pageId="33" pageNumber="34" phylum="Porifera" rank="family">Coelosphaeridae</taxonomicName>
