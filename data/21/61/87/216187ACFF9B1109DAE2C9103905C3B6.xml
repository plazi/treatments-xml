(
<taxonomicName box="[604,699,1663,1689]" class="Reptilia" higherTaxonomySource="GBIF" kingdom="Animalia" pageId="41" pageNumber="94" phylum="Arthropoda" rank="class">Reptilia</taxonomicName>
