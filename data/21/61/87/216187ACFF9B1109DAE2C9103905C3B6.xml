<taxonomicName authorityName="E. Olivier" authorityYear="1907" box="[1238,1437,1555,1581]" class="Insecta" family="Lampyridae" genus="Photinus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="94" phylum="Arthropoda" rank="species" species="decorus">
<emphasis box="[1238,1437,1555,1581]" italics="true" pageId="41" pageNumber="94">Photinus decorus</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Motschulsky" authorityYear="1854" box="[198,345,1591,1617]" class="Insecta" family="Lampyridae" genus="Robopus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="94" phylum="Arthropoda" rank="species" species="roseicollis">
