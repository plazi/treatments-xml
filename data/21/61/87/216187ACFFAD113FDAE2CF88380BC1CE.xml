<taxonomicName class="Insecta" family="Lampyridae" genus="Photinus" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="84" phylum="Arthropoda" rank="species" species="maratimus">
<emphasis italics="true" pageId="31" pageNumber="84">Photinus maratimus</emphasis>
</taxonomicName>
