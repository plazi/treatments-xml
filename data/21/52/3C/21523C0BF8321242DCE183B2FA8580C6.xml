<paragraph blockId="3.[151,694,1438,1464]" box="[151,694,1438,1464]" pageId="3" pageNumber="48">
<heading bold="true" box="[151,694,1438,1464]" fontSize="11" level="1" pageId="3" pageNumber="48" reason="1">
<treatmentCitationGroup box="[151,694,1438,1464]" pageId="3" pageNumber="48">
<taxonomicName authority="Priesner 1933: 194" authorityName="Priesner" authorityPageNumber="194" authorityYear="1933" box="[151,694,1438,1464]" class="Insecta" family="Thripidae" genus="Scolothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="48" phylum="Arthropoda" rank="species" species="lanzarotensis">
