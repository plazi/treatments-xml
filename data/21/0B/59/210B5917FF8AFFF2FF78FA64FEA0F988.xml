<taxonomicName baseAuthorityName="Tschek" baseAuthorityYear="1871" box="[342,588,1383,1405]" class="Insecta" family="Ichneumonidae" genus="Campoletis" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="222" phylum="Arthropoda" rank="species" species="crassicornis">
