<taxonomicName authority="Pascoe" authorityName="Pascoe. Unfortunately" authorityYear="1865" box="[158,423,1625,1650]" class="Insecta" family="Curculionidae" genus="Asytesta" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="maura">
