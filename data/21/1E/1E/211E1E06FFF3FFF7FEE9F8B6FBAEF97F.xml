<taxonomicName authorityName="U. AKKEMIK" authorityYear="2019" box="[1194,1305,1084,1108]" class="Pinopsida" family="Cupressaceae" genus="Cupressinoxylon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="0" pageNumber="289" phylum="Tracheophyta" rank="species" species="pliocenica">
