<pageBreakToken pageId="null" pageNumber="856">Pastinaca</pageBreakToken>
<normalizedToken originalValue="satíva" pageId="null" pageNumber="856">sativa</normalizedToken>
<authorityName pageId="null" pageNumber="856">L.</authorityName>
</taxonomicName>
</paragraph>
