<number box="[597,626,1617,1643]" pageId="2" pageNumber="3" value="19.0">19</number>
marginal setae. Tergal setae: I, 
<number box="[1001,1033,1617,1643]" pageId="2" pageNumber="3" value="27.0">27</number>
; II–VII, 
<number box="[1134,1210,1617,1643]" pageId="2" pageNumber="3" value="35.0" valueMax="39.0" valueMin="31.0">31–39</number>
; VIII, 
<number box="[1286,1319,1617,1643]" pageId="2" pageNumber="3" value="25.0">25</number>
