<number box="[1019,1052,1657,1683]" pageId="2" pageNumber="3" value="27.0">27</number>
; IV–V, 
<number box="[1146,1223,1657,1683]" pageId="2" pageNumber="3" value="52.5" valueMax="53.0" valueMin="52.0">52–53</number>
; VI, 
<number box="[1286,1318,1657,1683]" pageId="2" pageNumber="3" value="65.0">65</number>
; VII, 
<number box="[321,353,1697,1723]" pageId="2" pageNumber="3" value="41.0">41</number>
; VIII, 
<number box="[434,467,1697,1723]" pageId="2" pageNumber="3" value="28.0">28</number>
