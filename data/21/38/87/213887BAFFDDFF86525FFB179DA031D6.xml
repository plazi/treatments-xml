<number box="[563,577,1479,1505]" pageId="9" pageNumber="10" value="4.0">4</number>
setae, 
<number box="[662,735,1479,1505]" pageId="9" pageNumber="10" value="24.5" valueMax="25.0" valueMin="24.0">24–25</number>
other setae; III, 
<number box="[934,1011,1479,1505]" pageId="9" pageNumber="10" value="24.0" valueMax="25.0" valueMin="23.0">23–25</number>
; IV, 
<number box="[1068,1144,1479,1505]" pageId="9" pageNumber="10" value="33.0" valueMax="35.0" valueMin="31.0">31–35</number>
; V, 
<number box="[1192,1268,1479,1505]" pageId="9" pageNumber="10" value="39.0" valueMax="40.0" valueMin="38.0">38–40</number>
; VI, 
<number box="[264,340,1519,1545]" pageId="9" pageNumber="10" value="36.0" valueMax="37.0" valueMin="35.0">35–37</number>
