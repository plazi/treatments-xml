<number box="[683,757,1873,1899]" pageId="5" pageNumber="6" value="25.5" valueMax="29.0" valueMin="22.0">22–29</number>
other setae; III, 
<number box="[947,1024,1873,1899]" pageId="5" pageNumber="6" value="28.0" valueMax="31.0" valueMin="25.0">25–31</number>
; IV, 
<number box="[1077,1153,1873,1899]" pageId="5" pageNumber="6" value="48.0" valueMax="51.0" valueMin="45.0">45–51</number>
