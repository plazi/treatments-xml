<number box="[858,935,559,585]" pageId="7" pageNumber="8" value="69.0" valueMax="74.0" valueMin="64.0">64–74</number>
; VI, 
<number box="[993,1070,559,585]" pageId="7" pageNumber="8" value="53.5" valueMax="58.0" valueMin="49.0">49–58</number>
