<quantity box="[901,1005,1620,1646]" metricMagnitude="-1" metricUnit="m" metricValue="7.747" metricValueMax="8.382" metricValueMin="7.112" pageId="12" pageNumber="13" unit="in" value="30.5" valueMax="33.0" valueMin="28.0">28–33 in</quantity>
