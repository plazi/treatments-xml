<quantity box="[353,460,1793,1819]" metricMagnitude="-1" metricUnit="m" metricValue="6.731" metricValueMax="7.366" metricValueMin="6.096" pageId="7" pageNumber="8" unit="in" value="26.5" valueMax="29.0" valueMin="24.0">24–29 in</quantity>
ventral. Dimensions: TW, 
<number box="[805,926,1793,1819]" pageId="7" pageNumber="8" value="0.46499999999999997" valueMax="0.48" valueMin="0.45">0.45–0.48</number>
; HL, 
<number box="[1000,1121,1793,1819]" pageId="7" pageNumber="8" value="0.29500000000000004" valueMax="0.31" valueMin="0.28">0.28–0.31</number>
