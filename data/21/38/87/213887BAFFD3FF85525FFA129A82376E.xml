<number box="[536,656,1833,1859]" pageId="7" pageNumber="8" value="0.41000000000000003" valueMax="0.43" valueMin="0.39">0.39–0.43</number>
; MSL, 
<number box="[749,871,1833,1859]" pageId="7" pageNumber="8" value="0.15000000000000002" valueMax="0.16" valueMin="0.14">0.14–0.16</number>
