<number box="[901,1022,679,705]" pageId="11" pageNumber="12" value="0.405" valueMax="0.43" valueMin="0.38">0.38–0.43</number>
; MSL, 
<number box="[1108,1229,679,705]" pageId="11" pageNumber="12" value="0.13" valueMax="0.14" valueMin="0.12">0.12–0.14</number>
