<taxonomicName genus="Zingiberaceae" lsidName="Zingiberaceae" pageId="265" pageNumber="266" rank="genus">Zingiberaceae</taxonomicName>
) (
<bibRefCitation author="Uhmann, E" journalOrPublisher="The Annals and Magazine of Natural History" pageId="295" pageNumber="296" pagination="324 - 337" title="Hispinen aus dem Britischen Museum. V. Teil. 129. Beitrag zur Kenntnis der Hispinae (Coleopt., Chrysom.)." url="10.1080/00222935008654055" volume="3" year="1950 a">Uhmann 1950a</bibRefCitation>
); 
<taxonomicName class="Liliopsida" family="Zingiberaceae" genus="Renealmia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Renealmia" order="Zingiberales" pageId="265" pageNumber="266" phylum="Tracheophyta" rank="genus">Renealmia</taxonomicName>
sp. (
<taxonomicName genus="Zingiberaceae" lsidName="Zingiberaceae" pageId="265" pageNumber="266" rank="genus">Zingiberaceae</taxonomicName>
) (
<bibRefCitation author="McKenna, DD" journalOrPublisher="Molecular Phylogenetics and Evolution" pageId="290" pageNumber="291" pagination="117 - 131" title="Molecular phylogenetics and evolution of host plant use in the Neotropical rolled leaf ' hispine' beetle genus Cephaloleia (Chevrolat) (Coleoptera: Cassidinae)." url="10.1016/j.ympev.2005.06.011" volume="37" year="2005">McKenna and Farrell 2005</bibRefCitation>
); 
<taxonomicName class="Liliopsida" family="Costaceae" genus="Costus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Costus pulverulentus" order="Zingiberales" pageId="265" pageNumber="266" phylum="Tracheophyta" rank="species" species="pulverulentus">Costus pulverulentus</taxonomicName>
, C. Presl. (
<taxonomicName genus="Costaceae" lsidName="Costaceae" pageId="265" pageNumber="266" rank="genus">Costaceae</taxonomicName>
) (
<bibRefCitation author="Meskins, C" journalOrPublisher="International Journal of Tropical Insect Science" pageId="290" pageNumber="291" pagination="159 - 171" title="A comparison of hispine beetles (Coleoptera: Chrysomelidae) associated with three orders of monocot host plants in lowland Panama." volume="27" year="2008">Meskins et al. 2008</bibRefCitation>
); 
<taxonomicName class="Liliopsida" family="Zingiberaceae" genus="Renealmia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Renealmia pluriplicata" order="Zingiberales" pageId="265" pageNumber="266" phylum="Tracheophyta" rank="species" species="pluriplicata">Renealmia pluriplicata</taxonomicName>
