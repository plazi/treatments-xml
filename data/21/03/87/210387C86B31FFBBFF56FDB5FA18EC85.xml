<emphasis box="[1099,1256,676,699]" italics="true" pageId="3" pageNumber="67">
... 
<taxonomicName authority="Drake and Poor" authorityName="Drake and Poor" box="[1124,1256,676,699]" class="Insecta" family="Tingidae" genus="Cottothucha" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="67" phylum="Arthropoda" rank="genus">Cottothucha</taxonomicName>
