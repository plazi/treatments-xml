<subSubSection type="description">
<paragraph pageId="19" pageNumber="418">
Measurements. - Length: 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="5.33" metricValueMax="5.66" metricValueMin="5.0" unit="�m" value="533.0" valueMax="566.0" valueMin="500.0">
