<taxonomicName id="4C294D22845408691AF5368B62D7F971" authorityName="Simon" authorityYear="1887" box="[917,1010,1701,1725]" class="Arachnida" family="Caponiidae" genus="Caponia" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
