<taxonomicName id="1BBC0DFEFF84FFB39AA1D5D6FD9C529A" box="[415,585,1667,1689]" class="Placodermi" family="Plourdosteidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Arthrodira" pageId="17" pageNumber="212" phylum="Chordata" rank="family">Plourdosteidae</taxonomicName>
