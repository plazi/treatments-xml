<quantity id="4CF7058AC214FFEEFC67FA98FBDC17C6" box="[1000,1144,2006,2032]" metricMagnitude="2" metricUnit="m" metricValue="2.75" metricValueMax="5.0" metricValueMin="0.5" pageId="55" pageNumber="56" unit="m" value="275.0" valueMax="500.0" valueMin="50.0">50 to 500 m</quantity>
