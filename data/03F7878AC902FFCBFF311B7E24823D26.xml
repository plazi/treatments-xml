<quantity id="4CA69B79C902FFC4FE5D1823249D3A70" box="[507,680,554,580]" metricMagnitude="-3" metricUnit="m" metricValue="2.8499999999999996" metricValueMax="2.9" metricValueMin="2.8" pageId="7" pageNumber="46" unit="mm" value="2.8499999999999996" valueMax="2.9" valueMin="2.8">2.80–2.90 mm</quantity>
