<quantity id="4CA69B79C902FFC4FCA81823258E3A70" box="[782,955,554,580]" metricMagnitude="-3" metricUnit="m" metricValue="3.35" metricValueMax="3.5" metricValueMin="3.2" pageId="7" pageNumber="46" unit="mm" value="3.35" valueMax="3.5" valueMin="3.2">3.20–3.50 mm</quantity>
(female); including forewing 
<quantity id="4CA69B79C902FFC4FA83182326F03A5E" metricMagnitude="-3" metricUnit="m" metricValue="5.0" metricValueMax="5.1" metricValueMin="4.9" pageId="7" pageNumber="46" unit="mm" value="5.0" valueMax="5.1" valueMin="4.9">4.90–5.10 mm</quantity>
