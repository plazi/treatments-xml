<quantity id="4CA69B79C902FFC4FE81185927E43A5E" box="[295,465,592,618]" metricMagnitude="-3" metricUnit="m" metricValue="5.55" metricValueMax="5.7" metricValueMin="5.4" pageId="7" pageNumber="46" unit="mm" value="5.55" valueMax="5.7" valueMin="5.4">5.40–5.70 mm</quantity>
(female); forewing length 
<quantity id="4CA69B79C902FFC4FCAA185925823A5E" box="[780,951,592,618]" metricMagnitude="-3" metricUnit="m" metricValue="4.25" metricValueMax="4.4" metricValueMin="4.1" pageId="7" pageNumber="46" unit="mm" value="4.25" valueMax="4.4" valueMin="4.1">4.10–4.40 mm</quantity>
(male), 
<quantity id="4CA69B79C902FFC4FBBF185922F13A5E" box="[1049,1220,592,618]" metricMagnitude="-3" metricUnit="m" metricValue="4.65" metricValueMax="4.7" metricValueMin="4.6" pageId="7" pageNumber="46" unit="mm" value="4.65" valueMax="4.7" valueMin="4.6">4.60–4.70 mm</quantity>
