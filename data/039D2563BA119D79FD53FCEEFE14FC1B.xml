<subSubSection id="C32EC7FEBA119D78FD53FC89FC4DFBAD" pageId="100" pageNumber="101" type="reference_group">
<paragraph id="8B8B9475BA119D78FD53FC89FB1EFC6B" blockId="100.[700,1229,830,931]" pageId="100" pageNumber="101">
<treatmentCitationGroup id="AB24B35BBA119D78FD53FC89FB1EFC6B" pageId="100" pageNumber="101">
Now 
<taxonomicName id="4C34EFF6BA119D78FD16FC89FCAAFC41" authority="(Swinhoe, 1870)" baseAuthorityName="Swinhoe" baseAuthorityYear="1870" class="Aves" family="Vireonidae" genus="Erpornis" kingdom="Animalia" order="Passeriformes" pageId="100" pageNumber="101" phylum="Chordata" rank="subSpecies" species="zantholeuca" subSpecies="tyrannulus">
