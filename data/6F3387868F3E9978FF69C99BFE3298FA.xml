<number id="515224B78F3E9978FDA2CB35FD5B9E4E" box="[604,659,566,591]" pageId="15" pageNumber="316" value="6.5" valueMax="12.0" valueMin="1.0">1–12</number>
.viii. 
<number id="515224B78F3E9978FD3ACB35FCC99E4E" box="[708,769,566,591]" pageId="15" pageNumber="316" value="1999.0">1999</number>
