<collectingCountry id="79F376568613FFB5FDCEFD5DFD75FCA6" box="[581,731,757,782]" name="United States of America" pageId="45" pageNumber="46">United States</collectingCountry>
to central 
<collectingCountry id="79F376568613FFB5FCDAFD5DFC6EFCA6" box="[849,960,757,782]" name="Argentina" pageId="45" pageNumber="46">Argentina</collectingCountry>
(
<collectingRegion id="C320F8248613FFB5FC44FD5DFBC4FCA6" box="[975,1130,757,782]" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="45" pageNumber="46">Buenos Aires</collectingRegion>
) (
<figureCitation id="99DF2A438613FFB5FB0AFD5DFB78FCA6" box="[1153,1238,757,782]" captionStart="FIGURE 37" captionStartId="15.[151,250,1277,1299]" captionTargetId="figure-322@15.[358,1229,193,1255]" captionTargetPageId="15" captionText="FIGURE 37. Distribution of Anaelia and Eusolenophora." figureDoi="http://doi.org/10.5281/zenodo.5121925" httpUri="https://zenodo.org/record/5121925/files/figure.png" pageId="45" pageNumber="46">Fig. 37</figureCitation>
