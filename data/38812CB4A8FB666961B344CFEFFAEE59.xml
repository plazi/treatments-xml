<paragraph id="AAC1070646250E138FFCE76A3459F7B4" pageId="35" pageNumber="174">
<taxonomicName id="6D203B34308692847015479DC11209FF" authorityName="Heraty" authorityYear="1994" class="Insecta" family="Eucharitidae" genus="Orasema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Orasema koghisiana" order="Hymenoptera" pageId="35" pageNumber="174" phylum="Arthropoda" rank="species" species="koghisiana">
