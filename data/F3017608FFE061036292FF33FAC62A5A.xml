<taxonomicNameLabel id="52EFA677FFE061026132F9E0FC062D65" box="[823,901,1550,1571]" pageId="4" pageNumber="105" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="BCA8BC9DFFE0610261B3F9E0FAE72D65" authority="(Bezzi)" authorityName="Bezzi" baseAuthorityName="Bezzi" box="[950,1380,1549,1571]" class="Insecta" family="Tephritidae" genus="Bactrocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="species" species="bipustualata" subGenus="Parazeugodacus">
