<number id="3DA82483B72D3738FDA3927DFD692B41" box="[591,644,348,374]" pageId="22" pageNumber="23" value="1.28">1.28</number>
(
<number id="3DA82483B72D3738FD7A927DFCFC2B41" box="[662,785,348,374]" pageId="22" pageNumber="23" value="1.275" valueMax="1.32" valueMin="1.23">1.23–1.32</number>
)X as long as femur, 
<number id="3DA82483B72D3738FBF8927DFBA42B41" box="[1044,1097,348,374]" pageId="22" pageNumber="23" value="2.88">2.88</number>
(
<number id="3DA82483B72D3738FBB7927DFB352B41" box="[1115,1240,348,374]" pageId="22" pageNumber="23" value="2.885" valueMax="2.93" valueMin="2.84">2.84–2.93</number>
