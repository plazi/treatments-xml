<taxonomicName id="C8E50FC406FF0D224D148960B4BBE2EC" class="Tremellomycetes" family="Tremellaceae" genus="Tremella" higherTaxonomySource="GBIF,CoL" kingdom="Fungi" order="Tremellales" phylum="Basidiomycota" rank="species" species="terrestris">Tremella terrestris</taxonomicName>
