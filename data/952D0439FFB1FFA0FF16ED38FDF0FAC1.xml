<quantity id="DA7C18CAFFB1FFA0FE29EDE7FDE0FAAB" box="[461,538,1279,1302]" metricMagnitude="-2" metricUnit="m" metricValue="3.4" pageId="59" pageNumber="290" unit="mm" value="34.0">34 mm</quantity>
), aspect proche de celui d’ 
<taxonomicName id="DA84CEACFFB1FFA0FCD1EDE7FC52FAAB" authorityName="Kirby" authorityYear="1889" box="[821,936,1279,1302]" class="Insecta" family="Libellulidae" genus="Hemistigma" kingdom="Animalia" order="Odonata" pageId="59" pageNumber="290" phylum="Arthropoda" rank="genus">
