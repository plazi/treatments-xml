<taxonomicName id="4CDE9393A6012C5CFF17FCF8125BFCCE" authority="Wheeler, W. M., 1920: 52" authorityName="Wheeler, W. M." authorityPageNumber="52" authorityYear="1920" box="[151,574,798,820]" class="Insecta" family="Formicidae" genus="Phrynoponera" kingdom="Animalia" order="Hymenoptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="genus">
