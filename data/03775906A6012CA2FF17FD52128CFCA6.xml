<taxonomicName id="4CDE9393A6012C5CFCD9FCF81407FCCE" authorityName="André" authorityYear="1892" box="[857,1122,798,820]" class="Insecta" family="Formicidae" genus="Bothroponera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="species" species="gabonensis">
<emphasis id="B9AA3402A6012C5CFCD9FCF81407FCCE" box="[857,1122,798,820]" italics="true" pageId="126" pageNumber="127">Bothroponera gabonensis</emphasis>
</taxonomicName>
<treatmentCitation id="0A7FCE01A6012C5CFBECFCF8157EFCCE" author="Andre, E." box="[1132,1307,798,820]" page="50" pageId="126" pageNumber="127" year="1892">
<bibRefCitation id="EF4F95E1A6012C5CFBECFCF8157EFCCE" author="Andre, E." box="[1132,1307,798,820]" pageId="126" pageNumber="127" pagination="45 - 56" refId="ref131403" refString="Andre, E. (1892) Materiaux myrmecologiques. Revue d'Entomologie (Caen), 11, 45 - 56." type="journal article" year="1892">
<collectingRegion id="491A26F2A6012C5CFBECFCF814CBFCCE" box="[1132,1198,798,820]" country="Saint Vincent and the Grenadines" name="Saint Andrew" pageId="126" pageNumber="127">André</collectingRegion>
, 1892: 50
</bibRefCitation>
