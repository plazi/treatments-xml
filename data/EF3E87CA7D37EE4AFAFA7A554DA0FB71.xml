<taxonomicName id="A0974D5F7D37EE4AF9CA7AE54D9CFB71" authority="Wahlberg (Naglis &amp; Negrobov 2014)" authorityName="Wahlberg (Naglis &amp; Negrobov" authorityYear="2014" class="Insecta" family="Dolichopodidae" genus="Thinophilus" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="455" phylum="Arthropoda" rank="genus">
