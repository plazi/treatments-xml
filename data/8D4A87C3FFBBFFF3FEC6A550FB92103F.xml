, 
<taxonomicName id="C2E34D56FFBCFFF3FC49A229FF4F1774" box="[113,221,565,588]" class="Magnoliopsida" family="Rosaceae" genus="Cotoneaster" kingdom="Plantae" order="Rosales" pageId="4" pageNumber="249" phylum="Tracheophyta" rank="genus">
<emphasis id="3797EAC7FFBCFFF3FC49A229FF4F1774" box="[113,221,565,588]" italics="true" pageId="4" pageNumber="249">Cotoneaster</emphasis>
</taxonomicName>
, 
<taxonomicName id="C2E34D56FFBCFFF3FCD4A228FED61774" box="[236,324,564,588]" class="Magnoliopsida" family="Rhamnaceae" genus="Rhamnus" kingdom="Plantae" order="Rosales" pageId="4" pageNumber="249" phylum="Tracheophyta" rank="genus">
<emphasis id="3797EAC7FFBCFFF3FCD4A228FED61774" box="[236,324,564,588]" italics="true" pageId="4" pageNumber="249">Rhamnus</emphasis>
</taxonomicName>
, 
<taxonomicName id="C2E34D56FFBCFFF3FD6BA229FE0D1774" box="[339,415,565,588]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="249" phylum="Tracheophyta" rank="genus">
<emphasis id="3797EAC7FFBCFFF3FD6BA229FE0D1774" box="[339,415,565,588]" italics="true" pageId="4" pageNumber="249">Quercus</emphasis>
</taxonomicName>
in vineyards and roadsides; 
