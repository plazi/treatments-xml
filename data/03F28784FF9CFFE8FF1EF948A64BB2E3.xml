<specimenCount id="9D5DFD1BFF9CFFEBFEA1F80CA494B91E" box="[311,392,2030,2057]" pageId="12" pageNumber="223" type="generic">
<quantity id="4CA39B77FF9CFFEBFEA1F80CA498B91E" box="[311,388,2030,2057]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="12" pageNumber="223" unit="m" value="600.0">600 m</quantity>
.
</specimenCount>
