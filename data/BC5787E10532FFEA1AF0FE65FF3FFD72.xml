<taxonomicName id="F3FE4D740535FFEA1887FE65FB61FE02" authority="Gorochov, 2013" authorityName="Gorochov" authorityYear="2013" box="[736,1204,474,500]" class="Liliopsida" family="Araceae" genus="Rhaphidophora" kingdom="Plantae" order="Alismatales" pageId="5" pageNumber="134" phylum="Tracheophyta" rank="species" species="spinifera">
