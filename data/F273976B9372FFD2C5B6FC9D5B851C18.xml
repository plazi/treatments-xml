<taxonomicName id="BDDA5DFE9372FFD2C701FC175B1F1BC3" authority="(FALDERMANN, 1835)" baseAuthorityName="FALDERMANN" baseAuthorityYear="1835" box="[517,865,945,967]" class="Insecta" family="Carabidae" genus="Dicheirotrichus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="1281" phylum="Arthropoda" rank="species" species="discolor">
