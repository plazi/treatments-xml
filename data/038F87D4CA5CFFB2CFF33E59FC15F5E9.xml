<taxonomicName id="4C264D41CA5CFFB3C2053F65F74EFC16" authorityName="Gray" authorityYear="1865" box="[2144,2248,877,906]" class="Mammalia" family="Mustelidae" genus="Pekania" kingdom="Animalia" order="Carnivora" pageId="12" pageNumber="631" phylum="Chordata" rank="genus">Pekania</taxonomicName>
