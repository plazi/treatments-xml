<taxonomicName id="87C614A2FFB44D4B5FC5FF1CFC7EFF42" authority="Huang 2006" authorityName="Huang" authorityYear="2006" box="[489,987,151,176]" class="Arachnida" family="Phytoptidae" genus="Sierraphytoptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="277" phylum="Arthropoda" rank="species" species="taiwanensus">
