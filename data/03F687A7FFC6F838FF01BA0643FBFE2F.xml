<taxonomicName id="4C5F4D32FFC7F838FC99B9E143E2FE2F" baseAuthorityName="Mosely" baseAuthorityYear="1954" box="[856,1001,387,411]" class="Magnoliopsida" family="Apocynaceae" genus="Mortoniella" kingdom="Plantae" order="Gentianales" pageId="63" pageNumber="62" phylum="Tracheophyta" rank="species" species="meralda">
