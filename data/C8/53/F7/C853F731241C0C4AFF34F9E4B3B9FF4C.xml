<quantity box="[1121,1225,908,932]" metricMagnitude="-4" metricUnit="m" metricValue="2.8" pageId="15" pageNumber="538" unit="mm" value="0.28">0.28 mm</quantity>
; PL 
<quantity box="[1278,1381,908,932]" metricMagnitude="-4" metricUnit="m" metricValue="2.4" pageId="15" pageNumber="538" unit="mm" value="0.24">0.24 mm</quantity>
, PW 
<quantity box="[151,313,943,968]" metricMagnitude="-4" metricUnit="m" metricValue="2.45" metricValueMax="2.5" metricValueMin="2.4" pageId="15" pageNumber="538" unit="mm" value="0.245" valueMax="0.25" valueMin="0.24">0.24–0.25 mm</quantity>
(mean 
