); EL 
<quantity box="[558,720,943,968]" metricMagnitude="-4" metricUnit="m" metricValue="4.55" metricValueMax="4.6" metricValueMin="4.5" pageId="15" pageNumber="538" unit="mm" value="0.455" valueMax="0.46" valueMin="0.45">0.45–0.46 mm</quantity>
(mean 
<quantity box="[803,906,943,967]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="15" pageNumber="538" unit="mm" value="0.45">0.45 mm</quantity>
), EW 
<quantity box="[974,1136,943,968]" metricMagnitude="-4" metricUnit="m" metricValue="3.85" metricValueMax="3.9" metricValueMin="3.8" pageId="15" pageNumber="538" unit="mm" value="0.385" valueMax="0.39" valueMin="0.38">0.38–0.39 mm</quantity>
(mean 
<quantity box="[1218,1322,943,967]" metricMagnitude="-4" metricUnit="m" metricValue="3.8" pageId="15" pageNumber="538" unit="mm" value="0.38">0.38 mm</quantity>
), EI 
<number pageId="15" pageNumber="538" value="1.1949999999999998" valueMax="1.23" valueMin="1.16">1.16– 1.23</number>
(mean 
<number box="[284,337,980,1004]" pageId="15" pageNumber="538" value="1.18">1.18</number>
).
</paragraph>
