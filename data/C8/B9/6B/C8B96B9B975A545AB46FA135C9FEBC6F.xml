<paragraph pageId="0" pageNumber="255">
<emphasis bold="true" pageId="0" pageNumber="255">Figure 19.</emphasis>
<taxonomicName class="Arachnida" family="Oonopidae" genus="Opopaea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opopaea wundanyi" order="Araneae" pageId="0" pageNumber="255" phylum="Arthropoda" rank="species" species="wundanyi">
