= 221–236 (229 ± 4.3); SC 
<subScript attach="left" box="[433,445,1550,1564]" fontSize="6" pageId="24" pageNumber="25">
<specimenCount box="[433,445,1550,1564]" pageId="24" pageNumber="25" type="female">♀</specimenCount>
</subScript>
= 80–90 (84 ± 3.8), SC 
<subScript attach="left" box="[714,726,1550,1564]" fontSize="6" pageId="24" pageNumber="25">
<specimenCount box="[714,726,1550,1564]" pageId="24" pageNumber="25" type="male">♂</specimenCount>
</subScript>
= 78–89 (85 ± 3.3); SCR 
<subScript attach="left" box="[1015,1027,1550,1564]" fontSize="6" pageId="24" pageNumber="25">
<specimenCount box="[1015,1027,1550,1564]" pageId="24" pageNumber="25" type="female">♀</specimenCount>
</subScript>
= 0.28, SCR 
<subScript attach="left" box="[1175,1187,1550,1564]" fontSize="6" pageId="24" pageNumber="25">
<specimenCount box="[1175,1187,1550,1564]" pageId="24" pageNumber="25" type="male">♂</specimenCount>
</subScript>
= 0.27; D = 17-17-15 (100%); SL 
<subScript attach="left" box="[282,291,1588,1602]" fontSize="6" pageId="24" pageNumber="25">E</subScript>
