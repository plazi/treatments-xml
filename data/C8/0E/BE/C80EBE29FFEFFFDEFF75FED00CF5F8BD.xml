<taxonomicName authority="Guentheri Boulenger, 1895: 31" authorityName="Guentheri Boulenger" authorityPageNumber="31" authorityYear="1895" box="[391,873,294,319]" class="Reptilia" family="Colubridae" genus="Stegonotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="40" pageNumber="41" phylum="Chordata" rank="genus">
