); F6, 89 (
<number box="[1252,1298,1748,1772]" pageId="5" pageNumber="262" value="124.0">124</number>
); clava, 
<number box="[1394,1436,1748,1772]" pageId="5" pageNumber="262" value="347.0">347</number>
