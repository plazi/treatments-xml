<subSubSection box="[199,1106,223,248]" pageId="7" pageNumber="264" type="etymology">
<paragraph blockId="7.[151,1437,151,536]" box="[199,1106,223,248]" pageId="7" pageNumber="264">
<emphasis bold="true" box="[199,334,223,248]" pageId="7" pageNumber="264">Etymology.</emphasis>
