<number box="[1196,1228,223,248]" pageId="4" pageNumber="261" value="86.0">86</number>
); pedicel, 
<number box="[1346,1374,223,248]" pageId="4" pageNumber="261" value="67.0">67</number>
(
<number box="[1391,1423,223,248]" pageId="4" pageNumber="261" value="36.0">36</number>
); F1, 34 (
<number box="[238,270,260,284]" pageId="4" pageNumber="261" value="30.0">30</number>
