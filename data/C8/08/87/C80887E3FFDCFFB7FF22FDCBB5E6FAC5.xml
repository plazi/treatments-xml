<heading bold="true" box="[151,527,591,617]" fontSize="11" level="1" pageId="2" pageNumber="55" reason="1">
<emphasis bold="true" box="[151,527,591,617]" pageId="2" pageNumber="55">
<taxonomicName authority="Durante" authorityName="Durante" box="[151,385,591,617]" class="Insecta" family="Arctiidae" genus="Asuroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="genus">
