<subSubSection pageId="4" pageNumber="267" type="type species">
<paragraph pageId="4" pageNumber="267">Type species.</paragraph>
<paragraph pageId="4" pageNumber="267">
<taxonomicName authority="Bashkuev, 2011" authorityName="Bashkuev" authorityYear="2011" class="Insecta" family="Mesopsychidae" genus="Permopsyche" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Permopsyche issadensis" order="Mecoptera" pageId="4" pageNumber="267" phylum="Arthropoda" rank="species" species="issadensis">Permopsyche issadensis</taxonomicName>
