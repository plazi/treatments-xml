<taxonomicName class="Insecta" family="Mordellidae" genus="Mordellistena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mordellistena thuringiaca" order="Coleoptera" pageId="0" pageNumber="151" phylum="Arthropoda" rank="species" species="thuringiaca">
