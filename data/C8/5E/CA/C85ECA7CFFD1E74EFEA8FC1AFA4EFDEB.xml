on 
<taxonomicName box="[1079,1259,417,440]" class="Magnoliopsida" family="Moraceae" genus="Ficus" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="135" phylum="Tracheophyta" rank="species" species="sycomorus">
