<materialsCitation ID-GBIF-Occurrence="3079225302" collectingDate="2008-06-30" collectingDateMax="2012-09-06" collectingDateMin="2008-06-30" collectorName="S. S. Choi, Is. Ulleung" county="Mt. Jogyeo" elevation="343" latitude="34.997665" location="Mt. Daedun" longLatPrecision="1" longitude="127.28103" municipality="Mt. Dureun" specimenCount="1">
