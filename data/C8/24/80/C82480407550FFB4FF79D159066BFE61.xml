(
<emphasis bold="true" box="[290,462,617,643]" italics="true" pageId="1" pageNumber="424">Rhyacophylax</emphasis>
) 
<emphasis bold="true" box="[479,570,617,643]" italics="true" pageId="1" pageNumber="424">necator</emphasis>
</taxonomicName>
