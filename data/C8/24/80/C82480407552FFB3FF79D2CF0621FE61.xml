<emphasis bold="true" box="[151,673,511,538]" pageId="3" pageNumber="426">
<taxonomicName box="[151,572,511,538]" class="Insecta" family="Hydropsychidae" genus="Smicridea" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="426" phylum="Arthropoda" rank="species" species="franciscana" status="sp. nov." subGenus="Smicridea">
<emphasis bold="true" box="[151,273,511,537]" italics="true" pageId="3" pageNumber="426">Smicridea</emphasis>
(
<emphasis bold="true" box="[290,412,511,537]" italics="true" pageId="3" pageNumber="426">Smicridea</emphasis>
) 
<emphasis bold="true" box="[429,572,511,537]" italics="true" pageId="3" pageNumber="426">franciscana</emphasis>
