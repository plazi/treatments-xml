<materialsCitation ID-GBIF-Occurrence="1572325100" country="South Georgia and the South Sandwich Islands" location="Moubray Bay" municipality="Robertson Bay" pageId="26" pageNumber="27" specimenCount="1" stateProvince="Cape">
