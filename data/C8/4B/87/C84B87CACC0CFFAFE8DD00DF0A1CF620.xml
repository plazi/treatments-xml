<location LSID="urn:lsid:plazi:treatment:C84B87CACC0CFFAFE8DD00DF0A1CF620:453D6007CC0CFFAFE8DD028D0E7AF7B0" box="[151,357,714,739]" country="South Georgia and the South Sandwich Islands" name="Livingston islands" pageId="26" pageNumber="27" stateProvince="Coast">Livingston islands</location>
(
