<location LSID="urn:lsid:plazi:treatment:C84B87CACC36FF97E8DD032F0C4AF76E:453D6007CC34FF97E95802630EFBF76E" box="[274,484,548,573]" country="Antarctica" county="Livingston Island" municipality="Trinity Island" name="Possession Islands" pageId="34" pageNumber="35" stateProvince="Cape">Possession Islands</location>
, 
