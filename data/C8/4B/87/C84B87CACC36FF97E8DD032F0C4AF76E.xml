<location LSID="urn:lsid:plazi:treatment:C84B87CACC36FF97E8DD032F0C4AF76E:453D6007CC34FF97EA4F01F00DA8F483" box="[517,695,439,464]" country="Antarctica" county="Livingston Island" municipality="Trinity Island" name="Balleny Islands" pageId="34" pageNumber="35" stateProvince="Cape">Balleny Islands</location>
