<number box="[657,688,1346,1372]" pageId="8" pageNumber="9" value="22.0">22</number>
º04.484’S
</geoCoordinate>
, 
<geoCoordinate box="[819,983,1346,1372]" direction="east" orientation="longitude" pageId="8" pageNumber="9" precision="1" value="166.33167">
<number box="[819,864,1346,1372]" pageId="8" pageNumber="9" value="166.0">166</number>
º 
<number box="[873,959,1346,1372]" pageId="8" pageNumber="9" value="19.9">19.900</number>
’E
</geoCoordinate>
, 
<date box="[998,1126,1346,1372]" pageId="8" pageNumber="9" value="2003-11-15">
