<taxonomicName authority="Noei, 2017" authorityName="Noei" authorityYear="2017" box="[521,1067,1233,1256]" class="Arachnida" family="Trombidiidae" genus="Cicaditrombium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="1" pageNumber="240" phylum="Arthropoda" rank="species" species="lorestanensis">
