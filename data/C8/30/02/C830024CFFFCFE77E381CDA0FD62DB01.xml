<number box="[994,1009,948,972]" pageId="23" pageNumber="24" value="3.0">3</number>
(
<number box="[1019,1030,948,972]" pageId="23" pageNumber="24" value="1.0">1</number>
), III: 
<number box="[1101,1118,948,972]" pageId="23" pageNumber="24" value="2.0">2</number>
