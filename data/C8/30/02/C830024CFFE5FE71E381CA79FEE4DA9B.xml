<emphasis box="[1026,1051,737,760]" italics="true" pageId="17" pageNumber="18">ps</emphasis>
<number box="[1051,1059,751,764]" pageId="17" pageNumber="18" value="4.0">
<emphasis box="[1051,1059,751,764]" italics="true" pageId="17" pageNumber="18">4</emphasis>
</number>
