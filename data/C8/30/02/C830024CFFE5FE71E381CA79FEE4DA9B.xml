<number box="[538,546,790,803]" pageId="17" pageNumber="18" value="3.0">
<emphasis box="[538,546,790,803]" italics="true" pageId="17" pageNumber="18">3</emphasis>
</number>
densely barbed), other setae long (
<emphasis box="[936,948,776,799]" italics="true" pageId="17" pageNumber="18">c</emphasis>
<number box="[949,957,790,803]" pageId="17" pageNumber="18" value="1.0">
<emphasis box="[949,957,790,803]" italics="true" pageId="17" pageNumber="18">1</emphasis>
</number>
&gt; 
<emphasis box="[972,984,776,799]" italics="true" pageId="17" pageNumber="18">c</emphasis>
<number box="[985,993,790,803]" pageId="17" pageNumber="18" value="1.0">
<emphasis box="[985,993,790,803]" italics="true" pageId="17" pageNumber="18">1</emphasis>
