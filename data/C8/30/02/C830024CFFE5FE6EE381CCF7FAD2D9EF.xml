- 
<number box="[977,994,1366,1390]" pageId="14" pageNumber="15" value="2.0">2</number>
(
<number box="[1003,1020,1366,1390]" pageId="14" pageNumber="15" value="2.0">2</number>
)- 
<number box="[1038,1053,1366,1390]" pageId="14" pageNumber="15" value="5.0">5</number>
(
<number box="[1063,1074,1366,1390]" pageId="14" pageNumber="15" value="1.0">1</number>
); II: 
<number box="[1128,1164,1366,1390]" pageId="14" pageNumber="15" value="2.0" valueMax="3.0" valueMin="1.0">1-3</number>
