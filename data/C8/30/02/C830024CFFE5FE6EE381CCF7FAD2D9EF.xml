(
<number box="[1201,1212,1366,1390]" pageId="14" pageNumber="15" value="1.0">1</number>
)- 
<number box="[1232,1247,1366,1390]" pageId="14" pageNumber="15" value="3.0">3</number>
(
<number box="[1257,1268,1366,1390]" pageId="14" pageNumber="15" value="1.0">1</number>
), III: 
<number box="[1333,1350,1366,1390]" pageId="14" pageNumber="15" value="2.0">2</number>
- 
<number box="[1359,1376,1366,1390]" pageId="14" pageNumber="15" value="2.0">2</number>
- 
<number box="[1386,1397,1366,1390]" pageId="14" pageNumber="15" value="1.0">1</number>
(
<number box="[1406,1417,1366,1390]" pageId="14" pageNumber="15" value="1.0">1</number>
)- 
<number box="[151,168,1402,1426]" pageId="14" pageNumber="15" value="2.0">2</number>
