<emphasis box="[301,309,1869,1882]" italics="true" pageId="10" pageNumber="11">1</emphasis>
</number>
longest, setae 
<emphasis box="[472,497,1855,1878]" italics="true" pageId="10" pageNumber="11">ps</emphasis>
<number box="[497,505,1869,1882]" pageId="10" pageNumber="11" value="4.0">
<emphasis box="[497,505,1869,1882]" italics="true" pageId="10" pageNumber="11">4</emphasis>
</number>
shortest; setae 
<emphasis box="[675,687,1855,1878]" italics="true" pageId="10" pageNumber="11">c</emphasis>
<number box="[687,708,1869,1882]" pageId="10" pageNumber="11" value="2.0" valueMax="3.0" valueMin="1.0">
