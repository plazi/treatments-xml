<emphasis box="[791,799,1831,1844]" italics="true" pageId="10" pageNumber="11">1</emphasis>
</number>
&gt; 
<emphasis box="[814,826,1816,1839]" italics="true" pageId="10" pageNumber="11">c</emphasis>
<number box="[827,835,1831,1844]" pageId="10" pageNumber="11" value="1.0">
<emphasis box="[827,835,1831,1844]" italics="true" pageId="10" pageNumber="11">1</emphasis>
</number>
