Cona County, 
<collectingRegion box="[520,581,1775,1801]" country="China" name="Xizang" pageId="125" pageNumber="126">Tibet</collectingRegion>
, SW 
<collectingCountry box="[640,707,1775,1801]" name="China" pageId="125" pageNumber="126">China</collectingCountry>
