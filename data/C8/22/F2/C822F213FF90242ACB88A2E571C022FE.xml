Mêdog County, 
<collectingRegion box="[540,601,1123,1149]" country="China" name="Xizang" pageId="93" pageNumber="94">Tibet</collectingRegion>
, SW 
<collectingCountry box="[660,727,1123,1149]" name="China" pageId="93" pageNumber="94">China</collectingCountry>
