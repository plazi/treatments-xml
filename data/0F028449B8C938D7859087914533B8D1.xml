<subSubSection id="C291E32A449EF8EAB87E4BAE339AD327" type="nomenclature">
<paragraph id="E0B9488E0B346BE377C7FD35C6F4135B" pageNumber="395">
<taxonomicName id="ACEE92415FB19A749072EFF08A7B60FD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:226148" class="Insecta" family="Formicidae" genus="Meranoplus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Meranoplus tricuspidatus SchÃ¶dl" order="Hymenoptera" pageNumber="395" phylum="Arthropoda" rank="species" species="tricuspidatus">Meranoplus tricuspidatus Schoedl</taxonomicName>
