<taxonomicName id="5858F414EAE312F2011CC8485A8361D9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:240987" authority="Kieffer, 1904" authorityName="Kieffer" authorityYear="1904" class="Insecta" family="Bethylidae" genus="Pseudisobrachium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Pseudisobrachium triacutum Waichert &amp; Azevedo" order="Hymenoptera" pageNumber="5" phylum="Arthropoda" rank="species" species="triacutum">Pseudisobrachium triacutum</taxonomicName>
