<emphasis id="CCC8202DAE4DCF2EFF79FC88FCE8FCCD" bold="true" box="[151,863,859,886]" pageId="18" pageNumber="268">
8. 
<taxonomicName id="39BC87BCAE4DCF2EFF5BFC8FFCE8FCCD" authority="(Wagner, 1939)" baseAuthorityName="Wagner" baseAuthorityYear="1939" box="[181,863,859,886]" class="Insecta" family="Cicadellidae" genus="Anaceratagallia" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="268" phylum="Arthropoda" rank="species" species="frisia" subGenus="Anaceratagallia">
<emphasis id="CCC8202DAE4DCF2EFF5BFC8FFEC0FCCD" bold="true" box="[181,375,859,886]" italics="true" pageId="18" pageNumber="268">Anaceratagallia</emphasis>
(
<emphasis id="CCC8202DAE4DCF2EFE66FC8FFDFDFCCD" bold="true" box="[392,586,859,886]" italics="true" pageId="18" pageNumber="268">Anaceratagallia</emphasis>
) 
<emphasis id="CCC8202DAE4DCF2EFDB5FC8FFD2CFCCE" bold="true" box="[603,667,859,885]" italics="true" pageId="18" pageNumber="268">frisia</emphasis>
(
<bibRefCitation id="9A2D81CEAE4DCF2EFD47FC88FCE0FCCD" author="Wagner, W." box="[681,855,860,886]" pageId="18" pageNumber="268" pagination="112 - 113" refId="ref14473" refString="Wagner, W. (1939) Eine neue Agallia - Art (Homoptera-Jassidae) von der Nordseeinsel Borkum. Abhandlungen herausgegeben vom Naturwissenschaftlichen Verein zu Bremen, 31 (1), 112 - 113." type="journal article" year="1939">Wagner, 1939</bibRefCitation>
