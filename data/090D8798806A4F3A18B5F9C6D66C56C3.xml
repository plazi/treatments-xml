<paragraph id="811B368E806A4F3818B5F9E1D39D51E6" blockId="5.[151,550,1610,1671]" box="[151,398,1645,1671]" pageId="5" pageNumber="51">
(
<figureCitation id="199F2A0B806A4F3818BDF9E1D2F551E6" box="[159,230,1645,1671]" captionStart="FIGURES 1 – 10" captionStartId="2.[151,269,1752,1776]" captionTargetBox="[302,1282,194,1726]" captionTargetId="figure@2.[302,1285,194,1728]" captionTargetPageId="2" captionText="FIGURES 1 – 10. Adults of Ernocornutia. 1, E. firna, sp. n., holotype; 2, E. paracatopta, sp. n., holotype; 3, E. chiribogana, sp. n., holotype; 4, E. termasiana, sp. n., holotype; 5, E. sangayana, sp. n., holotype, male; 6, E. pilaloana, sp. n., holotype; 7, E. pululahuana, sp. n., holotype; 8, E. paracatopta, sp. n., paratype, female; 9, E. termasiana, sp. n., paratype female; 10, E. sangayana, sp. n., paratype, female." httpUri="https://zenodo.org/record/181117/files/figure.png" pageId="5" pageNumber="51">Fig. 3</figureCitation>
, 
<figureCitation id="199F2A0B806A4F3818D1F9E1D30251E6" box="[243,273,1645,1671]" captionStart="FIGURES 11 – 14" captionStartId="4.[151,269,1857,1881]" captionTargetBox="[339,1238,211,1821]" captionTargetId="figure@4.[272,1315,194,1833]" captionTargetPageId="4" captionText="FIGURES 11 – 14. Male genitalia of Ernocornutia. 11 a, b, E. firna, sp. n., holotype; 12 a, b, E. paracatopta, sp. n., holotype; 13 a, b, E. chiribogana, sp. n., holotype; 14 a, b, E. termasiana, sp. n., holotype." httpUri="https://zenodo.org/record/181118/files/figure.png" pageId="5" pageNumber="51">13</figureCitation>
a, 
<number id="376C24A9806A4F38190EF9E1D35A51E6" box="[300,329,1645,1671]" pageId="5" pageNumber="51" value="13.0">13</number>
b, 
<number id="376C24A9806A4F381945F9E1D39651E6" box="[359,389,1645,1671]" pageId="5" pageNumber="51" value="19.0">19</number>
