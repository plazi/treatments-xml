<taxonomicName id="CEE946A3FFF9FFE5FEE6F82D1C8AF861" authority="Mu &amp; Jin &amp; Zhang, 2019" authorityName="Mu &amp; Jin &amp; Zhang" authorityYear="2019" box="[295,456,1925,1949]" class="Arachnida" family="Liocranidae" genus="Agroeca" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="103" phylum="Arthropoda" rank="species" species="tumida" status="sp. n.">
