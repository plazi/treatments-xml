locality: 
<location id="3F78A199FFA5FF9BBB0A2BF7FE50C69C" LSID="urn:lsid:plazi:treatment:B20E4654FFA6FF9BBAF42D4DFDF8C484:3F78A199FFA5FF9BBB0A2BF7FE50C69C" box="[361,479,153,179]" country="Brazil" name="Blumenau" pageId="14" pageNumber="15" stateProvince="Santa Catarina">Blumenau</location>
, 
<collectingRegion id="F86339A0FFA5FF9BBB892BF7FD5FC69C" box="[490,720,153,179]" country="Brazil" name="Santa Catarina" pageId="14" pageNumber="15">Santa Catarina State</collectingRegion>
, 
<collectingCountry id="42B0B7D2FFA5FF9BB8B82BF7FCACC69C" box="[731,803,153,179]" name="Brazil" pageId="14" pageNumber="15">Brazil</collectingCountry>
