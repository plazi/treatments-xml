<taxonomicName id="72DA8AAF407F1B51B006FE62971C5212" authority="Matsumura" authorityName="Matsumura" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="okubella">
