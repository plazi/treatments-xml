<paragraph id="8BE78B58FFF4FFE6FCD7AC5BFE31249A" blockId="4.[813,1455,1875,2030]" lastBlockId="5.[132,775,1493,1679]" lastPageId="5" lastPageNumber="4" pageId="4" pageNumber="3">
<taxonomicName id="4C58F0DBFFF4FFE7FCD7AC5BFC1F2598" authorityName="Walker" authorityYear="1852" box="[813,1003,1907,1933]" class="Insecta" family="Ropalomeridae" genus="Ropalomera" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="tibialis">
