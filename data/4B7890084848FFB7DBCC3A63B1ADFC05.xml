<subSubSection id="8BCB7295484BFFB7DB9E3C0BB6D4FDD3" box="[197,1163,574,600]" pageId="15" pageNumber="16" type="etymology">
<paragraph id="C36E211E484BFFB7DB9E3C0BB6D4FDD3" blockId="15.[151,1438,152,910]" box="[197,1163,574,600]" pageId="15" pageNumber="16">
<emphasis id="F1A5FD0C484BFFB7DB9E3C0BB30AFDD3" bold="true" box="[197,341,574,600]" pageId="15" pageNumber="16">Etymology:</emphasis>
