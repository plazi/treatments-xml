(
<number id="75193339484BFFB7D9053F05B036FEC1" box="[606,617,304,330]" pageId="15" pageNumber="16" value="1.0">1</number>
)- 
<number id="75193339484BFFB7D9273F05B0D2FEC1" box="[636,653,304,330]" pageId="15" pageNumber="16" value="4.0">4</number>
(
<number id="75193339484BFFB7D9CD3F05B0F8FEC1" box="[662,679,304,330]" pageId="15" pageNumber="16" value="2.0">2</number>
)- 
<number id="75193339484BFFB7D9E13F05B085FEC1" box="[698,730,304,330]" pageId="15" pageNumber="16" value="14.0">14</number>
(
<number id="75193339484BFFB7D9BF3F05B0AAFEC1" box="[740,757,304,330]" pageId="15" pageNumber="16" value="2.0">2</number>
); legs II: 
<number id="75193339484BFFB7D83F3F05B1D6FEC1" box="[868,905,304,330]" pageId="15" pageNumber="16" value="2.0" valueMax="3.0" valueMin="1.0">1-3</number>
- 
