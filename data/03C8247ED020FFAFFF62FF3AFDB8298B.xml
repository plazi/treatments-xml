high, without gonophores, 
<collectionCode id="ED700DADD020FFAEFD45FE44FC9A290E" box="[688,778,489,513]" pageId="34" pageNumber="35">ROMIZ</collectionCode>
<specimenCode id="DBC73D13D020FFAEFCE4FE44FCCF290E" box="[785,863,489,513]" collectionCode="ROMIZ" pageId="34" pageNumber="35">B3903</specimenCode>
</materialsCitation>
