<taxonomicName id="4C47C9A3034D2B4CFC79C463FBE8F9BC" box="[1021,1102,1559,1582]" class="Liliopsida" family="Cyperaceae" genus="Cyperus" kingdom="Plantae" order="Poales" pageId="4" pageNumber="149" phylum="Tracheophyta" rank="genus">
