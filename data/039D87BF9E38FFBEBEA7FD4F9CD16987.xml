<emphasis id="B940EABB9E38FFBDBEA7FC1C9C076E99" bold="true" box="[140,295,905,932]" pageId="53" pageNumber="70">
<emphasis id="B940EABB9E38FFBDBEA7FC1C9DE66E9E" box="[140,198,906,932]" inLineHeading="true" pageId="53" pageNumber="70" reason="1">
<typeStatus id="548F880B9E38FFBDBEA7FC1C9DE66E9E" box="[140,198,906,932]" pageId="53" pageNumber="70">Type</typeStatus>
</emphasis>
species.
</emphasis>
<taxonomicName id="4C344D2A9E38FFBDBF05FC1C9F466E9E" authority="Kensley, 1978" authorityName="Kensley" authorityYear="1978" box="[302,614,906,932]" class="Malacostraca" family="Serolidae" genus="Serolis" kingdom="Animalia" order="Isopoda" pageId="53" pageNumber="70" phylum="Arthropoda" rank="species" species="brinki">
