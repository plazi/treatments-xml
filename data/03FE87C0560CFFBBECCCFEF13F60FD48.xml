<taxonomicName id="4C574D55560CFFBBE9DFFDFC3DD7FDAB" authority="Fitch, 1859" authorityName="Fitch" authorityYear="1859" class="Insecta" family="Gracillariidae" genus="Chrysaster" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="230" phylum="Arthropoda" rank="species" species="ostensackenella">
<emphasis id="B923EAC4560CFFBBE9DFFDFC3D45FDAB" italics="true" pageId="1" pageNumber="230">C. ostensackenella</emphasis>
