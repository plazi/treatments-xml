<taxonomicName id="4C574D55560CFFBBED67FDB93DB4FD48" box="[316,433,631,655]" class="Insecta" family="Gracillariidae" genus="Chrysaster" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="230" phylum="Arthropoda" rank="genus">
