, 
<taxonomicName id="A0AA7CFEB044FFD10A77FDE7A509FD9F" authority="Zetterstedt" authorityName="Zetterstedt" box="[1118,1397,557,581]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="rufitarse">
