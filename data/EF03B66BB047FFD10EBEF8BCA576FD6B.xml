</taxonomicName>
and 
<taxonomicName id="A0AA7CFEB044FFD10A70FD53A57EFD6B" box="[1113,1282,665,689]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="monilicornis">
