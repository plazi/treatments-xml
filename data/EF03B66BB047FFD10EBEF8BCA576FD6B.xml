<taxonomicName id="A0AA7CFEB044FFD10DB3FD53A45FFD6B" box="[922,1059,665,689]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="ephippius">
