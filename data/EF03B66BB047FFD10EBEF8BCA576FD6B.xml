<taxonomicName id="A0AA7CFEB044FFD10D95FDBCA42AFD57" box="[956,1110,629,653]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
