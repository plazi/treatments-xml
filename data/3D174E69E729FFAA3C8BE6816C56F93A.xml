<paragraph id="B501FF7FE729FFAA3C8BE1FE6C56F93A" blockId="2.[151,1436,1157,1769]" box="[151,1351,1746,1769]" pageId="2" pageNumber="67">
The synonymy 
<taxonomicName id="72BE84FCE729FFAA3D20E1FE6BD5F93A" authority="Stierlin, 1853" authorityName="Stierlin" authorityYear="1853" box="[316,708,1746,1769]" class="Insecta" family="Curculionidae" genus="Dichotrachelus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="67" phylum="Arthropoda" rank="species" species="rudeni">
<emphasis id="87CA236DE729FFAA3D20E1FE6B3FF93A" box="[316,558,1746,1769]" italics="true" pageId="2" pageNumber="67">Dichotrachelus rudeni</emphasis>
Stierlin, 
<number id="0376ED58E729FFAA3E93E1FE6BD5F93A" box="[655,708,1746,1769]" pageId="2" pageNumber="67" value="1853.0">1853</number>
</taxonomicName>
[= 
<taxonomicName id="72BE84FCE729FFAA3EF6E1FE6DBDF93A" authority="Fairmaire, 1861" authorityName="Fairmaire" authorityYear="1861" box="[746,1196,1746,1769]" class="Insecta" family="Curculionidae" genus="Dichotrachelus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="67" phylum="Arthropoda" rank="species" species="sabaudus">
<emphasis id="87CA236DE729FFAA3EF6E1FE6AEBF93A" box="[746,1018,1746,1769]" italics="true" pageId="2" pageNumber="67">Dichotrachelus sabaudus</emphasis>
