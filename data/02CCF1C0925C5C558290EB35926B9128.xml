<taxonomicName id="AEEE9EBB221E911095DD0A41D301A31C" authorityName="LaFerte-Senectere" authorityYear="1841" class="Hexapoda" family="Carabidae" genus="Tachys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="448" pageNumber="449" phylum="Arthropoda" rank="species" species="misellus">Tachys misellus</taxonomicName>
