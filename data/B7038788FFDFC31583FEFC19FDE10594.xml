<taxonomicName id="F8AA4D1DFFDFC31B83A7FA6AFDEE02BB" authority="Miers." authorityName="Miers." box="[405,627,1473,1499]" class="Magnoliopsida" family="Icacinaceae" kingdom="Plantae" order="Icacinales" pageId="71" pageNumber="72" phylum="Tracheophyta" rank="family">Icacinaceae Miers.</taxonomicName>
<emphasis id="0DDEEA8CFFDFC31B804EFA6AFD3502BB" bold="true" box="[636,680,1473,1499]" pageId="71" pageNumber="72">1–9</emphasis>
