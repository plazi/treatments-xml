<quantity id="4D759B4CFFC4FF86FE1AFB12FE46FAEF" box="[425,496,1267,1293]" metricMagnitude="0" metricUnit="m" metricValue="2.7178" pageId="25" pageNumber="226" unit="in" value="107.0">107 in</quantity>
<typeStatus id="5536880BFFC4FF86FE4BFB12FDECFAEF" box="[504,602,1267,1293]" pageId="25" pageNumber="226">holotype</typeStatus>
female. 
<taxonomicName id="4D8D4D2AFFC4FF86FD0FFB12FCFCFAEF" authorityName="Krefft" authorityYear="1871" box="[700,842,1267,1293]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="25" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
