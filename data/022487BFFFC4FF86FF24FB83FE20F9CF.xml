<quantity id="4D759B4CFFC4FF86FC2CFB12FC6EFAEF" box="[927,984,1267,1293]" metricMagnitude="0" metricUnit="m" metricValue="2.2098" pageId="25" pageNumber="226" unit="in" value="87.0">87 in</quantity>
<typeStatus id="5536880BFFC4FF86FC53FB12FBF4FAEF" box="[992,1090,1267,1293]" pageId="25" pageNumber="226">holotype</typeStatus>
female. 
<taxonomicName id="4D8D4D2AFFC4FF86FB17FB12FA84FAEF" authorityName="Krefft" authorityYear="1871" box="[1188,1330,1267,1293]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="25" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
width 
<quantity id="4D759B4CFFC4FF86FA33FB12FF1BFAD3" metricMagnitude="0" metricUnit="m" metricValue="1.1938" pageId="25" pageNumber="226" unit="in" value="47.0">47 in</quantity>
