<typeStatus id="5536880BFFC4FF86FA89FB2EFA2AFB0B" box="[1338,1436,1231,1257]" pageId="25" pageNumber="226">holotype</typeStatus>
female. 
<taxonomicName id="4D8D4D2AFFC4FF86FF41FB12FEECFAEF" authorityName="Ausserer" authorityYear="1871" box="[242,346,1267,1293]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="25" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
