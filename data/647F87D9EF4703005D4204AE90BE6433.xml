<taxonomicName id="2BD64D4CEF4703005E39020692536753" authority="scovitzi Faldermann, 1837" authorityName="Faldermann" authorityYear="1837" class="Insecta" family="Meloidae" genus="Cerocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="830" phylum="Arthropoda" rank="subSpecies" species="scovitzi" subGenus="Mesocerocoma" subSpecies="scovitzi">
