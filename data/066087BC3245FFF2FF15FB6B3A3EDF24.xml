<geoCoordinate id="EBFD506D3245FFF2FDB7FA783AB9DDF3" box="[522,651,1476,1500]" degrees="45.4212" direction="north" orientation="latitude" pageId="7" pageNumber="540" precision="5" value="45.4212">45.4212° N</geoCoordinate>
