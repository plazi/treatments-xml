<taxonomicName id="5BAA4D2A486CF00C0C4BFA1FFDF9FA03" authorityName="Matsuda &amp; Gosliner" authorityYear="2018" box="[340,573,1508,1531]" class="Gastropoda" family="Chromodorididae" genus="Doriprismatica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nudibranchia" pageId="20" pageNumber="521" phylum="Mollusca" rank="species" species="rossi">
