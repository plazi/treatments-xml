<taxonomicName id="4C0CEAA9FFD7FFB7B9B0FB82FC250C15" authority="Johnson &amp; Masner, 2006" authorityName="Johnson &amp; Masner" authorityYear="2006" box="[765,936,1117,1141]" class="Insecta" family="Scelionidae" genus="Nixonia" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="sicaria" status="new species">
