<caption id="DF3D74F9FFF05F21FACD7EC7D41AFA31" ID-DOI="http://doi.org/10.5281/zenodo.3756704" ID-Zenodo-Dep="3756704" httpUri="https://zenodo.org/record/3756704/files/figure.png" pageId="13" pageNumber="336" targetBox="[1419,1976,802,1388]" targetPageId="13">
