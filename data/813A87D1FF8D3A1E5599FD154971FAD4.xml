</taxonomicName>
and 
<taxonomicName id="CE934D44FF8B3A1E5650FBF44B4FFBF4" authority="Hayward, 1996" authorityName="Hayward" authorityYear="1996" box="[520,833,1102,1127]" class="Maxillopoda" family="Philichthyidae" genus="Colobomatus" kingdom="Animalia" order="Poecilostomatoida" pageId="13" pageNumber="574" phylum="Arthropoda" rank="species" species="arabicus">
