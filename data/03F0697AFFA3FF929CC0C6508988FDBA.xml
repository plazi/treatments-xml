<treatmentCitationGroup id="AB49FF42FFA3FF939CC0C61A8BD5FC7C" box="[151,702,877,900]" pageId="112" pageNumber="113">
<treatmentCitation id="0AF8FE7DFFA3FF939CC0C61A8B3DFC7C" author="Melander, A. L." box="[151,598,877,900]" page="213" pageId="112" pageNumber="113" year="1918">
<taxonomicName id="4C59A3EFFFA3FF939CC0C61A8B3DFC7C" authority="Melander, 1918: 213" authorityName="Melander" authorityPageNumber="213" authorityYear="1918" box="[151,598,877,900]" class="Insecta" family="Hybotidae" genus="Elaphropeza" kingdom="Animalia" order="Diptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="uniseta">
