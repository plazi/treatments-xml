<taxonomicName id="209C4D79322A1B4CAB9F5A1FFE49E772" authority="Saalmuller, 1884" authorityName="Saalmuller" authorityYear="1884" box="[151,462,869,895]" class="Insecta" family="Hesperiidae" genus="Ploetzia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
