<quantity id="4CAC5FB3FFB41856FF5F0A0BFED7FA82" box="[239,359,1341,1367]" metricMagnitude="4" metricUnit="m" metricValue="4.1335999999999995" pageId="14" pageNumber="15" unit="km" value="41.336">41,336 km</quantity>
<superScript id="7C215F1EFFB41856FED60A0BFEDEFA9E" attach="left" box="[358,366,1341,1355]" fontSize="6" pageId="14" pageNumber="15">2</superScript>
