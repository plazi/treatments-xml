<quantity id="4C9A6186FF92FFC9D773357FB9F3F321" box="[551,651,1026,1050]" metricMagnitude="4" metricUnit="m" metricValue="2.84" pageId="5" pageNumber="4" unit="km" value="28.4">28.4 km</quantity>
(
