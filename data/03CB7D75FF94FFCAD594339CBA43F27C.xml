<taxonomicName id="4C62B7E0FF92FFC9D0D232BFBA1FF4C1" authorityName="Shelley, 1996: 1353 -" authorityYear="1354" class="Diplopoda" family="Xystodesmidae" genus="Xystocheir" kingdom="Animalia" order="Polydesmida" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="brachymacris">
<emphasis id="B9161071FF92FFC9D0D232BFBA1FF4C1" italics="true" pageId="5" pageNumber="4">X. brachymacris</emphasis>
</taxonomicName>
