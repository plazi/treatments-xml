<subSubSection id="42D165512344FFC192BBD087A35DB0E5" box="[151,571,1669,1693]" pageId="0" pageNumber="63" type="materials_examined">
<paragraph id="0A7436DA2344FFC192BBD087A35DB0E5" blockId="0.[151,571,1669,1693]" box="[151,571,1669,1693]" pageId="0" pageNumber="63">
<typeStatus id="D57088782344FFC192BBD087A1AEB0E4" box="[151,200,1671,1692]" pageId="0" pageNumber="63">Type</typeStatus>
species: 
