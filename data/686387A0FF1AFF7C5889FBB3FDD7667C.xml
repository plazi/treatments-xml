<taxonomicName id="27CA4D35FF1AFF7C5B3CFA46FC7D649C" authorityName="Rondani" authorityYear="1868" box="[802,974,1483,1509]" class="Insecta" family="Sciomyzidae" genus="Elgiva" kingdom="Animalia" order="Diptera" pageId="215" pageNumber="216" phylum="Arthropoda" rank="species" species="lateritia">
