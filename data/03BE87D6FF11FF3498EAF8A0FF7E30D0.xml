<taxonomicName id="4C174D43FF11FF3598EAF8A0F8713768" authority="(Bean, 1880)" authorityName="Mecklenburg et al." authorityYear="2016" baseAuthorityName="Bean" baseAuthorityYear="1880" box="[151,560,1874,1901]" class="Actinopterygii" family="Cottidae" genus="Hemilepidotus" kingdom="Animalia" order="Scorpaeniformes" pageId="128" pageNumber="129" phylum="Chordata" rank="species" species="papilio">
