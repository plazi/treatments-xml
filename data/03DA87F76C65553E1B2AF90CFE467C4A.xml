, identified as 
<taxonomicName id="4C734D626C64553C1962FD24FC3B7CA1" box="[832,947,644,669]" class="Pycnogonida" family="Colossendeidae" genus="Colossendeis" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="pipetta">
<emphasis id="B907EAF36C64553C1962FD24FC3B7CA1" box="[832,947,644,669]" italics="true" pageId="17" pageNumber="18">C. pipetta</emphasis>
