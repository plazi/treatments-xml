<emphasis id="B925EAE9FFEDFFC5F5C7DE58FC3C2811" bold="true" box="[151,996,1118,1144]" pageId="1" pageNumber="192">
<taxonomicName id="4C514D78FFEDFFC5F5C7DE58FD552811" authority="Keller and Skelley" authorityName="Keller &amp; Skelley" authorityYear="2019" box="[151,653,1118,1144]" class="Insecta" family="Erotylidae" genus="Notaepytus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="192" phylum="Arthropoda" rank="species" species="quisqueya" status="new species">
