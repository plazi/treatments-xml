<taxonomicName id="ABFE4D50FFA0FFDAE3D06642FC1CFBBF" box="[742,922,1087,1110]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="approximata">
