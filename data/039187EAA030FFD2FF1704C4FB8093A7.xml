<quantity id="4CC09B19A030FFDDFE0105DEFDB692CE" box="[410,518,1434,1460]" metricMagnitude="-4" metricUnit="m" metricValue="3.3" pageId="7" pageNumber="72" unit="mm" value="0.33">0.33 mm</quantity>
