<collectionCode id="0CFEAE67FFAFFFE086C95A3BFCD4FA3C" box="[721,814,1397,1423]" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="14" pageNumber="15">NMNH</collectionCode>
) collected by C. Beutelspacher, 
<date id="1E511062FFAFFFE080D15A3BFA8EFA3C" box="[1225,1396,1397,1423]" pageId="14" pageNumber="15" value="1968-05-18">
<number id="DC272485FFAFFFE080D15A3BFB1CFA3C" box="[1225,1254,1397,1423]" pageId="14" pageNumber="15" value="18.0">18</number>
May 
<number id="DC272485FFAFFFE0812F5A3BFA8EFA3C" box="[1335,1396,1397,1423]" pageId="14" pageNumber="15" value="1968.0">1968</number>
