<emphasis id="B973EA80FFF67D02FE7A33B59F4679F2" box="[437,575,440,464]" italics="true" pageId="3" pageNumber="528">
<taxonomicName id="4C074D11FFF67D02FE7A33B59F7D79F2" authority="Aishan &amp; Triapitsyn &amp; Xu &amp; Lin &amp; Hu, 2016" authorityName="Aishan &amp; Triapitsyn &amp; Xu &amp; Lin &amp; Hu" authorityYear="2016" baseAuthorityName="Foerster" box="[437,516,440,464]" class="Insecta" family="Mymaridae" genus="Cosmocomoidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="528" phylum="Arthropoda" rank="species" sensu="stricto" species="atra" status="s.str.">C. atra</taxonomicName>
