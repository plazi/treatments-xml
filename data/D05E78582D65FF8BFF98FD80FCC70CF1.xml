<quantity id="9F0F64AB2D66FF8BFA13FEA2FA210AB5" box="[1308,1410,367,391]" metricMagnitude="-3" metricUnit="m" metricValue="5.47" pageId="14" pageNumber="15" unit="mm" value="5.47">5.47 mm</quantity>
, n = 
<number id="EE3FDB692D66FF8BFFA2FE59FF1D0A9E" box="[173,190,404,428]" pageId="14" pageNumber="15" value="4.0">4</number>
).
</paragraph>
