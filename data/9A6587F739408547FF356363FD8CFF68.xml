<emphasis id="20B8EAF339408540FF656652FEA5F817" bold="true" box="[199,376,1903,1928]" pageId="11" pageNumber="84">
<typeStatus id="CD77884339408540FF656652FEDFF817" box="[199,258,1903,1928]" pageId="11" pageNumber="84">Type</typeStatus>
material.
</emphasis>
<typeStatus id="CD77884339408540FE216652FE33F817" box="[387,494,1903,1928]" pageId="11" pageNumber="84">Holotype</typeStatus>
, 
<emphasis id="20B8EAF339408540FE5F6652FDCFF817" box="[509,530,1903,1928]" italics="true" pageId="11" pageNumber="84">♂</emphasis>
