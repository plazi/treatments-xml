<taxonomicName id="4C2A4D12E1331C6F4655FA2C7C4FF9C0" authorityName="Wang &amp; Wang" authorityYear="2008" box="[288,552,1522,1545]" class="Arachnida" family="Pteronyssidae" genus="Timalinyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="longitarsus">
