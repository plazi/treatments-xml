<quantity id="727F9B26F808FF80FC4AFD9DFC5BFDF7" box="[939,998,611,637]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="6" pageNumber="7" unit="cm" value="1.0">1 cm</quantity>
