<quantity id="727F9B26F808FF80FB8CFD78FB7DFD2B" box="[1133,1216,646,673]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="6" pageNumber="7" unit="cm" value="0.5">0.5 cm</quantity>
