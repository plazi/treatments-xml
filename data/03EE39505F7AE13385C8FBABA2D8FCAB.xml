Genus 
<taxonomicName id="4C47F3C55F7AE1308619FBAAA1CDFBB1" authority="Yu &amp; Xu, 2014" authorityName="Yu &amp; Xu" authorityYear="2014" box="[539,676,1068,1092]" class="Chromadorea" family="Xyalidae" genus="Pseudelzalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monhysterida" pageId="1" pageNumber="510" phylum="Nematoda" rank="genus" status="gen. nov.">
<emphasis id="B93354545F7AE1308619FBAAA1CDFBB1" bold="true" box="[539,676,1068,1092]" italics="true" pageId="1" pageNumber="510">Pseudelzalia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A200E92F5F7AE13086AFFBA8A07FFBB0" box="[685,790,1070,1094]" pageId="1" pageNumber="510" rank="genus">gen. nov.</taxonomicNameLabel>
