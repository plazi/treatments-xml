<taxonomicName id="F3DC3BE33E2E819910A34FBF0BFC104A" class="Polychaeta" family="Polynoidae" genus="Harmothoe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Harmothoe" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="genus">Harmothoe</taxonomicName>
