<taxonomicName id="4C064D5CFFAD157C355636F8E2F8FD44" authority="Barra, 1968" authorityName="Barra" authorityYear="1968" box="[1150,1436,672,698]" class="Entognatha" family="Isotomidae" genus="Isotomiella" kingdom="Animalia" order="Collembola" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="delamarei">
