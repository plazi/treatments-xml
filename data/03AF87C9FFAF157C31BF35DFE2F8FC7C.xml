<taxonomicName id="4C064D5CFFAD157C30033690E599FD1C" authority="Deharveng &amp; Oliveira, 1990" authorityName="Deharveng &amp; Oliveira" authorityYear="1990" box="[299,765,712,738]" class="Entognatha" family="Isotomidae" genus="Isotomiella" kingdom="Animalia" order="Collembola" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="spinifer">
