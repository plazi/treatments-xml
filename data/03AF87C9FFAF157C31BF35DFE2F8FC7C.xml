<taxonomicName id="4C064D5CFFAD157C33A336A8E302FCF4" authority="Mendonca &amp; Fernandes, 2003" authorityName="Mendonca &amp; Fernandes" authorityYear="2003" box="[651,1126,752,778]" class="Entognatha" family="Isotomidae" genus="Isotomiella" kingdom="Animalia" order="Collembola" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="fellina">
<emphasis id="B972EACDFFAD157C33A336A8E595FCF7" box="[651,753,752,777]" italics="true" pageId="7" pageNumber="8">I. fellina</emphasis>
Mendonça &amp; Fernandes, 
<number id="3DCE24F8FFAD157C350036A8E302FCF4" box="[1064,1126,752,778]" pageId="7" pageNumber="8" value="2003.0">2003</number>
