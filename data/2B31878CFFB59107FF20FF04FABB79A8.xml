<quantity id="64609B7FFFB59107FBACFBF8FB3C798B" box="[1051,1202,1125,1152]" metricMagnitude="2" metricUnit="m" metricValue="4.005" metricValueMax="6.77" metricValueMin="1.24" pageId="15" pageNumber="292" unit="m" value="400.5" valueMax="677.0" valueMin="124.0">124 to 677 m</quantity>
