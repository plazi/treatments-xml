<date id="093CEECCFFEEE92E8626FE9EC6CDF16D" pageId="8" pageNumber="9" value="2015-05-18">
<collectingDate id="19781724FFEEE92E8626FE9EC6CDF16D" pageId="8" pageNumber="9" value="2015-05-18">18 May 2015</collectingDate>
</date>
; 
<collectorName id="D077ADDAFFEEE92E823BFEE1C769F16A" box="[263,351,337,363]" pageId="8" pageNumber="9">Torsani</collectorName>
leg; 
<specimenCode id="2D246077FFEEE92E82A5FEE1C474F16A" box="[409,578,337,363]" collectionCode="MSNG" country="Italy" httpUri="http://grbio.org/cool/1trn-7b59" name="Museo Civico di Storia Naturale di Genova 'Giacomo Doria'" pageId="8" pageNumber="9">MSNG 61419</specimenCode>
