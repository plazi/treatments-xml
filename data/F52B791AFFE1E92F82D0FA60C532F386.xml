<quantity id="BA7A65E9FFEEE92E872FFE9EC25EF148" box="[1043,1128,302,329]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" pageId="8" pageNumber="9" unit="cm" value="1.5">1.5 cm</quantity>
