. 
<emphasis id="1884EA92FFA3F579D7CFF8A0FC6CF8E6" bold="true" box="[958,1001,1796,1820]" pageId="1" pageNumber="2">nov</emphasis>
</taxonomicNameLabel>
; 
<taxonomicName id="EDF04D03FFA3F579D789F8A0FA19F8E6" authority="Sosa &amp; Freitas" authorityName="Sosa &amp; Freitas" box="[1016,1436,1796,1820]" class="Insecta" family="Chrysopidae" genus="Titanochrysa" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="ferreirai" status="sp. nov.">
