</taxonomicNameLabel>
, and 
<taxonomicName id="EDF04D03FFA3F579D55CF88CFC80F8BA" authority="Sosa &amp; Freitas" authorityName="Sosa &amp; Freitas" box="[301,773,1832,1856]" class="Insecta" family="Chrysopidae" genus="Titanochrysa" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="trespuntensis" status="sp. nov.">
