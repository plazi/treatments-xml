<taxonomicName id="0C934D5BAC78FF829ED4FF62FCEF0E82" box="[700,830,188,212]" class="Magnoliopsida" family="Asteraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="594" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
