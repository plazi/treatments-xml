<taxonomicName id="68A3117587A6A4EC528B242A5F146090" LSID="http://zoobank.org/D0A64C11-6E02-40D6-A742-8D9E0FE08349" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus cinco" order="Hymenoptera" pageId="331" pageNumber="332" phylum="Arthropoda" rank="species" species="cinco">Heterospilus cinco Marsh</taxonomicName>
