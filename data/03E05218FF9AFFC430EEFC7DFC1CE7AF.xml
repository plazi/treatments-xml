SPECIES
</collectionCode>
. — 
<taxonomicName id="4C49988DFF9AFFC4317CFBA7FD8CE52A" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[300,612,1033,1057]" class="Gastropoda" family="Muricidae" genus="Murex" kingdom="Animalia" order="Neogastropoda" pageId="2" pageNumber="559" phylum="Mollusca" rank="species" species="scorpio">
