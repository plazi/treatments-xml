<taxonomicName id="5DA2B5EFA44027A8FAC074F1472BDEEF" class="Dicotyledoneae" family="Plantaginaceae" genus="Plantago" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Plantago" order="Plantaginales" pageId="0" pageNumber="57889" phylum="Angiospermae" rank="genus">
<emphasis id="72D22B867E8B3A813ADF02FFC7ACEB25" italics="true" pageId="0" pageNumber="57889">Plantago</emphasis>
