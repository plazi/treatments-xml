<taxonomicName id="EB45C3BA1EF8CB37F49F67BE1B9E21C8" class="Insecta" family="Carabidae" genus="Antennaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Antennaria" order="Coleoptera" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="genus">
<emphasis id="6F9FECB9B4EDA2EE409C244606969BEC" italics="true" pageId="0" pageNumber="57889">Antennaria</emphasis>
</taxonomicName>
spp. and 
<taxonomicName id="1298A6DF1BB1935753B0A63FF5324664" class="Magnoliopsida" family="Geometrida" genus="Salvia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Salvia" order="Lamiales" pageId="0" pageNumber="57889" phylum="Tracheophyta" rank="genus">
