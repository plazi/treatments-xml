<geoCoordinate id="EE0E507A8B43FFA05A15FA1BFD737458" box="[573,711,1518,1543]" direction="south" orientation="latitude" pageId="9" pageNumber="423" precision="15" value="-51.458336">
<number id="3DF2249A8B43FFA05A15FA1BFDEE7458" box="[573,602,1518,1543]" pageId="9" pageNumber="423" value="51.0">51</number>
° 
<number id="3DF2249A8B43FFA05A4BFA1BFD367458" box="[611,642,1518,1543]" pageId="9" pageNumber="423" value="27.0">27</number>
' 
<number id="3DF2249A8B43FFA05AADFA1BFD107458" box="[645,676,1518,1543]" pageId="9" pageNumber="423" value="30.0">30</number>
&quot; S
</geoCoordinate>
; 
<geoCoordinate id="EE0E507A8B43FFA05AFCFA1AFCD37459" box="[724,871,1519,1543]" direction="west" orientation="longitude" pageId="9" pageNumber="423" precision="15" value="-74.05">
<number id="3DF2249A8B43FFA05AFCFA1AFD467458" box="[724,754,1519,1543]" pageId="9" pageNumber="423" value="74.0">74</number>
°03'00” W
</geoCoordinate>
