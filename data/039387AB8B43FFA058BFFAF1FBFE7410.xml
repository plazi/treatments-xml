<taxonomicName id="4C3A4D3E8B43FFA05C2FF9E7FBCB7474" box="[1031,1151,1554,1579]" class="Hexactinellida" family="Rossellidae" genus="Rossella" kingdom="Animalia" order="Lyssacinosida" pageId="9" pageNumber="426" phylum="Porifera" rank="species" species="antarctica">
<collectingCountry id="F32D762D8B43FFA05C2FF9E7FBCB7474" box="[1031,1151,1554,1579]" name="Antarctica" pageId="9" pageNumber="423">Antarctica</collectingCountry>
</taxonomicName>
, southern South 
<collectingCountry id="F32D762D8B43FFA05D14F9E7FA287474" box="[1340,1436,1554,1579]" name="United States of America" pageId="9" pageNumber="423">America</collectingCountry>
