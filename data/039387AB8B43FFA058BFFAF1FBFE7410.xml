<taxonomicName id="4C3A4D3E8B43FFA05A34FA3EFC3377BC" authority="Schulze, 1887" authorityName="Schulze" authorityYear="1887" box="[540,903,1482,1507]" class="Hexactinellida" family="Rossellidae" genus="Rossella" kingdom="Animalia" order="Lyssacinosida" pageId="9" pageNumber="423" phylum="Porifera" rank="species" species="dubia">
