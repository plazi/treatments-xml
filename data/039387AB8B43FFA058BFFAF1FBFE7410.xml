<quantity id="4CC29B588B43FFA05AF3F9C2FCD67411" box="[731,866,1591,1615]" metricMagnitude="3" metricUnit="m" metricValue="1.004" metricValueMax="2.0" metricValueMin="0.008" pageId="9" pageNumber="423" unit="m" value="1004.0" valueMax="2000.0" valueMin="8.0">8 to 2000 m</quantity>
(Tabachnick, 
<number id="3DF2249A8B43FFA05C29F9C2FB8A7410" box="[1025,1086,1591,1615]" pageId="9" pageNumber="423" value="2002.0">2002</number>
).
