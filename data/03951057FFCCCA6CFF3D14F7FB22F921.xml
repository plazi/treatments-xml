<taxonomicName id="4C3CDAC2FFCCCA6CFCA7156EFC58FAA8" authorityName="Takaoka &amp; Davies" authorityYear="1995" box="[781,972,1380,1402]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="pahangense" subGenus="Daviesellum">
