: 
<taxonomicName id="BACE4D3DFFA3ED11762A98B0FD2050A1" authority="LI &amp; ZHOU" authorityName="LI &amp; ZHOU" authorityYear="2007" box="[294,659,1503,1524]" class="Insecta" family="Staphylinidae" genus="Paederus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="186" phylum="Arthropoda" rank="species" species="lineodenticulatus">
