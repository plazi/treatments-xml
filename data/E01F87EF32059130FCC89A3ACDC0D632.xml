Length 
<quantity id="AF4E9B1C32059133FCA39A9ACEC0D5A5" box="[870,1019,1415,1437]" metricMagnitude="-3" metricUnit="m" metricValue="4.78" metricValueMax="4.87" metricValueMin="4.69" pageId="1" pageNumber="161" unit="mm" value="4.78" valueMax="4.87" valueMin="4.69">4.69–4.87 mm</quantity>
