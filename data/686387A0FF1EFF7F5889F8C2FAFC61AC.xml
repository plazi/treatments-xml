<taxonomicName id="27CA4D35FF1EFF785889F8C2FEE66610" baseAuthorityName="Gorodkov" baseAuthorityYear="1986" box="[151,341,1871,1897]" class="Insecta" family="Scathophagidae" kingdom="Animalia" order="Diptera" pageId="211" pageNumber="212" phylum="Arthropoda" rank="family">
<emphasis id="D2BEEAA4FF1EFF785889F8C2FEE66610" bold="true" box="[151,341,1871,1897]" pageId="211" pageNumber="212">Scathophagidae</emphasis>
</taxonomicName>
