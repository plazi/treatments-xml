<emphasis id="05DFEAA6FFF43A3609E8F8D79713A834" bold="true" box="[151,312,1880,1907]" italics="true" pageId="26" pageNumber="278">
<taxonomicName id="F0AB4D37FFF43A3609E8F8D7970DA834" authorityName="Eichler" authorityYear="1951" box="[151,294,1880,1907]" class="Insecta" family="Philopteridae" genus="Allobrueelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="26" pageNumber="278" phylum="Arthropoda" rank="genus">Allobrueelia</taxonomicName>
