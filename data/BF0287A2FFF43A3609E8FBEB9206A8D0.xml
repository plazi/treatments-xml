<taxonomicName id="F0AB4D37FFF43A360A72F8D79590A835" authorityName="Gustafsson &amp; Bush &amp; Palma" authorityYear="2019" box="[781,955,1880,1906]" class="Insecta" family="Philopteridae" genus="Guimaraesiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="26" pageNumber="278" phylum="Arthropoda" rank="genus">
