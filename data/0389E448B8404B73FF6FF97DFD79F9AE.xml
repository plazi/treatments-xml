<taxonomicName id="4C202EDDB8424B73FA80FA38FF21FA43" class="Liliopsida" family="Arecaceae" genus="Syagrus" kingdom="Plantae" order="Arecales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="genus">
<emphasis id="B954894CB8424B73FA80FA38FF21FA43" italics="true" pageId="43" pageNumber="44">Syagrus</emphasis>
