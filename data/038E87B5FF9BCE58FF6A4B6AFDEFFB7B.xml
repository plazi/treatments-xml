<taxonomicName id="4C274D20FF9BCE58FF3B4B47FD62FEB3" authority="Miyatake, 1954" authorityName="Miyatake" authorityYear="1954" box="[198,679,296,322]" class="Insecta" family="Ciidae" genus="Omogocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="tuberculifrons">
