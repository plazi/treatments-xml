<paragraph id="8BB736CB1327925DFF3AFD4CFCB4FD0A" blockId="2.[151,1436,602,1423]" box="[151,818,671,695]" pageId="2" pageNumber="498">
<treatmentCitationGroup id="AB1811E51327925DFF3AFD4CFCB4FD0A" box="[151,818,671,695]" pageId="2" pageNumber="498">
<taxonomicName id="4C084D481327925DFF3AFD4CFDF0FD0A" authority="Li &amp; Liu, 2014a: 41" authorityName="Li &amp; Liu" authorityPageNumber="41" authorityYear="2014" box="[151,630,672,695]" class="Insecta" family="Miridae" genus="Camptotylidea" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="498" phylum="Arthropoda" rank="species" species="linzensis">
<emphasis id="B97CEAD91327925DFF3AFD4CFE13FD0A" box="[151,405,672,695]" italics="true" pageId="2" pageNumber="498">Camptotylidea linzensis</emphasis>
