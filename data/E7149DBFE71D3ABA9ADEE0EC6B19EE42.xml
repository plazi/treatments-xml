<paragraph id="F58294DE9E41D48E8011A27BC72397C2" pageId="7" pageNumber="146">
<taxonomicName id="77A6A6FDED9FE841C4463FAED9E75570" class="Insecta" family="Chironomidae" genus="Paracladopelma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracladopelma tokaraefea" order="Diptera" pageId="7" pageNumber="146" phylum="Arthropoda" rank="species" species="tokaraefea">Paracladopelma tokaraefea</taxonomicName>
