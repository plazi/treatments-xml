<superScript id="24A85F5EFFE92045114AFE25FCD9FE08" attach="left" box="[829,837,465,477]" fontSize="5" pageId="14" pageNumber="209">2</superScript>
; DT1-023, right DP 
<superScript id="24A85F5EFFE920451644FE25FBA7FE08" attach="left" box="[1075,1083,465,477]" fontSize="5" pageId="14" pageNumber="209">4</superScript>
; DT1-024, left M 
<subScript id="4F59F053FFE92045177DFE29FA8EFE3C" attach="right" box="[1290,1298,477,489]" fontSize="5" pageId="14" pageNumber="209">2</subScript>
; DT1-025, left M 
<subScript id="4F59F053FFE92045111AFE08FCE9FDDD" attach="left" box="[877,885,508,520]" fontSize="5" pageId="14" pageNumber="209">3</subScript>
