; 
<number id="F66924D2FFD8FFB2FCB9FC36B523FC33" box="[780,811,946,972]" pageId="6" pageNumber="59" value="20.0">20</number>
/ 
<date id="341F1035FFD8FFB2FC80FC36B5A6FC33" box="[821,942,946,972]" pageId="6" pageNumber="59" value="1917-08-30">
<number id="F66924D2FFD8FFB2FC80FC36B55CFC33" box="[821,852,946,972]" pageId="6" pageNumber="59" value="30.0">30</number>
- 
<number id="F66924D2FFD8FFB2FCE9FC36B5A6FC33" box="[860,942,946,972]" pageId="6" pageNumber="59" value="962.5" valueMax="1917.0" valueMin="8.0">8-1917</number>
</date>
, g. sl. Arct. 
<number id="F66924D2FFD8FFB2FBFEFC36B28DFC33" box="[1099,1157,946,972]" pageId="6" pageNumber="59" value="5860.0">5860</number>
