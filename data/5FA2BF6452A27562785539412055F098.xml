<taxonomicName id="2C424F4D7C91091561DF4DDEF102AEC1" LSID="urn:lsid:zoobank.org:act:635C2C5B-0B40-4821-AA42-30F7AEDB4BEA" class="Insecta" family="Eulophidae" genus="Achrysocharoides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Achrysocharoides callisetosus" order="Hymenoptera" pageId="6" pageNumber="85" phylum="Arthropoda" rank="species" species="callisetosus">Achrysocharoides callisetosus</taxonomicName>
