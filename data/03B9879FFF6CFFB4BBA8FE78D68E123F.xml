<taxonomicName id="4C104D0AFF6CFFB2BA7EF93DD0111758" authority="Pinkaew &amp; Muadsub, 2021" authorityName="Pinkaew &amp; Muadsub" authorityYear="2021" box="[321,567,1774,1798]" class="Insecta" family="Tortricidae" genus="Hiroshiinoueana" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="445" phylum="Arthropoda" rank="species" species="partita" status="sp. nov.">
<emphasis id="B964EA9BFF6CFFB2BA7EF93DD0111758" box="[321,567,1774,1798]" italics="true" pageId="3" pageNumber="445">Hiroshiinoueana partita</emphasis>
</taxonomicName>
<emphasis id="B964EA9BFF6CFFB2B902F93DD0A81758" bold="true" box="[573,654,1774,1798]" pageId="3" pageNumber="445">
