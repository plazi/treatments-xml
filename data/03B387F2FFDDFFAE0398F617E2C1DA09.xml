<taxonomicName id="4C1A4D67FFDAFFAC002DF564E74ED4CE" authority="Myers, 1998" authorityName="Myers" authorityYear="1998" box="[519,866,2777,2812]" class="Malacostraca" family="Aoridae" genus="Globosolembos" kingdom="Animalia" order="Amphipoda" pageId="12" pageNumber="199" phylum="Arthropoda" rank="species" species="forgesi" status="n.sp.">
