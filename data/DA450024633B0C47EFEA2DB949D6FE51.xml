<taxonomicName id="35E38BBED1732403FEF285253717D84F" authorityName="Westwood" authorityYear="1874" class="Insecta" family="Pteromalidae" genus="Oodera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oodera obscura" order="Hymenoptera" pageId="38" pageNumber="111" phylum="Arthropoda" rank="species" species="obscura">Oodera obscura</taxonomicName>
