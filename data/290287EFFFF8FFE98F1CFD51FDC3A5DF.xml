<taxonomicName id="66AB4D7AFFF9FFE98D14FA72FDE0A6CA" box="[319,531,1501,1524]" class="Reptilia" family="Gekkonidae" genus="Gecko" kingdom="Animalia" order="Squamata" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="spinicaudus">
