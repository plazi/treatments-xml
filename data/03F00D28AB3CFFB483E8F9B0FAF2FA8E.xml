<taxonomicName id="4C59C7BDAB3FFFB4870FFEBDFAF3FECD" authority="(Baker, 1906)" authorityName="Baker" authorityYear="1906" baseAuthorityName="Baker" baseAuthorityYear="1906" box="[1114,1398,302,329]" class="Insecta" family="Halictidae" genus="Lasioglossum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="ferrerii">
<emphasis id="B92D602CAB3FFFB4870FFEBDFB4AFECC" bold="true" box="[1114,1231,302,329]" pageId="50" pageNumber="51">
<emphasis id="B92D602CAB3FFFB4870FFEBDFBE9FECD" bold="true" box="[1114,1132,303,329]" italics="true" pageId="50" pageNumber="51">L</emphasis>
. 
<emphasis id="B92D602CAB3FFFB4872CFEBCFB4AFECC" bold="true" box="[1145,1231,302,328]" italics="true" pageId="50" pageNumber="51">ferrerii</emphasis>
</emphasis>
(
<bibRefCitation id="EFC8C1CFAB3FFFB4878BFEBCFAEBFECD" author="Baker" box="[1246,1390,302,329]" pageId="50" pageNumber="51" refString="Baker C. F. 1906. Halictinae de Cuba. In: Earle F. S. (ed.) Primer Informe de la Estacion Central Agronomica: 253 - 274. Ruiz y Hermano, Havana, Cuba." type="book chapter" year="1906">Baker, 1906</bibRefCitation>
