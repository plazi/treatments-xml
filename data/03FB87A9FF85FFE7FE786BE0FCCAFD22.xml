The 
<taxonomicName id="4C524D3CFF85FFE7FE2B6BE3FD39FE31" authorityName="ROHÁĆEK &amp; BARBER" authorityYear="2013" box="[491,763,473,498]" class="Insecta" family="Anthomyzidae" genus="Arganthomyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="98" pageNumber="95" phylum="Arthropoda" rank="species" species="socculata">
