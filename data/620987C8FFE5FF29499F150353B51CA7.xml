<number id="5C6824F9FFE4FF294ACB123B52BF1F5C" box="[963,1003,338,364]" pageId="5" pageNumber="56" value="1.2">1.2</number>
; prothorax width, 2.0; elytral length, 
<number id="5C6824F9FFE4FF29499F121151EB1FA2" box="[151,191,376,402]" pageId="5" pageNumber="56" value="5.5">5.5</number>
; humeral width, 
<number id="5C6824F9FFE4FF29488B121150FF1FA2" box="[387,427,376,402]" pageId="5" pageNumber="56" value="3.7">3.7</number>
