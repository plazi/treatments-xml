<taxonomicName id="31A94D7B5D8DFF4037EBFDA933D3A90D" authorityName="Webb" authorityYear="2006" box="[429,661,604,629]" class="Insecta" family="Therevidae" genus="Lysilinga" kingdom="Animalia" order="Diptera" pageId="219" pageNumber="220" phylum="Arthropoda" rank="species" species="crassiseta">
