)), 
<taxonomicName id="65D387EC352AFFA81586F920FCDBF97F" authority="A. Gray ex Torr." authorityName="A. Gray ex Torr." box="[468,784,1665,1691]" class="Magnoliopsida" family="Asteraceae" genus="Encelia" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="farinosa">
<emphasis id="90A7207D352AFFA81586F920FD9BF97F" box="[468,592,1665,1691]" italics="true" pageId="7" pageNumber="8">E. farinosa</emphasis>
A. Gray ex Torr.
</taxonomicName>
, 
<taxonomicName id="65D387EC352AFFA81749F923FC25F97F" box="[795,1006,1665,1691]" class="Magnoliopsida" family="Asteraceae" genus="Helianthus" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="annuus">
