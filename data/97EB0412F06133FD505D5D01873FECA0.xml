<paragraph id="AE22B6F05875979E13047E95D3506BC1" pageId="122" pageNumber="223">
<taxonomicName id="5CBF6F063A58CFEC3B94B444728BE8E8" authorityName="Burmeister" authorityYear="1847" class="Insecta" family="Scarabaeidae" genus="Cyclocephala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyclocephala variabilis" order="Coleoptera" pageId="122" pageNumber="223" phylum="Arthropoda" rank="species" species="variabilis">
