); 
<bibRefCitation id="EFEF33FBFF84FFA2FF48FA173094FE61" author="Medina" box="[192,488,377,402]" etAl="et al." firstAuthor="Medina" pageId="79" pageNumber="78" pagination="131 - 144" refId="ref215451" refString="Medina, C. A., A. Lopera-Toro, A. Vitolo, and B. D. Gill. 2001. Escarabajos coprofagos (Coleoptera: Scarabaeidae: Scarabaeinae) de Colombia. Biota Colombiana 2 (2): 131 - 144." type="journal article" year="2001">Medina et al. 2001: 136</bibRefCitation>
(Colombia - as 
<taxonomicName id="4C7E3589FF84FFA2FD27FA143245FE61" baseAuthorityName="Blanchard" baseAuthorityYear="1846" box="[687,825,377,401]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="orbignyi">
<emphasis id="B90A9218FF84FFA2FD27FA143245FE61" box="[687,825,377,401]" italics="true" pageId="79" pageNumber="78">D. orbignyi</emphasis>
</taxonomicName>
); 
<bibRefCitation id="EFEF33FBFF84FFA2FCDBFA1734DCFE61" author="Gonzalez-Alvarado, A. &amp; F. Z. Vaz-de-Mello" box="[851,1440,377,402]" journalOrPublisher="Annales de la Societe entomologique de France (N. S.)" pageId="79" pageNumber="78" pagination="431 - 476" part="50" refId="ref204827" refString="Gonzalez-Alvarado, A., and F. Z. Vaz-de-Mello. 2014. Taxonomic review of the subgenus Hybomidium Shipp 1897 (Coleoptera: Scarabaeidae: Scarabaeinae: Deltochilum). Annales de la Societe entomologique de France (N. S.) 50 (3 - 4): 431 - 476." title="Taxonomic review of the subgenus Hybomidium Shipp 1897 (Coleoptera: Scarabaeidae: Scarabaeinae: Deltochilum)" type="journal article" year="2014">Gonz√°lez-Alvarado and Vaz-de-Mello 2014: 451</bibRefCitation>
(Colombia, Panama, Trinidad and Tobago - as 
<taxonomicName id="4C7E3589FF84FFA2FD64FAF735CBFE41" baseAuthorityName="Westwood" baseAuthorityYear="1835" box="[748,1207,409,434]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="guildingii" subGenus="Hybomidium">
<emphasis id="B90A9218FF84FFA2FD64FAF732FDFE41" box="[748,897,409,433]" italics="true" pageId="79" pageNumber="78">Deltochilum</emphasis>
(
<emphasis id="B90A9218FF84FFA2FC07FAF73553FE41" box="[911,1071,409,433]" italics="true" pageId="79" pageNumber="78">Hybomidium</emphasis>
) 
<emphasis id="B90A9218FF84FFA2FBB6FAF735CBFE41" box="[1086,1207,409,433]" italics="true" pageId="79" pageNumber="78">guildingii</emphasis>
</taxonomicName>
; none - designation 
<typeStatus id="54C5F0A8FF84FFA2FF48FAD43014FE22" box="[192,360,442,466]" pageId="79" pageNumber="78">LECTOTYPE</typeStatus>
<taxonomicName id="4C7E3589FF84FFA2FEE7FAD73344FE21" authorityName="Buquet" authorityYear="1844" box="[367,568,441,465]" genus="Hyboma" kingdom="Animalia" pageId="79" pageNumber="78" rank="species" species="chalcea">
<emphasis id="B90A9218FF84FFA2FEE7FAD73344FE21" box="[367,568,441,465]" italics="true" pageId="79" pageNumber="78">Hyboma chalcea</emphasis>
</taxonomicName>
).
</paragraph>
<paragraph id="8BC14E0AFF84FFA2FF48FA8F3596FDC9" blockId="79.[192,1440,481,570]" pageId="79" pageNumber="78">
<emphasis id="B90A9218FF84FFA2FF48FA8F3062FE09" bold="true" box="[192,286,481,505]" pageId="79" pageNumber="78">Note 1.</emphasis>
<bibRefCitation id="EFEF33FBFF84FFA2FEACFA8F336DFE09" author="Paulian" box="[292,529,481,506]" firstAuthor="Paulian" pageId="79" pageNumber="78" pagination="213 - 296" refId="ref218901" refString="Paulian, R. 1938. Contribution a l'etude des canthonides americains [Coleopt. Lamellic.]. Annales de la Societe entomologique de France 107 (3 - 4): 213 - 296." type="journal article" year="1938">Paulian (1938: 262)</bibRefCitation>
considered 
<taxonomicName id="4C7E3589FF84FFA2FD29FA8F3214FE09" authorityName="Buquet" authorityYear="1844" box="[673,872,481,505]" genus="Hyboma" kingdom="Animalia" pageId="79" pageNumber="78" rank="species" species="chalcea">
<emphasis id="B90A9218FF84FFA2FD29FA8F3214FE09" box="[673,872,481,505]" italics="true" pageId="79" pageNumber="78">Hyboma chalcea</emphasis>
</taxonomicName>
a synonym of 
<emphasis id="B90A9218FF84FFA2FB9CFA8F3460FE09" box="[1044,1308,481,505]" italics="true" pageId="79" pageNumber="78">
<taxonomicName id="4C7E3589FF84FFA2FB9CFA8F3464FE09" baseAuthorityName="Blanchard" baseAuthorityYear="1846" box="[1044,1304,481,505]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="orbignyi">Deltochilum orbignyi</taxonomicName>
,
</emphasis>
while 
<bibRefCitation id="EFEF33FBFF84FFA2FAE3FA8F335CFDE9" author="Vulcano and Pereira" firstAuthor="Vulcano" pageId="79" pageNumber="78" pagination="570 - 685" refId="ref228948" refString="Vulcano, M. A., and F. S. Pereira. 1964. Catalogue of the Canthonini (Col. Scarab.) inhabiting the Western Hemisphere. Entomologische Arbeiten aus dem Museum G. Frey 15 (2): 570 - 685." type="journal article" year="1964">Vulcano and Pereira (1964: 649)</bibRefCitation>
considered 
<taxonomicName id="4C7E3589FF84FFA2FD3AF96C322DFDE9" baseAuthorityName="Westwood" baseAuthorityYear="1835" box="[690,849,513,537]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="guildingii">
<emphasis id="B90A9218FF84FFA2FD3AF96C322DFDE9" box="[690,849,513,537]" italics="true" pageId="79" pageNumber="78">H. guildingii</emphasis>
</taxonomicName>
a synonym of 
<taxonomicName id="4C7E3589FF84FFA2FB8BF96C350FFDE9" baseAuthorityName="Olivier" baseAuthorityYear="1789" box="[1027,1139,514,537]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="icarus">
<emphasis id="B90A9218FF84FFA2FB8BF96C350FFDE9" box="[1027,1139,514,537]" italics="true" pageId="79" pageNumber="78">D. icarus</emphasis>
</taxonomicName>
. These synonymizations are discussed and invalidated in 
<bibRefCitation id="EFEF33FBFF84FFA2FDDBF94F35DEFDCA" author="Gonzalez-Alvarado, A. &amp; F. Z. Vaz-de-Mello" box="[595,1186,545,570]" journalOrPublisher="Annales de la Societe entomologique de France (N. S.)" pageId="79" pageNumber="78" pagination="431 - 476" part="50" refId="ref204827" refString="Gonzalez-Alvarado, A., and F. Z. Vaz-de-Mello. 2014. Taxonomic review of the subgenus Hybomidium Shipp 1897 (Coleoptera: Scarabaeidae: Scarabaeinae: Deltochilum). Annales de la Societe entomologique de France (N. S.) 50 (3 - 4): 431 - 476." title="Taxonomic review of the subgenus Hybomidium Shipp 1897 (Coleoptera: Scarabaeidae: Scarabaeinae: Deltochilum)" type="journal article" year="2014">Gonz√°lez-Alvarado and Vaz-de-Mello (2014: 449</bibRefCitation>
, 454).
</paragraph>
<paragraph id="8BC14E0AFF84FFA2FF48F92734DCFCD1" blockId="79.[192,1440,585,801]" pageId="79" pageNumber="78">
<emphasis id="B90A9218FF84FFA2FF48F927305FFD91" bold="true" box="[192,291,585,609]" pageId="79" pageNumber="78">Note 2.</emphasis>
The records of 
<taxonomicName id="4C7E3589FF84FFA2FE66F9273279FD91" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[494,773,585,609]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="gibbosum">
<emphasis id="B90A9218FF84FFA2FE66F9273279FD91" box="[494,773,585,609]" italics="true" pageId="79" pageNumber="78">Deltochilum gibbosum</emphasis>
</taxonomicName>
from 
<collectingCountry id="F3690E9AFF84FFA2FCDCF92432C9FD92" box="[852,949,586,610]" name="Guyana" pageId="79" pageNumber="78">Guyana</collectingCountry>
and 
<collectingCountry id="F3690E9AFF84FFA2FC71F9273504FD91" box="[1017,1144,585,609]" name="Venezuela" pageId="79" pageNumber="78">Venezuela</collectingCountry>
east of the Orinoco in, respectively, 
<bibRefCitation id="EFEF33FBFF84FFA2FED6F90733D9FD72" author="Forsyth, A. B. &amp; B. D. Gill" bookContentInfo="70 p." box="[350,677,617,642]" editor="T. A. Parker &amp; III &amp; R. B. Foster &amp; L. H. Emmons &amp; P. Freed &amp; A. B. Forsyth &amp; B. Hoffman &amp; B. D. Gill" journalOrPublisher="Conservation International" pageId="79" pageNumber="78" refId="ref202721" refString="Forsyth, A. B., and B. D. Gill. 1993. Invertebrate indicators. p. 32 - 33, 70. In: T. A. Parker III, R. B. Foster, L. H. Emmons, P. Freed, A. B. Forsyth, B. Hoffman, and B. D. Gill. RAP Working Papers 5. A biological assessment of the Kanuku Mountain region of southwestern Guyana. Conservation International. 70 p." title="Invertebrate indicators" type="book" volumeTitle="RAP Working Papers 5. A biological assessment of the Kanuku Mountain region of southwestern Guyana" year="1993">Forsyth and Gill (1993: 70)</bibRefCitation>
and 
<bibRefCitation id="EFEF33FBFF84FFA2FD69F9073534FD72" author="Ferrer-Paris, J. R. &amp; A. Sanchez-Mercado &amp; J. P. Rodriguez" box="[737,1096,617,642]" journalOrPublisher="Revista de Biologia Tropical" pageId="79" pageNumber="78" pagination="89 - 110" part="61" refId="ref202516" refString="Ferrer-Paris, J. R., A. Sanchez-Mercado, and J. P. Rodriguez. 2013. Optimizacion del muestreo de invertebrados tropicales: Un ejemplo con escarabajos coprofagos (Coleoptera: Scarabaeinae) en Venezuela. Revista de Biologia Tropical 61 (1): 89 - 110." title="Optimizacion del muestreo de invertebrados tropicales: Un ejemplo con escarabajos coprofagos (Coleoptera: Scarabaeinae) en Venezuela" type="journal article" year="2013">Ferrer-Paris et al. (2013: 109)</bibRefCitation>
are probably attributable to misidentifications. 
<taxonomicName id="4C7E3589FF84FFA2FE2EF9E43340FD51" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[422,572,649,673]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="gibbosum">
<emphasis id="B90A9218FF84FFA2FE2EF9E43340FD51" box="[422,572,649,673]" italics="true" pageId="79" pageNumber="78">D. gibbosum</emphasis>
</taxonomicName>
is a species from North America, and the species 
<taxonomicName id="4C7E3589FF84FFA2FB06F9E4340AFD51" authorityName="Balthasar" authorityYear="1939" box="[1166,1398,649,673]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="densepunctatum">
<emphasis id="B90A9218FF84FFA2FB06F9E4340AFD51" box="[1166,1398,649,673]" italics="true" pageId="79" pageNumber="78">D. densepunctatum</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C7E3589FF84FFA2FA0CF9E4305AFD31" authorityName="H. W. Bates" authorityYear="1887" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="sublaeve">
<emphasis id="B90A9218FF84FFA2FA0CF9E434E4FD51" box="[1412,1432,650,673]" italics="true" pageId="79" pageNumber="78">D</emphasis>
. 
<emphasis id="B90A9218FF84FFA2FF48F9C7305AFD31" box="[192,294,681,705]" italics="true" pageId="79" pageNumber="78">sublaeve</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C7E3589FF84FFA2FED6F9C43372FD31" authorityName="Howden" authorityYear="1966" box="[350,526,682,705]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="panamensis">
<emphasis id="B90A9218FF84FFA2FED6F9C43372FD31" box="[350,526,682,705]" italics="true" pageId="79" pageNumber="78">D. panamensis</emphasis>
</taxonomicName>
, which for some time were considered subspecies of 
<taxonomicName id="4C7E3589FF84FFA2FBE7F9C4347FFD31" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[1135,1283,681,705]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="gibbosum">
<emphasis id="B90A9218FF84FFA2FBE7F9C4347FFD31" box="[1135,1283,681,705]" italics="true" pageId="79" pageNumber="78">D. gibbosum</emphasis>
</taxonomicName>
, appear to be confined to 
<collectingCountry id="F3690E9AFF84FFA2FECEF9A730E7FD11" box="[326,411,713,737]" name="Mexico" pageId="79" pageNumber="78">Mexico</collectingCountry>
(
<taxonomicName id="4C7E3589FF84FFA2FE20F9A433F3FD11" authorityName="Balthasar" authorityYear="1939" box="[424,655,713,737]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="densepunctatum">
<emphasis id="B90A9218FF84FFA2FE20F9A433F3FD11" box="[424,655,713,737]" italics="true" pageId="79" pageNumber="78">D. densepunctatum</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C7E3589FF84FFA2FD4FF9A43232FD11" authorityName="H. W. Bates" authorityYear="1887" box="[711,846,713,737]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="sublaeve">
<emphasis id="B90A9218FF84FFA2FD4FF9A43232FD11" box="[711,846,713,737]" italics="true" pageId="79" pageNumber="78">D. sublaeve</emphasis>
</taxonomicName>
) and 
<collectingCountry id="F3690E9AFF84FFA2FC06F9A4356CFD11" box="[910,1040,713,738]" name="Costa Rica" pageId="79" pageNumber="78">Costa Rica</collectingCountry>
, 
<collectingCountry id="F3690E9AFF84FFA2FB92F9A735F3FD11" box="[1050,1167,713,737]" name="Colombia" pageId="79" pageNumber="78">Colombia</collectingCountry>
, 
<collectingCountry id="F3690E9AFF84FFA2FB10F9A73583FD11" box="[1176,1279,713,737]" name="Ecuador" pageId="79" pageNumber="78">Ecuador</collectingCountry>
, 
<collectingCountry id="F3690E9AFF84FFA2FA80F9A43410FD11" box="[1288,1388,714,737]" name="Panama" pageId="79" pageNumber="78">Panama</collectingCountry>
and 
<collectingCountry id="F3690E9AFF84FFA2FF48F9843186FCF1" box="[192,250,746,769]" name="Peru" pageId="79" pageNumber="78">Peru</collectingCountry>
(
<taxonomicName id="4C7E3589FF84FFA2FE82F98430C1FCF1" authorityName="Howden" authorityYear="1966" box="[266,445,746,769]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="panamensis">
<emphasis id="B90A9218FF84FFA2FE82F98430C1FCF1" box="[266,445,746,769]" italics="true" pageId="79" pageNumber="78">D. panamensis</emphasis>
</taxonomicName>
). The material mentioned in 
<bibRefCitation id="EFEF33FBFF84FFA2FC96F987354BFCF2" author="Forsyth, A. B. &amp; B. D. Gill" bookContentInfo="70 p." box="[798,1079,745,770]" editor="T. A. Parker &amp; III &amp; R. B. Foster &amp; L. H. Emmons &amp; P. Freed &amp; A. B. Forsyth &amp; B. Hoffman &amp; B. D. Gill" journalOrPublisher="Conservation International" pageId="79" pageNumber="78" refId="ref202721" refString="Forsyth, A. B., and B. D. Gill. 1993. Invertebrate indicators. p. 32 - 33, 70. In: T. A. Parker III, R. B. Foster, L. H. Emmons, P. Freed, A. B. Forsyth, B. Hoffman, and B. D. Gill. RAP Working Papers 5. A biological assessment of the Kanuku Mountain region of southwestern Guyana. Conservation International. 70 p." title="Invertebrate indicators" type="book" volumeTitle="RAP Working Papers 5. A biological assessment of the Kanuku Mountain region of southwestern Guyana" year="1993">Forsyth and Gill (1993)</bibRefCitation>
and 
<bibRefCitation id="EFEF33FBFF84FFA2FBFBF98734DCFCF2" author="Ferrer-Paris, J. R. &amp; A. Sanchez-Mercado &amp; J. P. Rodriguez" box="[1139,1440,745,770]" journalOrPublisher="Revista de Biologia Tropical" pageId="79" pageNumber="78" pagination="89 - 110" part="61" refId="ref202516" refString="Ferrer-Paris, J. R., A. Sanchez-Mercado, and J. P. Rodriguez. 2013. Optimizacion del muestreo de invertebrados tropicales: Un ejemplo con escarabajos coprofagos (Coleoptera: Scarabaeinae) en Venezuela. Revista de Biologia Tropical 61 (1): 89 - 110." title="Optimizacion del muestreo de invertebrados tropicales: Un ejemplo con escarabajos coprofagos (Coleoptera: Scarabaeinae) en Venezuela" type="journal article" year="2013">Ferrer-Paris et al. (2013)</bibRefCitation>
probably belongs to 
<taxonomicName id="4C7E3589FF84FFA2FE25F864333AFCD1" baseAuthorityName="Westwood" baseAuthorityYear="1835" box="[429,582,777,801]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="guildingii">
<emphasis id="B90A9218FF84FFA2FE25F864333AFCD1" box="[429,582,777,801]" italics="true" pageId="79" pageNumber="78">D. guildingii</emphasis>
</taxonomicName>
, but as we cannot confirm this, we have not included these records here.
</paragraph>
<paragraph id="8BC14E0AFF84FFA2FF48F85F327AFBF9" blockId="79.[192,1440,817,1034]" pageId="79" pageNumber="78">
<emphasis id="B90A9218FF84FFA2FF48F85F3060FCB9" bold="true" box="[192,284,817,841]" pageId="79" pageNumber="78">Note 3.</emphasis>
According to 
<bibRefCitation id="EFEF33FBFF84FFA2FE33F85F357EFCB9" author="Gonzalez-Alvarado, A. &amp; F. Z. Vaz-de-Mello" box="[443,1026,817,842]" journalOrPublisher="Annales de la Societe entomologique de France (N. S.)" pageId="79" pageNumber="78" pagination="431 - 476" part="50" refId="ref204827" refString="Gonzalez-Alvarado, A., and F. Z. Vaz-de-Mello. 2014. Taxonomic review of the subgenus Hybomidium Shipp 1897 (Coleoptera: Scarabaeidae: Scarabaeinae: Deltochilum). Annales de la Societe entomologique de France (N. S.) 50 (3 - 4): 431 - 476." title="Taxonomic review of the subgenus Hybomidium Shipp 1897 (Coleoptera: Scarabaeidae: Scarabaeinae: Deltochilum)" type="journal article" year="2014">Gonz√°lez-Alvarado and Vaz-de-Mello (2014: 454)</bibRefCitation>
, the record of 
<taxonomicName id="4C7E3589FF84FFA2FB2CF85F34DCFCB9" baseAuthorityName="Blanchard" baseAuthorityYear="1846" box="[1188,1440,817,841]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="orbignyi">
<emphasis id="B90A9218FF84FFA2FB2CF85F34DCFCB9" box="[1188,1440,817,841]" italics="true" pageId="79" pageNumber="78">Deltochilum orbignyi</emphasis>
</taxonomicName>
for 
<collectingCountry id="F3690E9AFF84FFA2FF61F83F3020FC99" box="[233,348,849,873]" name="Colombia" pageId="79" pageNumber="78">Colombia</collectingCountry>
in 
<bibRefCitation id="EFEF33FBFF84FFA2FE0BF83F33CDFC99" author="Medina" box="[387,689,849,874]" etAl="et al." firstAuthor="Medina" pageId="79" pageNumber="78" pagination="131 - 144" refId="ref215451" refString="Medina, C. A., A. Lopera-Toro, A. Vitolo, and B. D. Gill. 2001. Escarabajos coprofagos (Coleoptera: Scarabaeidae: Scarabaeinae) de Colombia. Biota Colombiana 2 (2): 131 - 144." type="journal article" year="2001">Medina et al. (2001: 136)</bibRefCitation>
concerns in fact 
<taxonomicName id="4C7E3589FF84FFA2FCF6F83C3566FC99" baseAuthorityName="Westwood" baseAuthorityYear="1835" box="[894,1050,849,873]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="78" phylum="Arthropoda" rank="species" species="guildingii">
<emphasis id="B90A9218FF84FFA2FCF6F83C3566FC99" box="[894,1050,849,873]" italics="true" pageId="79" pageNumber="78">D. guildingii</emphasis>
</taxonomicName>
. Several more authors (
<bibRefCitation id="EFEF33FBFF84FFA2FAB7F83F3044FC79" author="Paulian" firstAuthor="Paulian" pageId="79" pageNumber="78" pagination="213 - 296" refId="ref218901" refString="Paulian, R. 1938. Contribution a l'etude des canthonides americains [Coleopt. Lamellic.]. Annales de la Societe entomologique de France 107 (3 - 4): 213 - 296." type="journal article" year="1938">Paulian 1938: 262</bibRefCitation>
; 
<bibRefCitation id="EFEF33FBFF84FFA2FECBF81F33C1FC79" author="Vulcano and Pereira" box="[323,701,881,905]" firstAuthor="Vulcano" pageId="79" pageNumber="78" pagination="570 - 685" refId="ref228948" refString="Vulcano, M. A., and F. S. Pereira. 1964. Catalogue of the Canthonini (Col. Scarab.) inhabiting the Western Hemisphere. Entomologische Arbeiten aus dem Museum G. Frey 15 (2): 570 - 685." type="journal article" year="1964">Vulcano and Pereira 1964: 650</bibRefCitation>
; 
<bibRefCitation id="EFEF33FBFF84FFA2FD40F81F32A7FC7A" author="Noriega" box="[712,987,881,906]" etAl="et al." firstAuthor="Noriega" pageId="79" pageNumber="78" pagination="77 - 86" refId="ref217549" refString="Noriega, J. A., C. Solis, F. Escobar, and E. Realpe. 2007. Escarabajos coprofagos (Coleoptera: Scarabaeidae) de la provincia de la Sierra Nevada de Santa Marta. Biota Colombiana 8 (1): 77 - 86." type="journal article" year="2007">Noriega et al. 2007: 82</bibRefCitation>
) have mentioned 
