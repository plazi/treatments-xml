<taxonomicName id="6DFD27A9BF70FFCC5ABBFB3837474F95" authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[452,553,1247,1273]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
