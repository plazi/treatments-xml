<taxonomicName id="6DFD27A9BF70FFCC58EFFB38316D4F95" box="[912,1027,1247,1273]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
