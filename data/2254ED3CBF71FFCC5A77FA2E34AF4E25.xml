, 
<taxonomicName id="6DFD27A9BF70FFCC58D3FB8F313A4FED" box="[940,1108,1128,1153]" class="Insecta" family="Noctuidae" genus="Feltia" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="subgothica">
<emphasis id="98898038BF70FFCC58D3FB8F313A4FED" box="[940,1108,1128,1153]" italics="true" pageId="29" pageNumber="30">F. subgothica</emphasis>
</taxonomicName>
, and 
<emphasis id="98898038BF70FFCC5FD5FB8F30454FED" box="[1194,1323,1128,1153]" italics="true" pageId="29" pageNumber="30">
<taxonomicName id="6DFD27A9BF70FFCC5FD5FB8F30494FED" box="[1194,1319,1128,1153]" class="Insecta" family="Noctuidae" genus="Feltia" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="tricosa">F. tricosa</taxonomicName>
