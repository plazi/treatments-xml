<taxonomicName id="6DFD27A9BF70FFCC5841FB7736A14FC5" authorityName="Dingy Cutworm Moth" baseAuthorityName="Guenee" baseAuthorityYear="1852" box="[830,975,1168,1193]" class="Insecta" family="Noctuidae" genus="Feltia" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="jaculifera">
