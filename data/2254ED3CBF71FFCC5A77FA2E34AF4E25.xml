<taxonomicName id="6DFD27A9BF70FFCC5832FAE036DC4E4D" authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[845,946,1287,1313]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
