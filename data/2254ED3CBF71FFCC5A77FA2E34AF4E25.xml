<taxonomicName id="6DFD27A9BF70FFCC5985FB8F36FD4FED" authorityName="Dingy Cutworm Moth" baseAuthorityName="Guenee" baseAuthorityYear="1852" box="[762,915,1128,1153]" class="Insecta" family="Noctuidae" genus="Feltia" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="jaculifera">
