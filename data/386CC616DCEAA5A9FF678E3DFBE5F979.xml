<taxonomicName id="77C50C83DCEAA5A9FF678E3DFDDFF9C9" authority="Borojevic, 1967" authorityName="Borojevic" authorityYear="1967" box="[151,529,1591,1616]" class="Calcarea" family="Sycettidae" genus="Sycon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosolenida" pageId="152" pageNumber="153" phylum="Porifera" rank="species" species="natalense">
