MATÉRIEL EXAMINÉ. — 
<materialsCitation id="39A1235DAC523B35FE72DE905940F948" box="[419,1283,1749,1769]" collectingDate="1957-05-19" collectingDateMax="1991-05" collectingDateMin="1957-05-19" collectionCode="V" collectorName="Ziela, U. &amp; du Zie" location="Nimba" pageId="30" pageNumber="423" specimenCount="1" stateProvince="Nimba">
<collectingRegion id="4B0DE7E2AC523B35FE72DE905DA2F948" box="[419,481,1749,1769]" country="Liberia" name="Nimba" pageId="30" pageNumber="423">Nimba</collectingRegion>
: 
<collectorName id="243C4CD6AC523B35FE25DE905E00F948" box="[500,579,1749,1769]" pageId="30" pageNumber="423">Ziéla, U.</collectorName>
<collectionCode id="EFD8B1C5AC523B35FD93DE905E16F948" box="[578,597,1749,1769]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="30" pageNumber="423" type="Museum">V</collectionCode>
., 1 et 30.III et 4, 
<date id="FD770FC0AC523B35FD22DE905FCCF948" box="[755,911,1749,1769]" pageId="30" pageNumber="423" value="1957-05-19" valueMax="1957-05-24" valueMin="1957-05-19">
<collectingDate id="ED33F628AC523B35FD22DE905FCCF948" box="[755,911,1749,1769]" pageId="30" pageNumber="423" value="1957-05-19" valueMax="1957-05-24" valueMin="1957-05-19">19 et 24.V. 1957</collectingDate>
</date>
; forêt-galerie 
<collectorName id="243C4CD6AC523B35FBC4DE905813F948" box="[1045,1104,1749,1769]" pageId="30" pageNumber="423">du Zié</collectorName>
, battage, 
<date id="FD770FC0AC523B35FB78DE905940F948" box="[1193,1283,1749,1769]" pageId="30" pageNumber="423" value="1991-05">
<collectingDate id="ED33F628AC523B35FB78DE905940F948" box="[1193,1283,1749,1769]" pageId="30" pageNumber="423" value="1991-05">mai 1991</collectingDate>
</date>
</materialsCitation>
