<geoCoordinate id="20A0F9F5CC6DFFD4B55BFAA8FB5A1948" box="[1117,1180,1305,1331]" degrees="0.067" direction="north" orientation="latitude" pageId="34" pageNumber="235" precision="55" value="0.067">0.067</geoCoordinate>
