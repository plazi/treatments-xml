</taxonomicName>
, 
<taxonomicName id="2BD0EDBDFFB6FFB7FD3CF98BFCF12C6D" authorityName="Chen" authorityYear="1980" box="[679,875,1575,1602]" class="Insecta" family="Thripidae" genus="Astrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="269" phylum="Arthropoda" rank="species" species="chisinliaoensis">
<emphasis id="DEA44A2CFFB6FFB7FD3CF98BFCF12C6D" box="[679,875,1575,1602]" italics="true" pageId="2" pageNumber="269">A. chisinliaoensis</emphasis>
</taxonomicName>
, and 
<taxonomicName id="2BD0EDBDFFB6FFB7FC33F98BFBB02C6D" authorityName="Kudo" authorityYear="1979" box="[936,1066,1576,1602]" class="Insecta" family="Thripidae" genus="Astrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="269" phylum="Arthropoda" rank="species" species="strasseni">
<emphasis id="DEA44A2CFFB6FFB7FC33F98BFBB02C6D" box="[936,1066,1576,1602]" italics="true" pageId="2" pageNumber="269">A. strasseni</emphasis>
</taxonomicName>
. However, four more 
<taxonomicName id="2BD0EDBDFFB6FFB7FA84F984FA062C6D" authorityName="Karny" authorityYear="1921" box="[1311,1436,1575,1601]" class="Insecta" family="Thripidae" genus="Astrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="269" phylum="Arthropoda" rank="genus">
