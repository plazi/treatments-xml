<emphasis id="B917EAB6FF84FFE1FC872E02778DF8BB" bold="true" box="[812,999,1878,1902]" pageId="2" pageNumber="21">
<collectorName id="26965372FF84FFE1FC872E02771BF8BB" box="[812,881,1878,1902]" pageId="2" pageNumber="21">Adult</collectorName>
habitus.
</emphasis>
<collectorName id="26965372FF84FFE1FC562E0270A2F8BB" box="[1021,1224,1878,1902]" pageId="2" pageNumber="21">Biocellate. Head</collectorName>
