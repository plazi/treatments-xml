<taxonomicName id="FFF03A80704C6D6C9C9CFE09FD982F63" authority="Pitsch 1993 a" authorityName="Pitsch" authorityYear="1993" box="[151,595,510,537]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="489" phylum="Arthropoda" rank="species" species="incognita">
