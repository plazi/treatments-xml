<taxonomicName id="57B78E88B559EB7EB2F013DEFB9DF3F0" authorityName="Joicey &amp; Talbot" authorityYear="1917" box="[950,1082,1937,1961]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="fulvitincta">
