<taxonomicName id="57B78E88B559EB7EB0BA13FAFD0CF394" authorityName="Schmidt" authorityYear="2020" box="[508,683,1973,1997]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="diversisimplex">
