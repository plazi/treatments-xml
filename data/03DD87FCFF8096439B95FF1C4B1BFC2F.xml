<quantity id="4C8C9B0FFF8196439A08FDEF4BE0FD93" box="[266,399,548,573]" metricMagnitude="2" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="9" pageNumber="482" unit="m" value="165.0" valueMax="180.0" valueMin="150.0">150–180 m</quantity>
