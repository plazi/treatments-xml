<taxonomicName id="4C744D69FF819643990DFCEA48F1FC97" box="[527,670,800,824]" class="Malacostraca" family="Solenoceridae" genus="Mesopenaeus" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="482" phylum="Arthropoda" rank="species" species="tropicalis">
