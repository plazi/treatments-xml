<collectingCountry id="F358761FFF58FF2EF232FCF42FB4771E" box="[359,444,777,801]" name="Taiwan" pageId="187" pageNumber="188">Taiwan</collectingCountry>
, 
<collectingCountry id="F358761FFF58FF2EF292FCF42C05771E" box="[455,525,777,801]" name="China" pageId="187" pageNumber="188">China</collectingCountry>
, 
<collectingCountry id="F358761FFF58FF2EF14DFCF42C77771E" box="[536,639,777,801]" name="Thailand" pageId="187" pageNumber="188">Thailand</collectingCountry>
, 
<collectingCountry id="F358761FFF58FF2EF1DCFCF42CE3771E" box="[649,747,777,801]" name="Vietnam" pageId="187" pageNumber="188">Vietnam</collectingCountry>
, 
<collectingCountry id="F358761FFF58FF2EF1A3FCF42D7F771E" box="[758,887,777,801]" name="Philippines" pageId="187" pageNumber="188">Philippines</collectingCountry>
, 
<collectingCountry id="F358761FFF58FF2EF0D7FCF42DFA771E" box="[898,1010,777,801]" name="Indonesia" pageId="187" pageNumber="188">Indonesia</collectingCountry>
