and 
<collectingCountry id="F35876365918276FFD9DAC8FFD17C88A" box="[594,748,1202,1228]" name="South Africa" pageId="31" pageNumber="32">South Africa</collectingCountry>
, from 
<quantity id="4CB79B435918276FFCF7AC8FFC3BC88A" box="[824,960,1202,1228]" metricMagnitude="2" metricUnit="m" metricValue="3.035" metricValueMax="3.72" metricValueMin="2.35" pageId="31" pageNumber="32" unit="m" value="303.5" valueMax="372.0" valueMin="235.0">
235– 
<specimenCount id="9D49FD2F5918276FFCBDAC8FFC3BC88A" box="[882,960,1202,1228]" pageId="31" pageNumber="32" type="generic">372 m</specimenCount>
</quantity>
