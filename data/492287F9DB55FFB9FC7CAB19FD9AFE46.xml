<subSubSection id="89916564DB51FFB9FF42AD11FE8FFE66" pageId="46" pageNumber="575" type="biology_ecology">
<paragraph id="C13436EFDB51FFB9FF42AD11FE8FFE66" blockId="46.[151,760,190,437]" pageId="46" pageNumber="575">
<emphasis id="F3FFEAFDDB51FFB9FF42AD11FE80FE86" bold="true" box="[151,283,350,373]" pageId="46" pageNumber="575">Host plant.</emphasis>
<taxonomicName id="068B4D6CDB51FFB9FEF0AD11FE53FE86" box="[293,456,350,373]" class="Magnoliopsida" family="Caprifoliaceae" kingdom="Plantae" order="Dipsacales" pageId="46" pageNumber="575" phylum="Tracheophyta" rank="family">Caprifoliaceae</taxonomicName>
: 
<taxonomicName id="068B4D6CDB51FFB9FE03AD11FD63FE86" authority="Wall." authorityName="Wall." box="[470,760,350,373]" class="Magnoliopsida" family="Caprifoliaceae" genus="Lonicera" kingdom="Plantae" order="Dipsacales" pageId="46" pageNumber="575" phylum="Tracheophyta" rank="species" species="acuminata">
<emphasis id="F3FFEAFDDB51FFB9FE03AD11FD2FFE86" box="[470,692,350,373]" italics="true" pageId="46" pageNumber="575">Lonicera acuminata</emphasis>
