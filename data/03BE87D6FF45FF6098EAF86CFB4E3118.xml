<quantity id="4CEF9B25FF45FF619C59F86DFE3837BF" box="[1060,1145,1951,1978]" metricMagnitude="-1" metricUnit="m" metricValue="4.6735999999999995" pageId="212" pageNumber="213" unit="in" value="18.4">18.4 in</quantity>
