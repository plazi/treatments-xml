<number id="3D9E24AC3333DC5EFB69FDDE317E33ED" box="[1223,1299,632,658]" pageId="10" pageNumber="11" value="76.74">76, 74</number>
, 
<number id="3D9E24AC3333DC5EFAB1FDDE310633ED" box="[1311,1387,632,658]" pageId="10" pageNumber="11" value="55.58">55, 58</number>
, 
<number id="3D9E24AC3333DC5EFAD9FDDE34D533C5" pageId="10" pageNumber="11" value="10.14">10, 14</number>
