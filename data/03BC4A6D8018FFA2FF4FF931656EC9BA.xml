<collectingCountry id="F302BBEB8005FFA2FEB9FE1B67D7C9BA" box="[353,435,439,465]" name="Bolivia" pageId="18" pageNumber="319">Bolivia</collectingCountry>
(
<collectingRegion id="49D135998005FFA2FE1AFE1B6477C9BA" box="[450,531,439,465]" country="Bolivia" name="La Paz" pageId="18" pageNumber="319">La Paz</collectingRegion>
) and 
<collectingCountry id="F302BBEB8005FFA2FD89FE1B64E0C9BA" box="[593,644,439,465]" name="Peru" pageId="18" pageNumber="319">Peru</collectingCountry>
(
