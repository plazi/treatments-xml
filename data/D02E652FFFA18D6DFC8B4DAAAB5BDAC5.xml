<collectingRegion id="9A431ADBFFA08D6DFB944BB1ACF1DAE5" box="[1134,1227,198,219]" country="Costa Rica" name="Cartago" pageId="51">Cartago</collectingRegion>
, 
<collectingRegion id="9A431ADBFFA08D6DFB2E4BB0AD60DAE2" box="[1236,1370,199,220]" country="Costa Rica" name="Puntarenas" pageId="51">Puntarenas</collectingRegion>
, 
<collectingRegion id="9A431ADBFFA08D6DFA9E4BB1AB67DAC5" country="Costa Rica" name="San Jose" pageId="51">San José</collectingRegion>
