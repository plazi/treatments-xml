<taxonomicName id="C8ACC8C6C00D57844CD9E417B9F055DA" class="Insecta" family="Ethmiidae" genus="Ethmia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ethmia duckworthi" order="Lepidoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="duckworthi">
<pageBreakToken id="C4742571D725A81C8B44A1D8AF5DBC4D" pageId="25" pageNumber="26" start="start">Ethmia</pageBreakToken>
