<taxonomicName id="4C7C4D51FA51765AFF6BFC6516A55CFC" baseAuthorityName="Gmelin" baseAuthorityYear="1789" box="[552,745,984,1004]" class="Aves" family="Caprimulgidae" genus="Nyctidromus" kingdom="Animalia" order="Caprimulgiformes" pageId="24" pageNumber="869" phylum="Chordata" rank="species" species="albicollis">
