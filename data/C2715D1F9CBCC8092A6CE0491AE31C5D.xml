<subSubSection id="D8A15C398111AEAF704882182932EEDA" pageId="null" pageNumber="547" type="nomenclature">
<paragraph id="E9CF86F889C5F25675A96C12969D080D" pageId="null" pageNumber="547">
<pageBreakToken id="1B2E2C26147DDF2F1A52B33F197ACADF" pageId="null" pageNumber="547" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="C4FC09665BB2AABD73DB0FEC023CC3F6" authority="L." class="Magnoliopsida" family="Fabaceae" genus="Anthyllis" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="547" phylum="Tracheophyta" rank="species" species="vulneraria">
Anthyllis 
