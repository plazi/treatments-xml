<collectingCountry id="958F93C015525271FD53F8DBFCEBF831" box="[710,775,1899,1922]" name="France" pageId="13" pageNumber="334">France</collectingCountry>
(
<collectingRegion id="2F5C1DB215525271FF1EF835FF00F82E" box="[139,236,1925,1949]" country="France" name="Aquitaine" pageId="13" pageNumber="334">Aquitaine</collectingRegion>
Basin) (
<bibRefCitation id="8909AEA115525271FEAEF835FDDAF82E" author="COSSMANN M. &amp; PEYROT A." box="[315,566,1925,1949]" pageId="13" pageNumber="334" pagination="5 - 321" refId="ref29500" refString="COSSMANN M. &amp; PEYROT A. 1921. - Conchologie neogenique de l'Aquitaine. Actes de la Societe linneenne de Bordeaux 73: 5 - 321. https: // biodiversitylibrary. org / page / 31662049" type="journal article" year="1921">Cossmann &amp; Peyrot 1921</bibRefCitation>
; 
<bibRefCitation id="8909AEA115525271FDD4F835FD15F82E" author="LOZOUET P. &amp; LESPORT J. F. &amp; RENARD P." box="[577,761,1925,1949]" pageId="13" pageNumber="334" pagination="1 - 189" refId="ref33615" refString="LOZOUET P., LESPORT J. F. &amp; RENARD P. 2001. - Revision des Gastropoda (Mollusca) du Stratotype de l'Aquitanien (Miocene Inf.): site de Saucats &quot; Lariey &quot;, Gironde, France. Cossmanniana h. s.: 1 - 189." type="book chapter" year="2001">
Lozouet 
<emphasis id="DFEC0F4215525271FD01F836FD2EF82E" box="[660,706,1925,1949]" italics="true" pageId="13" pageNumber="334">et al.</emphasis>
2001
</bibRefCitation>
); Proto-Mediterranean Sea: 
<collectingCountry id="958F93C015525271FE1CF82FFE20F804" box="[393,460,1951,1975]" name="Greece" pageId="13" pageNumber="334">Greece</collectingCountry>
(
<bibRefCitation id="8909AEA115525271FE48F82FFCEDF804" author="HARZHAUSER M. &amp; KOWALKE T." box="[477,769,1951,1975]" pageId="13" pageNumber="334" pagination="353 - 374" refId="ref31322" refString="HARZHAUSER M. &amp; KOWALKE T. 2001. - Early Miocene brackishwater Mollusca from the Eastern Mediterranean and from the central Paratethys - a faunistic and ecological comparison by selected faunas. Journal of the Czech Geological Society 46 (3): 353 - 374." type="journal article" year="2001">Harzhauser &amp; Kowalke 2001</bibRefCitation>
; 
