<taxonomicName id="3FCDA98931282AD8DCB2F502F7117BB3" authority="" authorityName="" baseAuthorityName="Cassola" baseAuthorityYear="2000" class="Insecta" family="Cicindelidae" genus="Calomera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Calomera mindanaoensis" order="Coleoptera" pageId="0" pageNumber="37" phylum="Arthropoda" rank="species" species="mindanaoensis">
<emphasis id="6C6163291A30F7CFB640D58555998A45" italics="true" pageId="0" pageNumber="37">Calomera mindanaoensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="6306DBA0723951D5FD5260413F053069" genus="C." lsidName="C. lacrymosa" pageId="0" pageNumber="37" rank="species" species="lacrymosa">
