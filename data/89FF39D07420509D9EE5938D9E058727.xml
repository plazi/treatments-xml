<taxonomicName id="D08E953F25061F745C87B8B7FB05EF00" baseAuthorityName="Doktouroff" baseAuthorityYear="1882" class="Insecta" family="Cicindelidae" genus="Cylindera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cylindera discreta subsp. elaphroides" order="Coleoptera" pageId="0" pageNumber="37" phylum="Arthropoda" rank="subSpecies" species="discreta" subSpecies="elaphroides">
