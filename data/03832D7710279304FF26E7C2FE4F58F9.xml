<treatmentCitationGroup id="AB3ABB4F10279301FF67E7A4FC985940" pageId="97" pageNumber="624">
<treatmentCitation id="0A8BBA7010279301FF67E7A4FDD258DB" author="Wunderlich J" box="[164,586,1758,1780]" page="546" pageId="97" pageNumber="624" year="1995">
