<collectionCode id="ED64AE59FFC9FFF5FC197BA5FC6FF2E8" box="[904,940,152,176]" pageId="5" pageNumber="425">SL</collectionCode>
; body depth at anus 
<number id="3DBD24BBFFC9FFF5FB077BAAFACBF2E8" box="[1174,1288,151,176]" pageId="5" pageNumber="425" value="11.45" valueMax="12.4" valueMin="10.5">10.5–12.4</number>
