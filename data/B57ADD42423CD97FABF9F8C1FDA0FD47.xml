<taxonomicName id="FAD317D7423DD97FABF9FDBDFD82FDEB" authority="Chemsak &amp; Noguera, 1993" authorityName="Chemsak &amp; Noguera" authorityYear="1993" box="[151,581,583,608]" class="Insecta" family="Cerambycidae" genus="Anopliomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="194" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis id="0FA7B046423DD97FABF9FDBDFECBFDEB" box="[151,268,583,608]" italics="true" pageId="5" pageNumber="194">A. gracilis</emphasis>
