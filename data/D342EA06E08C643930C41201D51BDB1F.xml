5. 
<taxonomicName id="68557980FF688A2CA78EE8A6CC7124E1" class="Insecta" family="Hydrophilidae" genus="Berosus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Berosus metalliceps" order="Coleoptera" pageId="12" pageNumber="85" phylum="Arthropoda" rank="species" species="metalliceps">Berosus metalliceps</taxonomicName>
