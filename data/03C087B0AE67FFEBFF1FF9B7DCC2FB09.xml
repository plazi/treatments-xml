<number id="3DA12481AE66FFECFED2FE81D9F4FEDA" box="[346,392,306,332]" pageId="27" pageNumber="28" value="209.0">209</number>
x 
<number id="3DA12481AE66FFECFE2DFE81D984FEDD" box="[421,504,306,332]" pageId="27" pageNumber="28" value="2.55" valueMax="2.6" valueMin="2.5">
2.5- 
<emphasis id="B91DEAB4AE66FFECFE5CFE81D984FEDD" box="[468,504,306,331]" italics="true" pageId="27" pageNumber="28">2.6</emphasis>
</number>
