<quantity id="9B169B0DFFE3FFD557AE22E48471FEC5" box="[1256,1360,253,278]" metricMagnitude="-3" metricUnit="m" metricValue="3.69" pageId="11" pageNumber="162" unit="mm" value="3.69">3.69 mm</quantity>
