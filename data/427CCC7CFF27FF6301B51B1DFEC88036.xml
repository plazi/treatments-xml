</taxonomicName>
and 
<emphasis id="F8A1A178FF27FF7C03AF1C48FC9F852F" box="[653,844,1074,1099]" italics="true" pageId="174" pageNumber="175">
<taxonomicName id="0DD506E9FF27FF7C03AF1C48FC9A852F" baseAuthorityName="Ellis" baseAuthorityYear="1913" box="[653,841,1074,1099]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="174" pageNumber="175" phylum="Arthropoda" rank="species" species="succinipenne">L. succinipenne</taxonomicName>
