<subSubSection id="82CF2EE1FF27FF7C01B51C48FB748528" box="[151,1191,1074,1100]" pageId="174" pageNumber="175" type="diagnosis">
<paragraph id="CA6A7D6AFF27FF7C01B51C48FB748528" blockId="174.[151,1437,1074,2020]" box="[151,1191,1074,1100]" pageId="174" pageNumber="175">
<emphasis id="F8A1A178FF27FF7C01B51C48FEC48528" bold="true" box="[151,279,1074,1100]" pageId="174" pageNumber="175">Diagnosis.</emphasis>
