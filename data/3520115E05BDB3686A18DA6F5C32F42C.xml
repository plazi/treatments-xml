<taxonomicName id="41612F2E9C927104D63D2D81F4776258" authority="L." class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="411" phylum="Tracheophyta" rank="species" species="caesius">
