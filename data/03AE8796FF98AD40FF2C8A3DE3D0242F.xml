<taxonomicName id="4C074D03FF98AD42FF2C8ADEE05320F7" authority="Liu &amp; Zhang, 2001" authorityName="Liu &amp; Zhang" authorityYear="2001" box="[136,561,1439,1461]" class="Insecta" family="Haglidae" genus="Phryganogryllacris" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="515" phylum="Arthropoda" rank="species" species="xiai">
