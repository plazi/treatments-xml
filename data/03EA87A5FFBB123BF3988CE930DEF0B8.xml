<taxonomicName id="4C434D30FFBB123BF64F8F8F33C4F050" authority="Hartweg" authorityName="Hartweg" class="Amphibia" family="Hylidae" genus="Plectrohyla" kingdom="Animalia" order="Anura" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="matudai">
<emphasis id="B937EAA1FFBB123BF64F8F8F36A2F1B4" box="[1344,1436,721,746]" italics="true" pageId="18" pageNumber="19">matudai</emphasis>
