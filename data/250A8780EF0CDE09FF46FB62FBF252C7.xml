<emphasis id="9FD7EA84EF0CDE04FA33FC19FC5E54C6" italics="true" pageId="1" pageNumber="79">A. chaima</emphasis>
Stark, 1999
</taxonomicName>
, 
<taxonomicName id="6AA34D15EF0CDE04FBFFFC36FAA254C6" authority="Stark, 2001" authorityName="Stark" authorityYear="2001" box="[1093,1395,937,961]" class="Insecta" family="Perlidae" genus="Anacroneuria" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="79" phylum="Arthropoda" rank="species" species="cotacachi">
<emphasis id="9FD7EA84EF0CDE04FBFFFC36FB0254C6" box="[1093,1235,937,961]" italics="true" pageId="1" pageNumber="79">A. cotacachi</emphasis>
Stark, 2001
</taxonomicName>
, 
<taxonomicName id="6AA34D15EF0CDE04FA33FC36FB6854E3" authority="Rojas &amp; Baena, 1993" authorityName="Rojas &amp; Baena" authorityYear="1993" class="Insecta" family="Perlidae" genus="Anacroneuria" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="79" phylum="Arthropoda" rank="species" species="farallonensis">
<emphasis id="9FD7EA84EF0CDE04FA33FC36FC1B54E3" italics="true" pageId="1" pageNumber="79">A. farallonensis</emphasis>
<bibRefCitation id="C9324B67EF0CDE04FC6AFC53FB6854E3" author="Rojas, A. M. &amp; M. L. Baena" box="[976,1209,972,996]" pageId="1" pageNumber="79" refId="ref6361" refString="Rojas, A. M. &amp; M. L. Baena. 1993. Anacroneuria farallonensis (Plecoptera: Perlidae) una nueva especie para Colombia. Boletin del Museo de" type="book" year="1993">Rojas &amp; Baena, 1993</bibRefCitation>
</taxonomicName>
(see 
<bibRefCitation id="C9324B67EF0CDE04FB4BFC53FCAC530F" author="Zuniga, M. C. &amp; B. P. Stark" pageId="1" pageNumber="79" pagination="209 - 224" refId="ref6871" refString="Zuniga, M. C. &amp; B. P. Stark. 2002. New species and records of Colombian Anacroneuria (Insecta, Plecoptera, Perlidae). Spixiana, 25: 209 - 224." type="journal article" year="2002">Zúñiga &amp; Stark 2002</bibRefCitation>
), 
<taxonomicName id="6AA34D15EF0CDE04FC2BFC6FFBA4530F" authority="Stark, 1999" authorityName="Stark" authorityYear="1999" box="[913,1141,1008,1032]" class="Insecta" family="Perlidae" genus="Anacroneuria" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="79" phylum="Arthropoda" rank="species" species="perija">
<emphasis id="9FD7EA84EF0CDE04FC2BFC6FFC395300" box="[913,1000,1008,1031]" italics="true" pageId="1" pageNumber="79">A.perija</emphasis>
Stark, 1999
</taxonomicName>
, 
