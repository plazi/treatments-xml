</typeStatus>
<collectionCode id="ED3B1CD54D73FFB3487B966BBAB6FD7A" LSID="urn:lsid:biocol.org:col:34988" box="[807,897,637,663]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="8" pageNumber="43">MNHN</collectionCode>
