<subSubSection id="A0C3B9E6FF94AB427C6FFE0AFDAB83AF" box="[312,633,484,510]" pageId="9" pageNumber="10" type="reference_group">
<paragraph id="E866EA6DFF94AB427C6FFE0AFDAB83AF" blockId="9.[264,633,444,510]" box="[312,633,484,510]" pageId="9" pageNumber="10">
<taxonomicName id="2FD991EEFF94AB427C6FFE0AFDA783AF" authority="Heller, 1875" authorityName="Heller" authorityYear="1875" box="[312,629,484,510]" class="Malacostraca" family="Amathillopsidae" genus="Amathillopsis" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis id="DAAD367FFF94AB427C6FFE0AFE0B83AC" box="[312,473,484,509]" italics="true" pageId="9" pageNumber="10">Amathillopsis</emphasis>
