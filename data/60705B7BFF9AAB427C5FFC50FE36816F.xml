<paragraph id="E866EA6DFF94AB427C6FFDB2FD028097" blockId="9.[264,1323,564,710]" pageId="9" pageNumber="10">
<taxonomicName id="2FD991EEFF94AB427C6FFDB2FE288027" box="[312,506,604,630]" class="Malacostraca" family="Amathillopsidae" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="10" phylum="Arthropoda" rank="subFamily" subFamily="Amathillopsinae">Amathillopsinae</taxonomicName>
