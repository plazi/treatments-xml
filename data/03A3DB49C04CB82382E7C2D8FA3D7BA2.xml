<collectingCountry id="F31D2ACFC04CB82380F6C395FD4D7B87" box="[646,748,1970,1995]" name="Australia" pageId="9" pageNumber="265">Australia</collectingCountry>
(GN 
<number id="3DC27878C04CB823815AC395FCC57B87" box="[810,868,1970,1995]" pageId="9" pageNumber="265" value="2590.0">2590</number>
