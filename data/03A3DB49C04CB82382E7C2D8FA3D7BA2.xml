<taxonomicName id="4C0A11DCC04CB82387F2C34CFEAC7BEA" class="Elasmobranchii" family="Hemiscylliidae" genus="Chiloscyllium" kingdom="Animalia" order="Orectolobiformes" pageId="9" pageNumber="265" phylum="Chordata" rank="species" species="punctatum">
