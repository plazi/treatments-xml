<emphasis id="3C95EAF06009FFB2FF75297BFEEBD5F1" bold="true" box="[197,339,1383,1409]" pageId="12" pageNumber="13">Comments.</emphasis>
<taxonomicName id="C9E14D616009FFB2FEEB2974FE5BD5F1" box="[347,483,1384,1409]" class="Insecta" family="Evaniidae" genus="Decevania" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="destituta" status="sp. nov.">
<emphasis id="3C95EAF06009FFB2FEEB2974FE5BD5F1" box="[347,483,1384,1409]" italics="true" pageId="12" pageNumber="13">D. destituta</emphasis>
</taxonomicName>
<emphasis id="3C95EAF06009FFB2FE5B297BFDF1D5F1" bold="true" box="[491,585,1383,1409]" pageId="12" pageNumber="13">
