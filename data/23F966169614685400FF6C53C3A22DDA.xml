<taxonomicName id="0C3DAEF192838696EBF4676E16DB5C59" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:186594" class="Insecta" family="Formicidae" genus="Ectatomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ectatomma (Gnamptogenys) sulcatum (Smith)" lsidName-HNS="Ectatomma (Gnamptogenys) sulcatum (Smith)" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="species" species="sulcatum" subGenus="Gnamptogenys">E. (G.) sulcatum F. Sm.</taxonomicName>
e 
<taxonomicName id="4904B1C2C531315DC29BBBBF1122B5DE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230696" class="Insecta" family="Formicidae" genus="Ectatomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Ectatomma (Gnamptogenys) sulcatum var. lineata" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="variety" species="sulcatum" subGenus="Gnamptogenys" variety="lineata">var. lineata</taxonomicName>
