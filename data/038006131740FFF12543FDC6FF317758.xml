and 
<taxonomicName id="4C29CC861740FFF02290F93CFB1776CD" baseAuthorityName="Coopmans, Krabbe &amp; Schulenberg" baseAuthorityYear="2001" box="[1163,1275,1766,1788]" class="Aves" family="Rhinocryptidae" genus="Scytalopus" kingdom="Animalia" order="Passeriformes" pageId="38" pageNumber="415" phylum="Chordata" rank="species" species="latrans">
