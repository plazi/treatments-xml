<taxonomicName id="5BD2DF2BF13493E4BBBD6AD682B4A153" authority="Lacordaire, 1848" authorityName="Lacordaire" authorityYear="1848" class="Insecta" family="Chrysomelidae" genus="Stereoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Stereoma" order="Coleoptera" pageId="13" pageNumber="24" phylum="Arthropoda" rank="genus">Stereoma</taxonomicName>
