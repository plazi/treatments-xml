<taxonomicName id="4C2901BE04411512FEB04F44FE10FE73" authorityName="Deshayes" authorityYear="1830" box="[403,507,365,391]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="72" pageNumber="73" phylum="Mollusca" rank="genus">
