<taxonomicName id="4C2901BE04411512FB434F44FAC6FE73" authorityName="Deshayes" authorityYear="1863" box="[1120,1325,365,391]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="72" pageNumber="73" phylum="Mollusca" rank="species" species="formosa">
<emphasis id="B95DA62F04411512FB434F44FAC6FE73" box="[1120,1325,365,391]" italics="true" pageId="72" pageNumber="73">Triphoris formosa</emphasis>
</taxonomicName>
. 
