<taxonomicName id="4C584D6FFFF3FFCF73D3A1E8FD9DF936" authority="Solier" authorityName="Solier" authorityYear="1851" box="[440,637,1708,1730]" class="Insecta" family="Coleoptera (awaiting allocation)" genus="Promecheilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="34" phylum="Arthropoda" rank="genus">
