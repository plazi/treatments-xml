(
<bibRefCitation id="521C3BAFFF9EFFCDD96551D8FA48A12E" author="Kensley, B." box="[1290,1429,494,516]" pageId="12" pageNumber="389" pagination="261 - 323" refId="ref5987" refString="Kensley, B. (1976) Isopodan and tanaidacean Crustacea from the St Paul and Amsterdam Islands, southern Indian Ocean. Annals of the South African Museum, 69, 261 - 323." type="journal article" year="1976">Kensley, 1976</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="3632465EFF9EFFCDDCF8523AFA41A10B" blockId="12.[151,1437,436,780]" box="[151,1436,524,546]" pageId="12" pageNumber="389">
