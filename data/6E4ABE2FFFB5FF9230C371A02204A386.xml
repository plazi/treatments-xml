. Holotype male, 
<materialsCitation id="568B0564FFB0FF923261734C24A8A21E" ID-GBIF-Occurrence="1324639468" collectingDate="2012-05-23" collectionCode="FSCA" collectorName="Basse Terre, NE &amp; RH Turnbow" country="Guadeloupe" latitude="16.14404" location="Guadeloupe" longLatPrecision="1" longitude="-61.74977" pageId="30" pageNumber="231" specimenCount="1" typeStatus="allotype">
<typeStatus id="3958B19BFFB0FF923261734C25DDA27A" box="[565,655,1057,1082]" pageId="30" pageNumber="231">allotype</typeStatus>
female labeled “ 
<collectingCountry id="9EF44FA9FFB0FF92331B734C2489A27A" box="[847,987,1057,1082]" name="Guadeloupe" pageId="30" pageNumber="231">Guadeloupe</collectingCountry>
, 
<collectorName id="4B166AEFFFB0FF9233BC734F23F3A279" box="[1000,1185,1058,1081]" pageId="30" pageNumber="231">Basse Terre, NE</collectorName>
Pigeon, 
<geoCoordinate id="83D769FEFFB0FF923553734C22CAA279" box="[1287,1432,1057,1082]" direction="north" orientation="latitude" pageId="30" pageNumber="231" precision="1" value="16.14404">16.14404º N</geoCoordinate>
