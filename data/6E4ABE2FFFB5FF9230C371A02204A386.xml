<collectionCode id="80F297FCFFB0FF9233F3732B24A0A21E" box="[935,1010,1094,1118]" country="USA" httpUri="http://grbio.org/cool/jekt-x82m" name="Florida State Collection of Arthropods, The Museum of Entomology" pageId="30" pageNumber="231">FSCA</collectionCode>
