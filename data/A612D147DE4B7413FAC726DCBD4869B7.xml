<taxonomicName id="70BFABD4C3B2A1FEF23FEDD9E7367C6F" class="Insecta" family="Carabidae" genus="Holosoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Holosoma hedini" order="Coleoptera" pageId="1" pageNumber="88" phylum="Arthropoda" rank="species" species="hedini">Holosoma hedini</taxonomicName>
