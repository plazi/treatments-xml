<taxonomicName id="4C2EF6D3FFC6FFE3FD7049FFA03FFC78" box="[753,864,991,1017]" class="Bacillariophyceae" family="Achnanthidiaceae" genus="Psammothidium" higherTaxonomySource="GBIF" kingdom="Chromista" order="Achnanthales" pageId="10" pageNumber="11" phylum="Bacillariophyta" rank="species" species="papilio">
