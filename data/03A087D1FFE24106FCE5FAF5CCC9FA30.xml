<taxonomicName id="4C094D44FFE14101FC97F9B6CA36F9CB" authority="(Sakai, 1969)" baseAuthorityName="Sakai" baseAuthorityYear="1969" box="[825,1128,1578,1605]" class="Malacostraca" family="Brusiniidae" genus="Brusinia" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="387" phylum="Arthropoda" rank="species" species="elongata">
<emphasis id="B97DEAD5FFE14101FC97F9B6CDE4F9CB" box="[825,954,1578,1605]" italics="true" pageId="2" pageNumber="387">B. elongata</emphasis>
(
<bibRefCitation id="EF984B36FFE14101FC62F9B7CA00F9CB" author="SAKAI T." box="[972,1118,1579,1605]" pageId="2" pageNumber="387" pagination="243 - 280" refId="ref7980" refString="SAKAI T. 1969. - Two new genera and twenty new species of crabs from Japan. Proceedings of the biological Society of Washington 82: 243 - 280, figs 1 - 20, pls 1, 2." type="journal article" year="1969">Sakai, 1969</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="4C094D44FFE14101FBD9F9B6CD26F9EB" authority="Stevcic, 1991" authorityName="Stevcic" authorityYear="1991" class="Malacostraca" family="Brusiniidae" genus="Brusinia" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="387" phylum="Arthropoda" rank="species" species="brucei">
<emphasis id="B97DEAD5FFE14101FBD9F9B6CA80F9CB" box="[1143,1246,1578,1605]" italics="true" pageId="2" pageNumber="387">B. brucei</emphasis>
<bibRefCitation id="EF984B36FFE14101FD64F9D6CD26F9EB" author="STEVCIC Z." box="[714,888,1609,1637]" pageId="2" pageNumber="387" refId="ref8053" refString="STEVCIC Z. 1991. - Note on some rare and aberrant australian crabs. The Beagle. Records of the Northern" type="book" year="1991">Števčić, 1991</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C094D44FFE14101FC26F9D6CAE1F9EB" authority="Moosa, 1996" authorityName="Moosa" authorityYear="1996" box="[904,1215,1610,1637]" class="Malacostraca" family="Brusiniidae" genus="Brusinia" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="387" phylum="Arthropoda" rank="species" species="profunda">
<emphasis id="B97DEAD5FFE14101FC26F9D6CA4BF9EB" box="[904,1045,1610,1637]" italics="true" pageId="2" pageNumber="387">B. profunda</emphasis>
<bibRefCitation id="EF984B36FFE14101FBB1F9D6CAE1F9EB" author="MOOSA M. K." box="[1055,1215,1610,1637]" pageId="2" pageNumber="387" pagination="503 - 530" refId="ref7721" refString="MOOSA M. K. 1996. - Crustacea Decapoda: Deepwater swimming crabs from the South-West Pacific, particularly New Caledonia (Brachyura, Portunidae), in CROSNIER A. (ed.), Resultats des campagnes MUSORSTOM, volume 15. Memoires du Museum national d'Histoire naturelle 168: 503 - 530, figs 1 - 8." type="journal article" year="1996">Moosa, 1996</bibRefCitation>
