<taxonomicName id="C2CE4D4CFFB7FFDBFD3F3EDAFCA7F85D" authorityName="A. Milne-Edwards" authorityYear="1879" box="[709,861,1957,1983]" class="Malacostraca" family="Varunidae" genus="Pyxidognathus" kingdom="Animalia" order="Decapoda" pageId="16" pageNumber="317" phylum="Arthropoda" rank="species" species="granulosus">
