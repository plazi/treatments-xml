<date id="FFC9F1A2FFABFFABFB18FD65FAD3FD7B" box="[1212,1363,658,684]" pageId="2" pageNumber="62" value="1989-09-12">
<number id="3DBFC545FFABFFABFB18FD65FB58FD7B" box="[1212,1240,658,684]" pageId="2" pageNumber="62" value="12.0">12</number>
