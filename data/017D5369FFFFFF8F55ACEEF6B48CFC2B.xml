<collectingCountry id="F1C3A2EFFFFFFF8F51A3EC58B25BFC9E" box="[1176,1288,839,863]" name="Indonesia" pageId="26" pageNumber="27">Indonesia</collectingCountry>
, 
<collectingCountry id="F1C3A2EFFFFFFF8F502CEC58B2CBFC9E" box="[1303,1432,839,863]" name="Philippines" pageId="26" pageNumber="27">Philippines</collectingCountry>
, South 
<collectingCountry id="F1C3A2EFFFFFFF8F55E4EC75B671FC43" box="[223,290,874,898]" name="China" pageId="26" pageNumber="27">China</collectingCountry>
Sea (Nansha Islands), 
<collectingCountry id="F1C3A2EFFFFFFF8F5718EC74B535FC43" box="[547,614,875,898]" name="Japan" pageId="26" pageNumber="27">Japan</collectingCountry>
, 
<collectingCountry id="F1C3A2EFFFFFFF8F574AEC75B47CFC43" box="[625,815,874,898]" name="Marshall Islands" pageId="26" pageNumber="27">Marshall Islands</collectingCountry>
