<geoCoordinate id="EE0627DA8C1CFFC4FD10FEE257641B22" box="[695,807,306,329]" direction="east" orientation="longitude" pageId="5" pageNumber="377" precision="92" value="34.838333">34°50.3’E</geoCoordinate>
