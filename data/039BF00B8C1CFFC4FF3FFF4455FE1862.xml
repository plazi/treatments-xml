<date id="FF8C67DD8C1CFFC4FC9DFEE257ED1B22" box="[826,942,306,329]" pageId="5" pageNumber="377" value="2013-03-18">
<collectingDate id="EFC89E358C1CFFC4FC9DFEE257ED1B22" box="[826,942,306,329]" pageId="5" pageNumber="377" value="2013-03-18">18.iii.2013</collectingDate>
