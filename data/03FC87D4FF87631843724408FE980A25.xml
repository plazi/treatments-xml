<geoCoordinate id="EE615005FF846318430D43DEFD100A02" box="[571,755,234,265]" degrees="36" direction="north" minutes="39" orientation="latitude" pageId="2" pageNumber="245" precision="15" seconds="41" value="36.661392">36° 39ʹ 41ʹʹ N</geoCoordinate>
, 
<geoCoordinate id="EE615005FF846318423743DEFC560A02" box="[769,949,234,265]" degrees="51" direction="east" minutes="25" orientation="longitude" pageId="2" pageNumber="245" precision="15" seconds="31" value="51.425278">51° 25ʹ 31ʹʹ E</geoCoordinate>
, H: 
<quantity id="4CAD9B27FF84631842DB43DEFBCC0A02" box="[1005,1071,236,265]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="2" pageNumber="245" unit="m" value="10.0">10 m</quantity>
a.s.l., orange garden, soil, 
<date id="FFEB1002FF84631844BA43DEFEDC0A25" pageId="2" pageNumber="245" value="2018-08-04">4 August 2018</date>
, 
