(Edwards)
</taxonomicName>
, 
<taxonomicName id="7A9E4DE5FFB40D70CCA9FBC806D23028" authority="(Olivier)" authorityName="Reinert &amp; Harbach &amp; Kitching" authorityYear="2006" baseAuthorityName="Reinert &amp; Harbach &amp; Kitching" baseAuthorityYear="2006" box="[1152,1421,1079,1101]" class="Insecta" family="Culicidae" genus="Dahliana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="geniculata">
<emphasis id="8FEAEA74FFB40D70CCA9FBC8067A3029" box="[1152,1317,1079,1101]" italics="true" pageId="66" pageNumber="67">Da. geniculata</emphasis>
(Olivier)
</taxonomicName>
and 
<taxonomicName id="7A9E4DE5FFB40D70CB75FBA7073B30EE" authority="(Sanchez-Covisa Villa, Rodriguez Rodriguez &amp; Guillen Llera)" authorityName="Reinert &amp; Harbach &amp; Kitching" authorityYear="2006" baseAuthorityName="Sanchez-Covisa Villa, Rodriguez Rodriguez &amp; Guillen Llera" baseAuthorityYear="2006" class="Insecta" family="Culicidae" genus="Dahliana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="gilcolladoi">
