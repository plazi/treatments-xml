<typeStatus id="E55B41AFFFA1B253F43BFABDFF47A08C" box="[114,163,1393,1416]" pageId="26" pageNumber="229">TYPE</typeStatus>
GENUS
</collectionCode>
. — 
<taxonomicName id="FDE0848EFFA1B253F552FABEFDF7A080" authority="BrOndsted, 1931" authorityName="BrOndsted" authorityYear="1931" box="[283,531,1394,1418]" class="Calcarea" family="Jenkinidae" genus="Jenkina" kingdom="Animalia" order="Leucosolenida" pageId="26" pageNumber="229" phylum="Porifera" rank="genus">
