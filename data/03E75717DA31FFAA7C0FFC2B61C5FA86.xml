<emphasis id="B93A3A13DA37FFAB7C7AFB22628BFB50" bold="true" box="[381,408,1157,1181]" pageId="20" pageNumber="21">28</emphasis>
, 
<taxonomicName id="4C4E9D82DA37FFAB7CA2FB21615CFB50" box="[421,591,1158,1181]" class="Insecta" family="Ephydridae" genus="Limnellia" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="maculipennis">
