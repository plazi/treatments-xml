</taxonomicName>
species. 
<emphasis id="B93A3A13DA37FFAB7E11FBC46023FBB6" bold="true" box="[790,816,1123,1147]" pageId="20" pageNumber="21">27</emphasis>
, 
<taxonomicName id="4C4E9D82DA37FFAB7E46FBC460C0FBB7" box="[833,979,1123,1146]" class="Insecta" family="Ephydridae" genus="Limnellia" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="abbreviata">
