ɗ; 
<collectionCode id="ED5F7EC4DA36FFAA7951FD9367B8FD83" LSID="urn:lsid:biocol.org:col:34070" box="[1110,1195,564,590]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="21" pageNumber="22">NZAC</collectionCode>
