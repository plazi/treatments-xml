<taxonomicName id="22A5BDF1FF8AFFB90268FE1BFEB0FDF9" authorityName="Banks" authorityYear="1929" class="Arachnida" family="Sparassidae" genus="Sparianthina" kingdom="Animalia" order="Araneae" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
<emphasis id="D7D11A60FF8AFFB90268FE1BFEB0FDF9" italics="true" pageId="57" pageNumber="58">Sparianthina</emphasis>
</taxonomicName>
sp., (
<taxonomicName id="22A5BDF1FF8AFFB90661FDC7FE47FDFA" baseAuthorityName="Martins" baseAuthorityYear="1991" box="[348,490,547,574]" class="Arachnida" family="Sparassidae" kingdom="Animalia" order="Araneae" pageId="57" pageNumber="58" phylum="Arthropoda" rank="family">Sparassidae</taxonomicName>
