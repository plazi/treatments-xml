<taxonomicName id="0CC74D20BE715557FD3DFF18FB974B86" authority="Forsskal" authorityName="Forsskal" box="[717,1043,151,176]" class="Actinopterygii" family="Lutjanidae" genus="Lutjanus" kingdom="Animalia" order="Perciformes" pageId="23" pageNumber="174" phylum="Chordata" rank="species" species="kasmira">
