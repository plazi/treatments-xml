<taxonomicName id="1CF05EC39740FF96FEC4FBA5FD56BB11" authority="De Haan, 1842" authorityName="De Haan" authorityYear="1842" box="[294,680,1064,1086]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="podocausta">
