<taxonomicName id="4C4CCBEDBA1F0D276C34F8E401FA9CEC" box="[1096,1240,712,737]" class="Insecta" family="Stenopsychidae" genus="Stenopsyche" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
