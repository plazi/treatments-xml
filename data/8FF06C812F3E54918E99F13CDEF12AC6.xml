<taxonomicName id="17BBEF225B18544FF5685D8430CCA7A5" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" baseAuthorityName="Gleason" baseAuthorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia ombrophila" order="Myrtales" pageId="198" pageNumber="199" phylum="Tracheophyta" rank="species" species="ombrophila" status="comb. nov.">
