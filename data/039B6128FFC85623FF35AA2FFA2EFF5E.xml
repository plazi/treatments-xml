<taxonomicName id="4C32ABBDFFCF5622FC45AAA0FB3BF96C" box="[999,1152,1745,1766]" class="Insecta" family="Aleyrodidae" genus="Aleurocanthus" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="ceracroceus">
<emphasis id="B9460C2CFFCF5622FC45AAA0FB3BF96C" box="[999,1152,1745,1766]" italics="true" pageId="12" pageNumber="13">A. ceracroceus</emphasis>
