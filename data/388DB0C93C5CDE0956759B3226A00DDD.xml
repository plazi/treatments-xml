<taxonomicName id="104D935C6A688C9590E9E36C2DD2F51C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31703" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName="Monomorium salomonis (Linnaeus)" lsidName-HNS="Monomorium salomonis (Linnaeus)" order="Hymenoptera" pageNumber="194" phylum="Arthropoda" rank="species" species="salomonis">salomonis</taxonomicName>
