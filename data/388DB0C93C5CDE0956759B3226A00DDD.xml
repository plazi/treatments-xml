<taxonomicName id="3292681B2B7E8EC6B2FA22716B70C99E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31740" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName="Monomorium termitarium Forel" lsidName-HNS="Monomorium termitarium Forel" order="Hymenoptera" pageNumber="194" phylum="Arthropoda" rank="species" species="termitarium" subGenus="Xeromyrmex">termitarium</taxonomicName>
