<taxonomicName id="4C6C04EEFFD13B2BFF42BB6CFD1DF9C8" authority="Cabanis, 1873" authorityName="Cabanis" authorityYear="1873" box="[223,727,1545,1571]" class="Aves" family="Furnariidae" genus="Cinclodes" kingdom="Animalia" order="Passeriformes" pageId="8" pageNumber="37" phylum="Chordata" rank="subSpecies" species="fuscus" subSpecies="rivularis">
