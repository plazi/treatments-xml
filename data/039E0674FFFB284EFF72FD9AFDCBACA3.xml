<taxonomicName id="4C37CCE1FFFB284EFA64FADEFF5CACA3" authorityName="Ingrisch" authorityYear="2006" class="Insecta" family="Mogoplistidae" genus="Ornebius" higherTaxonomySource="CoL" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="548" phylum="Arthropoda" rank="species" species="aureus">
<emphasis id="B9436B70FFFB284EFA64FADEFA2BAC47" box="[1409,1429,1377,1403]" italics="true" pageId="4" pageNumber="548">O</emphasis>
