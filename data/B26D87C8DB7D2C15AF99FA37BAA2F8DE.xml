<taxonomicName id="FDC44D5DDB7D2C15AD75F91FBAA0F921" authorityName="Dejean" authorityYear="1821" box="[635,705,1730,1755]" class="Insecta" family="Coleoptera" genus="Laena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="388" phylum="Arthropoda" rank="genus">
