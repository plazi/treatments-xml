SPECIES. — 
<taxonomicName id="3A8C97E0FF83CF20FEBAE934FD3EF9B1" authority="Shuto, 1969" authorityName="Shuto" authorityYear="1969" box="[299,668,1611,1635]" class="Gastropoda" family="Muricidae" genus="Flexopteron" kingdom="Animalia" order="Neogastropoda" pageId="13" pageNumber="494" phylum="Mollusca" rank="species" species="philippinensis">
