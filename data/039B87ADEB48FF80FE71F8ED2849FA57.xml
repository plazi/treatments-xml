<taxonomicName id="4C324D38EB48FF80FDD6F8ED2AA3FC8B" authority="(Kolliker, 1845)" baseAuthorityName="Kolliker" baseAuthorityYear="1845" box="[521,942,693,717]" class="Hoplonemertea" family="Carcinonemertidae" genus="Carcinonemertes" kingdom="Animalia" order="Monostilifera" pageId="4" pageNumber="917" phylum="Nemertea" rank="genus">
