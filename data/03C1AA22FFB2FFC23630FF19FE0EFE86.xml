<taxonomicName id="4C6860B7FFB2FFC336AFFF19F85FFF47" authorityName="Leach" authorityYear="1815" box="[402,583,219,245]" class="Insecta" family="Scydmaenidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="299" phylum="Arthropoda" rank="family">Scydmaenidae</taxonomicName>
