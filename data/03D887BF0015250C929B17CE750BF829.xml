<subSubSection id="C36B65220015250C92CB1196730AFA6B" box="[199,1295,1409,1434]" pageId="4" pageNumber="105" type="etymology">
<paragraph id="8BCE36A90015250C92CB1196730AFA6B" blockId="4.[151,1436,1085,2011]" box="[199,1295,1409,1434]" pageId="4" pageNumber="105">
<emphasis id="B905EABB0015250C92CB11967738FA68" box="[199,317,1410,1434]" italics="true" pageId="4" pageNumber="105">Etymology</emphasis>
