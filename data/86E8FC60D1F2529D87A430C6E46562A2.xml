<paragraph id="35172B6B4E82EF847DA9E131F4D923CF" pageNumber="263">
<taxonomicName id="C304A0E2A8100F5ACF2C1A0D8C5D80B6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29224" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Formica suecica Adlerz" order="Hymenoptera" pageNumber="263" phylum="Arthropoda" rank="species" species="suecica">Formica suecica</taxonomicName>
<taxonomicNameLabel id="542E939347AC573F7ECE69A64F67F33C" rank="species">n. sp.</taxonomicNameLabel>
</paragraph>
