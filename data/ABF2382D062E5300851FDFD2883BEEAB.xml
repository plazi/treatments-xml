<subSubSection id="B7C1F64F261D9EDCCF6C6560EA626635" pageId="0" pageNumber="323" type="reference_group">
<paragraph id="8495B781B947AE0E325182C77B1D662C" pageId="0" pageNumber="323">
<taxonomicName id="2ED7B5CC0D2A03664BDCF255200EF176" authorityName="Kozlov &amp; Kononova" authorityYear="2004" class="Insecta" family="Scelionidae" genus="Gryon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gryon ariantum" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="species" species="ariantum">Gryon ariantum</taxonomicName>
