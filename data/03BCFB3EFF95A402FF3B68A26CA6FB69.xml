<subSubSection id="C30F19A3FF95A402FF6A6C906CA6FB69" box="[198,1191,1189,1215]" pageId="15" pageNumber="42" type="etymology">
<paragraph id="8BAA4A28FF95A402FF6A6C906CA6FB69" blockId="15.[151,1437,266,1215]" box="[198,1191,1189,1215]" pageId="15" pageNumber="42">
