<normalizedToken id="837E60885D528C27CDB013FF833D7DD8" originalValue="überein">ueberein</normalizedToken>
; dagegen 
<normalizedToken id="E14B4A0085147DC4D0DF5FF05F7ABD5B" originalValue="paßt">passt</normalizedToken>
<taxonomicName id="A279CBE0BD0173D10CE08E74B97A9A78" class="Arachnida" family="Euphthiracaridae" genus="Euphthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageNumber="132" phylum="Arthropoda" rank="species" species="mixtus">E. mixtus</taxonomicName>
in die 
<normalizedToken id="14DAD0EEFC7A2564A611057F26360727" originalValue="Variabilität">Variabilitaet</normalizedToken>
von 
<taxonomicName id="5C75BB4DF74C9DA9B4335ADB71221D77" class="Arachnida" family="Euphthiracaridae" genus="Euphthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageNumber="132" phylum="Arthropoda" rank="species" species="monodactylus">E. monodactylus</taxonomicName>
