<taxonomicName id="DA84CEACFFB2FFA3FCCDEF86FC6AF908" authorityName="Newman" authorityYear="1833" box="[809,912,1694,1717]" class="Insecta" family="Libellulidae" genus="Orthetrum" kingdom="Animalia" order="Odonata" pageId="56" pageNumber="287" phylum="Arthropoda" rank="genus">
