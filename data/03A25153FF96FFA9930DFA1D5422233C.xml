species of 
<taxonomicName id="4C0B9BC6FF96FFA99004F928523422E5" authorityName="Schockaert &amp; Curini-Galletti &amp; Ridder &amp; Volonterio &amp; Artois" authorityYear="2009" box="[515,602,1697,1718]" class="Rhabditophora" family="Meidiamidae" genus="Serrula" kingdom="Animalia" order="Seriata" pageId="8" pageNumber="767" phylum="Platyhelminthes" rank="genus">
