<taxonomicName id="4C0068E3190D8231FAF3FE8DFECBFE4E" baseAuthorityName="Horvath" baseAuthorityYear="1908" class="Insecta" family="Pentatomidae" genus="Chlorochroa" kingdom="Animalia" order="Hemiptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="persimilis">
<emphasis id="B974CF72190D8231FAF3FE8DFECBFE4E" italics="true" pageId="34" pageNumber="35">Chlorochroa persimilis</emphasis>
</taxonomicName>
