<taxonomicName authority="(Dollman, 1909)" baseAuthorityName="Dollman" baseAuthorityYear="1909" class="Mammalia" family="Cercopithecidae" genus="Procolobus" kingdom="Animalia" order="Primates" pageId="10" pageNumber="11" phylum="Chordata" rank="subSpecies" species="badius" subGenus="Piliocolobus" subSpecies="ellioti">
