(
<bibRefCitation author="Tesch" box="[755,887,476,500]" pageId="2" pageNumber="22" refString="Tesch, J. J. (1915) The catometopous genus Macrophthalmus as represented in the collection of the Leiden Museum. Zoologische Mededeelingen, 1 (3 - 4), 149 - 204." type="journal article" year="1915">
Tesch 
<number box="[828,887,476,500]" pageId="2" pageNumber="22" value="1915.0">1915</number>
</bibRefCitation>
; 
<bibRefCitation author="Barnes" box="[899,1044,476,500]" pageId="2" pageNumber="22" refString="Barnes, R. S. K. (1970) The species of Macrophthalmus (Crustacea: Brachyura) in the collection of the British Museum (Natural History). Bulletin of the British Museum (Natural History) Zoology, 20 (7), 206 - 250" type="journal article" year="1970">
Barnes 
<number box="[984,1044,476,500]" pageId="2" pageNumber="22" value="1970.0">1970</number>
