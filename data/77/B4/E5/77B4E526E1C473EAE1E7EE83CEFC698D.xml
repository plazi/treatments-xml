<taxonomicName class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agrilus occipitalis" order="Coleoptera" pageId="6" pageNumber="41" phylum="Arthropoda" rank="species" species="occipitalis">Agrilus occipitalis</taxonomicName>
<normalizedToken originalValue="species–group">species-group</normalizedToken>
