<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="31" pageNumber="62" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
