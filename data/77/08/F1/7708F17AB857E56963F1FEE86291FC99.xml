<quantity box="[624,694,651,677]" metricMagnitude="4" metricUnit="m" metricValue="1.6" pageId="117" pageNumber="118" unit="km" value="16.0">16 km</quantity>
S, 
<date box="[728,882,651,678]" pageId="117" pageNumber="118" value="1965-04-16">16 April 1965</date>
(
<collectionCode box="[896,948,652,678]" pageId="117" pageNumber="118">RJL</collectionCode>
). PLATTE: 
<collectingCountry box="[1081,1189,652,678]" name="Guernsey" pageId="117" pageNumber="118">Guernsey</collectingCountry>
, 
