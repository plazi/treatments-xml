<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[273,458,441,467]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="order">COLEOPTERA</taxonomicName>
