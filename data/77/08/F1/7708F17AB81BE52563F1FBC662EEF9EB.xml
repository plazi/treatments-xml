(
<collectionCode box="[794,851,1438,1464]" country="Turkmenistan" lsid="urn:lsid:biocol.org:col:12935" name="Desert Experiment Station of the W.I.R." pageId="57" pageNumber="58" type="Herbarium">REP</collectionCode>
). PLATTE: Dwyer, 
<date box="[1073,1224,1437,1464]" pageId="57" pageNumber="58" value="1959-08-20">20 Aug. 1959</date>
