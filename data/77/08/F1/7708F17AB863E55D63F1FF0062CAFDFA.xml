<quantity box="[151,243,507,534]" metricMagnitude="0" metricUnit="m" metricValue="1.981" pageId="65" pageNumber="66" unit="m" value="1.981">1,981 m</quantity>
(
<quantity box="[257,349,506,534]" metricMagnitude="0" metricUnit="m" metricValue="1.9812" pageId="65" pageNumber="66" unit="ft" value="6.5">6,500 ft</quantity>
