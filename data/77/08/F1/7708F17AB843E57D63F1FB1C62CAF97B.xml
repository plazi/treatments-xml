<quantity box="[691,804,1294,1320]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" metricValueMax="12.0" metricValueMin="6.0" pageId="97" pageNumber="98" unit="mm" value="9.0" valueMax="12.0" valueMin="6.0">6–12 mm</quantity>
