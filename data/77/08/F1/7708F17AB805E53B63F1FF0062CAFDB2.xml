<taxonomicName baseAuthorityName="Williston" baseAuthorityYear="1885" box="[672,821,543,569]" class="Insecta" family="Asilidae" genus="Efferia" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="dubia">
