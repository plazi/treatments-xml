<collectionCode box="[151,202,1549,1575]" pageId="73" pageNumber="74">RJL</collectionCode>
). PLATTE: Dwyer, 
<date box="[430,581,1548,1575]" pageId="73" pageNumber="74" value="1959-10-23">23 Oct. 1959</date>
; 
<date box="[594,732,1549,1575]" pageId="73" pageNumber="74" value="1960-04-04">4 Apr. 1960</date>
, 
<date box="[744,895,1549,1575]" pageId="73" pageNumber="74" value="1960-10-24">24 Oct. 1960</date>
