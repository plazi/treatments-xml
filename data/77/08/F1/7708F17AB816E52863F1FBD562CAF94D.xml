<date box="[783,935,1384,1410]" pageId="52" pageNumber="53" value="1931-08-18">18 Aug. 1931</date>
(
<collectionCode box="[950,1011,1384,1410]" pageId="52" pageNumber="53">LDA</collectionCode>
) (specimen in 
<collectionCode box="[1176,1235,1384,1410]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" lsid="urn:lsid:biocol.org:col:34700" name="California Academy of Sciences" pageId="52" pageNumber="53" type="Museum">CAS</collectionCode>
