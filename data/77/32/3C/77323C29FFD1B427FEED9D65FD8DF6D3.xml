<subSubSection pageId="22" pageNumber="22" type="distribution">
<paragraph blockId="22.[279,981,789,881]" box="[279,695,847,881]" pageId="22" pageNumber="22">
<emphasis box="[279,348,847,881]" italics="true" pageId="22" pageNumber="22">
<taxonomicName authorityName="Diakonoff" authorityYear="1982" box="[279,343,847,881]" class="Insecta" family="Tortricidae" genus="Age" kingdom="Animalia" order="Lepidoptera" pageId="22" pageNumber="22" phylum="Arthropoda" rank="genus">Age</taxonomicName>
