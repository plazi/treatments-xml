[[ 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:65E9F392-A294-426E-9C9E-1695549A3390" class="Actinopterygii" family="Loricariidae" genus="Pseudancistrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pseudancistrus Bleeker 1862:2" order="Siluriformes" pageNumber="2" phylum="Chordata" rank="genus">Pseudancistrus Bleeker</taxonomicName>
