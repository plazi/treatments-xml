<paragraph blockId="9.[140,1108,1433,1672]" pageId="9" pageNumber="22">
<emphasis bold="true" box="[185,312,1504,1530]" pageId="9" pageNumber="22">
<emphasis box="[185,307,1504,1530]" inLineHeading="true" pageId="9" pageNumber="22" reason="1">Diagnosis</emphasis>
.
</emphasis>
<emphasis box="[320,564,1504,1530]" italics="true" pageId="9" pageNumber="22">
