<taxonomicName authorityName="H. Edwards" authorityYear="1885" box="[140,305,1293,1320]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="23" phylum="Arthropoda" rank="species" species="andromache">
