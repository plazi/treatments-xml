, 
<quantity box="[306,406,645,670]" metricMagnitude="2" metricUnit="m" metricValue="2.5" metricValueMax="5.0" metricValueMin="0.0" pageId="8" pageNumber="122" unit="m" value="250.0" valueMax="500.0" valueMin="0.0">0–500 m</quantity>
, bottom depths 
<quantity box="[582,711,645,670]" metricMagnitude="2" metricUnit="m" metricValue="6.955" metricValueMax="7.08" metricValueMin="6.83" pageId="8" pageNumber="122" unit="m" value="695.5" valueMax="708.0" valueMin="683.0">683–708 m</quantity>
, 
