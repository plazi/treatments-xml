<collectingCountry box="[364,429,1940,1966]" name="India" pageId="2" pageNumber="28">India</collectingCountry>
: 
<collectingRegion box="[439,562,1940,1966]" country="India" name="Karnataka" pageId="2" pageNumber="28">Karnataka</collectingRegion>
, 
<collectingRegion box="[574,655,1940,1966]" country="India" name="Kerala" pageId="2" pageNumber="28">Kerala</collectingRegion>
, 
<collectingRegion box="[667,806,1940,1966]" country="India" name="Tamil Nadu" pageId="2" pageNumber="28">Tamil Nadu</collectingRegion>
(
