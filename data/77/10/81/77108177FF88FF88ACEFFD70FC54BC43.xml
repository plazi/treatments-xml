<taxonomicName box="[662,763,1002,1027]" class="Insecta" family="Gerridae" genus="Eurygerris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="beieri">
