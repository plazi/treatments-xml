<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181705" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole veletis Wilson" order="Hymenoptera" pageNumber="246" phylum="Arthropoda" rank="species" species="veletis">veletis</taxonomicName>
