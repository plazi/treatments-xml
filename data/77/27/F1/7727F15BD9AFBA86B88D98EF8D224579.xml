<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:E630A39E-7B9B-4BA8-B9BD-9FFB516F84AE" class="Actinopterygii" family="Cyprinidae" genus="Crossocheilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Crossocheilus reticulatus Fowler 1935:128" order="Cypriniformes" pageNumber="6" phylum="Chordata" rank="species" species="reticulatus">Crossocheilus reticulatus</taxonomicName>
, 
<taxonomicName class="Actinopterygii" family="Cyprinidae" genus="Henicorhynchus" higherTaxonomySource="CoL" kingdom="Animalia" order="Cypriniformes" pageNumber="6" phylum="Chordata" rank="species" species="lineatus">Henicorhynchus lineatus</taxonomicName>
, 
