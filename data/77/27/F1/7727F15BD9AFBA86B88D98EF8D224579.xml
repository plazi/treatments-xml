, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:EB97BE42-AECC-453C-8FE2-D4E553CCB37F" class="Actinopterygii" family="Cobitidae" genus="Botia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Botia modesta Bleeker 1864:11" order="Cypriniformes" pageNumber="6" phylum="Chordata" rank="species" species="modesta">B. modesta</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:F87A5E61-6542-496D-8041-7360B954D167" class="Actinopterygii" family="Cobitidae" genus="Acantopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Acantopsis van Hasselt 1823:133" order="Cypriniformes" pageNumber="6" phylum="Chordata" rank="genus">Acantopsis</taxonomicName>
spp., 
<taxonomicName class="Actinopterygii" family="Akysidae" genus="Akysis" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="6" phylum="Chordata" rank="genus">Akysis</taxonomicName>
sp., 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:C4304186-4FD4-4672-BDF1-AC51D2C01419" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Glyptothorax Blyth 1860:154" order="Siluriformes" pageNumber="6" phylum="Chordata" rank="genus">Glyptothorax</taxonomicName>
