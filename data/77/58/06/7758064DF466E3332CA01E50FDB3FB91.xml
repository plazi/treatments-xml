b; Moulet 
<number box="[407,466,767,791]" pageId="4" pageNumber="339" value="1995.0">1995</number>
; Linnavuori 
<number box="[622,682,767,791]" pageId="4" pageNumber="339" value="2004.0">2004</number>
), Kerman (Hoberlandt 
<number box="[960,1019,767,791]" pageId="4" pageNumber="339" value="1955.0">1955</number>
; Modarres Awal 
<number box="[1229,1288,767,791]" pageId="4" pageNumber="339" value="1997.0">1997</number>
b), Khorasan (Modarres Awal 
<number box="[349,408,804,828]" pageId="4" pageNumber="339" value="1997.0">1997</number>
b), Kuhgiloyeh &amp; Boyerahmad, Markazi (Modarres Awal 
<number box="[1088,1147,804,828]" pageId="4" pageNumber="339" value="1997.0">1997</number>
