<taxonomicName id="CDA04D5FAF47FFF48156FD5EFEBCFD58" authority="Cope ED, 1876" authorityName="Cope ED" authorityYear="1876" baseAuthorityName="Cope" box="[68,362,692,712]" class="Sauropsida" family="Champsosauridae" genus="Champsosaurus" higherTaxonomySource="Manual Input" kingdom="Animalia" order="Choristodera" pageId="13" pageNumber="353" phylum="Chordata" rank="species" species="vacoinsulensii" status="sp. nov.">
<emphasis id="38D4EACEAF47FFF48156FD5EFEBCFD58" bold="true" box="[68,362,692,712]" pageId="13" pageNumber="353">Champsosaurus vaccinsulensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="23E757B5AF47FFF4806AFD52FE6BFD5F" box="[376,445,697,711]" pageId="13" pageNumber="353" rank="species">
