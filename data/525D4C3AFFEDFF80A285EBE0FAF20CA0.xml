<taxonomicName id="1DF486AFFFEDFF81A610ED1AFBBC0D8A" box="[1026,1151,1645,1670]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="4" pageNumber="5" phylum="Cnidaria" rank="species" species="ovatum">
