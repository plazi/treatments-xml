<number id="6C3CEF0BFFEDFF81A3DFED1AFDCD0D8B" box="[461,526,1645,1671]" pageId="4" pageNumber="5" value="1930.0">1930</number>
)
</bibRefCitation>
, 
<taxonomicName id="1DF486AFFFEDFF81A036ED1AFD0A0D8A" box="[548,713,1645,1670]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="4" pageNumber="5" phylum="Cnidaria" rank="species" species="cymiforme">
