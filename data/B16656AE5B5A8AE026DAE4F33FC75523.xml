<taxonomicName id="A0EA27E069C2C1735F5EB5694507B6A2" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora edulis" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="edulis">Passiflora edulis</taxonomicName>
