<taxonomicName id="99594EBB8B709865E2ED61AB47A1CE5A" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora quadrangularis" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="quadrangularis">Passiflora quadrangularis</taxonomicName>
