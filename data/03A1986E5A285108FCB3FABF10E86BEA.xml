<taxonomicName id="4C0852FB5A28510AFCB3FABF1652684A" baseAuthorityName="Thomas &amp; Schwann" baseAuthorityYear="1905" box="[835,1092,1323,1344]" class="Mammalia" family="Vespertilionidae" genus="Neoromicia" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="1014" phylum="Chordata" rank="species" species="matroka">
