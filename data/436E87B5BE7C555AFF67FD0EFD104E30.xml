<taxonomicName id="0CC74D20BE7C555AFEBBFC98FDB34804" box="[331,567,793,818]" class="Actinopterygii" family="Cyprinidae" genus="Salmophasia" kingdom="Animalia" order="Cypriniformes" pageId="26" pageNumber="177" phylum="Chordata" rank="species" species="bacaila">
