<taxonomicName id="0CC74D20BE7C555AFD97FC98FD794804" box="[615,765,793,818]" class="Actinopterygii" family="Cyprinidae" genus="Chela" kingdom="Animalia" order="Cypriniformes" pageId="26" pageNumber="177" phylum="Chordata" rank="species" species="bacala">
