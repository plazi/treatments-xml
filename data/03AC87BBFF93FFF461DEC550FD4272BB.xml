<emphasis id="B971EABFFF91FFF5644DC686FACA77BD" box="[1284,1305,1970,1993]" italics="true" pageId="10" pageNumber="11">bl</emphasis>
, basal loop; 
<emphasis id="B971EABFFF91FFF561DEC6E0FF7C779F" box="[151,175,2004,2027]" italics="true" pageId="10" pageNumber="11">cc</emphasis>
, central complex; 
<emphasis id="B971EABFFF91FFF5603CC6E0FE45779F" box="[373,406,2004,2027]" italics="true" pageId="10" pageNumber="11">md</emphasis>
