<taxonomicName id="4C24F7A2FF9CFF80C16970836A82FD12" authorityName="Monniot" authorityYear="1972" box="[283,483,692,714]" class="Ascidiacea" family="Polyclinidae" genus="Aplidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enterogona" pageId="4" pageNumber="523" phylum="Chordata" rank="species" species="bermudae">
