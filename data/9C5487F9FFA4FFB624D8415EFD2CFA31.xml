<subSubSection id="5CE76564FFA4FFB724D8402EFE89FAD1" pageId="38" pageNumber="39" type="materials_examined">
<paragraph id="144236EFFFA4FFB724D8402EFE89FAD1" blockId="38.[151,1437,1293,2015]" pageId="38" pageNumber="39">
<emphasis id="2689EAFDFFA4FFB724D8402EFE25FAB8" bold="true" box="[151,441,1293,1319]" pageId="38" pageNumber="39">
<typeStatus id="CB46884DFFA4FFB724D8402EFF4AFAB8" box="[151,214,1293,1319]" pageId="38" pageNumber="39">Type</typeStatus>
and only species.
</emphasis>
<taxonomicName id="D3FD4D6CFFA4FFB7258D402DFB90FAB8" authority="A. Milne-Edwards, 1880" authorityName="A. Milne-Edwards" authorityYear="1880" box="[450,1036,1293,1319]" class="Malacostraca" family="Parthenopidae" genus="Mesorhoea" kingdom="Animalia" order="Decapoda" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="cristatipes">
<emphasis id="2689EAFDFFA4FFB7258D402DFD4DFAB8" box="[450,721,1294,1319]" italics="true" pageId="38" pageNumber="39">Mesorhoea cristatipes</emphasis>
A. Milne-Edwards, 
<number id="A23524C8FFA4FFB72783402EFB90FAB8" box="[972,1036,1293,1319]" pageId="38" pageNumber="39" value="1880.0">1880</number>
</taxonomicName>
