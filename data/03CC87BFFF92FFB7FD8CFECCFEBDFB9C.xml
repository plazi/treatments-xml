<taxonomicName id="4C654D2AFFADFFB7FB31FBDEFF48FB9C" authority="Zone" authorityName="Zone" class="Cephalopoda" family="Xenodiscidae" genus="Arasella" kingdom="Animalia" order="Ammonoidea" pageId="96" pageNumber="97" phylum="Mollusca" rank="species" species="minuta">
<emphasis id="B911EABBFFADFFB7FB31FBDEFAC0FBF0" box="[1208,1398,1050,1076]" italics="true" pageId="96" pageNumber="97">Arasella minuta</emphasis>
