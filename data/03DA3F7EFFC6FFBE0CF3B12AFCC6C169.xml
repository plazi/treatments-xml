</emphasis>
, 
<location id="8EACD8B3FFC6FFBE0D2AB512FE39C146" LSID="urn:lsid:plazi:treatment:03DA3F7EFFC6FFBE0CF3B12AFCC6C169:8EACD8B3FFC6FFBE0D2AB512FE39C146" box="[363,430,1542,1564]" country="Brazil" municipality="Estado" name="Barueri" pageId="15" pageNumber="602" stateProvince="Sao Paulo">Barueri</location>
