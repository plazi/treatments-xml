(
<bibRefCitation id="6B744B61FFECFFF950C71F642983F8F1" author="Collingwood, C. A. &amp; Tigar, B. J. &amp; Agosti, D." box="[151,426,1817,1839]" journalOrPublisher="Journal of Arid Environments" pageId="12" pageNumber="545" pagination="505 - 512" part="37" refId="ref13355" refString="Collingwood, C. A., Tigar, B. J. and Agosti, D. 1997. Introduced ants in the United Arab Emirates. Journal of Arid Environments, 37: 505 - 512." title="Introduced ants in the United Arab Emirates" type="journal article" year="1997">
