<taxonomicName id="4C424AB5FFC0FFDCDAE46D39FB648C05" authorityName="Brongniart" authorityYear="1820" box="[1153,1256,843,866]" class="Branchiopoda" family="Limnadiidae" genus="Limnadia" kingdom="Animalia" order="Diplostraca" pageId="7" pageNumber="348" phylum="Arthropoda" rank="genus">
