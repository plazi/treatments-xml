<taxonomicName id="4C357092FFDBFFC9A999FB195BD6FAB0" authorityName="Antonietto &amp; Carmo &amp; Viviers &amp; Neto &amp; Hunt" authorityYear="2016" baseAuthorityName="Rosenfeld &amp; Raab" baseAuthorityYear="1983" box="[697,911,1264,1290]" class="Ostracoda" family="Krithidae" genus="Aracajuia" kingdom="Animalia" order="Podocopida" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="antiqua">
