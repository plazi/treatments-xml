<subSubSection box="[136,1045,1742,1769]" pageId="20" pageNumber="201" type="etymology">
<paragraph blockId="20.[136,1452,1742,2057]" box="[136,1045,1742,1769]" pageId="20" pageNumber="201">
<emphasis bold="true" box="[136,302,1742,1768]" pageId="20" pageNumber="201">Etymology:––</emphasis>
