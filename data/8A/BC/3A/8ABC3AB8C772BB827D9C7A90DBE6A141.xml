<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145212" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium blochmannii Forel" lsidName-HNS="Tetramorium blochmannii Forel" order="Hymenoptera" pageNumber="3" phylum="Arthropoda" rank="species" species="blochmannii">T. blochmannii</taxonomicName>
<taxonomicNameLabel rank="species">n. sp.</taxonomicNameLabel>
