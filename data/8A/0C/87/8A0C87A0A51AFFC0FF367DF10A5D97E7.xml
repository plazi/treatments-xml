<quantity box="[1196,1357,1234,1258]" metricMagnitude="-3" metricUnit="m" metricValue="2.995" metricValueMax="3.1" metricValueMin="2.89" pageId="6" pageNumber="37" unit="mm" value="2.995" valueMax="3.1" valueMin="2.89">2.89–3.10mm</quantity>
