<quantity box="[1262,1424,1197,1222]" metricMagnitude="-3" metricUnit="m" metricValue="1.69" metricValueMax="1.73" metricValueMin="1.65" pageId="6" pageNumber="37" unit="mm" value="1.69" valueMax="1.73" valueMin="1.65">1.65–1.73mm</quantity>
), tarsus (
<quantity box="[241,339,1234,1258]" metricMagnitude="-3" metricUnit="m" metricValue="2.03" pageId="6" pageNumber="37" unit="mm" value="2.03">2.03mm</quantity>
: 
<number box="[360,407,1234,1258]" pageId="6" pageNumber="37" value="1.19">1.19</number>
/ 
<number box="[416,465,1234,1258]" pageId="6" pageNumber="37" value="0.29">0.29</number>
/ 
<number box="[474,524,1234,1258]" pageId="6" pageNumber="37" value="0.14">0.14</number>
/ 
<number box="[532,582,1234,1258]" pageId="6" pageNumber="37" value="0.12">0.12</number>
