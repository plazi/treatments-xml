<quantity box="[1140,1238,1594,1618]" metricMagnitude="-3" metricUnit="m" metricValue="3.49" pageId="4" pageNumber="35" unit="mm" value="3.49">3.49mm</quantity>
), tibia (
<quantity box="[1326,1424,1593,1618]" metricMagnitude="-3" metricUnit="m" metricValue="5.22" pageId="4" pageNumber="35" unit="mm" value="5.22">5.22mm</quantity>
