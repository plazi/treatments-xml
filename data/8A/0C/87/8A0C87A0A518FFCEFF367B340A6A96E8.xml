<quantity box="[579,677,1522,1546]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" pageId="4" pageNumber="35" unit="mm" value="0.41">0.41mm</quantity>
), femur (
<quantity box="[778,876,1522,1546]" metricMagnitude="-3" metricUnit="m" metricValue="2.18" pageId="4" pageNumber="35" unit="mm" value="2.18">2.18mm</quantity>
), tibia (
<quantity box="[960,1058,1521,1546]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="4" pageNumber="35" unit="mm" value="1.6">1.60mm</quantity>
), tarsus (
<quantity box="[1157,1255,1522,1546]" metricMagnitude="-3" metricUnit="m" metricValue="1.71" pageId="4" pageNumber="35" unit="mm" value="1.71">1.71mm</quantity>
: 
<number box="[1266,1314,1522,1546]" pageId="4" pageNumber="35" value="0.94">0.94</number>
/ 
<number box="[1323,1372,1522,1546]" pageId="4" pageNumber="35" value="0.2">0.20</number>
/ 
<number box="[1380,1428,1522,1546]" pageId="4" pageNumber="35" value="0.17">0.17</number>
/ 
<number box="[151,201,1557,1582]" pageId="4" pageNumber="35" value="0.06">0.06</number>
/ 
<quantity box="[210,306,1557,1582]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="4" pageNumber="35" unit="mm" value="0.35">0.35mm</quantity>
); mid trochanter (
<quantity box="[508,670,1557,1582]" metricMagnitude="-4" metricUnit="m" metricValue="5.949999999999999" metricValueMax="6.4" metricValueMin="5.5" pageId="4" pageNumber="35" unit="mm" value="0.595" valueMax="0.64" valueMin="0.55">0.55–0.64mm</quantity>
), femur (
<quantity box="[773,934,1557,1582]" metricMagnitude="-3" metricUnit="m" metricValue="3.07" metricValueMax="3.09" metricValueMin="3.05" pageId="4" pageNumber="35" unit="mm" value="3.07" valueMax="3.09" valueMin="3.05">3.05–3.09mm</quantity>
), tibia (
<quantity box="[1021,1183,1557,1582]" metricMagnitude="-3" metricUnit="m" metricValue="2.92" metricValueMax="2.99" metricValueMin="2.85" pageId="4" pageNumber="35" unit="mm" value="2.92" valueMax="2.99" valueMin="2.85">2.85–2.99mm</quantity>
), tarsus (
<quantity box="[1285,1432,1557,1582]" metricMagnitude="-3" metricUnit="m" metricValue="2.33" metricValueMax="2.5" metricValueMin="2.16" pageId="4" pageNumber="35" unit="mm" value="2.33" valueMax="2.5" valueMin="2.16">2.16–2.5mm</quantity>
