<number box="[1258,1306,1485,1510]" pageId="4" pageNumber="35" value="0.43">0.43</number>
/ 
<number box="[1314,1427,1485,1510]" pageId="4" pageNumber="35" value="0.6699999999999999" valueMax="0.72" valueMin="0.62">0.62–0.72</number>
/ 
<quantity box="[151,312,1522,1546]" metricMagnitude="-4" metricUnit="m" metricValue="4.449999999999999" metricValueMax="4.699999999999999" metricValueMin="4.2" pageId="4" pageNumber="35" unit="mm" value="0.44499999999999995" valueMax="0.47" valueMin="0.42">0.42–0.47mm</quantity>
