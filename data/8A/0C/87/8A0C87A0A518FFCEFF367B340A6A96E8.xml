<number box="[151,259,1593,1618]" pageId="4" pageNumber="35" value="1.23" valueMax="1.3" valueMin="1.16">1.16–1.30</number>
/ 
<number box="[268,381,1593,1618]" pageId="4" pageNumber="35" value="0.385" valueMax="0.4" valueMin="0.37">0.37–0.40</number>
/ 
<number box="[390,440,1593,1618]" pageId="4" pageNumber="35" value="0.2">0.20</number>
/ 
<number box="[448,561,1593,1618]" pageId="4" pageNumber="35" value="0.14" valueMax="0.18" valueMin="0.1">0.10–0.18</number>
