<quantity box="[389,492,1746,1772]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="7" pageNumber="8" unit="mm" value="4.0" valueMax="5.0" valueMin="3.0">3–5 mm</quantity>
