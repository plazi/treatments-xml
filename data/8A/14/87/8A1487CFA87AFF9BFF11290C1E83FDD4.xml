in (
<bibRefCitation author="Sharkey" box="[895,1131,450,476]" pageId="11" pageNumber="25" refString="Sharkey, M. J., Laurenne, N. M., Quicke, D. L. J., Sharanowski, B. &amp; Murray, D. (2006) Revision of the Agathidinae (Hymenoptera: Braconidae) with comparisons of static and dynamic alignments. Cladistics, 22, 546 - 567." type="journal article" year="2006">
Sharkey 
<emphasis box="[1000,1059,450,475]" italics="true" pageId="11" pageNumber="12">et al.</emphasis>
<number box="[1068,1131,450,476]" pageId="11" pageNumber="12" value="2006.0">2006</number>
</bibRefCitation>
). 
<taxonomicName box="[1151,1259,450,475]" class="Insecta" family="Braconidae" genus="Braunsia" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
