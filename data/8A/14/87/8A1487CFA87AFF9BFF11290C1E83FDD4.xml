<taxonomicName box="[379,491,488,513]" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
