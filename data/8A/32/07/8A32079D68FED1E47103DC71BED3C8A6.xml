<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:189108" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole o'swaldi Forel" order="Hymenoptera" pageNumber="176" phylum="Arthropoda" rank="species" species="o'swaldi">Ph. O'Swaldi</taxonomicName>
