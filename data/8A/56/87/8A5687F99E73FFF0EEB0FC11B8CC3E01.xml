(
<taxonomicName box="[464,623,1954,1977]" class="Insecta" family="Curculionidae" genus="Arammichnus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="genus">Arammichnus</taxonomicName>
) 
<taxonomicName box="[639,823,1954,1977]" pageId="5" pageNumber="50" rank="species" species="pseudosetosulus" status="sp. n.">pseudosetosulus</taxonomicName>
