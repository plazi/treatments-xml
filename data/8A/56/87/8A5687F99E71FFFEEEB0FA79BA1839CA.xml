<typeStatus box="[151,212,1527,1553]" pageId="6" pageNumber="51">Type</typeStatus>
material. 
<typeStatus box="[340,459,1527,1553]" pageId="6" pageNumber="51">Holotype</typeStatus>
:
</emphasis>
female, labeled “Sicilia (PA) Palazzo Adriano, 
<quantity box="[1021,1098,1528,1554]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="6" pageNumber="51" unit="m" value="700.0">700 m</quantity>
, 
<date box="[1109,1235,1528,1554]" pageId="6" pageNumber="51" value="2006-10-23">
<number box="[1109,1139,1528,1554]" pageId="6" pageNumber="51" value="23.0">23</number>
.X. 
<number box="[1170,1235,1528,1554]" pageId="6" pageNumber="51" value="2006.0">2006</number>
</date>
, leg. Baviera C.” (
<collectionCode box="[159,255,1565,1591]" pageId="6" pageNumber="51">CMME</collectionCode>
). 
<emphasis bold="true" box="[275,398,1565,1591]" pageId="6" pageNumber="51">
