<emphasis box="[460,661,1778,1801]" italics="true" pageId="6" pageNumber="31">Myoleja korneyevi</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[676,739,1777,1801]" pageId="6" pageNumber="31">
<taxonomicNameLabel box="[676,739,1777,1801]" pageId="6" pageNumber="31" rank="species">n. sp.</taxonomicNameLabel>
