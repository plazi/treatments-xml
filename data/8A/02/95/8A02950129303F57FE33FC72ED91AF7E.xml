<quantity box="[252,386,801,826]" metricMagnitude="-3" metricUnit="m" metricValue="6.65" metricValueMax="6.8" metricValueMin="6.5" pageId="5" pageNumber="2931" unit="mm" value="6.65" valueMax="6.8" valueMin="6.5">6.5–6.8 mm</quantity>
