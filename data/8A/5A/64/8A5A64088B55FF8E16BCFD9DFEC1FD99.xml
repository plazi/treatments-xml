<number box="[360,464,1307,1331]" pageId="2" pageNumber="293" value="640.0" valueMax="710.0" valueMin="570.0">570–710</number>
), length of body (excluding gnathosoma) 
<number box="[934,976,1307,1331]" pageId="2" pageNumber="293" value="500.0">500</number>
(
<number box="[991,1095,1307,1331]" pageId="2" pageNumber="293" value="485.0" valueMax="500.0" valueMin="470.0">470–500</number>
); width 
<number box="[1186,1228,1307,1331]" pageId="2" pageNumber="293" value="275.0">275</number>
(
<number box="[1243,1347,1307,1331]" pageId="2" pageNumber="293" value="252.0" valueMax="304.0" valueMin="200.0">200–304</number>
), length of leg I 
<number box="[239,281,1343,1367]" pageId="2" pageNumber="293" value="425.0">425</number>
(
<number box="[296,399,1343,1367]" pageId="2" pageNumber="293" value="385.5" valueMax="408.0" valueMin="363.0">363–408</number>
), leg II 
<number box="[485,527,1343,1367]" pageId="2" pageNumber="293" value="275.0">275</number>
(
<number box="[542,645,1343,1367]" pageId="2" pageNumber="293" value="245.0" valueMax="260.0" valueMin="230.0">230–260</number>
), leg III 
<number box="[741,783,1343,1367]" pageId="2" pageNumber="293" value="338.0">338</number>
(
<number box="[798,902,1343,1367]" pageId="2" pageNumber="293" value="322.5" valueMax="341.0" valueMin="304.0">304–341</number>
), leg IV 
<number box="[999,1041,1343,1367]" pageId="2" pageNumber="293" value="413.0">413</number>
(
<number box="[1055,1159,1343,1367]" pageId="2" pageNumber="293" value="387.5" valueMax="400.0" valueMin="375.0">375–400</number>
