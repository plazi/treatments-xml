<subSubSection pageId="1" pageNumber="34" type="diagnosis">
<paragraph blockId="1.[264,1323,1092,1358]" pageId="1" pageNumber="34">
<emphasis bold="true" box="[312,442,1172,1198]" pageId="1" pageNumber="34">Diagnosis:</emphasis>
