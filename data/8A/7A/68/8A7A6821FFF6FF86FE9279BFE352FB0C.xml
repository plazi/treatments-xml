and 
<taxonomicName baseAuthorityName="Herklots" baseAuthorityYear="1851" box="[916,1102,178,199]" class="Malacostraca" family="Palaemonidae" genus="Macrobrachium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="18" pageNumber="2111" phylum="Arthropoda" rank="species" species="macrobrachion">
<emphasis box="[916,1102,178,199]" italics="true" pageId="18" pageNumber="2111">M. macrobrachion</emphasis>
