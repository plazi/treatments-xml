<collectingCountry box="[511,609,1658,1683]" name="Ethiopia" pageId="7" pageNumber="242">Ethiopia</collectingCountry>
, 
<collectingCountry box="[622,699,1658,1683]" name="Gabon" pageId="7" pageNumber="242">Gabon</collectingCountry>
, 
<collectingCountry box="[711,787,1658,1683]" name="Ghana" pageId="7" pageNumber="242">Ghana</collectingCountry>
, 
<collectingCountry box="[799,935,1659,1683]" name="Ivory Coast" pageId="7" pageNumber="242">Ivory Coast</collectingCountry>
, 
<collectingCountry box="[946,1022,1659,1683]" name="Kenya" pageId="7" pageNumber="242">Kenya</collectingCountry>
, 
<collectingCountry box="[1034,1121,1658,1683]" name="Nigeria" pageId="7" pageNumber="242">Nigeria</collectingCountry>
, 
<collectingCountry box="[1133,1281,1658,1683]" name="South Africa" pageId="7" pageNumber="242">South Africa</collectingCountry>
, 
<collectingCountry name="Sao Tome and Principe" pageId="7" pageNumber="242">São Tomé &amp; Principe</collectingCountry>
, 
<collectingCountry box="[258,362,1694,1719]" name="Tanzania" pageId="7" pageNumber="242">Tanzania</collectingCountry>
