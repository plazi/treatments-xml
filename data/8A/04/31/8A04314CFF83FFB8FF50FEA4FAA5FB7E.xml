<taxonomicName authority="(Swartz 1788: 69) Candolle (1828: 188)" authorityName="Candolle" authorityPageNumber="188" authorityYear="1828" baseAuthorityName="Swartz" baseAuthorityPageNumber="69" baseAuthorityYear="1788" box="[185,835,374,401]" class="Magnoliopsida" family="Melastomataceae" genus="Miconia" kingdom="Plantae" order="Myrtales" pageId="18" pageNumber="79" phylum="Tracheophyta" rank="species" species="prasina">
