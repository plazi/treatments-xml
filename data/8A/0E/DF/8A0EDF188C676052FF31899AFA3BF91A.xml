<geoCoordinate box="[121,202,1908,1929]" degrees="18" direction="north" minutes="20" orientation="latitude" pageId="49" pageNumber="182" precision="925" value="18.333334">18°20'N</geoCoordinate>
<geoCoordinate box="[210,301,1908,1929]" degrees="64" direction="west" minutes="55" orientation="longitude" pageId="49" pageNumber="182" precision="925" value="-64.916664">64°55'W</geoCoordinate>
