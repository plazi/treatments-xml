<specimenCount box="[451,513,1798,1821]" pageId="52" pageNumber="185" type="generic">
<quantity box="[451,509,1798,1821]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="52" pageNumber="185" unit="m" value="10.0">10 m</quantity>
.
</specimenCount>
</paragraph>
