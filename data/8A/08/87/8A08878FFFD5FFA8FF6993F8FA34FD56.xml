<taxonomicName baseAuthorityName="Guha &amp; Chaudhuri" baseAuthorityYear="1983" box="[1326,1437,601,623]" class="Insecta" family="Chironomidae" genus="Stictochironomus" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="180" phylum="Arthropoda" rank="species" species="obscurus">
