<taxonomicName authorityName="Tokunaga" authorityYear="1938" box="[1275,1436,367,388]" class="Insecta" family="Chironomidae" genus="Stictochironomus" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="180" phylum="Arthropoda" rank="species" species="multannulatus">
