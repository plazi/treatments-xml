<subSubSection box="[151,444,510,537]" pageId="1" pageNumber="568" type="nomenclature">
<paragraph blockId="1.[151,444,510,537]" box="[151,444,510,537]" pageId="1" pageNumber="568">
<heading bold="true" box="[151,444,510,537]" fontSize="11" level="1" pageId="1" pageNumber="568" reason="1">
<emphasis bold="true" box="[151,444,510,537]" pageId="1" pageNumber="568">
<taxonomicName authority="Weaver &amp; Gibon &amp; Chvojka &amp; Oláh, 2020" authorityName="Weaver &amp; Gibon &amp; Chvojka &amp; Oláh" authorityYear="2020" box="[151,299,510,537]" class="Insecta" family="Dipseudopsidae" genus="Ollieopteryx" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="568" phylum="Arthropoda" rank="genus" status="new genus">
