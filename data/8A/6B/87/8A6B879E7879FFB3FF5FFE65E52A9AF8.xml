<subSubSection box="[151,959,582,609]" pageId="1" pageNumber="568" type="type_taxon">
<paragraph blockId="1.[151,1437,582,2013]" box="[151,959,582,609]" pageId="1" pageNumber="568">
<emphasis bold="true" box="[151,308,582,609]" pageId="1" pageNumber="568">
<typeStatus box="[151,210,583,609]" pageId="1" pageNumber="568">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Weaver &amp; Gibon &amp; Chvojka &amp; Oláh, 2020" authorityName="Weaver &amp; Gibon &amp; Chvojka &amp; Oláh" authorityYear="2020" box="[316,573,583,609]" class="Insecta" family="Dipseudopsidae" genus="Ollieopteryx" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="568" phylum="Arthropoda" rank="species" species="fianarana" status="new species">
<emphasis box="[316,573,583,609]" italics="true" pageId="1" pageNumber="568">Ollieopteryx fianarana</emphasis>
