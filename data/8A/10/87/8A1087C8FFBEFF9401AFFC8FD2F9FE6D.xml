<paragraph blockId="16.[151,1436,151,392]" pageId="16" pageNumber="17">
<emphasis box="[199,315,188,212]" inLineHeading="true" pageId="16" pageNumber="17" reason="1">
<emphasis bold="true" box="[199,308,188,212]" pageId="16" pageNumber="17">Remarks</emphasis>
.
</emphasis>
<taxonomicName authorityName="Stoddart &amp; Lowry" authorityYear="2012" box="[326,604,188,212]" class="Malacostraca" family="Acidostomatidae" genus="Acidostoma" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="namibiensis">
