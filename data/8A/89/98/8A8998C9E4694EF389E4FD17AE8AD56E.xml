<taxonomicName baseAuthorityName="LeConte" baseAuthorityYear="1847" class="Hexapoda" family="Carabidae" genus="Anisodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="637" pageNumber="638" phylum="Arthropoda" rank="species" species="piceus">
