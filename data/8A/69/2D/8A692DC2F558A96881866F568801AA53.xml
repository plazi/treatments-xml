<subSubSection pageId="0" pageNumber="147" type="nomenclature">
<paragraph pageId="0" pageNumber="147">
<taxonomicName LSID="http://zoobank.org/5F9869BC-25A3-4633-B445-6C6B76453DD8" class="Insecta" family="Brassicaceae" genus="Parlatoria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parlatoria menglaensis" order="Brassicales" pageId="0" pageNumber="147" phylum="Arthropoda" rank="species" species="menglaensis">Parlatoria menglaensis</taxonomicName>
