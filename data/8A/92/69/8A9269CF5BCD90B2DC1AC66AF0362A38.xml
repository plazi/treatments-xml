<taxonomicName class="Insecta" family="Chrysomelidae" genus="Callispa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Callispa" order="Coleoptera" pageId="219" pageNumber="220" phylum="Arthropoda" rank="genus">Callispa</taxonomicName>
