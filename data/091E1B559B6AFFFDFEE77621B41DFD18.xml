<taxonomicName id="46B7D1C09B6AFFFDFDF676BFB507FE2B" authority="Terayama" authorityName="Terayama" box="[537,892,385,409]" class="Insecta" family="Bethylidae" genus="Formosiepyris" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="marishi">
