<number id="A23524C8FF88FF9B208B4796FB53FD50" box="[1220,1231,693,719]" pageId="10" pageNumber="11" value="1.0">1</number>
/0, propodus 
<number id="A23524C8FF88FF9B21214796FAE1FD50" box="[1390,1405,693,719]" pageId="10" pageNumber="11" value="2.0">2</number>
