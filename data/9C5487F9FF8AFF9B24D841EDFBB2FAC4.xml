<number id="A23524C8FF88FF9B266D4796FDACFD50" box="[546,560,693,719]" pageId="10" pageNumber="11" value="3.0">3</number>
, carpus 0/0, propodus, 
<number id="A23524C8FF88FF9B270C4796FCD2FD50" box="[835,846,693,719]" pageId="10" pageNumber="11" value="1.0">1</number>
