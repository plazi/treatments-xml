<number id="A23524C8FF88FF9B27C34796FC00FD50" box="[908,924,693,719]" pageId="10" pageNumber="11" value="4.0">4</number>
: merus 
<number id="A23524C8FF88FF9B27B74796FB9AFD50" box="[1016,1030,693,719]" pageId="10" pageNumber="11" value="5.0">5</number>
/(
<number id="A23524C8FF88FF9B20564796FBBBFD50" box="[1049,1063,693,719]" pageId="10" pageNumber="11" value="5.0">5</number>
+ 
<number id="A23524C8FF88FF9B20084796FBCBFD50" box="[1095,1111,693,719]" pageId="10" pageNumber="11" value="3.0">3</number>
