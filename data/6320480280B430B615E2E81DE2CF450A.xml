, 
<taxonomicName id="D45148E232E71156D2E9FBF804832BC4" baseAuthorityName="Broderip" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="107" pageNumber="108" phylum="Mollusca" rank="species" species="cactivorus" subGenus="Mesembrinus">
