<collectingCountry id="F30696E5B171FFFBFBD5FECEA2FBD21C" box="[1047,1196,297,323]" name="Sierra Leone" pageId="25" pageNumber="426">Sierra Leone</collectingCountry>
, 
<collectingCountry id="F30696E5B171FFFBFB7BFECEA352D21C" box="[1209,1285,297,323]" name="Ghana" pageId="25" pageNumber="426">Ghana</collectingCountry>
