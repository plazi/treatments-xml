<taxonomicName id="3CEE4D6BFFCF8C42B89BFAFB2374FAE3" box="[1245,1381,1301,1325]" class="Reptilia" family="Gekkonidae" genus="Microgecko" kingdom="Animalia" order="Squamata" pageId="18" pageNumber="319" phylum="Chordata" rank="genus">Microgecko</taxonomicName>
