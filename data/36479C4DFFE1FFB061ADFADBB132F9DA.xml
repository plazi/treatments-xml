<number id="08263F7CFFE1FFB0630BFA48B603FA62" box="[561,589,1444,1468]" pageId="1" pageNumber="56" value="95.0">95</number>
specimens
</specimenCount>
plus 
<specimenCount id="A8E8E6D2FFE1FFB0623CFA48B728FA62" box="[774,870,1444,1468]" pageId="1" pageNumber="56" type="exuvia">
<number id="08263F7CFFE1FFB0623CFA48B75AFA62" box="[774,788,1444,1468]" pageId="1" pageNumber="56" value="9.0">9</number>
