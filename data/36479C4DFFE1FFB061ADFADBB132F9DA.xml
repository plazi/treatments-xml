<number id="08263F7CFFE1FFB063E1FA93B6A7FA49" box="[731,745,1407,1431]" pageId="1" pageNumber="56" value="3.0">3</number>
contained 
<specimenCount id="A8E8E6D2FFE1FFB0625CFA93B7B1FA49" box="[870,1023,1407,1431]" pageId="1" pageNumber="56" type="generic">
<number id="08263F7CFFE1FFB0625CFA93B7CCFA49" box="[870,898,1407,1431]" pageId="1" pageNumber="56" value="85.0">85</number>
specimens
</specimenCount>
plus 
<specimenCount id="A8E8E6D2FFE1FFB06504FA93B0E0FA49" box="[1086,1198,1407,1431]" pageId="1" pageNumber="56" type="exuvia">
<number id="08263F7CFFE1FFB06504FA93B014FA49" box="[1086,1114,1407,1431]" pageId="1" pageNumber="56" value="25.0">25</number>
exuvia
</specimenCount>
containing eggs. Six moss samples from Site 
<number id="08263F7CFFE1FFB06093FA48B5F9FA62" box="[425,439,1444,1468]" pageId="1" pageNumber="56" value="5.0">5</number>
contained 
