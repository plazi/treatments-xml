<taxonomicName id="4C5A02B73B31FF9CFBD2B10CFB6CFE7B" box="[1100,1226,370,395]" class="Insecta" family="Limoniidae" genus="Praearchitipula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="80" phylum="Arthropoda" rank="species" species="notabilis">
<emphasis id="B92EA5263B31FF9CFBD2B10CFB6CFE7B" box="[1100,1226,370,395]" italics="true" pageId="3" pageNumber="80">P. notabilis</emphasis>
