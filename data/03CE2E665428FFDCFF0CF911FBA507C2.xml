<emphasis id="B91343625428FFDBFF0CF8E9FEF4006B" bold="true" box="[151,328,1861,1886]" pageId="19" pageNumber="462">
<typeStatus id="54DC21D25428FFDBFF0CF8E9FF6E006B" box="[151,210,1861,1886]" pageId="19" pageNumber="462">Type</typeStatus>
material.
</emphasis>
Dunas de Bilbao, 
<collectingRegion id="49A351925428FFDBFDBFF8E9FD30006B" box="[548,652,1861,1886]" country="Mexico" name="Coahuila de Zaragoza" pageId="19" pageNumber="462">Coahuila</collectingRegion>
, 
<collectingRegion id="49A351925428FFDBFD01F8E9FD4F006B" box="[666,755,1861,1886]" country="Mexico" name="Mexico" pageId="19" pageNumber="462">
<collectingCountry id="F370DFE05428FFDBFD01F8E9FD4F006B" box="[666,755,1861,1886]" name="Mexico" pageId="19" pageNumber="462">México</collectingCountry>
</collectingRegion>
