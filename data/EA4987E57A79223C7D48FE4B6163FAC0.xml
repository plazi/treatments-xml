<taxonomicName id="A5E04D707A79223C7DFEFCAC62BEFCE2" authorityName="Massena &amp; Souance" authorityYear="1854" box="[201,399,786,810]" class="Aves" family="Psittacidae" genus="Conurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psittaciformes" pageId="13" pageNumber="476" phylum="Chordata" rank="species" species="callipterus">Conurus callipterus</taxonomicName>
