<taxonomicName id="4C567488FF9273313353FAC4E13FFA4C" authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[318,568,1406,1429]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="97" pageNumber="98" phylum="Arthropoda" rank="species" species="ignotum" status="n. sp.">
<emphasis id="B922D319FF9273313353FAC4E13FFA4C" box="[318,568,1406,1429]" italics="true" pageId="97" pageNumber="98">Zoosphaerium ignotum</emphasis>
</taxonomicName>
<emphasis id="B922D319FF927331302CFAC7E178FA4C" bold="true" box="[577,639,1405,1429]" pageId="97" pageNumber="98">
<taxonomicNameLabel id="A2116E62FF927331302CFAC7E178FA4C" box="[577,639,1405,1429]" pageId="97" pageNumber="98" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, A, B, D–G 
<typeStatus id="54EDB1A9FF9273313162FAC4E068FA4C" box="[783,879,1406,1429]" pageId="97" pageNumber="98">holotype</typeStatus>
, C female 
<typeStatus id="54EDB1A9FF9273313185FAC4E743FA4C" box="[1000,1092,1406,1429]" pageId="97" pageNumber="98">paratype</typeStatus>
<emphasis id="B922D319FF9273313621FAC7E758FA4C" bold="true" box="[1100,1119,1405,1429]" pageId="97" pageNumber="98">A</emphasis>
