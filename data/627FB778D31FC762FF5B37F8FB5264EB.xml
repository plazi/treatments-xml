<taxonomicName id="2DD67DEDD31FC762FEFD37D7FDE96460" box="[305,543,1512,1536]" class="Insecta" family="Mythicomyiidae" genus="Eoacridophagus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="583" phylum="Arthropoda" rank="species" species="azari" status="sp. nov.">
