<taxonomicName id="2DD67DEDD31FC762FCF5344CFC3864EB" box="[825,974,1651,1675]" class="Insecta" family="Bombyliidae" genus="Acridophagus" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="583" phylum="Arthropoda" rank="genus">
<emphasis id="D8A2DA7CD31FC762FCF5344CFC3864EB" box="[825,974,1651,1675]" italics="true" pageId="0" pageNumber="583">Acridophagus</emphasis>
