<taxonomicName id="EF81DFE59041FF89FEA0F834FD61F84F" authority="Millard, 1977" authorityName="Millard" authorityYear="1977" box="[333,704,1967,1988]" class="Hydrozoa" family="Lafoeidae" genus="Zygophylax" kingdom="Animalia" order="Leptothecata" pageId="3" pageNumber="339" phylum="Cnidaria" rank="species" species="crozetensis">
<emphasis id="1AF578749041FF89FEA0F834FD8FF84F" box="[333,558,1967,1988]" italics="true" pageId="3" pageNumber="339">Zygophylax crozetensis</emphasis>
