<taxonomicName id="FDE0848EFF8CB27EF557FA5EFD63A0A1" authority="Doderlein, 1892" authorityName="Doderlein" authorityYear="1892" box="[286,647,1426,1451]" class="Calcarea" family="Minchinellidae" genus="Petrostroma" kingdom="Animalia" order="Lithonida" pageId="55" pageNumber="258" phylum="Porifera" rank="species" species="schulzei">
