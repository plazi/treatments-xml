<paragraph id="08130741FFCBFF8B6EF942A7FE34FD31" blockId="8.[189,1125,740,767]" box="[189,1125,740,767]" pageId="8" pageNumber="9">
<taxonomicName id="CFAC7CC2FFCBFF8B6EF942A7FBBAFD31" authority="Wichard &amp; Espeland &amp; Müller &amp; Wang, 2020" authorityName="Wichard &amp; Espeland &amp; Müller &amp; Wang" authorityYear="2020" box="[189,491,740,766]" class="Insecta" family="Odontoceridae" genus="Palaeopsilotreta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="cretacea" status="sp. nov.">
<emphasis id="3AD8DB53FFCBFF8B6EF942A7FBBAFD31" box="[189,491,740,766]" italics="true" pageId="8" pageNumber="9">Palaeopsilotreta cretacea</emphasis>
</taxonomicName>
