<emphasis id="10DDEAB34543FFECFD31FCC7FCFAD519" box="[765,885,833,858]" italics="true" pageId="2" pageNumber="31">Sylvilagus</emphasis>
</taxonomicName>
sp., 
<date id="561710614543FFECFC62FCC7FBDBD518" box="[942,1108,833,859]" pageId="2" pageNumber="31" value="1945-10-21" valueMax="1945-10-25" valueMin="1945-10-21">
<number id="946124864543FFECFC62FCC7FC7AD518" box="[942,1013,833,859]" pageId="2" pageNumber="31" value="23.0" valueMax="25.0" valueMin="21.0">21­25</number>
