</taxonomicName>
and 
<taxonomicName id="2F73C6FB49569DF4A459976B71340C3B" genus="T." lsidName="T. favus" pageId="11" pageNumber="112" rank="species" species="favus">
<emphasis id="67624EC9FC3275FC67E20197AD169773" italics="true" pageId="11" pageNumber="112">T. favus</emphasis>
</taxonomicName>
and 0.32 % between 
<taxonomicName id="6A6EDB67E7FFF2A1214B9D11A609E154" genus="T." lsidName="T. michaeli" pageId="11" pageNumber="112" rank="species" species="michaeli">
<emphasis id="D7B0DE2E0151D7DE05C62FF9098D1651" italics="true" pageId="11" pageNumber="112">T. michaeli</emphasis>
