<taxonomicName id="C17150CF0DEE611D63A017C91DC9DE65" class="Insecta" family="Staphylinidae" genus="Euplectus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectus elongatus" order="Coleoptera" pageId="4" pageNumber="35" phylum="Arthropoda" rank="species" species="elongatus">Euplectus elongatus</taxonomicName>
