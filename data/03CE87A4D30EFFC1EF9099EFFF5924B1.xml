<taxonomicName id="4C674D31D30EFFC1EE0C98E2FED026DD" box="[267,381,1477,1502]" class="Mammalia" family="Cricetidae" genus="Sigmodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="499" phylum="Chordata" rank="species" species="toltecus">
