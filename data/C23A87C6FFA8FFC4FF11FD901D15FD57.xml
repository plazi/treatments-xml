<date id="3E2D1010FFA8FFC3FACDFD3D1CD5FD15" pageId="27" pageNumber="28" value="1974-04-26">
<number id="FC5B24F7FFA8FFC3FACDFD3D1961FD39" box="[1355,1383,685,710]" pageId="27" pageNumber="28" value="26.0">26</number>
