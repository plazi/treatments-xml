</taxonomicName>
coral base, 
<quantity id="8D6B9B35FFA8FFC3FAA7FD421973FD16" box="[1313,1397,722,746]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="27" pageNumber="28" unit="m" value="9.0" valueMax="10.0" valueMin="8.0">8-10 m</quantity>
; K. Ruetzler col. 
<date id="3E2D1010FFA8FFC3FEABFD661DC2FCF1" box="[301,452,757,782]" pageId="27" pageNumber="28" value="1975-05-11">
