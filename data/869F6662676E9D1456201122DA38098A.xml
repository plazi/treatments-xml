<paragraph id="ECB71A47FF582E506902A8656E06DB88" pageId="null" pageNumber="506">
<taxonomicName id="49CC5415F5929665718C1931D6338974" class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="506" phylum="Tracheophyta" rank="genus">
