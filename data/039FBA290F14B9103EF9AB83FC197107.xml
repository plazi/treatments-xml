further differs from 
<taxonomicName id="4C3670BC0F11B9103ABDA9A3FA2876FB" authorityName="Baba &amp; Tirmizi" authorityYear="1979" box="[1235,1435,909,932]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="393" phylum="Arthropoda" rank="species" species="remotispinatus">
<emphasis id="B942D72D0F11B9103ABDA9A3FA2876FB" box="[1235,1435,909,932]" italics="true" pageId="4" pageNumber="393">U. remotispinatus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3670BC0F11B9103EA4A99FFED77697" authorityName="Baba" authorityYear="1988" box="[202,356,944,968]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="393" phylum="Arthropoda" rank="species" species="vandamae">
