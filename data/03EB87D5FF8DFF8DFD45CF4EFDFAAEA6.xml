<paragraph id="8BFD36C3FF8DFF8DFEFDCF68FC79A86F" blockId="8.[313,1268,827,903]" box="[313,971,827,850]" pageId="8" pageNumber="1166">
<taxonomicName id="4C424D40FF8DFF8DFEFDCF68FD54A86C" authority="Kraus, 1960: 253" authorityName="Kraus" authorityPageNumber="253" authorityYear="1960" box="[313,742,827,850]" class="Diplopoda" family="Pyrgodesmidae" genus="Muyudesmus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="1166" phylum="Arthropoda" rank="species" species="obliteratus">
