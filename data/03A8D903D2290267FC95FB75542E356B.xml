<paragraph id="8BBE6815D2290267FC95FAD852E236A0" blockId="38.[826,1469,1323,1470]" pageId="38" pageNumber="2820">
<taxonomicName id="4C011396D2290267FC95FAD8529B365D" box="[828,921,1324,1347]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="38" pageNumber="2820" phylum="Firmicutes" rank="species" species="sicerae">
