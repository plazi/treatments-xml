(
<location id="8EDBEEAFBB25C8427FB7FBEF281DCA4C" LSID="urn:lsid:plazi:treatment:03AD0962BB25C8437DC8FDB52ADECE99:8EDBEEAFBB25C8427FB7FBEF281DCA4C" box="[755,879,1047,1073]" country="United States of America" county="Grant Co." municipality="Sangre de Cristo Mts" name="Davis Mts" pageId="8" pageNumber="233" stateProvince="New Mexico">Davis Mts</location>
