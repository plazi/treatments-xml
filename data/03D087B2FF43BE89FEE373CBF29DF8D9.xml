<taxonomicName id="4C794D27FF43BE89FF017387F6FAFA1F" authority="Rudman, 1971" authorityName="Rudman" authorityYear="1971" box="[242,467,1510,1533]" class="Gastropoda" family="Retusidae" genus="Relichna" kingdom="Animalia" order="Cephalaspidea" pageId="167" pageNumber="754" phylum="Mollusca" rank="genus">
