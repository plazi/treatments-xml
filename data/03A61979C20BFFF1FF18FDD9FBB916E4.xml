<specimenCount id="9D0963E6C20BFFF1FC43FBF6FBB916E4" box="[972,1053,1720,1746]" pageId="40" pageNumber="41" type="generic">
<quantity id="4CF7058AC20BFFF1FC43FBF6FBBD16E4" box="[972,1049,1720,1746]" metricMagnitude="2" metricUnit="m" metricValue="8.45" pageId="40" pageNumber="41" unit="m" value="845.0">845 m</quantity>
.
</specimenCount>
</paragraph>
