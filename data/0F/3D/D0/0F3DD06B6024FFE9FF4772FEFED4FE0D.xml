<taxonomicName box="[960,1106,1263,1286]" class="Insecta" family="Xiphydriidae" genus="Konowia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="582" phylum="Arthropoda" rank="species" species="yasumatsui">
