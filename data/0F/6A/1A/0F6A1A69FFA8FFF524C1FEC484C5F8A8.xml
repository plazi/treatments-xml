<number box="[1217,1327,1730,1753]" pageId="1" pageNumber="96" value="1.0899999999999999" valueMax="1.19" valueMin="0.99">0.99–1.19</number>
; abdomen length 
<number box="[224,333,1765,1788]" pageId="1" pageNumber="96" value="3.3600000000000003" valueMax="3.41" valueMin="3.31">3.31–3.41</number>
, width 
<number box="[411,520,1765,1788]" pageId="1" pageNumber="96" value="2.6050000000000004" valueMax="2.99" valueMin="2.22">2.22–2.99</number>
, height 
<number box="[604,715,1765,1788]" pageId="1" pageNumber="96" value="2.79" valueMax="2.93" valueMin="2.65">2.65–2.93</number>
; sternum length 
<number box="[891,1000,1765,1788]" pageId="1" pageNumber="96" value="1.06" valueMax="1.08" valueMin="1.04">1.04–1.08</number>
