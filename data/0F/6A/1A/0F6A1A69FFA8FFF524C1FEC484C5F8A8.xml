, width 
<number box="[1119,1228,1661,1684]" pageId="1" pageNumber="96" value="2.12" valueMax="2.26" valueMin="1.98">1.98–2.26</number>
, height 
<number box="[1321,1431,1661,1684]" pageId="1" pageNumber="96" value="0.8049999999999999" valueMax="1.01" valueMin="0.6">0.60–1.01</number>
; abdomen length 
<number box="[344,454,1696,1719]" pageId="1" pageNumber="96" value="2.77" valueMax="3.08" valueMin="2.46">2.46–3.08</number>
, width 
<number box="[547,656,1696,1719]" pageId="1" pageNumber="96" value="2.02" valueMax="2.16" valueMin="1.88">1.88–2.16</number>
, height 
<number box="[756,866,1696,1719]" pageId="1" pageNumber="96" value="2.035" valueMax="2.15" valueMin="1.92">1.92–2.15</number>
