<taxonomicName authorityName="Behounek, Han &amp; Kononenko" authorityYear="2012" box="[546,655,693,716]" class="Insecta" family="Noctuidae" genus="Xizanga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="88" phylum="Arthropoda" rank="species" species="mysterica">
