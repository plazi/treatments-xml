<emphasis bold="true" box="[706,795,655,681]" pageId="7" pageNumber="530">
<taxonomicNameLabel box="[706,795,655,681]" pageId="7" pageNumber="530" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName authority="Ma &amp; Jing &amp; Zheng, 2021" authorityName="Ma &amp; Jing &amp; Zheng" authorityYear="2021" box="[850,1128,655,681]" class="Insecta" family="Gryllidae" genus="Progoniogryllus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="530" phylum="Arthropoda" rank="species" species="directus" status="sp. nov.">
