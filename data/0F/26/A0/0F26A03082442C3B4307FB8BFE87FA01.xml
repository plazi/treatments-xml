<taxonomicName authorityName="Ahrens" authorityYear="2005" class="Insecta" family="Melolonthidae" genus="Anomalophylla" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="tsangpoana">
