<collectingCountry box="[266,343,732,755]" name="Yemen" pageId="7" pageNumber="238">Yemen</collectingCountry>
, 
<collectingCountry box="[354,511,732,755]" name="Socotra Island" pageId="7" pageNumber="238">Socotra Island</collectingCountry>
.
