<emphasis box="[638,718,1132,1151]" italics="true" pageId="4" pageNumber="235">Z. novaki</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[723,792,1132,1151]" pageId="4" pageNumber="235" rank="species">sp. nov.</taxonomicNameLabel>
, male, holotype; 2 – 
<taxonomicName authority="Purchart, 2014" authorityName="Purchart" authorityYear="2014" box="[970,1050,1132,1151]" class="Insecta" family="Tenebrionidae" genus="Zophosis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="235" phylum="Arthropoda" rank="species" species="novaki" status="sp. nov.">
<emphasis box="[970,1050,1132,1151]" italics="true" pageId="4" pageNumber="235">Z. novaki</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1055,1124,1132,1151]" pageId="4" pageNumber="235" rank="species">sp. nov.</taxonomicNameLabel>
, female, paratype; 3 – 
<taxonomicName authority="Kaszab, 1972" authorityName=", Kaszab" authorityYear="1972" box="[329,545,1159,1178]" class="Insecta" family="Tenebrionidae" genus="Zophosis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="235" phylum="Arthropoda" rank="species" species="leonardii">
<emphasis box="[329,427,1159,1178]" italics="true" pageId="4" pageNumber="235">Z. leonardii</emphasis>
Kaszab, 1972
</taxonomicName>
, male, paratype; 4 – 
<taxonomicName authorityName=", Kaszab" authorityYear="1972" box="[714,811,1159,1178]" class="Insecta" family="Tenebrionidae" genus="Zophosis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="235" phylum="Arthropoda" rank="species" species="leonardii">
<emphasis box="[714,811,1159,1178]" italics="true" pageId="4" pageNumber="235">Z. leonardii</emphasis>
</taxonomicName>
, female, paratype; 5 – 
