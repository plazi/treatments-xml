<taxonomicName authority="Wainstein" authorityName="Wainstein" box="[557,1172,1319,1344]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="17" pageNumber="518" phylum="Arthropoda" rank="species" species="kazachstanicus" subGenus="Anthoseius">
