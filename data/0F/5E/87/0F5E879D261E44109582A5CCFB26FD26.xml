<number box="[574,616,1698,1723]" pageId="32" pageNumber="533" value="326.0">326</number>
(
<number box="[632,733,1698,1723]" pageId="32" pageNumber="533" value="317.5" valueMax="335.0" valueMin="300.0">300–335</number>
) long and 
<number box="[856,898,1699,1723]" pageId="32" pageNumber="533" value="148.0">148</number>
