<taxonomicName authority="Sharp, 1878" authorityName="Sharp" authorityYear="1878" class="Insecta" family="Erotylidae" genus="Cryptodacne" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptodacne" order="Coleoptera" pageId="166" pageNumber="167" phylum="Arthropoda" rank="genus">Cryptodacne</taxonomicName>
