<paragraph pageId="0" pageNumber="81">Notes.</paragraph>
<paragraph pageId="0" pageNumber="81">
<taxonomicName class="Magnoliopsida" family="Ranunculaceae" genus="Delphinium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Delphinium orientale" order="Ranunculales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="species" species="orientale">
