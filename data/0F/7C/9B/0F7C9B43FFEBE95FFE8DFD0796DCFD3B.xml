<taxonomicName authorityName="Candeze" authorityYear="1863" box="[689,764,1724,1749]" class="Insecta" family="Elateridae" genus="Silesis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="genus">
<emphasis box="[689,764,1724,1749]" italics="true" pageId="49" pageNumber="50">Silesis</emphasis>
</taxonomicName>
to 
