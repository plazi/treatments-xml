<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181871" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole euryscopa Wilson" lsidName-HNS="Pheidole euryscopa Wilson" order="Hymenoptera" pageNumber="367" phylum="Arthropoda" rank="species" species="euryscopa">euryscopa</taxonomicName>
