(
<taxonomicName box="[982,1130,151,175]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="24" pageNumber="175" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
