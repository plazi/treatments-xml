<subSubSection pageId="15" pageNumber="166" type="distribution">
<paragraph blockId="15.[151,1436,151,356]" box="[199,1118,151,176]" pageId="15" pageNumber="166">
<emphasis bold="true" box="[199,421,151,176]" pageId="15" pageNumber="166">Distribution. Asia:</emphasis>
<collectingCountry box="[428,529,151,175]" name="Armenia" pageId="15" pageNumber="166">Armenia</collectingCountry>
