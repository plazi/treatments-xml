<paragraph id="44312F06D9981E4F6B49293DFDA5CF94" pageId="18" pageNumber="205">
<taxonomicName id="D5268F5517E67A6B9483A71AC7DC6FF4" authorityName="Talamas &amp; Thompson &amp; Cutler &amp; Schoenberger &amp; Cuminale &amp; Jung &amp; Johnson &amp; Valerio &amp; Smith &amp; Haltermann &amp; Alvarez &amp; Schwantes &amp; Blewer &amp; Bodenreider &amp; Salzberg &amp; Luo &amp; Meislin &amp; Buffington" authorityYear="2017" baseAuthorityName="Talamas &amp; Thompson &amp; Cutler &amp; Schoenberger &amp; Cuminale &amp; Jung &amp; Johnson &amp; Valerio &amp; Smith &amp; Haltermann &amp; Alvarez &amp; Schwantes &amp; Blewer &amp; Bodenreider &amp; Salzberg &amp; Luo &amp; Meislin &amp; Buffington" baseAuthorityYear="2017" class="Insecta" family="Platygastridae" genus="Oxyteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oxyteleia" order="Hymenoptera" pageId="18" pageNumber="205" phylum="Arthropoda" rank="genus">
<emphasis id="0C6AC844171FE098DE5FE2E2D1CCCECD" italics="true" pageId="18" pageNumber="205">Oxyteleia</emphasis>
</taxonomicName>
