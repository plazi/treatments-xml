<taxonomicName id="4C7A4D1FB7ACFF1964F17ABADE83F839" authority="Thaxt. Mature" authorityName="Thaxt. Mature" box="[421,755,1928,1954]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Stigmatomyces" kingdom="Fungi" order="Laboulbeniales" pageId="253" pageNumber="254" phylum="Ascomycota" rank="species" species="constrictus">
