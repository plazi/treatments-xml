<emphasis id="B90EEA8EB7ACFF19674C7A2CDE5DF8A3" bold="true" box="[536,557,1822,1848]" pageId="253" pageNumber="254">A</emphasis>
</taxonomicName>
<emphasis id="B90EEA8EB7ACFF1967797A2DDE21F8A3" bold="true" box="[557,593,1822,1849]" pageId="253" pageNumber="254">–D</emphasis>
. 
<taxonomicName id="4C7A4D1FB7ACFF1967357A2DD8D8F8A3" authority="(Balazuc) A. Weir &amp; W. Rossi. A" authorityName="A. Weir &amp; W. Rossi. A" baseAuthorityName="Balazuc" box="[609,1192,1822,1849]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Fanniomyces" higherTaxonomySource="GBIF" kingdom="Fungi" order="Laboulbeniales" pageId="253" pageNumber="254" phylum="Ascomycota" rank="species" species="burdigalensis">
<emphasis id="B90EEA8EB7ACFF1967357A2DDF50F8A3" box="[609,800,1822,1849]" italics="true" pageId="253" pageNumber="254">S. burdigalensis</emphasis>
