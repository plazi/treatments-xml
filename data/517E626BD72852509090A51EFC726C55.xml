<taxonomicName id="1ED7A8FED72852509090A51EFC1A6F74" authority="Womersley, 1940" authorityName="Womersley" authorityYear="1940" box="[491,965,277,301]" class="Arachnida" family="Tetranychidae" genus="Schizonobia" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="385" phylum="Arthropoda" rank="species" species="sycophanta">
