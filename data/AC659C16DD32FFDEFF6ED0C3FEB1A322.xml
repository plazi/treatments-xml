<emphasis id="16B8F112DD32FFDEFF6ED0C3FC68A7EB" bold="true" box="[151,899,688,715]" pageId="5" pageNumber="179">
<taxonomicName id="E3CC5683DD32FFDEFF6ED0C3FD8CA7EB" authority="Xue &amp; Zhang" authorityName="Xue &amp; Zhang" box="[151,615,688,715]" class="Insecta" family="Cicadellidae" genus="Jogocerus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="179" phylum="Arthropoda" rank="species" species="viraktamathi" status="sp. nov.">
<emphasis id="16B8F112DD32FFDEFF6ED0C3FE5FA7EA" bold="true" box="[151,436,688,714]" italics="true" pageId="5" pageNumber="179">Jogocerus viraktamathi</emphasis>
Xue &amp; Zhang
</taxonomicName>
