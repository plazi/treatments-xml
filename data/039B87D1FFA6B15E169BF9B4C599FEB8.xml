<taxonomicName id="4C324D44FFA6B158169BF921C577F95D" authority="Bryant, 1948: 339" authorityName="Bryant" authorityPageNumber="339" authorityYear="1948" box="[151,558,1762,1784]" class="Arachnida" family="Segestriidae" genus="Ariadna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="multispinosa">
