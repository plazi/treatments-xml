<taxonomicName id="80EF4D02FFC9AB573CA4D7EBDC2CDB87" authorityName="Koch" authorityYear="1937" box="[151,299,1157,1182]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="84" phylum="Arthropoda" rank="species" species="bernhaueri">
