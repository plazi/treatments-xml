<quantity id="9D324E93C22FE41C3303F984FD61F998" box="[584,687,1629,1656]" metricMagnitude="-5" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="306" unit="mm" value="0.01">0.01 mm</quantity>
