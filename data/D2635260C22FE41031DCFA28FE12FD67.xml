<quantity id="9D324E93C22FE41C3201F984FC7FF998" box="[842,945,1629,1656]" metricMagnitude="-5" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="306" unit="mm" value="0.01">0.01 mm</quantity>
