<quantity id="125F35E6FF97C341FDA53E17B193D022" box="[578,648,1299,1326]" metricMagnitude="4" metricUnit="m" metricValue="2.1" pageId="43" pageNumber="44" unit="km" value="21.0">21 km</quantity>
