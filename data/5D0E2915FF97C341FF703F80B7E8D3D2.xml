<quantity id="125F35E6FF97C341FCA43E17B094D021" box="[835,911,1300,1326]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="43" pageNumber="44" unit="m" value="1100.0">1100m</quantity>
