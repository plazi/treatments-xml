<taxonomicName id="12A7E380FF97C341FC6D3D7FB7D6D399" baseAuthorityName="Tinkham" baseAuthorityYear="1936" box="[906,1229,1660,1686]" class="Insecta" family="Tettigoniidae" genus="Chandozhinskia" kingdom="Animalia" order="Orthoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="hastaticerca">
