<taxonomicName id="6EEE74318D9E9E3C9AA7DF2A9F05D087" authorityName="Pujade-Villar &amp; Paretas-Martinez" authorityYear="2011" class="Hexapoda" family="Figitidae" genus="Palmiriella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="10" pageNumber="31" phylum="Arthropoda" rank="genus">
