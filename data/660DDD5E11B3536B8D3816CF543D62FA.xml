<taxonomicName id="6281DEB30D3E27F1433C329B3AD2F3C5" authorityName="Paretas-Martinez" authorityYear="2011" class="Hexapoda" family="Figitidae" genus="Cicatrix" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="10" pageNumber="31" phylum="Arthropoda" rank="genus">
<emphasis id="AD24BC411D2957B9969028E38337F42C" italics="true" pageId="10" pageNumber="31">Cicatrix</emphasis>
