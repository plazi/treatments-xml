, 
<collectingCountry id="F30476205518FFB2FB29FC970039A20E" box="[1223,1292,821,845]" name="Brazil" pageId="5" pageNumber="354">Brazil</collectingCountry>
, 
<collectingCountry id="F30476205518FFB2FAC8FC9400BEA20D" box="[1318,1419,822,846]" name="Paraguay" pageId="5" pageNumber="354">Paraguay</collectingCountry>
