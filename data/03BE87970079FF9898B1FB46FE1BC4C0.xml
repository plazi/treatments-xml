<taxonomicName id="4C174D02007FFF989A6CFD3CFD00C4E3" authorityName="Blahnik &amp; Holzenthal" authorityYear="2011" box="[586,698,692,716]" class="Magnoliopsida" family="Apocynaceae" genus="Mortoniella" kingdom="Plantae" order="Gentianales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="pusilla">
