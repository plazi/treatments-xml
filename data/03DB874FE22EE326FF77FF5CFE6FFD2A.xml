<taxonomicName id="4C724DDAE22EE326FDA1FDD3FD11FDFF" box="[577,703,536,560]" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="254" phylum="Arthropoda" rank="species" species="luctuosa">
<emphasis id="B906EA4BE22EE326FDA1FDD3FD11FDFF" box="[577,703,536,560]" italics="true" pageId="8" pageNumber="254">A. luctuosa</emphasis>
</taxonomicName>
