<taxonomicName id="4C724DDAE22EE326FC61FDD3FF76FD9B" authority="Kerremans, 1913 (Bily &amp; Kuban, 2010)" authorityName="Kerremans, 1913 (Bily &amp; Kuban" authorityYear="2010" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="254" phylum="Arthropoda" rank="species" species="albovillosa">
