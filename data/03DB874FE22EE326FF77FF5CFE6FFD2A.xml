<taxonomicName id="4C724DDAE22EE326FF77FDD3FD98FDFF" authority="Obenberger, 1922" authorityName="Obenberger" authorityYear="1922" box="[151,566,535,560]" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="254" phylum="Arthropoda" rank="species" species="luctuosa">
<emphasis id="B906EA4BE22EE326FF77FDD3FECCFDFF" box="[151,354,536,560]" italics="true" pageId="8" pageNumber="254">Anthaxia luctuosa</emphasis>
