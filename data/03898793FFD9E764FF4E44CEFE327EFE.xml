<geoCoordinate id="EE145042FFC6E765FD0A44D6FCB878A4" box="[723,832,415,439]" direction="east" orientation="longitude" pageId="16" pageNumber="17" precision="925" value="167.98334">
<number id="3DE824A2FFC6E765FD0A44D6FCF878A4" box="[723,768,415,439]" pageId="16" pageNumber="17" value="167.0">167</number>
° 
<number id="3DE824A2FFC6E765FCD344D6FCD178A4" box="[778,809,415,439]" pageId="16" pageNumber="17" value="59.0">59</number>
'E
</geoCoordinate>
., coll. J.E. Feehan, 
<date id="FF9E1045FFC6E765FBC344D6FB7E78A4" box="[1050,1158,415,439]" pageId="16" pageNumber="17" value="1984-04-03">
