<collectionCode id="ED31AE40FFC6E765FC704432FC167880" LSID="urn:lsid:biocol.org:col:32981" box="[937,1006,379,403]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="16" pageNumber="17">ANIC</collectionCode>
