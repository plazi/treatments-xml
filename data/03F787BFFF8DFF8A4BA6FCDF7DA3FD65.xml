<taxonomicName id="4C5E4D2AFF8FFF954828FCDC7C52FC9F" box="[331,532,789,808]" class="Insecta" family="Carabidae" genus="Mecyclothorax" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="693" phylum="Arthropoda" rank="species" species="undetermined">
