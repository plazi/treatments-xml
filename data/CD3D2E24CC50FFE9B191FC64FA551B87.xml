<subSubSection id="0D8ECCB9CC50FFE9B1C1FAF5FAB11B30" pageId="31" pageNumber="232" type="description">
<paragraph id="452B9F32CC50FFE9B1C1FAF5FE0F19DF" blockId="31.[151,1437,1277,2044]" pageId="31" pageNumber="232">
<emphasis id="77E04320CC50FFE9B1C1FAF5FE731925" bold="true" box="[199,437,1347,1374]" pageId="31" pageNumber="232">Measurements. AzL</emphasis>
