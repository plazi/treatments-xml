<taxonomicName id="20E5306BA622D518BFE36807DAE7F44D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32933" class="Insecta" family="Formicidae" genus="Ocymyrmex" kingdom="Animalia" lsidName="Ocymyrmex barbiger Emery" lsidName-HNS="Ocymyrmex barbiger Emery" order="Hymenoptera" pageNumber="364" phylum="Arthropoda" rank="species" species="barbiger">Ocymyrmex barbiger</taxonomicName>
