<paragraph id="1C760703FFAFFF9AFFABF4D182A7F3CA" pageId="2" pageNumber="354">
<table id="6EC9F5A3FFAF0067FFABF4DE83B8F3CA" box="[124,1446,247,670]" gridcols="3" gridrows="8" pageId="2" pageNumber="354">
<tr id="A2F90541FFAF0067FFABF4DE83B8F059" box="[124,1446,247,269]" gridrow="0" pageId="2" pageNumber="354">
<th id="E1286C3DFFAF0067FFABF4DE876DF059" box="[124,371,247,269]" gridcol="0" gridrow="0" pageId="2" pageNumber="354">
<emphasis id="2EBDDB11FFAFFF9AFFABF4D186FBF059" bold="true" box="[124,229,248,269]" pageId="2" pageNumber="354">Character</emphasis>
</th>
<th id="E1286C3DFFAF0067FE18F4DE85ABF059" box="[463,949,247,269]" gridcol="1" gridrow="0" pageId="2" pageNumber="354">
<taxonomicName id="DBC97C80FFAFFF9AFE18F4DE85ABF059" authority="(Tumpeesuwan et al., 2007)" baseAuthorityName="Tumpeesuwan" baseAuthorityYear="2007" box="[463,949,247,269]" class="Gastropoda" family="Dyakiidae" genus="Phuphania" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="2" pageNumber="354" phylum="Mollusca" rank="species" species="globosa">
