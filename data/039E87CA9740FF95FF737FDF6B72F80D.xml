<taxonomicName id="4C374D5F9743FF92FEFD7ACA6982F235" authority="Li" authorityName="Li" authorityYear="2017" box="[262,472,2030,2052]" class="Arachnida" family="Dictynidae" genus="Brommella" kingdom="Animalia" order="Araneae" pageId="43" pageNumber="168" phylum="Arthropoda" rank="species" species="spirula" status="sp. nov.">
