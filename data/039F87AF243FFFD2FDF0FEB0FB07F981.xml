<collectingCountry id="F3217629243FFFD2FE3BFB4CFE0FFAF5" box="[396,495,1269,1295]" name="Vietnam" pageId="72" pageNumber="73">Vietnam</collectingCountry>
(
<collectingRegion id="49F2F85B243FFFD2FDB5FB4CFD75FAF5" box="[514,661,1269,1295]" country="Vietnam" name="Binh Thuan" pageId="72" pageNumber="73">Binh Thuan</collectingRegion>
