, 
<taxonomicName id="8FF5DDF2A9DC05A236EE324961553036" authorityName="Druce" authorityYear="1917" baseAuthorityName="Lour." class="Magnoliopsida" family="Flacourtiaceae" genus="Homalium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Homalium cochinchinense" order="Malpighiales" pageId="3" pageNumber="100" phylum="Tracheophyta" rank="species" species="cochinchinense">
