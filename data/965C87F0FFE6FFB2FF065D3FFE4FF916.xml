<collectionCode id="78E4AE23FFE6FFB2FD0F5D87FD6FFA46" box="[670,728,1488,1511]" pageId="1" pageNumber="67">ISSN</collectionCode>
<number id="A83D24C1FFE6FFB2FD4F5D87FCE1FA46" box="[734,854,1488,1511]" pageId="1" pageNumber="67" value="2748.5" valueMax="3682.0" valueMin="1815.0">1815-3682</number>
