<taxonomicName id="89B64D7CFF85FFEE11E04F0BD5BB24DB" box="[151,463,1901,1926]" class="Insecta" family="Miridae" genus="Glaucopterum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="36" phylum="Arthropoda" rank="species" species="alborubrum" status="sp.nov.">
<emphasis id="7CC2EAEDFF85FFEE11E04F0BD5BB24DB" bold="true" box="[151,463,1901,1926]" italics="true" pageId="9" pageNumber="36">Glaucopterum alborubrum</emphasis>
</taxonomicName>
<taxonomicNameLabel id="67F15796FF85FFEE10A14F0BD65C24D8" box="[470,552,1901,1925]" pageId="9" pageNumber="36" rank="species">sp.nov.</taxonomicNameLabel>
