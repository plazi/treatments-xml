<paragraph id="959992C1972E2FE9DB31259B672222D9" pageNumber="144">
<pageStartToken id="EBC05E929E265F6189F0DCE4C8D4C0BF" pageNumber="144">
<pageStartToken id="56131CA64BC2865D2012682CC8D33CE7" pageNumber="144">59</pageStartToken>
</pageStartToken>
. 
<taxonomicName id="8EF17F88514E00E63206DF62517A1DD1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29176" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica polyctena Förster" order="Hymenoptera" pageNumber="144" phylum="Arthropoda" rank="species" species="polyctena">Formica polyctena Forster</taxonomicName>
