<quantity box="[455,601,1612,1637]" metricMagnitude="-3" metricUnit="m" metricValue="3.95" metricValueMax="4.3" metricValueMin="3.6" pageId="14" pageNumber="15" unit="mm" value="3.95" valueMax="4.3" valueMin="3.6">3.6–4.3 mm</quantity>
