<taxonomicName authority="Eschscholtz, 1829" authorityName="Eschscholtz" authorityYear="1829" class="Insecta" family="Tenebrionidae" genus="Praocis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Praocis concinna" order="Coleoptera" pageId="8" pageNumber="61" phylum="Arthropoda" rank="species" species="concinna">Praocis concinna</taxonomicName>
