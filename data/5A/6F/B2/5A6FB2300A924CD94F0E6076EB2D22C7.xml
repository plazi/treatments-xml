<taxonomicName authorityName="van Noort &amp; Buffington" authorityYear="2013" class="Hexapoda" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="behara">
