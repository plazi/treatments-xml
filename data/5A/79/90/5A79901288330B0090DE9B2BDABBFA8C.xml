<collectingCountry box="[1333,1384,1941,1965]" name="Italy" pageId="1" pageNumber="441">Italy</collectingCountry>
and 
<collectingCountry box="[151,230,1977,2001]" name="Greece" pageId="1" pageNumber="441">Greece</collectingCountry>
(
