<taxonomicName class="Insecta" family="Lycidae" genus="Lygistopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lygistopterus" order="Coleoptera" pageId="156" pageNumber="157" phylum="Arthropoda" rank="genus">Lygistopterus</taxonomicName>
