<taxonomicName baseAuthorityName="Martynov" baseAuthorityYear="1930" box="[482,595,841,863]" class="Insecta" family="Rhyacophilidae" genus="Rhyacophila" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="7" phylum="Arthropoda" rank="species" species="tibetana">
<emphasis bold="true" box="[482,595,841,863]" italics="true" pageId="4" pageNumber="7">H. tibetana</emphasis>
</taxonomicName>
