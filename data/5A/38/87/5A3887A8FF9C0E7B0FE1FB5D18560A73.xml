<taxonomicName authorityName="Sars" authorityYear="1866" box="[960,1075,1587,1612]" class="Ostracoda" family="Pontocyprididae" genus="Pontocypris" kingdom="Animalia" order="Podocopida" pageId="13" pageNumber="388" phylum="Arthropoda" rank="genus">
