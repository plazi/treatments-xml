<collectingRegion box="[669,770,1654,1677]" country="Hungary" name="Veszprem" pageId="16" pageNumber="391">Veszprem</collectingRegion>
, Bakony, 
<collectingCountry box="[228,322,1680,1704]" name="Hungary" pageId="16" pageNumber="391">Hungary</collectingCountry>
(Bunza &amp; Kozur 1971); 
<taxonomicName authorityName="Kozur &amp; Mostler" authorityYear="1979" class="Polycystinea" family="Hagiastridae" genus="Tetraporobrachia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Spumellaria" pageId="16" pageNumber="391" phylum="Retaria" rank="species" species="haeckeli">
<emphasis italics="true" pageId="16" pageNumber="391">Tetraporobrachia haeckeli</emphasis>
</taxonomicName>
