<collectingCountry box="[134,223,1281,1305]" name="Hungary" pageId="21" pageNumber="396">Hungary</collectingCountry>
(Kristan-Tollmann 
<emphasis box="[425,474,1280,1304]" italics="true" pageId="21" pageNumber="396">et al.</emphasis>
1991b); 
<taxonomicName authority="Torres" authorityName="Torres" box="[569,667,1280,1304]" class="Liliopsida" family="Poaceae" genus="Nicoraella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="21" pageNumber="396" phylum="Tracheophyta" rank="genus">
