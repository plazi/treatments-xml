<subSubSection pageId="58" pageNumber="59" type="reference_group">
<paragraph pageId="58" pageNumber="59">
<taxonomicName authority="Chevalier, 1909" authorityName="Chevalier" authorityYear="1909" class="Magnoliopsida" family="Lamiaceae" genus="Leocus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Leocus lyratus" order="Lamiales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="species" species="lyratus">Leocus lyratus</taxonomicName>
