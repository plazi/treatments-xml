<typeStatus box="[647,759,1454,1480]" pageId="25" pageNumber="26">paratypes</typeStatus>
(
<typeStatus box="[775,880,1453,1479]" pageId="25" pageNumber="26">holotype</typeStatus>
and rest of 
<typeStatus box="[1018,1130,1454,1480]" pageId="25" pageNumber="26">paratypes</typeStatus>
