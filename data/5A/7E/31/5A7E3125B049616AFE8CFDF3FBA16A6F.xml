accumulated between rocks, 
<geoCoordinate box="[343,490,604,630]" direction="north" orientation="latitude" pageId="8" pageNumber="9" precision="15" value="46.000557">
<number box="[343,376,604,630]" pageId="8" pageNumber="9" value="46.0">46</number>
°00’02”N
</geoCoordinate>
, 
<geoCoordinate box="[503,660,604,630]" direction="east" orientation="longitude" pageId="8" pageNumber="9" precision="15" value="116.07361">
<number box="[503,550,604,630]" pageId="8" pageNumber="9" value="116.0">116</number>
°04’ 
<number box="[597,628,604,630]" pageId="8" pageNumber="9" value="25.0">25</number>
”E
</geoCoordinate>
, 
<date box="[673,835,604,630]" pageId="8" pageNumber="9" value="2003-05-31">
<number box="[673,702,604,630]" pageId="8" pageNumber="9" value="31.0">31</number>
