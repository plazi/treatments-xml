<taxonomicName id="4C4575F6A8785B4EFCCBB57A05D9FCB9" box="[847,952,832,858]" class="Insecta" family="Mutillidae" genus="Smicromyrmilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="species" species="margalla" status="sp. nov.">Margalla</taxonomicName>
