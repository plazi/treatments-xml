<collectingRegion id="49F42791091AFFA317CDFF55FBD7FF03" box="[918,1034,223,249]" country="Brazil" name="Sao Paulo" pageId="59" pageNumber="60">São Paulo</collectingRegion>
, 
<collectingRegion id="49F42791091AFFA31048FF55FBBFFF03" box="[1043,1122,223,249]" country="Brazil" name="Parana" pageId="59" pageNumber="60">Paraná</collectingRegion>
