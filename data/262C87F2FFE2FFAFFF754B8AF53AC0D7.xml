(
<bibRefCitation id="CA144B15FFE3FFAFFF1948AFF19DC2E2" author="Larsen" box="[251,397,224,248]" pageId="3" pageNumber="392" refString="Larsen, K. (2000) Revision of the genus Collettea (Crustacea: Tanaidacea). Invertebrate Taxonomy, 14, 681 - 693. http: // dx. doi. org / 10.1071 / IT 98015" type="journal article" year="2000">Larsen 2000</bibRefCitation>
).
</materialsCitation>
</paragraph>
<paragraph id="AE3A36E4FFE3FFAFFF25494BF53AC0D7" blockId="3.[151,1437,151,717]" pageId="3" pageNumber="392">
<materialsCitation id="1EED3CB9FFE3FFAFFF25494BF2A0C35A" ID-GBIF-Occurrence="1427552985" country="Antarctica" location="southern Pacific" pageId="3" pageNumber="392" specimenCount="1">
<emphasis id="9CF1EAF6FFE3FFAFFF25494BF163C307" bold="true" box="[199,371,260,285]" pageId="3" pageNumber="392">Other species.</emphasis>
