<taxonomicName id="1AD00917E20D573E418FE0CBB5FF5F59" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180225" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster senegalensis Roger" order="Hymenoptera" pageNumber="193" phylum="Arthropoda" rank="species" species="senegalensis">CREMASTOGASTER SENEGALENSIS</taxonomicName>
