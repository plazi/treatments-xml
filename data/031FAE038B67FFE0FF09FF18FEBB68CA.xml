, 
<collectingCountry id="F3A15F858B67FFE0FF09FE7DFF71685E" box="[151,255,508,534]" name="Tanzania" pageId="19" pageNumber="20">Tanzania</collectingCountry>
, 
<collectingCountry id="F3A15F858B67FFE0FE94FE7DFEC6685E" box="[266,328,508,534]" name="Democratic Republic of the Congo" pageId="19" pageNumber="20">Zaire</collectingCountry>
