<taxonomicName id="C7F14D37FFDA8A3CFBCEFABCFA2CDC27" authorityName="Hill" authorityYear="1942" box="[1026,1449,1309,1348]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="21" pageNumber="300" phylum="Chordata" rank="species" species="ruwenzorii">
