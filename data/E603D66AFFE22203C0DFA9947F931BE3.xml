<geoCoordinate id="0B9E01BBFFE02203C404ACAC7E861FFB" box="[1100,1161,408,431]" degrees="5" direction="south" minutes="11" orientation="latitude" pageId="2" pageNumber="67" precision="925" value="-5.1833334">S5 11</geoCoordinate>
