<geoCoordinate id="0B9E01BBFFE02203C398ACAC7E031FFB" box="[976,1036,408,431]" degrees="4" direction="south" minutes="11" orientation="latitude" pageId="2" pageNumber="67" precision="925" value="-4.1833334">S4 11</geoCoordinate>
