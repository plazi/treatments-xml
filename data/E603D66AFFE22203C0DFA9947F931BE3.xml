<geoCoordinate id="0B9E01BBFFE02203C37FACAC797B1FFB" box="[823,884,408,431]" degrees="2" direction="south" minutes="10" orientation="latitude" pageId="2" pageNumber="67" precision="925" value="-2.1666667">S2 10</geoCoordinate>
