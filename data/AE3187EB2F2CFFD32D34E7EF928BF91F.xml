<taxonomicName id="E1984D7E2F2CFFD32DDCE7EC9453FF06" authority="CUVIER &amp; VALENCIENNES, 1828" authorityName="Cuvier &amp; Valenciennes" authorityYear="1828" box="[933,1385,193,217]" class="Actinopterygii" family="Latidae" genus="Lates" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="85" phylum="Chordata" rank="genus">
