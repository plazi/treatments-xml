(
<collectionCode id="ED74AE37FFAA4375FD4AC2BF8B08F864" LSID="urn:lsid:biocol.org:col:1013" box="[735,817,2012,2036]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:1013" name="Essig Museum of Entomology" pageId="46" pageNumber="47">EMEC</collectionCode>
