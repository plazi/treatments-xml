<taxonomicName id="2DA04D5DFFE0FF2D4827112453C81C54" authority="Bates, 1881" authorityName="Bates" authorityYear="1881" box="[303,668,589,612]" class="Insecta" family="Cerambycidae" genus="Carphontes" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="52" phylum="Arthropoda" rank="species" species="posticalis">
<emphasis id="D8D4EACCFFE0FF2D48271124534C1C54" box="[303,536,589,612]" italics="true" pageId="1" pageNumber="52">Carphontes posticalis</emphasis>
<bibRefCitation id="8E314B2FFFE0FF2D4B17112453C81C54" author="Bates" box="[543,668,589,612]" pageId="1" pageNumber="56" refString="Bates, H. W. (1881) Biologia Centrali-Americana, Insecta, Coleoptera. London, 5, 153 - 224." type="journal article" year="1881">
Bates, 
<number id="5C6824F9FFE0FF2D4B6F112453C81C54" box="[615,668,589,612]" pageId="1" pageNumber="52" value="1881.0">1881</number>
