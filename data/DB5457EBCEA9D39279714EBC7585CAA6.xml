<taxonomicName id="A9BBDA098510CA1ECB1E5EB404DED63A" class="Insecta" family="Dytiscidae" genus="Agaporomorphus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agaporomorphus tambopatensis" order="Coleoptera" pageId="7" pageNumber="70" phylum="Arthropoda" rank="species" species="tambopatensis">Agaporomorphus tambopatensis</taxonomicName>
