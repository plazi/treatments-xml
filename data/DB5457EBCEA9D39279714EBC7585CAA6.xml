<taxonomicName id="ADFDE660264551E59773B9A1C011D368" class="Insecta" family="Dytiscidae" genus="Agaporomorphus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agaporomorphus grandisinuatus" order="Coleoptera" pageId="7" pageNumber="70" phylum="Arthropoda" rank="species" species="grandisinuatus">Agaporomorphus grandisinuatus</taxonomicName>
