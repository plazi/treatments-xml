<taxonomicName id="26A24D486B296047577AF99888C3F982" authorityName="Price &amp; Dalgleish" authorityYear="2006" box="[324,480,1610,1635]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="9" pageNumber="170" phylum="Arthropoda" rank="species" species="rufi">
