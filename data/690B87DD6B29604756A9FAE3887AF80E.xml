<taxonomicName id="26A24D486B296047520AF9BC8C1FF967" baseAuthorityName="Vieillot" baseAuthorityYear="1822" box="[1076,1340,1646,1671]" class="Aves" family="Thraupidae" genus="Tachyphonus" kingdom="Animalia" order="Passeriformes" pageId="9" pageNumber="170" phylum="Chordata" rank="species" species="coronatus">
