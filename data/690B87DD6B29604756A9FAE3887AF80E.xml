<taxonomicName id="26A24D486B2960475472F9BD8BB9F966" authorityName="Price &amp; Dalgleish" authorityYear="2006" box="[588,666,1647,1671]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="9" pageNumber="170" phylum="Arthropoda" rank="species" species="rufi">
