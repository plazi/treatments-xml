<collectingCountry id="99B5765B6B29604754E8F9988A97F982" box="[726,948,1610,1635]" name="Trinidad and Tobago" pageId="9" pageNumber="170">
Trinidad &amp; 
<collectingRegion id="2366F8296B296047555EF9988A97F982" box="[864,948,1610,1635]" country="Trinidad and Tobago" name="Tobago" pageId="9" pageNumber="170">Tobago</collectingRegion>
