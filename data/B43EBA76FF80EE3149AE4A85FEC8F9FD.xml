<taxonomicName id="FB9770E3FF80EE304B544C86FD59F923" authorityName="Shavrin &amp; Zanetti" authorityYear="2021" box="[621,714,1683,1707]" class="Insecta" family="Staphylinidae" genus="Lesteva" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="419" phylum="Arthropoda" rank="species" species="osellai">
