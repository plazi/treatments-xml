<quantity id="4CF4C0CBFFC8FFC20E95FF1BFE01FF1A" box="[452,511,168,195]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="256" unit="m" value="20.0">20 m</quantity>
