<quantity id="99C6CA6DE18F2AC3D642E0A5B4B0D5F4" metricMagnitude="-2" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" unit="mm" value="27.5" valueMax="30.0" valueMin="25.0">25-30 mm</quantity>
