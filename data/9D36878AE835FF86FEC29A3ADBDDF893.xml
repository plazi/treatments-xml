<heading id="4E6881F0E835FF80FEC29A3ADC95FA85" bold="true" box="[264,897,1342,1368]" fontSize="11" level="1" pageId="3" pageNumber="52" reason="1">
<emphasis id="27EBEA8EE835FF80FEC29A3ADC95FA85" bold="true" box="[264,897,1342,1368]" pageId="3" pageNumber="52">
<taxonomicName id="D29F4D1FE835FF80FEC29A3ADDF0FA85" authority="Miller" authorityName="Miller" box="[264,740,1342,1368]" class="Insecta" family="Dytiscidae" genus="Agaporomorphus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="tambopatensis" status="new species">
