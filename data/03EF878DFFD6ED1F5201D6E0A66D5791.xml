<taxonomicName id="4C464D18FFD7ED1C51FED3F3A45B53C5" box="[759,848,1717,1738]" class="Insecta" family="Cicadellidae" genus="Scaphoideus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="bifidus" status="sp. nov.">
<emphasis id="B932EA89FFD7ED1C51FED3F3A45B53C5" box="[759,848,1717,1738]" italics="true" pageId="13" pageNumber="14">S. bifidus</emphasis>
</taxonomicName>
