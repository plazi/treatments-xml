<quantity id="4CBE9B7EFFD4ED1F5179D42AA5C05489" box="[624,715,364,390]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="14" pageNumber="15" unit="mm" value="4.1">4.1 mm</quantity>
long, 
<quantity id="4CBE9B7EFFD4ED1F5012D42AA48C5489" box="[795,903,364,390]" metricMagnitude="-3" metricUnit="m" metricValue="1.02" pageId="14" pageNumber="15" unit="mm" value="1.02">1.02 mm</quantity>
wide across eyes. Female 
<quantity id="4CBE9B7EFFD4ED1F57C6D42AA2265489" box="[1231,1325,364,390]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="14" pageNumber="15" unit="mm" value="4.5">4.5 mm</quantity>
long and 
<quantity id="4CBE9B7EFFD4ED1F527CD4D2A6D554A1" box="[373,478,404,430]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="14" pageNumber="15" unit="mm" value="1.2">1.20 mm</quantity>
wide across eyes.
</paragraph>
</subSubSection>
