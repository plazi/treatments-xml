<collectionCode id="ED38DAAFFFF57F1C9D17E016D350FC29" box="[1062,1090,873,899]" country="Austria" lsid="urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="48" pageNumber="49" type="Herbarium">W</collectionCode>
