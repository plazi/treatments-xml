<taxonomicName id="4C2939E9FFF57F1C9BB6E0EFD5ECFC03" authorityName="J. Gonzales" authorityYear="2011" baseAuthorityName="J. Gonzales" baseAuthorityYear="2011" box="[647,766,912,937]" class="Polypodiopsida" family="Gleicheniaceae" genus="Sticherus" kingdom="Plantae" order="Gleicheniales" pageId="48" pageNumber="49" phylum="Tracheophyta" rank="species" species="lanosus">
