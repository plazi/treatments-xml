<paragraph id="863F36DF79360C2B3DA7FC5DFEA8FBBE" blockId="6.[151,1436,658,1342]" pageId="6" pageNumber="7">
<taxonomicName id="41804D5C79360C2B3DA7FC5DFE9AFBE0" box="[198,317,1007,1032]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="genus">
