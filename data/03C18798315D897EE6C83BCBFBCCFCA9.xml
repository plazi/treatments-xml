species 
<taxonomicName id="4C684D0D315D897CE7793A54FD40FC2F" authority="Karg, 1965" authorityName="Karg" authorityYear="1965" box="[294,653,264,287]" class="Arachnida" family="Eviphididae" genus="Iphidosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="48" phylum="Arthropoda" rank="species" species="pratensis">
