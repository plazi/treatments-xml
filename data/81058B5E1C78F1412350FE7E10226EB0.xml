r. 
<taxonomicName id="F097786C787149D100182DF7BBF8556F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26966" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Camponotus naegelii Forel" lsidName-HNS="Camponotus naegelii Forel" order="Hymenoptera" pageNumber="42" phylum="Arthropoda" rank="species" species="naegelii">C. Naegelii</taxonomicName>
Forel
