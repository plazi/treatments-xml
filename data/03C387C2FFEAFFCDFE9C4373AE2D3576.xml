<taxonomicName id="4C6A4D57FFEDFFCCFE0C4330AF6536E6" box="[408,584,1655,1678]" class="Entognatha" family="Arrhopalitidae" genus="Arrhopalites" kingdom="Animalia" order="Collembola" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="heteroculatus" status="sp. n.">
<emphasis id="B91EEAC6FFEDFFCCFE0C4330AF6536E6" box="[408,584,1655,1678]" italics="true" pageId="20" pageNumber="21">
A. 
<taxonomicName id="4C6A4D57FFEDFFCCFE2C4330AF6536E6" box="[440,584,1655,1678]" pageId="20" pageNumber="21" rank="species" species="heteroculatus" status="sp. n.">heteroculatus</taxonomicName>
