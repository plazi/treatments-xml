<taxonomicName id="0CDC4D57FFFD6538FBF5EC63D1B61030" authority="Hutton, 1884" authorityName="Hutton" authorityYear="1884" box="[1120,1436,1084,1110]" class="Insecta" family="Urodidae" genus="Patula" kingdom="Animalia" order="Lepidoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="lucetta">
