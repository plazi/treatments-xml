<taxonomicName id="DA540D7DFBE5E0623B64C5EFB0FCC54B" authority="Hershkovitz, 1993" authorityName="Hershkovitz" authorityYear="1993" class="Mammalia" family="Cricetidae" genus="Microakodontomys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microakodontomys transitorius" order="Rodentia" pageId="14" pageNumber="119" phylum="Chordata" rank="species" species="transitorius">
