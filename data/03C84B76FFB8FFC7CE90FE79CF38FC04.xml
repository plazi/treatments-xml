<taxonomicName id="4C6181E3FFB8FFC7CFFBFE13CE50FDEB" box="[508,619,505,527]" class="Arachnida" family="Phytoseiidae" genus="Neoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="442" phylum="Arthropoda" rank="genus">
<emphasis id="B9152672FFB8FFC7CFFBFE13CE50FDEB" box="[508,619,505,527]" italics="true" pageId="1" pageNumber="442">Neoseiulus</emphasis>
</taxonomicName>
