<taxonomicName id="F04C546B39C1CF8CA3B69565FC6D05CA" LSID="http://zoobank.org/71CE0862-31F7-4ADD-B977-F00479198873" class="Reptilia" family="Liolaemidae" genus="Liolaemus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liolaemus leftrarui" order="Squamata" pageId="8" pageNumber="129" phylum="Chordata" rank="species" species="leftrarui">Liolaemus leftrarui</taxonomicName>
<taxonomicNameLabel id="B1B41F01372A30D9F1273B806FDA0A4B" pageId="8" pageNumber="129">sp. n.</taxonomicNameLabel>
