<paragraph id="8BDB36BEFF8D650DDAD5FF319F25FE9F" blockId="4.[143,607,351,375]" box="[143,607,351,375]" pageId="4" pageNumber="1340">
<taxonomicName id="4C644D3DFF8D650DDAD5FF319F21FE9F" authority="O. Fabricius, 1780" authorityName="O. Fabricius" authorityYear="1780" box="[143,603,351,375]" class="Pycnogonida" family="Pycnogonidae" genus="Pycnogonum" kingdom="Animalia" order="Pantopoda" pageId="4" pageNumber="1340" phylum="Arthropoda" rank="species" species="grossipes">
