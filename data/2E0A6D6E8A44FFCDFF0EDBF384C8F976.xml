<taxonomicName id="61A3A7FB8A44FFCFFF2FDBF0860BF9F8" authority="David &amp; Ranganath" authorityName="David &amp; Ranganath" box="[182,835,1521,1548]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="29" phylum="Arthropoda" rank="species" species="chettalli" status="sp.nov." subGenus="Calodacus">
