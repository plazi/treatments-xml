<taxonomicName id="4C3D4D16FFF5FFC7FEB5F987FDBD3FBE" authority="Allsopp" authorityName="Allsopp" authorityYear="2020" box="[333,600,1650,1675]" class="Insecta" family="Melolonthidae" genus="Hypolepida" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="599" phylum="Arthropoda" rank="species" species="braes" status="new species">
<emphasis id="B949EA87FFF5FFC7FEB5F987FDE43FBE" box="[333,513,1651,1675]" italics="true" pageId="4" pageNumber="599">Hypolepida braes</emphasis>
