<subSubSection id="C5BE19ECE47EFFA0FFC49218CCE6F9AC" box="[199,1437,1626,1651]" pageId="18" pageNumber="469" type="etymology">
<paragraph id="8D1B4A67E47EFFA0FFC49218CCE6F9AC" blockId="18.[151,1437,1518,2011]" box="[199,1437,1626,1651]" pageId="18" pageNumber="469">
<emphasis id="BFD09675E47EFFA0FFC49218C835F9AD" bold="true" box="[199,334,1626,1651]" pageId="18" pageNumber="469">Etymology.</emphasis>
