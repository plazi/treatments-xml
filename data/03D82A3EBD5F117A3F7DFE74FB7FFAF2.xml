<taxonomicName id="4C71E0ABBD5F117A3F18FE74FC76FE06" authorityName="Broun" authorityYear="1915" box="[861,928,391,412]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="rustica">
