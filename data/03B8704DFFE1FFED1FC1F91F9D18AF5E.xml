<taxonomicName id="4C11BAD8FFE1FFEC1FC1F85D98BAA9FA" authority="Rohwer, 1920: 413" authorityName="Rohwer" authorityPageNumber="413" authorityYear="1920" box="[151,461,1955,1977]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Labenidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="195" phylum="Arthropoda" rank="genus">
