<subSubSection id="AEF95CB2FFB8FF9A309371A2263FA1DC" pageId="22" pageNumber="223" type="materials_examined">
<paragraph id="E65C0F39FFB8FF9A309371A227B9A114" blockId="22.[151,1437,258,2020]" pageId="22" pageNumber="223">
<emphasis id="D497D32BFFB8FF9A309371A2263DA0A8" bold="true" box="[199,367,1743,1768]" pageId="22" pageNumber="223">Type material</emphasis>
. Holotype male, 
<materialsCitation id="568B0564FFB8FF9A326371A2247EA14B" ID-GBIF-Occurrence="1324639485" collectingDate="2012-05-22" collectionCode="FSCA" collectorName="R. H. Turnbow" country="Guadeloupe" county="Cretes" location="Basse Terre" municipality="Trace" pageId="22" pageNumber="223" specimenCount="1" typeStatus="allotype">
<typeStatus id="3958B19BFFB8FF9A326371A225C3A0A8" box="[567,657,1743,1768]" pageId="22" pageNumber="223">allotype</typeStatus>
female labeled “ 
