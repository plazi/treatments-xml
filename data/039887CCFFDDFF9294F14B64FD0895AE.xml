, 
<collectingCountry id="F326764AFFD3FF92948F49E6FEC695AE" box="[233,346,1421,1446]" name="Colombia" pageId="25" pageNumber="526">Colombia</collectingCountry>
, 
<collectingCountry id="F326764AFFD3FF92950049E6FE5995AE" box="[358,453,1421,1446]" name="Ecuador" pageId="25" pageNumber="526">Ecuador</collectingCountry>
, and 
<collectingCountry id="F326764AFFD3FF92959949E5FDAE95AD" box="[511,562,1422,1445]" name="Peru" pageId="25" pageNumber="526">Peru</collectingCountry>
(
