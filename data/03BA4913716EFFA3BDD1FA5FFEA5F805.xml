<taxonomicName id="4C138386716FFFA3BEDDF853FB94F852" box="[923,1122,1931,1957]" class="Magnoliopsida" family="Erythroxylaceae" kingdom="Plantae" order="Malpighiales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="family">Erythroxylaceae</taxonomicName>
