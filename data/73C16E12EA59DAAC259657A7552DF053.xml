<geoCoordinate id="DAA19C4019D9FF488506E0EEEFD42A29" degrees="27" direction="south" minutes="30" orientation="latitude" precision="925" value="-27.5">27 30'S</geoCoordinate>
, 
<geoCoordinate id="21E36EFF4C0137905883E2E73623C7A8" degrees="32" direction="east" minutes="05" orientation="longitude" precision="925" value="32.083332">32 05'E</geoCoordinate>
