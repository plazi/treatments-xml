<collectingCountry id="60A6F9D3C30E91367A488B2FFEE77974" box="[266,330,348,372]" name="China" pageId="7" pageNumber="584">China</collectingCountry>
(
<collectingRegion id="DA7577A1C30E91367A1A8B2FFE0B7974" box="[344,422,348,372]" country="China" name="Shanxi" pageId="7" pageNumber="584">Shanxi</collectingRegion>
