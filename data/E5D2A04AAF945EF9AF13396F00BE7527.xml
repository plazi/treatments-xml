<taxonomicName id="3980AD285E3AFCF6DCFEEAE4AA12C55F" authorityName="Wood &amp; Bright" authorityYear="1992" class="Insecta" family="Curculionidae" genus="Cryphalus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cryphalus piceae" order="Coleoptera" pageId="0" pageNumber="181" phylum="Arthropoda" rank="species" species="piceae">
