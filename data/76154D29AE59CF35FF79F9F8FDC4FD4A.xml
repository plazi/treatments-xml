3. 
<taxonomicName id="39BC87BCAE59CF3AFF5BF9FFFC34F9FD" authority="Dlabola, 1967" authorityName="Dlabola" authorityYear="1967" box="[181,899,1579,1606]" class="Insecta" family="Cicadellidae" genus="Anaceratagallia" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="256" phylum="Arthropoda" rank="species" species="chalchica" subGenus="Anaceratagallia">
