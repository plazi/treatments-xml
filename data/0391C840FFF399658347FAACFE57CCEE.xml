<paragraph id="8B877956FFF299628377FCF4FEA5CEF6" blockId="27.[264,1324,284,1870]" pageId="27" pageNumber="28">
<emphasis id="B94CA544FFF299628377FCF4FDEBCE7E" bold="true" box="[312,521,804,830]" italics="true" pageId="27" pageNumber="28">
<collectingCountry id="F32F39C6FFF299628377FCF4FE97CE7E" box="[312,373,804,830]" name="Malta" pageId="27" pageNumber="28">Male</collectingCountry>
