<quantity id="9B297FD1732F015DFE44FF637A4AFF05" box="[471,576,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="6" pageNumber="459" unit="mm" value="2.4">2.40 mm</quantity>
