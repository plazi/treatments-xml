, 
<quantity id="9B297FD1732F015DFCF7FF637BC7FF05" box="[868,973,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="1.35" pageId="6" pageNumber="459" unit="mm" value="1.35">1.35 mm</quantity>
