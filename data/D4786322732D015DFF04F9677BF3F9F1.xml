<number id="EA19C013732F015DFE8BFE28792FFE5E" box="[280,293,476,500]" pageId="6" pageNumber="459" value="5.0">5</number>
/ 
<number id="EA19C013732F015DFEBCFE287936FE5E" box="[303,316,476,500]" pageId="6" pageNumber="459" value="3.0">3</number>
<number id="EA19C013732F015DFED1FE287993FE5E" box="[322,409,476,500]" pageId="6" pageNumber="459" value="1.9" valueMax="2.3" valueMin="1.5">1.5–2.3</number>
