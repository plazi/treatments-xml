<emphasis id="32EF2972FF89FFB7658CF92C2A734632" bold="true" box="[159,267,1771,1795]" pageId="44" pageNumber="45">Diagnosis.</emphasis>
<taxonomicName id="C79B8EE3FF89FFB76401F92B2A1D4632" box="[274,357,1772,1795]" class="Actinopterygii" family="Poeciliidae" genus="Poecilia" kingdom="Animalia" order="Cyprinodontiformes" pageId="44" pageNumber="45" phylum="Chordata" rank="genus">
