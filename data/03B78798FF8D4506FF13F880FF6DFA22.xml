<number id="3DD624A9FF8E4500FE7EF8ECFDA8F8A9" box="[506,514,1862,1876]" pageId="2" pageNumber="62" value="2.0">2</number>
</emphasis>
anterior 
<emphasis id="B96AEA9CFF8E4500FDF5F89FFD35F8B2" bold="true" box="[625,671,1845,1871]" pageId="2" pageNumber="62">Dm</emphasis>
<emphasis id="B96AEA9CFF8E4500FD1BF8ECFD0DF8A9" bold="true" box="[671,679,1862,1876]" pageId="2" pageNumber="62">
<number id="3DD624A9FF8E4500FD1BF8ECFD0DF8A9" box="[671,679,1862,1876]" pageId="2" pageNumber="62" value="1.0">1</number>
</emphasis>
; 
<emphasis id="B96AEA9CFF8E4500FD3DF89FFD4CF8B2" bold="true" box="[697,742,1845,1871]" pageId="2" pageNumber="62">Vm</emphasis>
<emphasis id="B96AEA9CFF8E4500FD62F8ECFD44F8A9" bold="true" box="[742,750,1862,1876]" pageId="2" pageNumber="62">
<number id="3DD624A9FF8E4500FD62F8ECFD44F8A9" box="[742,750,1862,1876]" pageId="2" pageNumber="62" value="1.0">1</number>
</emphasis>
at level 
<emphasis id="B96AEA9CFF8E4500FCDFF89FFC22F8B2" bold="true" box="[859,904,1845,1871]" pageId="2" pageNumber="62">Dm</emphasis>
<emphasis id="B96AEA9CFF8E4500FC0CF8ECFC3AF8A9" bold="true" box="[904,912,1862,1876]" pageId="2" pageNumber="62">
<number id="3DD624A9FF8E4500FC0CF8ECFC3AF8A9" box="[904,912,1862,1876]" pageId="2" pageNumber="62" value="1.0">1</number>
</emphasis>
; 
<emphasis id="B96AEA9CFF8E4500FC27F89FFC7AF8B2" bold="true" box="[931,976,1845,1871]" pageId="2" pageNumber="62">Vm</emphasis>
<emphasis id="B96AEA9CFF8E4500FC54F8ECFC72F8A9" bold="true" box="[976,984,1862,1876]" pageId="2" pageNumber="62">
<number id="3DD624A9FF8E4500FC54F8ECFC72F8A9" box="[976,984,1862,1876]" pageId="2" pageNumber="62" value="4.0">4</number>
</emphasis>
at level of proximal border of dorsal depression; 
<emphasis id="B96AEA9CFF8E4500FEA0F8CAFEFBF887" bold="true" box="[292,337,1888,1914]" pageId="2" pageNumber="62">Dm</emphasis>
