<quantity id="722B9B79FF86FFCFA21CFE50FBB131FF" box="[1042,1090,444,466]" metricMagnitude="0" metricUnit="m" metricValue="2.3876" pageId="8" pageNumber="440" unit="in" value="94.0">94 in</quantity>
