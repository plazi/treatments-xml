<quantity id="722B9B79FF86FFCFA4B6FE50FCB531FF" box="[696,838,444,466]" metricMagnitude="-3" metricUnit="m" metricValue="1.42" metricValueMax="1.44" metricValueMin="1.4" pageId="8" pageNumber="440" unit="mm" value="1.42" valueMax="1.44" valueMin="1.4">1.40–1.44 mm</quantity>
