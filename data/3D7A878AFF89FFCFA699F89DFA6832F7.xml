<taxonomicName id="72D34D1FFF86FFCFA313FE35FA6831C2" authorityName="Puthz" authorityYear="2008" box="[1309,1435,473,495]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="440" phylum="Arthropoda" rank="species" species="viridivestis">
