<taxonomicName id="72D34D1FFF89FFC0A59CF878FBE43781" authorityName="Champion" authorityYear="1925" box="[914,1047,1940,1964]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="439" phylum="Arthropoda" rank="species" species="viridanus">
