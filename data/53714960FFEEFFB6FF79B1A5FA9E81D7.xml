<taxonomicName id="1CD883F5FFEEFFB6FDE8B0ABFCF781F3" authority="Fabricius" authorityName="Fabricius" box="[518,836,1970,1995]" class="Insecta" family="Pyralidae" genus="Pyralis" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="emargana">
