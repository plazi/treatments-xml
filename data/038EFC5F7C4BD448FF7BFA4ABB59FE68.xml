<taxonomicName id="4C2736CA7C4DD449FE17F892BB24F87A" box="[507,596,1919,1941]" class="Insecta" family="Platygastridae" genus="Trichacis" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
