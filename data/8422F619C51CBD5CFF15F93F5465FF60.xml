<taxonomicName id="CB8B3C8CC51DBD5CFDF0FF175245FF60" authorityName="Moure" authorityYear="1950" baseAuthorityName="Perty" baseAuthorityYear="1833" box="[626,813,187,213]" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="244" phylum="Arthropoda" rank="species" species="xanthocnemis">
