<collectingCountry id="F3686CCBFFC40368EBB6FE5B313026A0" box="[493,562,418,442]" name="Chile" pageId="14" pageNumber="13">Chile</collectingCountry>
, 
<collectingRegion id="49BBE2B9FFC40368E866FE5B30E026A0" box="[573,994,418,442]" country="Chile" name="Region Metropolitana" pageId="14" pageNumber="13">Región Metropolitana de Santiago</collectingRegion>
