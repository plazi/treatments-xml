<collectionCode id="ED6EB49EFFC40368EA8DFDFB32252500" box="[214,295,514,538]" country="Czech Republic" httpUri="http://grbio.org/cool/kyg7-47c6" name="Collection of Algae of Charles University, Prague" pageId="14" pageNumber="13">CAUP</collectionCode>
