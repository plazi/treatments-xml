<taxonomicName id="46DD42D9B65EFFFCFF6CFE7903C8FE22" box="[151,387,476,500]" class="Magnoliopsida" family="Poaceae" genus="Centotheca" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="lappacea">
<emphasis id="B3A9E548B65EFFFCFF6CFE7903C8FE22" box="[151,387,476,500]" italics="true" pageId="45" pageNumber="46">Centotheca lappacea</emphasis>
</taxonomicName>
(L.) Desv. (
<taxonomicName id="46DD42D9B65EFFFCFDF1FE790027FE22" authorityName="" authorityYear="1895" box="[522,620,476,500]" class="Magnoliopsida" family="Poaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
