<paragraph id="8B8936DB120CE142FCED9C28FB1BFF73" blockId="18.[806,1422,1360,1903]" box="[806,1200,1360,1383]" pageId="18" pageNumber="65">
<materialsCitation id="3B5E3C86120CE142FCED9C28FB1BFF73" box="[806,1200,1360,1383]" pageId="18" pageNumber="65" specimenCode="CRBA-LX1789" specimenCount="1" typeStatus="paratype">
Female (
<typeStatus id="548D8879120CE142FC409C29FC58FF72" box="[907,1011,1361,1382]" pageId="18" pageNumber="65">paratype</typeStatus>
, 
<specimenCode id="DB909EA0120CE142FBCB9C29FB02FF73" box="[1024,1193,1361,1383]" collectionCode="CRBA-LX" pageId="18" pageNumber="65">CRBA-LX1789</specimenCode>
