<paragraph id="8B806D06BF44C248CDC8B6ADE63F1CA3" blockId="75.[106,745,151,242]" pageId="75" pageNumber="228">
<materialsCitation id="3B57675BBF44C248CDC8B6ADE63B1CA3" collectionCode="R" country="Sweden" county="Swedish Museum of Natural History" location="Palaeobotanical Collections" municipality="Department of Palaeobiology" pageId="75" pageNumber="228" specimenCount="1" stateProvince="Stockholm">
