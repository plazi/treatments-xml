, 
<collectingRegion id="49FBA3E4BF44C248CD32B6EFE7981CA3" box="[106,224,218,242]" country="Sweden" name="Stockholm" pageId="75" pageNumber="228">Stockholm</collectingRegion>
, 
<collectingCountry id="F3282D96BF44C248CDB3B6EFE63B1CA3" box="[235,323,218,242]" name="Sweden" pageId="75" pageNumber="228">Sweden</collectingCountry>
