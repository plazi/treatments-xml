<number id="0E687725FFE2FFA939B6F989FF25A4D0" box="[151,180,1656,1682]" pageId="27" pageNumber="128" value="23.0">23</number>
. 
<taxonomicName id="7FA01E81FFE2FFA939E2F989FC16A4D0" authority="Blanchard, 1851" authorityName="Blanchard" authorityYear="1851" box="[195,903,1656,1682]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="128" phylum="Arthropoda" rank="species" species="longicornis" subGenus="Phyllophaga">
<emphasis id="8AD4B910FFE2FFA939E2F989FECDA4D0" bold="true" box="[195,348,1656,1682]" italics="true" pageId="27" pageNumber="128">Phyllophaga</emphasis>
(
<emphasis id="8AD4B910FFE2FFA9384CF989FD97A4D0" bold="true" box="[365,518,1656,1682]" italics="true" pageId="27" pageNumber="128">Phyllophaga</emphasis>
) 
<emphasis id="8AD4B910FFE2FFA93B36F989FD31A4D0" bold="true" box="[535,672,1656,1682]" italics="true" pageId="27" pageNumber="128">longicornis</emphasis>
(Blanchard, 
<number id="0E687725FFE2FFA93A62F989FCEEA4D0" box="[835,895,1656,1682]" pageId="27" pageNumber="128" value="1851.0">1851</number>
