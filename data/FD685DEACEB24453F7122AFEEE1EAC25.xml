<taxonomicName id="7A5D50182007CEB02DEFD8216E085EE8" authorityName="Linsenmaier" authorityYear="1968" class="Insecta" family="Chrysididae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis tenuimediata" order="Hymenoptera" pageId="24" pageNumber="103" phylum="Arthropoda" rank="species" species="tenuimediata">
