<taxonomicName id="4C2D018EA438C91696892ED7FEB1FE79" box="[151,326,402,428]" class="Insecta" family="Hybotidae" genus="Syneches" kingdom="Animalia" order="Diptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="vidali">
