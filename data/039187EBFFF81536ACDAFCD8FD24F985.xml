(
<bibRefCitation id="EFA94B0CFFF81536ADA2FAECFD1BFAC7" author="Chace" box="[495,641,1354,1380]" pageId="12" pageNumber="55" refString="Chace, F. A. (1972) The shrimps of the Smithsonian-Bredin Caribbean expeditions with a summary of the West Indian shallow-water species (Crustacea: Decapoda: Natantia). Smithsonian Contributions to Zoology, 98, 1 - 179." type="journal article" year="1972">
Chace 
<number id="3DF024DAFFF81536AE0FFAECFD1BFAC7" box="[578,641,1354,1380]" pageId="12" pageNumber="13" value="1972.0">1972</number>
</bibRefCitation>
); 
<collectingCountry id="F32F766DFFF81536AED8FAECFD7AFAC7" box="[661,736,1354,1380]" name="Belize" pageId="12" pageNumber="13">Belize</collectingCountry>
(Macdonald et al. 
<number id="3DF024DAFFF81536AFF1FAECFC60FAC7" box="[956,1018,1354,1380]" pageId="12" pageNumber="13" value="2006.0">2006</number>
; 
<bibRefCitation id="EFA94B0CFFF81536A84BFAECFB77FAC7" author="Rios" box="[1030,1261,1354,1380]" lastPageId="12" lastPageNumber="57" pageId="12" pageNumber="56" refString="Rios, R. and Duffy, J. E. (2007) A review of the sponge-dwelling snapping shrimp from Carrie Bow Cay, Belize, with description of Zuzalpheus, new genus, and six new species (Crustacea: Decapoda: Alpheidae). Zootaxa, 1602, 1 - 89." type="journal article" year="2007">
Ríos &amp; Duffy 
<number id="3DF024DAFFF81536A8E3FAECFB77FAC7" box="[1198,1261,1354,1380]" pageId="12" pageNumber="13" value="2007.0">2007</number>
