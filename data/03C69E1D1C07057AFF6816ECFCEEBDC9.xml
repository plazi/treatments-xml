<taxonomicName id="4C6F54881C07057AFD5D152AFCA2BD02" authorityName="Sturm" authorityYear="1827" box="[677,802,1581,1607]" class="Insecta" family="Carabidae" genus="Bembidion" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="34" phylum="Arthropoda" rank="species" species="fulvipes">
