<caption id="DF2D660AFFABB309E92708A7FC7BFA33" ID-DOI="http://doi.org/10.5281/zenodo.5030088" ID-Zenodo-Dep="5030088" httpUri="https://zenodo.org/record/5030088/files/figure.png" pageId="23" pageNumber="24" startId="23.[264,368,1427,1451]" targetBox="[309,1248,320,1401]" targetPageId="23">
