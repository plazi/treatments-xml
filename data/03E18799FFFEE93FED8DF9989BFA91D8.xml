, 
<taxonomicName id="4C484D0CFFE0E93CEC8AFDEE9B1390D0" authority="Mayr &amp; Fahlbusch, 1975" authorityName="Mayr &amp; Fahlbusch" authorityYear="1975" class="Mammalia" family="Cricetidae" genus="Microtocricetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="17" pageNumber="324" phylum="Chordata" rank="species" species="molassicus">
