</taxonomicName>
<taxonomicNameLabel id="A24157F5DF447203A43EFF09FDC9FF35" box="[582,642,224,247]" pageId="5" pageNumber="6" rank="species">n. sp.</taxonomicNameLabel>
<taxonomicName id="4C064D1FDF447203A4F0FF09FC6BFF3A" authority="Gerken, 2012" authorityName="Gerken" authorityYear="2012" box="[648,800,224,248]" class="Malacostraca" family="Nannastacidae" genus="Aotearocumella" kingdom="Animalia" order="Cumacea" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="echinoseta" status="n. sp.">
<emphasis id="B972EA8EDF447203A4F0FF09FC6BFF3A" box="[648,800,224,248]" italics="true" pageId="5" pageNumber="6">A. echinoseta</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24157F5DF447203A55EFF09FC2FFF35" box="[806,868,224,247]" pageId="5" pageNumber="6" rank="species">n. sp.</taxonomicNameLabel>
