long, 
<quantity id="4CAC36A2FFBE1839B8082C59FE22FE84" box="[264,386,284,310]" metricMagnitude="1" metricUnit="m" metricValue="6.5" metricValueMax="8.0" metricValueMin="5.0" pageId="2" pageNumber="39" unit="m" value="65.0" valueMax="80.0" valueMin="50.0">50–80 m</quantity>
