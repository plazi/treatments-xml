<subSubSection id="BEC36531FFCEFFC16CEEFCAD4255369F" pageId="26" pageNumber="27" type="diagnosis">
<paragraph id="F66636BAFFCEFFC16CEEFCAD4255369F" blockId="26.[151,1436,838,937]" pageId="26" pageNumber="27">
<emphasis id="C4ADEAA8FFCEFFC16CEEFCAD471036B2" bold="true" box="[199,322,874,900]" pageId="26" pageNumber="27">Diagnosis.</emphasis>
