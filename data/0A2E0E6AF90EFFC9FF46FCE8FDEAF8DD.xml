<materialsCitation id="32EFB521F90EFFC9FEA4FAE1FDF7FAE5" ID-GBIF-Occurrence="1913888626" country="China" location="Hubei" pageId="31" pageNumber="232" specimenCount="1" stateProvince="Hubei" typeStatus="holotype">
