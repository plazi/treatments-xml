<taxonomicName id="4C674D72B328927F30DAFAA7F07FFACD" authority="Bosc d'Antic, 1812: 72" authorityName="Bosc d'Antic" authorityPageNumber="72" authorityYear="1812" box="[151,609,1352,1374]" class="Liliopsida" family="Orchidaceae" genus="Dipodium" kingdom="Animalia" order="Asparagales" pageId="3" pageNumber="524" phylum="Tracheophyta" rank="species" species="apiarium">
<emphasis id="B913EAE3B328927F30DAFAA7F37CFACD" box="[151,354,1352,1374]" italics="true" pageId="3" pageNumber="524">Dipodium apiarium</emphasis>
<treatmentCitation id="0AC610E0B328927F3126FAA6F07FFACD" author="Bosc" box="[363,609,1352,1374]" page="72" pageId="3" pageNumber="524" year="1812">
