<taxonomicName id="4C434D30FFB61236F3988A8E3269F4B6" authority="Mertens" authorityName="Mertens" box="[151,343,2000,2025]" class="Amphibia" family="Hylidae" genus="Phyllomedusa" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="rohdei">
<emphasis id="B937EAA1FFB61236F3988A8E33E1F4B7" box="[151,223,2000,2025]" italics="true" pageId="31" pageNumber="32">rohdei</emphasis>
