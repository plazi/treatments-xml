<taxonomicName id="4C434D30FFB61236F7928B80364AF5AA" box="[1181,1396,1758,1780]" class="Amphibia" family="Hylidae" genus="Phyllomedusa" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="rohdei">
