, 
<location id="8E8F6008FFB9F77400D6168E4DF9C88B" LSID="urn:lsid:plazi:treatment:03F987C5FFB9F776035D16344F82C9A7:8E8F6008FFB9F77400D6168E4DF9C88B" county="Broadly" latitude="-15.031111" longLatPrecision="21" longitude="124.94889" municipality="Periphery" name="Coronation Island" pageId="40" pageNumber="257" stateProvince="Western Australia">Coronation Island</location>
, southern section, 
<geoCoordinate id="EE645014FFB9F77403B716AD4F2BC88B" box="[450,574,1768,1791]" degrees="15" direction="south" minutes="01" orientation="latitude" pageId="40" pageNumber="257" precision="15" seconds="52" value="-15.031111">15°01'52&quot;S</geoCoordinate>
