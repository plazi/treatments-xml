<subSubSection id="54D554BCFFBFA657FCBBD362FBC3F946" pageId="9" pageNumber="736" type="biology_ecology">
<paragraph id="1C700737FFBFA657FCBBD362FBC3F946" pageId="9" pageNumber="736">
<emphasis id="2EBBDB25FFBFA657FCBBD362FC0DF92F" bold="true" box="[843,945,1641,1667]" pageId="9" pageNumber="736">Biology.</emphasis>
