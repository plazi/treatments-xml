<taxonomicName id="BFF24D37E762FF880B52FB1AF8FFEFE1" box="[620,762,1187,1213]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName id="BFF24D37E762FF880A3AFB1AF989EFE2" box="[772,908,1187,1214]" class="Insecta" family="Hesperiidae" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="family">Hesperiidae</taxonomicName>
) on 
<taxonomicName id="BFF24D37E762FF880A83FB1AFE0DEFE1" box="[957,1032,1187,1213]" class="Liliopsida" family="Cannaceae" genus="Canna" kingdom="Plantae" order="Zingiberales" pageId="9" pageNumber="210" phylum="Tracheophyta" rank="genus">
<emphasis id="4A86EAA6E762FF880A83FB1AFE0DEFE1" box="[957,1032,1187,1213]" italics="true" pageId="9" pageNumber="210">Canna</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="BFF24D37E762FF880D00FB1AFEB9EFE1" box="[1086,1212,1187,1213]" class="Liliopsida" family="Cannaceae" kingdom="Plantae" order="Zingiberales" pageId="9" pageNumber="210" phylum="Tracheophyta" rank="family">
<emphasis id="4A86EAA6E762FF880D00FB1AFEB9EFE1" box="[1086,1212,1187,1213]" italics="true" pageId="9" pageNumber="210">Cannaceae</emphasis>
</taxonomicName>
).
</paragraph>
