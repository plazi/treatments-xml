<taxonomicName id="36DFFEF3FFD3FF89C45FFA6EFDB5F983" baseAuthorityName="Illiger" baseAuthorityYear="1815" box="[336,594,1515,1539]" class="Mammalia" family="Canidae" genus="Chrysocyon" kingdom="Animalia" order="Carnivora" pageId="10" pageNumber="795" phylum="Chordata" rank="species" species="brachyurus">
