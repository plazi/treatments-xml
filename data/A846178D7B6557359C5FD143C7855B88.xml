<taxonomicName id="CC1DD94F3ACB17338A3004E9D298A386" class="Insecta" family="Meliaceae" genus="Guarea" kingdom="Animalia" lsidName="Guarea guidonia" order="Hymenoptera" pageId="0" pageNumber="101" phylum="Arthropoda" rank="species" species="guidonia">
