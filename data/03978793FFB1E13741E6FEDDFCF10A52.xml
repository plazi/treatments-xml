<taxonomicName id="4C3E4D06FFB1E13743CEFD74FC2A0CCA" box="[703,971,741,765]" class="Arachnida" family="Philodromidae" genus="Philodromus" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="ganxiensis">
