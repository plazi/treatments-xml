<taxonomicName id="4C3E4D06FFB1E1374066FC98FE8D0D16" box="[279,364,777,801]" class="Arachnida" family="Thomisidae" genus="Epidius" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
<emphasis id="B94AEA97FFB1E1374066FC98FE8D0D16" box="[279,364,777,801]" italics="true" pageId="14" pageNumber="15">Epidius</emphasis>
</taxonomicName>
