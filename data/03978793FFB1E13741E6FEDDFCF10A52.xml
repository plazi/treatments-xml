<taxonomicName id="4C3E4D06FFB1E13745E6FD74FAC60CCA" box="[1175,1319,741,765]" class="Arachnida" family="Philodromidae" genus="Philodromus" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
