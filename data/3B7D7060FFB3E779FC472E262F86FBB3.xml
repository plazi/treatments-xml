<taxonomicName id="74D4BAF5FFB3E77AFC472E262978F817" authority="Fuhrmann, 1916" authorityName="Fuhrmann" authorityYear="1916" box="[982,1285,1909,1935]" class="Cestoda" family="Monticelliidae" genus="Goezeella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Proteocephalidea" pageId="1" pageNumber="336" phylum="Platyhelminthes" rank="genus">
