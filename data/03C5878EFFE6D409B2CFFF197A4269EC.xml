<taxonomicName id="4C6C4D1BFFE6D409B375FF6C7D2C68C2" authority="Hoffmeister, 1843" authorityName="Hoffmeister" authorityYear="1843" box="[301,629,226,249]" class="Clitellata" family="Lumbriculidae" genus="Rhynchelmis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lumbriculida" pageId="3" pageNumber="4" phylum="Annelida" rank="species" species="limosella">
