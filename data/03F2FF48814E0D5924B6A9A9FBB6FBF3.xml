<taxonomicName id="4C5B35DD814C0D5925E2A8ABFDD5FC63" box="[451,603,980,1005]" class="Magnoliopsida" family="Urticaceae" genus="Pilea" kingdom="Plantae" order="Rosales" pageId="103" pageNumber="104" phylum="Tracheophyta" rank="species" species="rupipendia">
