<taxonomicName id="4C5B35DD814C0D592495AF1AFECFFBF3" authority="Green, 1901" authorityName="Green" authorityYear="1901" box="[180,321,1124,1149]" class="Insecta" family="Coccidae" genus="Antecerococcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="gallicolus">
