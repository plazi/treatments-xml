<taxonomicName id="4C5B35DD814C0D592556AF1AFD86FBF3" authority="Green, 1901" authorityName="Green" authorityYear="1901" box="[375,520,1124,1149]" class="Insecta" family="Coccidae" genus="Antecerococcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="philippiae">
<emphasis id="B92F924C814C0D592556AF1AFD86FBF3" box="[375,520,1124,1149]" italics="true" pageId="103" pageNumber="104">A. philippiae</emphasis>
