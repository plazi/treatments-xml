<quantity id="4CDF97E3D87CFFE8BDF7E6E62B3CFD51" box="[673,810,643,667]" metricMagnitude="2" metricUnit="m" metricValue="6.1341" metricValueMax="12.192" metricValueMin="0.0762" pageId="104" pageNumber="103" unit="ft" value="2012.5" valueMax="4000.0" valueMin="25.0">25–4000 ft.</quantity>
, Champion./ 
<taxonomicName id="4C274185D87CFFE8BC92E6E12D00FD51" authority="Gorh" authorityName="Gorham" authorityYear="1897" box="[964,1302,643,667]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="104" pageNumber="103" phylum="Arthropoda" rank="species" species="unipunctata">
