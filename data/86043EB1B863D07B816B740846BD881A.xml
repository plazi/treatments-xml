<taxonomicName id="B34645FC32B7939E015A06F0B1123488" class="Magnoliopsida" family="Moraceae" genus="Dorstenia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dorstenia" order="Rosales" pageId="2" pageNumber="51" phylum="Tracheophyta" rank="genus">
