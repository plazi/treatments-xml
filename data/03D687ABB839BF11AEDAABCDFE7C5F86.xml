and 
<taxonomicName id="4C7F4D3EB83ABF11AEB3ADA7FE855F86" box="[254,377,980,1004]" class="Malacostraca" family="Isaeidae" genus="Chevalia" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="27" phylum="Arthropoda" rank="species" species="pacifica">
<emphasis id="B90BEAAFB83ABF11AEB3ADA7FE855F86" box="[254,377,980,1004]" italics="true" pageId="2" pageNumber="27">C. pacifica</emphasis>
