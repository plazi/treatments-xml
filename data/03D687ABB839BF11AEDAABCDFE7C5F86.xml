<emphasis id="B90BEAAFB83ABF11AA3CADC2FB305FA2" bold="true" box="[1137,1228,944,968]" pageId="2" pageNumber="27">
<taxonomicNameLabel id="A23857D4B83ABF11AA3CADC2FB305FA2" box="[1137,1228,944,968]" pageId="2" pageNumber="27" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; and 
<taxonomicName id="4C7F4D3EB83ABF11AB46ADC2FA8F5FA2" box="[1291,1395,944,968]" class="Malacostraca" family="Isaeidae" genus="Chevalia" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="27" phylum="Arthropoda" rank="species" species="setosa" status="sp. nov.">
<emphasis id="B90BEAAFB83ABF11AB46ADC2FA8F5FA2" box="[1291,1395,944,968]" italics="true" pageId="2" pageNumber="27">C. setosa</emphasis>
</taxonomicName>
<emphasis id="B90BEAAFB83ABF11AB37ADC2FF345F86" bold="true" pageId="2" pageNumber="27">
<taxonomicNameLabel id="A23857D4B83ABF11AB37ADC2FF345F86" pageId="2" pageNumber="27" rank="species">sp. nov.</taxonomicNameLabel>
