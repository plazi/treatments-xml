<taxonomicName id="E7D44D1EFFB1FFBC08CEFF4CFD55FF4C" authority="Mergl, 1996" authorityName="Mergl" authorityYear="1996" box="[1009,1331,188,212]" class="Lingulata" family="Ephippelasmatidae" genus="Numericoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acrotretida" pageId="71" pageNumber="72" phylum="Brachiopoda" rank="species" species="vulcanogena">
