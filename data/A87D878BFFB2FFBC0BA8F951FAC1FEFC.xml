<emphasis id="12A0EA8FFFB0FFBD09FAF84FFA86F84D" box="[709,736,1982,2005]" italics="true" pageId="70" pageNumber="71">N.</emphasis>
? 
<taxonomicName id="E7D44D1EFFB0FFBD09CBF84FFB9BF84D" authority="(Biernat, 1973)" baseAuthorityName="Biernat" baseAuthorityYear="1973" box="[756,1021,1981,2005]" class="Lingulata" family="Acrotretidae" genus="Cyrtonotreta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acrotretida" pageId="70" pageNumber="71" phylum="Brachiopoda" rank="species" species="spinosa">
<emphasis id="12A0EA8FFFB0FFBD09CBF84FFB2CF84D" box="[756,842,1982,2005]" italics="true" pageId="70" pageNumber="71">spinosa</emphasis>
(
<bibRefCitation id="44454B6CFFB0FFBD0867F84CFB93F84D" author="Biernat, G." box="[856,1013,1981,2005]" pageId="70" pageNumber="71" pagination="1 - 120" refId="ref72933" refString="Biernat, G. (1973) Ordovician inarticulate brachiopods from Poland and Estonia. Palaeontologia Polonica, 28, 1 - 120, 40 pls." type="journal article" year="1973">Biernat, 1973</bibRefCitation>
)
</taxonomicName>
from the Dapingian and Darriwilian of 
<collectingCountry id="58C3760DFFB1FFBC0B8AFF66F963FF37" box="[181,261,151,175]" name="Poland" pageId="71" pageNumber="72">Poland</collectingCountry>
, South 
<collectingCountry id="58C3760DFFB1FFBC0A65FF66F9FBFF37" box="[346,413,151,175]" name="China" pageId="71" pageNumber="72">China</collectingCountry>
