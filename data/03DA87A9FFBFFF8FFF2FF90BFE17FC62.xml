<typeStatus id="54C8881DFFBFFF8EFEEBF8FCFE16F8E7" box="[339,448,1853,1879]" pageId="8" pageNumber="26">Holotype</typeStatus>
ɗ (
<collectionCode id="ED62AE7AFFBFFF8EFE51F8FCFD9CF8E7" LSID="urn:lsid:biocol.org:col:34988" box="[489,586,1853,1879]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="8" pageNumber="26">MNHN</collectionCode>
