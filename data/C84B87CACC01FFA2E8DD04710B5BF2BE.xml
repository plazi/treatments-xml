<taxonomicName id="87E24D5FCC01FFA2EB5407660C61F26A" box="[798,894,1824,1849]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="23" pageNumber="24" phylum="Cnidaria" rank="species" species="densa">S. densa</taxonomicName>
, 
<taxonomicName id="87E24D5FCC01FFA2EBC307660B04F26A" box="[905,1051,1824,1849]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="23" pageNumber="24" phylum="Cnidaria" rank="species" species="dichotoma">S. dichotoma</taxonomicName>
</emphasis>
and 
<taxonomicName id="87E24D5FCC01FFA2EC1B07660BFAF26A" box="[1105,1253,1825,1849]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="23" pageNumber="24" phylum="Cnidaria" rank="species" species="nonscripta">
