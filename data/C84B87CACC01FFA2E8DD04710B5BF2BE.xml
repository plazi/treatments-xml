<quantity id="871A9B39CC01FFA2EBEC06BB0B5BF247" box="[934,1092,1788,1813]" metricMagnitude="2" metricUnit="m" metricValue="2.82" metricValueMax="4.05" metricValueMin="1.59" pageId="23" pageNumber="24" unit="m" value="282.0" valueMax="405.0" valueMin="159.0">159 to 405 m</quantity>
, epibiotic on 
<taxonomicName id="87E24D5FCC01FFA2ECA806BB0FF4F26A" class="Hydrozoa" family="Sertulariidae" genus="Antarctoscyphus" kingdom="Animalia" order="Leptothecata" pageId="23" pageNumber="24" phylum="Cnidaria" rank="species" species="spiralis">
<emphasis id="7296EACECC01FFA2ECA806BB0FF4F26A" italics="true" pageId="23" pageNumber="24">Antarctoscyphus spiralis</emphasis>
</taxonomicName>
, 
<taxonomicName id="87E24D5FCC01FFA2E8B307670EE1F26A" box="[249,510,1824,1849]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="23" pageNumber="24" phylum="Cnidaria" rank="species" species="exaggeratum">
<emphasis id="7296EACECC01FFA2E8B307670EE1F26A" box="[249,510,1824,1849]" italics="true" pageId="23" pageNumber="24">Halecium exaggeratum</emphasis>
