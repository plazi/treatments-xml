<taxonomicName id="108A4D72713AFE9BFE91FD83FE3C1A02" authorityName="Machado &amp; Oswald" authorityYear="2020" box="[345,468,550,575]" class="Insecta" family="Myrmeleontidae" genus="Riekoleon" kingdom="Animalia" order="Neuroptera" pageId="313" pageNumber="314" phylum="Arthropoda" rank="species" species="squamosus">
<emphasis id="E5FEEAE3713AFE9BFE91FD83FE3C1A02" box="[345,468,550,575]" italics="true" pageId="313" pageNumber="314">squamosus</emphasis>
