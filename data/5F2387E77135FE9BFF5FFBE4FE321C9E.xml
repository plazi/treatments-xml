(
<figureCitation id="4FB12A747135FE94FF57FBC0FEE31C42" box="[159,267,1125,1151]" captionStart="FIGURE 162" captionStartId="303.[151,250,1271,1296]" captionTargetBox="[151,1434,181,1246]" captionTargetId="figure@303.[151,1435,181,1246]" captionTargetPageId="303" captionText="FIGURE 162. Distribution of Riekoleon species (part)." figureDoi="http://doi.org/10.5281/zenodo.3897152" httpUri="https://zenodo.org/record/3897152/files/figure.png" pageId="310" pageNumber="311">Figs. 162</figureCitation>
, 
<figureCitation id="4FB12A747135FE94FEDFFBC0FE921CBD" box="[279,378,1125,1152]" captionStart-0="FIGURE 167" captionStart-1="FIGURE 168" captionStartId-0="311.[151,250,1506,1531]" captionStartId-1="312.[151,250,1878,1903]" captionTargetBox-0="[151,1433,565,1470]" captionTargetBox-1="[151,1431,190,1852]" captionTargetId-0="figure@311.[151,1436,561,1481]" captionTargetId-1="figure@312.[151,1436,181,1852]" captionTargetPageId-0="311" captionTargetPageId-1="312" captionText-0="FIGURE 167. Riekoleon squamosus sp. nov.: a) head, anterior; b) abdomen base, lateral (note scale-like setae); c) head and thorax, dorsal; d) wings, dorsal." captionText-1="FIGURE 168. Riekoleon squamosus sp. nov.: female: a) terminalia, lateral; b) terminalia, ventral (note pregenital plate hidden beneath sternite). Scale bars = 1mm." figureDoi-0="http://doi.org/10.5281/zenodo.3897162" figureDoi-1="http://doi.org/10.5281/zenodo.3897164" httpUri-0="https://zenodo.org/record/3897162/files/figure.png" httpUri-1="https://zenodo.org/record/3897164/files/figure.png" pageId="310" pageNumber="311">167–168</figureCitation>
