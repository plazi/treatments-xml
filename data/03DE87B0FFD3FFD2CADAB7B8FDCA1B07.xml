<paragraph id="8BC836A6FFD3FFDCCADAB7D3FD4D1F7F" blockId="23.[151,1437,1275,2037]" pageId="23" pageNumber="424">
<emphasis id="B903EAB4FFD3FFDCCADAB7D3FEA51F53" bold="true" box="[151,290,1275,1300]" pageId="23" pageNumber="424">Dimensions</emphasis>
. 
<typeStatus id="54CC8804FFD3FFDCCB7CB7D3FE1E1F53" box="[305,409,1275,1300]" pageId="23" pageNumber="424">Holotype</typeStatus>
female length 
<number id="3DBF2481FFD3FFDCC809B7D3FDE91F53" box="[580,622,1275,1300]" pageId="23" pageNumber="424" value="515.0">515</number>
Μm, breadth 
<number id="3DBF2481FFD3FFDCC94AB7D4FCB61F53" box="[775,817,1276,1300]" pageId="23" pageNumber="424" value="237.0">237</number>
Μm; 
<typeStatus id="54CC8804FFD3FFDCC922B7D4FC481F53" box="[879,975,1276,1300]" pageId="23" pageNumber="424">paratype</typeStatus>
female length 
