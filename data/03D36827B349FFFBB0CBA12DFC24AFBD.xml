<tr id="354ADB73B34B0006B0FAA59AFA10AE9C" box="[166,1415,227,252]" gridrow="1" pageId="2" pageNumber="394">
<th id="769BB20FB34B0006B0FAA59AFD9FAE9C" box="[166,520,227,252]" gridcol="0" gridrow="1" pageId="2" pageNumber="394">Krauss’ organ of male</th>
<td id="769BB20FB34B0006B26EA59AFC1AAE9C" box="[562,909,227,252]" gridcol="1" gridrow="1" pageId="2" pageNumber="394">Without ramified keel</td>
<td id="769BB20FB34B0006B3E5A59AFA10AE9C" box="[953,1415,227,252]" gridcol="2" gridrow="1" pageId="2" pageNumber="394">With 3 ramified keels</td>
</tr>
<tr id="354ADB73B34B0006B0FAA470FA10AF42" box="[166,1415,265,290]" gridrow="2" pageId="2" pageNumber="394">
<th id="769BB20FB34B0006B0FAA470FD9FAF42" box="[166,520,265,290]" gridcol="0" gridrow="2" pageId="2" pageNumber="394">Tegmina of male</th>
<td id="769BB20FB34B0006B26EA470FC1AAF42" box="[562,909,265,290]" gridcol="1" gridrow="2" pageId="2" pageNumber="394">Narrower, length 2.5 times width</td>
<td id="769BB20FB34B0006B3E5A470FA10AF42" box="[953,1415,265,290]" gridcol="2" gridrow="2" pageId="2" pageNumber="394">Wider, length 1.8 times width,</td>
</tr>
<tr id="354ADB73B34B0006B0FAA457FA10AF27" box="[166,1415,302,327]" gridrow="3" pageId="2" pageNumber="394">
<th id="769BB20FB34B0006B0FAA457FD9FAF27" box="[166,520,302,327]" gridcol="0" gridrow="3" pageId="2" pageNumber="394">Cercus of male</th>
<td id="769BB20FB34B0006B26EA457FC1AAF27" box="[562,909,302,327]" gridcol="1" gridrow="3" pageId="2" pageNumber="394">Gently tapering at apical half part</td>
<td id="769BB20FB34B0006B3E5A457FA10AF27" box="[953,1415,302,327]" gridcol="2" gridrow="3" pageId="2" pageNumber="394">Suddenly tapering at apical half part</td>
</tr>
<tr id="354ADB73B34B0006B0FAA42DFA10AF0D" box="[166,1415,340,365]" gridrow="4" pageId="2" pageNumber="394">
<th id="769BB20FB34B0006B0FAA42DFD9FAF0D" box="[166,520,340,365]" gridcol="0" gridrow="4" pageId="2" pageNumber="394">Interspace of mesosternum in male</th>
<td id="769BB20FB34B0006B26EA42DFC1AAF0D" box="[562,909,340,365]" gridcol="1" gridrow="4" pageId="2" pageNumber="394">Minimum width 1.4 times length</td>
<td id="769BB20FB34B0006B3E5A42DFA10AF0D" box="[953,1415,340,365]" gridcol="2" gridrow="4" pageId="2" pageNumber="394">Minimum width shorter than length</td>
</tr>
<tr id="354ADB73B34B0006B0FAA403FA10AFF3" box="[166,1415,378,403]" gridrow="5" pageId="2" pageNumber="394">
<th id="769BB20FB34B0006B0FAA403FD9FAFF3" box="[166,520,378,403]" gridcol="0" gridrow="5" pageId="2" pageNumber="394">Epiphallus</th>
<td id="769BB20FB34B0006B26EA403FC1AAFF3" box="[562,909,378,403]" gridcol="1" gridrow="5" pageId="2" pageNumber="394">With 36 spikes</td>
<td id="769BB20FB34B0006B3E5A403FA10AFF3" box="[953,1415,378,403]" gridcol="2" gridrow="5" pageId="2" pageNumber="394">With 26 spikes</td>
</tr>
</table>
</paragraph>
