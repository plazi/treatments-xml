<paragraph id="41A907E88AEBC56ADBC35FD5C36B2FFE" pageId="0" pageNumber="1">
<taxonomicName id="67D097EEE08E974F94F656016EAD3723" authorityName="Wood &amp; Bright" authorityYear="1992" class="Insecta" family="Curculionidae" genus="Cryptoxyleborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cryptoxyleborus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Cryptoxyleborus</taxonomicName>
