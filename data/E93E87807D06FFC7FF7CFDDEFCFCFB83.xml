<emphasis id="53E3EA847D06FFC7FF7CFBF3FEBFFB82" box="[151,283,1062,1089]" italics="true" pageId="18" pageNumber="19">S. hyblensis</emphasis>
</taxonomicName>
<emphasis id="53E3EA847D06FFC7FEC9FBF2FEFEFB82" bold="true" box="[290,346,1062,1088]" pageId="18" pageNumber="19">
<taxonomicNameLabel id="48D057FF7D06FFC7FEC9FBF2FEFEFB82" box="[290,346,1062,1088]" pageId="18" pageNumber="19" rank="species">sp.n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="A6974D157D06FFC7FE83FBF3FDBAFB82" authority="Bello’ &amp; Osella &amp; Baviera, 2019" authorityName="Bello’ &amp; Osella &amp; Baviera" authorityYear="2019" box="[360,542,1062,1089]" class="Insecta" family="Curculionidae" genus="Solariola" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="paulimagrinii" status="sp.n.">
