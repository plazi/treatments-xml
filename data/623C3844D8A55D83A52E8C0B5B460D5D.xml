<taxonomicName id="1AAAE472D5A979A19EA2783593B3511F" class="Scyphozoa" family="Nausithoidae" genus="Nausithoe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nausithoe" order="Coronatae" pageId="0" pageNumber="1" phylum="Cnidaria" rank="genus">
