der 
<taxonomicName authority="L." class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="665" phylum="Tracheophyta" rank="species" species="cyparissias">
