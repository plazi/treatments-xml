<subSubSection box="[314,865,1222,1244]" pageId="12" pageNumber="231" type="nomenclature">
<paragraph blockId="12.[314,865,1222,1244]" box="[314,865,1222,1244]" pageId="12" pageNumber="231">
<heading box="[314,865,1222,1244]" centered="true" fontSize="9" level="2" pageId="12" pageNumber="231" reason="2">
<taxonomicName authority="Romero and Johnson" authorityName="Romero and Johnson" box="[314,735,1223,1244]" class="Insecta" family="Chrysomelidae" genus="Neltumius" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="231" phylum="Arthropoda" rank="species" species="dospatrias" status="new species">
<emphasis box="[314,518,1223,1244]" italics="true" pageId="12" pageNumber="231">Neltumius dospatrias</emphasis>
Romero and Johnson
</taxonomicName>
, 
<emphasis bold="true" box="[746,865,1222,1243]" pageId="12" pageNumber="231">
<taxonomicNameLabel box="[746,865,1222,1243]" pageId="12" pageNumber="231" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
