<emphasis bold="true" box="[151,379,1720,1744]" pageId="42" pageNumber="43">FIGURES 102–111:</emphasis>
<taxonomicName authority="Kejval, 2009" authorityName="Kejval" authorityYear="2009" box="[387,613,1720,1743]" class="Insecta" family="Anthicidae" genus="Chileanthicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="discus" status="sp. nov.">
<emphasis box="[387,613,1720,1743]" italics="true" pageId="42" pageNumber="43">Chileanthicus discus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[621,709,1720,1744]" pageId="42" pageNumber="43">
<taxonomicNameLabel box="[621,709,1720,1744]" pageId="42" pageNumber="43" rank="species">sp. nov.</taxonomicNameLabel>
