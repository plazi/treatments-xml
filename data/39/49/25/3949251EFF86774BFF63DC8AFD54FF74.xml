(
<typeStatus box="[577,681,1656,1682]" pageId="43" pageNumber="44">holotype</typeStatus>
<quantity box="[689,784,1656,1682]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="43" pageNumber="44" unit="mm" value="2.7">2.7 mm</quantity>
