<collectingCountry box="[364,471,1582,1608]" name="Australia" pageId="65" pageNumber="66">Australia</collectingCountry>
(
<collectingRegion box="[487,678,1582,1608]" country="Australia" name="South Australia" pageId="65" pageNumber="66">South Australia</collectingRegion>
).
</paragraph>
