[
<collectionCode box="[894,916,873,899]" name="Tavera, Department of Geology and Geophysics" pageId="54" pageNumber="55">T</collectionCode>
.] fastigiatus 
<emphasis italics="true" pageId="54" pageNumber="55">Marseul and probably stands close to</emphasis>
[
<collectionCode box="[502,524,913,939]" name="Tavera, Department of Geology and Geophysics" pageId="54" pageNumber="55">T</collectionCode>
