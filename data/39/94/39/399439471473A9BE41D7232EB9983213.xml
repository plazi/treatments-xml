<normalizedToken originalValue="Körperform">Koerperform</normalizedToken>
ausgezeichnete 
<taxonomicName class="Insecta" family="Asilidae" genus="Laphria" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="19" phylum="Arthropoda" rank="genus">Laphria</taxonomicName>
