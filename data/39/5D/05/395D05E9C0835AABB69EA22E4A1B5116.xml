<geoCoordinate degrees="22" direction="north" minutes="34.365" orientation="latitude" precision="1" value="22.57275">22°34.365'N</geoCoordinate>
, 
<geoCoordinate degrees="114" direction="east" minutes="0.400" orientation="longitude" precision="1" value="114.00667">114°0.400'E</geoCoordinate>
