<emphasis box="[196,209,636,659]" italics="true" pageId="5" pageNumber="2316">p</emphasis>
5100, 
<emphasis box="[288,301,636,659]" italics="true" pageId="5" pageNumber="2316">p</emphasis>
95(65–)67, 
