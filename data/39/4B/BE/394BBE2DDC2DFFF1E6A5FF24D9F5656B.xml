<emphasis bold="true" box="[151,281,787,812]" pageId="5" pageNumber="141">FIGURE 24</emphasis>
. 
<emphasis bold="true" box="[294,584,787,812]" pageId="5" pageNumber="141">
<taxonomicName authorityName="Balasubramanian &amp; Muthukatturaja" authorityYear="2019" box="[294,528,787,812]" class="Insecta" family="Leptophlebiidae" genus="Thraulus" kingdom="Animalia" order="Ephemeroptera" pageId="5" pageNumber="141" phylum="Arthropoda" rank="species" species="thiagarajani">
<emphasis bold="true" box="[294,528,787,812]" italics="true" pageId="5" pageNumber="141">Thraulus thiagarajani</emphasis>
