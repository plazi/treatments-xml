<subSubSection pageId="1" pageNumber="185" type="reference_group">
<paragraph blockId="1.[151,1435,544,596]" pageId="1" pageNumber="185">
<treatmentCitationGroup pageId="1" pageNumber="185">
<taxonomicName authority="Wygodzinsky, 1966: 129" authorityName="Wygodzinsky" authorityPageNumber="129" authorityYear="1966" box="[151,538,544,566]" class="Insecta" family="Reduviidae" genus="Proguithera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="185" phylum="Arthropoda" rank="genus">
<emphasis box="[151,274,544,566]" italics="true" pageId="1" pageNumber="185">Proguithera</emphasis>
<treatmentCitation author="Wygodzinsky" box="[282,538,544,566]" page="129" pageId="1" pageNumber="185" year="1966">
<bibRefCitation author="Wygodzinsky" box="[282,488,544,566]" pageId="1" pageNumber="185" refString="Wygodzinsky, P. W. (1966) A monograph of the Emesinae (Reduviidae, Hemiptera). Bulletin of the American Museum of Natural History, 133, 1 - 614." type="journal article" year="1966">Wygodzinsky, 1966</bibRefCitation>
