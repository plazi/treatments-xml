<paragraph blockId="30.[151,1436,1250,1311]" pageId="30" pageNumber="431">
<taxonomicName box="[151,223,1250,1275]" class="Insecta" family="Mantispidae" genus="Buyda" kingdom="Animalia" order="Neuroptera" pageId="30" pageNumber="431" phylum="Arthropoda" rank="genus">
