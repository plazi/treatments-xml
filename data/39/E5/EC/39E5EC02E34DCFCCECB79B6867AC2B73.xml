<taxonomicName class="Insecta" family="Delphacidae" genus="Peregrinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Peregrinus maidis" order="Hemiptera" pageId="32" pageNumber="77" phylum="Arthropoda" rank="species" species="maidis">Peregrinus maidis</taxonomicName>
