<taxonomicName class="Insecta" family="Delphacidae" genus="Nycheuma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nycheuma endymion" order="Hemiptera" pageId="32" pageNumber="77" phylum="Arthropoda" rank="species" species="endymion">Nycheuma endymion</taxonomicName>
