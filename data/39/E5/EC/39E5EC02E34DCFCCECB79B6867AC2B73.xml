<taxonomicName class="Insecta" family="Delphacidae" genus="Metadelphax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metadelphax propinqua" order="Hemiptera" pageId="32" pageNumber="77" phylum="Arthropoda" rank="species" species="propinqua">Metadelphax propinqua</taxonomicName>
