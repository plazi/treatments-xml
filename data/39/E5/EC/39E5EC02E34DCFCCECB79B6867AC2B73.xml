<taxonomicName family="Delphacidae" lsidName="" pageId="32" pageNumber="77" rank="family">Delphacidae</taxonomicName>
<taxonomicName lsidName="" pageId="32" pageNumber="77" rank="subfamily" subfamily="Delphacinae">Delphacinae</taxonomicName>
