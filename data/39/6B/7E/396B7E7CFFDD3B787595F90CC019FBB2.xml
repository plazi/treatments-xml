<collectingCountry box="[671,789,549,575]" name="Bolivia" pageId="33" pageNumber="34">BOLIVIA</collectingCountry>
, 
<collectingRegion box="[800,951,549,575]" country="Bolivia" name="Cochabamba" pageId="33" pageNumber="34">Cochabamba</collectingRegion>
: Ioari, 
<quantity box="[1030,1117,549,575]" metricMagnitude="3" metricUnit="m" metricValue="3.4" pageId="33" pageNumber="34" unit="m" value="3400.0">3400 m</quantity>
, 
<date box="[1130,1233,549,575]" pageId="33" pageNumber="34" value="1946-02-07">7.ii.1946</date>
. L. E. Pena. 
<typeStatus pageId="33" pageNumber="34">Paratype</typeStatus>
male: 
<collectingCountry box="[275,393,585,611]" name="Bolivia" pageId="33" pageNumber="34">BOLIVIA</collectingCountry>
, 
