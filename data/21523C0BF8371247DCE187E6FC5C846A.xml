<taxonomicName id="6EFBF69EF8371247DCE187E6FD23868F" authority="Priesner 1933: 195" authorityName="Priesner" authorityPageNumber="195" authorityYear="1933" box="[151,740,458,484]" class="Insecta" family="Thripidae" genus="Scolothrips" kingdom="Animalia" order="Thysanoptera" pageId="6" pageNumber="51" phylum="Arthropoda" rank="species" species="quadrimaculatus">
