<taxonomicName id="47E608F1E85AFA037A7BC81002778DE8" authorityName="Lee &amp; Yoo &amp; Kim" authorityYear="2021" box="[535,847,1649,1675]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="466" phylum="Arthropoda" rank="species" species="pseudoauriculatus">
