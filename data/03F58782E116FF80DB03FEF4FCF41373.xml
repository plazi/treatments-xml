<taxonomicName id="4C5C4D17E116FF81D9FFFB75FADB11C0" authority="Fennah, 1958" authorityName="Fennah" authorityYear="1958" box="[1012,1318,1181,1202]" class="Insecta" family="Cixiidae" genus="Oliarus" kingdom="Animalia" order="Hemiptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="draconarius">
