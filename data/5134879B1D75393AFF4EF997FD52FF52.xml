<emphasis id="EBE9EA9F1D753938FF4EF997FDEFF9E4" bold="true" box="[151,574,1588,1614]" pageId="8" pageNumber="9">
Genus 
<taxonomicName id="1E9D4D0E1D753938FF37F997FDEFF9E4" authority="Warren, 1897" authorityName="Warren" authorityYear="1897" box="[238,574,1588,1614]" class="Insecta" family="Geometridae" genus="Prasinocyma" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
