<taxonomicName id="3D89E0B275BBA028925477CEDF5A9916" class="Gastropoda" family="Plectopylidae" genus="Endothyrella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Endothyrella oglei" order="Stylommatophora" pageId="10" pageNumber="11" phylum="Mollusca" rank="species" species="oglei">Endothyrella oglei</taxonomicName>
