<taxonomicName id="7876A7B363D4FBCB4E2BC2BCA463996A" class="Gastropoda" family="Plectopylidae" genus="Endothyrella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Endothyrella serica" order="Stylommatophora" pageId="10" pageNumber="11" phylum="Mollusca" rank="species" species="serica">Endothyrella serica</taxonomicName>
