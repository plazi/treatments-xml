<emphasis id="B966EA83FFEDFFADFED5FC50FE2AFBD1" bold="true" box="[264,387,998,1024]" italics="true" pageId="5" pageNumber="56">Atopsyche</emphasis>
(
<emphasis id="B966EA83FFEDFFADFE48FC50FDBBFBD1" bold="true" box="[405,530,998,1024]" italics="true" pageId="5" pageNumber="56">Atopsaura</emphasis>
) 
