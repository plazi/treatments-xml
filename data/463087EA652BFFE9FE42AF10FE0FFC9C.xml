<taxonomicName id="09994D7F652BFFE7FE96AC86FD67F97D" authority="Gir." authorityName="Gir." authorityYear="1915" box="[382,686,1669,1694]" class="Insecta" family="Encyrtidae" genus="Arhopoideus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="687" phylum="Arthropoda" rank="species" species="secundus">
