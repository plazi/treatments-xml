<paragraph id="1A5721BAA70202484D9D4A893A3E57F4" pageId="0" pageNumber="19">
<taxonomicName id="4FD8E40CAD016198110A4F1129F83C05" class="Insecta" family="Elcanidae" genus="Parelcana" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parelcana tenuis" order="Orthoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="species" species="tenuis">
