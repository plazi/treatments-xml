<collectingCountry id="F32D14BFE31D4B47FEBFFDE9FE76FD5E" box="[355,430,639,665]" name="Belize" pageId="5" pageNumber="6">Belize</collectingCountry>
(
<bibRefCitation id="EFAB29DEE31D4B47FE18FDE9FDA0FD5E" author="Thomas" box="[452,632,639,665]" pageId="5" pageNumber="16" refString="Thomas, J. D. (1993). Identification manual for marine Amphipoda (Gammaridea): I. Common coral reef and rocky bottom amphipods of South Florida. Final Report, Contract No. SP 290, Florida Department of Environmental Protection, Tallahassee, Florida. 83. pp." type="journal article" year="1993">
Thomas, 
<number id="3DF24608E31D4B47FDE5FDE9FDA0FD5E" box="[569,632,639,665]" pageId="5" pageNumber="6" value="1993.0">1993</number>
</bibRefCitation>
); eastward to Hawaii, 
<collectingCountry id="F32D14BFE31D4B47FC45FDE9FBC5FD5E" box="[921,1053,639,665]" name="Micronesia" pageId="5" pageNumber="6">Micronesia</collectingCountry>
(
<bibRefCitation id="EFAB29DEE31D4B47FBEFFDE9FB30FD5E" author="Barnard" box="[1075,1256,639,665]" pageId="5" pageNumber="13" refString="Barnard, J. L. (1970) Sublittoral Gammaridea (Amphipoda) of the Hawaiian Islands. Smithsonian Contributions to Zoology 34, 1 - 286." type="journal article" year="1970">
Barnard, 
<number id="3DF24608E31D4B47FB75FDE9FB30FD5E" box="[1193,1256,639,665]" pageId="5" pageNumber="6" value="1970.0">1970</number>
</bibRefCitation>
), 
<collectingCountry id="F32D14BFE31D4B47FADFFDE9FAF4FD5E" box="[1283,1324,639,665]" name="Fiji" pageId="5" pageNumber="6">Fiji</collectingCountry>
Island (
<bibRefCitation id="EFAB29DEE31D4B47FEBAFD31FDDEFD06" author="Myers" box="[358,518,679,705]" pageId="5" pageNumber="15" refString="Myers, A. A. (1985) Shallow water, coral reef and mangrove Amphipoda (Gammaridea) of Fiji. Records of the Australian Museum, Supplement 5, 143 pp." type="book" year="1985">
Myers, 
<number id="3DF24608E31D4B47FE1BFD31FDDEFD06" box="[455,518,679,705]" pageId="5" pageNumber="6" value="1985.0">1985</number>
</bibRefCitation>
); 
<collectingCountry id="F32D14BFE31D4B47FDFDFD31FCC8FD06" box="[545,784,679,705]" name="Papua New Guinea" pageId="5" pageNumber="6">Papua New Guinea</collectingCountry>
(
<bibRefCitation id="EFAB29DEE31D4B47FCEFFD31FC09FD06" author="Myers" box="[819,977,679,705]" pageId="5" pageNumber="15" refString="Myers, A. A. (1995) The Amphipoda (Crustacea) of Madang Lagoon: Aoridae, Isaeidae, Ischyroceridae and Neomegamphopidae, Records of the Australian Museum, Supplement 22, 26 - 95." type="book chapter" year="1995">
Myers, 
<number id="3DF24608E31D4B47FC49FD31FC09FD06" box="[917,977,679,705]" pageId="5" pageNumber="6" value="1995.0">1995</number>
</bibRefCitation>
) and 
<collectingCountry id="F32D14BFE31D4B47FBC2FD31FB74FD06" box="[1054,1196,679,705]" name="Madagascar" pageId="5" pageNumber="6">Madagascar</collectingCountry>
(
<bibRefCitation id="EFAB29DEE31D4B47FB1EFD31FE9FFD2E" author="Ledoyer" pageId="5" pageNumber="14" refString="Ledoyer, M. (1982) Crustaces amphipodes gammariens. Familles des Acanthonotozomatidae a Gammaridae. Faune de Madagascar, 59, 1 - 598." type="journal article" year="1982">
Ledoyer, 
<number id="3DF24608E31D4B47FED4FD59FE9FFD2E" box="[264,327,719,745]" pageId="5" pageNumber="6" value="1982.0">1982</number>
