also 
<taxonomicName id="4C204D1298545A56DB44FCA8D114FCDC" authority="(A. Smith, 1833)" baseAuthorityName="A. Smith" baseAuthorityYear="1833" box="[293,769,802,828]" class="Mammalia" family="Vespertilionidae" genus="Miniopterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="72" pageNumber="73" phylum="Chordata" rank="species" species="natalensis">
<emphasis id="B954EA8398545A56DB44FCA8D026FCDC" box="[293,563,802,828]" italics="true" pageId="72" pageNumber="73">Miniopterus natalensis</emphasis>
(A. Smith, 1833)
</taxonomicName>
below.
