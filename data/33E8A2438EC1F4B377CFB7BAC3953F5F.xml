<taxonomicName id="696281D448314056B84BE0132A078EFB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:140843" class="Insecta" family="Formicidae" genus="Eciton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Eciton (Acamatus) impudens Mann" order="Hymenoptera" pageNumber="413" phylum="Arthropoda" rank="species" species="impudens" subGenus="Acamatus">Eciton (Acamatus) impudens Mann</taxonomicName>
