<taxonomicName id="4C3B1A8BFF9FF91BFBB6FDD2FB51FD9C" box="[1065,1227,536,562]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="13" pageNumber="210" phylum="Tracheophyta" rank="species" species="undetermined">
