<location id="8EFA603DFFF7863FFC97D487FC23FD6E" LSID="urn:lsid:plazi:treatment:038C87F0FFF7863CFF1ED4D7FE5FFBF3:8EFA603DFFF7863FFC97D487FC23FD6E" box="[827,977,714,734]" country="Honduras" name="Palo de Comba" pageId="1" pageNumber="602" stateProvince="Yoro">
