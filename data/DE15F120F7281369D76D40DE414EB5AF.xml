<taxonomicName id="825263412A1E979EE42977761D46A905" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27219" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus sylvaticus (Olivier)" order="Hymenoptera" pageId="2" pageNumber="375" phylum="Arthropoda" rank="species" species="sylvaticus">
