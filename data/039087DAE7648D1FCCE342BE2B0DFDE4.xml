<quantity id="4CC19B29E7638D1ECCE344AF2810FB13" box="[455,521,1154,1178]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="4" unit="mm" value="1.0">1 mm</quantity>
