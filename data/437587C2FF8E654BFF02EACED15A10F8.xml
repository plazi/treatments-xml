<taxonomicName id="0CDC4D57FF8E654BFEE3EB3BD606171B" authority="Pfr" authorityName="Pfeiffer" authorityYear="1857" box="[374,556,867,894]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="74" pageNumber="75" phylum="Mollusca" rank="species" species="sciadium">H. sciadium Pfr</taxonomicName>
