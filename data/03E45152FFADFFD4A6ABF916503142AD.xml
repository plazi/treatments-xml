1.2.9. 
<taxonomicName id="4C4D9BC7FFADFFD4A6E1F916573A4383" authorityName="Tsacas" authorityYear="1974" box="[221,517,1714,1739]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="stuckenbergi">
<emphasis id="B9393C56FFADFFD4A6E1F916573A4383" bold="true" box="[221,517,1714,1739]" italics="true" pageId="50" pageNumber="51">Curtonotum stuckenbergi</emphasis>
</taxonomicName>
