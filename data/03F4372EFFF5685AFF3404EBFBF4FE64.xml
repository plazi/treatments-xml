<quantity id="4CA52BDDFFF5685DFB1C073DFAFFF94D" box="[1215,1315,1644,1669]" metricMagnitude="-2" metricUnit="m" metricValue="1.61" pageId="19" pageNumber="420" unit="mm" value="16.1">16.1 mm</quantity>
