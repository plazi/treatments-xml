<taxonomicName id="571863A2B2D0DE43365315F2931CF253" LSID="http://zoobank.org/AC0F5EA3-4526-43FD-85F3-4544FFC0EF5E" authority="Fisher" authorityName="Fisher" authorityYear="2015" class="Insecta" family="Braconidae" genus="Colemanus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Colemanus keeleyorum" order="Hymenoptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="keeleyorum">Colemanus keeleyorum Fisher</taxonomicName>
