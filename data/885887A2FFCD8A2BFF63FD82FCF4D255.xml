<taxonomicName id="C7F14D37FFCD8A2BFA85FBACF9B6DD57" authorityName="Kock, Csorba &amp; K M. Howell" authorityYear="2000" box="[1353,1587,1037,1076]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="2" pageNumber="281" phylum="Chordata" rank="species" species="maendeleo">
