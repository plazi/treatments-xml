<collectingCountry id="59A6A4E19B13FFB0907AFCF34D2D1E19" box="[531,607,790,811]" name="Mexico" pageId="20" pageNumber="21">Mexico</collectingCountry>
, 
<collectingCountry id="59A6A4E19B13FFB09001FCF34DE81E19" box="[616,666,790,811]" name="Cuba" pageId="20" pageNumber="21">Cuba</collectingCountry>
.............................................. 
