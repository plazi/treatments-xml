</taxonomicName>
[
<taxonomicName id="4C554D537B2FFFB0FBFCF8D3FB53DF0D" box="[1104,1197,1795,1819]" class="Insecta" family="Nymphalidae" genus="Speyeria" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="28" phylum="Arthropoda" rank="species" species="atlantis">
