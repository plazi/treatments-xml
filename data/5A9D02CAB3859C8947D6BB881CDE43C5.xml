Genus 
<taxonomicName id="4CDCD982EF970EFFD4076FAABA540893" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2389" class="Insecta" family="Formicidae" genus="Cyphomyrmex" kingdom="Animalia" lsidName-HNS="Cyphomyrmex Mayr" order="Hymenoptera" pageNumber="32" phylum="Arthropoda" rank="genus">Cyphomyrmex Mayr</taxonomicName>
