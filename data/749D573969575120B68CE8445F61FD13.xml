<paragraph id="869C7E87F10F40772C2A563FF2FEA848" pageId="0" pageNumber="48907">
<taxonomicName id="60A6B369F8A9ED11972D2825FB75DD7C" authority="Fleming &amp; Wood, 2020" authorityName="Fleming &amp; Wood" authorityYear="2020" class="Insecta" family="Tachinidae" genus="Chorotegamyia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chorotegamyia" order="Diptera" pageId="0" pageNumber="48907" phylum="Arthropoda" rank="genus">Chorotegamyia</taxonomicName>
<taxonomicName id="2313D91AF62248AFF0FE8D572832B851" authority="Fleming &amp; Wood, 2020" authorityName="Fleming &amp; Wood" authorityYear="2020" class="Insecta" family="Tachinidae" genus="Chorotegamyia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chorotegamyia aureofacies" order="Diptera" pageId="0" pageNumber="48907" phylum="Arthropoda" rank="species" species="aureofacies">Chorotegamyia aureofacies</taxonomicName>
