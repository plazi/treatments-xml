<taxonomicName id="BBBA4D1FFFC2381C837CFC6733B5736A" authorityName="Hausmann" authorityYear="1991" box="[266,392,1006,1032]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="muelleri">
<emphasis id="4ECEEA8EFFC2381C837CFC6733B5736A" box="[266,392,1006,1032]" italics="true" pageId="14" pageNumber="15">N. muelleri</emphasis>
