<taxonomicName id="BBBA4D1FFFC2381C808CFBF7314A73FA" box="[762,887,1150,1176]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="waltheri">
<emphasis id="4ECEEA8EFFC2381C808CFBF7314A73FA" box="[762,887,1150,1176]" italics="true" pageId="14" pageNumber="15">N. waltheri</emphasis>
</taxonomicName>
