<taxonomicName id="BBBA4D1FFFC2381C838FFBD330857316" authorityName="Wagner" authorityYear="1919" box="[505,696,1114,1140]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="palaestinensis">
