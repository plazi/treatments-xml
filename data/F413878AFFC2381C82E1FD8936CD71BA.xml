<taxonomicName id="BBBA4D1FFFC2381C82E1FB2B332B73DE" authorityName="Hausmann" authorityYear="1991" box="[151,278,1186,1212]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="muelleri">
<emphasis id="4ECEEA8EFFC2381C82E1FB2B332B73DE" box="[151,278,1186,1212]" italics="true" pageId="14" pageNumber="15">N. muelleri</emphasis>
</taxonomicName>
; ampulla superior narrowed at the centre in 
<taxonomicName id="BBBA4D1FFFC2381C8178FB2B31F273DE" authorityName="Wagner" authorityYear="1919" box="[782,975,1186,1212]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="palaestinensis">
<emphasis id="4ECEEA8EFFC2381C8178FB2B31F273DE" box="[782,975,1186,1212]" italics="true" pageId="14" pageNumber="15">N. palaestinensis</emphasis>
</taxonomicName>
