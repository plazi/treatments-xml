<emphasis id="4ECEEA8EFFC2381C873CFABB32F17212" italics="true" pageId="14" pageNumber="15">N. waltheri</emphasis>
</taxonomicName>
; 1/ 
<quantity id="BB429B79FFC2381C8298FADE33277212" box="[238,282,1366,1392]" metricMagnitude="-1" metricUnit="m" metricValue="1.016" pageId="14" pageNumber="15" unit="in" value="4.0">4 in</quantity>
<taxonomicName id="BBBA4D1FFFC2381C8357FADF33DD7212" authorityName="Wagner" authorityYear="1919" box="[289,480,1366,1392]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="palaestinensis">
