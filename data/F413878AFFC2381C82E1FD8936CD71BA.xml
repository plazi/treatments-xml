<taxonomicName id="BBBA4D1FFFC2381C8129FBD331D67316" authorityName="Hausmann &amp; Wimmer" authorityYear="1994" box="[863,1003,1114,1140]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="aphrodite">
<emphasis id="4ECEEA8EFFC2381C8129FBD331D67316" box="[863,1003,1114,1140]" italics="true" pageId="14" pageNumber="15">N. aphrodite</emphasis>
