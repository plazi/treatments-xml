, 
<taxonomicName id="BBBA4D1FFFC2381C86BBFAF337B172F6" authorityName="Wagner" authorityYear="1919" box="[1229,1420,1402,1428]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="palaestinensis">
