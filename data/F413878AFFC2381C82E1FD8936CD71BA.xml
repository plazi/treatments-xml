<taxonomicName id="BBBA4D1FFFC2381C8654FA1736DC72DA" authorityName="Wagner" authorityYear="1919" box="[1058,1249,1438,1464]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="palaestinensis">
<emphasis id="4ECEEA8EFFC2381C8654FA1736DC72DA" box="[1058,1249,1438,1464]" italics="true" pageId="14" pageNumber="15">N. palaestinensis</emphasis>
