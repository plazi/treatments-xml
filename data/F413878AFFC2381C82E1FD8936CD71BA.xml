<taxonomicName id="BBBA4D1FFFC2381C81CEFAF3360872F6" box="[952,1077,1402,1428]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="waltheri">
<emphasis id="4ECEEA8EFFC2381C81CEFAF3360872F6" box="[952,1077,1402,1428]" italics="true" pageId="14" pageNumber="15">N. waltheri</emphasis>
</taxonomicName>
, 
<taxonomicName id="BBBA4D1FFFC2381C8634FAF336FD72F6" authorityName="Hausmann" authorityYear="1991" box="[1090,1216,1402,1428]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="muelleri">
<emphasis id="4ECEEA8EFFC2381C8634FAF336FD72F6" box="[1090,1216,1402,1428]" italics="true" pageId="14" pageNumber="15">N. muelleri</emphasis>
