<taxonomicName id="BBBA4D1FFFC2381C8670FB2B36AE73DE" authorityName="Hausmann &amp; Wimmer" authorityYear="1994" box="[1030,1171,1186,1212]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="aphrodite">
<emphasis id="4ECEEA8EFFC2381C8670FB2B36AE73DE" box="[1030,1171,1186,1212]" italics="true" pageId="14" pageNumber="15">N. aphrodite</emphasis>
