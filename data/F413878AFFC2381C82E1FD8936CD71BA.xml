<taxonomicName id="BBBA4D1FFFC2381C8333FBD333FE7316" authorityName="Hausmann" authorityYear="1991" box="[325,451,1114,1140]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="muelleri">
