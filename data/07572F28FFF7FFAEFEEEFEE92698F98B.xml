<taxonomicName id="48FEE5BDFFF7FFA0FED2FEE924E3FE82" box="[308,481,353,379]" class="Insecta" family="Baetidae" genus="Potamocloeon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
