<paragraph id="D17E02402A0B3F52E9EA58B1168B6C98" pageId="24" pageNumber="79">
<taxonomicName id="B66E40E58BB26C147CC40F38EDBE2462" class="Insecta" family="Rhinotermitidae" genus="Prorhinotermes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Prorhinotermes inopinatus" order="Blattodea" pageId="24" pageNumber="79" phylum="Arthropoda" rank="species" species="inopinatus">Prorhinotermes inopinatus</taxonomicName>
