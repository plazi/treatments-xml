<paragraph id="AEC1EEA92366F83AB44F760A3F681E2A" pageId="0" pageNumber="65">
<taxonomicName id="46E757C535BE14AF8505DC23344F9CE9" class="Dicotyledoneae" family="Leguminosae" genus="Glycyrrhiza" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Glycyrrhiza" order="Fabales" pageId="0" pageNumber="65" phylum="Angiospermae" rank="genus">
