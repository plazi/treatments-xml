<collectingRegion id="49A9A6CC34643F5CFB64FA78FAAC620A" box="[1183,1383,1041,1065]" country="Brazil" name="Rio Grande do Sul" pageId="30" pageNumber="223">Rio Grande do Sul</collectingRegion>
