<number id="3D8724DAFFDA85577F00FE55FCD9FE07" box="[836,865,402,427]" pageId="12" pageNumber="335" value="69.0">69</number>
<taxonomicName id="4C4F4D7EFFDA85577F2CFE55FC4AFE07" box="[872,1010,402,427]" class="Insecta" family="Lycaenidae" genus="Thecla" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="335" phylum="Arthropoda" rank="species" species="arria">Thecla arria</taxonomicName>
