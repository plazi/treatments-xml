<taxonomicName id="973DCB4274033EE9D68C51E05CFDEDA3" authorityName="Mill" authorityYear="1768" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus elegantissimus" order="Solanales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="elegantissimus">
