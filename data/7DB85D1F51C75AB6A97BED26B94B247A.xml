<bibRefCitation id="9A421FD8564D828E2D7A9951215673CA" author="Tohme, G" journalOrPublisher="National Council for Scientific Research, Beirut" pageId="213" pageNumber="214" pagination="1 - 609" refId="B363" refString="Tohme, G, Tohme, H, 2007. Illustrated Flora of Lebanon. National Council for Scientific Research, Beirut: 1 - 609" title="Illustrated Flora of Lebanon" year="2007">
<normalizedToken id="EF0DC9F5933E96D1D199CCF1AE089201" originalValue="TohmÃ©">Tohme</normalizedToken>
and 
<normalizedToken id="3FA1A14887D785965B661F19FF4A6BA5" originalValue="TohmÃ©">Tohme</normalizedToken>
2007
</bibRefCitation>
: 215 (photo as 
<taxonomicName id="486381A0CA562C1D9288B81BFFEEDB8A" authorityName="Mill" authorityYear="1768" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus elegantissimus" order="Solanales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="elegantissimus">
<emphasis id="8479AB4D23EE488617B4493285D2A1CF" italics="true" pageId="65" pageNumber="66">
<pageBreakToken id="45EED7D1EF040D25B50698A6A0EED907" pageId="65" pageNumber="66" start="start">Convolvulus</pageBreakToken>
