<taxonomicName id="4C3CDAC2FFE6CA46FF6A109DFCE4FF62" authority="Takaoka &amp; Davies, 1995" authorityName="Takaoka &amp; Davies" authorityYear="1995" box="[192,880,151,176]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="grossifilum" subGenus="Simulium">
