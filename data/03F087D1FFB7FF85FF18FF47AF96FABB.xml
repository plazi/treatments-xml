<taxonomicName id="4C594D44FFB7FF85FEC0FB57AD63FB2B" authority="(Kurz) A. N. Egan &amp; B. Pan" authorityName="A. N. Egan &amp; B. Pan" baseAuthorityName="Kurz" box="[334,848,1166,1193]" class="Magnoliopsida" family="Fabaceae" genus="Teyleria" kingdom="Plantae" order="Fabales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="stricta">
<emphasis id="B92DEAD5FFB7FF85FEC0FB57AFC8FB2A" box="[334,507,1166,1192]" italics="true" pageId="16" pageNumber="17">Teyleria stricta</emphasis>
(Kurz) A. N. Egan &amp; B. Pan
</taxonomicName>
<emphasis id="B92DEAD5FFB7FF85FCD7FB57AD49FB2A" box="[857,890,1166,1192]" italics="true" pageId="16" pageNumber="17">bis</emphasis>
(2015: 208), synonymized 
<taxonomicName id="4C594D44FFB7FF85FB37FB56AFEDFB4E" authority="(Backer 1911: 358) Backer (1939: 108)" authorityName="Backer" authorityPageNumber="108" authorityYear="1939" baseAuthorityName="Backer" baseAuthorityPageNumber="358" baseAuthorityYear="1911" class="Magnoliopsida" family="Fabaceae" genus="Teyleria" kingdom="Plantae" order="Fabales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="koordersii">
<emphasis id="B92DEAD5FFB7FF85FB37FB56AB7AFB2A" box="[1209,1353,1166,1192]" italics="true" pageId="16" pageNumber="17">T. koordersii</emphasis>
(
<bibRefCitation id="EFC84B36FFB7FF85FAD4FB57AECBFB4F" author="Backer, C. A." pageId="16" pageNumber="17" refId="ref11653" refString="Backer, C. A. (1911) Schoolflora voor Java. Batavia. G. Kolff &amp; Co., 112 pp." type="book" year="1911">Backer 1911: 358</bibRefCitation>
) 
<bibRefCitation id="EFC84B36FFB7FF85FE88FB6BAFEDFB4E" author="Backer, C. A." box="[262,478,1202,1228]" pageId="16" pageNumber="17" pagination="107 - 110" refId="ref11678" refString="Backer, C. A. (1939) Notes on the flora of Java I. Bulletin du Jardin Botanique de Buitenzorg, ser. 3 16: 107 - 110." type="journal article" year="1939">Backer (1939: 108)</bibRefCitation>
</taxonomicName>
to 
<taxonomicName id="4C594D44FFB7FF85FD8EFB6AAA79FB4E" authority="(Merrill 1910: 122) Maesen (1985: 119)" authorityName="Maesen" authorityPageNumber="119" authorityYear="1985" baseAuthorityName="Merrill" baseAuthorityPageNumber="122" baseAuthorityYear="1910" box="[512,1098,1202,1229]" class="Magnoliopsida" family="Fabaceae" genus="Teyleria" kingdom="Plantae" order="Fabales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="tetragona">
<emphasis id="B92DEAD5FFB7FF85FD8EFB6AACB4FB4E" box="[512,647,1203,1228]" italics="true" pageId="16" pageNumber="17">T. tetragona</emphasis>
(Merrill 1910: 122) 
<bibRefCitation id="EFC84B36FFB7FF85FCE5FB6AAA79FB4E" author="Van der Maesen, L. J. G." box="[875,1098,1202,1229]" pageId="16" pageNumber="17" pagination="1 - 132" refId="ref15638" refString="Van der Maesen, L. J. G. (1985) Revision of the genus Pueraria DC. with some notes on Teyleria Backer. Agricultural University Wageningen Papers 85 (1): 1 - 132." type="journal article" year="1985">Maesen (1985: 119)</bibRefCitation>
</taxonomicName>
