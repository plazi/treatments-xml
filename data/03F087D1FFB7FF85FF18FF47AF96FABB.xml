<taxonomicName id="4C594D44FFB7FF85FBDEFB0FAA98FB72" authorityName="Backer" authorityYear="1939" box="[1104,1195,1238,1264]" class="Magnoliopsida" family="Fabaceae" genus="Teyleria" kingdom="Plantae" order="Fabales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="genus">
<emphasis id="B92DEAD5FFB7FF85FBDEFB0FAA98FB72" box="[1104,1195,1238,1264]" italics="true" pageId="16" pageNumber="17">Teyleria</emphasis>
</taxonomicName>
