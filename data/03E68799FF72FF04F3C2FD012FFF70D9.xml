<subSubSection id="C3556504FF72FF04F3C2FD012C43772A" box="[151,587,763,790]" pageId="145" pageNumber="146" type="nomenclature">
<paragraph id="8BF0368FFF72FF04F3C2FD012C43772A" blockId="145.[151,587,763,790]" box="[151,587,763,790]" pageId="145" pageNumber="146">
<heading id="D0B881E3FF72FF04F3C2FD012C43772A" bold="true" box="[151,587,763,790]" fontSize="11" level="1" pageId="145" pageNumber="146" reason="1">
<emphasis id="B93BEA9DFF72FF04F3C2FD012C43772A" bold="true" box="[151,587,763,790]" pageId="145" pageNumber="146">
<taxonomicName id="4C4F4D0CFF72FF04F3C2FD012F947729" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[151,412,764,790]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="plurima">
<emphasis id="B93BEA9DFF72FF04F3C2FD012F947729" bold="true" box="[151,412,764,790]" italics="true" pageId="145" pageNumber="146">Saccharodite plurima</emphasis>
</taxonomicName>
