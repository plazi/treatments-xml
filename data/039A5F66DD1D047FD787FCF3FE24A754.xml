<emphasis id="B9473262DD1D047CD235FA99FFDDA35E" box="[1317,1429,1310,1333]" italics="true" pageId="41" pageNumber="42">B. picipes</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3395F3DD1D047CD787FAC4FB63A331" box="[151,299,1346,1370]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="infrequens">
