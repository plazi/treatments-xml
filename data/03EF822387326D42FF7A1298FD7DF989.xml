<taxonomicName id="4C4648B687326D42FD8413EFFD30FA41" box="[614,728,1470,1495]" class="Magnoliopsida" family="Combretaceae" genus="Terminalia" kingdom="Plantae" order="Myrtales" pageId="8" pageNumber="197" phylum="Tracheophyta" rank="genus">terminalia</taxonomicName>
