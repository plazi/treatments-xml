<figureCitation id="137E74D05965FFB914E5FAD8FF00FA96" box="[159,230,1313,1337]" captionStart="FIGURE 6" captionStartId="8.[151,255,1945,1969]" captionTargetBox="[313,1277,646,1926]" captionTargetId="figure@8.[313,1277,646,1926]" captionTargetPageId="8" captionText="FIGURE 6. Aysenia cylindrica Ramírez (AMNH). a male dorsal habitus; b epigyne, ventral view, Ramírez (2003); c male left palp, ventral view; d male left palp, retrolateral view." httpUri="https://zenodo.org/record/280161/files/figure.png" pageId="2" pageNumber="3">
Figs 
<number id="3D8D7A725965FFB914ADFAD8FF00FA96" box="[215,230,1313,1337]" pageId="2" pageNumber="3" value="6.0">6</number>
</figureCitation>
b, 
<number id="3D8D7A725965FFB9157BFAD8FEF8FA96" box="[257,286,1313,1337]" pageId="2" pageNumber="3" value="13.0">13</number>
