, 
<taxonomicName id="F21C61C38EC99A7DF2A09826F7DC96F9" baseAuthorityName="Doering" baseAuthorityYear="1877" class="Gastropoda" family="Bulimulidae" genus="Spixia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="32" pageNumber="33" phylum="Mollusca" rank="species" species="multispirata">
