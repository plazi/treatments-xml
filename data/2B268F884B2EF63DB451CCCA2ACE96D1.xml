<taxonomicName id="C7A21BBE3475AEBE24D132D4A6ECD11C" genus="E." lsidName="E. orientalis" pageId="0" pageNumber="47" rank="species" species="orientalis">E. orientalis</taxonomicName>
as 
<taxonomicName id="A0F78F81E35800B9E40CF0D90DB6FCA2" genus="E." lsidName="E. palmatus" pageId="0" pageNumber="47" rank="species" species="palmatus">E. palmatus</taxonomicName>
collected from 
<taxonomicName id="20CA5E3EBF71084475CB9EEBD599BA96" class="Tubothalamea" family="Milioliporidae" genus="Cucurbita" higherTaxonomySource="CoL" kingdom="Chromista" lsidName="Cucurbita moschata" order="Miliolida" pageId="0" pageNumber="47" phylum="Foraminifera" rank="species" species="moschata">Cucurbita moschata</taxonomicName>
