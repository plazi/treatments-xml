<taxonomicName id="3D0548723F30E7776BBC58C4D6D494B2" class="Liliopsida" family="Arecaceae" genus="Washingtonia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Washingtonia filifera" order="Arecales" pageId="0" pageNumber="47" phylum="Tracheophyta" rank="species" species="filifera">Washingtonia filifera</taxonomicName>
