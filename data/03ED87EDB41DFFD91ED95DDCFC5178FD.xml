<taxonomicName id="4C444D78B41EFFD91ED958CFFEAB78FD" box="[151,280,223,248]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="grandiunca">
