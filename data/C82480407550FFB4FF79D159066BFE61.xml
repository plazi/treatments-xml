<taxonomicName id="878D4AD57550FFB6FF79D1590741FD4E" box="[151,570,617,643]" class="Insecta" family="Hydropsychidae" genus="Smicridea" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="424" phylum="Arthropoda" rank="species" species="necator" status="sp. nov." subGenus="Rhyacophylax">
<emphasis id="72F9ED447550FFB6FF79D159046AFD4E" bold="true" box="[151,273,617,643]" italics="true" pageId="1" pageNumber="424">Smicridea</emphasis>
(
<emphasis id="72F9ED447550FFB6FECCD15904B5FD4E" bold="true" box="[290,462,617,643]" italics="true" pageId="1" pageNumber="424">Rhyacophylax</emphasis>
) 
<emphasis id="72F9ED447550FFB6FE31D1590741FD4E" bold="true" box="[479,570,617,643]" italics="true" pageId="1" pageNumber="424">necator</emphasis>
</taxonomicName>
