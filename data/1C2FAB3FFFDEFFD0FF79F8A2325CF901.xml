<taxonomicName id="538661AAFFDCFFD0FD3EFB4D35CCFB6D" box="[720,925,1177,1199]" class="Insecta" family="Cicadellidae" genus="Reticuluma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="259" phylum="Arthropoda" rank="species" species="testacea">
