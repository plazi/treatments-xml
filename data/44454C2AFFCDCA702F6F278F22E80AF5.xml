<emphasis id="FE98212EFFCDCA7E2F6F278F22A709E5" bold="true" box="[264,695,724,750]" pageId="22" pageNumber="23">
<taxonomicName id="0BEC86BFFFCDCA7E2F6F278F21D809E5" authority="Jaschhof &amp; Hippa, 2005" authorityName="Jaschhof &amp; Hippa" authorityYear="2005" box="[264,456,724,750]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="spinosa" status="sp. n.">
