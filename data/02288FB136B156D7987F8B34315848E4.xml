<taxonomicName id="8BF2D94918E546A139AAB812F27CC875" authorityName="Latreille" authorityYear="1802" class="Hexapoda" family="Tenebrionidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="family">Tenebrionidae</taxonomicName>
