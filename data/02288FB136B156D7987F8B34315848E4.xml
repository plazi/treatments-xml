<taxonomicName id="805516A8175EB6C49BD0D115CDC9AF4B" class="Sordariomycetes" family="Clavicipitaceae" genus="Cordyceps" kingdom="Fungi" lsidName="" order="Coleoptera" pageId="0" pageNumber="79" phylum="Ascomycota" rank="order">Coleoptera</taxonomicName>
