<taxonomicName id="D1BF8697FFCEFFFDFB0743E8FA7CA2BF" box="[1248,1436,692,716]" class="Liliopsida" family="Arecaceae" genus="Euterpe" kingdom="Animalia" order="Arecales" pageId="2" pageNumber="398" phylum="Tracheophyta" rank="species" species="globosa">
