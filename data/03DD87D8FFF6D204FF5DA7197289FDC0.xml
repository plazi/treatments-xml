<collectingCountry id="F363765EFFF6D204FE27A797720EFE24" box="[455,564,462,488]" name="Australia" pageId="3" pageNumber="4">Australia</collectingCountry>
(genera 
<taxonomicName id="4C744D4DFFF6D204FD7DA7977476FE24" authority="Karanovic, 2003" authorityName="Karanovic" authorityYear="2003" box="[669,1100,462,488]" class="Ostracoda" family="Candonidae" genus="Deminutiocandona" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Podocopida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
<emphasis id="B900EADCFFF6D204FD7DA7977344FE24" box="[669,894,462,488]" italics="true" pageId="3" pageNumber="4">Deminutiocandona</emphasis>
Karanovic, 2003
</taxonomicName>
, 
<taxonomicName id="4C744D4DFFF6D204FBB9A79770C2FDC0" authority="Karanovic, 2007" authorityName="Karanovic" authorityYear="2007" class="Ostracoda" family="Candonidae" genus="Kencandona" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Podocopida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
