<taxonomicName id="4C744D4DFFF6D204FED1A7AB729DFDC0" authority="Karanovic, 2007" authorityName="Karanovic" authorityYear="2007" box="[305,679,498,524]" class="Ostracoda" family="Candonidae" genus="Leicacandona" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Podocopida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
