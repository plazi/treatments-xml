<collectionCode id="ED075AEFFFE9FFC4FC777DB836DC31ED" LSID="urn:lsid:biocol.org:col:34925" box="[912,997,675,699]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="18" pageNumber="19">AMNH</collectionCode>
