<quantity id="4C869B51FFE2EE75FF27FD0C8A2C6EA9" box="[226,341,678,699]" metricMagnitude="-3" metricUnit="m" metricValue="2.55" metricValueMax="2.8" metricValueMin="2.3" pageId="2" pageNumber="225" unit="mm" value="2.55" valueMax="2.8" valueMin="2.3">2.3–2.8 mm</quantity>
........................................................................... 
<taxonomicName id="4C7E4D37FFE2EE75FB1DFD0F8EE46EA9" authority="Hava" authorityName="Hava" authorityYear="2003" box="[1240,1437,677,699]" class="Insecta" family="Nosodendridae" genus="Nosoglobulus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="225" phylum="Arthropoda" rank="genus">
