<heading id="D0E4AF0A6A5FF57BFF06FF148B2DFF1B" bold="true" box="[151,333,151,177]" fontSize="11" level="1" pageId="107" pageNumber="108" reason="1">
<emphasis id="B967C4746A5FF57BFF06FF148B2DFF1B" bold="true" box="[151,333,151,177]" pageId="107" pageNumber="108">
‘ 
<taxonomicName id="4C1363E56A5FF57BFF30FF148A92FF1B" authorityName=": Hansen" authorityYear="1913" box="[161,242,151,177]" class="Malacostraca" family="Typhlotanaidae" genus="Typhlotanais" kingdom="Animalia" order="Tanaidacea" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="mixtus">
