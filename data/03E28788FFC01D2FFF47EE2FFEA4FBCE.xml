<emphasis id="B93FEA8CFFC11D28FCF0EE8AFC55F804" box="[800,908,1954,1975]" italics="true" pageId="3" pageNumber="364">P. huemeri</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20C57F7FFC11D28FC41EE8AFC38F804" box="[913,993,1954,1975]" pageId="3" pageNumber="364" rank="species">
<emphasis id="B93FEA8CFFC11D28FC41EE8AFC38F804" bold="true" box="[913,993,1954,1975]" pageId="3" pageNumber="364">sp. nov.</emphasis>
</taxonomicNameLabel>
, holotype, ♂; 9. 
<taxonomicName id="4C4B4D1DFFC11D28FB5BEE8AFAAAF804" authority="Wang" authorityName="Wang &amp; Jia" authorityYear="2017" box="[1163,1395,1954,1975]" class="Insecta" family="Oecophoridae" genus="Promalactis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="364" phylum="Arthropoda" rank="species" species="grandacantha" status="sp. nov.">
