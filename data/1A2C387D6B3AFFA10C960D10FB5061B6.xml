<taxonomicName id="5585F2E86B3DFFA00D1D0F3DFD79672D" authority="Feng &amp; Huang &amp; Luo, 2020" authorityName="Feng &amp; Huang &amp; Luo" authorityYear="2020" box="[284,642,1878,1902]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="CoL" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="567" phylum="Arthropoda" rank="species" species="shuangcha" status="sp. nov." subGenus="Gymnaeta">
