<taxonomicName id="18D54D3EFF8DFB0EFD17FB9A49BB8147" authority="Liu &amp; Kuang, 1998" authorityName="Liu &amp; Kuang" authorityYear="1998" box="[754,1270,1084,1110]" class="Arachnida" family="Diptilomiopidae" genus="Rhyncaphytoptus" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="53" phylum="Arthropoda" rank="species" species="mori">
