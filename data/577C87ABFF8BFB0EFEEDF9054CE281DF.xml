long, 
<number id="691D249AFF8BFB08FCA4F8984E138249" box="[833,862,1854,1880]" pageId="2" pageNumber="51" value="88.0">88</number>
wide, 
<number id="691D249AFF8BFB08FC4BF8984E868249" box="[942,971,1854,1880]" pageId="2" pageNumber="51" value="85.0">85</number>
thick; light yellow in color.
