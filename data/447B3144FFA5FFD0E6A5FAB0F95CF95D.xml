<taxonomicName id="0BD2FBD1FFA5FFD0E72EF97DFB46F904" authorityName="Karayeva (1978:" authorityYear="1595" box="[284,368,1670,1695]" class="Bacillariophyceae" family="Lyrellaceae" genus="Lyrella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Lyrellales" pageId="2" pageNumber="3" phylum="Ochrophyta" rank="genus">
