<taxonomicName id="B3AA8E9EFFECFF8CECBE4879FAE7F4F0" authority="Gray, 1830" authorityName="Gray" authorityYear="1830" box="[933,1332,2846,2875]" class="Mammalia" family="Viverridae" genus="Viverra" kingdom="Animalia" order="Carnivora" pageId="14" pageNumber="221" phylum="Chordata" rank="species" species="maculata">Viverra maculata Gray, 1830</taxonomicName>
, 
<materialsCitation id="C4C2FF40FFECFF8CEDE94822FCBCF4A9" ID-GBIF-Occurrence="3712312303" box="[754,879,2885,2914]" pageId="14" pageNumber="221">
