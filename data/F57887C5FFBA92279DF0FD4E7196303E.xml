<taxonomicName id="BAD14D50FFBA92249A4BFD4E71A431E9" authority="Worthy, 2009" authorityName="Worthy" authorityYear="2009" box="[1038,1188,703,724]" class="Aves" family="Anatidae" genus="Pinpanetta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anseriformes" pageId="5" pageNumber="416" phylum="Chordata" rank="genus" status="GEN. NOV.">PINPANETTA</taxonomicName>
<emphasis id="4FA5EAC1FFBA92249AEBFD33702631E9" bold="true" box="[1198,1318,703,724]" pageId="5" pageNumber="416">
<taxonomicNameLabel id="549657BAFFBA92249AEBFD33702631E9" box="[1198,1318,703,724]" pageId="5" pageNumber="416" rank="genus">GEN. NOV.</taxonomicNameLabel>
