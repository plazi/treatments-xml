<taxonomicName id="4C724D72FF8DFFC892F0E84B14B3F99D" authority="(Swartz 1788: 106) Candolle 1825: 332" authorityName="Candolle" authorityPageNumber="332" authorityYear="1825" baseAuthorityName="Swartz" baseAuthorityPageNumber="106" baseAuthorityYear="1788" box="[178,910,1598,1625]" class="Magnoliopsida" family="Fabaceae" genus="Desmodium" kingdom="Plantae" order="Fabales" pageId="11" pageNumber="256" phylum="Tracheophyta" rank="species" species="adscendens">
