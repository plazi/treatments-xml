<taxonomicName id="4C2901BE048315D0FC13493BFB2CF8D8" authority="Hinds, 1843" authorityName="Hinds" authorityYear="1843" box="[816,1223,1810,1837]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="138" pageNumber="139" phylum="Mollusca" rank="species" species="elegans" subGenus="Ino">
