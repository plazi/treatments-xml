<specimenCount id="9D21C5E8FFDA1877FB9B95BEC447FE19" box="[1075,1096,461,485]" pageId="8" pageNumber="2000" type="male">♂</specimenCount>
, 
<quantity id="4CDFA384FFDA1877FF27959FC115FDF8" box="[143,282,492,516]" metricMagnitude="-2" metricUnit="m" metricValue="8.0772" metricValueMax="8.636" metricValueMin="7.5184" pageId="8" pageNumber="2000" unit="in" value="3.1799999999999997" valueMax="3.4" valueMin="2.96">2.96–3.40 in</quantity>
<specimenCount id="9D21C5E8FFDA1877FE8C959EC13DFDF9" box="[292,306,493,517]" pageId="8" pageNumber="2000" type="female">♀</specimenCount>
), ThL = 
