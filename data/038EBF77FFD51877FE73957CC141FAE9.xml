<quantity id="4CDFA384FFDA1877FF7095BEC134FE18" box="[216,315,461,485]" metricMagnitude="-3" metricUnit="m" metricValue="3.52" pageId="8" pageNumber="2000" unit="mm" value="3.52">3.52 mm</quantity>
