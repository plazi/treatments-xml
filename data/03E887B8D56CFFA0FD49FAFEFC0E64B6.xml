<taxonomicName id="4C414D2DD56CFFA0FCBBFAFEFC4B6459" box="[780,1009,1349,1373]" class="Mammalia" family="Bovidae" genus="Tragoportax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Artiodactyla" pageId="3" pageNumber="244" phylum="Chordata" rank="species" species="amalthea">
