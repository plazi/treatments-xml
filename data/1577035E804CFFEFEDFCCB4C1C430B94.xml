<taxonomicName id="5ADEC9CB804CFFEFE962CB851DCA0BB9" authorityName="Garman" authorityYear="1899" box="[1033,1175,1452,1477]" class="Actinopterygii" family="Ogcocephalidae" genus="Halieutopsis" kingdom="Animalia" order="Lophiiformes" pageId="72" pageNumber="73" phylum="Chordata" rank="genus">
