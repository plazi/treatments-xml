<taxonomicName id="75F04D3DFFCCFFB2FE8E5E2FFE46991F" box="[309,445,1847,1868]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="hawaiiensis">
