<taxonomicName id="75F04D3DFFCCFFB2FBA55E2FFB86991F" authorityName="Mound &amp; Masumoto" authorityYear="2005" box="[1054,1149,1847,1868]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="hoddlei">
