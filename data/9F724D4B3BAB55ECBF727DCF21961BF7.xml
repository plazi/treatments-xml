<taxonomicName id="DC99427D972E04C701A38C44019E3451" authorityName="Hausskn. &amp; Bornm., Mitth. Thuering. Bot. Vereins 6: 66. 1894. (Bornmueller" authorityYear="1894" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus holosericeus subsp. macrocalycinus" order="Solanales" pageId="165" pageNumber="166" phylum="Tracheophyta" rank="subSpecies" species="holosericeus" subSpecies="macrocalycinus">Convolvulus holosericeus subsp. macrocalycinus</taxonomicName>
<normalizedToken id="63639F408FA0ABA8A8E9A7E773AA98FF" originalValue="×">x</normalizedToken>
<taxonomicName id="A69E0F02610BE6F3F3A955B21D00B4A0" authorityName="Stace, Bot. J. Linn. Soc. 64: 58. 1971. (Stace" authorityYear="1971" baseAuthorityName="Boiss." class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus boissieri subsp. compactus" order="Solanales" pageId="165" pageNumber="166" phylum="Tracheophyta" rank="subSpecies" species="boissieri" subSpecies="compactus">Convolvulus boissieri subsp. compactus</taxonomicName>
