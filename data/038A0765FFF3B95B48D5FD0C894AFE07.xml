<taxonomicName id="4C23CDF0FFFFB959491BF8398BCAF857" authorityName="Girault Fauna" authorityYear="1915" box="[345,464,1936,1960]" class="Insecta" family="Mymaridae" genus="Anagroidea" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
