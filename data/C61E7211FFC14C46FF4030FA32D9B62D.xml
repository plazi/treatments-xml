<location id="4B6895DCFFC14C46FBAD327E368EB265" LSID="urn:lsid:plazi:treatment:C61E7211FFC14C46FF4030FA32D9B62D:4B6895DCFFC14C46FBAD327E368EB265" box="[1146,1190,785,811]" name="The" pageId="65" pageNumber="66">The</location>
<typeStatus id="910C7DA5FFC14C46FB79327E373DB265" box="[1198,1301,785,811]" pageId="65" pageNumber="66">lectotype</typeStatus>
</materialsCitation>
is 
<quantity id="894F6EE2FFC14C46FAE0327E37B5B265" box="[1335,1437,785,811]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="65" pageNumber="66" unit="mm" value="3.7">3.70 mm</quantity>
