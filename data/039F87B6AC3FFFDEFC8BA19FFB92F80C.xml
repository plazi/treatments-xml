<number id="3DFE2487AC3FFFDEFD9DA4CAFE51F948" box="[385,446,1664,1688]" pageId="13" pageNumber="156" value="1996.0">1996</number>
); 
<specimenCount id="9D30FD29AC3FFFDEFDC8A4CAFDAEF948" box="[468,577,1664,1688]" pageId="13" pageNumber="156" type="female">
<number id="3DFE2487AC3FFFDEFDC8A4CAFE0DF948" box="[468,482,1664,1688]" pageId="13" pageNumber="156" value="4.0">4</number>
females
</specimenCount>
