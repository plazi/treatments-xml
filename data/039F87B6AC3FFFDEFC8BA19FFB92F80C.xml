<taxonomicName id="4C364D23AC3FFFDEFEFEA7BBFCBEF9D8" box="[738,849,1520,1544]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="13" pageNumber="156" phylum="Tracheophyta" rank="species" species="glauca">
