, 
<collectingCountry id="F36D760CB60EFEBB61F87B42D902F911" box="[1196,1394,1648,1674]" name="United Kingdom" pageId="351" pageNumber="352">United Kingdom</collectingCountry>
, 
<collectingCountry id="F36D760CB60EFEBB65E97BA4DD50F92B" box="[189,288,1686,1712]" name="Portugal" pageId="351" pageNumber="352">Portugal</collectingCountry>
