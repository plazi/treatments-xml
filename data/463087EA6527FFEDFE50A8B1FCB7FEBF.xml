<taxonomicName id="09994D7F6520FFECFEE9AC1DFDEDF9D6" authority="(Girault)" baseAuthorityName="Girault" baseAuthorityYear="1933" box="[257,548,1566,1589]" class="Insecta" family="Signiphoridae" genus="Chartocerus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="692" phylum="Arthropoda" rank="species" species="delicatus">
