from 
<taxonomicName id="75C91F19D810FF2326FC69506804E1B5" genus="Al." lsidName="Al. auriel" pageId="0" pageNumber="81" rank="species" species="auriel">
<emphasis id="CD36DFF2FB04701FED9B03DB67692A7A" italics="true" pageId="0" pageNumber="81">Al. auriel</emphasis>
</taxonomicName>
.
</paragraph>
