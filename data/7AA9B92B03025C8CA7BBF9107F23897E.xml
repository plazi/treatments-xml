<taxonomicName id="DB63975CD00AFA014FB79E7D29A630F7" genus="Al." lsidName="Al. mephisto" pageId="0" pageNumber="81" rank="species" species="mephisto">
<emphasis id="594C836C44FF1AB3AEEF3CCD7A4CEEA7" italics="true" pageId="0" pageNumber="81">Al. mephisto</emphasis>
</taxonomicName>
and 
<taxonomicName id="B3ECA2FE0159CD75E725BD427CABEABA" genus="Al." lsidName="Al. tasmanica" pageId="0" pageNumber="81" rank="species" species="tasmanica">
<emphasis id="5FE912F37B716F90F4DC16314F734DCC" italics="true" pageId="0" pageNumber="81">Al. tasmanica</emphasis>
</taxonomicName>
, and the evenly setose mesoscutum and mesoscutellum distinguishes 
<taxonomicName id="DA8E9DC216E810ECA0613A0553719237" genus="Al." lsidName="Al. teres" pageId="0" pageNumber="81" rank="species" species="teres">
<emphasis id="5BA96A91B0F4B08F6B89E4D4D041DD2B" italics="true" pageId="0" pageNumber="81">Al. teres</emphasis>
