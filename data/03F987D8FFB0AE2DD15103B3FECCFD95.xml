<taxonomicName id="4C504D4DFFB2AE2CD21D0073FE80F9DE" authorityName="SALAzAR-Vallejo" authorityYear="2012" baseAuthorityName="SALAzAR-Vallejo" baseAuthorityYear="2012" box="[113,343,1578,1604]" class="Polychaeta" family="Flabelligeridae" genus="Trophoniella" kingdom="Animalia" order="Terebellida" pageId="50" phylum="Annelida" rank="species" species="minuta" status="n. comb.">
