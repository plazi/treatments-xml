and 
<taxonomicName id="D0DD4D6EFFFE5240FD6224D69711FA96" box="[699,858,949,973]" class="Insecta" family="Ceratopogonidae" genus="Parabezzia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="396" phylum="Arthropoda" rank="species" species="caribbeana">
<emphasis id="25A9EAFFFFFE5240FD6224D69711FA96" box="[699,858,949,973]" italics="true" pageId="6" pageNumber="396">P. caribbeana</emphasis>
