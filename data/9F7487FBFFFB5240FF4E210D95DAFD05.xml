<taxonomicName id="D0DD4D6EFFFE5240FDDB24D69630FA96" box="[514,635,950,973]" class="Insecta" family="Ceratopogonidae" genus="Parabezzia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="396" phylum="Arthropoda" rank="species" species="arenosa">
<emphasis id="25A9EAFFFFFE5240FDDB24D69630FA96" box="[514,635,950,973]" italics="true" pageId="6" pageNumber="396">P. arenosa</emphasis>
