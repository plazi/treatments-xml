<quantity id="4CEF9B25FF02FF2699BBFD99FBBF3283" box="[454,510,619,646]" metricMagnitude="-1" metricUnit="m" metricValue="2.794" pageId="147" pageNumber="148" unit="in" value="11.0">11 in</quantity>
