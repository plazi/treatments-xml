<quantity id="4CEF9B25FF02FF269C9DFD40FF0432C8" box="[1248,1349,690,717]" metricMagnitude="2" metricUnit="m" metricValue="1.26" metricValueMax="2.51" metricValueMin="0.01" pageId="147" pageNumber="148" unit="m" value="126.0" valueMax="251.0" valueMin="1.0">1–251 m</quantity>
(
<quantity id="4CEF9B25FF02FF269D28FD41FA9832F7" metricMagnitude="2" metricUnit="m" metricValue="1.258824" metricValueMax="2.508504" metricValueMin="0.009144" pageId="147" pageNumber="148" unit="ft" value="413.0" valueMax="823.0" valueMin="3.0">3–823 ft</quantity>
