<quantity id="4C9D398BFF90146FFDAAF20FFD39E337" box="[569,657,1280,1304]" metricMagnitude="3" metricUnit="m" metricValue="2.8" pageId="37" pageNumber="188" unit="m" value="2800.0">2800 m</quantity>
