<taxonomicName id="4C65EFEDFF90146FFEFAF3B7FD1AE2FF" authority="Cameron" authorityName="Cameron" box="[361,690,1208,1232]" class="Insecta" family="Staphylinidae" genus="Quedius" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="188" phylum="Arthropoda" rank="species" species="muscicola">
