<quantity id="4C9D398BFF90146FFE5CF3F0FDBEE337" box="[463,534,1279,1304]" metricMagnitude="2" metricUnit="m" metricValue="6.5" pageId="37" pageNumber="188" unit="m" value="650.0">650 m</quantity>
