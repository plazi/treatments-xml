<specimenCount id="9D2EFD71FF99FFA3FDEB7AAFE4626698" box="[517,537,959,980]" pageId="13" pageNumber="1600" type="male">♂</specimenCount>
: 
<collectingCountry id="F33F7668FF99FFA3FDCA7AAEE4016698" box="[548,634,958,980]" name="Australia" pageId="13" pageNumber="1600">Australia</collectingCountry>
, 
<collectingRegion id="49ECF81AFF99FFA3FD6B7AAEE5486698" box="[645,819,958,980]" country="Australia" name="Northern Territory" pageId="13" pageNumber="1600">Northern Territory</collectingRegion>
