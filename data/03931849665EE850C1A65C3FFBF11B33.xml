<taxonomicName id="4C3AD2DC665EE850C30A5DCDFD6B18AC" box="[571,706,1433,1459]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
