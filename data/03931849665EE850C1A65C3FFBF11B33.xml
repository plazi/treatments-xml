<taxonomicName id="4C3AD2DC665EE850C0B75DCDFD8518AC" box="[390,556,1433,1459]" class="Magnoliopsida" family="Polygonaceae" kingdom="Plantae" order="Polygonales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Polygonaceae</taxonomicName>
