, 
<taxonomicName id="4C3AD2DC665EE850C4585D27FED118AC" class="Liliopsida" family="Zingiberaceae" kingdom="Plantae" order="Zingiberales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Zingiberaceae</taxonomicName>
