<taxonomicName id="4C3AD2DC665EE850C37A5D27FD6E1892" box="[587,711,1395,1421]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
, 
<taxonomicName id="4C3AD2DC665EE850C3E75D27FCFC1892" box="[726,853,1395,1421]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
