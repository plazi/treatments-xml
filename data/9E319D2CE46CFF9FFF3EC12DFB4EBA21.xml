<caption id="42E77CB2E46AFF9CFF3EC17EFBADB8E2" ID-DOI="http://doi.org/10.5281/zenodo.5305477" ID-Zenodo-Dep="5305477" httpUri="https://zenodo.org/record/5305477/files/figure.png" pageId="5" pageNumber="64" startId="5.[151,273,1916,1940]" targetBox="[178,1422,408,1901]" targetPageId="5">
