<taxonomicName id="52997B5F5AD676515623F0DEF2630E96" class="Phaeophyceae" family="Sargassaceae" genus="Turbinaria" higherTaxonomySource="CoL" kingdom="Chromista" lsidName="Turbinaria ornata" order="Fucales" pageId="7" pageNumber="8" phylum="Ochrophyta" rank="species" species="ornata">Turbinaria ornata</taxonomicName>
), leaves on driftline; 
<geoCoordinate id="BD824664B063E1AFA4E434BDC9342E00" direction="north" orientation="latitude" precision="1" value="7.33065">07°19.839'N</geoCoordinate>
, 
<geoCoordinate id="1FD46FE6FB25EF437030924E38DB4209" direction="east" orientation="longitude" precision="1" value="134.40257">134°24.154'E</geoCoordinate>
; leg. S. De Grave &amp; C. Burras, 22 May 2002; 3 females,OUMNH.ZC.2002-24-0086,outside Risong, seagrass hand dredge of sediment, 1 m, 
<geoCoordinate id="B15C1524907EB353390668AD0BFDCA7A" direction="north" orientation="latitude" precision="1" value="7.2988">07°17.928'N</geoCoordinate>
, 
<geoCoordinate id="AA7C4FD741B6F164A80931778D92DB1E" direction="east" orientation="longitude" precision="1" value="134.47784">134°28.671'E</geoCoordinate>
; leg. S. De Grave &amp; C. Burras, 22 May 2002.
</paragraph>
