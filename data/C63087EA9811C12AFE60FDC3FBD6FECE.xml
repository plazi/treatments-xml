<subSubSection id="068365779811C12FFFFBFDF3FD14FDCC" box="[113,739,569,594]" pageId="1" pageNumber="496" type="type_taxon">
<paragraph id="4E2636FC9811C12FFFFBFDF3FD14FDCC" blockId="1.[113,1089,569,1586]" box="[113,739,569,594]" pageId="1" pageNumber="496">
<emphasis id="7CEDEAEE9811C12FFFFBFDF3FEF2FDCC" bold="true" box="[113,261,569,593]" pageId="1" pageNumber="496">
<typeStatus id="9122885E9811C12FFFFBFDF3FF5EFDCC" box="[113,169,569,593]" pageId="1" pageNumber="496">Type</typeStatus>
