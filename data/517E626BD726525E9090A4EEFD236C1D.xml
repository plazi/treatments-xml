<taxonomicName id="1ED7A8FED726525E9090A4EEFC5E6EA4" authority="Reck, 1948" authorityName="Reck" authorityYear="1948" box="[491,897,229,253]" class="Arachnida" family="Tetranychidae" genus="Eotetranychus" kingdom="Animalia" order="Trombidiformes" pageId="8" pageNumber="387" phylum="Arthropoda" rank="species" species="rubiphilus">
<emphasis id="EBA30F6FD726525E9090A4EEFC5E6EA4" bold="true" box="[491,897,229,253]" pageId="8" pageNumber="387">
