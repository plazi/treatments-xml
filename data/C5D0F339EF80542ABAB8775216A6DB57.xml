<taxonomicName id="AFD9EF747DF85C474DC9CC234F1E3603" class="Magnoliopsida" family="Urticaceae" genus="Cecropia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Rosales" pageId="0" pageNumber="609" phylum="Tracheophyta" rank="family">Urticaceae</taxonomicName>
