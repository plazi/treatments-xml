<taxonomicName id="4C4C37B00473F85A2A261C58FE37FEC5" authorityName="Guerra-García" authorityYear="2020" box="[293,435,290,315]" class="Malacostraca" family="Caprellidae" genus="Luisacaprella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="3" pageNumber="2819" phylum="Arthropoda" rank="genus">
