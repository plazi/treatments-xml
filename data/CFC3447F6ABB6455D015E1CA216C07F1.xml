<emphasis id="9300F154C6F66428919871857FB4EE8A" italics="true" pageId="0" pageNumber="1">Fraudatrix</emphasis>
</taxonomicName>
, 
<taxonomicName id="D97531D876862CF69D466354A302ED6C" authorityName="Eichhoff" authorityYear="1864" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="genus">
