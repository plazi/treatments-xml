<collectingCountry id="0AF6767C242BBD72FF37DC541A32A131" box="[151,269,534,560]" name="Honduras" pageId="53" pageNumber="54">Honduras</collectingCountry>
, 
<collectingCountry id="0AF6767C242BBD72FEB9DC541AA9A131" box="[281,406,534,560]" name="Nicaragua" pageId="53" pageNumber="54">Nicaragua</collectingCountry>
