<geoCoordinate id="17D5502B242BBD72FC54DFB21F5FA10B" box="[1012,1120,496,522]" degrees="85" direction="west" minutes="34" orientation="longitude" pageId="53" pageNumber="54" precision="925" value="-85.566666">85°34’W</geoCoordinate>
