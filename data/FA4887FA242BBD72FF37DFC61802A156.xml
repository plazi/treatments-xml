<geoCoordinate id="17D5502B242BBD72FD7CDFB2187EA10B" box="[732,833,496,522]" degrees="15" direction="north" minutes="42" orientation="latitude" pageId="53" pageNumber="54" precision="925" value="15.7">15°42’N</geoCoordinate>
