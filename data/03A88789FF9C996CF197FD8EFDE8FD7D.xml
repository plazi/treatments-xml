<taxonomicName id="4C014D1CFF9D996FF019F8D1FDFDF867" authority="Liu &amp; Chen" authorityName="Liu &amp; Chen" authorityYear="2020" box="[281,565,1913,1938]" class="Insecta" family="Braconidae" genus="Apanteles" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="altus" status="sp. nov.">
