<taxonomicName id="00F44D1E255DE074FD60B9F3FCDBFC49" box="[654,822,945,971]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
