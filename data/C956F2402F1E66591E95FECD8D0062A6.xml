<taxonomicName id="86FF38D52F1E66591E95FECD8D2967C6" authority="Yu &amp; Yang" authorityName="Yu &amp; Yang" box="[151,580,381,407]" class="Insecta" family="Cicadellidae" genus="Membranacea" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="99" phylum="Arthropoda" rank="species" species="distincta" status="sp. nov.">
<emphasis id="738B9F442F1E66591E95FECD8EDE67C6" bold="true" box="[151,435,381,407]" italics="true" pageId="2" pageNumber="99">Membranacea distincta</emphasis>
Yu &amp; Yang
</taxonomicName>
, 
<taxonomicNameLabel id="68B8223F2F1E66591C53FECD8DC267C6" box="[593,687,381,407]" pageId="2" pageNumber="99" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
