<paragraph id="3B256F598E2B9B75FF27FF707840FF69" blockId="19.[151,543,151,212]" box="[151,543,151,177]" pageId="19" pageNumber="20">
<heading id="606DD8358E2B9B75FF27FF707840FF69" bold="true" box="[151,543,151,177]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis id="09EEB34B8E2B9B75FF27FF707840FF69" bold="true" box="[151,543,151,177]" pageId="19" pageNumber="20">
<emphasis id="09EEB34B8E2B9B75FF27FF707BD8FF69" bold="true" box="[151,391,151,177]" italics="true" pageId="19" pageNumber="20">
<taxonomicName id="FC9A14DA8E2B9B75FF27FF707BDDFF69" box="[151,386,151,177]" class="Insecta" family="Dipseudopsidae" genus="Hyalopsyche" kingdom="Animalia" order="Trichoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="orissa" status="new species">Hyalopsyche orissa</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="12DD0E308E2B9B75FE3FFF707840FF69" box="[399,543,151,177]" pageId="19" pageNumber="20" rank="species">new species</taxonomicNameLabel>
</emphasis>
