<paragraph id="992436BB9B7DFFF8FF43FED0FEBEFE96" blockId="36.[151,1437,151,1077]" pageId="36" pageNumber="37">
<emphasis id="ABEFEAA99B7DFFF8FF43FED0FE93FEB3" bold="true" box="[199,294,295,320]" pageId="36" pageNumber="37">Habitus</emphasis>
. ♂ 
<collectionCode id="FF8AAE7E9B7DFFF8FECAFEDFFEC6FECC" box="[334,371,296,319]" pageId="36" pageNumber="37">BL</collectionCode>
<quantity id="5E639B5E9B7DFFF8FE07FEDFFDA6FECC" box="[387,531,296,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.7999999999999998" metricValueMax="1.9" metricValueMin="1.7" pageId="36" pageNumber="37" unit="mm" value="1.7999999999999998" valueMax="1.9" valueMin="1.7">1.7–1.9 mm</quantity>
, 
<collectionCode id="FF8AAE7E9B7DFFF8FDA2FEDFFDFCFEB3" box="[550,585,296,320]" pageId="36" pageNumber="37">BL</collectionCode>
/BW ~ 
<number id="2F53249C9B7DFFF8FD19FEDFFD77FEB3" box="[669,706,296,320]" pageId="36" pageNumber="37" value="1.7">1.7</number>
; ♀ 
<collectionCode id="FF8AAE7E9B7DFFF8FD60FEDFFCBBFECC" box="[740,782,296,319]" pageId="36" pageNumber="37">BL</collectionCode>
<quantity id="5E639B5E9B7DFFF8FC99FED0FC18FECC" box="[797,941,295,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="1.9" metricValueMin="1.6" pageId="36" pageNumber="37" unit="mm" value="1.75" valueMax="1.9" valueMin="1.6">1.6–1.9 mm</quantity>
, 
<collectionCode id="FF8AAE7E9B7DFFF8FC44FEDFFC56FEB3" box="[960,995,296,320]" pageId="36" pageNumber="37">BL</collectionCode>
/BW ~ 
