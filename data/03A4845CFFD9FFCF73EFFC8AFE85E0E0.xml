<taxonomicName id="4C0D4EC9FFD9FFCF7071FC74FC51E6A4" authority="Harris &amp; Drake, 1941" authorityName="Harris &amp; Drake" authorityYear="1941" box="[590,941,927,946]" class="Insecta" family="Anthocoridae" genus="Nidicola" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="59" phylum="Arthropoda" rank="species" species="marginata">
