<taxonomicName id="A8DC4D03FF987119FB4EFCD21519FC2B" class="Diplopoda" family="Schizopetalidae" genus="Balkanopetalum" kingdom="Animalia" order="Callipodida" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="rhodopinum">
