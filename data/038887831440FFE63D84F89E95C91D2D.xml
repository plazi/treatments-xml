<taxonomicName id="4C214D161440FFE63C47FAC197021A5D" authorityName="Sinha" authorityYear="1969" box="[364,517,763,788]" class="Mammalia" family="Vespertilionidae" genus="Pipistrellus" kingdom="Animalia" order="Chiroptera" pageId="11" pageNumber="158" phylum="Chordata" rank="species" species="peguensis">
