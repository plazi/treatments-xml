<emphasis id="B96AEAED8216136625A5824EFE91F8F8" bold="true" box="[151,291,1869,1892]" pageId="53" pageNumber="54">FIGURE 31.</emphasis>
<taxonomicName id="4C1E4D7C82161366241F824CFD86F8F8" authority="Moradmand, 2013" authorityName="Moradmand" authorityYear="2013" box="[301,564,1870,1892]" class="Arachnida" family="Sparassidae" genus="Eusparassus" kingdom="Animalia" order="Araneae" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="schoemanae" status="spec. nov.">
