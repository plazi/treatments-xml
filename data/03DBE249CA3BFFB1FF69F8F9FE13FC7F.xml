<subSubSection id="C36800D4CA3BFFB3FF69F86EFC5DF801" box="[151,982,1954,1980]" pageId="1" pageNumber="45" type="materials_examined">
<paragraph id="8BCD535FCA3BFFB3FF69F86EFC5DF801" blockId="1.[151,1436,1954,2018]" box="[151,982,1954,1980]" pageId="1" pageNumber="45">
