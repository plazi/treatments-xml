<collectingRegion id="49E3F4E4D854FFC0BDCEE2662B5AF9D1" box="[664,844,1539,1563]" country="Brazil" name="Rio de Janeiro" pageId="64" pageNumber="63">Rio de Janeiro</collectingRegion>
