<treatmentCitationGroup id="AB7811AA7658FF9EFF1DF9B0DD030DBB" box="[151,822,1658,1681]" pageId="3" pageNumber="50">
<taxonomicName id="4C684D077658FF9EFF1DF9B0DD030DBB" authority="Obenberger, 1940: 84" authorityName="Obenberger" authorityPageNumber="84" authorityYear="1940" box="[151,822,1658,1681]" class="Insecta" family="Buprestidae" genus="Chrysobothris" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="subSpecies" species="ichthyomorpha" subSpecies="chiriquita">
<emphasis id="B91CEA967658FF9EFF1DF9B0DC720DBB" box="[151,583,1658,1681]" italics="true" pageId="3" pageNumber="50">Chrysobothris ichthyomorpha chiriquita</emphasis>
<treatmentCitation id="0AC910957658FF9EFDC4F9B0DD030DBB" author="Obenberger" box="[590,822,1658,1681]" page="84" pageId="3" pageNumber="50" year="1940">
<bibRefCitation id="EFF94B757658FF9EFDC4F9B0DD250DBB" author="Obenberger" box="[590,784,1658,1681]" pageId="3" pageNumber="50" refString="Obenberger, J. (1940) Nove formy rodu Chrysobothris Eschsch. (Col. Bupr.). De generis Chrysobothris Eschsch. formy novis (Col. Bupr.). Acta Entomologica Musaei Nationalis Pragae, 18, 79 - 106." type="journal article" year="1940">
