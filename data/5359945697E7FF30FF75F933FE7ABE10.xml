<materialsCitation id="6B982F1D97E7FF31FE55F818FAD2B881" ID-GBIF-Occurrence="2028147581" box="[439,1324,1941,1966]" collectionCode="MNHN" collectorName="Kodaikanal" country="India" location="Tamil Nadu" pageId="176" pageNumber="177" specimenCount="1" specimenCount-male="1" stateProvince="Tamil Nadu" typeStatus="syntype">
