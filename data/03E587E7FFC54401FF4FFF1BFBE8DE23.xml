<location id="8E93602AFFC54400FEE2FE11FECEDEC8" LSID="urn:lsid:plazi:treatment:03E587E7FFC54401FF4FFF1BFBE8DE23:8E93602AFFC54400FEE2FE11FECEDEC8" box="[314,363,402,427]" country="South Africa" name="Cap" pageId="10" pageNumber="11" stateProvince="Western Cape">Cap</location>
, v. Ludw. 37” [
<collectingCountry id="F35B7661FFC54400FDCAFE11FD02DEC8" box="[530,679,402,427]" name="South Africa" pageId="10" pageNumber="11">South Africa</collectingCountry>
, 
<collectingRegion id="4988F813FFC54400FD6AFE11FD4BDEC8" box="[690,750,402,427]" country="South Africa" name="Western Cape" pageId="10" pageNumber="11">Cape</collectingRegion>
, 1828–1837, 
<collectorName id="26B95327FFC54400FC5DFE10FBA9DEC8" box="[901,1036,402,427]" pageId="10" pageNumber="11">von Ludwig</collectorName>
leg. (
<bibRefCitation id="EFDD4B00FFC54400FB92FE11FABFDEC8" author="Hauser" box="[1098,1306,402,427]" etAl="et al." firstAuthor="Hauser" pageId="10" pageNumber="11" pagination="1 - 78" refId="ref28403" refString="Hauser, C. L., Bartsch, D., Holstein, J. &amp; Steiner, A. (2003) The Lepidoptera type material of G. A. W. Herrich-Schaffer in the Staatliches Museum fur Naturkunde, Stuttgart. Stuttgarter Beitrage zur Naturkunde, Serie A (Biologie), 657, 1 - 78." type="journal article" year="2003">Häuser et al. 2003</bibRefCitation>
)] (
<collectionCode id="ED5DAE34FFC54400FAEFFE11FA2CDEC8" box="[1335,1417,402,427]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34840" lsid="urn:lsid:biocol.org:col:34840" name="Staatliches Museum fuer Naturkund Stuttgart" pageId="10" pageNumber="11">SMNS</collectionCode>
)
