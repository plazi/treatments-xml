<number id="6E2E8E5DFFDEFF8E9476F309FCBC1E2C" box="[863,891,784,810]" pageId="7" pageNumber="34" value="11.0">11</number>
<typeStatus id="075D22D8FFDEFF8E94AAF309FC331E2C" box="[899,1012,784,810]" pageId="7" pageNumber="34">paratypes</typeStatus>
from &quot;Alaska Mountains,. between Mendenhall Glacier and Montana Creek Trail, 
<geoCoordinate id="BDD2FABDFFDEFF8E95EFF321FCE11E54" box="[710,806,821,850]" direction="north" orientation="latitude" pageId="7" pageNumber="34" precision="925" value="58.483334">
<number id="6E2E8E5DFFDEFF8E95EFF321FD241E54" box="[710,739,824,850]" pageId="7" pageNumber="34" value="58.0">58</number>
o 
<number id="6E2E8E5DFFDEFF8E95C2F321FCCC1E54" box="[747,779,824,850]" pageId="7" pageNumber="34" value="29.0">29</number>
'N
</geoCoordinate>
, 
<geoCoordinate id="BDD2FABDFFDEFF8E941AF321FC611E54" box="[819,934,821,850]" direction="west" orientation="longitude" pageId="7" pageNumber="34" precision="925" value="-134.66667">
<number id="6E2E8E5DFFDEFF8E941AF321FC981E54" box="[819,863,824,850]" pageId="7" pageNumber="34" value="134.0">134</number>
o 
<number id="6E2E8E5DFFDEFF8E944EF321FC411E54" box="[871,902,824,850]" pageId="7" pageNumber="34" value="40.0">40</number>
'W
</geoCoordinate>
, on goat dung in snowfields, 
<quantity id="1F1E319FFFDEFF8E922CF321FAA01E54" box="[1285,1383,824,850]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="7" pageNumber="34" unit="m" value="1000.0">1,000 m</quantity>
alt., 
<date id="AC58BABAFFDEFF8E97BEF344FEEE1E71" box="[151,297,861,887]" pageId="7" pageNumber="34" value="1980-07-14">
<number id="6E2E8E5DFFDEFF8E97BEF344FF731E71" box="[151,180,861,887]" pageId="7" pageNumber="34" value="14.0">14</number>
.VII. 
<number id="6E2E8E5DFFDEFF8E97C0F344FEEE1E71" box="[233,297,861,887]" pageId="7" pageNumber="34" value="1980.0">1980</number>
