<taxonomicName id="34E0248CE075FFF676552392A4B0FF4A" baseAuthorityName="Leech" baseAuthorityYear="1897" box="[1061,1193,152,176]" class="Insecta" family="Geometridae" genus="Lobogonodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="437" phylum="Arthropoda" rank="species" species="erectaria">
