<paragraph id="8BC936B1A82DCD5BFF759646B328FDC6" blockId="18.[151,1437,519,956]" box="[197,1062,594,620]" pageId="18" pageNumber="59">
<taxonomicName id="4C764D32A82DCD5BFF759646B6C9FDC6" authority="Piza, 1980" authorityName="Piza" authorityYear="1980" box="[197,455,594,620]" class="Insecta" family="Phaneropteridae" genus="Anapolisia" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="59" phylum="Arthropoda" rank="genus">
