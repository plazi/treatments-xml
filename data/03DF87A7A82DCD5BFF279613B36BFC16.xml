<taxonomicName id="4C764D32A82DCD5BFDEE9646B328FDC6" authority="Kocak &amp; Kemal, 2008" authorityName="Kocak &amp; Kemal" authorityYear="2008" box="[606,1062,594,620]" class="Insecta" family="Tettigoniidae" genus="Tropicophyllum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="59" phylum="Arthropoda" rank="genus">
