<taxonomicName id="FDA8B9B6FFEFFFBAFE6FF8D4DAABC3B6" authority="Red Clay" authorityName="Red Clay" box="[485,719,1915,1940]" class="Mammalia" family="Equidae" genus="Hipparion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="2" pageNumber="403" phylum="Chordata" rank="genus">
