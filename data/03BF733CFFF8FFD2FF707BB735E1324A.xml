S[cops] 
<taxonomicName id="4C16B9A9FFF8FFD5FF0B7BEF345B365C" authority="Licht" authorityName="Licht" box="[236,354,1268,1290]" class="Aves" family="Aves" genus="Scops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aves" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="longipennis">longipennis</taxonomicName>
