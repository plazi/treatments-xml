<location id="8EB63194FFDECB22FC41E06A2D8B9793" LSID="urn:lsid:plazi:treatment:03C0D659FFDECB22FE6BE1EB29CC9493:8EB63194FFDECB22FC41E06A2D8B9793" box="[1017,1182,1405,1429]" county="Ningshan County" name="Mount Taibai" pageId="11" pageNumber="2729">Mount Taibai</location>
