<paragraph id="8BD6674FFFDECB22FF32E06A2DB79793" blockId="11.[138,1186,1405,1429]" box="[138,1186,1405,1429]" pageId="11" pageNumber="2729">
<materialsCitation id="3B016D12FFDECB22FF32E06A2D8B9793" box="[138,1182,1405,1429]" county="Ningshan County" location="Mount Taibai" pageId="11" pageNumber="2729" specimenCount="1">
