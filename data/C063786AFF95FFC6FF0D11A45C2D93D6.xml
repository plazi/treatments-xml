/ 
<number id="FE02DB5BFF95FFC6FAFF13595AB997C2" box="[1381,1432,618,642]" pageId="59" pageNumber="60" value="0.4">0.40</number>
; pronotum 
<number id="FE02DB5BFF95FFC6FE9113BD5E1C97E6" box="[267,317,654,678]" pageId="59" pageNumber="60" value="0.39">0.39</number>
/ 
<number id="FE02DB5BFF95FFC6FEDC13BD5E5997E6" box="[326,376,654,678]" pageId="59" pageNumber="60" value="0.57">0.57</number>
