<taxonomicName id="FBDD4D48801C816FFA22F804FC351C95" box="[836,927,1929,1952]" class="Secernentea" family="Heteroderidae" genus="Heterodera" kingdom="Animalia" order="Tylenchida" pageId="12" pageNumber="13" phylum="Nematoda" rank="species" species="mani">
