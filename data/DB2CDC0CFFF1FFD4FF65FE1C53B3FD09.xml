<taxonomicName id="94851699FFF1FFD4FB9DFDA4565EFD9E" box="[1135,1291,581,605]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophioplocus" kingdom="Animalia" order="Ophiurida" pageId="16" pageNumber="289" phylum="Echinodermata" rank="species" species="imbricatus">
