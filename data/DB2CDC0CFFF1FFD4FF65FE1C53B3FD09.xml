<taxonomicName id="94851699FFF1FFD4FE13FDA75125FD9E" box="[481,624,581,605]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophioplocus" kingdom="Animalia" order="Ophiurida" pageId="16" pageNumber="289" phylum="Echinodermata" rank="genus">
