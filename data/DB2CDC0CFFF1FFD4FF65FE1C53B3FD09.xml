<taxonomicName id="94851699FFF1FFD4FE9CFD8851ECFD41" authority="Koehler, 1904" authorityName="Koehler" authorityYear="1904" box="[366,697,617,642]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophioplocus" kingdom="Animalia" order="Ophiurida" pageId="16" pageNumber="289" phylum="Echinodermata" rank="species" species="declinans">
<emphasis id="61F1B108FFF1FFD4FE9CFD8852ABFD42" box="[366,510,617,641]" italics="true" pageId="16" pageNumber="289">
