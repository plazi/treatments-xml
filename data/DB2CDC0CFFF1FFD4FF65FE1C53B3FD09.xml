<taxonomicName id="94851699FFF1FFD4FEE4FD6C5198FD65" authority="Irimura &amp; Yoshino, 1999" authorityName="Irimura &amp; Yoshino" authorityYear="1999" box="[278,717,653,678]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophioplocus" kingdom="Animalia" order="Ophiurida" pageId="16" pageNumber="289" phylum="Echinodermata" rank="species" species="giganteus">
