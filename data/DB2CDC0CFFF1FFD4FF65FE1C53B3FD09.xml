<taxonomicName id="94851699FFF1FFD4FBE2FD8857F4FD42" box="[1040,1185,618,641]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophioplocus" kingdom="Animalia" order="Ophiurida" pageId="16" pageNumber="289" phylum="Echinodermata" rank="species" species="japonicus">
<emphasis id="61F1B108FFF1FFD4FBE2FD8857F4FD42" box="[1040,1185,618,641]" italics="true" pageId="16" pageNumber="289">
<collectingCountry id="2B922D8AFFF1FFD4FBE2FD885772FD42" box="[1040,1063,618,641]" name="Iceland" pageId="16" pageNumber="289">O</collectingCountry>
. japonicus
</emphasis>
</taxonomicName>
HL Clark, 
<number id="E54D7F3DFFF1FFD4FAD4FD885608FD41" box="[1318,1373,618,642]" pageId="16" pageNumber="289" value="1911.0">1911</number>
