<taxonomicName id="6EE38E81FFCDDE4B2268FCD1FC91FCEA" authority="(DiStant, 1881)" authorityName="Lee" authorityYear="2016" baseAuthorityName="DiStant" baseAuthorityYear="1881" box="[567,867,775,799]" class="Insecta" family="Cicadidae" genus="Changa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="597" phylum="Arthropoda" rank="species" species="sita" status="comb. nov.">
<emphasis id="9B972910FFCDDE4B2268FCD1FD4BFCEA" box="[567,697,775,799]" italics="true" pageId="1" pageNumber="597">Changa sita</emphasis>
