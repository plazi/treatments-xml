<collectionCode id="D2B406B63B224E15788E3C2C5497FDE4" LSID="urn:lsid:biocol.org:col:12716" box="[1202,1261,548,572]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:12716" name="Bristol, Clifton and West of England Zoological Society's Gardens" pageId="6" pageNumber="327">CON</collectionCode>
