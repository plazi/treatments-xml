<paragraph id="8B9B36B470352C01FF5EFD3ACA21FD79" blockId="5.[151,594,690,743]" box="[151,594,721,743]" pageId="5" pageNumber="48">
<treatmentCitationGroup id="AB34119A70352C01FF5EFD3ACA21FD79" box="[151,594,721,743]" pageId="5" pageNumber="48">
<taxonomicName id="4C244D3770352C01FF5EFD3ACA71FD79" authority="Curtiss, 1938: 170" authorityName="Curtiss" authorityPageNumber="170" authorityYear="1938" box="[151,514,721,743]" class="Malacostraca" family="Cancridae" genus="Cancer" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="48" phylum="Arthropoda" rank="species" species="tahiticus" status="n. syn.">
<emphasis id="B950EAA670352C01FF5EFD3AC94CFD79" box="[151,319,721,743]" italics="true" pageId="5" pageNumber="48">Cancer tahiticus</emphasis>
<treatmentCitation id="0A8510A570352C01FE8CFD39CA71FD79" author="Curtiss" box="[325,514,721,743]" page="170" pageId="5" pageNumber="48" year="1938">
<bibRefCitation id="EFB54B4570352C01FE8CFD39CA71FD79" author="Curtiss" box="[325,514,721,743]" firstAuthor="Curtiss" pageId="5" pageNumber="48" pagination="1 - 193" refId="ref10067" refString="Curtiss, A. (1938) A short zoology of Tahiti in the Society Islands. Guide Printing, Brooklyn, New York, pp. i - xvi, 1 - 193." type="book chapter" year="1938">
Curtiss, 
<number id="3DEC249370352C01FE53FD3AC9A3FD79" box="[410,464,722,743]" pageId="5" pageNumber="48" value="1938.0">1938</number>
: 
<number id="3DEC249370352C01FE13FD3ACA71FD79" box="[474,514,722,743]" pageId="5" pageNumber="48" value="170.0">170</number>
</bibRefCitation>
</treatmentCitation>
</taxonomicName>
, 
<taxonomicNameLabel id="A26357DD70352C01FDC5FD3ACA21FD79" box="[524,594,722,743]" pageId="5" pageNumber="48">
<emphasis id="B950EAA670352C01FDC5FD3ACA3FFD79" bold="true" box="[524,588,722,743]" pageId="5" pageNumber="48">n. syn</emphasis>
