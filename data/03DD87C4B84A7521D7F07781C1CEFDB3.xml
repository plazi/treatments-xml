<taxonomicName id="4C744D51B8487526D62073FFC07FFA52" box="[327,512,1478,1500]" class="Insecta" family="Leptophlebiidae" genus="Thraulodes" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="574" phylum="Arthropoda" rank="species" species="luisae" status="sp. nov.">
