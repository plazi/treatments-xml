<taxonomicName id="4C3C4D6F5E35FFF1FC5BFE0543EA1E80" box="[933,1014,441,464]" class="Insecta" family="Coreidae" genus="Cercinthus" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="473" phylum="Arthropoda" rank="species" species="griseus">griseus</taxonomicName>
