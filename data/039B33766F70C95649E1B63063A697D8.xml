<emphasis id="B9465E726F70C95649B1B2A8603B97D9" bold="true" box="[199,345,1977,2004]" pageId="25" pageNumber="26">Distribution</emphasis>
. South 
<collectingCountry id="F325C2F06F70C95648C6B2AB609197D9" box="[432,499,1978,2004]" name="China" pageId="25" pageNumber="26">China</collectingCountry>
(
<collectingRegion id="49F64C826F70C9564B77B2AB630297D9" box="[513,608,1978,2004]" country="China" name="Sichuan" pageId="25" pageNumber="26">Sichuan</collectingRegion>
, 
<collectingRegion id="49F64C826F70C9564B1DB2AB63DA97D8" box="[619,696,1978,2005]" country="China" name="Fujian" pageId="25" pageNumber="26">Fujian</collectingRegion>
).
