<taxonomicName id="5BD230D9FFCB8D2D3C96FEB3FC08FE43" baseAuthorityName="d'Orbigny" baseAuthorityYear="1850" box="[760,965,374,400]" class="Gastropoda" family="Batillariidae" genus="Batillaria" kingdom="Animalia" pageId="18" pageNumber="321" phylum="Mollusca" rank="species" species="subacuta">
