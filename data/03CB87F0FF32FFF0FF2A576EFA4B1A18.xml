<emphasis id="B916EAF4FF32FFF3FE7D56BFFDDC1DE3" box="[448,523,1458,1479]" italics="true" pageId="25" pageNumber="26">
Jv 
<number id="3DAA24C1FF32FFF3FE6B56BFFE361DE3" box="[470,481,1458,1479]" pageId="25" pageNumber="26" value="2.0">2</number>
