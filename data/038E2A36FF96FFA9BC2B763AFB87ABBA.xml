<typeStatus id="549C2582FF96FFA9BD9E76B9FF1AAD69" box="[136,191,1012,1035]" pageId="2" pageNumber="189">Type</typeStatus>
species. – 
<taxonomicName id="4C27E0A3FF96FFA9BC2176B9FD45AD69" authority="Thorell, 1881" authorityName="Thorell" authorityYear="1881" box="[311,736,1012,1035]" class="Arachnida" family="Salticidae" genus="Plexippus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="189" phylum="Arthropoda" rank="species" species="macrognathus">
