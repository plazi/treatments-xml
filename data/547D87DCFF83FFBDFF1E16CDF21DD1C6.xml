<taxonomicName id="1BD44D49FF83FFBCFB2B156AF30BD026" box="[1186,1347,816,840]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="fortunensis">
<emphasis id="EEA0EAD8FF83FFBCFB2B156AF30BD026" box="[1186,1347,816,840]" italics="true" pageId="14" pageNumber="50">
<collectingCountry id="A4C3765AFF83FFBCFB2B156AF2F1D029" box="[1186,1209,816,839]" name="Iceland" pageId="14" pageNumber="50">O</collectingCountry>
