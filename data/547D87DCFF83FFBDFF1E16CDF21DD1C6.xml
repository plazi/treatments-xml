<taxonomicName id="1BD44D49FF83FFBCFC80149DF522D1B0" box="[777,874,710,734]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="alleni">
<emphasis id="EEA0EAD8FF83FFBCFC80149DF522D1B0" box="[777,874,710,734]" italics="true" pageId="14" pageNumber="50">
