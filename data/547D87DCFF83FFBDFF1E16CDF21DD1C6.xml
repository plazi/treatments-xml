<quantity id="1B2C9B2FFF83FFBCFE47152CF455D0E0" box="[462,541,886,910]" metricMagnitude="-2" metricUnit="m" metricValue="2.2606" pageId="14" pageNumber="50" unit="in" value="0.89">0.89 in</quantity>
<taxonomicName id="1BD44D49FF83FFBCFDAD152DF4EBD0E0" box="[548,675,886,910]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="complex">
