<taxonomicName id="1BD44D49FF83FFBCFD6414B0F52DD06F" box="[749,869,746,769]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="savagei">
