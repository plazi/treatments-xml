<emphasis id="EEA0EAD8FF83FFBCFB6A14B0F313D06F" box="[1251,1371,746,769]" italics="true" pageId="14" pageNumber="50">
<collectingCountry id="A4C3765AFF83FFBCFB6A14B0F2B2D06F" box="[1251,1274,746,769]" name="Iceland" pageId="14" pageNumber="50">O</collectingCountry>
