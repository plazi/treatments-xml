<taxonomicName id="1BD44D49FF83FFBCFDE0156AF541D026" box="[617,777,816,840]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="fortunensis">
