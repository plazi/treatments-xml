<quantity id="1B2C9B2FFF83FFBCFC43156AF26AD026" box="[970,1058,816,840]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="14" pageNumber="50" unit="mm" value="1.2">1.2 mm</quantity>
