. alleni
</emphasis>
</taxonomicName>
); from 
<taxonomicName id="1BD44D49FF83FFBCFC37149DF27ED1B0" box="[958,1078,711,734]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="savagei">
