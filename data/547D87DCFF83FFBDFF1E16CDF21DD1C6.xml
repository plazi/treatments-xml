<taxonomicName id="1BD44D49FF83FFBCFBFE152DF2BED0E0" box="[1143,1270,886,910]" class="Amphibia" family="Plethodontidae" genus="Oedipina" kingdom="Animalia" order="Caudata" pageId="14" pageNumber="50" phylum="Chordata" rank="species" species="complex">
