<paragraph id="0045E60AD83AFFA6FF59FA61FD5AF9DD" blockId="3.[175,1005,1337,1571]" pageId="3">
<taxonomicName id="C7FA9D89D83AFFA6FF59FA61FE70FA61" authority="Schaeffer" authorityName="Schaeffer" authorityYear="1910" box="[199,481,1417,1438]" class="Insecta" family="Carabidae" genus="Pterostichus" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="species" species="palmi">
