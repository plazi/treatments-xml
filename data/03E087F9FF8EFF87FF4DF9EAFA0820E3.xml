<taxonomicName id="4C494D6CFF8EFF87FF4DF9EAFE742362" authority="Schinz" authorityName="Schinz" box="[170,426,1561,1587]" class="Magnoliopsida" family="Passifloraceae" genus="Adenia" kingdom="Plantae" order="Malpighiales" pageId="8" pageNumber="87" phylum="Tracheophyta" rank="species" species="glauca">
<emphasis id="B93DEAFDFF8EFF87FF4DF9EAFE8E2362" box="[170,336,1561,1587]" italics="true" pageId="8" pageNumber="87">ADENIA GLAUCA</emphasis>
