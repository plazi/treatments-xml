, 
<geoCoordinate id="EE0D5062927E433EFCC8F48CFC42FE7A" box="[858,990,446,469]" degrees="16" direction="west" minutes="26. 03" orientation="longitude" pageId="4" pageNumber="41" precision="9" value="-16.433834">16º26.03’W</geoCoordinate>
, 
<quantity id="4CC19B40927E433EFC7DF48CFBDEFE7A" box="[1007,1090,446,469]" metricMagnitude="3" metricUnit="m" metricValue="4.8469999999999995" pageId="4" pageNumber="41" unit="m" value="4847.0">4847 m</quantity>
, 
<date id="FF871065927E433EFBC0F48CFB44FE7A" box="[1106,1240,446,469]" pageId="4" pageNumber="41" value="1997-07-25">25.VII.1997</date>
