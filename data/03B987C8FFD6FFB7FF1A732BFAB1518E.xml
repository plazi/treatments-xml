<number id="3DD824F9FFD4FFB7FDAE75D4FDA152FF" box="[547,600,188,213]" pageId="38" pageNumber="39" value="29.5">29.5</number>
; mid basitarsus length, 
<number id="3DD824F9FFD4FFB7FCD275D4FC8552FE" box="[863,892,188,212]" pageId="38" pageNumber="39" value="11.0">11</number>
; mid tibial spur length, 
<number id="3DD824F9FFD4FFB7FB0A75D4FB6E52FE" box="[1159,1175,188,212]" pageId="38" pageNumber="39" value="8.0">8</number>
