<paragraph id="8BAF36DEFFD7FFB7FF4A72C3FB6552FE" blockId="37.[151,1437,1566,2023]" lastBlockId="38.[151,1437,151,933]" lastPageId="38" lastPageNumber="39" pageId="37" pageNumber="38">
Mesosoma (
<figureCitation id="132B2A5BFFD7FFB4FED572C2FE4B55E9" box="[344,434,1962,1987]" captionStart="FIGURES 91 – 95" captionStartId="37.[151,264,1468,1491]" captionTargetBox="[206,1406,355,1426]" captionTargetId="figure@37.[151,1436,337,1447]" captionTargetPageId="37" captionText="FIGURES 91 – 95. Metaphycus albidus Hayat, sp. nov. Female: 91, antenna; 92, palps; 93, fore wing; 94, mesosoma; 95, part of mesosoma with legs, and metasoma." httpUri="https://zenodo.org/record/285951/files/figure.png" pageId="37" pageNumber="38">
