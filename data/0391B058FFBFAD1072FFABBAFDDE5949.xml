<date id="FF86278EFFBFAD107189AA22FBE05E54" box="[964,1086,320,340]" pageId="19" pageNumber="2044" value="1999-10-15" valueMax="1999-10-21" valueMin="1999-10-15">15-21.X.1999</date>
and 
<date id="FF86278EFFBFAD107349AA3AFEA05E6C" box="[260,382,344,364]" pageId="19" pageNumber="2044" value="1999-10-15" valueMax="1999-10-21" valueMin="1999-10-15">15-21.X.1999</date>
