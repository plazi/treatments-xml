<paragraph id="1E0E36850E5BFFB1FFA5FB50FF0E51A1" blockId="13.[106,235,1217,1241]" box="[106,235,1217,1241]" pageId="13" pageNumber="178">
<emphasis id="2CC5EA970E5BFFB1FFA5FB50FF0E51A1" bold="true" box="[106,235,1217,1241]" pageId="13" pageNumber="178">Bemerkung:</emphasis>
