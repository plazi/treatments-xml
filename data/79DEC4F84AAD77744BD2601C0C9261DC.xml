, 
<bibRefCitation id="BCA5F91845C6D87B1C348968590157BB" author="Campbell, JM" editor="Bousquet, Y" journalOrPublisher="Agriculture Canada, Ottawa" pageId="26" pageNumber="55" pagination="86 - 129" title="Family Staphylinidae" volumeTitle="Checklist of beetles of Canada and Alaska" year="1991">Campbell and Davies 1991</bibRefCitation>
, 
<bibRefCitation id="A7300B636AD10B7BB446F936D681E062" author="Lundgren, RW" editor="Peck, SB" journalOrPublisher="Arthropods of Florida and Neighboring Land Areas" pageId="27" pageNumber="56" pagination="1 - 180" title="Family Staphylinidae" volume="16" year="1998">Lundgren 1998</bibRefCitation>
) 2 
<taxonomicName id="86C3E5AA0AF04F52A449F0F606C20769" class="Insecta" family="Staphylinidae" genus="Porrhodites" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Porrhodites fenestralis" order="Coleoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="species" species="fenestralis">Porrhodites fenestralis</taxonomicName>
(Zetterstedt) (Campbell 1984) 3 
<taxonomicName id="EBE7E3E0436EEF28F5676B2BDAC05C55" class="Insecta" family="Staphylinidae" genus="Ischnosoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ischnosoma flavicolle" order="Coleoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="species" species="flavicolle">Ischnosoma flavicolle</taxonomicName>
(LeConte) (
<bibRefCitation id="730A728ED986FB83AB1A8BA9818B6143" author="Campbell, JM" journalOrPublisher="Memoirs of the Entomological Society of Canada" pageId="26" pageNumber="55" pagination="1 - 169" title="A revision of the genera Mycetoporus Mannerheimand Ischnosoma Stephens (Coleoptera: Staphylinidae: Tachyporinae) of North and Central America." volume="156" year="1991">Campbell 1991</bibRefCitation>
) 4 
<taxonomicName id="A1F7A2B6B417A8713887FFE9DD7FD21F" class="Insecta" family="Staphylinidae" genus="Sepedophilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sepedophilus campbelli" order="Coleoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="species" species="campbelli">Sepedophilus campbelli</taxonomicName>
