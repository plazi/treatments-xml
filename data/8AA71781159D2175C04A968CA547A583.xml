<taxonomicName id="AB4D82D58DE2833CD28BA93DA0013A0A" class="Insecta" family="Apidae" genus="Trigona" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigona meadewaldoi" order="Hymenoptera" pageId="8" pageNumber="27" phylum="Arthropoda" rank="species" species="meadewaldoi">Trigona meadewaldoi</taxonomicName>
