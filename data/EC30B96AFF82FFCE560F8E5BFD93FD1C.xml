<taxonomicName id="A39973FFFF82FFCF57118833FB92F8EE" box="[780,1075,1892,1919]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="variety" species="orientalis" variety="gibbosa">
