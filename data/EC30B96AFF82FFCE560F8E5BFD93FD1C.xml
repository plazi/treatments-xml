<taxonomicName id="A39973FFFF82FFCF503F884BFB7AF8A5" box="[1058,1243,1820,1846]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="ovata">
