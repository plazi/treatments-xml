<taxonomicName id="A39973FFFF83FFCE55BF8EE8FD99FE48" box="[418,568,446,473]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="orientalis">C. orientalis</taxonomicName>
