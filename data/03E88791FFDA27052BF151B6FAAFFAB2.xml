<taxonomicName id="4C414D04FFDA270529AC553FFEFBF8AB" authority="Walker, 1872" authorityName="Walker" authorityYear="1872" class="Insecta" family="Lygaeidae" genus="Ophthalmicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="584" phylum="Arthropoda" rank="species" species="cinarescens">
