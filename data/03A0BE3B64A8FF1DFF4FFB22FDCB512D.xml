<taxonomicName id="4C0974AE64A8FF1DFEEBFA71FE685295" authorityName="Thomas" authorityYear="1907" box="[307,425,1510,1535]" class="Mammalia" family="Dipodidae" genus="Sicista" kingdom="Animalia" order="Rodentia" pageId="162" pageNumber="163" phylum="Chordata" rank="species" species="caudata">
