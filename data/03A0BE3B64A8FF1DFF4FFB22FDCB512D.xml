<taxonomicName id="4C0974AE64A8FF1DFCB7FA55FB6552B1" authority="(Corbet, 1978)" baseAuthorityName="Corbet" baseAuthorityYear="1978" box="[879,1188,1474,1499]" class="Mammalia" family="Erinaceidae" genus="Erinaceus" kingdom="Animalia" order="Erinaceomorpha" pageId="162" pageNumber="163" phylum="Chordata" rank="species" species="concolor">
