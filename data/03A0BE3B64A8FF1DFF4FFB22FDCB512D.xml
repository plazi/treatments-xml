<taxonomicName id="4C0974AE64A8FF1DFE36FA71FC535295" authority="(Holden &amp; Musser 2005)" baseAuthorityName="Holden &amp; Musser" baseAuthorityYear="2005" box="[494,914,1510,1535]" class="Mammalia" family="Erinaceidae" genus="Erinaceus" kingdom="Animalia" order="Erinaceomorpha" pageId="162" pageNumber="163" phylum="Chordata" rank="species" species="concolor">
