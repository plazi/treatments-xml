</quantity>
(N=3), width 
<quantity id="4CF89B6FFFE0F422464B31D17EAA3B90" box="[783,947,1439,1463]" metricMagnitude="-3" metricUnit="m" metricValue="1.14" metricValueMax="1.27" metricValueMin="1.01" pageId="30" pageNumber="31" unit="mm" value="1.14" valueMax="1.27" valueMin="1.01">
