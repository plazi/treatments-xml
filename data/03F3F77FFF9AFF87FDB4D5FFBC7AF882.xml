<taxonomicName id="4C5A3DEAFF9BFF87FDB4D6D2BDE4F915" authorityName="Kloss" authorityYear="1921" box="[565,687,1695,1720]" class="Mammalia" family="Pteropodidae" genus="Balionycteris" kingdom="Animalia" order="Chiroptera" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="seimundi">
