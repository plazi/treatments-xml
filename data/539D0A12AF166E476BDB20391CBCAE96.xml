<taxonomicName id="2CE832DB08FDC81CB3B0F6BFD47AABD1" authority="Clark, 1865" authorityName="Clark" authorityYear="1865" class="Insecta" family="Chrysomelidae" genus="Sphenoraia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sphenoraia chujoi" order="Coleoptera" pageId="9" pageNumber="130" phylum="Arthropoda" rank="species" species="chujoi">Sphenoraia chujoi</taxonomicName>
