<taxonomicName id="4C364D20FFA94745FC14FA5AFBC6FA31" authorityName="Kuschel" authorityYear="2008" baseAuthorityName="Montrouzier" baseAuthorityYear="2008" box="[923,1036,1410,1434]" class="Insecta" family="Curculionidae" genus="Orthorhinoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="109" pageNumber="208" phylum="Arthropoda" rank="species" species="centurio">
