<taxonomicName id="4C2F4D69FFA4A7623AC90794FCCE77A9" authorityName="Stechow" authorityYear="1919" box="[699,820,404,428]" class="Hydrozoa" family="Kirchenpaueriidae" genus="Oswaldella" kingdom="Animalia" order="Leptothecata" pageId="48" pageNumber="852" phylum="Cnidaria" rank="genus">
