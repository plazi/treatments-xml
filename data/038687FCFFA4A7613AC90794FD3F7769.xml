<taxonomicName id="4C2F4D69FFA4A76239D30012FBCC7022" authority="Pena Cantero and Garcia Carrascosa, 1998" authorityName="Pena Cantero and Garcia Carrascosa" authorityYear="1998" box="[417,1078,1554,1576]" class="Hydrozoa" family="Kirchenpaueriidae" genus="Oswaldella" kingdom="Animalia" order="Leptothecata" pageId="48" pageNumber="852" phylum="Cnidaria" rank="species" species="vervoorti">
