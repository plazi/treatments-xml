<taxonomicName id="37A04D0C2A5BFF877C4497B5FB9260D5" authority="Parkinson, 1811" authorityName="Parkinson" authorityYear="1811" box="[855,1113,995,1019]" class="Insecta" family="Chrysomelidae" genus="Harpax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="182" phylum="Arthropoda" rank="genus">
