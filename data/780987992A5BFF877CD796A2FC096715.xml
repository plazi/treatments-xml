<quantity id="37589B6A2A5BFF877B3697B2FB9260D5" box="[1061,1113,995,1018]" metricMagnitude="1" metricUnit="m" metricValue="4.59994" pageId="9" pageNumber="182" unit="in" value="1811.0">1811</quantity>
</taxonomicName>
