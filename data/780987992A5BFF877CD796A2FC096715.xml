<taxonomicName id="37A04D0C2A5BFF877BFB97B5FAF260D4" authorityName="Parkinson" authorityYear="1811" box="[1256,1337,996,1019]" class="Insecta" family="Chrysomelidae" genus="Harpax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="182" phylum="Arthropoda" rank="genus">
