<quantity id="B5199B09248CBDD5FE53DA401960A718" box="[499,607,1026,1049]" metricMagnitude="4" metricUnit="m" metricValue="1.25" metricValueMax="1.5" metricValueMin="1.0" pageId="146" pageNumber="147" unit="km" value="12.5" valueMax="15.0" valueMin="10.0">10–15 km</quantity>
