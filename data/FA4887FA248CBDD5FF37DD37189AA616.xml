<quantity id="B5199B09248CBDD5FC85DA601861A738" box="[805,862,1058,1081]" metricMagnitude="1" metricUnit="m" metricValue="9.0" pageId="146" pageNumber="147" unit="m" value="90.0">90 m</quantity>
