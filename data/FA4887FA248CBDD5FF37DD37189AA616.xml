<geoCoordinate id="17D5502B248CBDD5FDF9DA601995A738" box="[601,682,1058,1081]" degrees="5" direction="north" minutes="35" orientation="latitude" pageId="146" pageNumber="147" precision="925" value="5.5833335">5°35’N</geoCoordinate>
