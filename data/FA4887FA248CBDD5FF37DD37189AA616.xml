<geoCoordinate id="17D5502B248CBDD5FD17DA601827A738" box="[695,792,1058,1081]" degrees="76" direction="west" minutes="37" orientation="longitude" pageId="146" pageNumber="147" precision="925" value="-76.61667">76°37’W</geoCoordinate>
