<taxonomicName id="DE8B9CB3304096D433E3448DD5DA092E" LSID="http://zoobank.org/6C7DCAE6-9192-49F3-B4DC-77A9F0DCF4A3" authority="Bläser, Krogmann &amp; Peters, 2015" authorityName="Bläser, Krogmann &amp; Peters" authorityYear="2015" class="Insecta" family="Pteromalidae" genus="Pteropilosa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pteropilosa lailarabanorum" order="Hymenoptera" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="lailarabanorum">Pteropilosa lailarabanorum</taxonomicName>
