<taxonomicName id="BFFD1EE9FC1F83AE1349E46B9F0042FD" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia maculipennis" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="maculipennis">Loboscelidia maculipennis</taxonomicName>
