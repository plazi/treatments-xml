<paragraph id="6F43F2F486F3A83A9CF6D2D89E7693CE" pageId="6" pageNumber="7">
Four 
<taxonomicName id="1223EFB9CB4283195B98F4C60F3AA9D1" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">Loboscelidia</taxonomicName>
