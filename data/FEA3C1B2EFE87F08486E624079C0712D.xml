<taxonomicName id="B7825D54ACA7C97742B0A1CAF6586B9F" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia brunnea" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="brunnea">Loboscelidia brunnea</taxonomicName>
