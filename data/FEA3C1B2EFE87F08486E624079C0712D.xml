<taxonomicName id="0415CC5915B6491F95E91C09D2FD3246" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia maai" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="maai">Loboscelidia maai</taxonomicName>
