<typeStatus id="3210A5D7E057FFB71965FBF0FF57486B" box="[113,162,1086,1109]" pageId="19" pageNumber="666">TYPE</typeStatus>
LOCALITY
</collectionCode>
. — 
<collectingCountry id="95BC5BE5E057FFB71824FBF0FE834869" box="[304,374,1086,1110]" name="Gabon" pageId="19" pageNumber="666">Gabon</collectingCountry>
