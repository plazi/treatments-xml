<subSubSection id="9AA47D9EE86DAD26A3A8FEBCFBC6FE47" pageId="8" pageNumber="435" type="biology_ecology">
<paragraph id="D2012E15E86DAD26A3A8FEBCFE73FEAE" blockId="8.[151,1436,152,410]" pageId="8" pageNumber="435">
<emphasis id="E0CAF207E86DAD26A3A8FEBCFED4FE91" bold="true" box="[198,303,306,332]" pageId="8" pageNumber="435">Ecology.</emphasis>
