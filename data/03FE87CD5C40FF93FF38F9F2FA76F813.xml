, 
<taxonomicName id="4C574D585C40FF93FE3FF860FD0BF813" authority="(Erichson, 1839)" baseAuthorityName="Erichson" baseAuthorityYear="1839" box="[400,737,1925,1951]" class="Insecta" family="Mantispidae" genus="Dicromantispa" kingdom="Animalia" order="Neuroptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="gracilis">
