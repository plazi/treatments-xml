</taxonomicName>
, 
<taxonomicName id="4C574D585C40FF93FD41F860FBDCF813" authority="Hoffman, 2002" authorityName="Hoffman" authorityYear="2002" box="[750,1078,1925,1951]" class="Insecta" family="Mantispidae" genus="Dicromantispa" kingdom="Animalia" order="Neuroptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="synapsis">
