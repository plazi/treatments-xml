<taxonomicName id="4C574D585C40FF93FA88F885FE6EF813" authority="(Gerstaecker, 1888)" baseAuthorityName="Gerstaecker" baseAuthorityYear="1888" class="Insecta" family="Mantispidae" genus="Dicromantispa" kingdom="Animalia" order="Neuroptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="debilis">
