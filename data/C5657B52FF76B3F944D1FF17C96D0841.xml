to off 
<collectingCounty id="A412B2C8FF76B3F9468BFCC7CB770809" box="[717,881,781,805]" pageId="234" pageNumber="235">Puerto Pizarro</collectingCounty>
, 
<collectingCountry id="35DB8AD4FF76B3F9473AFCC7CBB50809" box="[892,947,782,805]" name="Peru" pageId="234" pageNumber="235">Peru</collectingCountry>
