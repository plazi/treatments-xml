<collectingCountry id="F368DDED7703FFA74CBB5E83FC82AEA7" box="[725,813,186,212]" name="Mexico" pageId="96" pageNumber="97">Mexico</collectingCountry>
, 
<collectingCountry id="F368DDED7703FFA74D535E83FC28AEA7" box="[829,903,186,212]" name="Belize" pageId="96" pageNumber="97">Belize</collectingCountry>
, 
<collectingCountry id="F368DDED7703FFA74DF65E82FC5EAEA6" box="[920,1009,187,213]" name="Panama" pageId="96" pageNumber="97">Panama</collectingCountry>
(
<collectingRegion id="49BB539F7703FFA74A6A5E83FB14AEA6" box="[1028,1211,186,213]" country="Panama" name="Bocas del Toro" pageId="96" pageNumber="97">Bocas del Toro</collectingRegion>
