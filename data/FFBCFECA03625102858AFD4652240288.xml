; 
<geoCoordinate id="C58FD5097BE1F9011703E02C4ACE9CBA" degrees="67" direction="north" minutes="32" orientation="latitude" precision="15" seconds="50" value="67.54722">67°32'50&quot;N</geoCoordinate>
