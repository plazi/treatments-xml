<collectingCountry id="F37276410579486C407DFB4DFE481BE3" box="[359,426,1161,1187]" name="China" pageId="3" pageNumber="260">China</collectingCountry>
(
<collectingRegion id="49A1F8330579486C40A3FB4DFDF71BE3" box="[441,533,1161,1187]" country="China" name="Yunnan" pageId="3" pageNumber="260">Yunnan</collectingRegion>
