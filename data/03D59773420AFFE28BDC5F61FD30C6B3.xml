<quantity id="4C848B80420AFFED8AE05F8DFDB3C67A" box="[427,547,1791,1815]" metricMagnitude="-2" metricUnit="m" metricValue="4.0" metricValueMax="4.2" metricValueMin="3.8" pageId="7" pageNumber="373" unit="mm" value="40.0" valueMax="42.0" valueMin="38.0">38–42 mm</quantity>
