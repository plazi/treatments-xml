<paragraph id="87B958BF45A4BF6190E3B476459E97BB" pageId="15" pageNumber="70">
<taxonomicName id="DC0329DA2401EA8587A3BED83FA21E9D" class="Insecta" family="Rhinotermitidae" genus="Rhinotermes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhinotermes intermedius" order="Blattodea" pageId="15" pageNumber="70" phylum="Arthropoda" rank="species" species="intermedius">Rhinotermes intermedius</taxonomicName>
