<subSubSection id="17AD6561FFFAFFD9FF10FBCCFD13552D" pageId="28" pageNumber="27" type="etymology">
<paragraph id="5F0836EAFFFAFFD9FF10FBCCFD13552D" blockId="28.[192,1440,995,1115]" pageId="28" pageNumber="27">
<emphasis id="6DC3EAF8FFFAFFD9FF10FBCCFED5554D" bold="true" box="[192,339,1059,1083]" pageId="28" pageNumber="27">Etymology</emphasis>
