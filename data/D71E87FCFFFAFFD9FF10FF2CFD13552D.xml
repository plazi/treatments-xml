<taxonomicName id="98B74D69FFFAFFD9FE25FBCCFDE1554D" box="[501,615,1059,1083]" class="Insecta" family="Leiodidae" genus="Zeadolopus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="dominica">
