<taxonomicName id="4C475EA4FFBE2D53FDF9FE5DBD17F8C5" box="[628,895,450,475]" class="Insecta" family="Hemiptera" genus="Atomophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="bipunctata">
