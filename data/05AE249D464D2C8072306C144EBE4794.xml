<taxonomicName id="4E46EA9C05539AFA41707C3A580692C1" genus="T." lsidName="T. benoiti" pageId="0" pageNumber="43" rank="species" species="benoiti">T. benoiti</taxonomicName>
(Brignoli, 1978), 
<taxonomicName id="E2879D46A9749E8CB6F13A50E7231890" genus="T." lsidName="T. brevidens" pageId="0" pageNumber="43" rank="species" species="brevidens">T. brevidens</taxonomicName>
