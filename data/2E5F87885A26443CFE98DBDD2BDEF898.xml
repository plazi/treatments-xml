<subSubSection id="EEEC65155A27443CFEA8DBAA29CDF928" pageId="11" pageNumber="12" type="description">
<paragraph id="A649369E5A27443CFEA8DBAA29CDF928" blockId="11.[264,1323,284,1881]" pageId="11" pageNumber="12">
<emphasis id="9482EA8C5A27443CFEA8DBAA29D4F902" bold="true" box="[312,427,1705,1731]" pageId="11" pageNumber="12">Variation</emphasis>
