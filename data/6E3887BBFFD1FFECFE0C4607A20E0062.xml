<paragraph id="E62E36ADFFD1FFECFF6246A8A20E0062" blockId="8.[151,1127,1212,1332]" pageId="8" pageNumber="233">
<taxonomicName id="21914D2EFFD1FFECFF6246A8A25D0182" authorityName="Gistel" authorityYear="1857" box="[181,366,1213,1236]" class="Insecta" family="Nitidulidae" genus="Thalycra" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="233" phylum="Arthropoda" rank="species" species="aridosa">
