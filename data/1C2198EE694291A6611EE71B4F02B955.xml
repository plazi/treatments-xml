<normalizedToken id="121C27C5DAFA3D1C7E4C657904B17852" originalValue="Aripuanã">Aripuana</normalizedToken>
, Parque Nacional dos Campos 
<normalizedToken id="4EC423DAA8990FCD0C11D8B52ECED304" originalValue="Amazônicos">Amazonicos</normalizedToken>
, Rio Roosevelt, Madeira Basin, 
<geoCoordinate id="05856DB2CE10F3CE729E3CF6F9311234" direction="south" orientation="latitude" precision="15" value="-8.1975">8°11'51&quot;S</geoCoordinate>
, 
<geoCoordinate id="674B91DAB656A1932EE764C6506ED67F" direction="west" orientation="longitude" precision="1" value="-60.972">60°58'19.2&quot;W</geoCoordinate>
