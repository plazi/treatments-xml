<emphasis id="B91EEAF73A3A7D5BFBF6E38AFD57FA2F" bold="true" box="[151,666,1478,1504]" pageId="32" pageNumber="253">
<taxonomicName id="4C6A4D663A3A7D5BFBF6E38AFDE2FA2F" authority="Khalaim et Broad" authorityName="Khalaim et Broad" box="[151,559,1478,1504]" class="Insecta" family="Ichneumonidae" genus="Stethantyx" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="253" phylum="Arthropoda" rank="species" species="osa" status="sp. nov.">
