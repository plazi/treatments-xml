<taxonomicName id="6DDA238FA3162164468C282879CD7A3D" class="Rosopsida" family="Nothofagaceae" genus="Nothofagus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Nothofagus" order="Fagales" pageId="5" pageNumber="48" phylum="Magnoliophyta" rank="genus">
<emphasis id="E49215A2D976D7919BD7CCAABA927374" italics="true" pageId="5" pageNumber="48">Nothofagus</emphasis>
</taxonomicName>
