<taxonomicName id="8D72AA7D9843FE7BD0EEC17B2315BE93" authorityName="R. M. Schuster" authorityYear="1963" class="Jungermanniopsida" family="Lepidoziaceae" genus="Acromastigum" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="Acromastigum cavifolium" order="Jungermanniales" pageId="5" pageNumber="48" phylum="Marchantiophyta" rank="species" species="cavifolium">
<emphasis id="44C6644DD605EA5605D57391A74F0707" italics="true" pageId="5" pageNumber="48">Acromastigum cavifolium</emphasis>
