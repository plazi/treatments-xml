<taxonomicName id="CE6218C5C8395B865B48F8CF6362F38D" class="Magnoliopsida" family="Proteaceae" genus="Banksia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Banksia" order="Proteales" pageId="5" pageNumber="48" phylum="Magnoliophyta" rank="genus">
