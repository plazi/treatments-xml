<taxonomicName id="F38B09B71753A97C9981F8D17A56F8CF" authorityName="Oláh &amp; Johanson" authorityYear="2008" box="[395,533,1818,1843]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="pluvialis">
