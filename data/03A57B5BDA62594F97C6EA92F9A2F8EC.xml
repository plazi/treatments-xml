<geoCoordinate id="EE38AC8ADA62594F97A8EBF9F96AFA03" box="[716,860,1481,1508]" degrees="115" direction="east" minutes="27" orientation="longitude" pageId="50" pageNumber="51" precision="15" seconds="1" value="115.45027">115º27’1” E</geoCoordinate>
