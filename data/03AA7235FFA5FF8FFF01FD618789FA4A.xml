<taxonomicName id="4C03B8A0FFA5FF8FFF46FA34862FFA2B" box="[239,369,1422,1446]" class="Magnoliopsida" kingdom="Plantae" order="Gentianales" pageId="27" pageNumber="1030" phylum="Tracheophyta" rank="order">Gentianales</taxonomicName>
: Family 
