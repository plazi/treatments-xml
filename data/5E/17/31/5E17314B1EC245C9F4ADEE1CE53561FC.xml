<paragraph pageId="null" pageNumber="788">
<taxonomicName authority="L." class="Magnoliopsida" family="Caryophyllaceae" genus="Gypsophila" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="788" phylum="Tracheophyta" rank="species" species="repens">
Gypsophila 
<normalizedToken originalValue="répens" pageId="null" pageNumber="788">repens</normalizedToken>
<authorityName pageId="null" pageNumber="788">L.</authorityName>
</taxonomicName>
</paragraph>
