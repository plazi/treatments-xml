<number box="[1017,1089,1940,1964]" pageId="3" pageNumber="638" value="28110.0">28110</number>
, MI QLD 
<number box="[1206,1262,1940,1964]" pageId="3" pageNumber="638" value="2379.0">2379</number>
(
<number box="[1278,1295,1940,1964]" pageId="3" pageNumber="638" value="2.0">2</number>
