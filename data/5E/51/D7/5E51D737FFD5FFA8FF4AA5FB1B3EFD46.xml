Woodwick 
<number box="[1364,1424,295,320]" pageId="10" pageNumber="645" value="1964.0">1964</number>
</bibRefCitation>
), Mediterranean (
<bibRefCitation author="Bick" box="[332,457,332,357]" pageId="10" pageNumber="645" refString="Bick, A. (2001) The morphology and ecology of Dipolydora armata (Polychaeta, Spionidae) from the western Mediterranean Sea. Acta Zoologica, 82, 177 - 187. [Stockholm] http: // dx. doi. org / 10.1046 / j. 1463 - 6395.2001.00078. x" type="journal article" year="2001">
Bick 
<number box="[397,457,332,356]" pageId="10" pageNumber="645" value="2001.0">2001</number>
</bibRefCitation>
), 
<collectingCountry box="[480,606,332,357]" name="Philippines" pageId="10" pageNumber="645">Philippines</collectingCountry>
