<taxonomicName authority="Saussure" authorityName="Saussure" box="[536,830,1948,1974]" class="Insecta" family="Vespidae" genus="Vespula" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="sulphurea">
<emphasis box="[536,691,1949,1974]" italics="true" pageId="5" pageNumber="66">V. sulphurea</emphasis>
(Saussure)
</taxonomicName>
in northwest 
<collectingCountry box="[1003,1097,1948,1974]" name="Mexico" pageId="5" pageNumber="66">Mexico</collectingCountry>
(Baja California Norte), 
<taxonomicName class="Insecta" family="Vespidae" genus="Vespula" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="maculifrons">
<emphasis italics="true" pageId="5" pageNumber="66">V. maculifrons</emphasis>
</taxonomicName>
in northeast 
<collectingCountry box="[443,536,1986,2012]" name="Mexico" pageId="5" pageNumber="66">Mexico</collectingCountry>
