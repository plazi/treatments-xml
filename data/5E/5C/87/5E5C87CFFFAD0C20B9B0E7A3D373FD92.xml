<location LSID="urn:lsid:plazi:treatment:5E5C87CFFFAD0C20B9B0E7A3D373FD92:D32A6002FFA00C20B9B0E02CD2B3FE02" box="[151,237,366,392]" county="Kura River" latitude="41.133335" longLatPrecision="1158" longitude="42.85" municipality="Asikzulal" name="Turkish" pageId="8" pageNumber="51">Turkish</location>
part of 
<location LSID="urn:lsid:plazi:treatment:5E5C87CFFFAD0C20B9B0E7A3D373FD92:D32A6002FFA00C20B860E02DD399FE02" box="[327,455,366,393]" county="Kura River" latitude="41.133335" longLatPrecision="1158" longitude="42.85" municipality="Asikzulal" name="Kura River" pageId="8" pageNumber="51">Kura River</location>
, including in relatively larger streams at lower altitude (e.g. stream 
<collectingMunicipality box="[1218,1328,366,392]" pageId="8" pageNumber="51">Aşıkzülal</collectingMunicipality>
, 
<geoCoordinate box="[1339,1436,366,392]" degrees="41" direction="north" minutes="26" orientation="latitude" pageId="8" pageNumber="51" precision="925" value="41.433334">41°26’N</geoCoordinate>
<geoCoordinate box="[151,247,402,428]" degrees="42" direction="east" minutes="37" orientation="longitude" pageId="8" pageNumber="51" precision="925" value="42.616665">42°37’E</geoCoordinate>
, altitude 
