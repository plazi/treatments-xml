<location LSID="urn:lsid:plazi:treatment:5E5C87CFFFAD0C20B9B0E7A3D373FD92:D32A6002FFA00C20BDDAE065D77CFECB" box="[1277,1314,295,321]" county="Kura River" latitude="41.133335" longLatPrecision="1158" longitude="42.85" municipality="Asikzulal" name="We" pageId="8" pageNumber="51">We</location>
