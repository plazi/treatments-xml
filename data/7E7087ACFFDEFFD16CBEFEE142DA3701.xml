<subSubSection id="BEC36531FFDEFFD16CEEFE3E42DA3701" pageId="10" pageNumber="11" type="diagnosis">
<paragraph id="F66636BAFFDEFFD16CEEFE3E42DA3701" blockId="10.[151,1436,469,568]" pageId="10" pageNumber="11">
<emphasis id="C4ADEAA8FFDEFFD16CEEFE3E47103725" bold="true" box="[199,322,505,531]" pageId="10" pageNumber="11">Diagnosis.</emphasis>
