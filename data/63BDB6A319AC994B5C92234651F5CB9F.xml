sp. (
<taxonomicName id="5B71C466810DA31B55E941047CE41395" genus="Costaceae" lsidName="Costaceae" pageId="124" pageNumber="125" rank="genus">Costaceae</taxonomicName>
) (
<bibRefCitation id="57D74DE3DCA7B0A6B62C75CAA7BF543A" pageId="124" pageNumber="125">Staines 1996</bibRefCitation>
); 
<taxonomicName id="E7C14A30F3A36FFA161A539F818564D2" class="Liliopsida" family="Zingiberaceae" genus="Renealmia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Renealmia" order="Zingiberales" pageId="124" pageNumber="125" phylum="Tracheophyta" rank="genus">Renealmia</taxonomicName>
