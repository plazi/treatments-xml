<taxonomicName id="FC64791ADF33EF5BB17D117E6AA6EF88" genus="Zingiberaceae" lsidName="Zingiberaceae" pageId="124" pageNumber="125" rank="genus">Zingiberaceae</taxonomicName>
) (
