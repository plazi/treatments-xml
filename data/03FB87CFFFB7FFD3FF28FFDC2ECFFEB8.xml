<subSubSection id="C3486552FFB4FFD4FF78FAB32908FF5D" pageId="3" pageNumber="241" type="materials_examined">
<paragraph id="8BED36D9FFB4FFD4FF78FAB32908FF5D" blockId="3.[151,1437,151,573]" pageId="3" pageNumber="241">
