<paragraph id="8B888D405A0CFFA39FF80117E1340A34" blockId="33.[189,1091,1367,1394]" box="[189,1091,1367,1394]" pageId="33" pageNumber="34">
NW 
<collectingCountry id="F320CDD05A0CFFA39FB00118E4F00A34" box="[245,391,1367,1393]" name="Madagascar" pageId="33" pageNumber="34">Madagascar</collectingCountry>
