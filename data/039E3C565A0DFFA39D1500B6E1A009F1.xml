<subSubSection id="C32DDECB5A0CFFA39FF801D8E1C90A9B" box="[189,1214,1431,1503]" pageId="33" pageNumber="34" type="biology_ecology">
<paragraph id="8B888D405A0CFFA39FF801D8E46A0AF4" blockId="33.[189,285,1431,1457]" box="[189,285,1431,1457]" pageId="33" pageNumber="34">
<heading id="D0C03A2C5A0CFFA39FF801D8E46A0AF4" bold="true" box="[189,285,1431,1457]" fontSize="11" level="3" pageId="33" pageNumber="34" reason="3">
