<taxonomicName id="4C374D5F973FFFEEFE237C996A14FBE5" authority="Li" authorityName="Li" authorityYear="2017" box="[472,590,444,468]" class="Arachnida" family="Hahniidae" genus="Cicurina" kingdom="Animalia" order="Araneae" pageId="87" pageNumber="212" phylum="Arthropoda" rank="species" species="dong" status="sp. nov.">
