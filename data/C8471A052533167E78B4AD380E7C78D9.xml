<taxonomicName id="BFC1E5956F8D7DBC2292A5AEF24C12CD" authority="(L.) Vill." class="Gastropoda" family="Xenophoridae" genus="Stellaria" kingdom="Animalia" order="Littorinimorpha" pageId="null" pageNumber="803" phylum="Mollusca" rank="species" species="media">
