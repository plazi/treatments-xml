<subSubSection id="C34E6527FF6A2EAD64AFFAF6D323CF98" pageId="134" pageNumber="135" type="diagnosis">
<paragraph id="8BEB36ACFF6A2EAD64AFFAF6D652CFBA" blockId="134.[151,1436,1324,1637]" pageId="134" pageNumber="135">
<emphasis id="B920EABEFF6A2EAD64AFFAF6D375CF5F" bold="true" box="[199,314,1359,1384]" pageId="134" pageNumber="135">Diagnosis</emphasis>
