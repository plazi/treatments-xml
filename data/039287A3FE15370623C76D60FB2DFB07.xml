<taxonomicName id="4C3B4D36FE12370623C76AE1FED7FF7A" box="[151,311,223,248]" class="Insecta" family="Raphidiidae" genus="Megaraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="genus">
