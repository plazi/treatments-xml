, 
<taxonomicName id="4C3B4D36FE12370621076BA8FCD9FE2D" box="[599,825,406,431]" class="Insecta" family="Raphidiidae" genus="Florissantoraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="genus" status="gen. nov.">
