<emphasis id="B94FEAA7FE12370622906B70FD80FEE5" box="[448,608,334,359]" italics="true" pageId="20" pageNumber="421">Megaraphidia</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3B4D36FE12370621206B70FCB2FEE5" box="[624,850,334,359]" class="Insecta" family="Raphidiidae" genus="Florissantoraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B94FEAA7FE12370621206B70FCB2FEE5" box="[624,850,334,359]" italics="true" pageId="20" pageNumber="421">Florissantoraphidia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27C57DCFE123706200B6B6EFC28FEE5" box="[859,968,336,360]" pageId="20" pageNumber="421" rank="genus">gen. nov.</taxonomicNameLabel>
]; branches of CuA, MP short, not longer then width of 
<emphasis id="B94FEAA7FE123706226C6B4CFE8DFE09" box="[316,365,370,395]" italics="true" pageId="20" pageNumber="421">
<number id="3DF32492FE123706226C6B4CFEA9FE09" box="[316,329,370,395]" pageId="20" pageNumber="421" value="2.0">2</number>
