, 
<taxonomicName id="4C3B4D36FE12370620A66B4CFB38FE09" box="[1014,1240,370,395]" class="Insecta" family="Raphidiidae" genus="Florissantoraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B94FEAA7FE12370620A66B4CFB38FE09" box="[1014,1240,370,395]" italics="true" pageId="20" pageNumber="421">Florissantoraphidia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27C57DCFE12370627B16B4DFAAFFE08" box="[1249,1359,371,395]" pageId="20" pageNumber="421" rank="genus">gen. nov.</taxonomicNameLabel>
]. 
