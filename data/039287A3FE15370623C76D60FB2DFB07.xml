wing: 
<emphasis id="B94FEAA7FE123706238D6BA8FEE1FE2D" box="[221,257,406,431]" italics="true" pageId="20" pageNumber="421">doi</emphasis>
single [two in 
<taxonomicName id="4C3B4D36FE12370622F96BA8FDA9FE2D" box="[425,585,406,431]" class="Insecta" family="Raphidiidae" genus="Megaraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="genus">
<emphasis id="B94FEAA7FE12370622F96BA8FDA9FE2D" box="[425,585,406,431]" italics="true" pageId="20" pageNumber="421">Megaraphidia</emphasis>
