[distinctly longer than width of 
<emphasis id="B94FEAA7FE12370621B96B4CFCFCFE09" box="[745,796,370,395]" italics="true" pageId="20" pageNumber="421">
<number id="3DF32492FE12370621B96B4CFD16FE09" box="[745,758,370,395]" pageId="20" pageNumber="421" value="2.0">2</number>
doi
</emphasis>
in 
<taxonomicName id="4C3B4D36FE12370620146B4CFC04FE09" box="[836,996,370,395]" class="Insecta" family="Raphidiidae" genus="Megaraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="genus">
<emphasis id="B94FEAA7FE12370620146B4CFC04FE09" box="[836,996,370,395]" italics="true" pageId="20" pageNumber="421">Megaraphidia</emphasis>
