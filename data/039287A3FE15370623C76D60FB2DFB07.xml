<quantity id="4CC39B50FE12370621BC6A82FCA3FF56" box="[748,835,188,212]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="20" pageNumber="421" unit="mm" value="10.0">10 mm</quantity>
