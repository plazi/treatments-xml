<heading id="D0B281E1CF54831DC1922DBB8CF1F9D8" bold="true" box="[151,689,1522,1549]" fontSize="11" level="1" pageId="7" pageNumber="480" reason="1">
<taxonomicName id="4C454D0ECF54831DC1922DBB8CF1F9D8" authority="(Ameghino, 1887 a)" authorityName="Ameghino" authorityYear="1887" baseAuthorityName="Ameghino" baseAuthorityYear="1887" box="[151,689,1522,1549]" class="Mammalia" family="Interatheriidae" genus="Protypotherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Notoungulata" pageId="7" pageNumber="480" phylum="Chordata" rank="species" species="australe">
