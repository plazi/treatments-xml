<taxonomicName id="9F3F39B74DE0CC84C316F9A570E270D4" LSID="828718" authority="S. H. He &amp; S. L. Liu" class="Agaricomycetes" family="Hydnodontaceae" genus="Dextrinocystis" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Dextrinocystis calamicola" order="Trechisporales" pageId="4" pageNumber="101" phylum="Basidiomycota" rank="species" species="calamicola">Dextrinocystis calamicola S.H. He &amp; S.L. Liu</taxonomicName>
