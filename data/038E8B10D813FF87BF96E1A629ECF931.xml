<taxonomicName id="4C274185D813FF87BCAEE2A12CD1F911" authorityName="Crotch" authorityYear="1873" box="[1016,1223,1732,1755]" class="Insecta" family="Coccinellidae" genus="Cephaloscymnus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="zimmermanni">
<emphasis id="B953E614D813FF87BCAEE2A12C1DF911" box="[1016,1035,1732,1755]" italics="true" pageId="7" pageNumber="6">C</emphasis>
. 
<emphasis id="B953E614D813FF87BB4DE2A12CD1F911" box="[1051,1223,1732,1755]" italics="true" pageId="7" pageNumber="6">zimmermanni</emphasis>
</taxonomicName>
