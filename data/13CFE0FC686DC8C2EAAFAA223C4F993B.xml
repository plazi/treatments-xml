<normalizedToken id="CAC77FA1C8C590438D665DB8E6B44205" originalValue="verschmälert" pageId="null" pageNumber="262">verschmaelert</normalizedToken>
; 
<normalizedToken id="0F87A9D43DD32A6725962BE28D6A9B56" originalValue="Kronblätter" pageId="null" pageNumber="262">Kronblaetter</normalizedToken>
hell 
<normalizedToken id="B3CB3CC2E1992DDFE79BAF57E599E825" originalValue="gelbgrün" pageId="null" pageNumber="262">gelbgruen</normalizedToken>
</td>
<td id="93AF568BD4B775CDCFD477009B48F385" align="left" pageId="null" pageNumber="262" valign="bottom">
<italic id="615286F575A9CC8B3813E6B5084257E8" pageId="null" pageNumber="262">S. maximum</italic>
(Nr. 5a)
</td>
