<td id="48AFA979FB75F210C6B5F7C84AFC4B69" align="left" pageId="null" pageNumber="262" valign="top">
2. 
<normalizedToken id="804F888C0093AEBFB5F147FA8701B7CC" originalValue="Blätter" pageId="null" pageNumber="262">Blaetter</normalizedToken>
am Grunde 
<normalizedToken id="B337CACBDE5EBC2AA2D50B826D3173FB" originalValue="herzförmig" pageId="null" pageNumber="262">herzfoermig</normalizedToken>
