<taxonomicName id="8DEC4D30FFCB4F7CFC8BFA9D43F5F80E" box="[844,1164,1998,2023]" class="Insecta" family="Pamphagidae" genus="Asiotmethis" kingdom="Animalia" order="Orthoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="subSpecies" species="limbatus" subSpecies="motasi">
