<collectionCode id="ED00AE240927FFACFECE213D98EAFECC" box="[340,414,260,284]" pageId="10" pageNumber="69">QSBG</collectionCode>
, Chiang Mai, 
<collectingCountry id="F30676710927FFACFDA7213D9BD0FECD" box="[573,676,260,285]" name="Thailand" pageId="10" pageNumber="69">Thailand</collectingCountry>
