and 
<taxonomicName id="F58C8D9EFFB8FFF4FB904975FB68F8B9" authority="Fu-Ming &amp; Mao &amp; Chang, 2007" authorityName="Fu-Ming &amp; Mao &amp; Chang" authorityYear="2007" box="[1052,1204,1840,1865]" class="Insecta" family="Tettigoniidae" genus="Pseudokuzicus" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="28" phylum="Arthropoda" rank="species" species="spinus" status="sp. nov." subGenus="Pseudokuzicus">
