3. 
<taxonomicName id="F58C8D9EFFBEFFF2FF384B02FD74FA91" authority="Fu-Ming &amp; Mao &amp; Chang, 2007" authorityName="Fu-Ming &amp; Mao &amp; Chang" authorityYear="2007" box="[180,680,1351,1377]" class="Insecta" family="Tettigoniidae" genus="Pseudokuzicus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="acinacus" status="sp. nov." subGenus="Pseudokuzicus">
<emphasis id="00F82A0FFFBEFFF2FF384B02FD74FA91" bold="true" box="[180,680,1351,1377]" italics="true" pageId="3" pageNumber="26">Pseudokuzicus (Pseudokuzicus) acinacus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="1BCB9774FFBEFFF2FD234B02FCD0FA91" box="[687,780,1351,1377]" pageId="3" pageNumber="26" rank="species">sp. nov.</taxonomicNameLabel>
