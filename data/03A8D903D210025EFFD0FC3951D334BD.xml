<taxonomicName id="4C011396D210025EFF5CFBAF50B4376C" box="[245,438,1115,1139]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="31" pageNumber="2813" phylum="Firmicutes" rank="species" species="casei">
