<quantity id="4CE2E281C922FFFB61168B60D9FFFDCB" box="[1092,1199,517,541]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="28" pageNumber="428" unit="mm" value="0.85">0.85 mm</quantity>
, height 
