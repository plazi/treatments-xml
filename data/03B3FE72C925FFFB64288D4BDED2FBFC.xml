<quantity id="4CE2E281C922FFFB67B78B4DDE1AFD96" box="[741,842,552,576]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="28" pageNumber="428" unit="mm" value="1.2">1.20 mm</quantity>
