<collectionCode id="ED7DE7A8FFD23B28FEDFB897FE4EF9E7" box="[322,388,1522,1548]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="11" pageNumber="40">ZMB</collectionCode>
