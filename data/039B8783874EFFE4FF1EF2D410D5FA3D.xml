<taxonomicName id="4C324D16874EFFE4FB28F4681202FB02" authority="(Defrance, 1825)" baseAuthorityName="Defrance" baseAuthorityYear="1825" class="Gastropoda" family="Seraphsidae" genus="Diameza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="20" pageNumber="421" phylum="Mollusca" rank="species" species="fragilis">
