<taxonomicName id="4C4D4D31FF92C90AE62144B5FD3BF88F" authority="Lin and Huber" authorityName="Lin and Huber" authorityYear="2007" box="[323,762,1786,1812]" class="Insecta" family="Mymaridae" genus="Cleruchoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="noackae">
