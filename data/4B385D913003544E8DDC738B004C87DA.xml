<taxonomicName id="C310466F07FC62D2192AB70D646CC07B" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia crenulata" order="Coleoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="crenulata">Cephaloleia crenulata</taxonomicName>
