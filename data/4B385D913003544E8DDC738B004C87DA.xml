<taxonomicName id="1B4565B8340595E063D25C28DCB49B17" LSID="http://zoobank.org/7129312B-E74E-468F-89A6-20334EFA1B56" authority="Staines" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia crenulata" order="Coleoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="crenulata">Cephaloleia crenulata Staines</taxonomicName>
