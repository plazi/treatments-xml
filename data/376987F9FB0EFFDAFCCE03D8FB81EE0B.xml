<taxonomicName id="78C04D6CFB0FFFDAFE2D0017FD27E84B" authorityName="Galileo &amp; Martins" authorityYear="2009" box="[421,643,1816,1840]" class="Insecta" family="Cerambycidae" genus="Eburodacrys" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="146" phylum="Arthropoda" rank="species" species="amabilis" status="sp. nov.">
