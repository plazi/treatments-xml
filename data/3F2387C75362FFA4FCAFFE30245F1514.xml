<collectingCountry id="CF9D76415362FFA4FB03FD3A25E914D1" name="Paraguay" pageId="1" pageNumber="2">Paraguay</collectingCountry>
, and eastern 
<collectingCountry id="CF9D76415362FFA4FC17FD1924EE14D1" box="[922,1009,736,759]" name="Bolivia" pageId="1" pageNumber="2">Bolivia</collectingCountry>
