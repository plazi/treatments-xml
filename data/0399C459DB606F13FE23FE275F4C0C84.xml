<paragraph id="8B8F754FDB606F13FF7CFDD75F4C0C84" blockId="75.[212,1172,605,661]" pageId="75" pageNumber="1579">
<materialsCitation id="3B587F12DB606F13FF7CFDD75F480C84" country="India" elevation="2850" location="Arunachal Pradesh" pageId="75" pageNumber="1579" specimenCount="1" stateProvince="Arunachal Pradesh">
