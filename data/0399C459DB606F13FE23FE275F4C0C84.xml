<quantity id="4CC8D8AADB606F13FE35FDF759190C84" box="[413,561,637,661]" metricMagnitude="3" metricUnit="m" metricValue="2.85" metricValueMax="3.0" metricValueMin="2.7" pageId="75" pageNumber="1579" unit="m" value="2850.0" valueMax="3000.0" valueMin="2700.0">
