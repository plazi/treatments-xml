<taxonomicName id="DF782897EDFEC7D239331D460BDB50D2" class="Insecta" family="Curculionidae" genus="Arixyleborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arixyleborus hirsutulus" order="Coleoptera" pageId="7" pageNumber="116" phylum="Arthropoda" rank="species" species="hirsutulus">
