<taxonomicName id="4C434D74FF80125A1290FE695C0B6F5E" authorityName="Chandler" authorityYear="2009" box="[956,1112,500,525]" class="Insecta" family="Mycetophilidae" genus="Sciophila" kingdom="Animalia" order="Diptera" pageId="49" phylum="Arthropoda" rank="species" species="immodesta">
