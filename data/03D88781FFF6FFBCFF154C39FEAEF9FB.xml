<taxonomicName id="4C714D14FFF6FFBCFCAE4B89FC69FA35" box="[812,999,1494,1520]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="undetermined">
