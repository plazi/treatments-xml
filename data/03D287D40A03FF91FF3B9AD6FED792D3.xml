<emphasis id="B90FEAD00A00FF91FC909DBEFC2291D6" box="[828,932,440,464]" italics="true" pageId="3" pageNumber="288">ligulacea</emphasis>
</taxonomicName>
<emphasis id="B90FEAD00A00FF91FC079DB1FB8291D6" bold="true" box="[939,1028,439,464]" pageId="3" pageNumber="288">
<taxonomicNameLabel id="A23C57AB0A00FF91FC079DB1FB8291D6" box="[939,1028,439,464]" pageId="3" pageNumber="288" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, veins M 
<number id="3DB324E50A00FF91FBC09DC0FBF291D2" box="[1132,1140,454,468]" pageId="3" pageNumber="288" value="3.0">3</number>
