<taxonomicName id="BEB84D6A0D4B3B0AFBB5F9D517C6DE64" box="[1105,1172,1600,1621]" class="Magnoliopsida" family="Euphorbiaceae" genus="Croton" kingdom="Plantae" order="Malpighiales" pageId="6" pageNumber="232" phylum="Tracheophyta" rank="genus">
