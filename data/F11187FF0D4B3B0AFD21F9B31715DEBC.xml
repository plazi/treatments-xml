<taxonomicName id="BEB84D6A0D4B3B0AFD50F9C91014DE40" box="[692,838,1628,1649]" class="Magnoliopsida" family="Euphorbiaceae" kingdom="Plantae" order="Malpighiales" pageId="6" pageNumber="232" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
