<taxonomicName id="4C4617FBFFE7FF9B44ACFB85FDCCFBE0" authority="Leuckart, 1849" authorityName="Leuckart" authorityYear="1849" box="[238,545,1045,1071]" class="Polychaeta" family="Terebellidae" genus="Thelepus" kingdom="Animalia" order="Terebellida" pageId="1" pageNumber="588" phylum="Annelida" rank="genus">
