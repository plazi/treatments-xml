<taxonomicName id="4C494D285A1B3E17FCA1FDA5736DD9AF" authority="Macquart, 1834" authorityName="Macquart" authorityYear="1834" box="[873,1323,633,659]" class="Insecta" family="Keroplatidae" genus="Mycetophila" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="bicolour">
