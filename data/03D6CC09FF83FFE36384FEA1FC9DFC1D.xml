<taxonomicName id="4C7F069CFF83FFE361AAFE6DFD58FE5C" box="[678,762,414,436]" class="Magnoliopsida" family="Tamaricaceae" genus="Tamarix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="2" pageNumber="291" phylum="Magnoliophyta" rank="genus">Tamarix</taxonomicName>
