. 
<collectingCounty id="62A10593FF83FFE3604DFC61FC26FC44" box="[833,900,914,940]" pageId="2" pageNumber="291">Regio</collectingCounty>
transcaspica: 
<collectingRegion id="49BBB3FDFF83FFE36713FC61FB32FC44" box="[1055,1168,914,940]" country="Turkmenistan" name="Ashgabat" pageId="2" pageNumber="291">Aschabad</collectingRegion>
, 
<collectingMunicipality id="6BA4E765FF83FFE36796FC61FB44FC44" box="[1178,1254,914,940]" pageId="2" pageNumber="291">Sulukl</collectingMunicipality>
¨, as fines 
<location id="8EA02BC4FF83FFE3665FFC61FA05FC44" LSID="urn:lsid:plazi:treatment:03D6CC09FF83FFE36384FEA1FC9DFC1D:8EA02BC4FF83FFE3665FFC61FA05FC44" box="[1363,1447,914,940]" country="Turkmenistan" county="Regio" municipality="Sulukl" name="Persiae" pageId="2" pageNumber="291" stateProvince="Ashgabat">Persiae</location>
, inter 
<location id="8EA02BC4FF83FFE363CEFC45FEEBFC38" LSID="urn:lsid:plazi:treatment:03D6CC09FF83FFE36384FEA1FC9DFC1D:8EA02BC4FF83FFE363CEFC45FEEBFC38" box="[194,329,950,976]" country="Turkmenistan" county="Regio" municipality="Sulukl" name="Merjen Ulja" pageId="2" pageNumber="291" stateProvince="Ashgabat">Merjen Ulja</location>
et 
<location id="8EA02BC4FF83FFE36266FC45FE70FC38" LSID="urn:lsid:plazi:treatment:03D6CC09FF83FFE36384FEA1FC9DFC1D:8EA02BC4FF83FFE36266FC45FE70FC38" box="[362,466,950,976]" country="Turkmenistan" county="Regio" municipality="Sulukl" name="Kulkulab" pageId="2" pageNumber="291" stateProvince="Ashgabat">Kulkulab</location>
, 
<date id="FFC15BDFFF83FFE362EAFC45FD22FC38" box="[486,640,950,976]" pageId="2" pageNumber="291" value="1900-07-20">
<collectingDate id="EF85A237FF83FFE362EAFC45FD22FC38" box="[486,640,950,976]" pageId="2" pageNumber="291" value="1900-07-20">20. July 1900</collectingDate>
</date>
, det. 
<determinerName id="7E1C91DBFF83FFE361B6FC44FCB4FC39" box="[698,790,951,977]" pageId="2" pageNumber="291">J. Freyn</determinerName>
, 
<emphasis id="B90BA10DFF83FFE3602DFC44FC1AFC38" box="[801,952,950,977]" italics="true" pageId="2" pageNumber="291">
<collectorName id="268A18C9FF83FFE3602DFC44FCD8FC39" box="[801,890,951,977]" pageId="2" pageNumber="291">Sintenis</collectorName>
646b
</emphasis>
(G-00359282!, G-00359283!, G-00359283_ a!, LD-1618152, LD-1617256, P-04958259!, W-1902-5368!)
</materialsCitation>
.
</paragraph>
