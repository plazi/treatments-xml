<taxonomicName id="4C7F069CFF83FFE36756FE31FA09FE30" authority="Freyn et Sint. Regio" authorityName="Freyn et Sint. Regio" box="[1114,1451,450,472]" class="Magnoliopsida" family="Tamaricaceae" genus="Tamarix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="2" pageNumber="291" phylum="Magnoliophyta" rank="variety" species="karakalensis" variety="scoparia">var. scoparia Freyn et Sint. Regio</taxonomicName>
transcaspica; 
<collectingRegion id="49BBB3FDFF83FFE3624CFE15FE00FE14" box="[320,418,486,508]" country="Turkmenistan" name="Ashgabat" pageId="2" pageNumber="291">Aschabad</collectingRegion>
