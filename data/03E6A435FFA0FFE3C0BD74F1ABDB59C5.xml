near 
<collectingCounty id="62916DAFFFA0FFDCC65F7609ADEA5D20" box="[1095,1206,565,589]" pageId="31" pageNumber="102">Akonekye</collectingCounty>
, 
<locationDeviation id="9A420C01FFA0FFDCC0BD7672AA335D0B" box="[677,879,590,614]" location="Ambam" pageId="31" pageNumber="102">
<quantity id="4CB7B8C6FFA0FFDCC0BD7672ABB85D0B" box="[677,740,590,614]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="31" pageNumber="102" unit="km" value="15.0">15 km</quantity>
