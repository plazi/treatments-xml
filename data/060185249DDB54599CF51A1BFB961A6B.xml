<taxonomicName id="1E8ABA9B7407AA9E8B10CD6A79A658B3" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Picea crassifolia" order="Pinales" pageId="0" pageNumber="93" phylum="Tracheophyta" rank="species" species="crassifolia">
<emphasis id="C8F2DE77B9F4B1897AB07BE1983AAB67" italics="true" pageId="0" pageNumber="93">Picea crassifolia</emphasis>
</taxonomicName>
