<taxonomicName id="37E7FEFBFFA78441FF5FF9D5FB97D6F9" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[889,1050,1644,1670]" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="6" pageNumber="108" phylum="Chordata" rank="species" species="musculus">
