<taxonomicName id="37E7FEFBFFA78441F80FF9D4FB54D6F9" authorityName="Linnaeus" authorityYear="1758" box="[1065,1241,1644,1670]" class="Mammalia" family="Talpidae" genus="Talpa" kingdom="Animalia" order="Soricomorpha" pageId="6" pageNumber="108" phylum="Chordata" rank="species" species="europaea">
