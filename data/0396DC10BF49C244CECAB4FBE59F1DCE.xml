<geoCoordinate id="EE0B0BC1BF49C245CECEB2D0E36618AF" box="[918,1054,1253,1278]" degrees="39" direction="north" minutes="06" orientation="latitude" pageId="70" pageNumber="223" precision="15" seconds="13" value="39.10361">39°06′13″ N</geoCoordinate>
, 
<geoCoordinate id="EE0B0BC1BF49C245C971B2D0E3D218AE" box="[1065,1194,1253,1279]" degrees="9" direction="west" minutes="14" orientation="longitude" pageId="70" pageNumber="223" precision="15" seconds="47" value="-9.246389">9°14′47″ W</geoCoordinate>
