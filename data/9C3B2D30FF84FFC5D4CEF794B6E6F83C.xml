<date id="602CBAE6FF84FFC4D50BF625B40FFE89" box="[338,529,294,318]" pageId="16" pageNumber="17" value="1988-10-16">
<number id="A25A8E01FF84FFC4D50BF625B770FE89" box="[338,366,294,318]" pageId="16" pageNumber="17" value="16.0">16</number>
October 
<number id="A25A8E01FF84FFC4D58FF625B40FFE89" box="[470,529,294,318]" pageId="16" pageNumber="17" value="1988.0">1988</number>
</date>
, J. Starý coll. 
<typeStatus id="CB292284FF84FFC4D6F6F62BB508FE88" box="[687,790,296,319]" pageId="16" pageNumber="17">
