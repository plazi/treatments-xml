Hampson (
<number id="3D8A24B8FFE6C321A5DE07DC43C96283" box="[646,707,1210,1236]" pageId="6" pageNumber="57" value="1926.0">1926</number>
)
</bibRefCitation>
listed in the genus 
<number id="3D8A24B8FFE6C321A4EB07DC42CB6283" box="[947,961,1210,1236]" pageId="6" pageNumber="57" value="4.0">4</number>
species (
<taxonomicName id="4C424D1CFFE6C321A36807DD45D46283" box="[1072,1246,1211,1236]" class="Insecta" family="Noctuidae" genus="Madopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="57" phylum="Arthropoda" rank="species" species="flavomacula">
<emphasis id="B936EA8DFFE6C321A36807DD45D46283" box="[1072,1246,1211,1236]" italics="true" pageId="6" pageNumber="57">P. flavomacula</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C424D1CFFE6C321A3B607DD44726283" box="[1262,1400,1211,1236]" class="Insecta" family="Noctuidae" genus="Paragabara" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="57" phylum="Arthropoda" rank="species" species="acygonia">
<emphasis id="B936EA8DFFE6C321A3B607DD44726283" box="[1262,1400,1211,1236]" italics="true" pageId="6" pageNumber="57">P. acygonia</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C424D1CFFE6C321A2DE07DD41F562AE" class="Insecta" family="Noctuidae" genus="Paragabara" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="57" phylum="Arthropoda" rank="species" species="biundata">
<emphasis id="B936EA8DFFE6C321A2DE07DD41F562AE" italics="true" pageId="6" pageNumber="57">P. biundata</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C424D1CFFE6C321A6560786409262AE" box="[270,408,1248,1273]" class="Insecta" family="Noctuidae" genus="Paragabara" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="57" phylum="Arthropoda" rank="species" species="pectinata">
