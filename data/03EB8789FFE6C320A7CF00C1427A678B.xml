<taxonomicName id="4C424D1CFFE6C321A43D0786451262AE" box="[869,1048,1248,1273]" class="Insecta" family="Noctuidae" genus="Paragabara" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="57" phylum="Arthropoda" rank="species" species="ochreipennis">
