<taxonomicName id="75A14FFCECDF45B2155017AE368CF3C0" class="Arachnida" family="Brachychthoniidae" genus="Brachychthonius" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="590" phylum="Arthropoda" rank="variety" species="hystricinus" variety="tuxeni">Brachychthonius hystricinus Forssl. var. tuxeni</taxonomicName>
