<taxonomicName id="B5DA2EA59314A5B3FFC836254B63D389" family="Menispermaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Menispermaceae</taxonomicName>
, 
<taxonomicName id="09E6FEB8571A8A00F5D3270F6DB7B389" family="Moraceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Moraceae</taxonomicName>
, 
<taxonomicName id="E872E0085D5AE657CC30695623E0B748" family="Poaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Poaceae</taxonomicName>
