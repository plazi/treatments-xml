<taxonomicName id="DD466915E4B755D7A57F1565FFAC9CBB" family="Acanthaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Acanthaceae</taxonomicName>
, 
<taxonomicName id="2059BACFB24E4FFC5F2F703389EB8913" family="Amaranthaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Amaranthaceae</taxonomicName>
, 
<taxonomicName id="DA8461EE69B469CAD5EDBE597C470372" family="Apocynaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Apocynaceae</taxonomicName>
, 
<taxonomicName id="5A22AC7B0C08D0650CCE9209591D7AEE" family="Araceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Araceae</taxonomicName>
, 
<taxonomicName id="0418F32D6538755B3B3355F0950276B5" family="Arecaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Arecaceae</taxonomicName>
, 
<taxonomicName id="D5BB066C828C113FE1F29F6A949091E9" family="Aristolochiaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Aristolochiaceae</taxonomicName>
, 
<taxonomicName id="23BDA03F362D7B41BD7C25BD572C6C77" family="Asteraceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Asteraceae</taxonomicName>
, 
<taxonomicName id="16155B4E27B3D5C9150DDE574ED410F4" family="Chenopodiaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Chenopodiaceae</taxonomicName>
, 
<taxonomicName id="60EAAEDCF1AC1012E6D2BE04E4E84F75" family="Cucurbitaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Cucurbitaceae</taxonomicName>
, 
<taxonomicName id="BA7C6FBBCC263CC171159F2A2BF38D6A" family="Euphorbiaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Euphorbiaceae</taxonomicName>
, 
<taxonomicName id="8E0F564D443C3D859865CD013E758A76" family="Fabaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Fabaceae</taxonomicName>
, 
<taxonomicName id="6BD60978809C43A8AAC3B3D1CAAC9A4D" family="Malvaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Malvaceae</taxonomicName>
