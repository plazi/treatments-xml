<taxonomicName id="90317F85993E66BE3C555C14A91AA2F5" class="Insecta" family="Pseudococcidae" genus="Coccidohystrix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coccidohystrix insolita" order="Hemiptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="insolita">Coccidohystrix insolita</taxonomicName>
: 
<taxonomicName id="3E1393F3F8F42BBEC13D2F64A9B18C83" kingdom="Fungi" lsidName="" pageId="0" pageNumber="1042" rank="kingdom">Fungi</taxonomicName>
: 
<taxonomicName id="314BC393DD35D27A9FF5AD5E952CB7AE" class="Sordariomycetes" family="Clavicipitaceae" genus="Metarhizium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Metarhizium anisopliae" order="Hypocreales" pageId="0" pageNumber="1042" phylum="Ascomycota" rank="species" species="anisopliae">Metarhizium anisopliae</taxonomicName>
; 
<taxonomicName id="AC7AF243CEE5AF1075A41B4D5994FE59" class="Insecta" lsidName="" pageId="0" pageNumber="1042" rank="class">Insecta</taxonomicName>
: 
<taxonomicName id="A390DEF772C0B141D92032E5FD457173" lsidName="" order="Coleoptera" pageId="0" pageNumber="1042" rank="order">Coleoptera</taxonomicName>
: 
<taxonomicName id="3DDD966C8E17751B5022B2A5112B8759" family="Coccinellidae" lsidName="" pageId="0" pageNumber="1042" rank="family">Coccinellidae</taxonomicName>
: 
<taxonomicName id="94D20376F203A111260711DC9663B814" authority="Casey, 1908" authorityName="Casey" authorityYear="1908" class="Insecta" family="Coccinellidae" genus="Anegleis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Anegleis cardoni" order="Coleoptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="cardoni">Anegleis cardoni</taxonomicName>
(Weise); 
<taxonomicName id="FCF74E462DF4D482028546F477DFBEFA" class="Insecta" family="Coccinellidae" genus="Hyperaspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperaspis maindronia" order="Coleoptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="maindronia">Hyperaspis maindronia</taxonomicName>
; 
<taxonomicName id="85F0A0B65799B881DBB2E2F81891A80F" class="Insecta" family="Coccinellidae" genus="Nephus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nephus regularis" order="Coleoptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="regularis">Nephus regularis</taxonomicName>
; 
<taxonomicName id="743E338C42B117A5D3C3489C1B77105A" lsidName="" order="Lepidoptera" pageId="0" pageNumber="1042" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName id="C012C2A44E4833396D4A4703258AE3B4" family="Lycaenidae" lsidName="" pageId="0" pageNumber="1042" rank="family">Lycaenidae</taxonomicName>
