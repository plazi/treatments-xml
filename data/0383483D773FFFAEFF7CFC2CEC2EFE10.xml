<quantity id="4CD254CE773EFFAEFC67FEC9ED8FFE58" box="[908,1009,367,391]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" pageId="3" pageNumber="60" unit="mm" value="0.75">0.75 mm</quantity>
