<taxonomicName id="3EE34D2EB262121D7A6FFDA6FB6CFD89" authority="Hubner, 1818" authorityName="Hubner" authorityYear="1818" box="[1084,1204,553,577]" class="Insecta" family="Pyralidae" genus="Diaphania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="514" phylum="Arthropoda" rank="genus">
<emphasis id="CB97EABFB262121D7A6FFDA6FB6CFD89" box="[1084,1204,553,577]" italics="true" pageId="13" pageNumber="514">Diaphania</emphasis>
</taxonomicName>
reported in 
<collectingCountry id="81F4763DB262121D7B6EFDA6FA44FD89" box="[1341,1436,553,577]" name="Réunion" pageId="13" pageNumber="514">Reunion</collectingCountry>
is 
<taxonomicName id="3EE34D2EB262121D7EE3FDC1FE2EFDAD" authority="Saunders" authorityName="Saunders" authorityYear="1818" box="[176,502,589,614]" class="Insecta" family="Pyralidae" genus="Diaphania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="514" phylum="Arthropoda" rank="species" species="indica">
