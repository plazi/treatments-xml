<taxonomicName id="BBE03DB6D51CB254843141A9E9E9FE66" authorityName="Janson" authorityYear="1881" box="[296,413,460,481]" class="Insecta" family="Cetoniidae" genus="Euphoria" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="precaria">
