<quantity id="BB18EBD0D51CB25485E44279E924FDB6" box="[253,336,540,561]" metricMagnitude="3" metricUnit="m" metricValue="1.75" pageId="62" pageNumber="63" unit="m" value="1750.0">1,750 m</quantity>
