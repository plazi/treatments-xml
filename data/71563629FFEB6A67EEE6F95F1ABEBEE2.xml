<taxonomicName id="3EFFFCBCFFD76A64EFB2F8501B0ABCC8" box="[451,521,1954,1975]" pageId="33" pageNumber="184" rank="species" species="ntensis" status="sp. nov.">ntensis</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="D0B8E656FFD76A64EC62F8501B65BCC8" box="[531,614,1954,1975]" pageId="33" pageNumber="184" rank="species">
