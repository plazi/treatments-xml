<taxonomicName id="3EFFFCBCFFD76A64EF5DF8531B0ABCC8" box="[300,521,1953,1975]" class="Malacostraca" family="Talitridae" genus="Microrchestia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="33" pageNumber="184" phylum="Arthropoda" rank="species" species="ntensis" status="sp. nov.">
