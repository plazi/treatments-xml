</paragraph>
</subSubSection>
<subSubSection id="C37F39C5E629FF9B379D7D60FBCDF961" box="[199,1121,1656,1681]" pageId="10" pageNumber="49" type="etymology">
<paragraph id="8BDA6A4EE629FF9B379D7D60FBCDF961" blockId="10.[151,1436,1224,1717]" box="[199,1121,1656,1681]" pageId="10" pageNumber="49">
