<taxonomicName id="081375CD8F0B46DA01E1BF864AB65180" authorityName="Gmelin" authorityYear="1788" class="Aves" family="Alcedinidae" genus="Alcedo" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Alcedo Bengalensis" order="Coraciiformes" pageId="6" pageNumber="473" phylum="Chordata" rank="species" species="Bengalensis">Alcedo Bengalensis</taxonomicName>
