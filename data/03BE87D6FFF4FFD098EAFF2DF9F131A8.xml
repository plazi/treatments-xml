<quantity id="4CEF9B25FFF4FFD09B30FEB9F9FC3163" box="[845,957,331,358]" metricMagnitude="2" metricUnit="m" metricValue="4.15" metricValueMax="8.0" metricValueMin="0.3" pageId="101" pageNumber="102" unit="m" value="415.0" valueMax="800.0" valueMin="30.0">30–800 m</quantity>
(
<quantity id="4CEF9B25FFF4FFD09BB6FEB9FE1F3163" box="[971,1118,331,358]" metricMagnitude="2" metricUnit="m" metricValue="4.151376" metricValueMax="7.997952" metricValueMin="0.3048" pageId="101" pageNumber="102" unit="ft" value="1362.0" valueMax="2624.0" valueMin="100.0">100–2,624 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FFF4FFD09CCBFEB9FB40318C" author="Fitch and Lavenberg" firstAuthor="Fitch" pageId="101" pageNumber="102" refId="ref279229" refString="Fitch, J. E. &amp; Lavenberg, R. J. (1968) Deep-Water Fishes of California. California Natural History Guides, No. 25, University of California Press, Berkeley." type="book" year="1968">Fitch and Lavenberg 1968</bibRefCitation>
; max.: Nakabo in 
<bibRefCitation id="EF864B31FFF4FFD099B3FE9DF828318C" author="Nakabo" box="[462,617,367,393]" firstAuthor="Nakabo" pageId="101" pageNumber="102" refId="ref304533" refString="Nakabo, T. (Ed.). (2002) Fishes of Japan with Pictorial Keys to the Species. Tokai University Press, Tokyo." type="book" year="2002">Nakabo 2002</bibRefCitation>
