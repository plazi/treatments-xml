<emphasis id="5E8AEAC1FFC9FFB3E1F165C5FEBCF838" bold="true" box="[199,314,1976,2001]" pageId="72" pageNumber="73">Diagnosis</emphasis>
. 
<taxonomicName id="ABFE4D50FFC9FFB3E07F65C5FDADF838" box="[329,555,1976,2001]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="saltoensis">
<emphasis id="5E8AEAC1FFC9FFB3E07F65C5FDADF838" box="[329,555,1976,2001]" italics="true" pageId="72" pageNumber="73">Novalena saltoensis</emphasis>
</taxonomicName>
differs from 
<taxonomicName id="ABFE4D50FFC9FFB3E3F265C4FCA7F838" box="[708,801,1976,2001]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="clara">
<emphasis id="5E8AEAC1FFC9FFB3E3F265C4FCA7F838" box="[708,801,1976,2001]" italics="true" pageId="72" pageNumber="73">N. clara</emphasis>
</taxonomicName>
, 
<taxonomicName id="ABFE4D50FFC9FFB3E20665C4FC29F838" box="[816,943,1976,2001]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="franckei">
<emphasis id="5E8AEAC1FFC9FFB3E20665C4FC29F838" box="[816,943,1976,2001]" italics="true" pageId="72" pageNumber="73">N. franckei</emphasis>
</taxonomicName>
