<number id="CD6424BAF757FFE85092FD60FB77FD15" box="[1188,1221,644,670]" pageId="20" pageNumber="21" value="42.0">42</number>
° 
<number id="CD6424BAF757FFE850F8FD60FB69FD15" box="[1230,1243,644,670]" pageId="20" pageNumber="21" value="8.0">8</number>
’58.0”
</geoCoordinate>
, 
<geoCoordinate id="1E98505AF757FFE8553EFD48FE76FD4D" box="[264,452,684,710]" direction="west" orientation="longitude" pageId="20" pageNumber="21" precision="1" value="-122.70692">
W 
<number id="CD6424BAF757FFE85511FD48FEE6FD4D" box="[295,340,684,710]" pageId="20" pageNumber="21" value="122.0">122</number>
° 
<number id="CD6424BAF757FFE8556BFD48FECFFD4D" box="[349,381,684,710]" pageId="20" pageNumber="21" value="42.0">42</number>
’ 
<number id="CD6424BAF757FFE855B4FD48FE08FD4D" box="[386,442,684,710]" pageId="20" pageNumber="21" value="24.9">24.9</number>
”
</geoCoordinate>
