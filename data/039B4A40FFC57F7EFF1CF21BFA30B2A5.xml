<quantity id="4CCA56B3FFC57F7EFC6FF2C8FBDFB2A6" box="[1019,1092,1220,1242]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="29" pageNumber="114" unit="mm" value="1.5">1.5 mm</quantity>
