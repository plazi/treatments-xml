<quantity id="4CCA56B3FFC57F7EFEB0F2C8FEC0B2A6" box="[292,347,1220,1242]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="29" pageNumber="114" unit="mm" value="1.0">1 mm</quantity>
