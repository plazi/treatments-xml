<quantity id="4CCA56B3FFC57F7EFEF2F287FE07B2DC" box="[358,412,1163,1184]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="29" pageNumber="114" unit="mm" value="1.0">1 mm</quantity>
