<quantity id="4CCA56B3FFC57F7EFD07F214FD45B251" box="[659,734,1048,1069]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="29" pageNumber="114" unit="mm" value="0.1">0.1 mm</quantity>
