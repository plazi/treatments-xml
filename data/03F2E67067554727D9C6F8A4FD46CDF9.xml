<quantity id="4CA3FA8367544720DB65FE1DFD39CD85" box="[564,653,402,429]" metricMagnitude="-3" metricUnit="m" metricValue="9.4" pageId="27" pageNumber="268" unit="mm" value="9.4">9.4 mm</quantity>
