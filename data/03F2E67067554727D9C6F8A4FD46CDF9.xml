, BL 
<quantity id="4CA3FA8367544720DAC6FE1DFB8FCD85" box="[919,1083,402,429]" metricMagnitude="-2" metricUnit="m" metricValue="1.165" metricValueMax="1.18" metricValueMin="1.15" pageId="27" pageNumber="268" unit="mm" value="11.65" valueMax="11.8" valueMin="11.5">11.5–11.8 mm</quantity>
