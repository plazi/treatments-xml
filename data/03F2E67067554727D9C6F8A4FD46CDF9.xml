<quantity id="4CA3FA8367544720D9C6FE55FE94CDDD" box="[151,288,474,501]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" metricValueMax="2.4" metricValueMin="2.2" pageId="27" pageNumber="268" unit="mm" value="2.3" valueMax="2.4" valueMin="2.2">2.2–2.4 mm</quantity>
