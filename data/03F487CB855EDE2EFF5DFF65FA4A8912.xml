<collectionCode id="ED4CAE18855EDE2EFE47FE9BFE058CE5" LSID="urn:lsid:biocol.org:col:34840" box="[397,473,361,385]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34840" name="Staatliches Museum fuer Naturkund Stuttgart" pageId="7" pageNumber="141">SMNS</collectionCode>
