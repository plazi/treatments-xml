<taxonomicName id="4C774D03BE48046F7A363F86FB568A94" box="[895,1154,1708,1730]" class="Insecta" family="Miridae" genus="Halophylus" kingdom="Animalia" order="Hemiptera" pageId="113" pageNumber="113" phylum="Arthropoda" rank="species" species="tecticornii">
