<taxonomicName id="4C3CDAC2FFE4CA44FF3D15E4FE95F9D4" box="[151,257,1518,1542]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="B9487D53FFE4CA44FF3D15E4FE95F9D4" box="[151,257,1518,1542]" italics="true" pageId="45" pageNumber="46">Simulium</emphasis>
</taxonomicName>
(
<taxonomicName id="4C3CDAC2FFE4CA44FEBB15E4FEEFF9D4" box="[273,379,1518,1542]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
