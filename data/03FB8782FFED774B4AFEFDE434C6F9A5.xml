<quantity id="4CAA9B71FFED774B48EEFA5C3798F839" box="[647,761,1521,1547]" metricMagnitude="-2" metricUnit="m" metricValue="1.616" pageId="21" pageNumber="86" unit="mm" value="16.16">16.16 mm</quantity>
