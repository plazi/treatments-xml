<quantity id="4CF79B2F336D8865950A29EE8696F99C" box="[569,671,1520,1547]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" pageId="17" pageNumber="418" unit="mm" value="13.5">13.5 mm</quantity>
, 
<specimenCount id="9D09FD43336D8865959929EF86B6F99C" box="[682,703,1521,1547]" pageId="17" pageNumber="418" type="female">♀</specimenCount>
13.6–14.0 mm and width: 
<emphasis id="B97BEAD8336D886594DD29EE800AF99C" box="[1006,1027,1520,1547]" italics="true" pageId="17" pageNumber="418">
<specimenCount id="9D09FD43336D886594DD29EE800AF99C" box="[1006,1027,1520,1547]" pageId="17" pageNumber="418" type="male">♂</specimenCount>
</emphasis>
<quantity id="4CF79B2F336D8865933929EF806BF99C" box="[1034,1122,1521,1547]" metricMagnitude="-3" metricUnit="m" metricValue="6.2" pageId="17" pageNumber="418" unit="mm" value="6.2">6.2 mm</quantity>
