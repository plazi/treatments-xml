<taxonomicName id="4C4F4D25FFEAC83970C47B85EF0D71E5" authority="Gibon, 2013" authorityName="Gibon" authorityYear="2013" box="[1001,1139,1456,1480]" class="Insecta" family="Philopotamidae" genus="Rossodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="157" phylum="Arthropoda" rank="species" species="hertui" status="n. sp.">
