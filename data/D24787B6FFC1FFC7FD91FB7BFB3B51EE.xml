<taxonomicName id="9DEE4D23FFC2FFC6FF7AF9FCFE2F511E" box="[251,415,1618,1640]" class="Insecta" family="Phoridae" genus="Conicera" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="116" phylum="Arthropoda" rank="species" species="tibialis">
