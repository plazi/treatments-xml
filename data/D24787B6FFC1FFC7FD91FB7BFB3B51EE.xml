<taxonomicName id="9DEE4D23FFC3FFC7FEBAFA0DFE6D52CF" box="[315,477,1443,1465]" class="Insecta" family="Phoridae" genus="Conicera" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="117" phylum="Arthropoda" rank="species" species="tibialis">
