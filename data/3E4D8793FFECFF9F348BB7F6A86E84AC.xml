<quantity id="711C9B60FFECFF9F3051B79CA8918254" box="[1101,1182,253,277]" metricMagnitude="3" metricUnit="m" metricValue="1.097" pageId="4" pageNumber="143" unit="m" value="1097.0">1097m</quantity>
, 
<date id="C25A1045FFECFF9F30A8B79CA9288254" box="[1204,1319,253,277]" pageId="4" pageNumber="143" value="2002-10-10">
<number id="002C24A2FFECFF9F30A8B79CA8DE8254" box="[1204,1233,253,277]" pageId="4" pageNumber="143" value="10.0">10</number>
.x. 
<number id="002C24A2FFECFF9F30FBB79CA9288254" box="[1255,1319,253,277]" pageId="4" pageNumber="143" value="2002.0">2002</number>
