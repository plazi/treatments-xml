<uri id="FFF53AD7FF811773FEB6FE0FED419220" box="[370,1119,505,529]" pageId="42" pageNumber="2030">
urn:lsid:zoobank.org:act: 
<uuid id="FFC20C00FF811773FDB3FE0FED419220" box="[631,1119,505,529]" pageId="42" pageNumber="2030">2E2F274E-9B00-4D11-9E67-576BBF759247</uuid>
