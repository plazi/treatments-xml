<taxonomicName id="AA8E4D15FFFBFFDFFF6858D4FE16FAC1" box="[196,424,1272,1297]" class="Mammalia" family="Muridae" genus="Oryzomys" kingdom="Animalia" order="Rodentia" pageId="22" pageNumber="53" phylum="Chordata" rank="species" species="palustris">
