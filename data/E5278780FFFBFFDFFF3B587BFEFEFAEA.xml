<taxonomicName id="AA8E4D15FFFBFFDFFF6A58FCFE6DFB39" box="[198,467,1232,1257]" class="Arachnida" family="Listrophoridae" genus="Prolistrophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="22" pageNumber="53" phylum="Arthropoda" rank="species" species="bakeri">
