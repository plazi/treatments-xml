</taxonomicName>
; 
<taxonomicName id="60804D019349FFF6FA11F9D81D4C579D" baseAuthorityName="Klapalek" baseAuthorityYear="1916" class="Insecta" family="Perlidae" genus="Claassenia" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="310" phylum="Arthropoda" rank="species" species="gigas">
