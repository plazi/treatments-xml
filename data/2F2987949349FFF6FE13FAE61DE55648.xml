<collectingRegion id="6544F8609349FFF6FB97F9FC1AD55754" box="[1037,1252,1611,1635]" country="China" name="Sichuan" pageId="7" pageNumber="310">Sichuan Province</collectingRegion>
[
<taxonomicName id="60804D019349FFF6FB67F9FB1D9557B0" authority="(Navas)" baseAuthorityName="Navas" class="Insecta" family="Perlodidae" genus="Perlodes" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="310" phylum="Arthropoda" rank="species" species="brevipennis">
