<emphasis id="598B0DE9A1ADC97BA79F71DB3E6B9CA5" bold="true" italics="true" pageId="0" pageNumber="1">bpp</emphasis>
basoposterior process, 
<emphasis id="53A43B304E34C6F5CE7E55D97D495E2C" bold="true" italics="true" pageId="0" pageNumber="1">dmg</emphasis>
distomesal groove, 
<emphasis id="60EA01800B775BD3800A90F2A5E15FD9" bold="true" italics="true" pageId="0" pageNumber="1">dmp</emphasis>
distomesal process, 
<emphasis id="0A2B313852DE1B29ABC03A85FC4BE55D" bold="true" italics="true" pageId="0" pageNumber="1">f</emphasis>
