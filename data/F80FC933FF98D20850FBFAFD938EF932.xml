(
<bibRefCitation id="143705D4FF9BD2085333FAA6931AFA8F" author="Crawford JC" box="[872,1040,1287,1312]" pageId="18" pageNumber="772" pagination="1 - 10" refId="ref18970" refString="Crawford JC. 1912. Descriptions of new Hymenoptera. No 4. Proc U. S. N. M. 42: 1 - 10." type="journal article" year="1912">Crawford, 1912</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicNameLabel id="59E1194CFF9BD208547BFAA6938EFA8F" box="[1056,1156,1287,1312]" pageId="18" pageNumber="772" rank="species">
