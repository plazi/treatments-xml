<quantity id="E22D9B681C0DFFAE5D9AFDDCFD80FD41" box="[469,554,618,642]" metricMagnitude="3" metricUnit="m" metricValue="1.8288" pageId="12" pageNumber="54" unit="ft" value="6000.0">6000 ft</quantity>
), 
<date id="516B104D1C0DFFAE5E70FDDCFD06FD41" box="[575,684,618,642]" pageId="12" pageNumber="54" value="1928-11-03">
<number id="931D24AA1C0DFFAE5E70FDDCFDE1FD41" box="[575,587,618,642]" pageId="12" pageNumber="54" value="3.0">3</number>
