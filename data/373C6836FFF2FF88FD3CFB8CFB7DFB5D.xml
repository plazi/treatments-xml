</taxonomicName>
differs from 
<taxonomicName id="7895A2A3FFF3FF8BFD17F9FFFC64F961" authorityName="Schaum" authorityYear="1841" box="[744,911,1655,1677]" class="Insecta" family="Cetoniidae" genus="Gymnetis" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="atropurpurea">
