<quantity id="82454FBAFFE4053BEEBFFC61ED3CFC67" box="[479,624,935,956]" metricMagnitude="-3" metricUnit="m" metricValue="7.779999999999999" metricValueMax="8.12" metricValueMin="7.44" pageId="9" pageNumber="206" unit="mm" value="7.779999999999999" valueMax="8.12" valueMin="7.44">7.44-8.12 mm</quantity>
(ẍ = 
<quantity id="82454FBAFFE4053BEDD0FC61EC5FFC66" box="[688,787,935,957]" metricMagnitude="-3" metricUnit="m" metricValue="7.79" pageId="9" pageNumber="206" unit="mm" value="7.79">7.79 mm</quantity>
*); width 
<quantity id="82454FBAFFE4053BEC14FC61EB49FC67" box="[884,1029,935,956]" metricMagnitude="-3" metricUnit="m" metricValue="2.27" metricValueMax="2.4" metricValueMin="2.14" pageId="9" pageNumber="206" unit="mm" value="2.27" valueMax="2.4" valueMin="2.14">2.14-2.40 mm</quantity>
