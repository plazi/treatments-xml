<taxonomicName id="9BB93F8B576B306621CDBE4BFDD3985E" box="[506,634,512,536]" class="Insecta" family="Scydmaenidae" genus="Cephennium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="296" phylum="Arthropoda" rank="species" species="fairchild" status="sp. n.">
<emphasis id="6ECD981A576B306621CDBE4BFDD3985E" box="[506,634,512,536]" italics="true" pageId="9" pageNumber="296">C. fairchild</emphasis>
</taxonomicName>
