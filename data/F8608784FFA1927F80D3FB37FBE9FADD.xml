<paragraph id="70763692FFA1927E80B6FAE0FEC2F9B9" blockId="2.[151,685,1294,1668]" pageId="2" pageNumber="239">
<collectionCode id="16D8AE57FFA1927E80B6FAE0FE13FAC7" box="[151,446,1385,1408]" pageId="2" pageNumber="239">
