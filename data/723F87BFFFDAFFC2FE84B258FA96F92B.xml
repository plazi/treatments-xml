<taxonomicName id="3D964D2AFFDAFFC2FD7EB77BFC63F996" authorityName="Scarabino" authorityYear="2008" box="[746,949,1585,1608]" class="Scaphopoda" family="Gadilidae" genus="Cadulus" kingdom="Animalia" order="Gadilida" pageId="45" pageNumber="260" phylum="Mollusca" rank="species" species="deverdensis">
