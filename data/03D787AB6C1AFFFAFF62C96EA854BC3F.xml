<quantity id="4C869B586C1AFFFAFC41C843A8B8BB1F" box="[948,1016,848,874]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="28" pageNumber="29" unit="mm" value="8.0">8 mm</quantity>
