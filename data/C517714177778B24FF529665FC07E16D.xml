<taxonomicName id="8ABEBBD477778B24FF52962DFDF8E7F5" authority="Kellogg 1896: 532" authorityName="Kellogg" authorityPageNumber="532" authorityYear="1896" box="[151,589,877,900]" class="Insecta" family="Menoponidae" genus="Menopon" kingdom="Animalia" order="Phthiraptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="melanorum">
