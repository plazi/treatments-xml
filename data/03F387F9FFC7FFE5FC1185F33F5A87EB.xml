NZC-ZSI 6944/ 
<materialsCitation id="3B323CB2FFC7FFE6FB7185933EB887E2" country="India" location="Manipur" pageId="1" pageNumber="95" specimenCount="1" stateProvince="Manipur" typeStatus="holotype">
18; male 
<typeStatus id="54E1884DFFC7FFE6FA86859338F88762" box="[1334,1432,1522,1545]" pageId="1" pageNumber="95">holotype</typeStatus>
; 
<collectingCountry id="F34D767FFFC7FFE6FC8F86733E198742" box="[831,889,1554,1577]" name="India" pageId="1" pageNumber="95">India</collectingCountry>
, 
