<taxonomicName id="4C4688D6FF6EFF32FBE254E0C485F994" authority="(Forest, 1963)" baseAuthorityName="Forest" baseAuthorityYear="1963" class="Malacostraca" family="Scyllaridae" genus="Acantharctus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="150" pageNumber="649" phylum="Arthropoda" rank="species" species="posteli">
<emphasis id="B9322F47FF6EFF32FBE254E0C41EF994" italics="true" pageId="150" pageNumber="649">Acantharctus posteli</emphasis>
(Forest, 1963)
</taxonomicName>
and the eastern Pacific 
<taxonomicName id="4C4688D6FF6EFF32FB015718C47BF9FD" authority="(Bouvier, 1909)" authorityName="Holthuis" authorityYear="2002" baseAuthorityName="Bouvier" baseAuthorityYear="1909" class="Malacostraca" family="Scyllaridae" genus="Acantharctus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="150" pageNumber="649" phylum="Arthropoda" rank="species" species="delfini" status="n. comb.">
<emphasis id="B9322F47FF6EFF32FB015718C3C2F994" box="[1158,1247,1551,1576]" italics="true" pageId="150" pageNumber="649">A. delfini</emphasis>
(
<bibRefCitation id="EFD78EA4FF6EFF32FD56573EC442F9FD" author="BOUVIER E. L." box="[721,863,1577,1601]" pageId="150" pageNumber="649" pagination="213 - 215" refId="ref133902" refString="BOUVIER E. L. 1909. - Arctus Delfini sp. nov. Revista Chilena de Historia natural 13: 213 - 215, fig. 30." type="journal article" year="1909">Bouvier, 1909</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel id="A201923CFF6EFF32FCEB573FC4D8F9FD" box="[876,965,1576,1601]" pageId="150" pageNumber="649" rank="species">n. comb.</taxonomicNameLabel>
</paragraph>
