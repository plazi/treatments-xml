<quantity id="0E19CEE10464FFE7FEFDF8D577AB1704" box="[345,441,1816,1843]" metricMagnitude="0" metricUnit="m" metricValue="6.398" pageId="52" pageNumber="53" unit="m" value="6.398">6,398 m</quantity>
