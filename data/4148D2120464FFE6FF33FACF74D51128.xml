<quantity id="0E19CEE10464FFE7FCEFF8D575BB1704" box="[843,937,1816,1843]" metricMagnitude="0" metricUnit="m" metricValue="2.968" pageId="52" pageNumber="53" unit="m" value="2.968">2,968 m</quantity>
