<taxonomicName id="4C664D0CFFEBC753921C15596186A33F" authorityName="Dietz" authorityYear="1891" box="[538,658,1306,1327]" class="Insecta" family="Curculionidae" genus="Chelonychus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="25" phylum="Arthropoda" rank="species" species="longipes">
