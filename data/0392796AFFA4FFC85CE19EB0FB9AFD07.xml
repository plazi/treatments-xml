<subScript id="17BFCA39FFA3FFC85CAF9E53FF41FEB1" attach="left" box="[217,225,313,327]" fontSize="6" pageId="20" pageNumber="171">3</subScript>
; 
<collectionCode id="ED2A50B9FFA3FFC85C869E4DFEA3FEB7" box="[240,259,295,321]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="20" pageNumber="171">R</collectionCode>
