<collectionCode id="ED2A50B9FFA3FFC8586D9E4DFB8EFEB7" box="[1051,1070,295,321]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="20" pageNumber="171">R</collectionCode>
<subScript id="17BFCA39FFA3FFC858589E53FB96FEB1" attach="left" box="[1070,1078,313,327]" fontSize="6" pageId="20" pageNumber="171">5</subScript>
