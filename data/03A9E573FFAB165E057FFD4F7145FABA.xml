<taxonomicName id="4C002FE6FFAB165E057FFD4F717CFD3D" authority="Oken, 1817" authorityName="Oken" authorityYear="1817" box="[264,579,684,710]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="genus">
