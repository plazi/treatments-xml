<taxonomicName id="F5854D58F004D87D6B43FD0BFD05FD3E" authorityName="ROTH" authorityYear="1928" box="[525,720,668,689]" class="Liliopsida" family="Poaceae" genus="Ammophila" kingdom="Plantae" order="Poales" pageId="29" pageNumber="412" phylum="Tracheophyta" rank="species" species="atlantica">
