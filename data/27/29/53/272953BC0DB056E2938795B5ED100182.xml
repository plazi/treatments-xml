<subSubSection pageId="49" pageNumber="50" type="distribution">
<paragraph pageId="49" pageNumber="50">Distribution.</paragraph>
<paragraph pageId="49" pageNumber="50">A very common cosmopolitan weed of all temperate regions which also grows in upland regions throughout the tropics.</paragraph>
</subSubSection>
<subSubSection pageId="49" pageNumber="50" type="notes">
<paragraph pageId="49" pageNumber="50">Notes.</paragraph>
<paragraph pageId="49" pageNumber="50">
