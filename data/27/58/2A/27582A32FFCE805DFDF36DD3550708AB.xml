/ 
<number pageId="9" pageNumber="190" value="9.5" valueMax="10.4" valueMin="8.6">8.60– 10.40</number>
; prothorax: length, 
<number box="[446,493,943,967]" pageId="9" pageNumber="190" value="1.35">1.35</number>
–2.00/ 
<number box="[568,682,943,967]" pageId="9" pageNumber="190" value="2.05" valueMax="2.3" valueMin="1.8">1.80–2.30</number>
; anterior width, 
<number box="[875,989,943,967]" pageId="9" pageNumber="190" value="1.15" valueMax="1.35" valueMin="0.95">0.95–1.35</number>
/ 
<number box="[997,1110,943,967]" pageId="9" pageNumber="190" value="1.35" valueMax="1.5" valueMin="1.2">1.20–1.50</number>
; posterior width, 
<number box="[1316,1428,943,967]" pageId="9" pageNumber="190" value="1.225" valueMax="1.5" valueMin="0.95">0.95–1.50</number>
