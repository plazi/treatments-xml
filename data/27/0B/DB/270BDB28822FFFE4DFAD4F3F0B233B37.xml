<collectingCountry box="[664,747,1870,1894]" name="Guinea-Bissau" pageId="0" pageNumber="490">Guinea</collectingCountry>
</emphasis>
, 
<collectingMunicipality box="[759,1029,1870,1895]" pageId="0" pageNumber="490">Easy Highlands Province</collectingMunicipality>
