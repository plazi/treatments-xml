<emphasis box="[607,725,1880,1903]" italics="true" pageId="5" pageNumber="42">Serranillus</emphasis>
</taxonomicName>
species. 1– 
<taxonomicName authority="Barr" authorityName="Barr" box="[850,1021,1880,1903]" class="Insecta" family="Carabidae" genus="Serranillus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="42" phylum="Arthropoda" rank="species" species="jeanneli">
<taxonomicName box="[850,963,1880,1903]" genus="S." pageId="5" pageNumber="42" rank="species" species="jeanneli">
<emphasis box="[850,963,1880,1903]" italics="true" pageId="5" pageNumber="42">
S. 
<taxonomicName authority="Barr" authorityName="Barr" box="[877,963,1880,1903]" pageId="5" pageNumber="42" rank="species" species="jeanneli">jeanneli</taxonomicName>
</emphasis>
</taxonomicName>
Barr
</taxonomicName>
; 2– 
<taxonomicName authority="Jeannel" authorityName="Jeannel" box="[1059,1294,1880,1903]" class="Insecta" family="Carabidae" genus="Serranillus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="42" phylum="Arthropoda" rank="species" species="dunavani">
<emphasis box="[1059,1185,1880,1903]" italics="true" pageId="5" pageNumber="42">
S. 
