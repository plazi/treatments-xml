<paragraph blockId="12.[227,1436,1689,1751]" pageId="12" pageNumber="13">
<taxonomicName authorityName="De Deckker" authorityYear="1981" box="[227,464,1690,1716]" class="Ostracoda" family="Cyprididae" genus="Bennelongia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="nimala">
