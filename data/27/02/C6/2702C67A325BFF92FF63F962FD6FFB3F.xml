<paragraph blockId="3.[151,1437,152,585]" pageId="3" pageNumber="64">
Total length 
<number box="[345,465,152,178]" pageId="3" pageNumber="64" value="2.15" valueMax="2.25" valueMin="2.05">2.05–2.25</number>
, 
<quantity box="[477,585,152,178]" metricMagnitude="-3" metricUnit="m" metricValue="2.12" pageId="3" pageNumber="64" unit="mm" value="2.12">2.12 mm</quantity>
. Wing length 
<number box="[749,869,152,178]" pageId="3" pageNumber="64" value="1.525" valueMax="1.65" valueMin="1.4">1.40–1.65</number>
