<geoCoordinate degrees="45.2067" direction="north" orientation="latitude" precision="5" value="45.2067">45.2067°N</geoCoordinate>
, 
<geoCoordinate degrees="66.6505" direction="west" orientation="longitude" precision="5" value="-66.6505">66.6505°W</geoCoordinate>
