<taxonomicName class="Insecta" family="Curculionidae" genus="Anthonomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthonomus" order="Coleoptera" pageId="254" pageNumber="255" phylum="Arthropoda" rank="genus">Anthonomus</taxonomicName>
