<taxonomicName class="Insecta" family="Curculionidae" genus="Brachonyx" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachonyx" order="Coleoptera" pageId="254" pageNumber="255" phylum="Arthropoda" rank="genus">Brachonyx</taxonomicName>
