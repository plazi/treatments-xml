<subSubSection pageId="null" pageNumber="373" type="nomenclature">
<paragraph pageId="null" pageNumber="373">
<taxonomicName authority="L." class="Liliopsida" family="Poaceae" genus="Lolium" kingdom="Plantae" order="Poales" pageId="null" pageNumber="373" phylum="Tracheophyta" rank="species" species="perenne">
Lolium 
<normalizedToken originalValue="perénne" pageId="null" pageNumber="373">perenne</normalizedToken>
<authorityName pageId="null" pageNumber="373">L.</authorityName>
</taxonomicName>
</paragraph>
