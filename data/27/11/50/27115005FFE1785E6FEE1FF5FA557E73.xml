<collectingCountry box="[1155,1238,1534,1555]" name="Ecuador" pageId="20" pageNumber="21">Ecuador</collectingCountry>
) 
<taxonomicName authority="Nielson" authorityName="Nielson" authorityYear="1988" box="[1250,1436,1534,1555]" class="Insecta" family="Cicadellidae" genus="Tinocripus" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="huggerti">
