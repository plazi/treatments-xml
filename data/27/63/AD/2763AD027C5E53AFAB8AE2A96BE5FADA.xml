<paragraph pageId="0" pageNumber="67">Type.</paragraph>
<paragraph pageId="0" pageNumber="67">
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Macropsychanthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Macropsychanthus lauterbachii" order="Fabales" pageId="0" pageNumber="67" phylum="Tracheophyta" rank="species" species="lauterbachii">
