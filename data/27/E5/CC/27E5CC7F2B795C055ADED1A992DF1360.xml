<paragraph pageId="50" pageNumber="51">
<taxonomicName authority="Spinola, 1843" authorityName="Spinola" authorityYear="1843" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis crassimargo" order="Hymenoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="crassimargo">Chrysis crassimargo Spinola, 1843</taxonomicName>
