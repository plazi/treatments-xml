<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:571AC91B-CC78-4F3E-AC73-C2BC5B63444B" class="Actinopterygii" family="Ariidae" genus="Ketengus" kingdom="Animalia" lsidName-ZBK="Ketengus typus Bleeker 1847:9" order="Siluriformes" pageNumber="69" phylum="Chordata" rank="species" species="typus">Ketengus typus</taxonomicName>
