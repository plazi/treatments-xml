<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:B293B6B1-C1F8-4EA8-AA70-87E0EF542E48" class="Actinopterygii" family="Ariidae" genus="Cochlefelis" kingdom="Animalia" lsidName-ZBK="Cochlefelis Whitley 1941:8" order="Siluriformes" pageNumber="69" phylum="Chordata" rank="genus">Cochlefelis</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:23001896-466C-47FA-820E-9D5BC782C425" class="Insecta" family="Cerambycidae" genus="Sciades" kingdom="Animalia" lsidName-ZBK="Bagrus Sciades emphysetus Müller &amp; Troschel 1849:8" order="Coleoptera" pageNumber="69" phylum="Arthropoda" rank="species" species="emphysetus">Sciades emphysetus</taxonomicName>
