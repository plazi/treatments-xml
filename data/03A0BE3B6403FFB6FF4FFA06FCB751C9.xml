ORDER 
<taxonomicName id="4C0974AE6403FFB6FED0FA06FD4E52C0" authority="Brandt, 1855" authorityName="Brandt" authorityYear="1855" box="[264,655,1424,1450]" class="Mammalia" kingdom="Animalia" order="Lagomorpha" pageId="9" pageNumber="10" phylum="Chordata" rank="order">LAGOMORPHA Brandt, 1855</taxonomicName>
