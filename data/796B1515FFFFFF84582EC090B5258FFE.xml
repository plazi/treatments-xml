<caption id="A5BDF48BFFFDFF8459B4C693B3CB8C2C" ID-DOI="http://doi.org/10.5281/zenodo.5738922" ID-Zenodo-Dep="5738922" httpUri="https://zenodo.org/record/5738922/files/figure.png" pageId="11" pageNumber="90" startId="11.[156,187,984,1006]" targetBox="[302,1286,206,944]" targetPageId="11">
