<subSubSection id="C309F5BFD859966D7DAEF9A2FA21F9F6" box="[192,1440,1635,1659]" pageId="45" pageNumber="44" type="etymology">
<paragraph id="8BACA634D859966D7DAEF9A2FA21F9F6" blockId="45.[192,1440,1635,1659]" box="[192,1440,1635,1659]" pageId="45" pageNumber="44">
<emphasis id="B9677A26D859966D7DAEF9A2FED6F9F6" bold="true" box="[192,343,1635,1659]" pageId="45" pageNumber="44">Etymology.</emphasis>
