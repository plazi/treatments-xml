<quantity id="4CF0B6A4DC59FFD36E45F880FB9EF8FE" box="[1065,1141,1880,1906]" metricMagnitude="4" metricUnit="m" metricValue="6.5" pageId="19" pageNumber="20" unit="km" value="65.0">65 km</quantity>
