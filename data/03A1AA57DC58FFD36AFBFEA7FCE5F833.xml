<collectionCode id="ED198384DC58FFD26929FCE4FC71FCDA" LSID="urn:lsid:biocol.org:col:34871" box="[837,922,828,854]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="18" pageNumber="19">USNM</collectionCode>
<number id="3DC00966DC58FFD269CEFCE4FBE0FCDA" box="[930,1035,828,854]" pageId="18" pageNumber="19" value="1107079.0">1107079</number>
, spring west of Two Mile Spring, 
<number id="3DC00966DC58FFD26AFBFCBAFF1AFCF0" box="[151,241,866,892]" pageId="18" pageNumber="19" value="453320.0">453320</number>
E, 
<number id="3DC00966DC58FFD26B70FCBAFE6EFCF0" box="[284,389,866,892]" pageId="18" pageNumber="19" value="4782496.0">4782496</number>
N, 
