<taxonomicName id="BAF84D70FFB2FF85FEF99D8825E57EED" authorityName=": Berg" authorityYear="1879" box="[344,663,1949,1973]" class="Insecta" family="Pentatomidae" genus="Euschistus" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="452" phylum="Arthropoda" rank="species" species="picticornis" subGenus="Lycipta">
