</taxonomicName>
(along with 
<taxonomicName id="77C50C83DCF7A5B4FCB98CFEFBD0FA94" authorityName="Row" authorityYear="1909" box="[841,1054,1268,1293]" class="Calcarea" family="Amphoriscidae" genus="Leucilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosolenida" pageId="133" pageNumber="134" phylum="Porifera" rank="species" species="crosslandi">
