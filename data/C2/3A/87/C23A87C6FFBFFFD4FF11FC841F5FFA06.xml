coral rubble, 
<quantity box="[151,218,998,1022]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="12" pageNumber="13" unit="m" value="0.3">0.3 m</quantity>
; K. Ruetzler col. 
<date box="[414,559,998,1022]" pageId="12" pageNumber="13" value="1974-04-23">
<number box="[414,442,998,1022]" pageId="12" pageNumber="13" value="23.0">23</number>
