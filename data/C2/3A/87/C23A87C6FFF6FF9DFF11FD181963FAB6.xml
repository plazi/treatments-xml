</number>
x 
<number box="[403,445,1220,1244]" pageId="69" pageNumber="70" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
(
<emphasis box="[463,535,1220,1244]" italics="true" pageId="69" pageNumber="70">
<date box="[463,535,1220,1244]" pageId="69" pageNumber="70" value="1980-10-02">
<number box="[463,491,1220,1244]" pageId="69" pageNumber="70" value="80.0">80</number>
