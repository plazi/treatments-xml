<number box="[709,806,1998,2022]" pageId="73" pageNumber="74" value="690.0" valueMax="940.0" valueMin="440.0">440–940</number>
<date box="[812,887,1997,2022]" pageId="73" pageNumber="74" value="1915-10-06">
