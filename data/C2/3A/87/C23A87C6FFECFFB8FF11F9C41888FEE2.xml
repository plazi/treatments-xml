</taxonomicName>
coral rubble, 
<quantity box="[151,225,1824,1848]" metricMagnitude="0" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="95" pageNumber="96" unit="m" value="8.5" valueMax="9.0" valueMin="8.0">8–9 m</quantity>
; K. Ruetzler col. 
<date box="[421,559,1824,1849]" pageId="95" pageNumber="96" value="1973-05-07">
