<number box="[1161,1259,406,431]" pageId="62" pageNumber="63" value="320.0" valueMax="450.0" valueMin="190.0">190–450</number>
<date box="[1266,1329,407,431]" pageId="62" pageNumber="63">
x 
<number box="[1287,1329,407,431]" pageId="62" pageNumber="63" value="5.0" valueMax="7.0" valueMin="3.0">3–7</number>
</date>
(
<emphasis box="[1344,1426,407,431]" italics="true" pageId="62" pageNumber="63">
<number box="[1344,1386,407,431]" pageId="62" pageNumber="63" value="274.0">274</number>
<date box="[1393,1426,407,431]" pageId="62" pageNumber="63">
x 
<number box="[1412,1426,407,431]" pageId="62" pageNumber="63" value="4.0">4</number>
</date>
</emphasis>
