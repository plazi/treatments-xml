<number box="[338,436,188,213]" pageId="75" pageNumber="76" value="715.0" valueMax="890.0" valueMin="540.0">540–890</number>
<date box="[443,534,188,213]" pageId="75" pageNumber="76" value="1932-10-15">
x 
<number box="[464,534,188,213]" pageId="75" pageNumber="76" value="23.5" valueMax="32.0" valueMin="15.0">15–32</number>
</date>
(
<emphasis box="[550,647,188,212]" italics="true" pageId="75" pageNumber="76">
<number box="[550,592,188,212]" pageId="75" pageNumber="76" value="721.0">721</number>
<date box="[599,647,188,212]" pageId="75" pageNumber="76" value="1923-10">
x 
<number box="[619,647,188,212]" pageId="75" pageNumber="76" value="23.0">23</number>
</date>
</emphasis>
) Μm; II: 
<number box="[748,845,188,213]" pageId="75" pageNumber="76" value="207.5" valueMax="300.0" valueMin="115.0">115–300</number>
<date box="[852,929,188,212]" pageId="75" pageNumber="76" value="2012-10-03">
x 
