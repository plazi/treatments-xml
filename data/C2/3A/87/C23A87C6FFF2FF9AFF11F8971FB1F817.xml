<date box="[472,505,1893,1916]" pageId="66" pageNumber="67">
x 
<number box="[491,505,1893,1916]" pageId="66" pageNumber="67" value="7.0">7</number>
</date>
</emphasis>
) Μm; styles II (n= 
<number box="[708,722,1892,1917]" pageId="66" pageNumber="67" value="5.0">5</number>
): 
<number box="[742,784,1892,1917]" pageId="66" pageNumber="67" value="200.0">200</number>
