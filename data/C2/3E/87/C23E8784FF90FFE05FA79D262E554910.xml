<emphasis bold="true" box="[192,346,792,818]" pageId="65" pageNumber="64">Distribution.</emphasis>
<collectingCountry box="[354,467,792,818]" name="Colombia" pageId="65" pageNumber="64">Colombia</collectingCountry>
, 
<collectingCountry box="[479,562,792,818]" name="Mexico" pageId="65" pageNumber="64">Mexico</collectingCountry>
