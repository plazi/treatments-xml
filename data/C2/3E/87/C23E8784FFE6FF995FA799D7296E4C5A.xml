<collectingCountry box="[353,406,323,349]" name="United States of America" pageId="56" pageNumber="55">USA</collectingCountry>
(
<collectingRegion box="[420,515,323,350]" country="United States of America" name="Arizona" pageId="56" pageNumber="55">Arizona</collectingRegion>
) (
<bibRefCitation author="Hodkinson ID" box="[536,728,323,349]" pageId="56" pageNumber="55" pagination="1179 - 1243" refId="ref55657" refString="Hodkinson ID. 1988. The Nearctic Psylloidea (Insecta: Homoptera): An annotated check list. Journal of Natural History 22: 1179 - 1243." type="journal article" year="1988">Hodkinson 1988</bibRefCitation>
