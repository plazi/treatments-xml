<collectingCountry box="[468,530,1162,1188]" name="Cuba" pageId="35" pageNumber="34">Cuba</collectingCountry>
, 
<collectingCountry box="[540,625,1162,1188]" name="Mexico" pageId="35" pageNumber="34">Mexico</collectingCountry>
, 
<collectingCountry box="[636,692,1163,1189]" name="United States of America" pageId="35" pageNumber="34">USA</collectingCountry>
