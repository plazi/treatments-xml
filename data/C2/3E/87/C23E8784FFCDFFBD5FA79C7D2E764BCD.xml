<subSubSection box="[192,1420,897,925]" pageId="28" pageNumber="27" type="materials_examined">
<paragraph blockId="28.[192,1420,897,925]" box="[192,1420,897,925]" pageId="28" pageNumber="27">
<emphasis bold="true" box="[192,429,898,924]" pageId="28" pageNumber="27">Materials examined.</emphasis>
Interceptions from 
<emphasis bold="true" box="[652,737,898,924]" pageId="28" pageNumber="27">
<collectingCountry box="[652,737,898,924]" name="Mexico" pageId="28" pageNumber="27">Mexico</collectingCountry>
</emphasis>
