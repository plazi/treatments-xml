<emphasis bold="true" box="[749,803,897,923]" pageId="28" pageNumber="27">
<collectingCountry box="[749,803,897,923]" name="United States of America" pageId="28" pageNumber="27">USA</collectingCountry>
</emphasis>
: 
<emphasis bold="true" box="[816,936,898,924]" pageId="28" pageNumber="27">
<collectingRegion box="[816,936,898,924]" country="United States of America" name="California" pageId="28" pageNumber="27">California</collectingRegion>
</emphasis>
and 
<emphasis bold="true" box="[991,1056,898,924]" pageId="28" pageNumber="27">
<collectingRegion box="[991,1056,898,924]" country="United States of America" name="Texas" pageId="28" pageNumber="27">Texas</collectingRegion>
</emphasis>
(
<collectionCode box="[1070,1141,898,924]" country="USA" httpUri="http://grbio.org/cool/jekt-x82m" name="Florida State Collection of Arthropods, The Museum of Entomology" pageId="28" pageNumber="27" type="Museum">FSCA</collectionCode>
