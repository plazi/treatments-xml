Key to 
<collectingRegion box="[281,375,800,829]" country="Uruguay" name="Florida" pageId="49" pageNumber="48">Florida</collectingRegion>
species of 
<taxonomicName authorityName="Crawford" authorityYear="1914" box="[511,670,800,829]" class="Insecta" family="Psyllidae" genus="Heteropsylla" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="48" phylum="Arthropoda" rank="genus">
