, det. 
<determinerName box="[188,334,1064,1083]" pageId="6" pageNumber="81">E. Kirschenhofer</determinerName>
(
<collectionCode box="[345,414,1064,1083]" pageId="6" pageNumber="81">DWCB</collectionCode>
