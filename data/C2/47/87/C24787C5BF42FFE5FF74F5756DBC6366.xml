<emphasis box="[541,744,1977,2003]" italics="true" pageId="17" pageNumber="168">Isometopus webbi</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[760,821,1978,2004]" pageId="17" pageNumber="168" rank="species">n. sp.</taxonomicNameLabel>
and 
<taxonomicName authority="Yeshwanth &amp; Chérot &amp; Henry, 2021" authorityName="Yeshwanth &amp; Chérot &amp; Henry" authorityYear="2021" box="[878,1093,1977,2003]" class="Insecta" family="Miridae" genus="Isometopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="168" phylum="Arthropoda" rank="species" species="wolskii" status="n. sp.">
