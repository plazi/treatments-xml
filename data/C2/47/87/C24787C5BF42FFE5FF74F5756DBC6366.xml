</taxonomicName>
, 
<taxonomicNameLabel box="[1109,1170,1978,2004]" pageId="17" pageNumber="168" rank="species">n. sp.</taxonomicNameLabel>
on 
<taxonomicName box="[1214,1297,1978,2004]" class="Magnoliopsida" family="Malvaceae" genus="Grewia" kingdom="Plantae" order="Malvales" pageId="17" pageNumber="168" phylum="Tracheophyta" rank="genus">
<emphasis box="[1214,1297,1978,2004]" italics="true" pageId="17" pageNumber="168">Grewia</emphasis>
</taxonomicName>
sp. (
<taxonomicName class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="17" pageNumber="168" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
