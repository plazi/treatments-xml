<paragraph blockId="12.[151,568,1800,1827]" box="[151,568,1800,1827]" pageId="12" pageNumber="163">
<heading bold="true" box="[151,568,1800,1827]" fontSize="11" level="1" pageId="12" pageNumber="163" reason="1">
<emphasis bold="true" box="[151,568,1800,1827]" pageId="12" pageNumber="163">
Tribe 
<taxonomicName authority="Bergroth, 1924" authorityName="Bergroth" authorityYear="1924" baseAuthorityName="Cassis &amp; Schuh" baseAuthorityYear="2012" box="[226,568,1800,1827]" class="Insecta" family="Miridae" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="163" phylum="Arthropoda" rank="tribe" tribe="Myiommini">
Myiommini 
<bibRefCitation author="Bergroth, E." box="[380,568,1800,1827]" pageId="12" pageNumber="163" pagination="3 - 9" refId="ref17796" refString="Bergroth, E. (1924) On the Isometopidae (Hem. Het.) of North America. Notulae Entomologicae, 4, 3 - 9." type="journal article" year="1924">Bergroth, 1924</bibRefCitation>
</taxonomicName>
