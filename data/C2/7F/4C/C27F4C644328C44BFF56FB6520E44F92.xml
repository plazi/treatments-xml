(
<figureCitation box="[159,280,1260,1287]" captionStart="FIGURE 1" captionStartId="2.[151,250,1972,1996]" captionTargetBox="[201,1386,823,1929]" captionTargetId="figure-330@2.[186,1401,813,1949]" captionTargetPageId="2" captionText="FIGURE 1. Kamendaka jejuensis sp. n., holotype.A. habitus, dorsal view; B. ditto, lateral view; C. ditto, ventral view; D. frons, ventral view; E. head, pronotum and mesonotum, lateral view; F. ditto, dorsal view. Scale bar: A–C: 3.0mm; D–E: 1.0mm." figureDoi="http://doi.org/10.5281/zenodo.5964560" httpUri="https://zenodo.org/record/5964560/files/figure.png" pageId="1" pageNumber="594">Figs 1A–F</figureCitation>
, 
<figureCitation box="[291,361,1261,1287]" captionStart="FIGURE 2" captionStartId="3.[151,250,1874,1898]" captionTargetBox="[310,1274,195,1833]" captionTargetId="figure-18@3.[298,1290,181,1850]" captionTargetPageId="3" captionText="FIGURE 2. Kamendaka jejuensis sp. n., holotype.A. right forewing; B. ditto, observed in alcohol; C. right hindwing, observed in alcohol. Scale bar: A–C: 3.0mm." figureDoi="http://doi.org/10.5281/zenodo.5964562" httpUri="https://zenodo.org/record/5964562/files/figure.png" pageId="1" pageNumber="594">2A–C</figureCitation>
, 
<figureCitation box="[372,436,1261,1287]" captionStart="FIGURE 3" captionStartId="4.[151,250,1815,1839]" captionTargetBox="[226,1359,194,1774]" captionTargetId="figure-29@4.[217,1371,181,1791]" captionTargetPageId="4" captionText="FIGURE 3. Kamendaka jejuensis sp. n., A. male genitalia, lateral view; B. male anal tube, caudal view; C. male pygofer, caudal view; D. ditto, lateral view; E. gonostyli, ventral view; F. right gonostylus, lateral view; G. ditto, anterior view; H. aedeagus, left-lateral view; I. ditto, dorsal view; J. ditto, right-lateral view; K. female genitalia, lateral view; L. ditto, ventral view; M. ditto, caudal view; N. female anal tube, dorsal view; O. gonapophysis VIII, lateral view; P. gonoplac with gonapophysis IX, ventral view. Abbreviations: fp, first process; sp, second process; tp, third process. Scale bar: A, C–E, L–M: 0.5mm; F–G, H–K, O–P: 0.3mm; B, N: 0.1mm." figureDoi="http://doi.org/10.5281/zenodo.5964566" httpUri="https://zenodo.org/record/5964566/files/figure.png" pageId="1" pageNumber="594">3A–P</figureCitation>
, 
<figureCitation box="[447,462,1261,1287]" captionStart="FIGURE 4" captionStartId="5.[151,250,1589,1613]" captionTargetBox="[381,1211,186,1553]" captionTargetId="figure-18@5.[368,1219,181,1565]" captionTargetPageId="5" captionText="FIGURE 4. Kamendaka jejuensis sp. n., male gonostyli, ventral view. Scale bar: 0.5mm." figureDoi="http://doi.org/10.5281/zenodo.5964570" httpUri="https://zenodo.org/record/5964570/files/figure.png" pageId="1" pageNumber="594">4</figureCitation>
