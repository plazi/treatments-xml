<taxonomicName authority="Mirzayans" authorityName="Mirzayans" box="[408,738,1684,1709]" class="Insecta" family="Pamphagidae" genus="Neoparanothrotes" kingdom="Animalia" order="Orthoptera" pageId="130" pageNumber="131" phylum="Arthropoda" rank="genus">
