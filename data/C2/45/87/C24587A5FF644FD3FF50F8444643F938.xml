<taxonomicName authority="Mirzayans" authorityName="Mirzayans" box="[960,1254,1647,1672]" class="Insecta" family="Pamphagidae" genus="Neoparanothrotes" kingdom="Animalia" order="Orthoptera" pageId="130" pageNumber="131" phylum="Arthropoda" rank="species" species="borumandi">
