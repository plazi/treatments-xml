<collectingCountry box="[769,818,1325,1348]" name="Iran" pageId="124" pageNumber="125">Iran</collectingCountry>
</materialsCitation>
. 
<materialsCitation ID-GBIF-Occurrence="1416806371" country="Iran" location="The Avroman (Hewraman) mountains" pageId="124" pageNumber="125" specimenCount="1" stateProvince="Kordestan province" typeStatus="holotype">
<location LSID="urn:lsid:plazi:treatment:C24587A5FF9A4F2DFF50FEDF4224FA80:4F336068FF9A4F2DFC85F87F4384FAAD" box="[834,1277,1324,1349]" country="Iran" name="The Avroman (Hewraman) mountains" pageId="124" pageNumber="125" stateProvince="Kordestan province">The Avroman (Hewraman) mountains</location>
