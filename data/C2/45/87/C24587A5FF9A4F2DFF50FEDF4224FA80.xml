<subSubSection pageId="124" pageNumber="125" type="discussion">
<paragraph blockId="124.[151,1437,1143,1384]" pageId="124" pageNumber="125">
<materialsCitation ID-GBIF-Occurrence="1416806392" country="Iraq" location="Mistshenko" pageId="124" pageNumber="125" specimenCount="1" typeStatus="holotype">
<emphasis bold="true" box="[199,315,1287,1312]" pageId="124" pageNumber="125">Remarks.</emphasis>
