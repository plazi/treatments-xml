<taxonomicName authority="Uvarov. But" authorityName="Uvarov. But" box="[790,1182,620,645]" class="Insecta" family="Pamphagidae" genus="Eremocharis" kingdom="Animalia" order="Orthoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="subSpecies" species="granulosa" subSpecies="rufipes">
