</taxonomicNameLabel>
<taxonomicName authority="Fu &amp; Leduc &amp; Rao &amp; Cai, 2019" authorityName="Fu &amp; Leduc &amp; Rao &amp; Cai" authorityYear="2019" box="[365,612,441,467]" class="Adenophorea" family="Comesomatidae" genus="Dorylaimopsis" kingdom="Animalia" order="Desmodorida" pageId="12" pageNumber="445" phylum="Nematoda" rank="species" species="jinyuei" status="sp. n.">
<emphasis box="[365,612,441,467]" italics="true" pageId="12" pageNumber="445">Dorylaimopsis jinyuei</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[619,682,441,467]" pageId="12" pageNumber="445" rank="species">
<emphasis bold="true" box="[619,682,441,467]" pageId="12" pageNumber="445">sp. n.</emphasis>
</taxonomicNameLabel>
