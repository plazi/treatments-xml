<paragraph pageId="null" pageNumber="547">
<pageBreakToken pageId="null" pageNumber="547" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName authority="L." class="Magnoliopsida" family="Fabaceae" genus="Anthyllis" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="547" phylum="Tracheophyta" rank="species" species="vulneraria">
Anthyllis 
<normalizedToken originalValue="Vulnerária" pageId="null" pageNumber="547">Vulneraria</normalizedToken>
<authorityName pageId="null" pageNumber="547">L.</authorityName>
</taxonomicName>
</paragraph>
