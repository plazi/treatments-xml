), 
<taxonomicName authority="(Reichenow)" authorityName="Reichenow" baseAuthorityName="Reichenow" baseAuthorityYear="1897" box="[347,908,1207,1232]" class="Aves" family="Nectariniidae" genus="Chalcomitra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="22" pageNumber="423" phylum="Chordata" rank="subSpecies" species="senegalensis" subSpecies="lamperti">
