(
<taxonomicName authorityName="Vigors" authorityYear="1825" box="[657,816,1172,1197]" class="Aves" family="Nectariniidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="22" pageNumber="423" phylum="Chordata" rank="family">Nectariniidae</taxonomicName>
