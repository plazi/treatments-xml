<taxonomicName authorityName="Vigors" authorityYear="1825" box="[668,827,1804,1829]" class="Aves" family="Nectariniidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="424" phylum="Chordata" rank="family">Nectariniidae</taxonomicName>
