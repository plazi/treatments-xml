<taxonomicName authority="(Reichenow)" authorityName="Reichenow" baseAuthorityName="Reichenow" baseAuthorityYear="1891" box="[849,1436,1804,1829]" class="Aves" family="Nectariniidae" genus="Cyanomitra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="424" phylum="Chordata" rank="subSpecies" species="verticalis" subSpecies="viridisplendens">
