<taxonomicName box="[382,542,362,387]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psittaciformes" pageId="20" pageNumber="421" phylum="Chordata" rank="order">Psittaciformes</taxonomicName>
