<paragraph blockId="25.[151,1436,550,1583]" pageId="25" pageNumber="426">
<taxonomicName authorityName="" authorityYear="1847" box="[199,331,1198,1223]" class="Aves" family="Thraupidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="25" pageNumber="426" phylum="Chordata" rank="family">Thraupidae</taxonomicName>
