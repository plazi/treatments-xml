<taxonomicName box="[342,408,1199,1222]" class="Aves" family="Fringillidae" genus="Tiaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="25" pageNumber="426" phylum="Chordata" rank="genus">Tiaris</taxonomicName>
canOrus
</emphasis>
(Gmelin) (= 
<emphasis box="[658,865,1198,1223]" italics="true" pageId="25" pageNumber="426">
<taxonomicName authorityName="Bonaparte" authorityYear="1850" box="[658,778,1198,1223]" class="Aves" family="Aves (awaiting allocation)" genus="Phonipara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aves (awaiting allocation)" pageId="25" pageNumber="426" phylum="Chordata" rank="genus">PhOnipara</taxonomicName>
