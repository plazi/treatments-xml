<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[657,978,964,990]" class="Mammalia" family="Sciuridae" genus="Tamias" kingdom="Animalia" order="Rodentia" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="striatus">
