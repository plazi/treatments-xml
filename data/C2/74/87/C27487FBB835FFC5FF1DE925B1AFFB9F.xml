<paragraph blockId="7.[136,672,374,401]" box="[136,672,374,401]" pageId="7" pageNumber="182">
<heading bold="true" box="[136,672,374,401]" fontSize="11" level="1" pageId="7" pageNumber="182" reason="1">
<emphasis bold="true" box="[136,672,374,401]" pageId="7" pageNumber="182">
Key to the species of 
<emphasis bold="true" box="[386,561,374,401]" italics="true" pageId="7" pageNumber="182">
<taxonomicName authorityName="P. Kumm" authorityYear="1871" baseAuthorityName="Fr." box="[386,495,374,401]" class="Agaricomycetes" family="Hymenogastraceae" genus="Psilocybe" kingdom="Fungi" order="Agaricales" pageId="7" pageNumber="182" phylum="Basidiomycota" rank="genus" sensu="stricto">Psilocybe</taxonomicName>
