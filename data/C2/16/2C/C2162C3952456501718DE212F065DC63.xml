<taxonomicName class="Magnoliopsida" family="Oleaceae" genus="Fraxinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fraxinus" order="Lamiales" pageId="10" pageNumber="179" phylum="Tracheophyta" rank="genus">Fraxinus</taxonomicName>
