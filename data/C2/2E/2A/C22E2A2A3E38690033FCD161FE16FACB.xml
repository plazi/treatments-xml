<emphasis box="[303,513,867,890]" italics="true" pageId="8" pageNumber="9">Glycyrrhiza glabra</emphasis>
</taxonomicName>
and 
<taxonomicName box="[572,707,867,890]" class="Magnoliopsida" family="Fabaceae" genus="Glycyrrhiza" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="uralensis">
