<taxonomicName class="Actinopterygii" family="Ariidae" genus="Cathorops" kingdom="Animalia" order="Siluriformes" pageNumber="46" phylum="Chordata" rank="genus">Cathorops</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:8E024180-3250-46F3-A751-1D583938D54E" class="Actinopterygii" family="Ariidae" genus="Cinetodus" kingdom="Animalia" lsidName-ZBK="Cinetodus Ogilby 1898:32" order="Siluriformes" pageNumber="46" phylum="Chordata" rank="genus">Cinetodus</taxonomicName>
