<taxonomicName family="Anthomyiidae" lsidName="" pageId="26" pageNumber="63" rank="family">Anthomyiidae</taxonomicName>
and 
<taxonomicName family="Scathophagidae" lsidName="" pageId="26" pageNumber="63" rank="family">Scathophagidae</taxonomicName>
