<paragraph blockId="13.[151,542,1856,1915]" box="[151,542,1856,1882]" pageId="13" pageNumber="262">
<heading bold="true" box="[151,542,1856,1882]" fontSize="11" level="1" pageId="13" pageNumber="262" reason="1">
<emphasis bold="true" box="[151,542,1856,1882]" pageId="13" pageNumber="262">
<taxonomicName box="[151,391,1856,1882]" class="Insecta" family="Chrysomelidae" genus="Argopistes" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="262" phylum="Arthropoda" rank="species" species="woodleyi" status="new species">
<emphasis bold="true" box="[151,391,1856,1882]" italics="true" pageId="13" pageNumber="262">Argopistes woodleyi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[398,542,1856,1882]" pageId="13" pageNumber="262" rank="species">new species</taxonomicNameLabel>
</emphasis>
