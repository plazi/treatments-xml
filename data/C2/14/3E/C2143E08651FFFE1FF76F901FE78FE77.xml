, Ƥ 
<number box="[791,843,1889,1913]" pageId="0" pageNumber="50" value="8.8">8.80</number>
; greatest body width 
<number box="[1086,1107,1889,1913]" pageId="0" pageNumber="50" value="3.0">3</number>
<number box="[1115,1167,1889,1913]" pageId="0" pageNumber="50" value="1.88">1.88</number>
; Ƥ 
<number box="[1209,1262,1889,1913]" pageId="0" pageNumber="50" value="1.92">1.92</number>
. Brachypterous form, length 
<number box="[296,317,1925,1949]" pageId="0" pageNumber="50" value="3.0">3</number>
<number box="[324,376,1925,1949]" pageId="0" pageNumber="50" value="8.26">8.26</number>
, Ƥ 
<number box="[415,468,1925,1949]" pageId="0" pageNumber="50" value="8.2">8.20</number>
; greatest body width 
<number box="[707,728,1925,1949]" pageId="0" pageNumber="50" value="3.0">3</number>
<number box="[735,787,1925,1949]" pageId="0" pageNumber="50" value="1.84">1.84</number>
; Ƥ 
<number box="[827,879,1925,1949]" pageId="0" pageNumber="50" value="1.92">1.92</number>
. Width of head 
<number box="[1056,1077,1925,1949]" pageId="0" pageNumber="50" value="3.0">3</number>
<number box="[1084,1197,1925,1949]" pageId="0" pageNumber="50" value="1.54" valueMax="1.56" valueMin="1.52">1.52−1.56</number>
and Ƥ 
<number box="[1280,1332,1925,1949]" pageId="0" pageNumber="50" value="1.52">1.52</number>
