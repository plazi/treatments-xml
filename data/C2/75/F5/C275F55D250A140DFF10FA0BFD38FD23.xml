<quantity box="[545,722,152,178]" metricMagnitude="-2" metricUnit="m" metricValue="2.15" metricValueMax="2.25" metricValueMin="2.05" pageId="4" pageNumber="59" unit="mm" value="21.5" valueMax="22.5" valueMin="20.5">20.5–22.5 mm</quantity>
