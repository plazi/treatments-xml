<taxonomicName authority="Zone" authorityName="Zone" box="[831,1168,1587,1611]" class="Globothalamea" family="Truncorotaloididae" genus="Morozovella" kingdom="Chromista" order="Rotaliida" pageId="14" pageNumber="28" phylum="Foraminifera" rank="species" species="velascoensis">
