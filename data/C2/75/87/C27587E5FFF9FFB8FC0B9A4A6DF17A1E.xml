<taxonomicName baseAuthorityName="CUSHMAN" baseAuthorityYear="1936" box="[1090,1254,1831,1855]" class="Globothalamea" family="Tritaxiidae" genus="Tritaxia" kingdom="Chromista" order="Lituolida" pageId="20" pageNumber="34" phylum="Foraminifera" rank="species" species="midwayensis">
