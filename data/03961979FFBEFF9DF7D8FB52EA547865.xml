<geoCoordinate id="EE0BCEA8FFBEFF9DF610FB04EFAC79F1" box="[292,420,1238,1263]" degrees="36" direction="north" minutes="09" orientation="latitude" pageId="17" pageNumber="1122" precision="15" seconds="34" value="36.159447">36°09'34&quot;N</geoCoordinate>
