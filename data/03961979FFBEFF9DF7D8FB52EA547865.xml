<quantity id="4CC7058AFFBEFF9DF501FB04EC7579F0" box="[565,637,1238,1263]" metricMagnitude="2" metricUnit="m" metricValue="1.1" pageId="17" pageNumber="1122" unit="m" value="110.0">110 m</quantity>
