<treatmentCitationGroup id="4DC8D925B75BA0699ADFFD38FD904B58" box="[151,521,755,780]" pageId="15" pageNumber="246">
<taxonomicName id="AAD88588B75BA0699ADFFD38FD9C4B58" authority="Uzel, 1895: 160" authorityName="Uzel" authorityPageNumber="160" authorityYear="1895" box="[151,517,755,780]" class="Insecta" family="Thripidae" genus="Dendrothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="246" phylum="Arthropoda" rank="species" species="tiliae">
<emphasis id="5FAC2219B75BA0699ADFFD38FEC04B5F" box="[151,345,755,779]" italics="true" pageId="15" pageNumber="246">Dendrothrips tiliae</emphasis>
<treatmentCitation id="EC79D81AB75BA0699B17FD38FD9C4B58" author="Uzel, H." box="[351,517,755,780]" page="160" pageId="15" pageNumber="246" year="1895">
<bibRefCitation id="094983FAB75BA0699B17FD38FE4B4B58" author="Uzel, H." box="[351,466,755,780]" pageId="15" pageNumber="246" refId="ref10751" refString="Uzel, H. (1895) Monographie der Ordnung Thysanoptera. K ˆ nigratz, Bohemia, 472 pp." type="book" year="1895">Uzel, 1895</bibRefCitation>
