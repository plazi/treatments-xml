<paragraph id="9D1336CFFFEAFF9BFF72F969FCC9C591" blockId="52.[151,822,1722,1745]" box="[151,822,1722,1745]" pageId="52" pageNumber="53">
<treatmentCitationGroup id="BDBC11E1FFEAFF9BFF72F969FCC9C591" box="[151,822,1722,1745]" pageId="52" pageNumber="53">
<taxonomicName id="5AAC4D4CFFEAFF9BFF72F969FE39C591" authority="Moulton" authorityName="Moulton" authorityYear="1935" box="[151,454,1722,1745]" class="Insecta" family="Thripidae" genus="Anaphothrips" kingdom="Animalia" order="Thysanoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="varii">
<emphasis id="AFD8EADDFFEAFF9BFF72F969FE9EC591" box="[151,353,1722,1745]" italics="true" pageId="52" pageNumber="53">Anaphothrips varii</emphasis>
Moulton
</taxonomicName>
, in 
<treatmentCitation id="1C0D10DEFFEAFF9BFE09F969FCC9C591" author="Moulton, D. &amp; Newman, L. J." box="[492,822,1722,1745]" page="98" pageId="52" pageNumber="53" year="1935">
