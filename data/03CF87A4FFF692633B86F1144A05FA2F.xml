<taxonomicNameLabel id="A22157DBFFF792603ADCF2424C73F8BC" box="[461,520,1857,1880]" pageId="5" pageNumber="27" rank="species">n. sp.</taxonomicNameLabel>
, paratype, Japan; 9, 
<taxonomicName id="4C664D31FFF7926039F3F2424D32F8BC" box="[738,841,1857,1880]" class="Insecta" family="Ypsolophidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="27" phylum="Arthropoda" rank="species" species="sordida" status="n. sp.">
<emphasis id="B912EAA0FFF7926039F3F2424D32F8BC" box="[738,841,1857,1880]" italics="true" pageId="5" pageNumber="27">Y. sordida</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22157DBFFF792603842F2424DF6F8BC" box="[851,909,1857,1880]" pageId="5" pageNumber="27" rank="species">n. sp.</taxonomicNameLabel>
, holotype, China; 10, 
<taxonomicName id="4C664D31FFF792603F91F2424B60F8BC" box="[1152,1307,1857,1880]" class="Insecta" family="Ypsolophidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="27" phylum="Arthropoda" rank="species" species="contractella">
