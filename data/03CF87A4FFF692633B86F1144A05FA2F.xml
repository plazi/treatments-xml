<emphasis id="B912EAA0FFF792603F88F2234A91F8D3" box="[1177,1258,1824,1847]" italics="true" pageId="5" pageNumber="27">Y. helva</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22157DBFFF792603FE3F2234B56F8D3" box="[1266,1325,1824,1847]" pageId="5" pageNumber="27" rank="species">n. sp.</taxonomicNameLabel>
, holotype, China; 8, 
<taxonomicName id="4C664D31FFF792603BEEF2424FBDF8BC" box="[255,454,1857,1880]" class="Insecta" family="Ypsolophidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="27" phylum="Arthropoda" rank="species" species="pseudoparallela" status="n. sp.">
<emphasis id="B912EAA0FFF792603BEEF2424FBDF8BC" box="[255,454,1857,1880]" italics="true" pageId="5" pageNumber="27">Y. pseudoparallela</emphasis>
