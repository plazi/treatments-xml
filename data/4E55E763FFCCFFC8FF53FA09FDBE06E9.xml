<taxonomicName id="01FC2DF6FFCCFFC8FEA8FAB6FD34058A" authority="Rohwer, 1919: 3" authorityName="Rohwer" authorityPageNumber="3" authorityYear="1919" box="[364,705,1413,1436]" pageId="26" pageNumber="27" rank="variety" variety="luzonensis">
