<collectingRegion id="49A7F81915502810A7D6F900FD9CF8E8" box="[469,552,1791,1816]" country="Israel" name="Central" pageId="2" pageNumber="78">Central</collectingRegion>
Аsia (
<collectingCountry id="F374766B15502810A47FF900FCB7F8E8" box="[636,771,1791,1816]" name="Kazakhstan" pageId="2" pageNumber="78">Kazakhstan</collectingCountry>
, 
<collectingCountry id="F374766B15502810A517F900FC21F8E8" box="[788,917,1791,1816]" name="Uzbekistan" pageId="2" pageNumber="78">Uzbekistan</collectingCountry>
, 
<collectingCountry id="F374766B15502810A5A5F8FFFB9EF8E8" box="[934,1066,1792,1816]" name="Kyrgyzstan" pageId="2" pageNumber="78">Kyrgyzstan</collectingCountry>
, 
<collectingCountry id="F374766B15502810A239F900FB63F8E8" box="[1082,1239,1791,1816]" name="Turkmenistan" pageId="2" pageNumber="78">Turkmenistan</collectingCountry>
