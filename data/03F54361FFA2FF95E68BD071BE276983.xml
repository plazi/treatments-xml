‘ 
<typeStatus id="54E74CD5FFA2FF95E5D5D1BEBE8769D8" box="[969,1022,1937,1961]" pageId="26" pageNumber="30">type</typeStatus>
<number id="3D94E050FFA2FF95E219D1BEB93C69D8" box="[1029,1093,1937,1961]" pageId="26" pageNumber="30" value="2657.0">2657</number>
