<taxonomicName id="D2AD68D3FFEDFFBEFBE1FC1253680B34" authorityName="Novais &amp; Dumas &amp; Nessimian" authorityYear="2019" box="[1039,1123,1018,1043]" class="Insecta" family="Philopotamidae" genus="Alterosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="cornuta">
