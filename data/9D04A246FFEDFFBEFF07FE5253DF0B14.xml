<subSubSection id="5DB740DBFFEDFFBEFCF4FC3253DF0B14" pageId="5" pageNumber="6" type="etymology">
<paragraph id="15121350FFEDFFBEFCF4FC3253DF0B14" blockId="5.[794,1418,986,1075]" pageId="5" pageNumber="6">
<emphasis id="27D9CF42FFEDFFBEFCF4FC3254AA0CD4" bold="true" box="[794,929,986,1011]" pageId="5" pageNumber="6">Etymology:</emphasis>
