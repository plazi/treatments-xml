<taxonomicName id="C21D1D783918845229FADACF2F05AECF" genus="Platygastroidea" lsidName="Platygastroidea puncticollis" pageId="0" pageNumber="7991" rank="species" species="puncticollis">puncticollis</taxonomicName>
