<taxonomicName id="EB2704EDBC6513EF24EE55947CE3026A" LSID="urn:lsid:zoobank.org:act:E1775F62-BDBF-4F45-8806-9DD61B227C11" class="Insecta" family="Ephydridae" genus="Allotrichoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Allotrichoma (Neotrichoma)" order="Diptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="subGenus" subGenus="Neotrichoma">Neotrichoma</taxonomicName>
