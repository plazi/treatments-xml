and 
<taxonomicName id="1E9D4D0E1D6D3920FADDFEEEFAB3FECF" box="[1284,1378,332,357]" class="Insecta" family="Geometridae" genus="Prasinocyma" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="oblita">
