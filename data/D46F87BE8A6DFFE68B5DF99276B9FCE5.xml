<quantity id="9B3E9B4D8A6EFFE6882AFEDA76B7FEAD" box="[639,729,324,350]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="6" pageNumber="7" unit="mm" value="4.2">4.2 mm</quantity>
