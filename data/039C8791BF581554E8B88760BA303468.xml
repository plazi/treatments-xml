<paragraph id="8B8A3687BF581554E8B887F9BC2B36F1" blockId="37.[151,605,1158,1182]" box="[151,605,1158,1182]" pageId="37" pageNumber="38">
<treatmentCitationGroup id="AB2511A9BF581554E8B887F9BC2B36F1" box="[151,605,1158,1182]" pageId="37" pageNumber="38">
