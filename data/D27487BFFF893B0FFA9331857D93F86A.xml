<taxonomicName id="9DDD4D2AFF893B0FFA93318573D1F9FB" authority="Thorell 1897" authorityName="Thorell" authorityYear="1897" box="[1303,1732,1511,1550]" class="Arachnida" family="Pisauridae" genus="Eurychoera" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="40" phylum="Arthropoda" rank="genus">
<emphasis id="68A9EABBFF893B0FFA93318570C9F9F8" box="[1303,1500,1511,1549]" italics="true" pageId="11" pageNumber="40">Eurychoera</emphasis>
<bibRefCitation id="3E4C4B58FF893B0FFA6E318A73D1F9FB" author="Thorell" box="[1514,1732,1512,1550]" journalOrPublisher="Svenska Vetenskap-Akademiens Handlingar" pageId="11" pageNumber="40" pagination="1 - 36" part="22" refString="Thorell, T. 1897. Araneae paucae Asie australis. Bihang tili Kongl. Svenska Vetenskap-Akademiens Handlingar 22 (6): 1 - 36." title="Araneae paucae Asie australis. Bihang tili Kongl" type="journal article" year="1897">Thorell 1897</bibRefCitation>
