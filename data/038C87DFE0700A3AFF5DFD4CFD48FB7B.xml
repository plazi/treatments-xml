<emphasis id="B951EADBE0700A3AFF0DFB03FE82FB1F" bold="true" box="[199,308,1168,1194]" pageId="8" pageNumber="565">Remarks</emphasis>
. 
<taxonomicName id="4C254D4AE0700A3AFE89FB03FD74FB1F" authorityName="Viraktamath &amp; Webb" authorityYear="2019" box="[323,706,1168,1194]" class="Insecta" family="Cicadellidae" genus="Ulopsina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="565" phylum="Arthropoda" rank="species" species="himalayana" subGenus="Indoulopa">
<emphasis id="B951EADBE0700A3AFE89FB03FE1CFB1F" box="[323,426,1168,1194]" italics="true" pageId="8" pageNumber="565">Ulopsina</emphasis>
(
<emphasis id="B951EADBE0700A3AFE70FB03FD9BFB1F" box="[442,557,1168,1194]" italics="true" pageId="8" pageNumber="565">Indoulopa</emphasis>
) 
