<taxonomicName id="A8047D8DFE4214ADEAE006C5EA5A402A" authorityName="Brusina" authorityYear="1874" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis inconstans subsp. costulata" order="Pteropoda" pageId="86" pageNumber="87" phylum="Mollusca" rank="subSpecies" species="inconstans" subSpecies="costulata">
