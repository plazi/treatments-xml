<taxonomicName id="C5F3AE9D8B57FF8E1222FE37FB39FE2D" box="[1033,1218,404,428]" class="Arachnida" family="Pseudocheylidae" genus="Anoplocheylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="294" phylum="Arthropoda" rank="species" species="malayeriensis">
