<number id="B43BC7398B56FF8F1792F894FE2DF8CE" box="[441,470,1847,1871]" pageId="2" pageNumber="293" value="10.0">10</number>
(φ,ĸ)- 
<number id="B43BC7398B56FF8F143EF894FDC7F8CE" box="[533,572,1847,1871]" pageId="2" pageNumber="293" value="6.0" valueMax="7.0" valueMin="5.0">5-7</number>
- 
<number id="B43BC7398B56FF8F146EF894FDAFF8CE" box="[581,596,1847,1871]" pageId="2" pageNumber="293" value="7.0">7</number>
, genua 
<number id="B43BC7398B56FF8F1481F894FD42F8CE" box="[682,697,1847,1871]" pageId="2" pageNumber="293" value="7.0">7</number>
- 
<number id="B43BC7398B56FF8F14EAF894FD34F8CE" box="[705,719,1847,1871]" pageId="2" pageNumber="293" value="5.0">5</number>
- 
<number id="B43BC7398B56FF8F14F3F894FD13F8CE" box="[728,744,1847,1871]" pageId="2" pageNumber="293" value="4.0">4</number>
- 
<number id="B43BC7398B56FF8F14DBF894FCFBF8CE" box="[752,768,1847,1871]" pageId="2" pageNumber="293" value="4.0">4</number>
, telofemora 
