<taxonomicName id="D1836DB1FFDAFFD9FEE8FDFCC675FD9D" authority="Fabricius" authorityName="Fabricius" box="[351,557,518,544]" class="Insecta" family="Formicidae" genus="Lasius" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
<emphasis id="24F7CA20FFDAFFD9FEE8FDFCC5F7FD9D" bold="true" box="[351,431,518,544]" italics="true" pageId="12" pageNumber="13">Lasius</emphasis>
