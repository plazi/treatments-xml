<emphasis id="24F7CA20FFDAFFD9FE11FCBBC66BFCE6" bold="true" box="[422,563,833,859]" pageId="12" pageNumber="13">
<taxonomicNameLabel id="3FC4775BFFDAFFD9FE11FCBBC66BFCE6" box="[422,563,833,859]" pageId="12" pageNumber="13">comb. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="D1836DB1FFDAFFD9FDFEFCBBC68DFCE7" box="[585,725,833,858]" class="Insecta" family="Formicidae" genus="Pogonomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="californicus">
