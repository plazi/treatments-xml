<collectingCountry id="F344760DFFD4D113FD9EFEB7FD110724" box="[614,684,294,319]" name="Israel" pageId="6" pageNumber="67">Israeli</collectingCountry>
<taxonomicName id="4C534D1EFFD4D113FD4BFEB7FC950724" box="[691,808,294,319]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="67" phylum="Arthropoda" rank="genus">
