<paragraph id="BE1742489E689308A3F8F9E4FD31DC7A" blockId="49.[151,755,1536,1559]" box="[151,755,1536,1559]" pageId="49" pageNumber="50">
<emphasis id="8CDC9E5A9E689308A3F8F9E4FEDBDC7A" bold="true" box="[151,281,1536,1559]" pageId="49" pageNumber="50">FIGURE 85</emphasis>
. 
<taxonomicName id="79A839CB9E689308A24AF9E6FDCADC7A" authorityName="Perkins" authorityYear="2012" box="[293,520,1537,1559]" class="Insecta" family="Epimetopidae" genus="Epimetopus" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="coleuncus">
<emphasis id="8CDC9E5A9E689308A24AF9E6FDCADC7A" box="[293,520,1537,1559]" italics="true" pageId="49" pageNumber="50">Epimetopus coleuncus</emphasis>
