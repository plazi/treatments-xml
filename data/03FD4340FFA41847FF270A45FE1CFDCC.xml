<quantity id="4CAC5FB3FFA51847FCCA0E10FC6EFE97" box="[890,990,294,322]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="31" pageNumber="32" unit="m" value="3.0">3,000 m</quantity>
