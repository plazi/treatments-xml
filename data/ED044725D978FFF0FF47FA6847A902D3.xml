<taxonomicName id="A2AD8DB0D97AFFFFFE8EF9BF4488066D" authorityName="Turner" authorityYear="1922" box="[350,642,1592,1614]" class="Insecta" family="Aulacidae" genus="Pristaulacus" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="emarginaticeps">
