<taxonomicName id="107209A288F8BFCF02F7D83BD40854E1" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina laxa" order="Stylommatophora" pageId="51" pageNumber="52" phylum="Mollusca" rank="species" species="laxa">Montenegrina laxa</taxonomicName>
