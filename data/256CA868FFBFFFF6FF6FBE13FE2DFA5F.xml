, 
<collectingCountry id="D5D259EEFFBFFFF6FB51B9E3FAA0FA3B" box="[1193,1287,1425,1449]" name="Rwanda" pageId="37" pageNumber="38">Rwanda</collectingCountry>
, and 
<collectingCountry id="D5D259EEFFBFFFF6FABDB9E3FA3BFA3B" box="[1349,1436,1425,1449]" name="Uganda" pageId="37" pageNumber="38">Uganda</collectingCountry>
