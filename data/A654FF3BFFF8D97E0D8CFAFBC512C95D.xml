<taxonomicName id="E9FD35AEFFF7D97E0CF4FF79C32ECB88" box="[495,619,189,215]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="9" pageNumber="12" phylum="Arthropoda" rank="genus">mesosoma</taxonomicName>
