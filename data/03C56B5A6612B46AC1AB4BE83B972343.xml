<subSubSection id="C37689C76612B46AC1FB4AFD3B972343" pageId="71" pageNumber="72" type="biology_ecology">
<paragraph id="8BD3DA4C6612B46AC1FB4AFD39BE20F7" blockId="71.[151,1436,289,530]" box="[199,420,397,422]" pageId="71" pageNumber="72">
<emphasis id="B918065E6612B46AC1FB4AFD393D20F7" bold="true" box="[199,295,397,422]" pageId="71" pageNumber="72">Biology.</emphasis>
