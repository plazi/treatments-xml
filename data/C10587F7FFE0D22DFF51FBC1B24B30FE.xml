<taxonomicName id="8EAC4D62FFE0D22DFCADFB71B36330D3" authorityName="Holmgren" authorityYear="1860" box="[875,1117,1194,1220]" class="Insecta" family="Ichneumonidae" genus="Mesochorus" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="235" phylum="Arthropoda" rank="species" species="orbitalis">
