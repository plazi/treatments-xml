<specimenCount id="9D3FFD58FFABFF82E28C48DD340A6CBA" box="[151,291,935,960]" pageId="4" pageNumber="5" type="generic">
<number id="3DF124F6FFABFF82E28C48DD358C6CBA" box="[151,165,936,960]" pageId="4" pageNumber="5" value="4.0">4</number>
