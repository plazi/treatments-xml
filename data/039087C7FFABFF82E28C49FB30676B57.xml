<taxonomicName id="4C394D52FFABFF82E28C48B937396C9E" authority="Straneo, 1993" authorityName="Straneo" authorityYear="1993" box="[151,528,972,996]" class="Insecta" family="Carabidae" genus="Ogmopleura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="minor">
