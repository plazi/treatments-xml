<paragraph id="8B8936AB3B4BFFAA6839A76AFDAFFA0A" blockId="16.[125,1172,1354,1410]" pageId="16" pageNumber="798">
Wingspan 
<quantity id="4CCE9B4E3B4BFFAA68BAA76AFEA4FAEA" box="[254,338,1354,1378]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" pageId="16" pageNumber="798" unit="mm" value="16.0">16 mm</quantity>
