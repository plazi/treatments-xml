<taxonomicName id="4C1740B7FFF2FFEDFEA4B500606BFA37" authority="Simon" authorityName="Simon" box="[352,608,1393,1417]" class="Arachnida" family="Salticidae" genus="Pochyta" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="fastibilis">
