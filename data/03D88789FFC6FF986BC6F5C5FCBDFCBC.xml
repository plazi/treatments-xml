<collectionCode id="ED60AE5AFFC6FF986BE4F58DFF23FC98" box="[181,243,762,786]" httpUri="http://grbio.org/cool/fx5p-e4j3" name="Culture Collection of Algae at the University of Vienna" pageId="2" pageNumber="56">ASW</collectionCode>
