<taxonomicName id="4C4F4D0CFFF1FF87F1E4FD062DAB772D" authority="(Melichar)" baseAuthorityName="Melichar" baseAuthorityYear="1915" box="[689,931,762,787]" class="Insecta" family="Derbidae" genus="Alara" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="hyalina">
