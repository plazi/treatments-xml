<taxonomicName id="4C4F4D0CFFF1FF87F21DFD062C16772C" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,542,763,787]" class="Insecta" family="Derbidae" genus="Alara" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="australiensis">
