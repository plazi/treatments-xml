<taxonomicName id="4C4F4D0CFFF1FF87F793FD062B36772C" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1222,1342,763,787]" class="Insecta" family="Derbidae" genus="Alara" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="linigera" status="sp. nov.">
