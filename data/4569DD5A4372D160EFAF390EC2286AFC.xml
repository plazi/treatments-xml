<paragraph id="CD468DB519965BF6C2AD4A5A4CFEA4C4" pageId="10" pageNumber="121">
<taxonomicName id="E7F790B6BA509AEC23B31BBBECB0206C" class="Insecta" family="Vespidae" genus="Odynerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Odynerus septemfasciatus" order="Hymenoptera" pageId="10" pageNumber="121" phylum="Arthropoda" rank="species" species="septemfasciatus">Odynerus septemfasciatus</taxonomicName>
