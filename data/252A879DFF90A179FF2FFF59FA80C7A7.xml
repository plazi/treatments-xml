<taxonomicName id="6A834D08FF90A179FC6CFE9EFBD6C741" box="[980,1137,336,361]" class="Insecta" family="Tropiduchidae" genus="Leusaba" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="59" phylum="Arthropoda" rank="species" species="marginalis">
