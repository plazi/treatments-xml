<paragraph id="27253699FFD96802FF4DDBBCEDC1A6BC" blockId="43.[151,1437,151,1547]" pageId="43" pageNumber="44">
<emphasis id="15EEEA8BFFD96802FF4DDBBCEC60A69F" bold="true" box="[197,341,943,968]" pageId="43" pageNumber="44">
<emphasis id="15EEEA8BFFD96802FF4DDBBCEC7BA69F" bold="true" box="[197,334,943,968]" italics="true" pageId="43" pageNumber="44">Dimensions</emphasis>
