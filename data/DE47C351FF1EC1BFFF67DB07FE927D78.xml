<collectingCountry id="2EF932D7FF1FC1BFFE97DAF6FE507F91" box="[359,422,1195,1221]" name="Chile" pageId="133" pageNumber="134">Chile</collectingCountry>
, 
<collectingCountry id="2EF932D7FF1FC1BFFE42DAF6FDE47F91" box="[434,530,1195,1221]" name="Ecuador" pageId="133" pageNumber="134">Ecuador</collectingCountry>
