<taxonomicName id="7ED84D58FFA97757BD2BA4B69DC58C86" authority="Morishita, 1924" authorityName="Morishita" authorityYear="1924" box="[780,1195,1686,1711]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="69" pageNumber="70" phylum="Platyhelminthes" rank="species" species="vagum">
