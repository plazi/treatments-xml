3. 
<taxonomicName id="A478739B45CA6AAE2BA647C490C4D714" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31935" class="Insecta" family="Formicidae" genus="Myrmecia" kingdom="Animalia" lsidName-HNS="Myrmecia simillima Smith" order="Hymenoptera" pageId="144" pageNumber="144" phylum="Arthropoda" rank="species" species="simillima">Myrmecia simillima</taxonomicName>
