<collectionCode id="D9993082FFFA753AFE7AFD83FE1DE767" box="[384,405,601,625]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="10" pageNumber="97" type="Herbarium">G</collectionCode>
[
<specimenCode id="EF2E003CFFFA753AFE65FD83FD96E767" box="[415,542,601,625]" collectionCode="G" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="10" pageNumber="97" type="Herbarium">G00341484</specimenCode>
