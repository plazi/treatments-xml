<taxonomicName id="AA8E4D15FFE7FFC3FCF85F94FC10FC01" box="[852,942,952,977]" class="Mammalia" family="Soricidae" genus="Blarina" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="41" phylum="Chordata" rank="genus">
