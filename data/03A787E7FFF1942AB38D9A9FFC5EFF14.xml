<heading id="D0F9819DFFF1942CB38D9A9FFD39F8FF" bold="true" box="[151,695,1784,1811]" fontSize="11" level="1" pageId="27" pageNumber="328" reason="1">
<taxonomicName id="4C0E4D72FFF1942CB38D9A9FFD39F8FF" authority="Bequaert" authorityName="Bequaert" authorityYear="1937" box="[151,695,1784,1811]" class="Insecta" family="Vespidae" genus="Polistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="328" phylum="Arthropoda" rank="species" species="testaceicolor" subGenus="Epicnemius">
<emphasis id="B97AEAE3FFF1942CB38D9A9FFD39F8FF" bold="true" box="[151,695,1784,1811]" pageId="27" pageNumber="328">
