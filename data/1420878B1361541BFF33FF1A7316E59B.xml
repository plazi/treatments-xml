<treatmentCitationGroup id="BC9911B31361541BFF33FE80746AE062" box="[151,524,258,280]" pageId="16" pageNumber="17">
<emphasis id="AEFDEA8F1361541BFF33FE807769E062" box="[151,271,258,280]" italics="true" pageId="16" pageNumber="17">Euacanthus</emphasis>
<taxonomicName id="5B894D1E1361541BFEB0FE80746EE062" authority="Distant, 1918: 11" authorityName="Distant" authorityPageNumber="11" authorityYear="1918" box="[276,520,258,280]" class="Insecta" family="Cicadellidae" genus="Evacanthus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="bellus">
