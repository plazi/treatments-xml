<collectionCode id="9EE31AE833621B45FE9EA690FE4FFC04" box="[305,397,939,962]" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="11" pageNumber="56">NHMW</collectionCode>
