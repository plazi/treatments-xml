<taxonomicName id="FAF4BD73C2157FF0DC5D755FB69D6655" baseAuthorityName="Morelet" baseAuthorityYear="1860" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="116" pageNumber="117" phylum="Mollusca" rank="subSpecies" species="rhodolarynx" subSpecies="papillatus">
