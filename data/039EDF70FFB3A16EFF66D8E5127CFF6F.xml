<location id="8EE838BDFFB3A16DFB5DDEDE1554F9DA" LSID="urn:lsid:plazi:treatment:039EDF70FFB3A16EFF66D8E5127CFF6F:8EE838BDFFB3A16DFB5DDEDE1554F9DA" box="[1196,1387,1518,1543]" country="New Caledonia" county="Pacific" municipality="This" name="western Atlantic" pageId="13" pageNumber="164">western Atlantic</location>
(
<taxonomicName id="4C3715E5FFB3A16DFA73DEDE1122F9F6" class="Crinoidea" family="Comasteridae" genus="Comactinia" kingdom="Animalia" order="Comatulida" pageId="13" pageNumber="164" phylum="Echinodermata" rank="species" species="echinoptera">
<emphasis id="B943B274FFB3A16DFA73DEDE1122F9F6" italics="true" pageId="13" pageNumber="164">C. echinoptera</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3715E5FFB3A16DFEAEDD22122CF9F6" box="[351,531,1555,1579]" class="Crinoidea" family="Comasteridae" genus="Comactinia" kingdom="Animalia" order="Comatulida" pageId="13" pageNumber="164" phylum="Echinodermata" rank="species" species="meridionalis">
<emphasis id="B943B274FFB3A16DFEAEDD22122CF9F6" box="[351,531,1555,1579]" italics="true" pageId="13" pageNumber="164">C. meridionalis</emphasis>
</taxonomicName>
) and 
<location id="8EE838BDFFB3A16DFDAEDD231371F9F6" LSID="urn:lsid:plazi:treatment:039EDF70FFB3A16EFF66D8E5127CFF6F:8EE838BDFFB3A16DFDAEDD231371F9F6" box="[607,846,1554,1579]" country="New Caledonia" county="Pacific" municipality="This" name="Indo-western Pacific" pageId="13" pageNumber="164">Indo-western Pacific</location>
waters (
<taxonomicName id="4C3715E5FFB3A16DFC4BDD22142BF9F7" box="[954,1044,1555,1578]" class="Crinoidea" family="Comasteridae" genus="Comactinia" kingdom="Animalia" order="Comatulida" pageId="13" pageNumber="164" phylum="Echinodermata" rank="species" species="titan">
