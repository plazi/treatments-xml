<emphasis id="B903EAC5FF89F90169C1F8DD1C62A711" box="[719,1075,1873,1897]" italics="true" pageId="98" pageNumber="99">
<taxonomicName id="4C774D54FF89F90169C1F8DD1B13A711" box="[719,834,1873,1897]" class="Arachnida" family="Chthoniidae" genus="Chthonius" kingdom="Animalia" order="Pseudoscorpiones" pageId="98" pageNumber="99" phylum="Arthropoda" rank="genus">Chthonius</taxonomicName>
(
<taxonomicName id="4C774D54FF89F901685BF8DD1C7AA711" box="[853,1067,1873,1897]" class="Arachnida" family="Chthoniidae" genus="Ephippiochthonius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pseudoscorpiones" pageId="98" pageNumber="99" phylum="Arthropoda" rank="genus">Ephippiochthonius</taxonomicName>
)
</emphasis>
