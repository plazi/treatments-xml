<geoCoordinate id="94EBE3B7FFD3FF89C103F97DFB43F890" box="[1036,1188,1783,1808]" degrees="049" direction="west" minutes="56.49" orientation="longitude" pageId="10" pageNumber="795" precision="9" value="-49.9415">049°56.49ʹ W</geoCoordinate>
