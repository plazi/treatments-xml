<emphasis id="C3AB5962FFD3FF89C622F8E8FC2CF805" bold="true" box="[813,971,1901,1925]" pageId="10" pageNumber="795">Identification.</emphasis>
<taxonomicName id="36DFFEF3FFD3FF89C6DCF8EBFB4AF805" baseAuthorityName="E. Geoffroy Saint-Hilaire" baseAuthorityYear="1803" box="[979,1197,1901,1925]" class="Mammalia" family="Felidae" genus="Puma" kingdom="Animalia" order="Carnivora" pageId="10" pageNumber="795" phylum="Chordata" rank="species" species="yagouaroundi">
