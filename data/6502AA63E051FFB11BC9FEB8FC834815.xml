<collectionCode id="8BBA83B0E051FFB11A9DFD85FC294E5D" box="[905,988,587,610]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="21" pageNumber="668">MNHN</collectionCode>
