<taxonomicName id="4C4E4D106D7DFFA3DD0AF102E5550729" authority="Hall &amp; Williams 1962" authorityName="Hall &amp; Williams" authorityYear="1962" box="[601,1026,511,538]" class="Insecta" family="Diaspididae" genus="Cryptoparlatoreopsis" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="372" phylum="Arthropoda" rank="species" species="euphorbiae">
