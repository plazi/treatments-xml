<subSubSection id="4DC3BA9EFF91FF99FEB316716CCAE947" box="[312,973,1579,1605]" pageId="5" pageNumber="52" type="etymology">
<paragraph id="0566E915FF91FF99FEB316716CCAE947" blockId="5.[264,1323,1299,1885]" box="[312,973,1579,1605]" pageId="5" pageNumber="52">
