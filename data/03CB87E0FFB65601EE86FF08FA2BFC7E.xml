<taxonomicName id="4C624D75FFB65600ECE1F8FFFBC5F8C2" authority="Ferrari-Perez" authorityName="Ferrari-Perez" box="[752,1146,1888,1914]" class="Mammalia" family="Leporidae" genus="Romerolagus" kingdom="Animalia" order="Lagomorpha" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="diazi">
