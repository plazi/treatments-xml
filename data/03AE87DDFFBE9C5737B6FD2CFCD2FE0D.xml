<collectingCountry id="F310765BFFBF9C57360DFE4CFE3EFE0D" box="[379,490,483,507]" name="Thailand" pageId="53" pageNumber="52">Thailand</collectingCountry>
(
<collectingRegion id="49C3F829FFBF9C57368CFE4CFD2EFE0D" box="[506,762,483,507]" country="Thailand" name="Nakhon Ratchasima" pageId="53" pageNumber="52">Nakhon Ratchasima</collectingRegion>
