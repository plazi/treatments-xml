<taxonomicName id="56BB4D3B8914FFF7FDB7F91B195A0000" authority="Drake" authorityName="Drake" box="[609,820,260,285]" class="Insecta" family="Tingidae" genus="Trachypeplus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="544" phylum="Arthropoda" rank="species" species="chinensis">
