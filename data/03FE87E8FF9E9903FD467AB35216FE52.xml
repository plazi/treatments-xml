<quantity id="4CAF9B1BFF9E9902FBBF789F5214F964" box="[1036,1110,1682,1703]" metricMagnitude="-3" metricUnit="m" metricValue="8.7" pageId="24" pageNumber="25" unit="mm" value="8.7">8.7 mm</quantity>
