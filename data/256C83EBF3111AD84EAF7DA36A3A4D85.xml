<taxonomicName id="7D29AA902404EABEF70FE299982EEB34" authority="Solier, 1844" authorityName="Solier" authorityYear="1844" class="Insecta" family="Tenebrionidae" genus="Cylindrothorus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cylindrothorus" order="Coleoptera" pageId="184" pageNumber="185" phylum="Arthropoda" rank="genus">Cylindrothorus</taxonomicName>
