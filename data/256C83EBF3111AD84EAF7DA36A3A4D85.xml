<taxonomicName id="44A37DFB1679C92F3861E1B34EADF88E" authority="Pic, 1954" authorityName="Pic" authorityYear="1954" class="Insecta" family="Tenebrionidae" genus="Alleculina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Alleculina" order="Coleoptera" pageId="184" pageNumber="185" phylum="Arthropoda" rank="genus">Alleculina</taxonomicName>
