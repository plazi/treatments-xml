<taxonomicName id="E1C81386FFE1FF86FC274849FC5EFCEB" box="[906,979,795,820]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="64" pageNumber="65" phylum="Tracheophyta" rank="genus">
