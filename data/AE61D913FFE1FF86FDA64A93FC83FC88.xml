<quantity id="E130C5E0FFE1FF86FF10486FFE9BFC87" box="[189,278,829,856]" metricMagnitude="3" metricUnit="m" metricValue="1.63" pageId="64" pageNumber="65" unit="m" value="1630.0">1630 m</quantity>
in 
<collectingCountry id="5EDF2895FFE1FF86FE96486FFE2DFC88" box="[315,416,829,855]" name="Bulgaria" pageId="64" pageNumber="65">Bulgaria</collectingCountry>
