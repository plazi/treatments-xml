<quantity id="E130C5E0FFE1FF86FDA64848FDEAFCEB" box="[523,615,794,821]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="64" pageNumber="65" unit="m" value="1500.0">1500 m</quantity>
