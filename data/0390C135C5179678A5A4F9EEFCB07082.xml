<taxonomicName id="4C390BA0C5089678A1A0FF09FADE70E9" box="[1171,1382,152,177]" class="Acoelomorpha" family="Hofsteniidae" genus="Hofstenia" kingdom="Animalia" order="Acoela" pageId="16" pageNumber="17" phylum="Xenacoelomorpha" rank="species" species="miamia">
