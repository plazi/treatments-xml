</taxonomicName>
collected from 
<collectingCountry id="F32E30B3C5089678A417FF51FE2A7082" box="[292,402,192,218]" name="Bahamas" pageId="16" pageNumber="17">Bahamas</collectingCountry>
, 
<collectingCountry id="F32E30B3C5089678A4ADFF51FE547082" box="[414,492,192,218]" name="Belize" pageId="16" pageNumber="17">Belize</collectingCountry>
