<emphasis id="B902EA9B9F3CA03E1ACAFD7CFD8EFC9F" bold="true" box="[189,570,740,769]" pageId="16" pageNumber="17">
<emphasis id="B902EA9B9F3CA03E1ACAFD7CFE23FC9F" bold="true" box="[189,407,740,769]" italics="true" pageId="16" pageNumber="17">Key to species of</emphasis>
<taxonomicName id="4C764D0A9F3CA03E1BE8FD7CFD8EFC9F" authorityName="Guerin-Meneville" authorityYear="1845" box="[415,570,741,769]" class="Insecta" family="Bostrichidae" genus="Xylopertha" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">Xylopertha</taxonomicName>
