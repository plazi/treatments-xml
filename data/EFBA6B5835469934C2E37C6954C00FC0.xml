<taxonomicName id="D982C7A9C86E491844C0A326D0E74957" class="Insecta" family="Scarabaeidae" genus="Coprophanaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coprophanaeus (Coprophanaeus) jasius" order="Coleoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="jasius" subGenus="Coprophanaeus">Coprophanaeus (Coprophanaeus) jasius</taxonomicName>
