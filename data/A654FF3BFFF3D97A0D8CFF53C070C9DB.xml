<taxonomicName id="E9FD35AEFFF3D97A0FDCFE4EC2BECAFB" authority="Rohwer, 1915" authorityName="Rohwer" authorityYear="1915" box="[711,1019,394,420]" class="Insecta" family="Tenthredinidae" genus="Homalomma" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="caliroae">
