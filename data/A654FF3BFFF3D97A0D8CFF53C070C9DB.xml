<taxonomicName id="E9FD35AEFFF3D97A0D8CFE4EC3CBCAFB" authority="Rohwer, 1915" authorityName="Rohwer" authorityYear="1915" box="[151,654,394,420]" class="Insecta" family="Tenthredinidae" genus="Homalomma" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="eriocampoides">
<emphasis id="1C89923FFFF3D97A0D8CFE4EC0A6CAFC" box="[151,483,394,419]" italics="true" pageId="13" pageNumber="14">Homalomma eriocampoides</emphasis>
