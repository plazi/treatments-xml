<taxonomicName id="1B8D9D99FFA2893CCA2FFC06CF78FBCB" authority="Id." authorityName="Id." box="[1130,1396,1009,1034]" class="Insecta" family="Scirtidae" genus="Cyphon" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="fenestratus">Cyphon fenestratus Id.</taxonomicName>
