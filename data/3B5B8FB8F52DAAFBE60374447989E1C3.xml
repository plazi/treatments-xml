<taxonomicName id="4044802DAA12013DAC17A5E7AC0C8A29" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138732" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica sublaevis Nylander" order="Hymenoptera" pageNumber="78" phylum="Arthropoda" rank="species" species="sublaevis">Myrmica sublaevis Nylander</taxonomicName>
