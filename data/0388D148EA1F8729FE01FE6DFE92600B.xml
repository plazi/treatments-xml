<emphasis id="B955BC4CEA1F8729FFE6FE3DFEB462D8" bold="true" box="[113,261,458,483]" pageId="21" pageNumber="572">
<typeStatus id="549ADEFCEA1F8729FFE6FE3DFF1862D9" box="[113,169,459,483]" pageId="21" pageNumber="572">Type</typeStatus>
