<taxonomicName id="4C0A11DCC055B83A8180C54AFBDF7DC9" box="[1008,1150,364,389]" class="Elasmobranchii" family="Rhinobatidae" genus="Rhinobatos" kingdom="Animalia" order="Rajiformes" pageId="16" pageNumber="272" phylum="Chordata" rank="species" species="schlegelii">
<emphasis id="B97EB64DC055B83A8180C54AFBDF7DC9" box="[1008,1150,364,389]" italics="true" pageId="16" pageNumber="272">R. schlegelii</emphasis>
</taxonomicName>
