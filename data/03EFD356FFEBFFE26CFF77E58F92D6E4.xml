<quantity id="4CBECFA5FFEBFFE26CFF73128C66D434" box="[151,295,1120,1146]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="48" pageNumber="49" unit="mm" value="0.85" valueMax="0.9" valueMin="0.8">0.8–0.9 mm</quantity>
