<taxonomicName id="7F97A7C49834692DA02B2A4A7AEA0FC5" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="cymosum">
