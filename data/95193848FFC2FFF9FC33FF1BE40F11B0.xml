<subSubSection id="55AADAD5FFC2FFF9FCC6FF32E40F11B0" lastPageId="26" lastPageNumber="27" pageId="25" pageNumber="26" type="materials_examined">
<paragraph id="1D0F895EFFC2FFFAFCC6FF32E073112F" blockId="25.[826,1327,235,257]" box="[826,1327,235,257]" pageId="25" pageNumber="26">
<emphasis id="2FC4554CFFC2FFFAFCC6FF32E698112E" box="[826,964,235,256]" italics="true" pageId="25" pageNumber="26">
<typeStatus id="C20B37FCFFC2FFFAFCC6FF32E633112E" box="[826,879,235,256]" pageId="25" pageNumber="26">Type</typeStatus>
