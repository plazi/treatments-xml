<subSubSection id="C3556539DD22FFD2FF2805E213E2FB3A" box="[151,1180,1230,1255]" pageId="5" pageNumber="506" type="materials_examined">
<paragraph id="8BF036B2DD22FFD2FF2805E213E2FB3A" blockId="5.[151,1437,1230,1506]" box="[151,1180,1230,1255]" pageId="5" pageNumber="506">
<emphasis id="B93BEAA0DD22FFD2FF2805E216C7FB3B" bold="true" box="[151,441,1230,1255]" pageId="5" pageNumber="506">
