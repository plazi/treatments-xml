<quantity id="DF6D9CC28773595CFC37BC84FB8CFD74" box="[966,1050,724,747]" metricMagnitude="3" metricUnit="m" metricValue="1.42" pageId="29" pageNumber="288" unit="m" value="1420.0">1420 m</quantity>
