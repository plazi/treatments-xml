</specimenCount>
with larvae from grass, 
<date id="D9741033FFE32B595AD1FA3671456ABF" box="[151,270,1482,1507]" pageId="9" pageNumber="190" value="1973-09-28">
<number id="1B0224D4FFE32B595AD1FA3670FE6ABF" box="[151,181,1482,1507]" pageId="9" pageNumber="190" value="28.0">28</number>
