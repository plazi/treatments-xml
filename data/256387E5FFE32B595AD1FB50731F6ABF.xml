<specimenCount id="BBCCFD7AFFE32B5959E1FA5A74576AE3" box="[935,1052,1446,1471]" pageId="9" pageNumber="190" type="female">
<number id="1B0224D4FFE32B5959E1FA5A73FE6AE3" box="[935,949,1446,1471]" pageId="9" pageNumber="190" value="5.0">5</number>
females
</specimenCount>
