<specimenCount id="BBCCFD7AFFE32B595E6AFA5B74366AE3" box="[1068,1149,1446,1471]" pageId="9" pageNumber="190" type="male">
<number id="1B0224D4FFE32B595E6AFA5B74716AE3" box="[1068,1082,1447,1471]" pageId="9" pageNumber="190" value="1.0">1</number>
