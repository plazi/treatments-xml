<taxonomicName id="D6D15DF6653B8515D0DF23D4FD30986F" authority="Thuy, 2013" authorityName="Thuy" authorityYear="2013" box="[495,712,1789,1815]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiogaleus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ophiurida" pageId="110" pageNumber="111" phylum="Echinodermata" rank="species" species="stans" status="sp. nov.">
