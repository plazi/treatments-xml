<taxonomicName id="4C29CC861742FFF2229EFBB9FAEC7441" baseAuthorityName="Whitney" baseAuthorityYear="1994" box="[1157,1280,1122,1143]" class="Aves" family="Rhinocryptidae" genus="Scytalopus" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="413" phylum="Chordata" rank="genus">
