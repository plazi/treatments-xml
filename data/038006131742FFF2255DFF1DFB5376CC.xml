<taxonomicName id="4C29CC861742FFF22570FB7BFC3D7483" authorityName="Lesson" authorityYear="1842" box="[875,977,1184,1205]" class="Aves" family="Rhinocryptidae" genus="Eugralla" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="413" phylum="Chordata" rank="genus">
