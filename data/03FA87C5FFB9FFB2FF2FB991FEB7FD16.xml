<typeStatus id="54E88871FFB8FFB2FF7FBF47FF17FE82" box="[199,255,333,359]" pageId="3" pageNumber="537">Type</typeStatus>
locality: 
<collectingCountry id="F3447643FFB8FFB2FED1BF47FE5AFE82" box="[361,434,333,359]" name="Brazil" pageId="3" pageNumber="537">Brazil</collectingCountry>
: 
<collectingRegion id="4997F831FFB8FFB2FE04BF47FDDBFE82" box="[444,563,333,359]" country="Brazil" name="Piaui" pageId="3" pageNumber="537">Piauí state</collectingRegion>
