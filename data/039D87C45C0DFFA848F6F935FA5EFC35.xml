<collectingRegion id="49F0F8305C0CFFA849DBFD23FE30FCE8" box="[442,510,764,789]" country="Brazil" name="Bahia" pageId="19" pageNumber="20">Bahia</collectingRegion>
, 
<collectingRegion id="49F0F8305C0CFFA84A6BFD23FD6CFCE8" box="[522,674,764,789]" country="Brazil" name="Minas Gerais" pageId="19" pageNumber="20">Minas Gerais</collectingRegion>
