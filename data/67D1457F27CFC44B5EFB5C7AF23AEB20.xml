<taxonomicName id="2679D3B034435D1C406B38342251240F" class="Arachnida" family="Stigmaeidae" genus="Cheylostigmaeus" kingdom="Animalia" order="Prostigmata" pageId="22" pageNumber="161" phylum="Arthropoda" rank="species" species="scutatus">Cheylostigmaeus scutatus (Halbert)</taxonomicName>
<normalizedToken id="5BE2190E561C172636C766BE07912336" originalValue="Männchen" pageId="22" pageNumber="161">Maennchen</normalizedToken>
