<taxonomicName id="3ABF4D18FFDB5F1184E30B89BA7EF853" authority="(Achard)" authorityName="Ach. J. Gomez-Zurita" baseAuthorityName="Achard" baseAuthorityYear="1923" class="Insecta" family="Chrysomelidae" genus="Calligrapha" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="sallei">
