<taxonomicName id="53ED4D68341E7C3BFB56665FFE290519" class="Insecta" family="Phoridae" genus="Gymnophora" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="quartomollis">
