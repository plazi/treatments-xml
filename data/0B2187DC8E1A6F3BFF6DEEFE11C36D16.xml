(
<figureCitation id="1BB32A4F8E1A6F3AFF65EEDB12DE6E2C" box="[159,245,1780,1805]" captionStart="FIGURES 7 – 12" captionStartId="4.[151,264,1220,1243]" captionTargetBox="[157,1425,195,1189]" captionTargetId="figure@4.[151,1436,193,1199]" captionTargetPageId="4" captionText="FIGURES 7 – 12. Head, pronotum &amp; fore legs: 7. semiflava; 8. varia; 9. magnus sp. n.; 10. longisetis sp. n.; 11. antennatus sp. n. Tergites IX – X: 12. antennatus sp. n." httpUri="https://zenodo.org/record/272598/files/figure.png" pageId="6" pageNumber="547">Figs 10</figureCitation>
, 
<figureCitation id="1BB32A4F8E1A6F3AFEFBEEDA13616E2C" box="[257,330,1780,1805]" captionStart="FIGURES 13 – 28" captionStartId="5.[151,264,1255,1278]" captionTargetBox="[154,1422,197,1223]" captionTargetId="figure@5.[151,1436,193,1235]" captionTargetPageId="5" captionText="FIGURES 13 – 28. Antenna: 13. fasciata; 14. magnus sp. n.; 15. exastis; 16. nayari; 17. longisetis sp. n.; 18. semiflava; 19. antennatus sp. n. Pelta: 20. bicoloripes; 21. fasciata; 22. flavescens; 23. semiflava; 24. magnus sp. n.; 25. longisetis sp. n.; 26. antennatus sp. n. Tergites IX – X of male: 27. exastis; 28. semiflava." httpUri="https://zenodo.org/record/272599/files/figure.png" pageId="6" pageNumber="547">17, 25</figureCitation>
, 
<figureCitation id="1BB32A4F8E1A6F3AFEAFEEDA13B66E2C" box="[341,413,1780,1805]" captionStart="FIGURES 29 – 46" captionStartId="6.[151,264,1573,1596]" captionTargetBox="[159,1431,520,1549]" captionTargetId="figure@6.[151,1436,519,1552]" captionTargetPageId="6" captionText="FIGURES 29 – 46. Antenna: 29. bicoloripes; 30. cingulata; 31. flavescens; 32. varia. Mesopresternum: 33. bicoloripes; 34. fasciata; 35. flavescens; 36. semiflava; 37. magnus sp. n.; 38. longisetis sp. n.; 39. antennatus sp. n. Base of fore wing: 40. bicoloripes; 41. fasciata; 42. flavescens; 43. semiflava; 44. magnus sp. n.; 45. longisetis sp. n.; 46. antennatus sp. n." httpUri="https://zenodo.org/record/272600/files/figure.png" pageId="6" pageNumber="547">38, 45</figureCitation>
, 
<figureCitation id="1BB32A4F8E1A6F3AFE53EEDB13EC6E2C" box="[425,455,1780,1805]" captionStart="FIGURES 47 – 54" captionStartId="8.[151,264,1807,1830]" captionTargetBox="[152,1418,629,1765]" captionTargetId="figure@8.[151,1436,619,1768]" captionTargetPageId="8" captionText="FIGURES 47 – 54. Tergites IX – X of female: 47. bicoloripes; 48. cingulata; 49. fasciata; 50. flavescens; 51. nayari; 52. varia; 53. magnus sp. n.; 54. longisetis sp. n." httpUri="https://zenodo.org/record/272601/files/figure.png" pageId="6" pageNumber="547">54</figureCitation>
, 
<figureCitation id="1BB32A4F8E1A6F3AFE29EEDB13DB6E2C" box="[467,496,1780,1805]" captionStart="FIGURES 55 – 66" captionStartId="9.[151,264,1894,1917]" captionTargetBox="[153,1433,193,1872]" captionTargetId="figure@9.[153,1433,193,1873]" captionTargetPageId="9" captionText="FIGURES 55 – 66. Body: 55. magnus sp. n.; 56. fasciata; 57. bicoloripes; 58. antennatus sp. n.; 59. exastis; 60. flavescens; 61. semiflava; 62. longisetis sp. n.; 63. nayari; 64. varia; 65. holotype of ‘ brevibristla ’; 66. paratype of ‘ brevibristla ’." httpUri="https://zenodo.org/record/272602/files/figure.png" pageId="6" pageNumber="547">62</figureCitation>
