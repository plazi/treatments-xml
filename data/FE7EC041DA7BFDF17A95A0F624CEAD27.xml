</taxonomicName>
(
<taxonomicName id="D7C41813E1F0D2A2369E9448305EE7FD" authorityName="Whetzel ex Whetzel" authorityYear="1945" class="Leotiomycetes" family="Sclerotiniaceae" kingdom="Fungi" order="Helotiales" phylum="Ascomycota" rank="family">Sclerotiniaceae</taxonomicName>
