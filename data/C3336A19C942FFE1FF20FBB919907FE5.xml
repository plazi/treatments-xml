<subSubSection id="03808884C942FFE0FF7EFB0A1C3879E3" box="[201,1347,1215,1240]" pageId="2" pageNumber="37" type="etymology">
<paragraph id="4B25DB0FC942FFE0FF7EFB0A1C3879E3" blockId="2.[151,1436,1142,2037]" box="[201,1347,1215,1240]" pageId="2" pageNumber="37">
<emphasis id="79EE071DC942FFE0FF7EFB0A183479E3" bold="true" box="[201,335,1215,1239]" pageId="2" pageNumber="37">Etymology.</emphasis>
