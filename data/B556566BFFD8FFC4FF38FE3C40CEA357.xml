<collectorName id="900A82ABFFD8FFC5FEABFD6A402BA5B3" box="[304,365,658,680]" pageId="18" pageNumber="1371">Berlin</collectorName>
), 
<collectionCode id="5BEE7FB8FFD8FFC5FEE7FD6A40E2A5BC" box="[380,420,658,679]" pageId="18" pageNumber="1371">PTT</collectionCode>
in cPütz, cTang (
<collectionCode id="5BEE7FB8FFD8FFC5FDDFFD6A43C0A5B3" box="[580,646,658,680]" pageId="18" pageNumber="1371">SHNU</collectionCode>
), cPuthz und in der 
<collectionCode id="5BEE7FB8FFD8FFC5FCA0FD6A422BA5BC" box="[827,877,658,679]" country="Canada" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="18" pageNumber="1371">CNC</collectionCode>
