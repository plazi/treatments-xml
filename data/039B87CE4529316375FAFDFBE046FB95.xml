<taxonomicName id="4C324D5B452B316375FAFCB7E377FC05" authority="Petrescu" authorityName="Petrescu" authorityYear="2004" box="[151,563,871,896]" class="Malacostraca" family="Nannastacidae" genus="Cumella" kingdom="Animalia" order="Cumacea" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="spinifera">
