<collectionCode id="ED311DCCFFC6C91FFBCC7152FB906A3D" box="[1059,1147,1840,1866]" httpUri="http://grbio.org/cool/90vz-swjg" name="Museu Paraense Emilio Goeldi" pageId="2" pageNumber="61">MPEG</collectionCode>
