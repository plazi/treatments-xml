<taxonomicName id="4C20FE8AFFC7C91EFE9B7290FDA7687E" box="[372,588,1266,1289]" class="Insecta" family="Syrphidae" genus="Ornidia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="species" species="therezinhae" status="sp. n.">
