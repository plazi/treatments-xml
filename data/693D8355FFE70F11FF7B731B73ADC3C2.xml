<paragraph id="E12B3243FFE70F11FD597171709BC60C" blockId="15.[662,1188,211,1753]" lastBlockId="18.[128,654,210,1259]" lastPageId="18" lastPageNumber="19" pageId="15" pageNumber="16">
<emphasis id="D3E0EE51FFE70F0CFD5971717286C66C" box="[688,933,301,325]" italics="true" pageId="15" pageNumber="16">Brachypterous female</emphasis>
