Width 
<quantity id="266C9FA6FFE70F0CFC4471DA7573C6B7" box="[941,1104,390,414]" metricMagnitude="-4" metricUnit="m" metricValue="5.85" metricValueMax="5.8999999999999995" metricValueMin="5.8" pageId="15" pageNumber="16" unit="mm" value="0.585" valueMax="0.59" valueMin="0.58">0.58–0.59 mm</quantity>
, vertex 
<quantity id="266C9FA6FFE70F0CFD7F71F8721BC695" box="[662,824,420,444]" metricMagnitude="-4" metricUnit="m" metricValue="3.4499999999999997" metricValueMax="3.5" metricValueMin="3.4" pageId="15" pageNumber="16" unit="mm" value="0.345" valueMax="0.35" valueMin="0.34">0.34–0.35 mm</quantity>
. 
