<taxonomicName id="63864D49FFEEFFA9FC6DB0124B9FFD1A" authority="Ausserer" authorityName="Ausserer" authorityYear="1871" class="Arachnida" family="Theraphosidae" genus="Chaetopelma" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="398" phylum="Arthropoda" rank="genus">
