<taxonomicName id="63864D49FFEEFFA9FE63B0D2492EFD3A" authorityName="Ausserer" authorityYear="1871" box="[432,577,654,678]" class="Arachnida" family="Theraphosidae" genus="Chaetopelma" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="398" phylum="Arthropoda" rank="genus">
