(
<bibRefCitation id="EFD3BAEFD665FFED316890DCFD38A2BE" author="Marcus" box="[530,687,1461,1487]" pageId="1" pageNumber="58" refString="Marcus, E. (1952) Turbellaria Brasileiros (10). Boletins da Faculdade de Filosofia, Ciencias e Letras da Universidade de Sao Paulo. Zoologia, 17, 5 - 187." type="journal article" year="1952">
Marcus 
<number id="3D8AD539D665FFED310990DCFD38A2BE" box="[627,687,1461,1487]" pageId="1" pageNumber="52" value="1952.0">1952</number>
</bibRefCitation>
) and 
<collectingCountry id="F355878ED665FFED318B90DCFCF2A2BE" box="[753,869,1461,1487]" name="Colombia" pageId="1" pageNumber="52">Colombia</collectingCountry>
(
<bibRefCitation id="EFD3BAEFD665FFED300E90DCFBC8A2BE" author="Quiroga" box="[884,1119,1461,1487]" pageId="1" pageNumber="58" refString="Quiroga, S. Y., Bolanos, D. M. &amp; Litvaitis, M. K. (2004) A checklist of polyclad flatworms (Platyhelminthes: Polycladida) from the Caribbean coast of Colombia, South America. Zootaxa, 633, 1 - 12." type="journal article" year="2004">
