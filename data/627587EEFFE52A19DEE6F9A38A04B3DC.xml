<taxonomicName id="2DDC4D7BFFE02A19DC5AFDDB898FB228" box="[555,696,549,572]" class="Insecta" family="Formicidae" genus="Heteroponera" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="179" phylum="Arthropoda" rank="species" species="ecarinata">
