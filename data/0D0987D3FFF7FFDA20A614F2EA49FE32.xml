<date id="F11E1005FFF7FFD4255B139FECE3FA6C" box="[1386,1418,1473,1497]" pageId="23" pageNumber="24">
x 
<number id="336824E2FFF7FFD425B1139CECE3FA6C" box="[1408,1418,1473,1497]" pageId="23" pageNumber="24" value="1.0">1</number>
