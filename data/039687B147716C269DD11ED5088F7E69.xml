, female. 
<emphasis id="B94BEAB547726C279FA419360BFF7DF5" bold="true" box="[738,756,1908,1932]" pageId="14" pageNumber="15">A</emphasis>
, fore wing; 
<emphasis id="B94BEAB547726C279E2B19370A757DF4" bold="true" box="[877,894,1909,1933]" pageId="14" pageNumber="15">B</emphasis>
, mesopleuron; 
<emphasis id="B94BEAB547726C27995F19360D207DF5" bold="true" box="[1049,1067,1908,1932]" pageId="14" pageNumber="15">C</emphasis>
, propodeum; 
<emphasis id="B94BEAB547726C2799F019370DC37DF4" bold="true" box="[1206,1224,1909,1933]" pageId="14" pageNumber="15">D</emphasis>
, head, anterior view; 
<emphasis id="B94BEAB547726C279DD119DB09A37DC8" bold="true" box="[151,168,1945,1969]" pageId="14" pageNumber="15">E</emphasis>
, head, dorsal view; 
<emphasis id="B94BEAB547726C279C3219DB08887DC8" bold="true" box="[372,387,1945,1969]" pageId="14" pageNumber="15">F</emphasis>
, mesoscutum; 
<emphasis id="B94BEAB547726C279F5A19DA0B3B7DC9" bold="true" box="[540,560,1944,1968]" pageId="14" pageNumber="15">G</emphasis>
