<collectingCountry id="D1B477FB975DFFB2FEEBFD307B84FC87" box="[359,430,746,771]" name="Brazil" pageId="4" pageNumber="299">Brazil</collectingCountry>
, 
<collectingCountry id="D1B477FB975DFFB2FE35FD307832FC87" box="[441,536,746,771]" name="Ecuador" pageId="4" pageNumber="299">Ecuador</collectingCountry>
, 
