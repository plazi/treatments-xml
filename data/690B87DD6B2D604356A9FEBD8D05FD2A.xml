Ex 
<taxonomicName id="26A24D486B2D6043578EFE658A33FE2E" authority="(Wied, 1830)" authorityName="Wied" authorityYear="1830" baseAuthorityName="Wied" baseAuthorityYear="1830" box="[432,784,438,463]" class="Aves" family="Emberizidae" genus="Tiaris" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="174" phylum="Chordata" rank="species" species="fuliginosus">
