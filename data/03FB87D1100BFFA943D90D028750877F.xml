(HEL), 
<collectingCountry id="F3457657100CFFA947F50CE4810F80B7" box="[1217,1352,953,980]" name="France" pageId="28" pageNumber="1505">Frankreich</collectingCountry>
(EDI), 
<collectingCountry id="F3457657100CFFA942730C80858E809B" box="[327,457,989,1016]" name="Austria" pageId="28" pageNumber="1505">Österreich</collectingCountry>
(KZW, LINZ), 
<collectingCountry id="F3457657100CFFA941AF0C8086A5809B" box="[667,738,989,1016]" name="Poland" pageId="28" pageNumber="1505">Polen</collectingCountry>
(EDI, SAW), 
<collectingCountry id="F3457657100CFFA940940C808055809B" box="[928,1042,989,1016]" name="Russia" pageId="28" pageNumber="1505">Russland</collectingCountry>
(KR), 
<collectingCountry id="F3457657100CFFA947440C8080B7809B" box="[1136,1264,989,1016]" name="Sweden" pageId="28" pageNumber="1505">Schweden</collectingCountry>
