<subSubSection id="D5C406141A2AA6CD4AF6728E37250F35" pageId="0" pageNumber="57" type="reference_group">
<paragraph id="7674BA70367DBF3B3335A09792500553" pageId="0" pageNumber="57">
<taxonomicName id="8265CF41F2B94A563954AABBB11581C5" class="Insecta" family="Ichneumonidae" genus="Epimecis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Epimecis" order="Hymenoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="genus">Epimecis</taxonomicName>
<normalizedToken id="99D0EBD164E3AF88411E63DDD6F5BE74" originalValue="Brullé">Brulle</normalizedToken>
