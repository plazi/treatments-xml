<taxonomicName id="4C6D4D5EEFC23AA6FB60FC75FAFBFC14" baseAuthorityName="Stimpson" baseAuthorityYear="1852" box="[1189,1342,955,981]" class="Ascidiacea" family="Styelidae" genus="Styela" kingdom="Animalia" order="Pleurogona" pageId="147" pageNumber="148" phylum="Chordata" rank="species" species="partita">
