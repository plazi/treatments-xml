<taxonomicName id="EAC697D9BD3FFF9A954C335C73EFFBC8" box="[339,407,1068,1091]" class="Magnoliopsida" family="Malvaceae" genus="Malva" kingdom="Plantae" order="Malvales" pageId="5" pageNumber="162" phylum="Tracheophyta" rank="genus">
