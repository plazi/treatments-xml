<taxonomicName id="B96F42AAF8DDED67B3B2D0BCCE38DC24" class="Hydrozoa" family="Stylasteridae" genus="Stylaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stylaster stejnegeri" order="Anthoathecata" pageId="31" pageNumber="32" phylum="Cnidaria" rank="species" species="stejnegeri">Stylaster stejnegeri</taxonomicName>
