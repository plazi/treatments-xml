<taxonomicName id="4C3D4D6B4376FFFCCCA2F9F91CAFBA53" authority="Ball, 1927" authorityName="Ball" authorityYear="1927" class="Insecta" family="Cicadellidae" genus="Carneocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="500" phylum="Arthropoda" rank="species" species="balli">
