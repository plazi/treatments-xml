<number id="3DF524CF4376FFFCCD7FFA3E1925BA3E" box="[1330,1344,1528,1552]" pageId="10" pageNumber="500" value="3.0">3</number>
<number id="3DF524CF4376FFFCCD07FA3E1903BA3E" box="[1354,1382,1528,1552]" pageId="10" pageNumber="500" value="20.0">20</number>
