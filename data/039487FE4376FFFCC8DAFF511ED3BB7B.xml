<number id="3DF524CF4376FFFCCB65F9861F05BA76" box="[808,864,1600,1624]" pageId="10" pageNumber="500" value="1947.0">1947</number>
(
<collectingCountry id="F32A76784376FFFCCB3FF9861FDDBA76" box="[882,952,1600,1624]" name="Oman" pageId="10" pageNumber="500">Oman</collectingCountry>
<number id="3DF524CF4376FFFCCB8FF9861F9BBA76" box="[962,1022,1600,1624]" pageId="10" pageNumber="500" value="1947.0">1947</number>
