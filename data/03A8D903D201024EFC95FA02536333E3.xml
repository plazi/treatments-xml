<taxonomicName id="4C011396D201024FFC95F9E9553D352B" box="[828,1087,1565,1589]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="14" pageNumber="2796" phylum="Firmicutes" rank="species" species="gigeriorum">
