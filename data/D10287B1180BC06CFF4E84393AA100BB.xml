<taxonomicName id="9EAB4D24180DC06DFD4686A73A8507A4" authority="A. Cunn." authorityName="A. Cunn." box="[671,1017,1969,1995]" class="Magnoliopsida" family="Rubiaceae" genus="Coprosma" kingdom="Plantae" order="Gentianales" pageId="28" pageNumber="329" phylum="Tracheophyta" rank="species" species="propinqua">
