</paragraph>
<paragraph id="2F7736D4BE6CE672FF96FC9BEDABFC54" blockId="12.[151,1436,782,1095]" pageId="12" pageNumber="235">
<emphasis id="1DBCEAC6BE6CE672FF96FC9BEE71FC8C" bold="true" box="[199,337,818,843]" pageId="12" pageNumber="235">
<typeStatus id="F0738876BE6CE672FF96FC9BEE22FC8C" box="[199,258,818,843]" pageId="12" pageNumber="235">Type</typeStatus>
