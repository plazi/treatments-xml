<taxonomicName id="42BFB3893E34FFC9FC3BFB5AA4E1B16C" authorityName="Sharp" authorityYear="1882" box="[935,1166,1263,1288]" class="Insecta" family="Zopheridae" genus="Chorasus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="subcaecus">Chorasus subcaecus</taxonomicName>
