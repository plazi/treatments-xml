<number id="0A50925BFFAEFF98FF50C087FF5CF959" box="[151,164,1704,1727]" pageId="31" pageNumber="32" value="2.0">2</number>
Preepisternum 
<number id="0A50925BFFAEFF98FEABC087FE81F959" box="[364,377,1704,1727]" pageId="31" pageNumber="32" value="2.0">2</number>
