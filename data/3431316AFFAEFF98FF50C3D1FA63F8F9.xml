<paragraph id="BC27807CFFAEFF98FF50C0C7FA63F8F9" blockId="31.[151,1436,1608,1823]" pageId="31" pageNumber="32">
<keyLead id="07692349FFAEFF98FF50C0C7FA63F8F9" pageId="31" pageNumber="32">
- Preepisternum 
<number id="0A50925BFFAEFF98FEAFC0C7FE8DF919" box="[360,373,1768,1791]" pageId="31" pageNumber="32" value="2.0">2</number>
