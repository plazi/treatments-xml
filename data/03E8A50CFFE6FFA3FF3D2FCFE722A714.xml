</taxonomicName>
[
<emphasis id="B935C808FFE5FFA3FDFF2D87E6E4A715" box="[597,672,1279,1304]" italics="true" pageId="2" pageNumber="25">comus</emphasis>
] and 
<taxonomicName id="4C416F99FFE5FFA3FD492D87E71BA715" authorityName="Fabricius" authorityYear="1775" box="[739,863,1279,1304]" class="Insecta" family="Melyridae" genus="Malachius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="25" phylum="Arthropoda" rank="genus">
