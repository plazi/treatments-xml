</emphasis>
<taxonomicName id="4C416F99FFE6FFA0FEE92E79E7D0A516" authority="(Abeille de Perrin, 1885)" baseAuthorityName="Abeille de Perrin" baseAuthorityYear="1885" box="[323,916,1793,1819]" class="Insecta" family="Melyridae" genus="Clanoptilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="24" phylum="Arthropoda" rank="species" species="strangulatus">
