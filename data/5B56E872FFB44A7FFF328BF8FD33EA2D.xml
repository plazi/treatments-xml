<taxonomicName id="14FF22E7FFB44A7FFC258F0BFBD7EB20" authorityName="Wilson" authorityYear="2003" box="[896,1059,1122,1147]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="glomericeps">
