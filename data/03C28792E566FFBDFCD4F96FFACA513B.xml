<taxonomicName id="4C6B4D07E561FFBCFC1CFA63FC5352C1" baseAuthorityName="BONHOTE" baseAuthorityYear="1905" box="[912,1009,1513,1535]" class="Mammalia" family="Muridae" genus="Niviventer" kingdom="Animalia" order="Rodentia" pageId="12" phylum="Chordata" rank="species" species="huang">
