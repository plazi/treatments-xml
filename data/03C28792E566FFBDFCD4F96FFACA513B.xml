<taxonomicName id="4C6B4D07E561FFBCFB35FA63FA9852C1" authorityName="Ge &amp; Feijó &amp; Abramov &amp; Wen &amp; Liu &amp; Cheng &amp; Xia &amp; Lu &amp; Yang" authorityYear="2021" baseAuthorityName="Ge &amp; Feijó &amp; Abramov &amp; Wen &amp; Liu &amp; Cheng &amp; Xia &amp; Lu &amp; Yang" baseAuthorityYear="2021" box="[1209,1338,1513,1535]" class="Mammalia" family="Muridae" genus="Niviventer" kingdom="Animalia" order="Rodentia" pageId="12" phylum="Chordata" rank="species" species="mekongis">
