<quantity id="6E479B60FFE2FA0009C9FDAED635FD40" box="[1004,1094,625,651]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="3" pageNumber="37" unit="mm" value="0.5">0.5 mm</quantity>
