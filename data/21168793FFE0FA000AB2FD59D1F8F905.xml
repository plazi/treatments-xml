<quantity id="6E479B60FFE2FA0009F8FD45D64BFD7F" box="[989,1080,666,692]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="3" pageNumber="37" unit="mm" value="1.5">1.5 mm</quantity>
