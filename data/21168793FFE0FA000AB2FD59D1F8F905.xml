<taxonomicName id="6EBF4D06FFE2FA000904FAEED6ECFA80" authority="Sasakawa" authorityName="Sasakawa" box="[801,1183,1329,1355]" class="Insecta" family="Lauxaniidae" genus="Noonamyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="37" phylum="Arthropoda" rank="species" species="euphlebia">
