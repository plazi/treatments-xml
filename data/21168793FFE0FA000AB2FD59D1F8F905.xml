<number id="1F7724A2FFE2FA000B03FD1BD343FD15" box="[294,304,708,734]" pageId="3" pageNumber="37" value="1.0">1</number>
/ 
<number id="1F7724A2FFE2FA000B1EFD1BD33FFD15" box="[315,332,708,734]" pageId="3" pageNumber="37" value="4.0">4</number>
