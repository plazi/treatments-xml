<quantity id="6E479B60FFE2FA000E74FD45D6C3FD7F" box="[1105,1200,666,692]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="3" pageNumber="37" unit="mm" value="0.9">0.9 mm</quantity>
; ultimate section of CuA 
<number id="1F7724A2FFE2FA000AEBFD0BD2A5FD29" box="[206,214,724,738]" pageId="3" pageNumber="37" value="1.0">1</number>
