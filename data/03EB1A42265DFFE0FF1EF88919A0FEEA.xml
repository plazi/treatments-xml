<taxonomicName id="4C42D0D7265EFFE0FDC6FEA71E46FEEA" authorityName="Reichenow" authorityYear="1893" box="[591,890,259,285]" class="Aves" family="Platysteiridae" genus="Pachyprora" kingdom="Animalia" order="Passeriformes" pageId="6" pageNumber="255" phylum="Chordata" rank="subSpecies" species="molitor" subSpecies="puella">
