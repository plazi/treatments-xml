<taxonomicName id="139E6DA0FFF0BA6BFD9EB79CF48ED1E4" authority="Pfeiffer, 1855" authorityName="Pfeiffer" authorityYear="1855" box="[545,920,223,248]" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="10" pageNumber="11" phylum="Mollusca" rank="species" species="plumbeus">
<emphasis id="E6EACA31FFF0BA6BFD9EB79CF5E7D1EB" box="[545,753,224,248]" italics="true" pageId="10" pageNumber="11">Bulimus plumbeus</emphasis>
