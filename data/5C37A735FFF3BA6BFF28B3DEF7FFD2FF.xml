Pfeiffer, 
<number id="62560404FFF0BA6BFD3BB7EBF5AAD1BC" box="[644,700,151,175]" pageId="10" pageNumber="11" value="1848.0">1848</number>
</taxonomicName>
and 
<taxonomicName id="139E6DA0FFF0BA6BFD4EB7E4F324D1BC" authority="Reeve, 1848" authorityName="Reeve" authorityYear="1848" box="[753,1074,151,176]" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="10" pageNumber="11" phylum="Mollusca" rank="species" species="lindeni">
<emphasis id="E6EACA31FFF0BA6BFD4EB7E4F4B7D1A3" box="[753,929,152,176]" italics="true" pageId="10" pageNumber="11">Bulimus lindeni</emphasis>
<bibRefCitation id="B00F6BD2FFF0BA6BFC18B7E4F324D1BC" author="Reeve" box="[935,1074,151,175]" pageId="10" pageNumber="11" refString="Reeve, L. A. (1848 - 1850) Conchologia iconica: or illustrations of the shells of molluscous animals, 5. Monograph of the genus Bulimus. Reeve, Benham, and Reeve, London, 89 pls + legend., i - xi." type="journal volume" year="1848">
Reeve, 
<number id="62560404FFF0BA6BFC45B7EBF324D1BC" box="[1018,1074,151,175]" pageId="10" pageNumber="11" value="1848.0">1848</number>
</bibRefCitation>
</taxonomicName>
