<taxonomicName id="139E6DA0FFF0BA6BFE80B7E4F5AAD1BC" authority="Pfeiffer, 1848" authorityName="Pfeiffer" authorityYear="1848" box="[319,700,151,176]" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="10" pageNumber="11" phylum="Mollusca" rank="species" species="leptochilus">
