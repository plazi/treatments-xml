<taxonomicName id="1DB20EF0E80BFFC7FED4FB492935FAD7" authority="(Thunberg, 1815)" baseAuthorityName="Thunberg" baseAuthorityYear="1815" box="[382,890,1256,1280]" class="Insecta" family="Tetrigidae" genus="Phyllotettix" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="species" species="compressus">
