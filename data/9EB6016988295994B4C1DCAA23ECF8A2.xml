<taxonomicName id="F1AB811579CF8A0A5984D4027C9A4D3D" class="Insecta" family="Cixiidae" genus="Oecleus" kingdom="Animalia" lsidName="Oecleus cucullatus" order="Hemiptera" pageId="0" pageNumber="75303" phylum="Arthropoda" rank="species" species="cucullatus">
