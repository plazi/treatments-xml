<quantity id="4CFE9B6E199CFF5B68C9A1FB73BEFE22" box="[767,914,412,438]" metricMagnitude="2" metricUnit="m" metricValue="9.5" metricValueMax="10.0" metricValueMin="9.0" pageId="188" pageNumber="189" unit="m" value="950.0" valueMax="1000.0" valueMin="900.0">900–1000 m</quantity>
