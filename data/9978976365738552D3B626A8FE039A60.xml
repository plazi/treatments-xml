<caption id="45AE76FD6572855CD18D239EFE609893" ID-DOI="http://doi.org/10.5281/zenodo.3822856" ID-Zenodo-Dep="3822856" httpUri="https://zenodo.org/record/3822856/files/figure.png" pageId="39" pageNumber="40" startId="39.[189,232,1719,1745]" targetBox="[271,1316,265,1679]" targetPageId="39">
<paragraph id="116E26756572855CD18D239EFE609893" blockId="39.[189,1399,1719,2027]" pageId="39" pageNumber="40">
