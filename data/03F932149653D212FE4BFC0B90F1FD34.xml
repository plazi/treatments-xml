<quantity id="4CA82EE79652D217FBAEFB4B9788FB7C" box="[1059,1166,1166,1190]" metricMagnitude="-4" metricUnit="m" metricValue="1.1" pageId="43" pageNumber="2301" unit="mm" value="0.11">0.11 mm</quantity>
; autozooid width 0.73 ± 
<quantity id="4CA82EE79652D217FD8CFB6B916AFB1C" box="[513,620,1198,1222]" metricMagnitude="-5" metricUnit="m" metricValue="6.0" pageId="43" pageNumber="2301" unit="mm" value="0.06">0.06 mm</quantity>
; orifice length 0.20 ± 
<quantity id="4CA82EE79652D217FC0FFB6B90EBFB1C" box="[898,1005,1198,1222]" metricMagnitude="-5" metricUnit="m" metricValue="1.0" pageId="43" pageNumber="2301" unit="mm" value="0.01">0.01 mm</quantity>
; orifice width 0.20 ± 
<quantity id="4CA82EE79652D217FEABFB0B928DFB3C" box="[294,395,1230,1254]" metricMagnitude="-5" metricUnit="m" metricValue="1.0" pageId="43" pageNumber="2301" unit="mm" value="0.01">0.01 mm</quantity>
