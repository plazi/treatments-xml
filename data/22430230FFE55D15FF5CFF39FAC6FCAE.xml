<taxonomicName id="6DEAC8A5FFE55D15FF08FF3AFA9EFF02" authority="Agassiz, 2019" authorityName="Agassiz" authorityYear="2019" box="[195,470,153,180]" class="Insecta" family="Yponomeutidae" genus="Yponomeuta" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="gnophera" status="sp. nov.">
<emphasis id="989E6F34FFE55D15FF08FF3AFA9EFF02" bold="true" box="[195,470,153,180]" italics="true" pageId="24" pageNumber="25">Yponomeuta gnophera</emphasis>
</taxonomicName>
<taxonomicNameLabel id="83ADD24FFFE55D15FE16FF3AF972FF02" box="[477,570,154,180]" pageId="24" pageNumber="25" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
