<taxonomicName id="4C106195FFC2B659FED27E495684102C" authority="De Man, 1887" authorityName="De Man" authorityYear="1887" box="[305,641,1560,1583]" class="Malacostraca" family="Sesarmidae" genus="Sesarma" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="brockii">
