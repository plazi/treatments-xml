<taxonomicName id="E3A7FE88FF9A836AFDA85F64FC9CF8E7" box="[620,780,1796,1820]" class="Magnoliopsida" family="Brassicaceae" genus="Erysimum" kingdom="Plantae" order="Brassicales" pageId="39" pageNumber="39" phylum="Tracheophyta" rank="species" species="blancheanum">
