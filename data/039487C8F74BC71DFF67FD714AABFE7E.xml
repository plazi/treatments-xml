; n = 
<number id="3DF524F9F74BC71FFBDCF8A84A3EF8DF" box="[1068,1083,1806,1830]" pageId="8" pageNumber="9" value="6.0">6</number>
) long, inner 
<number id="3DF524F9F74BC71FFB3DF8A84AECF8DF" box="[1229,1257,1806,1830]" pageId="8" pageNumber="9" value="18.0">18</number>
