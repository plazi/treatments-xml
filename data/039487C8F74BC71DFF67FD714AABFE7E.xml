<taxonomicName id="4C3D4D5DF749C71DFDCBFEEB4CA8FE9D" box="[571,685,332,356]" class="Monogenea" family="Diplectanidae" genus="Darwinoplectanum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dactylogyridea" pageId="10" pageNumber="11" phylum="Platyhelminthes" rank="species" species="pillitae" status="n. gen.">
