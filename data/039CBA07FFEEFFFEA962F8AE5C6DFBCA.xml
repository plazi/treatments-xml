<taxonomicName id="4C357092FFECFFFEAAB2FCBD5A61FCD7" authorityName="Roth" authorityYear="1934" box="[402,568,852,877]" class="Ostracoda" family="Bythocytheridae" genus="Monoceratina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Podocopida" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
