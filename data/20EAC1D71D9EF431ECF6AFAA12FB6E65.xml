88. - 
<taxonomicName id="70FF7F8E0FC5CC4254834B853AF6E678" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230257" genus="Macromichoides" lsidName="Uploaded - Macromichoides aculeatus" lsidName-HNS="Uploaded - Macromichoides aculeatus" pageNumber="207" rank="species" species="aculeatus">Macromichoides aculeatus Mayr</taxonomicName>
