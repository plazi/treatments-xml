. 
<taxonomicName authority="Abeille de Perrin" authorityName="Abeille de Perrin" authorityYear="1897" box="[451,877,524,550]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="araxicola">
