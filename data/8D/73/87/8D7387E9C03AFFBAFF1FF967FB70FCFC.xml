<taxonomicName authorityName="Zhu &amp; Sha" authorityYear="1986" box="[602,717,261,284]" class="Arachnida" family="Linyphiidae" genus="Neriene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="compta">
