</taxonomicName>
, 
<taxonomicName authority="Bouillon 1985" authorityName="Bouillon" authorityYear="1985" box="[755,1047,1767,1791]" class="Hydrozoa" family="Hydractiniidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecata" pageId="7" pageNumber="8" phylum="Cnidaria" rank="family">
