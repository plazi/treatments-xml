and 
<bibRefCitation author="Bouillon" box="[198,359,1803,1827]" pageId="7" pageNumber="8" refString="Bouillon, J. (1999) Hydromedusae. In: Boltovskoy, D. (Ed) South Atlantic Zooplankton. Backhuys Publishers, Leiden, pp. 424 - 512." type="book chapter" year="1999">
Bouillon 
<number box="[303,359,1803,1827]" pageId="7" pageNumber="8" value="1999.0">1999</number>
</bibRefCitation>
as 
<taxonomicName box="[396,639,1803,1827]" class="Hydrozoa" family="Hydractiniidae" genus="Hydractinia" kingdom="Animalia" order="Anthoathecata" pageId="7" pageNumber="8" phylum="Cnidaria" rank="species" species="tournieri">
<emphasis box="[396,639,1803,1827]" italics="true" pageId="7" pageNumber="8">Hydractinia tournieri</emphasis>
