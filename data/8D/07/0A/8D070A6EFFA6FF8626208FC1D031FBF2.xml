<paragraph blockId="12.[264,1324,767,1113]" box="[264,387,767,792]" pageId="12" pageNumber="13">
<taxonomicName box="[264,387,767,792]" class="Insecta" family="Phlaeothripidae" genus="Comments" kingdom="Animalia" order="Thysanoptera" pageId="12" pageNumber="3" phylum="Arthropoda" rank="genus">
