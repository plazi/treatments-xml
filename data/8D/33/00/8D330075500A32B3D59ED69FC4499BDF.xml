<taxonomicName authority="Lucas, 1846" authorityName="Lucas" authorityYear="1846" class="Insecta" family="Tenebrionidae" genus="Cataphronetis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cataphronetis" order="Coleoptera" pageId="188" pageNumber="189" phylum="Arthropoda" rank="genus">Cataphronetis</taxonomicName>
