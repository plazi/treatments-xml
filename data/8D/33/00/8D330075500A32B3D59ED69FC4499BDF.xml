<taxonomicName authority="Germar, 1836" authorityName="Germar" authorityYear="1836" class="Insecta" family="Tenebrionidae" genus="Phtora" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Phtora" order="Coleoptera" pageId="188" pageNumber="189" phylum="Arthropoda" rank="genus">Phtora</taxonomicName>
