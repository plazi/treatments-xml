<emphasis box="[630,910,363,387]" italics="true" pageId="2" pageNumber="245">Cyclorhipidion bodoanum</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName=": Knizek" authorityYear="2011" box="[924,1042,363,387]" class="Insecta" family="Curculionidae" genus="Cyclorhipidion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="245" phylum="Arthropoda" rank="species" species="fukiense">
