<subSubSection box="[312,965,644,670]" pageId="3" pageNumber="50" type="etymology">
<paragraph blockId="3.[264,1323,284,950]" box="[312,965,644,670]" pageId="3" pageNumber="50">
<emphasis bold="true" box="[312,452,644,670]" pageId="3" pageNumber="50">Etymology.</emphasis>
