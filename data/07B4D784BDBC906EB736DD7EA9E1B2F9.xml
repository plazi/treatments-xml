b 
<taxonomicName id="AFB319E315B58DE88A455555ECE126BD" genus="T." lsidName="T. platani" pageId="1" pageNumber="2" rank="species" species="platani">T. platani</taxonomicName>
c 
<taxonomicName id="CB097FC0EE631785488228EE25D33A06" genus="T." lsidName="T. saltans" pageId="1" pageNumber="2" rank="species" species="saltans">T. saltans</taxonomicName>
d 
<taxonomicName id="3D6D1186C32B325085C9FE16EB499212" genus="T." lsidName="T. nevskyi" pageId="1" pageNumber="2" rank="species" species="nevskyi">T. nevskyi</taxonomicName>
