<taxonomicName id="48EB4D5CFFC6E31BFF01FCE0584D3E6A" authority="Felt 1907c: 18" authorityName="Felt" authorityPageNumber="18" authorityYear="1907" box="[151,537,770,793]" class="Insecta" family="Cecidomyiidae" genus="Rhopalomyia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="clarkei">
