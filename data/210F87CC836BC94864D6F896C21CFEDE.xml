<taxonomicName id="6EA64D59836AC94866F5FF04C418FF42" authority="Pentidotea" authorityName="Pentidotea" box="[692,855,222,248]" class="Malacostraca" family="Idoteidae" genus="Idothea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="7" pageNumber="554" phylum="Arthropoda" rank="species" species="viridis">
