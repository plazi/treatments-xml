<taxonomicName id="6EA64D59836AC9486627FF61C480FF6E" authority="(Leach, 1814)" baseAuthorityName="Leach" baseAuthorityYear="1814" box="[614,975,186,212]" class="Malacostraca" family="Mysidae" genus="Neomysis" kingdom="Animalia" order="Mysida" pageId="7" pageNumber="554" phylum="Arthropoda" rank="species" species="integer">
<emphasis id="9BD2EAC8836AC9486627FF61C467FF6E" box="[614,808,187,212]" italics="true" pageId="7" pageNumber="554">Neomysis integer</emphasis>
