<taxonomicName id="6EA64D59836AC9486024FF61C27AFF6E" baseAuthorityName="J.V.Thompson" baseAuthorityYear="1828" box="[1125,1333,186,212]" class="Malacostraca" family="Mysidae" genus="Neomysis" kingdom="Animalia" order="Mysida" pageId="7" pageNumber="554" phylum="Arthropoda" rank="species" species="vulgaris">
