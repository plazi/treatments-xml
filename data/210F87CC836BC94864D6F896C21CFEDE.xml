<taxonomicName id="6EA64D59836AC948649BFF04C56BFF43" authority="(Pallas, 1766)" baseAuthorityName="Pallas" baseAuthorityYear="1766" box="[218,548,222,249]" class="Malacostraca" family="Idoteidae" genus="Idotea" kingdom="Animalia" order="Isopoda" pageId="7" pageNumber="554" phylum="Arthropoda" rank="species" species="chelipes">
