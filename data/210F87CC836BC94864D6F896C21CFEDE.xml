;
</emphasis>
mollusca: 
<taxonomicName id="6EA64D59836AC94866E3FF4CC375FF0A" authority="(Montagu, 1803)" baseAuthorityName="Montagu" baseAuthorityYear="1803" box="[674,1082,150,176]" class="Gastropoda" family="Hydrobiidae" genus="Hydrobia" kingdom="Animalia" order="Littorinimorpha" pageId="7" pageNumber="554" phylum="Mollusca" rank="species" species="ventrosa">
