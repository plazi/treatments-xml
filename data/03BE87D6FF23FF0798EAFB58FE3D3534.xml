<quantity id="4CEF9B25FF23FF079B25FB01F9F83508" box="[856,953,1267,1293]" metricMagnitude="2" metricUnit="m" metricValue="1.7" metricValueMax="3.37" metricValueMin="0.03" pageId="178" pageNumber="179" unit="m" value="170.0" valueMax="337.0" valueMin="3.0">3–337 m</quantity>
