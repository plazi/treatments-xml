<quantity id="4CEF9B25FF23FF079ADBFAE4F8AC3534" box="[678,749,1302,1329]" metricMagnitude="2" metricUnit="m" metricValue="1.5" pageId="178" pageNumber="179" unit="m" value="150.0">150 m</quantity>
