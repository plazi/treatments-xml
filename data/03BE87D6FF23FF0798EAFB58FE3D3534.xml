<quantity id="4CEF9B25FF23FF079A86FAE5F9003537" box="[763,833,1303,1330]" metricMagnitude="2" metricUnit="m" metricValue="1.499616" pageId="178" pageNumber="179" unit="ft" value="492.0">492 ft</quantity>
