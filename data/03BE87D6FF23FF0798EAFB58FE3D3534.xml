<quantity id="4CEF9B25FF23FF079B39FB59F9CA34C0" box="[836,907,1195,1221]" metricMagnitude="-1" metricUnit="m" metricValue="2.1" pageId="178" pageNumber="179" unit="cm" value="21.0">21 cm</quantity>
