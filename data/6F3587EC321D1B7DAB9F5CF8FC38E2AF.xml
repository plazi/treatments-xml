, western 
<collectingCountry id="9F8B766A321B1B7DAF115F1FFBE2E270" box="[1049,1125,1637,1661]" name="Kenya" pageId="53" pageNumber="54">Kenya</collectingCountry>
, 
<date id="9322103A321B1B7DAF7D5F1FFA8DE273" box="[1141,1290,1637,1662]" pageId="53" pageNumber="54" value="2006-06-18">
