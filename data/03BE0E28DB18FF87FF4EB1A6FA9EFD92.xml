<subSubSection id="C30DECB5DB1AFF87FF1FB4BEFA9EFD92" pageId="22" pageNumber="23" type="etymology">
<paragraph id="8BA8BF3EDB1AFF87FF1FB4BEFB20FE22" blockId="22.[151,1436,152,578]" box="[198,1253,471,498]" pageId="22" pageNumber="23">
<emphasis id="B963632CDB1AFF87FF1FB4BEFE8EFE21" bold="true" box="[198,331,471,497]" pageId="22" pageNumber="23">Etymology</emphasis>
