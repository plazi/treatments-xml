<emphasis id="B95DEAF209050B0486ABFF6F286EFF74" bold="true" box="[151,296,226,252]" pageId="7" pageNumber="42">Etymology.</emphasis>
<taxonomicName id="4C294D6309050B04870DFF6F28C6FF73" box="[305,384,226,251]" class="Polychaeta" family="Syllidae" genus="Erseia" kingdom="Animalia" order="Phyllodocida" pageId="7" pageNumber="42" phylum="Annelida" rank="genus">
<emphasis id="B95DEAF209050B04870DFF6F28C6FF73" box="[305,384,226,251]" italics="true" pageId="7" pageNumber="42">Erseia</emphasis>
</taxonomicName>
