<taxonomicName id="9D271D20597DE885B7452FCD92AB5CF1" authority="Lacordaire, 1859" authorityName="Lacordaire" authorityYear="1859" lsidName="" pageId="179" pageNumber="180" rank="tribe" tribe="Nyctoporini">Nyctoporini Lacordaire, 1859</taxonomicName>
