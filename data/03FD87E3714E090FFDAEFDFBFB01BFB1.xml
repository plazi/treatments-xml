<emphasis id="B920EAE7714E0909FEA3FD18FDBABCE7" box="[353,628,703,727]" italics="true" pageId="51" pageNumber="2703">Material examined. 22</emphasis>
/ 
<emphasis id="B920EAE7714E0909FDBEFD18FD7FBCE7" box="[636,689,703,727]" italics="true" pageId="51" pageNumber="2703">1592</emphasis>
, 
<materialsCitation id="3B3C3CA8714E0909FD01FD18FE68BCC7" collectionCode="USNM" pageId="51" pageNumber="2703" specimenCode="USNM 1003273" specimenCount="1" typeStatus="holotype">
