<emphasis id="B920EAE7714E0909FCABFD67FC4CBCE7" box="[873,898,704,727]" italics="true" pageId="51" pageNumber="2703">ca</emphasis>
<quantity id="4CAC9B10714E0909FC4FFD18FC19BCE6" box="[909,983,703,727]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" pageId="51" pageNumber="2703" unit="mm" value="30.0">30 mm</quantity>
