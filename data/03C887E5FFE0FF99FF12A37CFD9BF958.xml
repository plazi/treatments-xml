<subSubSection id="C37B6578FFE0FF99FF12A3D2FA6FF910" pageId="11" pageNumber="212" type="materials_examined">
<paragraph id="8BDE36F3FFE0FF99FF12A3D2FD0EFB60" blockId="11.[151,1437,1229,1758]" box="[151,742,1229,1254]" pageId="11" pageNumber="212">
<emphasis id="B915EAE1FFE0FF99FF12A3D2FEDAFB60" bold="true" box="[151,306,1229,1254]" pageId="11" pageNumber="212">
<typeStatus id="54DA8851FFE0FF99FF12A3D2FF3AFB60" box="[151,210,1229,1254]" pageId="11" pageNumber="212">Type</typeStatus>
