<taxonomicName id="4C154D50FFEE823EFB5AFE3F59319584" box="[1191,1386,395,416]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="14" pageNumber="1081" phylum="Tracheophyta" rank="species" species="pacificum">Solanum pacificum</taxonomicName>
