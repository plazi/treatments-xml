<taxonomicName id="D4A0F2398EE072FE5ACAACB5B37C9DDC" class="Magnoliopsida" family="Asteraceae" genus="Achillea" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="genus">
