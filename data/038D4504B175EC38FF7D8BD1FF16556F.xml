<collectingCountry id="F333B482B175EC38FBF98A96FBB651C1" box="[1043,1092,464,487]" name="Peru" pageId="2" pageNumber="595">Peru</collectingCountry>
to 
<collectingCountry id="F333B482B175EC38FB8C8A96FB5251C1" box="[1126,1184,464,487]" name="Chile" pageId="2" pageNumber="595">Chile</collectingCountry>
(Leloup 
<number id="3DECE635B175EC38FAEB8A96FAC951C1" box="[1281,1339,464,487]" pageId="2" pageNumber="595" value="1956.0">1956</number>
