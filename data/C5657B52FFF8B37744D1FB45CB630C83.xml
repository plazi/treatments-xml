<taxonomicName id="8ACCB1C7FFF8B3774505F8BDCA030CA0" authorityName="Rathbun" authorityYear="1902" box="[323,517,1908,1932]" class="Malacostraca" family="Pandalidae" genus="Pandalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="jordani">
