<treatmentCitationGroup id="AB541197FFD9FFE2FF68FC3E6613FC36" box="[151,541,900,922]" pageId="6" pageNumber="411">
<emphasis id="B930EAABFFD9FFE2FF68FC3E6501FC36" box="[151,271,900,922]" italics="true" pageId="6" pageNumber="411">Microthrips</emphasis>
<taxonomicName id="4C444D3AFFD9FFE2FEEAFC3F6617FC36" authority="Morgan, 1913: 19" authorityName="Morgan" authorityPageNumber="19" authorityYear="1913" box="[277,537,900,922]" class="Insecta" family="Thripidae" genus="Leucothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="6" pageNumber="411" phylum="Arthropoda" rank="species" species="piercei">
