<bibRefCitation id="07D8163187AFCD0E54E3C10322B9DC06" author="Danisman, T" journalOrPublisher="Acta Zoologica Bulgarica" pageId="10" pageNumber="119" pagination="215 - 216" title="A contribution to the knowledge of Turkish jumping spider fauna (Araneae: Salticidae)." volume="64" year="2012">
<normalizedToken id="7F78B9503D2BAB315FB8CDC05D1B6E98" originalValue="Danişman">Danisman</normalizedToken>
et al. 2012
</bibRefCitation>
and 
<bibRefCitation id="94B42C6FDDC1476E209D847119B9E52F" author="Cosar, I" journalOrPublisher="Sekret" pageId="10" pageNumber="119" pagination="83 - 94" title="The jumping spiders' fauna of Kirikkale Province (Araneae, Salticidae)." volume="14" year="2014">
<normalizedToken id="52D461381E8EA16FDB782DE17A6789A7" originalValue="Coşar">Cosar</normalizedToken>
et al. 2014
</bibRefCitation>
(except 
<taxonomicName id="6CC5FE22D65A38F794C45F3CE46BDCF5" class="Arachnida" family="Salticidae" genus="Aelurillus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aelurillus luctuosus" order="Araneae" pageId="4" pageNumber="113" phylum="Arthropoda" rank="species" species="luctuosus">Aelurillus luctuosus</taxonomicName>
) kindly provided by 
<normalizedToken id="59E49FB8C797BCED562A980D4C3CD6E3" originalValue="Tarık">Tarik</normalizedToken>
<normalizedToken id="F23EADC8B0D171273F189FC63B50CE16" originalValue="Danişman">Danisman</normalizedToken>
in 2013 (
<taxonomicName id="73012423A2023ABD63C0CBE46BCBCE6B" class="Arachnida" family="Salticidae" genus="Aelurillus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aelurillus" order="Araneae" pageId="4" pageNumber="113" phylum="Arthropoda" rank="genus">Aelurillus</taxonomicName>
