<taxonomicName id="E18EF280A1049918A30538783846BE1E" class="Arachnida" family="Salticidae" genus="Aelurillus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aelurillus gershomi" order="Araneae" pageId="4" pageNumber="113" phylum="Arthropoda" rank="species" species="gershomi">Aelurillus gershomi</taxonomicName>
