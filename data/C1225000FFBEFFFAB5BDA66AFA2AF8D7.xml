Fig. 
<number id="FF43F331FFBEFFFAB4E4A42CFE44F8D7" box="[462,492,1872,1896]" pageId="31" pageNumber="32" value="12.0">
<date id="3D35C7D6FFBEFFFAB4E4A42CFE44F8D7" box="[462,492,1872,1896]" pageId="31" pageNumber="32" value="2012-01">12</date>
</number>
</figureCitation>
