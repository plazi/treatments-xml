<taxonomicName id="8E8B9A95FFBEFFFAB5BDA621FDC2FACC" authority="Schmiedeknecht 1927" authorityName="Schmiedeknecht" authorityYear="1927" box="[151,618,1373,1395]" class="Insecta" family="Ichneumonidae" genus="Homocidus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="conjungens">
