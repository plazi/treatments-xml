<taxonomicName id="7F137AD6BF0ED12E5563DC7098855B65" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181915" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole olsoni Wilson" order="Hymenoptera" pageNumber="478" phylum="Arthropoda" rank="species" species="olsoni">Pheidole olsoni</taxonomicName>
