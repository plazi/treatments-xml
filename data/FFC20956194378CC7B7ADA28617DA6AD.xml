<taxonomicName id="72B89EF91C013738886CB810E261EDED" authority="Hagen, 1853" authorityName="Hagen" authorityYear="1853" class="Insecta" family="Kalotermitidae" genus="Calotermes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Calotermes albipes" order="Blattodea" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="albipes">Calotermes albipes</taxonomicName>
