<taxonomicName id="7734D81C0135076B02C31AC1A7720177" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:235700" class="Insecta" family="Formicidae" genus="Anochetus" kingdom="Animalia" lsidName-HNS="Anochetus boltoni Fisher" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="boltoni">boltoni</taxonomicName>
