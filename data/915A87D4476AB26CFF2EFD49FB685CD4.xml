<caption id="4D8C664A476FB26CFF2EFC2FFE9E58C7" ID-DOI="http://doi.org/10.5281/zenodo.4711160" ID-Zenodo-Dep="4711160" httpUri="https://zenodo.org/record/4711160/files/figure.png" pageId="4" pageNumber="197" startId="4.[151,250,942,966]" targetBox="[177,1411,181,918]" targetPageId="4">
