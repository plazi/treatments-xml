<paragraph id="8BB04C7AFF9DE83C55E33163FB19F945" blockId="7.[151,1189,1647,1674]" box="[151,1189,1647,1674]" pageId="7" pageNumber="450">
<materialsCitation id="3B674627FF9DE83C55E33163FB1CF945" box="[151,1184,1647,1674]" collectionCode="MNHN" pageId="7" pageNumber="450" specimenCode="P1" specimenCount="1" typeStatus="holotype">
