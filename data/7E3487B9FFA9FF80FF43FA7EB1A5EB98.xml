Fabricius
</taxonomicName>
and 
<taxonomicName id="319D4D2CFFA9FF8FFD44FA7DB179EAB1" authority="Voet" authorityName="Voet" authorityYear="1806" box="[711,1034,1441,1466]" class="Insecta" family="Erotylidae" genus="Pseudochrysomela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="genus">
