<taxonomicName id="4C4293C8FF97757DFAF8F9F14839F9C1" box="[1289,1361,1612,1635]" class="Malacostraca" family="Progeryonidae" genus="Progeryon" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="310" phylum="Arthropoda" rank="species" species="mus">
