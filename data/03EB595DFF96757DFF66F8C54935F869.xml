<taxonomicName id="4C4293C8FF97757DFE33FA254F51FA0D" box="[450,569,1432,1455]" class="Malacostraca" family="Progeryonidae" genus="Progeryon" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="310" phylum="Arthropoda" rank="genus">
