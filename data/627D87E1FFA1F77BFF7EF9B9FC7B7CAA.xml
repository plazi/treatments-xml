<emphasis id="D8A0EAE5FFA1F745FF7EF9B9FCF27B34" bold="true" box="[151,802,1570,1596]" pageId="95" pageNumber="96">
<taxonomicName id="2DD44D74FFA1F745FF7EF9B9FD567B34" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,646,1570,1596]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="kala" status="new species">
<emphasis id="D8A0EAE5FFA1F745FF7EF9B9FE717B34" bold="true" box="[151,417,1570,1596]" italics="true" pageId="95" pageNumber="96">Cheumatopsyche kala</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFFA1F745FD7BF9B9FCF27B34" box="[658,802,1570,1596]" pageId="95" pageNumber="96" rank="species">new species</taxonomicNameLabel>
