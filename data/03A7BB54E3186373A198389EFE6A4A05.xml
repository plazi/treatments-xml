<location id="8ED15C99E3186373A0733F08FE244B01" LSID="urn:lsid:plazi:treatment:03A7BB54E3186373A198389EFE6A4A05:8ED15C99E3186373A0733F08FE244B01" box="[328,417,1066,1088]" county="Jahren" municipality="Fundorten" name="Bereichen" pageId="5" pageNumber="1494">Bereichen</location>
