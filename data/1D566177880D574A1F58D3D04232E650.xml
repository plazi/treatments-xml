; l = 
<quantity id="52077D84880D574A1C43D7F84151E7EF" box="[515,614,1237,1264]" metricMagnitude="-4" metricUnit="m" metricValue="1.2" pageId="23" pageNumber="600" unit="mm" value="0.12">0,12 mm</quantity>
