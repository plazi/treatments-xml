<taxonomicName id="4C750CFEFFC3D003FE00945FFDA2F897" box="[454,615,1829,1853]" class="Insecta" family="Cerambycidae" genus="Amphelictus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="65" phylum="Arthropoda" rank="species" species="fuscipennis">
