<paragraph id="6B2C3907E36CBB1C4B95E69D642DE8EC" pageId="null" pageNumber="590">
<normalizedToken id="CCCF9D9A72A3C9415949E3D6FA4C6884" originalValue="Herbst-Löwenzahn" pageId="null" pageNumber="590">Herbst-Loewenzahn</normalizedToken>
</paragraph>
