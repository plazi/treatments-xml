<taxonomicName id="D532E108EE1720F06BBBBD5708E982CA" class="Magnoliopsida" family="Asteraceae" genus="Leontodon" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="genus">Leontodon</taxonomicName>
<normalizedToken id="E0162D8DC989536CD2DD025BCFA8844B" originalValue="autumnális" pageId="null" pageNumber="590">autumnalis</normalizedToken>
<authorityName id="7C3E1C0888B0344862F6C27D8CDBB484" pageId="null" pageNumber="590">L.</authorityName>
