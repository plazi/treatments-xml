<taxonomicName id="27CA4D35FEBAFEDC59F9FA9AFCF66448" authority="Macquart, 1851" authorityName="Macquart" authorityYear="1851" box="[487,837,1302,1329]" class="Insecta" family="Tephritidae" genus="Acinia" kingdom="Animalia" order="Diptera" pageId="375" pageNumber="376" phylum="Arthropoda" rank="species" species="stellata">
