<taxonomicName id="4C454D29FF84D766FD0EFE7EFB8DFE79" baseAuthorityName="Hincks" baseAuthorityYear="1954" box="[679,1112,459,485]" class="Insecta" family="Diplatyidae" genus="Paradiplatys" kingdom="Animalia" order="Dermaptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="lamottei" subGenus="Paradiplatys">
