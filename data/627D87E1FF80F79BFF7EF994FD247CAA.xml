<heading id="B123819BFF80F764FF7EF994FC937B21" bold="true" box="[151,835,1551,1577]" fontSize="11" level="1" pageId="126" pageNumber="127" reason="1">
<emphasis id="D8A0EAE5FF80F764FF7EF994FC937B21" bold="true" box="[151,835,1551,1577]" pageId="126" pageNumber="127">
<taxonomicName id="2DD44D74FF80F764FF7EF994FD757B21" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,677,1551,1577]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="species" species="tanana" status="new species">
<emphasis id="D8A0EAE5FF80F764FF7EF994FE117B21" bold="true" box="[151,449,1551,1577]" italics="true" pageId="126" pageNumber="127">Cheumatopsyche tanana</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFF80F764FD5AF994FC937B21" box="[691,835,1551,1577]" pageId="126" pageNumber="127" rank="species">new species</taxonomicNameLabel>
