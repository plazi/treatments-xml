<taxonomicName id="4C35DBAA0965FF9D995ADB3EFCE3F99A" authority="Starega &amp; Snegovaya, 2008" authorityName="Starega &amp; Snegovaya" authorityYear="2008" box="[294,845,1618,1643]" class="Arachnida" family="Phalangiidae" genus="Homolophus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="29" pageNumber="330" phylum="Arthropoda" rank="species" species="chevrizovi">
