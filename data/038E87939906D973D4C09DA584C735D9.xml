<emphasis id="B953EA97990BD974D6459CD48217309C" box="[530,631,1952,1973]" italics="true" pageId="27" pageNumber="28">
T. 
<taxonomicName id="4C274D06990BD974D6799CD48217309C" box="[558,631,1952,1973]" pageId="27" pageNumber="28" rank="species" species="persica" status="n. sp.">persica</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26057EC990BD974D6D69CEB82DD309D" box="[641,701,1951,1972]" pageId="27" pageNumber="28" rank="species">
<emphasis id="B953EA97990BD974D6D69CEB82DD309D" bold="true" box="[641,701,1951,1972]" pageId="27" pageNumber="28">n. sp.</emphasis>
</taxonomicNameLabel>
, RMNH Coel. 38785; b, 
<taxonomicName id="4C274D06990BD974D79A9CD48455309C" box="[973,1077,1952,1973]" class="Anthozoa" family="Plexauridae" genus="Trimuricea" kingdom="Animalia" order="Alcyonacea" pageId="27" pageNumber="28" phylum="Cnidaria" rank="species" species="spinosa" status="n. sp.">
<emphasis id="B953EA97990BD974D79A9CD48455309C" box="[973,1077,1952,1973]" italics="true" pageId="27" pageNumber="28">
T. 
<taxonomicName id="4C274D06990BD974D7BF9CD48455309C" box="[1000,1077,1952,1973]" pageId="27" pageNumber="28" rank="species" species="spinosa" status="n. sp.">spinosa</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="B953EA97990BD974D0689CEB841B309D" bold="true" box="[1087,1147,1951,1972]" pageId="27" pageNumber="28">
<taxonomicNameLabel id="A26057EC990BD974D0689CEB841B309D" box="[1087,1147,1951,1972]" pageId="27" pageNumber="28" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, RMNH Coel. 39317; c, 
<taxonomicName id="4C274D06990BD974D1DE9CD4824930FA" authority="Thomson &amp; Simpson, 1909" authorityName="Thomson &amp; Simpson" authorityYear="1909" class="Anthozoa" family="Plexauridae" genus="Trimuricea" kingdom="Animalia" order="Alcyonacea" pageId="27" pageNumber="28" phylum="Cnidaria" rank="species" species="reticulata">
<taxonomicName id="4C274D06990BD974D1DE9CD4809930FA" genus="T." pageId="27" pageNumber="28" rank="species" species="reticulata">
<emphasis id="B953EA97990BD974D1DE9CD4809930FA" italics="true" pageId="27" pageNumber="28">
T. 
<taxonomicName id="4C274D06990BD974D4C09CC9809930FA" authority="Thomson &amp; Simpson, 1909" authorityName="Thomson &amp; Simpson" authorityYear="1909" box="[151,249,1981,2003]" pageId="27" pageNumber="28" rank="species" species="reticulata">reticulata</taxonomicName>
</emphasis>
</taxonomicName>
(Thomson &amp; Simpson, 1909)
</taxonomicName>
, BMNH 1933.3.13.18; d, 
<taxonomicName id="4C274D06990BD974D7659CCB83A130FA" box="[818,961,1981,2004]" class="Anthozoa" family="Plexauridae" genus="Trimuricea" kingdom="Animalia" order="Alcyonacea" pageId="27" pageNumber="28" phylum="Cnidaria" rank="species" species="tuberculosa" status="n. sp.">
<emphasis id="B953EA97990BD974D7659CCB83A130FA" box="[818,961,1981,2004]" italics="true" pageId="27" pageNumber="28">
T. 
<taxonomicName id="4C274D06990BD974D71D9CC983A130FA" box="[842,961,1981,2003]" pageId="27" pageNumber="28" rank="species" species="tuberculosa" status="n. sp.">tuberculosa</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="B953EA97990BD974D7909CCA846030FA" bold="true" box="[967,1024,1982,2003]" pageId="27" pageNumber="28">
<taxonomicNameLabel id="A26057EC990BD974D7909CCA846030FA" box="[967,1024,1982,2003]" pageId="27" pageNumber="28" rank="species">n. sp.</taxonomicNameLabel>
