<paragraph id="F8077233B2E3DB7E438F709802605B64" pageId="646" pageNumber="647">
<emphasis id="15C29BDBABA23453A8BD5C69EC59505A" bold="true" pageId="646" pageNumber="647">CAN</emphasis>
