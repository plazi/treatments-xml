<taxonomicName id="4C734D623B45FFBBFAACF8C5FED3F819" authority="Warren" authorityName="Warren" class="Insecta" family="Geometridae" genus="Abraxas" kingdom="Animalia" order="Lepidoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="flavisinuata">
