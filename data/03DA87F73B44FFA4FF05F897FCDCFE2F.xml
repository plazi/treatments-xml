<subSubSection id="C369656A3B44FFBAFF05F80FFACDF853" pageId="46" pageNumber="47" type="diagnosis">
<paragraph id="8BCC36E13B44FFBAFF05F80FFACDF853" blockId="46.[151,1437,1940,2001]" pageId="46" pageNumber="47">
<emphasis id="B907EAF33B44FFBAFF05F80FFEB6F82E" bold="true" box="[151,273,1940,1965]" pageId="46" pageNumber="47">Diagnosis.</emphasis>
