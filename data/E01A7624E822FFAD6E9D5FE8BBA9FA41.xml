and 
<collectingCountry id="10A487A2E822FFAD6F0F5E1DB854FAA6" box="[288,362,1367,1389]" name="Taiwan" pageId="14" pageNumber="1759">Taiwan</collectingCountry>
(
<bibRefCitation id="0C22BAC3E822FFAD6F575E1DBB2BFAA6" author="BORDONI A" box="[376,533,1367,1389]" pageId="14" pageNumber="1759" refId="ref19404" refString="BORDONI A. (2002): Xantholinini della Regione Orientale (Coleoptera: Staphylinidae). Classificazione, filogenesi e revisione tassonomica. - Monografie del Museo Regionale di Scienze Naturali, Torino 33: 998 pp." type="book" year="2002">BORDONI 2002</bibRefCitation>
