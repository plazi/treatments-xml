<taxonomicName id="4C204D24DF1AFF8BC1AEFDE6FDBA5CEA" authority="Leach, 1816" authorityName="Leach" authorityYear="1816" box="[238,512,626,652]" class="Branchiopoda" family="Chydoridae" genus="Chydorus" kingdom="Animalia" order="Diplostraca" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
