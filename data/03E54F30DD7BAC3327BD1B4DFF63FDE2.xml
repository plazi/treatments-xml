(
<figureCitation id="1377E2A3DD7BAC3327B51BA9FF4DFE56" box="[159,232,405,431]" captionStart="FIGURE 2" captionStartId="6.[151,250,1622,1647]" captionTargetBox="[151,1436,561,1597]" captionTargetId="figure@6.[151,1436,561,1597]" captionTargetPageId="6" captionText="FIGURE 2. Gynacantha apiaensis, male, Holotype; insert: S2, ventral; S2–3, ventral; anal appendages, dorsal." figureDoi="http://doi.org/10.5281/zenodo.3823702" httpUri="https://zenodo.org/record/3823702/files/figure.png" pageId="6" pageNumber="177">Figs 2</figureCitation>
, 
<figureCitation id="1377E2A3DD7BAC3327DE1BA9FE88FE56" box="[244,301,405,431]" captionStart="FIGURES 3–11" captionStartId="7.[151,264,1299,1324]" captionTargetBox="[151,1434,166,1268]" captionTargetId="figure@7.[151,1436,166,1274]" captionTargetPageId="7" captionText="FIGURES 3–11. Gynacantha species, male S2, ventral (to show arrangement of denticles along genital fossa): (3) G. rosenbergi; (4) G. dobsoni; (5) G. amphora; (6) G. vanuatua; (7) G. vitiana; (8) G. koroana; (9) G. stevensoni; (10) Gynacantha cf stevensoni; (11) G. apiaensis." figureDoi="http://doi.org/10.5281/zenodo.3823704" httpUri="https://zenodo.org/record/3823704/files/figure.png" pageId="6" pageNumber="177">6–11</figureCitation>
, 
<figureCitation id="1377E2A3DD7BAC3326131BA8FE27FE56" box="[313,386,404,431]" captionStart="FIGURES 12–21" captionStartId="8.[151,264,1686,1711]" captionTargetBox="[165,1421,201,1652]" captionTargetId="figure@8.[151,1436,201,1661]" captionTargetPageId="8" captionText="FIGURES 12–21. Gynacantha species, male anal appendages dorsal (to show differences between width of S10 and length of superior anal appendages): (12) G. rosenbergi; (13) G. dobsoni; (14) G. amphora; (15, 16) G. vanuatua; (17) G. vitiana; (18) G. koroana; (19) G. stevensoni; (20) G. cf stevensoni; (21) G. apiaensis." figureDoi="http://doi.org/10.5281/zenodo.3823706" httpUri="https://zenodo.org/record/3823706/files/figure.png" pageId="6" pageNumber="177">15–21</figureCitation>
, 
<figureCitation id="1377E2A3DD7BAC3326A71BA9FE0FFE56" box="[397,426,405,431]" captionStart="FIGURE 24" captionStartId="11.[151,250,1101,1126]" captionTargetBox="[197,1391,197,1076]" captionTargetId="figure@11.[197,1391,181,1076]" captionTargetPageId="11" captionText="FIGURE 24. Gynacantha vanuatua, male, Holotype; insert male: S2, ventral; anal appendages, dorsal." figureDoi="http://doi.org/10.5281/zenodo.3823712" httpUri="https://zenodo.org/record/3823712/files/figure.png" pageId="6" pageNumber="177">24</figureCitation>
