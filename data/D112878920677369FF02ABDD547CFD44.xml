<taxonomicName id="9EBB4D1C2067736AFEB0A8695355FDDD" authority="Smith, 2017" authorityName="Smith" authorityYear="2017" box="[293,537,536,558]" class="Insecta" family="Curculionidae" genus="Dinoxyleborus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="132" phylum="Arthropoda" rank="species" species="cognatoi" status="sp. nov.">
