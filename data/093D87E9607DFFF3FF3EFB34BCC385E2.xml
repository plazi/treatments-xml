<taxonomicName id="46944D7C607CFFF3FDA2FAD9BB7787BB" authority="Lin et Yang" authorityName="Lin et Yang" box="[523,770,1402,1425]" class="Insecta" family="Cerambycidae" genus="Heteroglenea" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="dolosa" status="sp. nov.">
