<taxonomicName id="F73559D05D87F0943B24350D505CBDC0" class="Hexapoda" family="Smilacaceae" genus="Smilax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Smilax havanensis" order="Hymenoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="species" species="havanensis">
