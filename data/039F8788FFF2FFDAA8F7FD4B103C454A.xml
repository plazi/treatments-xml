<taxonomicName id="4C364D1DFFF2FFDAA821FB8F108444CE" box="[309,433,1148,1174]" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="alba">
<emphasis id="B942EA8CFFF2FFDAA821FB8F108444CE" box="[309,433,1148,1174]" italics="true" pageId="18" pageNumber="19">
