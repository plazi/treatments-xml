<taxonomicName id="4C364D1DFFF2FFDAA8D4FB8F132344CE" box="[448,534,1148,1174]" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="veitchii">
