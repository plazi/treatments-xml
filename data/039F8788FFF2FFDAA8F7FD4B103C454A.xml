<taxonomicName id="4C364D1DFFF2FFDAA9A9FB8F101F44CE" box="[189,298,1148,1174]" class="Pinopsida" family="Pinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
