<quantity id="1669F2E2FF87C238561BEB306CB59B6E" box="[301,446,798,824]" metricMagnitude="-3" metricUnit="m" metricValue="5.25" metricValueMax="5.3" metricValueMin="5.2" pageId="23" pageNumber="24" unit="mm" value="5.25" valueMax="5.3" valueMin="5.2">5.2–5.3 mm</quantity>
