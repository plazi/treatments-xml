<taxonomicName id="4C30F5CF082D9422FF6F42C35F06FB48" authority="Reyes, 1994: 299" authorityName="Reyes" authorityPageNumber="299" authorityYear="1994" box="[151,630,1230,1252]" class="Insecta" family="Thripidae" genus="Trichromothrips" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="494" phylum="Arthropoda" rank="species" species="bruncurrum">
