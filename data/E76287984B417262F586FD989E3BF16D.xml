<collectingCountry id="17DC761E4B407262F40EFA839B8DF24E" box="[314,387,1302,1322]" name="Nepal" pageId="3" pageNumber="46">NEPAL</collectingCountry>
, Prov. 
<collectingRegion id="AD0FF86C4B407262F4F0FA839809F24E" box="[452,519,1302,1322]" country="Nepal" name="Karnali" pageId="3" pageNumber="46">Karnali</collectingRegion>
, Distr. Jumla: Churta E, obere Lagen. 
<geoCoordinate id="0AFF50494B407262F65CFA8399BEF24E" box="[872,944,1302,1322]" degrees="29" direction="north" minutes="09" orientation="latitude" pageId="3" pageNumber="46" precision="925" value="29.15">29°09'N</geoCoordinate>
<geoCoordinate id="0AFF50494B407262F68CFA8399F1F24E" box="[952,1023,1302,1322]" degrees="82" direction="east" minutes="31" orientation="longitude" pageId="3" pageNumber="46" precision="925" value="82.51667">82°31'E</geoCoordinate>
, 
