<taxonomicName id="59B44D448D21FFD24838BDEFFDC05ED7" box="[323,561,692,717]" class="Insecta" family="Coccinellidae" genus="Chnoodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="276" phylum="Arthropoda" rank="species" species="pentagona">
