<quantity id="4CDC9B03AC31B27CEBFE115D1903647B" box="[764,878,844,865]" metricMagnitude="3" metricUnit="m" metricValue="1.15" metricValueMax="1.2" metricValueMin="1.1" pageId="5" pageNumber="946" unit="m" value="1150.0" valueMax="1200.0" valueMin="1100.0">1100-1200m</quantity>
