<emphasis id="B952EA8CFF88FFE0DAD1F81CFF60240F" bold="true" box="[136,245,843,869]" pageId="30" pageNumber="31">
<typeStatus id="549D883CFF88FFE0DAD1F81CFF60240F" box="[136,245,843,869]" pageId="30" pageNumber="31">Holotype</typeStatus>
</emphasis>
