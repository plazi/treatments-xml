<paragraph id="BE128D77FFCCFF9F73F21BDE0FE720FD" blockId="2.[491,944,1154,1178]" box="[491,944,1154,1178]" pageId="2" pageNumber="4">
<heading id="E55A3A1BFFCCFF9F73F21BDE0FE720FD" box="[491,944,1154,1178]" fontSize="10" level="2" pageId="2" pageNumber="4" reason="4">
<taxonomicName id="79ADF6F4FFCCFF9F73F21BDE0FE720FD" authority="(Berlese, 1904)" authorityName="Palearctic" baseAuthorityName="Berlese" baseAuthorityYear="1904" box="[491,944,1154,1178]" class="Arachnida" family="Macrochelidae" genus="Macrocheles" kingdom="Animalia" order="Mesostigmata" pageId="2" pageNumber="4" phylum="Arthropoda" rank="species" species="subbadius">
