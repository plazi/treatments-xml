</taxonomicName>
<taxonomicName id="4C1C4D67AA004D45B7EFF8653BF0F837" authority="Bassett-Smith, 1898" authorityName="Bassett-Smith" authorityYear="1898" box="[384,708,1934,1956]" pageId="109" pageNumber="110" rank="species" species="platytarsis">
<taxonomicName id="4C1C4D67AA004D45B7EFF8653BB7F837" authority="Bassett-Smith" authorityName="Bassett-Smith" box="[384,643,1934,1956]" pageId="109" pageNumber="110" rank="species" species="platytarsis">
