<quantity id="DC369B24FFA9FFAE88F2436CFCD4F9D9" box="[709,786,1595,1616]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="27" unit="mm" value="0.1">0.1 mm</quantity>
