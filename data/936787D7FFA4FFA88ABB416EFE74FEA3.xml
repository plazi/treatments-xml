<quantity id="DC369B24FFA9FFAE886D436CFD60F9D9" box="[602,678,1595,1616]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="26" pageNumber="27" unit="mm" value="0.2">0.2 mm</quantity>
