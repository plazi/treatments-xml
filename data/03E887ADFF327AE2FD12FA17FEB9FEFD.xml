<taxonomicName id="4C414D38FF327AE3FD12FA17F9C0FA09" authority="Simon, 1893" authorityName="Simon" authorityYear="1893" box="[665,923,1432,1459]" class="Arachnida" family="Pholcidae" genus="Priscula" kingdom="Animalia" order="Araneae" pageId="226" pageNumber="227" phylum="Arthropoda" rank="genus">
