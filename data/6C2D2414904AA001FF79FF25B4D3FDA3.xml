<quantity id="237C38E7904AA001FE54FDC6B240FDA3" box="[442,530,573,598]" metricMagnitude="3" metricUnit="m" metricValue="1.365" pageId="37" pageNumber="488" unit="m" value="1365.0">
<elevation id="6FA97231904AA001FE54FDC6B240FDA3" box="[442,530,573,598]" metricMagnitude="3" metricUnit="m" metricValue="1.365" pageId="37" pageNumber="488" unit="m" value="1365.0">1365 m</elevation>
</quantity>
</materialsCitation>
; our samples were taken at depths of 
<quantity id="237C38E7904AA001FC58FDC6B3AFFDA3" box="[950,1021,573,598]" metricMagnitude="2" metricUnit="m" metricValue="6.529999999999999" pageId="37" pageNumber="488" unit="m" value="653.0">653 m</quantity>
and 
<specimenCount id="F2825E8B904AA001FBDDFDC5B4D3FDA3" box="[1075,1153,574,598]" pageId="37" pageNumber="488" type="generic">
