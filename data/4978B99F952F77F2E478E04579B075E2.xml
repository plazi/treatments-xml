<taxonomicName id="4CF32C017A2A39B2E53CF46DD7B10DAA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143037" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName-HNS="Polyrhachis defensus Smith" order="Hymenoptera" pageNumber="59" phylum="Arthropoda" rank="species" species="defensus">Polyrhachis defensus</taxonomicName>
