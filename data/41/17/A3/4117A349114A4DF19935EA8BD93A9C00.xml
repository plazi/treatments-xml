Schkuhr, 
<taxonomicName authorityName="Wormsk ex Drejer" authorityYear="1841" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex gynocrates" order="Poales" pageId="8" pageNumber="31" phylum="Tracheophyta" rank="species" species="gynocrates">
<emphasis italics="true" pageId="8" pageNumber="31">Carex gynocrates</emphasis>
</taxonomicName>
Wormsk. ex Drejer, 
<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex membranacea" order="Poales" pageId="8" pageNumber="31" phylum="Tracheophyta" rank="species" species="membranacea">
<emphasis italics="true" pageId="8" pageNumber="31">Carex membranacea</emphasis>
</taxonomicName>
Hook., 
<taxonomicName authorityName="Wahlenb" authorityYear="1803" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex microglochin" order="Poales" pageId="8" pageNumber="31" phylum="Tracheophyta" rank="species" species="microglochin">
