<taxonomicName authorityName="Moq" authorityYear="1849" class="Magnoliopsida" family="Phytolaccaceae" genus="Microtea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Microtea tenuifolia" order="Caryophyllales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="species" species="tenuifolia">
