<taxonomicName authority="" authorityName="" class="Insecta" family="Liviidae" genus="Yponomeutoidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Yponomeutoidea curvilineata" order="Hemiptera" pageId="4" pageNumber="73" phylum="Arthropoda" rank="species" species="curvilineata">curvilineata</taxonomicName>
