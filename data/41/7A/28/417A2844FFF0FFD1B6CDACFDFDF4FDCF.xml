<specimenCount box="[868,889,290,313]" pageId="18" pageNumber="121" type="female">♀</specimenCount>
, “ 
<collectingCountry box="[916,1009,289,314]" name="Mexico" pageId="18" pageNumber="121">México</collectingCountry>
