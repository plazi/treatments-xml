, “ 
<collectingRegion box="[943,1031,289,314]" country="India" name="Sikkim" pageId="4" pageNumber="107">Sikkim</collectingRegion>
”. 
<typeStatus box="[1057,1169,290,314]" pageId="4" pageNumber="107">Lectotype</typeStatus>
