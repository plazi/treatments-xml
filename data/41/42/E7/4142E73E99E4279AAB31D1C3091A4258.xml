<subSubSection pageId="null" pageNumber="197" type="vernacular_name">
<paragraph pageId="null" pageNumber="197">
<normalizedToken originalValue="Gefärbtes" pageId="null" pageNumber="197">Gefaerbtes</normalizedToken>
Laichkraut
</paragraph>
