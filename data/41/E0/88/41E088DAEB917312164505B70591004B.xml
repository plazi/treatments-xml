<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:183798" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus (Myrmepomis) Forel" order="Hymenoptera" pageNumber="239" phylum="Arthropoda" rank="subGenus" subGenus="Myrmepomis">Myrmepomis</taxonomicName>
