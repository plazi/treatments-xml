<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230841" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus (Myrmocamelus)" order="Hymenoptera" pageNumber="239" phylum="Arthropoda" rank="subGenus" subGenus="Myrmocamelus">Myrmocamelus</taxonomicName>
