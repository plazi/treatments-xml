<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:183792" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus (Myrmosericus) Forel" order="Hymenoptera" pageNumber="239" phylum="Arthropoda" rank="subGenus" subGenus="Myrmosericus">Myrmosericus</taxonomicName>
