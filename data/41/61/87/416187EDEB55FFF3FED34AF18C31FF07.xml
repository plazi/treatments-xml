<emphasis box="[567,936,422,464]" italics="true" pageId="12" pageNumber="117">
<taxonomicName authorityName="Metcalf" authorityYear="1946" box="[567,930,422,464]" class="Insecta" family="Tropiduchidae" genus="Swezeyaria" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="117" phylum="Arthropoda" rank="species" species="viridana" status="new species">Swezeyaria viridana</taxonomicName>
