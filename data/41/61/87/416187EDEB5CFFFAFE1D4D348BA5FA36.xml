, 
<collectingCountry box="[784,882,1788,1831]" name="India" pageId="5" pageNumber="110">India</collectingCountry>
, 
<collectingCountry box="[902,1011,1788,1831]" name="China" pageId="5" pageNumber="110">China</collectingCountry>
