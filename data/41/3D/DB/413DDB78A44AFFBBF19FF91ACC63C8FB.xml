<emphasis bold="true" box="[151,602,1711,1737]" pageId="32" pageNumber="33">
<taxonomicName authority="Thongphak &amp; Wang, 2007" authorityName="Thongphak &amp; Wang" authorityYear="2007" box="[151,493,1711,1737]" class="Insecta" family="Cerambycidae" genus="Uracanthus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="quadristriolatus" status="sp. nov.">
<emphasis bold="true" box="[151,493,1711,1737]" italics="true" pageId="32" pageNumber="33">Uracanthus quadristriolatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[508,602,1711,1737]" pageId="32" pageNumber="33" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
