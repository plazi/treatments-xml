<emphasis bold="true" box="[151,552,951,977]" pageId="35" pageNumber="36">
<taxonomicName authority="Thongphak &amp; Wang, 2007" authorityName="Thongphak &amp; Wang" authorityYear="2007" box="[151,445,951,977]" class="Insecta" family="Cerambycidae" genus="Uracanthus" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="punctulatus" status="sp. nov.">
<emphasis bold="true" box="[151,445,951,977]" italics="true" pageId="35" pageNumber="36">Uracanthus punctulatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[459,552,951,977]" pageId="35" pageNumber="36" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
