<taxonomicName class="Insecta" family="Phylliidae" genus="Phyllium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phyllium lelantos" order="Phasmida" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="lelantos">
