<taxonomicName authority="Schrottky, 1907" authorityName="Schrottky" authorityYear="1907" box="[377,668,1266,1288]" class="Insecta" family="Sphingidae" genus="Eumorpha" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="variety" species="combinata" variety="danielis">
