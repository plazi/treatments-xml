<emphasis box="[314,422,1417,1441]" italics="true" pageId="8" pageNumber="139">Microdota</emphasis>
</taxonomicName>
”. 
<taxonomicName box="[448,556,1417,1441]" class="Insecta" family="Staphylinidae" genus="Microdota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="139" phylum="Arthropoda" rank="genus">
