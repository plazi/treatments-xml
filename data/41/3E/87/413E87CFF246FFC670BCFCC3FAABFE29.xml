<subSubSection box="[199,1354,477,503]" pageId="5" pageNumber="136" type="etymology">
<paragraph blockId="5.[151,1436,153,503]" box="[199,1354,477,503]" pageId="5" pageNumber="136">
<emphasis bold="true" box="[199,334,477,503]" pageId="5" pageNumber="136">Etymology.</emphasis>
