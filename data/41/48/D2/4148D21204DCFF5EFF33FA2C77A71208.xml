<collectingCountry box="[1178,1295,1838,1864]" name="Greenland" pageId="140" pageNumber="141">Greenland</collectingCountry>
and 
<collectingCountry box="[1354,1436,1838,1864]" name="Iceland" pageId="140" pageNumber="141">Iceland</collectingCountry>
(
<bibRefCitation author="Heding" box="[159,307,1874,1900]" firstAuthor="Heding" pageId="140" pageNumber="141" pagination="1 - 39" refId="ref183688" refString="Heding, S. G. (1942) Holothurioidea, Part 2: Aspidochirota, Elasipoda, Dendrochirota. In: The Danish Ingolf Expedition. 4 (13). Hagerup, Copenhagen, pp. 1 - 39." type="book chapter" year="1942">Heding 1942</bibRefCitation>
