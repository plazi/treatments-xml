<taxonomicName authority="" authorityName="" baseAuthorityName="Lyman" baseAuthorityYear="1878" box="[503,634,441,467]" class="Ophiuroidea" family="Ophiuridae" genus="Ophiurolepis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ophiurida" pageId="48" pageNumber="49" phylum="Echinodermata" rank="species" species="inornata">
