<quantity box="[743,870,928,955]" metricMagnitude="2" metricUnit="m" metricValue="4.4" metricValueMax="5.0" metricValueMin="3.8" pageId="108" pageNumber="109" unit="m" value="440.0" valueMax="500.0" valueMin="380.0">380–500 m</quantity>
