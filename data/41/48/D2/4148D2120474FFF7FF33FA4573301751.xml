<taxonomicName authorityName="$ Koehler" authorityYear="1907" box="[1042,1261,1832,1858]" class="Ophiuroidea" family="Hemieuryalidae" genus="Ophioplus" kingdom="Animalia" order="Ophiurida" pageId="36" pageNumber="37" phylum="Echinodermata" rank="species" species="armatus">
