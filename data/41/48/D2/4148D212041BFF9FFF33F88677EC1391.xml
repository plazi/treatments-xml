(
<bibRefCitation author="Dilman" box="[479,633,332,359]" firstAuthor="Dilman" pageId="76" pageNumber="77" pagination="563 - 586" refId="ref178818" refString="Dilman, A. B. (2013) Asteroid fauna of the northern Mid-Atlantic Ridge: additional records. Marine Biology Research, 9 (5 - 6), 563 - 586. https: // doi. org / 10.1080 / 17451000.2012.749993" type="journal article" year="2013">Dilman 2013</bibRefCitation>
); 
<collectionCode box="[652,714,333,359]" pageId="76" pageNumber="77">AZO</collectionCode>
