<taxonomicName authorityName="Seamount (A. H. Clark" authorityYear="1949" box="[689,776,1803,1829]" class="Insecta" family="Curculionidae" genus="Atlantis" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="genus">
<emphasis box="[689,776,1803,1829]" italics="true" pageId="49" pageNumber="50">Atlantis</emphasis>
</taxonomicName>
