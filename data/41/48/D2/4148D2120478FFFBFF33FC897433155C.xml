<emphasis box="[356,375,1146,1171]" italics="true" pageId="40" pageNumber="41">c.</emphasis>
<geoCoordinate box="[383,482,1144,1171]" degrees="58" direction="north" minutes="30" orientation="latitude" pageId="40" pageNumber="41" precision="925" value="58.5">58°30’N</geoCoordinate>
