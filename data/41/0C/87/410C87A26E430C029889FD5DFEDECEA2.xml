<taxonomicName box="[1003,1106,845,866]" class="Liliopsida" family="Orchidaceae" genus="Coelogyne" kingdom="Plantae" order="Asparagales" pageId="31" pageNumber="125" phylum="Tracheophyta" rank="genus">
