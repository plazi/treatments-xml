<collectionCodeEnum box="[255,313,1481,1505]" pageId="9" pageNumber="981">
<collectionCodeEnum box="[255,290,1481,1505]" pageId="9" pageNumber="981">
<collectionCode box="[255,290,1481,1505]" country="Costa Rica" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15652" name="Museo Nacional de Costa Rica" pageId="9" pageNumber="981">CR</collectionCode>
