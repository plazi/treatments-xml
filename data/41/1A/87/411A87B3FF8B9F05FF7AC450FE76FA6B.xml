<collectingCountry box="[1104,1168,1434,1457]" name="Japan" pageId="25" pageNumber="26">Japan</collectingCountry>
and near 
<collectingRegion box="[1281,1391,1433,1458]" country="Japan" name="Hokkaido" pageId="25" pageNumber="26">Hokkaido</collectingRegion>
(
<bibRefCitation author="Taki, Is." pageId="25" pageNumber="26" pagination="323 - 423" refId="ref24609" refString="Taki, Is. (1938) Report on the biological survey of Mutsu Bay. 31. Studies on chitons of Mutsu Bay with general discussion on chitons of Japan. Science Reports of Tohoku Imperial Universit y, Series 4, Biology, 12, 323 - 423." type="journal article" year="1938">Is. Taki 1938</bibRefCitation>
