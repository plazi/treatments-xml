, 
<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia erugatus" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="erugatus">Cephaloleia erugatus</taxonomicName>
, 
<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia semivittata" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="semivittata">Cephaloleia semivittata</taxonomicName>
, 
<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia triangularis" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="triangularis">Cephaloleia triangularis</taxonomicName>
, 
<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia trivittata" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="trivittata">Cephaloleia trivittata</taxonomicName>
, 
<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia variabilis" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="variabilis">Cephaloleia variabilis</taxonomicName>
, and 
<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia vittata" order="Coleoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="vittata">Cephaloleia vittata</taxonomicName>
