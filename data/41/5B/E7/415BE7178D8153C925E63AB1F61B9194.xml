<taxonomicName class="Insecta" family="Ichneumonidae" genus="Sinarachna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinarachna" order="Hymenoptera" pageId="0" pageNumber="992" phylum="Arthropoda" rank="genus">Sinarachna</taxonomicName>
