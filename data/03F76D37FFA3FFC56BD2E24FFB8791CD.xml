<taxonomicName id="4C5EA7A2FFA2FFC56E79E548FEAD93A8" authorityName="Duncan, in Lochman &amp; Duncan" authorityYear="1944" class="Trilobita" family="Kingstoniidae" genus="Blountia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Ptychopariida" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="montanensis">
<emphasis id="B92A0033FFA2FFC56E79E548FEAD93A8" italics="true" pageId="13" pageNumber="14">Blountia montanensis</emphasis>
</taxonomicName>
