<taxonomicName id="4C5EA7A2FFA2FFC56AC1E581FDB393D4" authority="Westrop &amp; Eoff, 2020" authorityName="Westrop &amp; Eoff" authorityYear="2020" box="[388,573,441,467]" class="Trilobita" family="Kingstoniidae" genus="Blountia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Ptychopariida" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="tennesseensis" status="n. sp.">
<emphasis id="B92A0033FFA2FFC56AC1E581FE1B93D4" box="[388,405,441,466]" italics="true" pageId="13" pageNumber="14">B</emphasis>
