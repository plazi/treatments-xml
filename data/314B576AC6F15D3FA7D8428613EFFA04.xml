<taxonomicName id="77095B0A63F998751CF5AC0515F55B0A" class="Insecta" family="Arctiidae" genus="Pachydota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pachydota saduca" order="Lepidoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="saduca">
