<taxonomicName id="B13F06A655D8BA202AA7726EEBACD440" LSID="http://species-id.net/wiki/Eugerium_orbatum" authority="Szelenyi" authorityName="Graham" authorityYear="1959" class="Insecta" family="Eulophidae" genus="Eugerium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eugerium orbatum" order="Hymenoptera" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="orbatum">
Eugerium orbatum 
<normalizedToken id="B373D188AC6D191F86709E9E7E89A919" originalValue="Szelényi">Szelenyi</normalizedToken>
