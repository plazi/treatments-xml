<taxonomicName id="4FDBAFCCC88CF217EA0525C88BAE9B08" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus" order="Fagales" pageId="16" pageNumber="83" phylum="Tracheophyta" rank="genus">Quercus</taxonomicName>
