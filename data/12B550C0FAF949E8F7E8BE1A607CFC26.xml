<taxonomicName id="333326635D422C1B6F1CBDD482A7DA3E" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus" order="Fagales" pageId="16" pageNumber="83" phylum="Tracheophyta" rank="genus">Quercus</taxonomicName>
