<collectionCode id="99EDAE53FFE5ED2F0A2409A9FBAFBBE2" box="[374,430,1964,1988]" httpUri="http://grbio.org/cool/16z4-1tbx" name="Zoological Reference Collection, National University of Singapore" pageId="14" pageNumber="34">ZRC</collectionCode>
