<taxonomicName id="4C566AFF843EFFE4F048FEF7FC86FEAE" authorityName="Linnaeus" authorityYear="1758" box="[781,854,333,359]" class="Aves" family="Laridae" genus="Sterna" kingdom="Animalia" order="Charadriiformes" pageId="13" pageNumber="498" phylum="Chordata" rank="genus">
<emphasis id="B922CD6E843EFFE4F048FEF7FC86FEAE" box="[781,854,333,359]" italics="true" pageId="13" pageNumber="498">Sterna</emphasis>
