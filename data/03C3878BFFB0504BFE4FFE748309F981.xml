<paragraph id="8BD5369DFFB0504BFFC9FE3F812FFE4B" blockId="7.[108,1214,465,641]" box="[108,362,465,491]" pageId="7" pageNumber="8">
<taxonomicName id="4C6A4D1EFFB0504BFFC9FE3F812FFE4B" authority="Cope, 1882" authorityName="Cope" authorityYear="1882" box="[108,362,465,491]" class="Mammalia" family="Helaletidae" genus="Heptodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="7" pageNumber="8" phylum="Chordata" rank="genus">
