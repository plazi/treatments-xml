<number id="382D24C2FFF8FFE6B2133B90FCE3FC10" box="[852,877,937,959]" pageId="6" pageNumber="531" value="57.0">57</number>
</emphasis>
(
<number id="382D24C2FFF8FFE6B23F3B90FC0DFC10" box="[888,899,937,959]" pageId="6" pageNumber="531" value="1.0">1</number>
): 
<number id="382D24C2FFF8FFE6B2DF3B90FC28FC10" box="[920,934,937,959]" pageId="6" pageNumber="531" value="6.0">6</number>
