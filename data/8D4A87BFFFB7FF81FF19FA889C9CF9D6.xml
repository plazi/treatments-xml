<emphasis id="3797EABBFFB4FF81FE7AFA3F9EDFF9BF" box="[500,654,1513,1542]" italics="true" pageId="15" pageNumber="16">
<taxonomicName id="C2E34D2AFFB4FF81FE7AFA3F9E69F9BA" box="[500,568,1513,1539]" class="Ascidiacea" family="Styelidae" genus="Styela" kingdom="Animalia" order="Stolidobranchia" pageId="15" pageNumber="16" phylum="Chordata" rank="genus">Styela</taxonomicName>
