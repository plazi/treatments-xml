<number id="3B6A5670CE78FFC5F7B7FC4F17D6FC45" box="[727,766,968,990]" pageId="5" pageNumber="6" value="511.0">511</number>
</treatmentCitation>
, Pl. 
<number id="3B6A5670CE78FFC5F64AFC4F166DFC46" box="[810,837,968,989]" pageId="5" pageNumber="6" value="11.0">11</number>
