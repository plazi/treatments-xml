<paragraph id="8BC936827D71D10EFF3656B1FCCE0035" blockId="105.[128,655,856,1758]" lastBlockId="105.[728,1228,212,527]" pageId="105" pageNumber="105">
<materialsCitation id="3B1E3CDF7D71D10EFF3656B1FCCE0035" collectorName="da Saude &amp; Bosque da Saude. I" country="Brazil" county="Bosque da Saude" location="Avenida Bosque da Saude" municipality="Jardim da Saude." pageId="105" pageNumber="105" specimenCount="1" stateProvince="Sao Paulo">
TYPE LOCALITY: 
