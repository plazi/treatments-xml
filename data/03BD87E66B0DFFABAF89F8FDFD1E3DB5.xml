differs from 
<taxonomicName id="4C144D736B0DFFA9AB0DFF16FB673AD9" authority="(Frey)" baseAuthorityName="Frey" box="[1043,1209,1915,1941]" class="Insecta" family="Melolonthidae" genus="Serica" kingdom="Animalia" order="Coleoptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="levis">
