<heading id="D0C14346955E872BFF0BFD9FFD83FDE7" bold="true" box="[151,544,582,608]" fontSize="11" level="1" pageId="8" pageNumber="335" reason="1">
<emphasis id="B9422838955E872BFF0BFD9FFD83FDE7" bold="true" box="[151,544,582,608]" pageId="8" pageNumber="335">
<taxonomicName id="4C368FA9955E872BFF0BFD9FFE18FDE7" box="[151,443,582,608]" class="Asteroidea" family="Poraniidae" genus="Clavaporania" kingdom="Animalia" order="Valvatida" pageId="8" pageNumber="335" phylum="Echinodermata" rank="species" species="fitchorum">
<emphasis id="B9422838955E872BFF0BFD9FFE18FDE7" bold="true" box="[151,443,582,608]" italics="true" pageId="8" pageNumber="335">Clavaporania fitchorum</emphasis>
</taxonomicName>
