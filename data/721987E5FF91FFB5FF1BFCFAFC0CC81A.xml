<paragraph id="FA0F36F3FF91FFB0FF1BFCFAFDFCCAF4" blockId="1.[151,604,890,951]" box="[151,604,890,916]" pageId="1" pageNumber="2">
<heading id="A147819FFF91FFB0FF1BFCFAFDFCCAF4" bold="true" box="[151,604,890,916]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<taxonomicName id="3DB04D70FF91FFB0FF1BFCFAFDFCCAF4" authority="Yamaguti, 1935" authorityName="Yamaguti" authorityYear="1935" box="[151,604,890,916]" class="Secernentea" family="Cucullanidae" genus="Cucullanus" kingdom="Animalia" order="Ascaridida" pageId="1" pageNumber="2" phylum="Nematoda" rank="species" species="robustus">
