<quantity id="0A319B203622FF8EBB3E0E43FB904AF5" box="[886,1041,1902,1926]" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="1.63" metricValueMin="1.6199999999999999" pageId="7" pageNumber="8" unit="m" value="1625.0" valueMax="1630.0" valueMin="1620.0">1620–1630 m</quantity>
