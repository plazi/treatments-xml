</taxonomicName>
(
<taxonomicName id="4C604D30D401265764ECEF58FBD4C836" box="[975,1108,1640,1662]" class="Magnoliopsida" family="Polygalaceae" kingdom="Plantae" order="Fabales" pageId="1" pageNumber="276" phylum="Tracheophyta" rank="family">Polygalaceae</taxonomicName>
