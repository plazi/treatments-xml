<taxonomicName id="4C604D30D40126576590EF58FC42C835" authorityName="Wight" authorityYear="1840" box="[691,962,1640,1661]" class="Magnoliopsida" family="Polygalaceae" genus="Xanthophyllum" kingdom="Plantae" order="Fabales" pageId="1" pageNumber="276" phylum="Tracheophyta" rank="species" species="arnottianum">
