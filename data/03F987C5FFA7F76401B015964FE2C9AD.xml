<taxonomicName id="4C504D50FFA9F764005D15C24E0CCBE8" authority="Köhler, 2010" authorityName="Köhler" authorityYear="2010" box="[552,793,1415,1436]" class="Gastropoda" family="Camaenidae" genus="Amplirhagada" kingdom="Animalia" order="Stylommatophora" pageId="56" pageNumber="273" phylum="Mollusca" rank="species" species="dubitabile" status="n.sp.">
<emphasis id="B924EAC1FFA9F764005D15C24E0CCBE8" box="[552,793,1415,1436]" italics="true" pageId="56" pageNumber="273">Amplirhagada dubitabile</emphasis>
</taxonomicName>
