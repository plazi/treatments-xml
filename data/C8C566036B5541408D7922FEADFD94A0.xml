<paragraph id="C36A6D26692F85DB6ACE26E69001F998" pageId="4" pageNumber="31">
<taxonomicName id="CD5E434AD66733B6E664B596889A840F" class="Gastropoda" family="Partulidae" genus="Palaopartula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Palaopartula" order="Stylommatophora" pageId="4" pageNumber="31" phylum="Mollusca" rank="genus">Palaopartula</taxonomicName>
