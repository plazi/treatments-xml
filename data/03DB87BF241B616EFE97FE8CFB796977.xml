<taxonomicName id="4C724D2A241B616CFE97FEF7FD6B6B1C" authority="Keifer, 1969: 15" authorityName="Keifer" authorityPageNumber="15" authorityYear="1969" box="[264,669,358,382]" class="Arachnida" family="Eriophyidae" genus="Catachela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="machaerii">
