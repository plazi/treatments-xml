<taxonomicName id="98DF4D12FFA50D2A1B8AFD8216ACFDD9" box="[454,691,542,568]" class="Liliopsida" family="Pontederiaceae" genus="Pontederia" kingdom="Plantae" order="Commelinales" pageId="24" pageNumber="475" phylum="Tracheophyta" rank="species" species="crassipes">
