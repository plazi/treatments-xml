<taxonomicName id="8FEE4D41FFA9FF82FE95FD286483C9EF" authorityName="Olenev" authorityYear="1929" box="[261,448,655,681]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="semenovi">
<emphasis id="7A9AEAD0FFA9FF82FE95FD286483C9EF" box="[261,448,655,681]" italics="true" pageId="64" pageNumber="65">Ixodes semenovi</emphasis>
</taxonomicName>
