<taxonomicName id="4C274D20FFA2CE61FF3B4CB7FE41F900" authority="Lopes-Andrade, 2008" authorityName="Lopes-Andrade" authorityYear="2008" box="[198,388,1752,1777]" class="Insecta" family="Ciidae" genus="Scolytocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="cariborum" status="sp. nov.">
