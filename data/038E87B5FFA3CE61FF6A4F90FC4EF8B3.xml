<taxonomicName id="4C274D20FFA2CE61FD4C4CB7FCB0F900" authority="Lopes-Andrade, 2008" authorityName="Lopes-Andrade" authorityYear="2008" box="[689,885,1752,1777]" class="Insecta" family="Ciidae" genus="Scolytocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="kiskeyensis" status="sp. nov.">
<emphasis id="B953EAB1FFA2CE61FD4C4CB7FCB0F900" box="[689,885,1752,1777]" italics="true" pageId="17" pageNumber="18">Scol. kiskeyensis</emphasis>
</taxonomicName>
<emphasis id="B953EAB1FFA2CE61FC804CB8FC1EF900" bold="true" box="[893,987,1751,1777]" pageId="17" pageNumber="18">
<taxonomicNameLabel id="A26057CAFFA2CE61FC804CB8FC1EF900" box="[893,987,1751,1777]" pageId="17" pageNumber="18" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
