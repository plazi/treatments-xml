<quantity id="4C9D51E2FFC0D17BFC0E5A13065BF995" box="[921,1083,1637,1663]" metricMagnitude="3" metricUnit="m" metricValue="1.6" metricValueMax="1.8" metricValueMin="1.4" pageId="11" pageNumber="12" unit="m" value="1600.0" valueMax="1800.0" valueMin="1400.0">1400–1800 m</quantity>
