<number id="3420248AFFE16B3C949913055FD0F800" box="[424,459,548,572]" pageId="13" pageNumber="560" value="2.5">2.5</number>
turns in 
<emphasis id="B09CEABFFFE16B3C970713045CD4F800" box="[566,719,548,572]" italics="true" pageId="13" pageNumber="560">
<taxonomicName id="45E84D2EFFE16B3C970713045CDCF800" box="[566,711,548,572]" class="Adenophorea" family="Comesomatidae" genus="Dorylaimopsis" kingdom="Animalia" order="Desmodorida" pageId="13" pageNumber="560" phylum="Nematoda" rank="species" species="coomansi">D. coomansi</taxonomicName>
)
</emphasis>
and slenderer tail in males (cʹ = 
<number id="3420248AFFE16B3C916113055ABEF800" box="[1104,1189,548,572]" pageId="13" pageNumber="560" value="3.95" valueMax="4.3" valueMin="3.6">3.6–4.3</number>
<emphasis id="B09CEABFFFE16B3C919F13045ADEF800" box="[1198,1221,549,572]" italics="true" pageId="13" pageNumber="560">vs</emphasis>
cʹ = 
<quantity id="45109B48FFE16B3C903513055B6CF800" box="[1284,1399,548,572]" metricMagnitude="-2" metricUnit="m" metricValue="8.382" metricValueMax="8.636" metricValueMin="8.128" pageId="13" pageNumber="560" unit="in" value="3.3" valueMax="3.4" valueMin="3.2">3.2–3.4 in</quantity>
<taxonomicName id="45E84D2EFFE16B3C90B013045F19F85C" class="Adenophorea" family="Comesomatidae" genus="Dorylaimopsis" kingdom="Animalia" order="Desmodorida" pageId="13" pageNumber="560" phylum="Nematoda" rank="species" species="coomansi">
<emphasis id="B09CEABFFFE16B3C90B013045F19F85C" italics="true" pageId="13" pageNumber="560">D. coomansi</emphasis>
</taxonomicName>
) (Muthumbi, Soetaert &amp; Vincx, 
<number id="3420248AFFE16B3C974613665CA8F863" box="[631,691,583,607]" pageId="13" pageNumber="560" value="1997.0">1997</number>
).
