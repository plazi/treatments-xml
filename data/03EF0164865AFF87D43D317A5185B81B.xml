<taxonomicName id="4C46CBF1865AFF87D738375A511ABEFB" authorityName="Senna" authorityYear="1898" box="[890,1042,876,901]" class="Insecta" family="Brentidae" genus="Cerobates" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="610" phylum="Arthropoda" rank="species" species="punctulatus">
