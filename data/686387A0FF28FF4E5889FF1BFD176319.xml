<taxonomicName id="27CA4D35FF28FF4E5A7CFEE3FCCC60F1" authorityName="Rondani" authorityYear="1880" box="[610,895,366,392]" class="Insecta" family="Sphaeroceridae" genus="Sphaerocera" kingdom="Animalia" order="Diptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="pallidimana">
