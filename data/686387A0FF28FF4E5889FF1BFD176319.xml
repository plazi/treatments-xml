<taxonomicName id="27CA4D35FF28FF4E59EEFE73FD756360" authority="Loew" authorityName="Loew" box="[496,710,510,537]" class="Insecta" family="Sphaeroceridae" genus="Sphaerocera" kingdom="Animalia" order="Diptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="genus">
