(
<bibRefCitation id="EF994B3AFFAAFFB5FE58F955FDCB1509" author="Sick H" box="[504,617,1779,1803]" pageId="13" pageNumber="906" refId="ref21953" refString="Sick H (1997) Ornitologia brasileira. 2 nd ed. Nova Fronteira, Rio de Janeiro, Brazil, 912 pp." type="book" year="1997">Sick 1997</bibRefCitation>
