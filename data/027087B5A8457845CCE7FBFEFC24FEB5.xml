. 
<taxonomicName id="4DD94D20A8447843CD66FE11FE29F8C1" baseAuthorityName="Templeton" baseAuthorityYear="1836" box="[278,484,1821,1845]" class="Entognatha" family="Entomobryidae" genus="Heteromurus" kingdom="Animalia" order="Collembola" pageId="19" pageNumber="266" phylum="Arthropoda" rank="species" species="nitidus">
