<collectionCode id="ECC8AE66A8457842CCEFFD72FF11FB63" box="[159,220,1149,1175]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33074" lsid="urn:lsid:biocol.org:col:33074" name="China Agricultural University" pageId="18" pageNumber="265">CAU</collectionCode>
<date id="FE671063A8457842CCABFD72FE6FFB63" box="[219,418,1149,1175]" pageId="18" pageNumber="265" value="2018-02-29" valueMax="2018-04-29" valueMin="2018-02-29">
<collectingDate id="EE23E98BA8457842CCABFD72FE6FFB63" box="[219,418,1149,1175]" pageId="18" pageNumber="265" value="2018-02-29" valueMax="2018-04-29" valueMin="2018-02-29">2018-29-02 to 04</collectingDate>
</date>
). 
<collectorName id="272C5375A8457842CDC4FD72FD4AFB63" box="[436,647,1149,1175]" pageId="18" pageNumber="265">All Kuznetsova N.</collectorName>
, 
<collectorName id="272C5375A8457842CEE3FD72FCDBFB63" box="[659,790,1149,1175]" pageId="18" pageNumber="265">Potapov M.</collectorName>
and 
