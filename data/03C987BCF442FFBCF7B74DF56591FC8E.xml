<taxonomicName id="4C604D29F442FFBFF5FC4B9962ACF855" box="[732,898,1977,1999]" class="Insecta" family="Miridae" genus="Hallodapus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="414" phylum="Arthropoda" rank="species" species="albofasciatus">
