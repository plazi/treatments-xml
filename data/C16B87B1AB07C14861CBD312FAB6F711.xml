<taxonomicName id="8EC24D24AB00C14863EBD207FB81F76D" authority="Gorochov, 1998" authorityName="Gorochov" authorityYear="1998" box="[695,1075,788,813]" class="Insecta" family="Rhaphidophoridae" genus="Diestrammena" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="518" phylum="Arthropoda" rank="species" species="vietnamensis">
