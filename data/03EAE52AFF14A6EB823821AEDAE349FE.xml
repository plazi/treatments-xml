<taxonomicNameLabel id="A2043555FF14A6EA87802486D84D4A38" pageId="178" rank="species">new species</taxonomicNameLabel>
(stars), 
<taxonomicName id="4C432FBFFF14A6EA80B2249BDF7C4A39" authority="PLATNICK, 2002" authorityName="PLATNICK" authorityYear="2002" box="[966,1117,1790,1811]" class="Arachnida" family="Trochanteriidae" genus="Platorish" kingdom="Animalia" order="Araneae" pageId="178" phylum="Arthropoda" rank="species" species="churchillae" status="new species">
