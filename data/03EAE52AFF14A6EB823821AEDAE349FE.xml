<taxonomicName id="4C432FBFFF14A6EA8005257DD8C34A07" authorityName="PLATNICK" authorityYear="2002" box="[881,994,1816,1837]" class="Arachnida" family="Trochanteriidae" genus="Platorish" kingdom="Animalia" order="Araneae" pageId="178" phylum="Arthropoda" rank="species" species="gelorup">
