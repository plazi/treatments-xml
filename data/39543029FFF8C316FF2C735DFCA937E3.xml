<taxonomicName id="76FDFABCFFE6C316FE9C728DFD7034DB" authority="Cumming" authorityName="Cumming &amp; Leong &amp; Lohman" authorityYear="2017" box="[295,746,1409,1431]" class="Insecta" family="Phylliidae" genus="Phyllium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phasmida" pageId="16" pageNumber="127" phylum="Arthropoda" rank="species" species="antonkozlovi" status="sp. nov." subGenus="Phyllium">
<emphasis id="83895D2DFFE6C316FE9C728DFE1934DB" box="[295,387,1409,1431]" italics="true" pageId="16" pageNumber="117">Phyllium</emphasis>
