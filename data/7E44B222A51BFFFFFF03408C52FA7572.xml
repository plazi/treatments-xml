<caption id="A29253BCA51AFFFAFF03407757CD77B6" httpUri="https://zenodo.org/record/217751/files/figure.png" pageId="1" pageNumber="107" targetBox="[155,1436,155,1531]" targetPageId="1">
<paragraph id="F6520334A51AFFFAFF03407757CD77B6" blockId="1.[151,1436,1556,1712]" pageId="1" pageNumber="107">
<emphasis id="C499DF26A51AFFFAFF034077572E772C" bold="true" box="[151,275,1556,1578]" pageId="1" pageNumber="107">FIGURE 1.</emphasis>
<taxonomicName id="31ED78B7A51AFFFAFE8D407757FB772C" box="[281,454,1556,1578]" class="Maxillopoda" family="Pyrgomatidae" genus="Trevathana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sessilia" pageId="1" pageNumber="107" phylum="Arthropoda" rank="species" species="noae" status="sp. nov.">
