<taxonomicName id="82B8B17AE4B650DF1E027745E47D5E6B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141450" class="Insecta" family="Formicidae" genus="Odontomachus" kingdom="Animalia" lsidName="Odontomachus ruficeps r. acutidens Forel" lsidName-HNS="Odontomachus ruficeps r. acutidens Forel" order="Hymenoptera" pageNumber="56" phylum="Arthropoda" rank="subSpecies" species="ruficeps" subSpecies="acutidens">Odontomachus ruficeps Sm. r. acutidens</taxonomicName>
