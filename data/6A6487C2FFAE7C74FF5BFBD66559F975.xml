<date id="96731014FFAE7C74FE45FAC86320FA78" box="[393,560,1407,1432]" pageId="1" pageNumber="580" value="2007-02-14" valueMax="2007-02-16" valueMin="2007-02-14">14.–16.II.2007</date>
