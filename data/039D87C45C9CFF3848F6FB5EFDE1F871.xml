<heading id="D0C381BE5C9CFF3848F6FB5EFCE6FB66" bold="true" box="[151,808,1153,1179]" fontSize="11" level="1" pageId="131" pageNumber="132" reason="1">
<emphasis id="B940EAC05C9CFF3848F6FB5EFCE6FB66" bold="true" box="[151,808,1153,1179]" pageId="131" pageNumber="132">
<taxonomicName id="4C344D515C9CFF3848F6FB5EFDF9FB66" authority="Barbosa &amp; Azevedo" authorityName="Barbosa &amp; Azevedo" box="[151,567,1153,1179]" class="Insecta" family="Bethylidae" genus="Anisepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="dietrichorum">
<emphasis id="B940EAC05C9CFF3848F6FB5EFEFBFB66" bold="true" box="[151,309,1153,1179]" italics="true" pageId="131" pageNumber="132">dietrichorum</emphasis>
