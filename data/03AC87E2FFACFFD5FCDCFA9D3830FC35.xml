<taxonomicName id="4C054D77FFA3FFD5FEA5FCC738A9FCF3" authorityName="Hartert" authorityYear="1929" box="[346,462,832,855]" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="24" pageNumber="24" phylum="Chordata" rank="species" species="splendidus">
