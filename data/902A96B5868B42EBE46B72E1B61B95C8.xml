<taxonomicName id="BB10111552CBA699F59C4D347CECB2E0" class="Arachnida" family="Archaeidae" genus="Eriauchenius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eriauchenius" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Eriauchenius</taxonomicName>
