, 
<emphasis id="34F2EAD2FF95FFC3FD1F424C5F47FAB8" bold="true" box="[735,775,1348,1373]" pageId="21" pageNumber="472">Hf:</emphasis>
<number id="B04E24E7FF95FFC3FCCF424C5F5FFAB8" box="[783,799,1348,1373]" pageId="21" pageNumber="472" value="5.0">5</number>
, 
<emphasis id="34F2EAD2FF95FFC3FCEB424C5F12FAB8" bold="true" box="[811,850,1348,1373]" pageId="21" pageNumber="472">Ht:</emphasis>
