<taxonomicName id="D08F0A9E4843FFFDDB114FAC1EB8FC30" authorityName="Carl" authorityYear="1914" box="[574,741,976,999]" class="Insecta" family="Phaneropteridae" genus="Mimoscudderia" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="306" phylum="Arthropoda" rank="genus">
<emphasis id="25FBAD0F4843FFFDDB114FAC1EB8FC30" box="[574,741,976,999]" italics="true" pageId="9" pageNumber="306">Mimoscudderia</emphasis>
</taxonomicName>
and 
<taxonomicName id="D08F0A9E4843FFFDDA374FAC1FEDFC30" authorityName="Brunner von Wattenwyl" authorityYear="1891" box="[792,944,976,999]" class="Insecta" family="Phaneropteridae" genus="Parapyrrhicia" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="306" phylum="Arthropoda" rank="genus">
<emphasis id="25FBAD0F4843FFFDDA374FAC1FEDFC30" box="[792,944,976,999]" italics="true" pageId="9" pageNumber="306">Parapyrrhicia</emphasis>
</taxonomicName>
species (length in mm; data from descriptions
