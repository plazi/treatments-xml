<taxonomicName id="DC905EA9CE3202096F3AFB27669834DE" authorityName="Kerremans" authorityYear="1897" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="89" phylum="Arthropoda" rank="species" species="inflatus">
