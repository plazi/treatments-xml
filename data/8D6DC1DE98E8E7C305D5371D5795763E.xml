<normalizedToken id="0896DA441B70BD546FE6CC8D0B78A45C" originalValue="Suchitepéquez">Suchitepequez</normalizedToken>
, Zacapa. HONDURAS: 
<normalizedToken id="07C2060045422A177906A96108DC0F24" originalValue="Cortés">Cortes</normalizedToken>
, El 
<normalizedToken id="52E104C75032BA1FD10D815009E82310" originalValue="Paraíso">Paraiso</normalizedToken>
, Francisco 
<normalizedToken id="778215AAE97FAD0D8D88CA1AC070DFC6" originalValue="Morazán">Morazan</normalizedToken>
, 
<normalizedToken id="E2959349C140CBB1CB663695343CE87C" originalValue="Intibucá">Intibuca</normalizedToken>
