<taxonomicName id="4C3C48E09E059F15FED4F9F710091D42" box="[286,581,1629,1651]" class="Insecta" family="Cicadellidae" genus="Scaphoideus" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="361" phylum="Arthropoda" rank="species" species="proxaprocessus" status="sp. nov.">
