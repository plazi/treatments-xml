<subSubSection id="EAB08ABB1C59416BF3A10F6A5F9F5926" pageId="null" pageNumber="206" type="nomenclature">
<paragraph id="F6410C6F5659FBA3AE84C06413DED834" pageId="null" pageNumber="206">
<taxonomicName id="666960A12D41FBA0ECB1EB8B96088883" authority="L." class="Liliopsida" family="Potamogetonaceae" genus="Zannichellia" kingdom="Plantae" order="Alismatales" pageId="null" pageNumber="206" phylum="Tracheophyta" rank="species" species="palustris">
<pageBreakToken id="34F7B910F49C7AC0A8B946E9A7E78DB3" pageId="null" pageNumber="206">Zannichellia</pageBreakToken>
<normalizedToken id="057158D7556CC0196DCCDA74A928AF3F" originalValue="palústris" pageId="null" pageNumber="206">palustris</normalizedToken>
<authorityName id="76DBC1C32CA848B2CCAB26A009D6CEF6" pageId="null" pageNumber="206">L.</authorityName>
</taxonomicName>
