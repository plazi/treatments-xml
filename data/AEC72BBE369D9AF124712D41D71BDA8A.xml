<taxonomicName id="31CAE168FFCEBEB17EE98D1F97A66588" class="Magnoliopsida" family="Rosaceae" genus="Rosa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rosa" order="Rosales" pageId="88" pageNumber="89" phylum="Tracheophyta" rank="genus">Rosa</taxonomicName>
sp. (
<bibRefCitation id="1812BC5364328ABEE19023C2F7193F61" author="Chambers, VH" journalOrPublisher="The Entomologist's Monthly Magazine, Fourth Series" pageId="100" pageNumber="101" pagination="209 - 211" title="Bedfordshire sawflies: with some new food-plants." volume="96" year="1961">Chambers 1961</bibRefCitation>
, as 
<taxonomicName id="06728638C96C4B742B8DA14E349E647D" genus="P." lsidName="P. paedida" pageId="88" pageNumber="89" rank="species" species="paedida">P. paedida</taxonomicName>
), 
<taxonomicName id="A4EE7B2BBFAEC8E249B517EF251423CC" genus="R." lsidName="R. majalis" pageId="88" pageNumber="89" rank="species" species="majalis">R. majalis</taxonomicName>
