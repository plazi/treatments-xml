<taxonomicName id="AE161523832E8F51786666A47837CF6D" genus="R." lsidName="R. pimpinellifolia" pageId="88" pageNumber="89" rank="species" species="pimpinellifolia">R. pimpinellifolia</taxonomicName>
L. (
<bibRefCitation id="8432B2F7461F67C71FB4E7004CA97129" author="Dvorak, L" journalOrPublisher="Acta Musei Moraviae, Scientiae biologicae" pageId="102" pageNumber="103" pagination="53 - 92" title="Hymenoptera of Hady Hill, near the city of Brno (Czech Republic), collected during the Third Czech-Slovak Hymenoptera meeting." volume="93" year="2008">Dvorak et al. 2008</bibRefCitation>
), 
<taxonomicName id="4240B41AB9D09B003662E4EA8226E479" class="Magnoliopsida" family="Rosaceae" genus="Rosa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rosa onoei" order="Rosales" pageId="88" pageNumber="89" phylum="Tracheophyta" rank="species" species="onoei">Rosa onoei</taxonomicName>
