<typeStatus id="54EB880AFF84273EFE0DFAD7FDA6D76B" box="[433,537,1305,1331]" pageId="21" pageNumber="322">Holotype</typeStatus>
male microptera. 
<emphasis id="B924EABAFF84273EFD58FAD7FCCDD76C" bold="true" box="[740,882,1305,1332]" pageId="21" pageNumber="322">
<collectingRegion id="4994F84AFF84273EFD58FAD7FCCDD76C" box="[740,882,1305,1332]" country="Australia" name="Queensland" pageId="21" pageNumber="322">Queensland</collectingRegion>
</emphasis>
, 
<collectingMunicipality id="6B8BACD2FF84273EFC3CFAD7FBF0D76B" box="[896,1103,1305,1331]" pageId="21" pageNumber="322">Carnarvon Station</collectingMunicipality>
, 
<location id="8E8F6073FF84273EFBE5FAD7FAE5D76B" LSID="urn:lsid:plazi:treatment:03F987BEFF84273EFF2BFF57FAF5D454:8E8F6073FF84273EFBE5FAD7FAE5D76B" box="[1113,1370,1305,1331]" municipality="Carnarvon Station" name="White Stallion Springs" pageId="21" pageNumber="322" stateProvince="Queensland">White Stallion Springs</location>
, from 
<taxonomicName id="4C504D2BFF84273EFF2BFAF0FE82D70F" box="[151,317,1342,1367]" class="Magnoliopsida" family="Myrtaceae" genus="Leptospermum" higherTaxonomySource="CoL" kingdom="Plantae" order="Myrtales" pageId="21" pageNumber="322" phylum="Tracheophyta" rank="genus">
