<quantity id="52469B76FF89FFA551D40030FC16B56B" box="[898,952,1106,1130]" metricMagnitude="1" metricUnit="m" metricValue="4.2" pageId="30" pageNumber="31" unit="m" value="42.0">42 m</quantity>
