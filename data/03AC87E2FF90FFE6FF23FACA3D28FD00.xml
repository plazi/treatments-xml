<emphasis id="B971EAE6FF90FFE6FD42FE473AE6FE71" bold="true" box="[701,897,448,469]" pageId="43" pageNumber="43">
<specimenCode id="DBA39E8FFF90FFE6FD42FE473AE6FE71" box="[701,897,448,469]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="43" pageNumber="43">AMNH 699418</specimenCode>
