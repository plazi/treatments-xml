. 
<taxonomicName id="4C0A4D20FFF1FFACFE0CFA533972FA27" baseAuthorityName="Targioni-Tozzetti" baseAuthorityYear="1887" box="[482,771,1410,1433]" class="Insecta" family="Thripidae" genus="Mycterothrips" kingdom="Animalia" order="Thysanoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="consociatus">
