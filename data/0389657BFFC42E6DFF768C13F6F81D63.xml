<paragraph id="8B9FD46DFFC42E6FFC8F8DEDF0A01BE9" blockId="5.[812,1378,1841,2015]" box="[812,1169,1969,1989]" pageId="5" pageNumber="2894">
<taxonomicName id="4C20AFEEFFC42E6FFC8F8DEDF0A01BE9" authority="Siddiqi, 1982" authorityName="Siddiqi" authorityYear="1982" box="[812,1169,1969,1989]" class="Adenophorea" family="Qudsianematidae" genus="Sicorinema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="5" pageNumber="2894" phylum="Nematoda" rank="species" species="sericatum">
