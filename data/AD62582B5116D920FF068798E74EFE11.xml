<collectingCountry id="5DDCA9AD5116D920FDD487D3E7F9FE82" box="[606,692,237,263]" name="Canada" pageId="8" pageNumber="217">Canada</collectingCountry>
from 
<collectingRegion id="E70F27DF5116D920FD7687D3E619FE82" box="[764,852,237,263]" country="Canada" name="Quebec" pageId="8" pageNumber="217">Québec</collectingRegion>
