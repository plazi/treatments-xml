<taxonomicName id="E2CB92BE5116D920FCF68668E6ADFEF5" authorityName="Cesati &amp; De Notaris" authorityYear="1863" box="[892,992,342,368]" class="Sordariomycetes" family="Xylariaceae" genus="Daldinia" kingdom="Fungi" order="Xylariales" pageId="8" pageNumber="217" phylum="Ascomycota" rank="genus">
<emphasis id="17BF352F5116D920FCF68668E6ADFEF5" box="[892,992,342,368]" italics="true" pageId="8" pageNumber="217">Daldinia</emphasis>
</taxonomicName>
(Ascomycota: 
<taxonomicName id="E2CB92BE5116D920FF438644E407FE10" authority="Steiner 1984" box="[201,330,378,405]" class="Sordariomycetes" family="Xylariaceae" kingdom="Fungi" order="Xylariales" pageId="8" pageNumber="217" phylum="Ascomycota" rank="family">Xylariaceae</taxonomicName>
) (
<bibRefCitation id="415A94CC5116D920FEEA8644E4BAFE11" author="Steiner" box="[352,503,378,404]" firstAuthor="Steiner" pageId="8" pageNumber="217" pagination="424 - 455" refId="ref5999" refString="Steiner WE, Jr (1984) A review of the biology of phalacrid beetles (Coleoptera). In Wheeler Q, Blackwell M (Eds) Fungus-Insect Relationships: Perspectives in Ecology and Evolution. Columbia University Press, New York, 424 - 455." type="book chapter" year="1984">Steiner 1984</bibRefCitation>
