<taxonomicName id="E2CB92BE5116D920FF068668E444FEF5" authorityName="Casey" authorityYear="1890" box="[140,265,342,368]" class="Insecta" family="Phalacridae" genus="Litochropus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="217" phylum="Arthropoda" rank="genus">
