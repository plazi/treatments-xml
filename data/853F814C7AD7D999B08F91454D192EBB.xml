<taxonomicName id="A1828815371B3C02206C7E6991783F19" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34069" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole triconstricta Forel" order="Hymenoptera" pageNumber="221" phylum="Arthropoda" rank="species" species="triconstricta">Pheidole triconstricta</taxonomicName>
