<taxonomicName id="F91611FF0FB4CE156F512C476C380179" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181674" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole inversa Forel" order="Hymenoptera" pageNumber="221" phylum="Arthropoda" rank="species" species="inversa">inversa</taxonomicName>
