<taxonomicName id="0B6A440CD731F7780902B5506D80036C" class="Insecta" family="Noctuidae" genus="Xestia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xestia" order="Lepidoptera" pageId="1" pageNumber="134" phylum="Arthropoda" rank="genus">Xestia</taxonomicName>
