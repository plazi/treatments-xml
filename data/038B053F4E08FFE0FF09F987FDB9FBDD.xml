<taxonomicName id="4C22CFAA4E0BFFE0FEE4FCC8FDD3FCFD" box="[378,608,836,860]" class="Magnoliopsida" family="Fabaceae" genus="Genista" kingdom="Plantae" order="Fabales" pageId="38" pageNumber="239" phylum="Tracheophyta" rank="species" species="ephedroides">
