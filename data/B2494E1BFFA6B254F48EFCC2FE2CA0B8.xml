. — 
<taxonomicName id="FDE0848EFFA6B254F569FC8FFD83A651" authority="Polejaeff, 1883" authorityName="Polejaeff" authorityYear="1883" box="[288,615,834,859]" class="Calcarea" family="Jenkinidae" genus="Anamixilla" kingdom="Animalia" order="Leucosolenida" pageId="29" pageNumber="232" phylum="Porifera" rank="species" species="torresi">
