<location id="8E969F9EFFF4FFFE26F0FB17FCC310A1" LSID="urn:lsid:plazi:treatment:03E07853FFF4FFFE24F8FB91FC98125C:8E969F9EFFF4FFFE26F0FB17FCC310A1" box="[708,826,1215,1238]" country="Ghana" county="Trewavas" municipality="d'Accra" name="Dabo Crom" pageId="4" pageNumber="585">Dabo Crom</location>
