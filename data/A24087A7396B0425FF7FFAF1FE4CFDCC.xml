</paragraph>
<paragraph id="2A5636B1396B0424FF7FFA71FCFAFA6C" blockId="8.[192,927,1473,1530]" box="[192,872,1506,1530]" pageId="8" pageNumber="7">
<treatmentCitationGroup id="0AF9119F396B0424FF7FFA71FCFAFA6C" box="[192,872,1506,1530]" pageId="8" pageNumber="7">
<treatmentCitation id="AB4810A0396B0424FF7FFA71FD4AFA6C" author="Burmeister, H." box="[192,728,1506,1530]" page="101" pageId="8" pageNumber="7" year="1847">
<taxonomicName id="EDE94D32396B0424FF7FFA71FD4AFA6C" authority="Burmeister 1847: 101" authorityName="Burmeister" authorityPageNumber="101" authorityYear="1847" box="[192,728,1506,1530]" class="Insecta" family="Dynastidae" genus="Heteronychus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="fossator">
<emphasis id="189DEAA3396B0424FF7FFA71FE5CFA6C" box="[192,462,1506,1530]" italics="true" pageId="8" pageNumber="7">Heteronychus fossator</emphasis>
<bibRefCitation id="4E784B40396B0424FE6AFA71FD4AFA6C" author="Burmeister, H." box="[469,728,1506,1530]" pageId="8" pageNumber="7" refId="ref13952" refString="Burmeister, H. 1847. Handbuch der Entomologie. Volume 5, T. E. F. Enslin; Berlin, Germany. 584 p." type="book" year="1847">Burmeister 1847:101</bibRefCitation>
</taxonomicName>
</treatmentCitation>
