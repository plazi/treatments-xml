<paragraph id="995B37113076FFDCCB2FFC40FE70FB87" blockId="6.[264,1324,284,1030]" pageId="6" pageNumber="7">
<materialsCitation id="298C3D4C3076FFDCCB2FFC40FE7FFB87" ID-GBIF-Occurrence="3338660302" collectionCode="UFRJ" county="Lab. de Protistologia" location="Universidade Federal" municipality="Instuto de Biologia" pageId="6" pageNumber="7" specimenCount="2" stateProvince="Rio de Janeiro" typeStatus="holotype">
