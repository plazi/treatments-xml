<caption id="DF646677FF85FF98FFB2600FB819FAE1" ID-DOI="http://doi.org/10.5281/zenodo.5377213" ID-Zenodo-Dep="5377213" httpUri="https://zenodo.org/record/5377213/files/figure.png" pageId="18" pageNumber="421" startId="18.[119,130,1284,1301]" targetBox="[119,1217,198,1344]" targetPageId="18">
<paragraph id="8BA436FFFF85FF98FFB2600FB819FAE1" blockId="18.[119,1217,1284,1344]" pageId="18" pageNumber="421">
