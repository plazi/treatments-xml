</taxonomicName>
ex gr. 
<taxonomicName id="4C1B4D7CFF85FF98FEB3600FBB42FAB4" authority="Blasius, 1853" authorityName="Blasius" authorityYear="1853" box="[374,554,1284,1301]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="421" phylum="Chordata" rank="species" species="euryale">
<emphasis id="B96FEAEDFF85FF98FEB3600FB8D9FAB4" box="[374,433,1284,1301]" italics="true" pageId="18" pageNumber="421">euryale</emphasis>
Blasius, 1853
</taxonomicName>
; 
<emphasis id="B96FEAEDFF85FF98FDF7600FBB57FAB4" bold="true" box="[562,575,1284,1301]" pageId="18" pageNumber="421">A</emphasis>
