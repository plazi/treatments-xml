<taxonomicName id="4C1B4D7CFF85FF98FF16600FB853FAB4" authorityName="Lacepede" authorityYear="1799" box="[211,315,1284,1301]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="421" phylum="Chordata" rank="genus">
