<collectionCode id="EFC3AE0069555139C4DC712EF33FFCDE" box="[191,279,843,867]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="13" pageNumber="2523" type="Museum">NHMUK</collectionCode>
