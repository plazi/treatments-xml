<quantity id="4E2A9B2069555139C685714CF170FCFC" box="[742,856,809,833]" metricMagnitude="-2" metricUnit="m" metricValue="2.601" pageId="13" pageNumber="2523" unit="mm" value="26.01">26.01 mm</quantity>
