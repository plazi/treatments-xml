<location id="8C0D601E69555139C502712EF3BCFCDE" LSID="urn:lsid:plazi:treatment:017B87D369555139C5F070E0F124FA3B:8C0D601E69555139C502712EF3BCFCDE" box="[353,404,843,867]" country="Kenya" name="Lake" pageId="13" pageNumber="2523" stateProvince="Baringo">Lake</location>
<collectingRegion id="4B16F82769555139C5FC712EF3D0FCDE" box="[415,504,843,867]" country="Kenya" name="Baringo" pageId="13" pageNumber="2523">Baringo</collectingRegion>
, western central 
<collectingCountry id="F1C5765569555139C6A3712EF12FFCDE" box="[704,775,843,867]" name="Kenya" pageId="13" pageNumber="2523">Kenya</collectingCountry>
