<emphasis id="BBA6EAD769555139C5B1714DF3F7FCFD" box="[466,479,808,832]" italics="true" pageId="13" pageNumber="2523">–</emphasis>
<quantity id="4E2A9B2069555139C5BC714CF069FCFC" box="[479,577,809,833]" metricMagnitude="-2" metricUnit="m" metricValue="3.05" pageId="13" pageNumber="2523" unit="mm" value="30.5">30.5 mm</quantity>
