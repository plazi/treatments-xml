<taxonomicName id="D5F04D4EA7B8FF19084D8DB9FC4E039F" box="[589,926,1669,1693]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="174" pageNumber="175" phylum="Arthropoda" rank="species" species="sagax" subGenus="Dialictus">
<emphasis id="2084EADFA7B8FF19084D8DB9FD30039F" box="[589,736,1669,1693]" italics="true" pageId="174" pageNumber="175">Lasioglossum</emphasis>
(
<emphasis id="2084EADFA7B8FF1908EF8DB9FC80039F" box="[751,848,1669,1693]" italics="true" pageId="174" pageNumber="175">Dialictus</emphasis>
) 
<emphasis id="2084EADFA7B8FF1909608DBAFC4E039F" box="[864,926,1670,1693]" italics="true" pageId="174" pageNumber="175">sagax</emphasis>
</taxonomicName>
Ƥ 
<number id="A43824EAA7B8FF1909BA8DBBFC17039E" box="[954,967,1671,1692]" pageId="174" pageNumber="175" value="3.0">3</number>
, p. 
<number id="A43824EAA7B8FF1909EF8DB9FBC7039F" box="[1007,1047,1669,1693]" pageId="174" pageNumber="175" value="286.0">286</number>
