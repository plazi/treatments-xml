<paragraph id="7169A607FFF83940E3039C582A05BF91" blockId="5.[806,1421,1648,1700]" pageId="5" pageNumber="301">
<emphasis id="43A27A15FFF83940E3039C582D90BFB0" box="[806,952,1648,1669]" italics="true" pageId="5" pageNumber="301">Composition:</emphasis>
<taxonomicName id="B6D6DD84FFF83940E39A9C582AE2BFB3" authority="Roewer, 1952" authorityName="Roewer" authorityYear="1952" box="[959,1226,1648,1670]" class="Arachnida" family="Gerdesiidae" genus="Gerdesius" kingdom="Animalia" order="Opiliones" pageId="5" pageNumber="301" phylum="Arthropoda" rank="genus">
<emphasis id="43A27A15FFF83940E39A9C582A05BFB0" box="[959,1069,1648,1669]" italics="true" pageId="5" pageNumber="301">Gerdesius</emphasis>
<bibRefCitation id="1547DBF6FFF83940E4109C582AE2BFB3" author="Roewer CF" box="[1077,1226,1648,1670]" pageId="5" pageNumber="301" pagination="37 - 58" refId="ref14277" refString="Roewer CF. 1952. Neotropische Arachnida arthrogastra zumeist aus Peru. Senckenbergiana 33: 37 - 58." type="journal article" year="1952">Roewer, 1952</bibRefCitation>
