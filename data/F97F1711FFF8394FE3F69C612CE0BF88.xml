(
<typeStatus id="AE6D18A5FFF83940E4FC9C582B22BFB0" box="[1241,1290,1648,1669]" pageId="5" pageNumber="301">type</typeStatus>
genus) and 
<taxonomicName id="B6D6DD84FFF83940E3039CA72D94BF91" authority="Bragagnolo &amp; Hara &amp; Pinto-Da-Rocha, 2015" authorityName="Bragagnolo &amp; Hara &amp; Pinto-Da-Rocha" authorityYear="2015" box="[806,956,1679,1700]" class="Arachnida" family="Gerdesiidae" genus="Gonycranaus" kingdom="Animalia" order="Opiliones" pageId="5" pageNumber="301" phylum="Arthropoda" rank="genus" status="gen. nov.">
