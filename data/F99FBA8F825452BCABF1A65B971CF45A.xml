<taxonomicName id="7CE39156886C25585D5ED0E6469FB90D" authorityName="Johnston" authorityYear="1842" class="Demospongea" family="Dysideidae" genus="Dysidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Dysidea" order="Dictyoceratida" pageId="0" pageNumber="73" phylum="Porifera" rank="genus">
