<collectingRegion id="65079AC2FFD14C4EFE0E5F9CFDCAFA70" box="[457,573,1485,1511]" country="United States of America" name="California" pageId="4">California</collectingRegion>
, Riverside, 
<collectingRegion id="65079AC2FFD14C4EFD055F9CFCC1FA70" box="[706,822,1485,1511]" country="United States of America" name="California" pageId="4">California</collectingRegion>
, 
<collectingCountry id="DFD414B0FFD14C4EFC855F9CFC89FA70" box="[834,894,1485,1511]" name="United States of America" pageId="4">USA</collectingCountry>
