<emphasis id="B92BEAFEC549C255FD27FB2807A4F9F4" bold="true" box="[654,744,1604,1628]" pageId="5" pageNumber="474">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="4C5F4D6FC549C255FCB7FB280154F9F4" box="[798,1048,1604,1628]" class="Gastropoda" family="Palaeozygopleuridae" genus="Cimrmaniela" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Murchisoniina" pageId="5" pageNumber="474" phylum="Mollusca" rank="species" species="smoljaki" status="sp. nov.">
<emphasis id="B92BEAFEC549C255FCB7FB280154F9F4" box="[798,1048,1604,1628]" italics="true" pageId="5" pageNumber="474">Cimrmaniela smoljaki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2185785C549C255FBB6FB280135F9F4" box="[1055,1145,1604,1628]" pageId="5" pageNumber="474" rank="species">
<emphasis id="B92BEAFEC549C255FBB6FB280135F9F4" bold="true" box="[1055,1145,1604,1628]" pageId="5" pageNumber="474">sp. nov.</emphasis>
</taxonomicNameLabel>
</paragraph>
