</taxonomicName>
and 
<taxonomicName id="4C467BAAFF8EFFA2FE29FD47686CFD5A" authorityName="Caroline Greve" authorityYear="2013" box="[384,552,724,750]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="caxiuanensis">
