</paragraph>
<paragraph id="8BDB1745FFF4FFFDFF411B23FDF7D80B" blockId="29.[151,1436,1734,2013]" box="[151,522,1807,1833]" pageId="29" pageNumber="30">
<taxonomicName id="4C646CC6FFF4FFFDFF411B23FEA2D80B" baseAuthorityName="Gredler" baseAuthorityYear="1859" box="[151,351,1807,1833]" class="Gastropoda" family="Planorbidae" genus="Gyraulus" kingdom="Animalia" order="Hygrophila" pageId="29" pageNumber="30" phylum="Mollusca" rank="species" species="gredleri">
