<bibRefCitation id="EFF56AB4FFF4FFFDFEB81B23FDFBD80B" author="Hesse, P." box="[366,518,1807,1833]" pageId="29" pageNumber="30" pagination="230 - 240" refId="ref47674" refString="Hesse, P. (1929) Schnecken aus dem nordlichen Serbien. Archiv fur Molluskenkunde, 61 (1), 230 - 240." type="journal article" year="1929">
<collectingRegion id="49A0D9A7FFF4FFFDFEB81B23FE4CD80B" box="[366,433,1807,1833]" country="Germany" name="Hessen" pageId="29" pageNumber="30">Hesse</collectingRegion>
