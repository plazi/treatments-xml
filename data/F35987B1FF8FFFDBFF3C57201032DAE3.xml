<quantity id="BC089B42FF8EFFDBFDD452D5178BDB94" box="[639,728,151,176]" metricMagnitude="3" metricUnit="m" metricValue="2.455" pageId="13" pageNumber="96" unit="m" value="2455.0">2455 m</quantity>
