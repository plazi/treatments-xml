<emphasis id="B953E614D853FFC7BF96E5262952FE91" bold="true" box="[192,324,323,347]" pageId="71" pageNumber="70">Remarks.</emphasis>
<taxonomicName id="4C274185D853FFC7BE1DE52629FEFE91" authorityName="Gordon and Hanley" authorityYear="2017" box="[331,488,323,347]" class="Insecta" family="Coccinellidae" genus="Prodilis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="70" phylum="Arthropoda" rank="species" species="cora">
<emphasis id="B953E614D853FFC7BE1DE52629FEFE91" box="[331,488,323,347]" italics="true" pageId="71" pageNumber="70">Prodilis cora</emphasis>
</taxonomicName>
