<paragraph id="8B8C36EAFFC86164FFF8FC5FFEE2FBDD" blockId="9.[113,648,970,1073]" pageId="9" pageNumber="96">
<taxonomicName id="4C334D69FFC86164FFF8FC5FFF44FC0E" authorityName="Potonie" authorityYear="1956" box="[113,252,970,994]" class="Lycopodiopsida" family="Selaginellaceae" genus="Neoraistrickia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Selaginellales" pageId="9" pageNumber="96" phylum="Tracheophyta" rank="genus">
