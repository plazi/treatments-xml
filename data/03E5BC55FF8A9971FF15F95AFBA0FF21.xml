<taxonomicName id="4C4C76C0FF88997EFEE3F937FDDCF924" box="[353,554,1736,1759]" class="Insecta" family="Formicidae" genus="Parvimyrma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="sangi">
<emphasis id="B938D151FF88997EFEE3F937FDDCF924" box="[353,554,1736,1759]" italics="true" pageId="7" pageNumber="46">
Parvimyrma 
<taxonomicName id="4C4C76C0FF88997EFE72F937FDDCF924" box="[496,554,1736,1759]" pageId="7" pageNumber="46" rank="species" species="sangi">sangi</taxonomicName>
</emphasis>
