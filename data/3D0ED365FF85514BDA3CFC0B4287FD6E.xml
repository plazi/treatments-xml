<taxonomicName id="72A719F0FF855148DA3CFC0B473BFC45" authority="(Walsh)" baseAuthorityName="Walsh" baseAuthorityYear="1862" box="[835,1139,971,995]" class="Insecta" family="Perlidae" genus="Perlesta" kingdom="Animalia" order="Plecoptera" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="decipiens">
<emphasis id="87D3BE61FF855148DA3CFC0B4740FC45" box="[835,1032,971,995]" italics="true" pageId="9" pageNumber="40">Perlesta decipiens</emphasis>
