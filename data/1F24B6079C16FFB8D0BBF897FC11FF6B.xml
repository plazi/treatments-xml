<taxonomicName id="508D7C929C16FFB9D293F8ECFCF1F831" box="[703,892,1910,1935]" class="Insecta" family="Cecidomyiidae" genus="Peromyia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="552" phylum="Arthropoda" rank="species" species="gotohi">
