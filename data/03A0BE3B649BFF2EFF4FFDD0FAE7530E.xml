<collectingCountry id="F31E4FBD649BFF2EFD36FCE3FCF254E6" box="[750,819,885,908]" name="South Korea" pageId="145" pageNumber="146">Korea</collectingCountry>
as 
<taxonomicName id="4C0974AE649BFF2EFC82FCE3FC1A54E6" authorityName="Gray" authorityYear="1846" box="[858,987,885,908]" class="Mammalia" family="Delphinidae" genus="Steno" kingdom="Animalia" order="Cetacea" pageId="145" pageNumber="146" phylum="Chordata" rank="species" species="rostratus">
