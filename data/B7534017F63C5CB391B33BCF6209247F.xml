<normalizedToken id="F684F2911F4A677C4313D93E1715DA8A" originalValue="“Kadjudju”">&quot;Kadjudju&quot;</normalizedToken>
[Kajuju, 
<geoCoordinate id="DF517ADA23BC96DF40A1F99DD5DFE255" degrees="02" direction="south" minutes="09" orientation="latitude" precision="925" value="-2.15">02°09'S</geoCoordinate>
, 
<geoCoordinate id="FD7068E3924A03DCF2D1F4B0A6800DDC" degrees="28" direction="east" minutes="54" orientation="longitude" precision="925" value="28.9">28°54'E</geoCoordinate>
