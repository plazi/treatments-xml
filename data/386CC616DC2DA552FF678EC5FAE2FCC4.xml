<taxonomicName id="77C50C83DC12A551FE6B8DF2FE3FF997" authority="Haeckel, 1872" authorityName="Haeckel" authorityYear="1872" box="[411,497,1528,1550]" pageId="96" pageNumber="97" rank="species" species="pulvinar">
