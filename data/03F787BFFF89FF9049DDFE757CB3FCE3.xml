4. 
<taxonomicName id="4C5E4D2AFF89FF9349BBFE757C3BFE66" authority="Liebherr" authorityName="Liebherr" authorityYear="2017" box="[216,637,444,465]" class="Insecta" family="Carabidae" genus="Mecyclothorax" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="687" phylum="Arthropoda" rank="species" species="medioconstrictus" status="new species">
