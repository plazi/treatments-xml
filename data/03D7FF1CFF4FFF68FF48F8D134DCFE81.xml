<taxonomicName id="4C7E3589FF4EFF68FC4DFBB735D2FF01" baseAuthorityName="Le Peletier and Serville" baseAuthorityYear="1825" box="[965,1198,217,241]" class="Insecta" family="Rutelidae" genus="Chlorota" kingdom="Animalia" order="Coleoptera" pageId="133" pageNumber="132" phylum="Arthropoda" rank="species" species="terminata">
