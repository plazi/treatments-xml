<taxonomicName id="09FC4D14FFF4FFC0FE3CF9C4E25BF9DA" authority="BLASZAK, 1984: 596" authorityName="BLASZAK" authorityPageNumber="596" authorityYear="1984" box="[454,849,1585,1606]" class="Arachnida" family="Zerconidae" genus="Allozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="8" pageNumber="283" phylum="Arthropoda" rank="species" species="pulcher">
