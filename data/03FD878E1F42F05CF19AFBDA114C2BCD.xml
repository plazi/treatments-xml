<collectingCountry id="F34376081F41F05CF06AFE7014EF2BCD" box="[359,439,404,429]" name="Turkey" pageId="11" pageNumber="512">Turkey</collectingCountry>
(Gulf of 
<collectingRegion id="4990F87A1F41F05CF32DFE7017262BCD" box="[544,638,404,429]" country="Turkey" name="Antalya" pageId="11" pageNumber="512">Antalya</collectingRegion>
