<taxonomicName id="1CA04D337F01FFC32710FF1EFC8F23DC" baseAuthorityName="Cameron" baseAuthorityYear="1906" box="[818,876,223,246]" class="Insecta" family="Vespidae" genus="Asteloeca" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="traili">
