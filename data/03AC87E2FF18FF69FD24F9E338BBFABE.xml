<materialsCitation id="3B6D3CA9FF1FFF69FF85FEC839BAFE79" collectionCode="AMNH" country="Australia" county="Mathews Collection" location="Mathews" municipality="From" pageId="164" pageNumber="164" specimenCode="AMNH 696558" specimenCount="1" stateProvince="Queensland" typeStatus="holotype">
<typeStatus id="54BE8856FF1FFF69FF85FEC8399DFEC2" box="[122,250,335,359]" pageId="164" pageNumber="164">HOLOTYPE</typeStatus>
: 
<emphasis id="B971EAE6FF1FFF69FEF7FED638DCFEC2" bold="true" box="[264,443,337,358]" pageId="164" pageNumber="164">
<specimenCode id="DBA39E8FFF1FFF69FEF7FED638DCFEC2" box="[264,443,337,358]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="164" pageNumber="164">AMNH 696558</specimenCode>
</emphasis>
, said by 
<location id="8EDA602FFF1FFF69FDDEFED639E0FE20" LSID="urn:lsid:plazi:treatment:03AC87E2FF18FF69FD24F9E338BBFABE:8EDA602FFF1FFF69FDDEFED639E0FE20" country="Australia" county="Mathews Collection" municipality="From" name="Mathews" pageId="164" pageNumber="164" stateProvince="Queensland">Mathews</location>
