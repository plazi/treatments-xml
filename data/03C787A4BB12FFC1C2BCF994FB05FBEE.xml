<location id="8EB16069BB12FFC1C688FC32FA24FBD2" LSID="urn:lsid:plazi:treatment:03C787A4BB12FFC1C2BCF994FB05FBEE:8EB16069BB12FFC1C688FC32FA24FBD2" box="[1230,1443,1012,1036]" country="Armenia" county="eastern Pontic Mountains. The" municipality="Northern" name="Pontic Mountains" pageId="5" pageNumber="123" stateProvince="Kars">Pontic Mountains</location>
