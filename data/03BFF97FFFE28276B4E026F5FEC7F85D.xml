<subSubSection id="C30C1BE2FFE38276B4B0234CFE07F8C9" box="[199,506,1859,1886]" pageId="5" pageNumber="192" type="etymology">
<paragraph id="8BA94869FFE38276B4B0234CFE07F8C9" blockId="5.[151,1437,1715,1994]" box="[199,506,1859,1886]" pageId="5" pageNumber="192">
<emphasis id="B962947BFFE38276B4B0234CFEB3F8CA" bold="true" box="[199,334,1860,1886]" pageId="5" pageNumber="192">Etymology.</emphasis>
After 
<collectingRegion id="49D2868BFFE38276B5E2234BFE0BF8C9" box="[405,502,1859,1885]" country="Indonesia" name="Sumatera Utara" pageId="5" pageNumber="192">Sumatra</collectingRegion>
