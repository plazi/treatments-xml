<subSubSection id="75077E3FA377CE084677597281E4E630" pageId="76" pageNumber="77" type="names">
<paragraph id="44B7D2AE9EFC048FECC7E0C0DCB113D7" pageId="76" pageNumber="77">Names.</paragraph>
<paragraph id="F15D4E9A5A35EE1EE199A0AC66184C13" pageId="76" pageNumber="77">
<emphasis id="39068613D6EBB9E1AC65BC198E343A82" bold="true" pageId="76" pageNumber="77">Myanmar</emphasis>
: 
<emphasis id="622BC7E710C519B4409EB418A7A8C20D" italics="true" pageId="76" pageNumber="77">babu</emphasis>
, 
<emphasis id="013EADF14BCAC9033AC8BADEBEAAA369" italics="true" pageId="76" pageNumber="77">babul</emphasis>
, 
<emphasis id="C4590AA711C90B236D8597E68FBC4FE7" italics="true" pageId="76" pageNumber="77">subyu</emphasis>
. 
<emphasis id="04BABE8B72CAF20F87624AA93031ACFB" bold="true" pageId="76" pageNumber="77">English</emphasis>
