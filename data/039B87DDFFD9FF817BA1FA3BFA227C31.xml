<number id="3DFA24ECFFD9FF8E7BE8FA66FF517BD5" box="[222,233,1472,1496]" pageId="7" value="1.0">1</number>
</figureCitation>
E, 
<number id="3DFA24ECFFD9FF8E7A3AFA66FEA27BD5" box="[268,282,1472,1496]" pageId="7" value="2.0">2</number>
F, 
<number id="3DFA24ECFFD9FF8E7A03FA66FEFA7BD5" box="[309,322,1472,1496]" pageId="7" value="3.0">3</number>
E, 
<number id="3DFA24ECFFD9FF8E7A54FA19FEC97BD5" box="[354,369,1471,1496]" pageId="7" value="6.0">6</number>
)
</paragraph>
