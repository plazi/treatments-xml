<taxonomicName id="4C6149F2FFFBFF8EFE3B72B2FD449BE4" box="[386,668,1459,1482]" class="Diplopoda" family="Dorypetalidae" genus="Dorypetalum" kingdom="Animalia" order="Callipodida" pageId="8" pageNumber="37" phylum="Arthropoda" rank="species" species="bosporanum">
