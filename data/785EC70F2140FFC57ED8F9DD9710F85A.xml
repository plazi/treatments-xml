</taxonomicName>
(= 
<taxonomicName id="37F70D9A2140FFC57C70F80B9489F812" box="[575,725,1930,1955]" class="Arachnida" family="Withiidae" genus="Withius" kingdom="Animalia" order="Pseudoscorpiones" pageId="20" pageNumber="57" phylum="Arthropoda" rank="species" species="piger">
