, 
<collectingCountry id="F37B7661FFA6FFC9CEE6F965FCADF935" box="[763,853,1745,1766]" name="Germany" pageId="10" pageNumber="411">Germany</collectingCountry>
, 
<collectingCountry id="F37B7661FFA6FFC9CF43F965FC2BF935" box="[862,979,1745,1766]" name="Switzerland" pageId="10" pageNumber="411">Switzerland</collectingCountry>
, 
<collectingCountry id="F37B7661FFA6FFC9CFC0F965FBDCF935" box="[989,1060,1745,1766]" name="Austria" pageId="10" pageNumber="411">Austria</collectingCountry>
........... 
