<taxonomicName id="848385CAA1567BBC3D85BE1562B82286" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rumex" order="Caryophyllales" pageId="6" pageNumber="101" phylum="Tracheophyta" rank="genus">Rumex</taxonomicName>
