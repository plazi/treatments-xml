<collectionCode id="84BFAE57BA77FFAEAC13F9A21ADEF244" box="[890,988,1551,1577]" country="United Kingdom" httpUri="http://grbio.org/cool/e7dr-nxad" name="National Museum of Wales" pageId="19" pageNumber="20" type="Museum">NMWC</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAC81F9A21D31F244" box="[1000,1075,1551,1577]" country="United Kingdom" lsid="urn:lsid:biocol.org:col:14908" name="University of Nottingham" pageId="19" pageNumber="20" type="Herbarium">NHM</collectionCode>
