<collectionCode id="84BFAE57BA77FFAEAA0EFA4A1C95F26C" box="[1383,1431,1511,1537]" country="0" name="Museo de Cipolleti" pageId="19" pageNumber="20">MC</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAFFEF9A219C0F244" box="[151,194,1551,1577]" pageId="19" pageNumber="20">NC</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAFA7F9A219FCF244" box="[206,254,1551,1577]" country="Portugal" name="Universidade de Lisboa, Museu Bocage" pageId="19" pageNumber="20">MB</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAE63F9A2183EF244" box="[266,316,1551,1577]" country="Kenya" name="National Museum of Kenya" pageId="19" pageNumber="20">MK</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAE21F9A2187EF244" box="[328,380,1551,1577]" pageId="19" pageNumber="20">WD</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAEE1F9A218BBF244" box="[392,441,1551,1577]" pageId="19" pageNumber="20">WD</collectionCode>
&amp; SL (South Island) [
<collectionCode id="84BFAE57BA77FFAEADD5F9A21A10F244" box="[700,786,1551,1577]" country="New Zealand" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" lsid="urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="19" pageNumber="20" type="Other institutional type">NZAC</collectionCode>
, 
<collectionCode id="84BFAE57BA77FFAEAC74F9A21A6CF244" box="[797,878,1551,1577]" country="New Zealand" httpUri="http://grbio.org/cool/n5zm-bhpt" name="Lincoln University Entomology Research Museum" pageId="19" pageNumber="20">LUNZ</collectionCode>
