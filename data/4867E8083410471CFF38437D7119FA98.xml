Latin 
<taxonomicName id="07CE229D3410471CFE0B46A87724FA9B" authorityName="Morassi &amp; Bonfitto" authorityYear="2010" box="[420,517,1272,1297]" class="Gastropoda" family="Raphitomidae" genus="Acanthodaphne" kingdom="Animalia" order="Neogastropoda" pageId="11" pageNumber="65" phylum="Mollusca" rank="species" species="pungens">
