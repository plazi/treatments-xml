<taxonomicName id="4C514D3CFFBEFFABFF046280FDBCFB43" box="[233,540,1198,1220]" class="Insecta" family="Staphylinidae" genus="Cephennomicrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="420" phylum="Arthropoda" rank="species" species="transkeianus" status="sp. n.">
<emphasis id="B925EAADFFBEFFABFF046280FDBCFB43" box="[233,540,1198,1220]" italics="true" pageId="9" pageNumber="420">Cephennomicrus transkeianus</emphasis>
</taxonomicName>
<emphasis id="B925EAADFFBEFFABFDCE6280FDFBFB44" bold="true" box="[547,603,1198,1219]" pageId="9" pageNumber="420">
<taxonomicNameLabel id="A21657D6FFBEFFABFDCE6280FDFBFB44" box="[547,603,1198,1219]" pageId="9" pageNumber="420" rank="species">sp. n.</taxonomicNameLabel>
