<taxonomicName id="4C2F0DB1E279FFA7228FC2B6FE1CA5DA" authority="Salem, 1946" authorityName="Salem" authorityYear="1946" box="[151,504,1735,1757]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="166" phylum="Arthropoda" rank="species" species="longifilia">
