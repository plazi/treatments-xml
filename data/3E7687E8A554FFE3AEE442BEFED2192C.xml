<taxonomicName id="71DF4D7DA554FFE1AF9343D3FE9D1D94" authorityName="Smith &amp; Shenefelt" authorityYear="1955" box="[160,256,1487,1512]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="1171" phylum="Arthropoda" rank="tribe" tribe="Aptesini">Aptesini</taxonomicName>
