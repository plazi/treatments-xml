(
<bibRefCitation id="D24E4B0FA554FFE1ADA943B0FCAE1DB9" author="Walker F" box="[666,819,1452,1477]" pageId="57" pageNumber="1171" pagination="301 - 310" refId="ref25043" refString="Walker F. 1874. Descriptions of some Japanese Hymenoptera. Cistula Entomol. 1: 301 - 310." type="journal article" year="1874">Walker, 1874</bibRefCitation>
)
</taxonomicName>
(
<taxonomicName id="71DF4D7DA554FFE1AC7D43B0FB9C1DB9" authorityName="Latreille" authorityYear="1802" box="[846,1025,1452,1477]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="1171" phylum="Arthropoda" rank="family">Ichneumonidae</taxonomicName>
: 
<taxonomicName id="71DF4D7DA554FFE1AB2143B0FB1C1DB9" box="[1042,1153,1452,1477]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="1171" phylum="Arthropoda" rank="subFamily" subFamily="Cryptinae">Cryptinae</taxonomicName>
