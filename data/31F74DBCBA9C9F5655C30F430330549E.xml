<taxonomicName id="5D3C8D673DA0F6910C64B8431650AEAE" class="Gastropoda" family="Achatinidae" genus="Achatina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Achatina ceylanica" order="Stylommatophora" pageId="0" pageNumber="129" phylum="Mollusca" rank="species" species="ceylanica">Achatina ceylanica</taxonomicName>
