<taxonomicName id="4C3F4D220D37E0727E502653606DFEED" authority="Lamouroux, 1812" authorityName="Lamouroux" authorityYear="1812" box="[871,1148,259,285]" class="Hydrozoa" family="Campanulariidae" genus="Clytia" kingdom="Animalia" order="Leptothecata" pageId="19" pageNumber="20" phylum="Cnidaria" rank="genus">
<emphasis id="B94BEAB30D37E0727E50265367BAFEED" box="[871,939,259,285]" italics="true" pageId="19" pageNumber="20">Clytia</emphasis>
