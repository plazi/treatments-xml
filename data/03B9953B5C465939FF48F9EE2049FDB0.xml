<emphasis id="B964F83F5C455939FCDFFEDD2235FEB5" box="[768,888,260,285]" italics="true" pageId="98" pageNumber="99">acutifoliae</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C105FAE5C455939FC59FEDC254DFEB4" authorityName=": Viitasaari &amp; Vikberg" authorityYear="1985" box="[902,1024,261,284]" class="Insecta" family="Tenthredinidae" genus="Pontania" kingdom="Animalia" order="Hymenoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="arcticornis">
<emphasis id="B964F83F5C455939FC59FEDC254DFEB4" box="[902,1024,261,284]" italics="true" pageId="98" pageNumber="99">arcticornis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C105FAE5C455939FBD2FEDD25C6FEB5" authorityName=": Benes" authorityYear="2015" box="[1037,1163,260,285]" class="Insecta" family="Tenthredinidae" genus="Euura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="brevicornis">
