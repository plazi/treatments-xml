<taxonomicName id="27CA4D35FE91FEF75B0BF94FFC6967A2" authorityName="Rondani" authorityYear="1861" box="[789,986,1730,1755]" class="Insecta" family="Tachinidae" genus="Prosena" kingdom="Animalia" order="Diptera" pageId="348" pageNumber="349" phylum="Arthropoda" rank="species" species="epicurea">
