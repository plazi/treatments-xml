<quantity id="4CE19B0DFF806020786DF9FFFC79F9EB" box="[849,923,1596,1622]" metricMagnitude="4" metricUnit="m" metricValue="4.6" pageId="4" pageNumber="255" unit="km" value="46.0">46 km</quantity>
north of 
<location id="8EC66033FF8060207F35F9FFFB98F9EB" LSID="urn:lsid:plazi:treatment:03B087FEFF8660217BABF858FCBEFD3A:8EC66033FF8060207F35F9FFFB98F9EB" box="[1033,1146,1596,1622]" country="Chile" municipality="Playa Agua" name="Los Vilos" pageId="4" pageNumber="255" stateProvince="Coquimbo">Los Vilos</location>
, 
<date id="FFA71028FF8060207FB4F9F8FAE9F9EB" box="[1160,1291,1595,1622]" pageId="4" pageNumber="255" value="1988-11-05" valueMax="1988-11-06" valueMin="1988-11-05">
<collectingDate id="EFE3E9C0FF8060207FB4F9F8FAE9F9EB" box="[1160,1291,1595,1622]" pageId="4" pageNumber="255" value="1988-11-05" valueMax="1988-11-06" valueMin="1988-11-05">5-6.xi.1988</collectingDate>
</date>
, 
<collectorName id="26EC533EFF8060207E25F9FFFA7EF9EB" box="[1305,1436,1596,1622]" pageId="4" pageNumber="255">P. Goloboff</collectorName>
