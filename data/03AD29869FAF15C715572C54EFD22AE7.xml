[[ 
<taxonomicName id="DFD1077E1DF3CABABB24B505755CC380" LSID-ZBK="urn:lsid:zoobank.org:act:2360C094-A0AC-48DE-A5F5-D277750FE7A1" class="Elasmobranchii" family="Dasyatidae" genus="Dasyatis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Dasyatis Rafinesque 1810:16" order="Myliobatiformes" pageNumber="1" phylum="Chordata" rank="genus">Dasyatis Rafinesque</taxonomicName>
