<emphasis id="BF911C7F712D235565DDF91CFEC5F97A" bold="true" box="[151,282,1695,1719]" pageId="54" pageNumber="55">FIGURE 27</emphasis>
. 
<taxonomicName id="4AE5BBEE712D23556462F91CFDF9F97A" authority="Blake, 2022" authorityName="Blake" authorityYear="2022" box="[296,550,1695,1719]" class="Polychaeta" family="Cirratulidae" genus="Chaetozone" kingdom="Animalia" order="Terebellida" pageId="54" pageNumber="55" phylum="Annelida" rank="species" species="novagracilis" status="n. sp.">
<emphasis id="BF911C7F712D23556462F91CFDF9F97A" box="[296,550,1695,1719]" italics="true" pageId="54" pageNumber="55">Chaetozone novagracilis</emphasis>
</taxonomicName>
