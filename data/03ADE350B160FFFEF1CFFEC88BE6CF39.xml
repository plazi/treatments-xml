: 
<taxonomicName id="4C0429C5B167FFFEF349FDB78BAECE59" authority="(Hochst) Planch." authorityName="Planch." baseAuthorityName="Hochst" box="[529,996,548,572]" class="Magnoliopsida" family="Malvaceae" genus="Dombeya" kingdom="Plantae" order="Malvales" pageId="116" pageNumber="117" phylum="Tracheophyta" rank="species" species="rotundifolia">
