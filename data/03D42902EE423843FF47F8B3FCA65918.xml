<taxonomicName id="4C7DE397EE423840FEF6F804FDAC58F5" authority="Pascoe, 1885" authorityName="Pascoe" authorityYear="1885" box="[294,595,1945,1967]" class="Insecta" family="Dryophthoridae" genus="Nephius" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="382" phylum="Arthropoda" rank="species" species="gestroi">
