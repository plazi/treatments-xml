<paragraph id="1E1236950E7FFFDDFF06A8DBFD65FC0C" blockId="68.[143,664,940,993]" box="[143,664,971,993]" pageId="68" pageNumber="209">
Syn. to 
<taxonomicName id="D9AD4D160E7FFFDDFF6EA8DBFE11FC0C" authority="(Motas, 1927)" baseAuthorityName="MOTAS" baseAuthorityYear="1927" box="[231,492,971,993]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="68" pageNumber="209" phylum="Arthropoda" rank="species" species="legeri">
