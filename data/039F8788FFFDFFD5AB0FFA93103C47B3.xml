<emphasis id="B942EA8CFFFDFFD5A821F8A910F5472B" box="[309,448,1882,1907]" italics="true" pageId="29" pageNumber="30">Pinus mugo</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C364D1DFFFDFFD5A8DBF8AA1309472B" box="[463,572,1881,1907]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="sylvestris">
