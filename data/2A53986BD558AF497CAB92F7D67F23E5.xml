<paragraph id="931DB521A16DF87770113190CA4B2E8A" pageId="0" pageNumber="9042">
<taxonomicName id="5995818DA3FC737F589C100455FE1F04" class="Insecta" family="Ichneumonidae" genus="Ichneumonoidea" kingdom="Animalia" lsidName="Ichneumonoidea merdarius" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="merdarius">merdarius</taxonomicName>
