<paragraph id="8BBD9079FFF8F60EFF44FDDC7DFDFD9B" blockId="23.[164,779,524,1525]" box="[164,427,524,545]" pageId="23" pageNumber="700">
<emphasis id="B9764C6BFFF8F60EFF44FDDC7DFDFD9B" box="[164,427,524,545]" italics="true" pageId="23" pageNumber="700">Skeletal characteristics</emphasis>
</paragraph>
<paragraph id="8BBD9079FFF8F60EFF44FDFB7D12FCEE" blockId="23.[164,779,524,1525]" pageId="23" pageNumber="700">
<emphasis id="B9764C6BFFF8F60EFF44FDFB7D41FDFA" box="[164,279,555,576]" italics="true" pageId="23" pageNumber="700">
<taxonomicName id="4C02EBFAFFF8F60EFF44FDFB7D45FDFA" authorityName="Ehrenberg" authorityYear="1834" box="[164,275,555,576]" class="Cnidaria (awaiting allocation)" family="Cnidaria (awaiting allocation)" genus="Corallum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cnidaria (awaiting allocation)" pageId="23" pageNumber="700" phylum="Cnidaria" rank="genus">Corallum</taxonomicName>
