<taxonomicName id="D0D34D56FFDBFFF307CDFB8FF4EBFB84" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[1224,1436,1053,1079]" class="Insecta" family="Apidae" genus="Nomada" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="species" species="ruficornis">
