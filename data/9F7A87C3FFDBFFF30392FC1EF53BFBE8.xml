<taxonomicName id="D0D34D56FFDBFFF302A3FB8FF338FB84" baseAuthorityName="Fabricius" baseAuthorityYear="1781" box="[422,591,1053,1079]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="species" species="haemorrhoa">
