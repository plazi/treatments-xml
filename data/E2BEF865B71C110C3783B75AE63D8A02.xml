<subSubSection id="834D71F2210DC11D6CFE0A94C5709C84" pageId="null" pageNumber="590" type="vernacular_name">
<paragraph id="9DF240F1A2658BE1CEF447CDF3A77308" pageId="null" pageNumber="590">Mauerlattich</paragraph>
