<subSubSection id="62FEB6D9FF86EB2FFF70077D33D0F887" box="[199,1215,1820,1845]" pageId="9" pageNumber="235" type="etymology">
<paragraph id="2A5BE552FF86EB2FFF70077D33D0F887" blockId="9.[151,1437,1460,1953]" box="[199,1215,1820,1845]" pageId="9" pageNumber="235">
<emphasis id="18903940FF86EB2FFF70077D3621F887" bold="true" box="[199,334,1820,1845]" pageId="9" pageNumber="235">Etymology.</emphasis>
