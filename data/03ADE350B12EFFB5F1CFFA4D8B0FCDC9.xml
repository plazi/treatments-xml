(
<figureCitation id="133F4EC3B12EFFB7F1C7F99288BFCA7C" box="[159,245,1537,1561]" captionStart="FIGURES 33–38" captionStartId="193.[151,262,1818,1841]" captionTargetBox="[219,1368,232,1802]" captionTargetId="figure-146@193.[219,1370,181,1802]" captionTargetPageId="193" captionText="FIGURES 33–38. Adults. Scale bar 1 mm. 33, Cameraria perodeaui, holotype ♀, DRC, Bas-Congo, Luki-Mayumbe NR, 16.v.2007, leg. J. &amp; W. De Prins, specimen ID: RMCA ENT 000004799. 34, Cameraria perodeaui, paratype ♀, DRC, Bas-Congo, Luki-Mayumbe NR, 23.v.2007, leg. J. &amp; W. De Prins, specimen ID: RMCA ENT 000004798. 35, Cameraria sokoke, holotype ♂, Kenya, Arabuko Sokoke Forest, 30.iii.2004, leg. J. De Prins, specimen ID: RMCA ENT 000003275. 36, Cameraria zaira, holotype ♂, DRC, Ht. Katanga, Panda, 04.ii.1930, leg. J. Romieux, in MHNG. 37, Cameraria torridella, holotype ♂, Kenya, Rift Valley, Turi, 02.xii.1998, leg. D.J.L. Agassiz, specimen ID: RMCA ENT 000003121. 38, Phyllonorycter achilleus, holotype ♂, Kenya, Rift Valley, Turi, ex mine 16.x.1999, leg. D.J.L. Agassiz, specimen ID: RMCA ENT 000003125." figureDoi="http://doi.org/10.5281/zenodo.6052161" httpUri="https://zenodo.org/record/6052161/files/figure.png" pageId="61" pageNumber="62">Figs 38</figureCitation>
, 
<figureCitation id="133F4EC3B12EFFB7F059F992892FCA7C" box="[257,357,1537,1561]" captionStart="FIGURES 170–175" captionStartId="217.[151,264,1954,1977]" captionTargetBox="[343,1259,184,1943]" captionTargetId="figure-65@217.[328,1259,169,1955]" captionTargetPageId="217" captionText="FIGURES 170–175. Male genitalia. Scale bar 100 µm. 170–172, Phyllonorycter achilleus, holotype, genitalia prep. MRAC/ KMMA 00381, in RMCA. 170, ventral view. 171, valva. 172, aedoeagus. 173–175, Phyllonorycter adderis, holotype, genitalia prep. MRAC/KMMA 00462, in RMCA. 173, ventral view. 174, sternum VIII. 175, aedoeagus." figureDoi="http://doi.org/10.5281/zenodo.6052260" httpUri="https://zenodo.org/record/6052260/files/figure.png" pageId="61" pageNumber="62">170–172</figureCitation>
, 
<figureCitation id="133F4EC3B12EFFB7F029F99289D7CA7C" box="[369,413,1537,1561]" captionStart="FIGURES 303–305" captionStartId="249.[151,263,1769,1792]" captionTargetBox="[182,1409,197,1742]" captionTargetId="figure-56@249.[151,1436,193,1748]" captionTargetPageId="249" captionText="FIGURES 303–305. Female genitalia. Scale bar 200 µm. 303–304, Cameraria torridella, paratype, genitalia prep. MRAC/ KMMA 00291, in RMCA. 303, corpus bursae. 304, segments VII–X, ventral view. 305, Phyllonorycter achilleus, paratype, genitalia prep. MRAC/KMMA 00380, in RMCA." figureDoi="http://doi.org/10.5281/zenodo.6052390" httpUri="https://zenodo.org/record/6052390/files/figure.png" pageId="61" pageNumber="62">305</figureCitation>
