<collectionCode id="ED5742DEB004FFB5FA0DFBA67181FB96" box="[1420,1451,1089,1112]" country="Czech Republic" name="Západoceské muzeum v Plzni" pageId="1" pageNumber="124" type="Museum">PL</collectionCode>
<quantity id="4CBE77FEB004FFB5FCACFB8677BBFBB6" box="[813,913,1121,1145]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="1" pageNumber="124" unit="mm" value="0.7">0.70 mm</quantity>
