<quantity id="4CBE77FEB004FFB5FBEBFB8670E4FBB6" box="[1130,1230,1121,1145]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="1" pageNumber="124" unit="mm" value="2.3">2.30 mm</quantity>
