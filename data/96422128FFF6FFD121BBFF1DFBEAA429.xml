<taxonomicName id="D9EBEBBDFFF6FFD121BBFA9DFC10A7B9" authority="(Strobl) Pignatti (1973: 218)" authorityName="Pignatti" authorityPageNumber="218" authorityYear="1973" baseAuthorityName="Strobl" box="[136,972,1396,1423]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="1" pageNumber="212" phylum="Tracheophyta" rank="subSpecies" species="pratense" subSpecies="semipurpureum">
<emphasis id="2C9F4C2CFFF6FFD121BBFA9DFEB8A7B9" bold="true" box="[136,356,1396,1423]" italics="true" pageId="1" pageNumber="212">Trifolium pratense</emphasis>
subsp. 
<emphasis id="2C9F4C2CFFF6FFD1208CFA9DFDABA7B9" bold="true" box="[447,631,1396,1422]" italics="true" pageId="1" pageNumber="212">semipurpureum</emphasis>
(Strobl) 
