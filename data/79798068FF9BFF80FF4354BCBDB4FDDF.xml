<collectingRegion id="3314FF9CFF9DFF80FF0154EBBF1BFF6F" box="[213,308,192,218]" country="United States of America" name="Virginia" pageId="37" pageNumber="38">Virginia</collectingRegion>
and 
<collectingRegion id="3314FF9CFF9DFF80FEA854EBBC31FF6F" box="[380,542,192,218]" country="United States of America" name="West Virginia" pageId="37" pageNumber="38">West Virginia</collectingRegion>
