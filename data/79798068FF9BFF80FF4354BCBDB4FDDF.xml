<taxonomicName id="36D04AFDFF9DFF80FD0F54EBBD74FF6C" box="[731,859,192,217]" class="Diplopoda" family="Trichopetalidae" genus="Zygonopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chordeumatida" pageId="37" pageNumber="38" phylum="Arthropoda" rank="genus">
<emphasis id="C3A4ED6CFF9DFF80FD0F54EBBD74FF6C" box="[731,859,192,217]" italics="true" pageId="37" pageNumber="38">Zygonopus</emphasis>
</taxonomicName>
