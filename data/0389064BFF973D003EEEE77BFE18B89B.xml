<collectingCountry id="F337F7CDFF943D003C25E161FD0AB925" box="[604,711,548,573]" name="Australia" pageId="110" pageNumber="111">Australia</collectingCountry>
. 
<location id="8EFFE186FF943D003CABE161FCC9B925" LSID="urn:lsid:plazi:treatment:0389064BFF973D003EEEE77BFE18B89B:8EFFE186FF943D003CABE161FCC9B925" box="[722,772,548,573]" country="Australia" county="Admiralty Islands" municipality="Coral Reef" name="This" pageId="110" pageNumber="111" stateProvince="Gulf">This</location>
