<elevation id="000D506EFF943D003F4DE161FE55B924" box="[308,408,548,573]" metricMagnitude="1" metricUnit="m" metricValue="3.65" metricValueMax="4.6" metricValueMin="2.7" pageId="110" pageNumber="111" unit="m" value="36.5" valueMax="46.0" valueMin="27.0">
<quantity id="4CD81AB8FF943D003F4DE161FE55B924" box="[308,408,548,573]" metricMagnitude="1" metricUnit="m" metricValue="3.65" metricValueMax="4.6" metricValueMin="2.7" pageId="110" pageNumber="111" unit="m" value="36.5" valueMax="46.0" valueMin="27.0">27–46 m</quantity>
