<taxonomicName id="4C414D38FEDF7B0EFEF3FBC6F935FBDE" authority="Gonzalez-Sponga, 1999" authorityName="Gonzalez-Sponga" authorityYear="1999" box="[376,878,1097,1124]" class="Arachnida" family="Pholcidae" genus="Priscula" kingdom="Animalia" order="Araneae" pageId="271" pageNumber="272" phylum="Arthropoda" rank="species" species="chejapi">
