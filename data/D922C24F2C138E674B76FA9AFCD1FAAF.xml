<quantity id="9673DEBC2C138E644B85FA62FDA2FA2A" box="[450,531,1424,1445]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="9" pageNumber="308" unit="mm" value="4.1">4.1 mm</quantity>
