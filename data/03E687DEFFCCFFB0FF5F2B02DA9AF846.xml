<taxonomicName id="4C4F4D4BFFCCFFB0FD252A07DBA6FCFA" box="[749,954,858,882]" class="Aves" family="Threskiornithidae" kingdom="Animalia" order="Pelecaniformes" pageId="1" phylum="Chordata" rank="family">Threskiornithidae</taxonomicName>
