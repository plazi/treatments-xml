<taxonomicName id="4C1B4D387F20FF91F5B2FC3EFAA294B9" authorityName="Hanson" authorityYear="1941" box="[1255,1397,987,1011]" class="Insecta" family="Leuctridae" genus="Leuctra" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="170" phylum="Arthropoda" rank="species" species="variabilis">
