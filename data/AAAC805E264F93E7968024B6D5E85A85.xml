<taxonomicName id="4A1AE51CFFDAD7F1E400559BA6A40320" class="Insecta" family="Leucospidae" genus="Leucospis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leucospis bakeri" order="Hymenoptera" pageId="29" pageNumber="136" phylum="Arthropoda" rank="species" species="bakeri">Leucospis bakeri</taxonomicName>
