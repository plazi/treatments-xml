<number id="577924DEFFAB077089BDFAAFECACFAA3" box="[215,245,1302,1326]" pageId="5" pageNumber="92" value="22.0">22</number>
A); 
<number id="577924DEFFAB07708849FAAFED6BFAA3" box="[291,306,1302,1326]" pageId="5" pageNumber="92" value="2.0">2</number>
