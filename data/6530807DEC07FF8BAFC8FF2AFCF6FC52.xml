<taxonomicName id="2A994AE8EC07FF8BAEFDFEC8FDACFE92" authority="G. W. Muller, 1900" authorityName="G. W. Muller" authorityYear="1900" box="[213,630,299,323]" class="Ostracoda" family="Candonidae" genus="Candona" kingdom="Animalia" order="Podocopida" pageId="45" pageNumber="2909" phylum="Arthropoda" rank="species" species="dentata">
