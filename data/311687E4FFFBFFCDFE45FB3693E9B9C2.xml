<typeStatus id="66048850FFF8FFCCFC5DFD9C965FBF5F" box="[980,1069,606,630]" pageId="38" pageNumber="39">holotype</typeStatus>
of 
<taxonomicName id="7EBF4D71FFF8FFCCFBD9FD9C96BABF5F" box="[1104,1224,606,630]" class="Amphibia" family="Hylidae" genus="Myersiohyla" kingdom="Animalia" order="Anura" pageId="38" pageNumber="39" phylum="Chordata" rank="genus">
<emphasis id="8BCBEAE0FFF8FFCCFBD9FD9C96BABF5F" box="[1104,1224,606,630]" italics="true" pageId="38" pageNumber="39">Myersiohyla</emphasis>
</taxonomicName>
</subScript>
loveridgei (
<accessionNumber id="A6ECAB11FFF8FFCCFCADFDB991D6BFBA" box="[804,932,635,659]" httpUri="http://www.ncbi.nlm.nih.gov/protein/MCZ28565" pageId="38" pageNumber="39">MCZ 28565</accessionNumber>
). Scale bar = 
<quantity id="7E479B17FFF8FFCCFBA7FDB9961FBFBB" box="[1070,1133,635,659]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="38" pageNumber="39" unit="mm" value="2.0">2 mm</quantity>
