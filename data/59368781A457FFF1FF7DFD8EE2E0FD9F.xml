<collectingCountry id="A9887607A454FFF0FB03F80CE3BFF864" box="[1257,1431,1951,1977]" name="Argentina" pageId="10" pageNumber="11">ARGENTINA</collectingCountry>
, 
<emphasis id="E3EBEA85A454FFF0FF7DF854E732F83D" box="[151,282,1991,2016]" italics="true" pageId="10" pageNumber="11">Catamarca</emphasis>
, Andalgala, 
<date id="A5211057A454FFF0FE5EF854E404F83C" box="[436,556,1991,2017]" pageId="10" pageNumber="11" value="1972-10-25">
