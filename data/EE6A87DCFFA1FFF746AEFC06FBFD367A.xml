<emphasis id="54B7EAD8FFA1FFF14710F9F9FED1314B" box="[297,311,1558,1581]" italics="true" pageId="3" pageNumber="574">g</emphasis>
<emphasis id="54B7EAD8FFA1FFF1470EF9CBFED93157" box="[311,319,1572,1585]" italics="true" pageId="3" pageNumber="574">
<number id="D00B24EDFFA1FFF1470EF9CBFED93157" box="[311,319,1572,1585]" pageId="3" pageNumber="574" value="6.0">6</number>
</emphasis>
, all setae 
<emphasis id="54B7EAD8FFA1FFF14794F9F9FE5D314B" box="[429,443,1558,1581]" italics="true" pageId="3" pageNumber="574">g</emphasis>
<emphasis id="54B7EAD8FFA1FFF14782F9CBFE363157" box="[443,464,1572,1585]" italics="true" pageId="3" pageNumber="574">
<number id="D00B24EDFFA1FFF14782F9CBFE363157" box="[443,464,1572,1585]" pageId="3" pageNumber="574" value="7.5" valueMax="9.0" valueMin="6.0">6-9</number>
