<collectionCode id="ED06AE30FFCAF53E1351FCAA83D34E14" box="[844,925,770,796]" httpUri="http://grbio.org/cool/x2t0-vyyc" name="Departamento de Zoologia, Universidad de La Laguna" pageId="4" pageNumber="45">DZUL</collectionCode>
