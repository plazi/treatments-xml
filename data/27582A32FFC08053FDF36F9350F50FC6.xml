</emphasis>
<typeStatus id="704A2586FFC18053FCEB6E48509E0F57" box="[399,503,332,356]" pageId="7" pageNumber="188">Holotype</typeStatus>
male from 
<collectingCountry id="D7E6DBB4FFC18053FFF36E48524B0F57" box="[663,802,332,356]" name="Ecuador" pageId="7" pageNumber="188">ECUADOR</collectingCountry>
, 
