: 
<taxonomicName id="4C329CF0FF7B13D19DEEF9DBFC91F987" baseAuthorityName="Halbert" baseAuthorityYear="1915" box="[655,773,1629,1654]" class="Sphagnopsida" family="Sphagnaceae" genus="Sphagnum" kingdom="Plantae" order="Sphagnales" pageId="153" pageNumber="154" phylum="Bryophyta" rank="genus">
