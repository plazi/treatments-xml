<quantity id="2925F8C40C4AFF858134F8AF657AB483" box="[524,627,1906,1930]" metricMagnitude="-3" metricUnit="m" metricValue="3.47" pageId="2" pageNumber="49" unit="mm" value="3.47">3.47 mm</quantity>
; metatibia apical width 
<quantity id="2925F8C40C4AFF858046F8AF64EFB483" box="[894,998,1906,1930]" metricMagnitude="-4" metricUnit="m" metricValue="7.9" pageId="2" pageNumber="49" unit="mm" value="0.79">0.79 mm</quantity>
