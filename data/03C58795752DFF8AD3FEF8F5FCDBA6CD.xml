<emphasis id="B918EA91752CFF88D262F8E7FF0BA3D7" bold="true" box="[119,204,1905,1926]" pageId="13" pageNumber="495">Figure 8</emphasis>
<taxonomicName id="4C6C4D00752CFF88D2CDF8E7FE22A3D6" authority="Sayed" authorityName="Sayed" authorityYear="1950" box="[216,485,1905,1927]" class="Arachnida" family="Tenuipalpidae" genus="Brevipalpus" kingdom="Animalia" order="Trombidiformes" pageId="13" pageNumber="495" phylum="Arthropoda" rank="species" species="olearius">
