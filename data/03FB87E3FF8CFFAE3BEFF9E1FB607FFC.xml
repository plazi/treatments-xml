<taxonomicName id="4C524D76FF8DFFAE394EFEF6FC9A7D18" authority="Ragonot, 1891" authorityName="Ragonot" authorityYear="1891" box="[566,881,332,357]" class="Insecta" family="Pyralidae" genus="Penthesilea" kingdom="Animalia" order="Lepidoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAE7FF8DFFAE394EFEF6FD527D19" box="[566,697,332,357]" italics="true" pageId="45" pageNumber="46">Penthesilea</emphasis>
<bibRefCitation id="EFC34B04FF8DFFAE39BDFEF7FC9A7D18" author="Ragonot, E. - L." box="[709,881,332,357]" journalOrPublisher="Annales de la Societe Entomologique de France" pageId="45" pageNumber="46" pagination="434 - 546" part="10" refId="ref63432" refString="Ragonot, E. - L. (1891) Essai sur la classification des pyralites. Annales de la Societe Entomologique de France, Serie 6, 10 [1890], 434 - 546. [25 February] [Dated from unpublished reserch by the senior author.]" title="Essai sur la classification des pyralites" type="journal article" year="1891">Ragonot, 1891</bibRefCitation>
</taxonomicName>
; junior synonym of 
<taxonomicName id="4C524D76FF8DFFAE3F1DFEF6FAB37D18" authority="Billberg, 1820" authorityName="Billberg" authorityYear="1820" box="[1125,1368,332,357]" class="Insecta" family="Syrphidae" genus="Blera" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAE7FF8DFFAE3F1DFEF6FB4F7D19" box="[1125,1188,332,357]" italics="true" pageId="45" pageNumber="46">Blera</emphasis>
<bibRefCitation id="EFC34B04FF8DFFAE3FD7FEF6FAB37D18" author="Billberg, G. J." bookContentInfo="138 pp." box="[1199,1368,332,357]" journalOrPublisher="Stockholm" pageId="45" pageNumber="46" refId="ref51904" refString="Billberg, G. J. (1820) Enumeratio insectorum in museo Gust. Joh. Billberg. Gadelianis, [Stockholm], [2] + 138 pp. [31 December +] https: // doi. org / 10.5962 / bhl. title. 49763" title="Enumeratio insectorum in museo Gust. Joh. Billberg. Gadelianis" type="book" year="1820">Billberg, 1820</bibRefCitation>
</taxonomicName>
. 
<emphasis id="B926EAE7FF8DFFAE3E1FFEF6FEAA7DF4" bold="true" pageId="45" pageNumber="46">New Synonymy</emphasis>
.
