<taxonomicName id="4C524D76FF8DFFAE3B86FE2EFE747DD0" box="[254,415,404,428]" class="Insecta" family="Syrphidae" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="family">SYRPHIDAE</taxonomicName>
