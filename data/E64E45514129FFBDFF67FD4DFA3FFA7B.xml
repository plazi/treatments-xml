<quantity id="A91F59A24129FFBDFE6DFBE4FE5FFB9B" box="[413,502,1081,1102]" metricMagnitude="-2" metricUnit="m" metricValue="1.53" pageId="28" pageNumber="29" unit="mm" value="15.3">15.3 mm</quantity>
