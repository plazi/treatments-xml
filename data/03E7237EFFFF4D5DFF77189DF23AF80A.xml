<taxonomicName id="4C4EE9EBFFFF4D5DFF77189DF118F94F" authority="Allman, 1888" authorityName="Allman" authorityYear="1888" box="[151,593,1757,1783]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="12" pageNumber="13" phylum="Cnidaria" rank="species" species="fastigiatum">
<emphasis id="B93A4E7AFFFF4D5DFF77189DF118F94F" bold="true" box="[151,593,1757,1783]" pageId="12" pageNumber="13">
<emphasis id="B93A4E7AFFFF4D5DFF77189DF2E8F94F" bold="true" box="[151,417,1757,1783]" italics="true" pageId="12" pageNumber="13">Halecium fastigiatum</emphasis>
