<subSubSection id="9EE9D2FEBAE342AE61E3AEE8150FA2D7" pageId="15" pageNumber="36" type="etymology">
<paragraph id="BBDA4E1B6473B547AE204C5598D8AFE3" pageId="15" pageNumber="36">Etymology.</paragraph>
<paragraph id="D375A16E5124D047ADE79D4AD6220F05" pageId="15" pageNumber="36">
