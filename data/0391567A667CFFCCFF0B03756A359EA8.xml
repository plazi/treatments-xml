<subSubSection id="C322B4E7667CFFCCFF0B037569199CD0" pageId="26" pageNumber="307" type="nomenclature">
<paragraph id="8B87E76C667CFFCCFF0B03756B3A9C6E" blockId="26.[151,868,1125,1152]" box="[151,868,1125,1152]" pageId="26" pageNumber="307">
<heading id="D0CF5000667CFFCCFF0B03756B3A9C6E" bold="true" box="[151,868,1125,1152]" fontSize="11" level="1" pageId="26" pageNumber="307" reason="1">
<emphasis id="B94C3B7E667CFFCCFF0B03756B3A9C6E" bold="true" box="[151,868,1125,1152]" pageId="26" pageNumber="307">
<taxonomicName id="4C389CEF667CFFCCFF0B03756AEC9C6E" authority="Bean and Weed, 1909" authorityName="Bean and Weed" authorityYear="1909" box="[151,690,1125,1152]" class="Chondrichthyes" family="Rajidae" genus="Dactylobatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="26" pageNumber="307" phylum="Chordata" rank="species" species="armatus">
