<taxonomicName id="AFD085647259D9D90C6444F12FB3FEA2" class="Insecta" family="Noctuidae" genus="Diastema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diastema morata" order="Lepidoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="morata">
