<taxonomicName id="76A8C7DEF26BFFD9FE819AE0BAA4FC06" authority="(Brady, 1880)" baseAuthorityName="Brady" baseAuthorityYear="1880" box="[362,898,929,953]" class="Hexanauplia" family="Canthocamptidae" genus="Attheyella" kingdom="Animalia" order="Harpacticoida" pageId="3" pageNumber="90" phylum="Arthropoda" rank="species" species="trispinosa" subGenus="Brehmiella">
