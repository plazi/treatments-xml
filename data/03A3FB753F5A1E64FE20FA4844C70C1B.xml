<taxonomicName id="4C0A31E03F5D1E64FE74FA1044270C3B" authorityName="Irmler" authorityYear="2013" box="[487,577,1533,1556]" class="Insecta" family="Staphylinidae" genus="Holotrochus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="167" phylum="Arthropoda" rank="species" species="hamatus">
