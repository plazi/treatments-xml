<materialsCitation id="EF983CC00E6EEE415BF6F9022EEFF92E" ID-GBIF-Occurrence="1416805661" box="[151,1242,1752,1777]" collectionCode="ANIC" location="Wagga Wagga district" pageId="17" pageNumber="318" specimenCode="ANIC 29-38488" specimenCount="1" typeStatus="holotype">
