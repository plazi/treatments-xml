<taxonomicName id="43E002A91A62FFDC3886FF059333419C" authority="in Hobbs Jr. (1971: 20)" authorityName="in Hobbs Jr." authorityPageNumber="20" authorityYear="1971" box="[151,583,223,248]" class="Ostracoda" family="Entocytheridae" genus="Uncinocythere" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="87" pageNumber="88" phylum="Arthropoda" rank="genus">
<emphasis id="B694A5381A62FFDC3886FF05904F419C" box="[151,315,223,248]" italics="true" pageId="87" pageNumber="88">Uncinocythere</emphasis>
