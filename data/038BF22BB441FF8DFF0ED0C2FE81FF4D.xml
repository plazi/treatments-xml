</taxonomicName>
(
<taxonomicName id="4C2238BEB441FF8EFD62D2FEFCB1F84C" box="[763,875,1981,2007]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="13" pageNumber="514" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
