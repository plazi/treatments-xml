<taxonomicName id="4C604D39FF867766FC93F91AFBB5F940" authority="Mayr, 1865" authorityName="Mayr" authorityYear="1865" box="[826,1110,1673,1696]" class="Insecta" family="Formicidae" genus="Acromyrmex" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="304" phylum="Arthropoda" rank="genus">
<emphasis id="B914EAA8FF867766FC93F91AFC24F940" box="[826,967,1673,1696]" italics="true" pageId="1" pageNumber="304">Acromyrmex</emphasis>
Mayr, 1865
</taxonomicName>
(
<taxonomicName id="4C604D39FF867766FBC1F91AFAE5F940" box="[1128,1286,1673,1696]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="304" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
: 
<taxonomicName id="4C604D39FF867766FABDF91AFA74F940" box="[1300,1431,1673,1696]" class="Insecta" family="Formicidae" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="304" phylum="Arthropoda" rank="family">Formicidae</taxonomicName>
