(Grube, 
<number id="91067136FFD3FFBFFBEFFA1DFB51A6A7" box="[1132,1192,1478,1503]" pageId="17" pageNumber="168" value="1856.0">1856</number>
)
</taxonomicName>
, 
<taxonomicName id="E0CE1892FFD3FFBFFB3CFA1CFAD0A6A6" box="[1215,1321,1479,1502]" class="Polychaeta" family="Eunicidae" genus="Nicidion" kingdom="Animalia" order="Eunicida" pageId="17" pageNumber="168" phylum="Annelida" rank="species" species="cincta">
