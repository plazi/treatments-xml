<taxonomicName id="4C2B4D642177E519B401FCA57982D703" authority="(Rostock 1892)" baseAuthorityName="Rostock" baseAuthorityYear="1892" box="[290,700,828,853]" class="Insecta" family="Capniidae" genus="Capnopsis" kingdom="Animalia" order="Plecoptera" pageId="5" pageNumber="57" phylum="Arthropoda" rank="species" species="schilleri">
