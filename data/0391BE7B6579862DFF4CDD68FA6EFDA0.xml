<taxonomicName id="4C3874EE6579862EFEB5DD68FD80FABF" authority="Schneider" authorityName="Schneider" authorityYear="1851" box="[366,627,1339,1366]" class="Insecta" family="Chrysopidae" genus="Apochrysa" kingdom="Animalia" order="Neuroptera" pageId="5" pageNumber="334" phylum="Arthropoda" rank="genus">
