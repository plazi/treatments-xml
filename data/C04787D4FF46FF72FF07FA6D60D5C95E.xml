<taxonomicName id="8FEE4D41FF46FF6DFDB1F9D86600CDDF" box="[545,835,1663,1689]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="175" pageNumber="176" phylum="Arthropoda" rank="species" species="neumanni">
<emphasis id="7A9AEAD0FF46FF6DFDB1F9D86600CDDF" box="[545,835,1663,1689]" italics="true" pageId="175" pageNumber="176">Haemaphysalis neumanni</emphasis>
