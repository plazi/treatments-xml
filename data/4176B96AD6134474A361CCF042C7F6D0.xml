s.n. (holo 
<collectionCode id="AC95132D69A44026CDAC31F96B65056E" pageId="37" pageNumber="316">BO</collectionCode>
), Key Islands = 
<tnCandidate id="833F1620D6870607D0873D9DFDE60867" pageId="37" pageNumber="316">
<taxonomicName id="405ECF5815D2381724C4AE889F9B9B5C" authorityName="C. Linnaeus" authorityYear="1753" class="Polypodiopsida" family="Aspleniaceae" genus="Asplenium" kingdom="Plantae" order="Polypodiales" pageId="37" pageNumber="316" phylum="Tracheophyta" rank="genus">Asplenium</taxonomicName>
