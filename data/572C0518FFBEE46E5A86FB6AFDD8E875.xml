], 
<date id="AB3B92CEFFBEE46D5B3EFAAFFEF4EB0B" box="[265,369,1331,1351]" pageId="1" pageNumber="1302" value="1999-03-22">22.III.1999</date>
, Lencina / 
<typeStatus id="003E0AACFFBEE46D5BE2FAAFFDB1EB0B" box="[469,564,1331,1351]" pageId="1" pageNumber="1302">Holotypus</typeStatus>
