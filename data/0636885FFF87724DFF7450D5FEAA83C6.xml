: 
<taxonomicName id="499F42CAFF87724DFEC951AAFDA18087" authority="Stal, 1877" authorityName=": Stal" authorityYear="1877" box="[298,556,1440,1462]" class="Insecta" family="Tetrigidae" genus="Tettix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="palpatus">
<emphasis id="BCEBE55BFF87724DFEC951AAFE328087" box="[298,447,1440,1462]" italics="true" pageId="54" pageNumber="55">Tettix palpatus</emphasis>
<bibRefCitation id="EA0E44B8FF87724DFE2651ABFDA18087" box="[453,556,1440,1462]" pageId="54" pageNumber="55" refId="ref39167" refString="Stal, C. (1877) Orthoptera nova ex Insulis Philippinis descripsit. Ofversigt af Kongliga Vetenskaps-Akademiens Forhandlinger, 34 (10), 33 - 58." type="journal article">Stål, 1877</bibRefCitation>
</taxonomicName>
(= 
<taxonomicName id="499F42CAFF87724DFDAC51ABFD7A8087" baseAuthorityName="Stal" baseAuthorityYear="1877" box="[591,759,1440,1462]" class="Insecta" family="Tetrigidae" genus="Tefrinda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="palpata">
