<taxonomicName id="BABA4D4D16229347FEBAE00EB2F7FB72" authorityName="Hansen" authorityYear="1919" box="[259,429,1134,1153]" class="Malacostraca" family="Sergestidae" genus="Acetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="359" phylum="Arthropoda" rank="species" species="chinensis">
