<taxonomicName id="FBFAF5F466E379F7CAD0F5CE97E65AE1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136454" class="Insecta" family="Formicidae" genus="Myrmecocystus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmecocystus cursor var. hellenicus Forel" lsidName-HNS="Myrmecocystus cursor var. hellenicus Forel" order="Hymenoptera" pageNumber="74" phylum="Arthropoda" rank="variety" species="cursor" variety="hellenicus">M. hellenicus</taxonomicName>
