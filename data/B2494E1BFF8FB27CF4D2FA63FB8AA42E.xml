<taxonomicName id="FDE0848EFF8EB27CF7E9FEC6FBB6A42E" authorityName="Borojevic &amp; Boury-Esnault &amp; Vacelet" authorityYear="2000" box="[928,1106,266,292]" class="Calcarea" family="Trichogypsiidae" kingdom="Animalia" order="Baerida" pageId="53" pageNumber="256" phylum="Porifera" rank="family">Trichogypsiidae</taxonomicName>
