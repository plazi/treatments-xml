<typeStatus id="54911C907C55FFD1FE18FCE73CFCE20A" box="[427,508,772,792]" pageId="14" pageNumber="1445">Holotype</typeStatus>
() and 3 - 
<typeStatus id="54911C907C55FFD1FD39FCE73FE2E20A" box="[650,738,772,792]" pageId="14" pageNumber="1445">paratypes</typeStatus>
: 
<collectingCountry id="F33DE2A27C55FFD1FD5EFCE73E32E20A" box="[749,818,772,792]" name="Taiwan" pageId="14" pageNumber="1445">Taiwan</collectingCountry>
: 
<collectingRegion id="49EE6CD07C55FFD1FC8EFCE73EC8E20A" box="[829,968,772,792]" country="Taiwan" name="Taoyuan" pageId="14" pageNumber="1445">Taoyuan Hsien</collectingRegion>
: Takuanshan Forest, 
<quantity id="4CD20FD77C55FFD1FE96FCFF3C64E222" box="[293,356,796,816]" metricMagnitude="3" metricUnit="m" metricValue="1.65" pageId="14" pageNumber="1445" unit="m" value="1650.0">1650m</quantity>
, 
<date id="FF9484F27C55FFD1FEC1FCFF3CD7E222" box="[370,471,796,816]" pageId="14" pageNumber="1445" value="1990-04-17">17.IV.1990</date>
