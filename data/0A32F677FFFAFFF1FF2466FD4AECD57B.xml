<taxonomicName id="459B3CE2FFF9FFF1FDEB611D49F6D553" box="[600,701,1853,1878]" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="27" phylum="Chordata" rank="species" species="paripari">
