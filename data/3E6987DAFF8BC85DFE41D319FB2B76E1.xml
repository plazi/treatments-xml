<quantity id="71389B29FF8BC85DFF14D792FEDD7685" box="[170,278,1680,1707]" metricMagnitude="-2" metricUnit="m" metricValue="1.56" pageId="22" unit="mm" value="15.6">15.6 mm</quantity>
