(
<collectingCountry id="F33B761F4264FFB29A5C7EB9A8C7FB6A" box="[297,425,1222,1244]" name="Costa Rica" pageId="88" pageNumber="89">Costa Rica</collectingCountry>
, 
<collectorName id="26D953594264FFB29AC37EB9AB47FB6A" box="[438,553,1222,1244]" pageId="88" pageNumber="89">La Palma</collectorName>
, altitude 
<quantity id="4CD49B6A4264FFB299EE7EB9AB9FFB6D" box="[667,753,1222,1244]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="88" pageNumber="89" unit="m" value="1500.0">
<elevation id="0001D1BC4264FFB299EE7EB9AB9FFB6D" box="[667,753,1222,1244]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="88" pageNumber="89" unit="m" value="1500.0">1500 m</elevation>
