<taxonomicName id="4C3B4D6BFFD9FFDBFC0F4948FBC8FA03" authorityName="Jeannel" authorityYear="1953" box="[899,1049,1606,1630]" class="Insecta" family="Staphylinidae" genus="Leiochrotella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="380" phylum="Arthropoda" rank="genus">
