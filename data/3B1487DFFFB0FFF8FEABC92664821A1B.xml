<taxonomicName id="74BD4D4AFFB0FFF8FE67C81162051A30" box="[452,621,828,853]" class="Adenophorea" family="Comesomatidae" genus="Setosabatieria" kingdom="Animalia" order="Desmodorida" pageId="5" pageNumber="58" phylum="Nematoda" rank="genus">
