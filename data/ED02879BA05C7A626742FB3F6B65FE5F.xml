<taxonomicName id="A2AB4D0EA05E7A6264B8FEA96BF3FED8" authority="Chen &amp; Watabe &amp; Gao &amp; Takamori &amp; Zhang &amp; Aotsuka, 2005" authorityName="Chen &amp; Watabe &amp; Gao &amp; Takamori &amp; Zhang &amp; Aotsuka" authorityYear="2005" box="[683,914,299,323]" class="Insecta" family="Drosophilidae" genus="Amiota" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="297" phylum="Arthropoda" rank="species" species="jizushanensis" status="sp. nov." subGenus="Amiota">
<emphasis id="57DFEA9FA05E7A6264B8FEA96AA6FED8" box="[683,711,299,323]" italics="true" pageId="34" pageNumber="297">A.</emphasis>
(
<emphasis id="57DFEA9FA05E7A6264CBFEA96A95FED8" box="[728,756,299,323]" italics="true" pageId="34" pageNumber="297">A.</emphasis>
) 
<emphasis id="57DFEA9FA05E7A626516FEA96BF3FED8" box="[773,914,299,323]" italics="true" pageId="34" pageNumber="297">jizushanensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4CEC57E4A05E7A62658AFEA96B92FED8" box="[921,1011,299,323]" pageId="34" pageNumber="297" rank="species">sp. nov.</taxonomicNameLabel>
