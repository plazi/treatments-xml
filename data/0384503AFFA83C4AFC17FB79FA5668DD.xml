<taxonomicName id="4C2D9AAFFFAF3C4AFBC1FC22FAA76A4F" authority="Köhler, 2011" authorityName="Köhler" authorityYear="2011" box="[1145,1354,1008,1029]" class="Gastropoda" family="Camaenidae" genus="Amplirhagada" kingdom="Animalia" order="Stylommatophora" pageId="20" pageNumber="187" phylum="Mollusca" rank="species" species="coffea" status="n.sp.">
