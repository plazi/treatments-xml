<taxonomicName id="D7A95D530946C86EB23355B18959292C" class="Insecta" family="Curculionidae" genus="Laemosaccus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Laemosaccus" order="Coleoptera" pageId="267" pageNumber="268" phylum="Arthropoda" rank="genus">Laemosaccus</taxonomicName>
