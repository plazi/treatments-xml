<paragraph id="D8E276D1B7A556CCE7B8C2C5887DF480" pageId="54" pageNumber="54">
<taxonomicName id="79AB9FBB9DE21F1005D55DFC6F8E44A0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:29107" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica integra Nylander" order="Hymenoptera" pageId="54" pageNumber="54" phylum="Arthropoda" rank="species" species="integra">Formica integra</taxonomicName>
