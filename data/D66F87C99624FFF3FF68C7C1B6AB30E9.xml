<location id="5B1960049624FFF3FEEFC480B56731BD" LSID="urn:lsid:plazi:treatment:D66F87C99624FFF3FF68C7C1B6AB30E9:5B1960049624FFF3FEEFC480B56731BD" box="[314,639,723,747]" country="Canada" county="Both" municipality="West Virginia" name="Michigan's Upper Peninsula" pageId="7" pageNumber="169" stateProvince="Ontario">
