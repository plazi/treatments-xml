<taxonomicName id="05A1D8C0863DFFE2904A66E2FDA2D584" authority="Jørgen Berge, 2003" authorityName="Jørgen Berge" authorityYear="2003" box="[363,584,1687,1708]" class="Malacostraca" family="Stegocephalidae" genus="Stegocephaloides" kingdom="Animalia" order="Amphipoda" pageId="25" pageNumber="110" phylum="Arthropoda" rank="species" species="tucki" status="n.sp.">
<emphasis id="F0D57F51863DFFE2904A66E2FDA2D584" box="[363,584,1687,1708]" italics="true" pageId="25" pageNumber="110">Stegocephaloides tucki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="EBE6C22A863DFFE2936C66E2FD91D584" box="[589,635,1687,1708]" pageId="25" pageNumber="110" rank="species">n.sp.</taxonomicNameLabel>
holotype. 
<emphasis id="F0D57F51863DFFE293FE66E2FD15D584" box="[735,767,1687,1708]" italics="true" pageId="25" pageNumber="110">OP</emphasis>
, maxilla 1 outer plate; 
<emphasis id="F0D57F51863DFFE292F866E2FC1AD584" box="[985,1008,1687,1708]" italics="true" pageId="25" pageNumber="110">IP</emphasis>
, inner plate of maxilliped.
</paragraph>
</caption>
<caption id="96DEF3CB863EFFE193706227FBF8D140" box="[593,1042,594,616]" pageId="26" pageNumber="111" startId="26.[593,628,594,615]" targetBox="[235,1467,136,619]" targetPageId="26">
<paragraph id="C21EA343863EFFE193706227FBF8D140" blockId="26.[593,1042,594,616]" box="[593,1042,594,616]" pageId="26" pageNumber="111">
<emphasis id="F0D57F51863EFFE193706227FD7FD14F" bold="true" box="[593,661,594,615]" pageId="26" pageNumber="111">Fig. 21</emphasis>
. 
<taxonomicName id="05A1D8C0863EFFE193816226FC94D140" authority="Jørgen Berge, 2003" authorityName="Jørgen Berge" authorityYear="2003" box="[672,894,595,616]" class="Malacostraca" family="Stegocephalidae" genus="Stegocephaloides" kingdom="Animalia" order="Amphipoda" pageId="26" pageNumber="111" phylum="Arthropoda" rank="species" species="tucki" status="n.sp.">
<emphasis id="F0D57F51863EFFE193816226FC94D140" box="[672,894,595,616]" italics="true" pageId="26" pageNumber="111">Stegocephaloides tucki</emphasis>
