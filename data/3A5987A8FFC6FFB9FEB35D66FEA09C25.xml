<taxonomicName id="75F04D3DFFC7FFB9FBDB5804FED89F0E" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Malvaceae" genus="Hibiscus" kingdom="Plantae" order="Malvales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="species" species="mutabilis">
<emphasis id="8084EAACFFC7FFB9FBDB5804FED89F0E" italics="true" pageId="19" pageNumber="20">Hibiscus mutabilis</emphasis>
