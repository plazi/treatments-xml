<taxonomicName id="75F04D3DFFC6FFB8FC285E2CFBE8991D" box="[915,1043,1844,1870]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
(
<taxonomicName id="75F04D3DFFC6FFB8FB935E2CFB13991E" box="[1064,1256,1844,1869]" class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" kingdom="Plantae" order="Asterales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
<emphasis id="8084EAACFFC6FFB8FB935E2CFB13991E" box="[1064,1256,1844,1869]" italics="true" pageId="18" pageNumber="19">Chrysanthemum</emphasis>
</taxonomicName>
