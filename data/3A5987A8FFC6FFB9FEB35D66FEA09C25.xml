(
<taxonomicName id="75F04D3DFFC7FFB9FE7F585CFDA39F0E" authorityName="C.Linnaeus" authorityYear="1753" box="[452,600,324,349]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="19" pageNumber="20" phylum="Mollusca" rank="species" species="carica">
<emphasis id="8084EAACFFC7FFB9FE7F585CFDA39F0E" box="[452,600,324,349]" italics="true" pageId="19" pageNumber="20">Ficus carica</emphasis>
