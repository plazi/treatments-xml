<emphasis id="8084EAACFFC6FFB9FB405E2CFEB19F66" italics="true" lastPageId="19" lastPageNumber="20" pageId="18" pageNumber="19">Sol­ idago</emphasis>
), Leguminosae (
<taxonomicName id="75F04D3DFFC7FFB9FDAD5804FD6B9F66" box="[534,656,284,309]" class="Magnoliopsida" family="Fabaceae" genus="Lespedeza" kingdom="Plantae" order="Fabales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="genus">
<emphasis id="8084EAACFFC7FFB9FDAD5804FD6B9F66" box="[534,656,284,309]" italics="true" pageId="19" pageNumber="20">Lespedeza</emphasis>
</taxonomicName>
