<emphasis id="949D579F9DC75C5B62180D67EF02E0F8" bold="true" pageId="0" pageNumber="55">A, B</emphasis>
<taxonomicName id="999DB4299280EC34358F81CD38D21212" genus="P." lsidName="P. linearifolia" pageId="0" pageNumber="55" rank="species" species="linearifolia">
<emphasis id="0C53AE774C4B6A4FB7B9C7ABB70E18AD" italics="true" pageId="0" pageNumber="55">P. linearifolia</emphasis>
</taxonomicName>
<emphasis id="0E687815BAF7C9757C39FE4FB86C5A2E" bold="true" pageId="0" pageNumber="55">C, D</emphasis>
<taxonomicName id="A88E81F39EFA757772F117DC04B095F3" genus="P." lsidName="P. pseudolinearifolia" pageId="0" pageNumber="55" rank="species" species="pseudolinearifolia">
<emphasis id="1934E78EBFE1FCBC5898B106301A8A11" italics="true" pageId="0" pageNumber="55">P. pseudolinearifolia</emphasis>
</taxonomicName>
<emphasis id="EB947DC54A8F9C727D8AFAD990F20ED0" bold="true" pageId="0" pageNumber="55">E, F</emphasis>
