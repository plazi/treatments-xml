<geoCoordinate id="FDC5019EED4B77926CF4E4A9B8DA0F14" degrees="22" direction="north" minutes="28" orientation="latitude" precision="925" value="22.466667">22°28'N</geoCoordinate>
, 
<geoCoordinate id="8A7A74F9491275BE3D1A6CDE8998F3A4" degrees="106" direction="east" minutes="58" orientation="longitude" precision="925" value="106.96667">106°58'E</geoCoordinate>
