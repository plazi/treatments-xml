<taxonomicName id="4C74DFDDFF883A54096DFA590EC4FA67" box="[1138,1228,1410,1434]" class="Actinopterygii" family="Cyprinidae" genus="Rasbora" kingdom="Animalia" order="Cypriniformes" pageId="4" pageNumber="599" phylum="Chordata" rank="genus">
