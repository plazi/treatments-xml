<taxonomicName id="CAF37B53EA6811473659CAEDF5010A99" authority="Huds." authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Asteraceae" genus="Circaea" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="lutea">
