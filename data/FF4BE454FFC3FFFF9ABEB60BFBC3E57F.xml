<geoCoordinate id="12D63385FFC3FFFF9F1BB1FDFA23E788" box="[1293,1416,159,184]" degrees="13" direction="south" minutes="57" orientation="latitude" pageId="7" pageNumber="958" precision="15" seconds="24" value="-13.956667">13°57′24″S</geoCoordinate>
