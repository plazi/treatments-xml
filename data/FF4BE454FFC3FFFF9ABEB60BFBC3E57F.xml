<geoCoordinate id="12D63385FFC3FFFF9AD0B6ACFE95E0D7" box="[198,318,1998,2023]" degrees="14" direction="south" minutes="06" orientation="latitude" pageId="7" pageNumber="958" precision="15" seconds="33" value="-14.109167">14°06′33″S</geoCoordinate>
