<date id="035C7382FFC3FFFF99DDB1A0FB8EE7EA" box="[971,1061,194,218]" pageId="7" pageNumber="958" value="2007-06">VI.2007</date>
(HUESC 24076); Coastal Zone Station 4, 
<geoCoordinate id="12D63385FFC3FFFF99BCB181FB89E7CC" box="[938,1058,227,252]" degrees="13" direction="south" minutes="54" orientation="latitude" pageId="7" pageNumber="958" precision="15" seconds="56" value="-13.915555">13°54′56″S</geoCoordinate>
, 
<geoCoordinate id="12D63385FFC3FFFF9E23B181FB13E7CC" box="[1077,1208,227,252]" degrees="38" direction="west" minutes="58" orientation="longitude" pageId="7" pageNumber="958" precision="15" seconds="57" value="-38.982502">38°58′57″W</geoCoordinate>
