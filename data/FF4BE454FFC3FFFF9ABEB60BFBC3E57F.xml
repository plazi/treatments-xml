<geoCoordinate id="12D63385FFC3FFFF993BB1A3FC17E7EA" box="[813,956,193,218]" degrees="038" direction="west" minutes="59" orientation="longitude" pageId="7" pageNumber="958" precision="15" seconds="38" value="-38.99389">038°59′38″W</geoCoordinate>
