<taxonomicName id="4C58A0B51E29FF94FF0BA08AFE55DED9" authority="Butler, 1889" authorityName="Butler" authorityYear="1889" box="[145,440,1203,1225]" class="Insecta" family="Geometridae" genus="Hypephyra" kingdom="Animalia" order="Lepidoptera" pageId="54" pageNumber="311" phylum="Arthropoda" rank="genus">
