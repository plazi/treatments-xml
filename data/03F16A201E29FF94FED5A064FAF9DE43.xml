<taxonomicName id="4C58A0B51E29FF94FDE6A08AFE94DEF6" authority="Butler, 1889" authorityName="Butler" authorityYear="1889" class="Insecta" family="Geometridae" genus="Hypephyra" kingdom="Animalia" order="Lepidoptera" pageId="54" pageNumber="311" phylum="Arthropoda" rank="species" species="terrosa">
