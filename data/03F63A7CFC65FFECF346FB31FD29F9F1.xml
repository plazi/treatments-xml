<bibRefCitation id="EFCEF69BFC65FFECF346FA32FE05FA3F" author="SCHMID F" box="[340,480,1476,1498]" pageId="36" pageNumber="1045" pagination="239 - 283" refId="ref31594" refString="SCHMID F. (1983): Encore quelques Stactobia MCLACHLAN (Trichoptera, Hydroptilidae). - Le Naturaliste Canadien 110: 239 - 283." type="journal article" year="1983">SCHMID 1983</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C5FF0E9FC65FFECF3F9FA32FD04FA3F" authority="SCHMID 1983" authorityName="SCHMID" authorityYear="1983" box="[491,737,1476,1498]" class="Insecta" family="Hydroptilidae" genus="Stactobia" kingdom="Animalia" order="Trichoptera" pageId="36" pageNumber="1045" phylum="Arthropoda" rank="species" species="dwalur">
<emphasis id="B92B5778FC65FFECF3F9FA32FDA8FA3F" box="[491,589,1476,1498]" italics="true" pageId="36" pageNumber="1045">S. dwalur</emphasis>
<bibRefCitation id="EFCEF69BFC65FFECF046FA32FD04FA3F" author="SCHMID F" box="[596,737,1476,1498]" pageId="36" pageNumber="1045" pagination="239 - 283" refId="ref31594" refString="SCHMID F. (1983): Encore quelques Stactobia MCLACHLAN (Trichoptera, Hydroptilidae). - Le Naturaliste Canadien 110: 239 - 283." type="journal article" year="1983">SCHMID 1983</bibRefCitation>
