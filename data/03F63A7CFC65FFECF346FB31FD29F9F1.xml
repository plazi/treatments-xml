<taxonomicName id="4C5FF0E9FC65FFECF0FEFA32FC12FA3F" authority="IVANOV 1992" authorityName="IVANOV" authorityYear="1992" box="[748,1015,1476,1498]" class="Insecta" family="Hydroptilidae" genus="Stactobia" kingdom="Animalia" order="Trichoptera" pageId="36" pageNumber="1045" phylum="Arthropoda" rank="species" species="shahdara">
