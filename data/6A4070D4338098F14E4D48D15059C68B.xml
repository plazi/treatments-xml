<taxonomicName id="2BD23C14DC1A1826720745CDC89232A4" family="Loranthaceae" lsidName="" pageId="0" pageNumber="7938" rank="family">Loranthaceae</taxonomicName>
) (
<bibRefCitation id="AF06B05FADA2DCAAC8BA961B04B820A5" author="Holloway, J. D." journalOrPublisher="Malayan Nature Journal" pageId="0" pageNumber="7938" pagination="1 - 242" title="The moths of Borneo: family Geometridae, subfamilies Sterrhinae and Larentiinae" volume="51" year="1997">Holloway 1997</bibRefCitation>
), of 
<taxonomicName id="54B192EB1DD7B2E6475555035AA2DE82" class="Insecta" family="Geometridae" genus="Sauris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sauris hirudinata" order="Lepidoptera" pageId="0" pageNumber="7938" phylum="Arthropoda" rank="species" species="hirudinata">Sauris hirudinata</taxonomicName>
<normalizedToken id="093F745B8676CF0A101740B990EB6533" originalValue="GuenÃ©e">Guenee</normalizedToken>
, 1858 from 
<taxonomicName id="9AD219B18C4092CE5581E3264B657014" class="Magnoliopsida" family="Lauraceae" genus="Alseodaphne" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Alseodaphne" order="Laurales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Alseodaphne</taxonomicName>
sp. (
<taxonomicName id="7FC50ED7898D9AB6110D8A5F84CAD307" family="Lauraceae" lsidName="" pageId="0" pageNumber="7938" rank="family">Lauraceae</taxonomicName>
