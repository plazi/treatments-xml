<taxonomicName id="4B8A3D90B2103655855ABC7225E9A058" class="Magnoliopsida" family="Lythraceae" genus="Lagerstroemia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lagerstroemia" order="Myrtales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Lagerstroemia</taxonomicName>
