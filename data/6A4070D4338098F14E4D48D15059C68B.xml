<taxonomicName id="AE91C6C6AED6A0711D9F8ACECF59A0A4" class="Insecta" family="Geometridae" genus="Sauris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sauris eupitheciata" order="Lepidoptera" pageId="0" pageNumber="7938" phylum="Arthropoda" rank="species" species="eupitheciata">Sauris eupitheciata</taxonomicName>
