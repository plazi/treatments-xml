<taxonomicName id="E2A95CEBF40387542692B4AEDE96FE01" class="Magnoliopsida" family="Loranthaceae" genus="Loranthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Loranthus" order="Santalales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Loranthus</taxonomicName>
