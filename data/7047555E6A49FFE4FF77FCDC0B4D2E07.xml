<paragraph id="F851E4486A49FFE4FFC0FB600B4D2E07" blockId="7.[112,646,1167,1383]" lastBlockId="7.[675,1210,216,338]" pageId="7" pageNumber="916">
<taxonomicName id="3FEE9FCB6A49FFE4FFC0FB6009392B9D" baseAuthorityName="Roemer" baseAuthorityYear="1838" box="[113,304,1198,1224]" class="Ostracoda" family="Cytherellidae" genus="Cytherella" kingdom="Animalia" order="Platycopida" pageId="7" pageNumber="916" phylum="Arthropoda" rank="species" species="gyrosa">
