</taxonomicName>
(formerly 
<taxonomicName id="4C244D13FFB4FF99FD9DEB3AFD0E8A13" authorityName="Linnaeus" authorityYear="1767" box="[635,719,849,873]" class="Ascidiacea" family="Ascidiidae" genus="Ascidia" kingdom="Animalia" order="Phlebobranchia" pageId="75" pageNumber="76" phylum="Chordata" rank="genus">
