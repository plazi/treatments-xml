<taxonomicName id="4C244D13FFB4FF99FE77EB3AFE3D8A13" authorityName="Savigny" authorityYear="1816" box="[401,508,849,873]" class="Ascidiacea" family="Ascidiidae" genus="Phallusia" kingdom="Animalia" order="Phlebobranchia" pageId="75" pageNumber="76" phylum="Chordata" rank="genus">
