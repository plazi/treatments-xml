<taxonomicName id="4C1D4D44FFB5AF7FFF6F1583FE4D159B" authority="Barnard 1927" authorityName="Barnard" authorityYear="1927" box="[151,431,151,177]" class="Actinopterygii" family="Liparidae" genus="Psednos" kingdom="Animalia" order="Scorpaeniformes" pageId="32" pageNumber="33" phylum="Chordata" rank="genus">
