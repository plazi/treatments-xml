<collectingMunicipality id="6BC2722CFFBCFFB3FBEDF8E7FB21D002" box="[1066,1261,1834,1859]" pageId="16" pageNumber="264">Conghua District</collectingMunicipality>
, 
<location id="8EC6BE8DFFBCFFB3FB3EF8E7FA54D003" LSID="urn:lsid:plazi:treatment:03B05940FFBCFFB1FF50F972FC80D6C9:8EC6BE8DFFBCFFB3FB3EF8E7FA54D003" box="[1273,1432,1834,1859]" country="China" county="Guangzhou City" municipality="Conghua District" name="Lyutian Town" pageId="16" pageNumber="264" stateProvince="Guangdong">Lyutian Town</location>
, brook in 
<location id="8EC6BE8DFFBCFFB3FF3BF882FE46D026" LSID="urn:lsid:plazi:treatment:03B05940FFBCFFB1FF50F972FC80D6C9:8EC6BE8DFFBCFFB3FF3BF882FE46D026" box="[252,394,1870,1895]" country="China" county="Guangzhou City" municipality="Conghua District" name="Mt. Guifeng" pageId="16" pageNumber="264" stateProvince="Guangdong">Mt. Guifeng</location>
, 
<collectingDate id="EFE3377EFFBCFFB3FE53F883FDC2D026" box="[404,526,1870,1895]" pageId="16" pageNumber="264" value="2016-04-19">
