<taxonomicName id="4C51C2DFFF9CE102CC001416E5F9FEF1" authorityName="Camerano" authorityYear="1894" box="[379,475,304,325]" class="Insecta" family="Rutelidae" genus="Geniates" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="380" phylum="Arthropoda" rank="species" species="borelli">
<emphasis id="B925654EFF9CE102CC001416E5AEFEF1" box="[379,396,304,325]" italics="true" pageId="6" pageNumber="380">G</emphasis>
