<taxonomicName id="4C51C2DFFF9CE102CDB6146DE562FED4" authorityName="Kirby" authorityYear="1819" box="[205,320,331,352]" class="Insecta" family="Rutelidae" genus="Geniates" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="380" phylum="Arthropoda" rank="species" species="barbatus">
