<taxonomicName id="89FCC5FF9516FF84223FF833FE5BE794" authorityName="Skartveit &amp; Pika" authorityYear="2014" box="[281,468,2001,2026]" class="Insecta" family="Bibionidae" genus="Bibio" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="361" phylum="Arthropoda" rank="species" species="crassinervis">
