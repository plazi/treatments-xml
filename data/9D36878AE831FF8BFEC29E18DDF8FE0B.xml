<emphasis id="27EBEA8EE831FF84FEC29E18DC51FEE8" bold="true" box="[264,837,283,310]" pageId="7" pageNumber="56">
<taxonomicName id="D29F4D1FE831FF84FEC29E18DDBCFEE8" authority="Miller" authorityName="Miller" box="[264,680,283,310]" class="Insecta" family="Dytiscidae" genus="Agaporomorphus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="56" phylum="Arthropoda" rank="species" species="silvaticus" status="new species">
<emphasis id="27EBEA8EE831FF84FEC29E18DD47FEEB" bold="true" box="[264,595,284,310]" italics="true" pageId="7" pageNumber="56">Agaporomorphus silvaticus</emphasis>
Miller
</taxonomicName>
, 
<taxonomicNameLabel id="3CD857F5E831FF84FD7F9E1FDC51FEE8" box="[693,837,283,309]" pageId="7" pageNumber="56" rank="species">new species</taxonomicNameLabel>
