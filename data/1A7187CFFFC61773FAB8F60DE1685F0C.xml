<taxonomicName id="55D84D5AFFC41773FCC0FC39E37559AC" authorityName="Brown" authorityYear="1914" class="Reptilia" family="Ceratopsidae" genus="Anchiceratops" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="46" pageNumber="363" phylum="Chordata" rank="genus">
