<taxonomicName id="55D84D5AFFC41773FDA2FC39E08959D0" authorityName="Lambe" authorityYear="1914" box="[536,753,903,938]" class="Reptilia" family="Ceratopsidae" genus="Chasmosaurus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="46" pageNumber="363" phylum="Chordata" rank="genus">
