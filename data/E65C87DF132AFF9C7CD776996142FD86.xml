<taxonomicName id="A9F54D4A1329FF9A7D5C76DE66EBFA7C" authority="Desutter-Grandcolas" authorityName="Desutter-Grandcolas" box="[284,700,1446,1469]" class="Insecta" family="Haglotettigoniidae" genus="Luzara" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="408" phylum="Arthropoda" rank="species" species="brevipennis" status="n. sp.">
<emphasis id="5C81EADB1329FF9A7D5C76DE65B7FA7C" box="[284,480,1446,1469]" italics="true" pageId="10" pageNumber="408">Luzara brevipennis</emphasis>
Desutter-Grandcolas
</taxonomicName>
, 
<taxonomicNameLabel id="47B257A01329FF9A7E8676D16756FA7C" box="[710,769,1447,1468]" pageId="10" pageNumber="408" rank="species">
<emphasis id="5C81EADB1329FF9A7E8676D16756FA7C" bold="true" box="[710,769,1447,1468]" pageId="10" pageNumber="408">n. sp.</emphasis>
