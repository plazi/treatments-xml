, PW 
<quantity id="4CC29B6DFF904B756590FC371A1194C8" box="[151,303,948,972]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" metricValueMax="6.1" metricValueMin="5.8999999999999995" pageId="3" pageNumber="168" unit="mm" value="0.6" valueMax="0.61" valueMin="0.59">0.59-0.61 mm</quantity>
, EL 
<quantity id="4CC29B6DFF904B756458FC371AC994C8" box="[351,503,948,972]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="7.6" metricValueMin="7.4" pageId="3" pageNumber="168" unit="mm" value="0.75" valueMax="0.76" valueMin="0.74">0.74-0.76 mm</quantity>
, EW 
