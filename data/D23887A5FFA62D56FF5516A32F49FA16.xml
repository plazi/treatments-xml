<taxonomicName id="9D914D30FFA62D56FDAF128C2EB1FA3A" box="[621,745,1469,1493]" class="Maxillopoda" family="Scalpellidae" genus="Catherinum" kingdom="Animalia" order="Pedunculata" pageId="4" pageNumber="38" phylum="Arthropoda" rank="species" species="tortilum">
