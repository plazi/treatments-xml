<taxonomicName id="4C2901BE04021551FDFA4AE6FC16FB1D" authority="Jousseaume, 1884" authorityName="Jousseaume" authorityYear="1884" box="[729,1021,1231,1257]" class="Gastropoda" family="Triphoridae" genus="Viriola" kingdom="Animalia" pageId="11" pageNumber="12" phylum="Mollusca" rank="genus">
