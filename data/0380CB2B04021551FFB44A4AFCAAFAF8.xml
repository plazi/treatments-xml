<taxonomicName id="4C2901BE04021551FD134ADAFD96FAF9" authorityName="Jousseaume" authorityYear="1884" box="[560,637,1267,1293]" class="Gastropoda" family="Triphoridae" genus="Viriola" kingdom="Animalia" pageId="11" pageNumber="12" phylum="Mollusca" rank="genus">
