<paragraph id="672D36C23B2D0B3CFE850524FCBDFDB2" blockId="9.[264,825,579,606]" box="[264,825,579,606]" pageId="9" pageNumber="10">
<heading id="3C6581AE3B2D0B3CFE850524FCBDFDB2" bold="true" box="[264,825,579,606]" fontSize="11" level="1" pageId="9" pageNumber="10" reason="1">
<emphasis id="55E6EAD03B2D0B3CFE850524FCBDFDB2" bold="true" box="[264,825,579,606]" pageId="9" pageNumber="10">
<taxonomicName id="A0924D413B2D0B3CFE850524FD4AFDB2" authority="Brammer" authorityName="Brammer" authorityYear="2005" box="[264,718,579,606]" class="Insecta" family="Stratiomyidae" genus="Quichuamyia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="costaricensis" status="sp. nov.">
<emphasis id="55E6EAD03B2D0B3CFE850524FDCAFDB1" bold="true" box="[264,590,580,606]" italics="true" pageId="9" pageNumber="10">Quichuamyia costaricensis</emphasis>
Brammer
</taxonomicName>
, 
<taxonomicNameLabel id="4ED557AB3B2D0B3CFD560523FCBDFDB2" box="[731,825,579,605]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
