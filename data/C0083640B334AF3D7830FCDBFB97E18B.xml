<taxonomicName id="8FA1FCD5B334AF3D7B75F931FCBBE1D5" authorityName="Sokolov &amp; Shchenkov &amp; Gordeev" authorityYear="2021" box="[622,833,1669,1694]" class="Trematoda" family="Fellodistomidae" genus="Steringophorus" kingdom="Animalia" order="Plagiorchiida" pageId="9" pageNumber="876" phylum="Platyhelminthes" rank="species" species="occidentalipacificus">
