<taxonomicName id="8FA1FCD5B334AF3D79C9F913FECAE18B" authorityName="Gilbert" authorityYear="1890" box="[210,304,1703,1728]" class="Actinopterygii" family="Bathylagidae" genus="Bathylagus" kingdom="Animalia" order="Osmeriformes" pageId="9" pageNumber="876" phylum="Chordata" rank="species" species="pacificus">
