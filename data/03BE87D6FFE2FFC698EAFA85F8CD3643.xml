<geoCoordinate id="EE235007FFE2FFC69ACBFA6EF95935B2" box="[694,792,1436,1463]" degrees="60" direction="north" minutes="15" orientation="latitude" pageId="115" pageNumber="116" precision="925" value="60.25">60°15’N</geoCoordinate>
