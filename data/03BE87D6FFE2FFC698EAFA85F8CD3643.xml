<quantity id="4CEF9B25FFE2FFC69D2DF9FAFFD43626" box="[1360,1429,1544,1571]" metricMagnitude="2" metricUnit="m" metricValue="2.258568" pageId="115" pageNumber="116" unit="ft" value="741.0">741 ft</quantity>
