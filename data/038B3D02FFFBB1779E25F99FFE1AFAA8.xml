<paragraph id="8B9D8C14FFFBB1799E39F91BFDE2F949" blockId="54.[108,636,1667,1724]" box="[108,635,1700,1724]" pageId="54" pageNumber="54">
<collectingRegion id="49E642F6FFFBB1799E39F91BFF4EF949" box="[108,215,1700,1724]" country="Argentina" name="San Juan" pageId="54" pageNumber="54">San Juan</collectingRegion>
