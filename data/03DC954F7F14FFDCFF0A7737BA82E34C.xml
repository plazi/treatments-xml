<emphasis id="B901F84B7F14FFDCFF0A77FCBF3DE4A7" box="[151,275,905,927]" italics="true" pageId="30" pageNumber="31">Monalonian</emphasis>
Herrich-Schaeffer, 
<number id="3DBD367E7F14FFDCFE4677FCBC3FE4A7" box="[475,529,905,927]" pageId="30" pageNumber="31" value="1850.0">1850</number>
: 
<number id="3DBD367E7F14FFDCFD8677FCBC6AE4A7" box="[539,580,905,927]" pageId="30" pageNumber="31" value="168.0">168</number>
. 
<typeStatus id="54CE9AFB7F14FFDCFDD377FFBCAEE4A7" box="[590,640,906,927]" pageId="30" pageNumber="31">Type</typeStatus>
species: 
<taxonomicName id="4C755FDA7F14FFDCFD4177FCBAFEE4A7" authority="Herrich-Schaeffer, 1850" authorityName="Herrich-Schaeffer" authorityYear="1850" box="[732,1232,905,927]" class="Insecta" family="Miridae" genus="Monalonion" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="parviventre">
<emphasis id="B901F84B7F14FFDCFD4177FCBDFCE4A7" box="[732,978,905,927]" italics="true" pageId="30" pageNumber="31">Monalonion parviventre</emphasis>
Herrich-Schaeffer, 
<number id="3DBD367E7F14FFDCFB0777FCBAFEE4A7" box="[1178,1232,905,927]" pageId="30" pageNumber="31" value="1850.0">1850</number>
