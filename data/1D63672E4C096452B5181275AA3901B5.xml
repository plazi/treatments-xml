<taxonomicName id="8E8330BC65D1A40A822A3029376008A3" class="Magnoliopsida" family="Campanulaceae" genus="Campanula" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Campanula bononiensis" order="Asterales" pageId="48" pageNumber="71" phylum="Tracheophyta" rank="species" species="bononiensis">Campanula bononiensis</taxonomicName>
