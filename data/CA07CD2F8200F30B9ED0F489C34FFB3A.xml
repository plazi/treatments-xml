<taxonomicName id="85AE07BA8200F30B9F03F33DC0EAFBC2" authorityName="Harris &amp; Drake" authorityYear="1941" box="[324,511,1087,1113]" class="Insecta" family="Mesoveliidae" genus="Mesovelia" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="234" phylum="Arthropoda" rank="species" species="zeteki">
