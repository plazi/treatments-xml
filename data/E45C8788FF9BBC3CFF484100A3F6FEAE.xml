<taxonomicName id="ABF54D1DFF9ABC3CFEB84579A43DFEAE" box="[359,594,332,356]" class="Polychaeta" family="Onuphidae" genus="Paradiopatra" kingdom="Animalia" order="Eunicida" pageId="7" pageNumber="156" phylum="Annelida" rank="species" species="ehlersi">
