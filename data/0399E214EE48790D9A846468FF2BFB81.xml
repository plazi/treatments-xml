<taxonomicName id="4C302881EE48790C9BF164E5FD40FC73" box="[482,666,962,985]" class="Agaricomycetes" family="Hymenochaetaceae" genus="Phellinus" kingdom="Fungi" order="Hymenochaetales" pageId="2" pageNumber="57" phylum="Basidiomycota" rank="species" species="undetermined">
