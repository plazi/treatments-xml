<taxonomicName id="FFF34D4AA935FF990602FA16CA2FFA60" baseAuthorityName="Braun" baseAuthorityYear="1939" box="[347,598,1410,1434]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="218" phylum="Arthropoda" rank="species" species="holodisci">
