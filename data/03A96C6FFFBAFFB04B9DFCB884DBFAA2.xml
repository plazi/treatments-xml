<taxonomicName id="4C00A6FAFFBAFFB04F92FB128034FAD6" authorityName="Loginova" authorityYear="1967" box="[1176,1287,1262,1287]" class="Insecta" family="Psyllidae" genus="Syringilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
