<taxonomicName id="4C26A1F5FFB24E14FBA4FDFDD888362C" box="[1055,1165,584,608]" class="Pauropoda" family="Pauropodidae" genus="Songius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tetramerocerata" pageId="5" pageNumber="298" phylum="Arthropoda" rank="species" species="hlavaci">
