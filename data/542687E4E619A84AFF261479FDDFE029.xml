<collectingCountry id="A4987662E619A84FFDF512FDFD27E5F4" box="[580,645,1156,1181]" name="Palau" pageId="19" pageNumber="470">Palau</collectingCountry>
, 
<geoCoordinate id="B9BB5035E619A84FFD2112FDFC83E5F4" box="[656,801,1156,1181]" direction="north" orientation="latitude" pageId="19" pageNumber="470" precision="1" value="7.2921667">07°17.530'N</geoCoordinate>
, 
<geoCoordinate id="B9BB5035E619A84FFC9D12FDFC65E5F5" box="[812,967,1156,1180]" direction="east" orientation="longitude" pageId="19" pageNumber="470" precision="1" value="134.5145">134°30.870'E</geoCoordinate>
.
