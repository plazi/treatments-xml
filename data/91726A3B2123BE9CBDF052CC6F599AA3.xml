<taxonomicName id="B2D6AF3ABD63AC3342BFC0EF47CA41A5" class="Arachnida" family="Gnaphosidae" genus="Micaria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Micaria canestrinii" order="Araneae" pageId="6" pageNumber="159" phylum="Arthropoda" rank="species" species="canestrinii">Micaria canestrinii</taxonomicName>
