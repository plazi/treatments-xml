<paragraph id="8BFA36DBFFC0FF90E6CCCB71CB34F8D3" blockId="1.[151,531,1823,1850]" box="[151,531,1823,1850]" pageId="1" pageNumber="238">
<heading id="D0B281B7FFC0FF90E6CCCB71CB34F8D3" bold="true" box="[151,531,1823,1850]" fontSize="11" level="1" pageId="1" pageNumber="238" reason="1">
<emphasis id="B931EAC9FFC0FF90E6CCCB71CB34F8D3" bold="true" box="[151,531,1823,1850]" pageId="1" pageNumber="238">
The 
<taxonomicName id="4C454D58FFC0FF90E694CB71C846F8D0" baseAuthorityName="Walker" baseAuthorityYear="1854" box="[207,353,1823,1850]" class="Insecta" family="Erebidae" genus="Hypoprepia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="238" phylum="Arthropoda" rank="species" species="delineata">
<emphasis id="B931EAC9FFC0FF90E694CB71C846F8D0" bold="true" box="[207,353,1823,1850]" italics="true" pageId="1" pageNumber="238">A. delineata</emphasis>
