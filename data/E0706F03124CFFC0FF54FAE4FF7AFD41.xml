</taxonomicName>
(currently in 
<taxonomicName id="AFD9A596124DFFC0FEF2FDB6FE01FDA5" authorityName="Monterosato" authorityYear="1884" box="[305,420,583,608]" class="Gastropoda" family="Buccinidae" genus="Chauvetia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="55" pageNumber="56" phylum="Mollusca" rank="genus">
