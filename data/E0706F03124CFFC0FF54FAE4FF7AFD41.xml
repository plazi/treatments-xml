<taxonomicName id="AFD9A596124DFFC0FC97FE0EFB43FDDD" authority="Brugnone, 1873" authorityName="Brugnone" authorityYear="1873" box="[852,1254,511,536]" class="Gastropoda" family="Buccinidae" genus="Lachesis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="55" pageNumber="56" phylum="Mollusca" rank="species" species="recondita">
