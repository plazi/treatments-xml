<subSubSection id="87BC2DA36DBDFDDA8DE3350AEBD1E468" type="etymology">
<paragraph id="30C3F4542D9A4AEC660757290DE4E791" pageNumber="652">
etymology L 
<taxonomicName id="33913DE797887EF2E31440F9C9C14AA4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182008" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole ambigua Wilson" lsidName-HNS="Pheidole ambigua Wilson" order="Hymenoptera" pageNumber="652" phylum="Arthropoda" rank="species" species="ambigua">ambigua</taxonomicName>
