<emphasis id="57CFEA93FFFFFFCDFF23F9BDBB6BF9D9" bold="true" box="[151,839,1637,1664]" pageId="27" pageNumber="178">
<taxonomicName id="A2BB4D02FFFFFFCDFF23F9BDBA49F9D9" authority="(Marikovskij)" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2020" baseAuthorityName="Jaschhof &amp; Jaschhof" baseAuthorityYear="2020" box="[151,613,1637,1664]" class="Insecta" family="Cecidomyiidae" genus="Isocolpodia" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="178" phylum="Arthropoda" rank="species" species="unidentata" status="new combination">
<emphasis id="57CFEA93FFFFFFCDFF23F9BDB981F926" bold="true" box="[151,429,1637,1664]" italics="true" pageId="27" pageNumber="178">Isocolpodia unidentata</emphasis>
(Marikovskij)
</taxonomicName>
