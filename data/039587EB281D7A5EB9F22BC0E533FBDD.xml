<emphasis id="B948EAEF281F7A5EB9A2294EE60CFB92" bold="true" box="[199,346,1051,1075]" inLineHeading="true" pageId="2" pageNumber="599" reason="1">Distribution.</emphasis>
<taxonomicName id="4C3C4D7E281F7A5EB8052949E6A5FB92" authorityName="Heller" authorityYear="1912" box="[352,499,1052,1075]" class="Insecta" family="Curculionidae" genus="Metapocyrtus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="599" phylum="Arthropoda" rank="genus">
