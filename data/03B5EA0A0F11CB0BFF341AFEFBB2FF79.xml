<taxonomicName id="4C1C209F0F11CB05FF341BACFDD4F824" authority="Iwatsuki, 2013: 85" authorityName="Iwatsuki" authorityPageNumber="85" authorityYear="2013" box="[151,622,1931,1957]" class="Actinopterygii" family="Sparidae" genus="Acanthopagrus" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="77" phylum="Chordata" rank="species" species="sheim">
<emphasis id="B968870E0F11CB05FF341BACFE2EF825" bold="true" box="[151,404,1931,1956]" italics="true" pageId="6" pageNumber="77">Acanthopagrus sheim</emphasis>
Iwatsuki, 
<number id="3DD4493B0F11CB05FDAA1BAAFDFFF824" box="[521,581,1933,1957]" pageId="6" pageNumber="77" value="2013.0">2013</number>
: 
<number id="3DD4493B0F11CB05FDF31BABFDD4F824" box="[592,622,1932,1957]" pageId="6" pageNumber="77" value="85.0">85</number>
</taxonomicName>
