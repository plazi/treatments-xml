<paragraph id="83B85AA760EC5B9F60481D13A6C95307" pageId="0" pageNumber="6900">
<taxonomicName id="451E510AC6378A5BD51286248B587827" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. hearseyi" order="Biddulphiales" pageId="0" pageNumber="6900" phylum="Diatomeae" rank="species" species="hearseyi">B. hearseyi</taxonomicName>
