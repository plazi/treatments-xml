<collectionCode id="EFD8B1C5AC783B1FFC80DCA15F27FB59" box="[849,868,1252,1272]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="52" pageNumber="445" type="Museum">V</collectionCode>
