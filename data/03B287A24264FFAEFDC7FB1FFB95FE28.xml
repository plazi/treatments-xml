<taxonomicName id="4C1B4D374266FFAFFE30F9F5FDC2F997" baseAuthorityName="Wood" baseAuthorityYear="1910" box="[417,612,1543,1565]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="554" phylum="Arthropoda" rank="species" species="gregaria">
<emphasis id="B96FEAA64266FFAFFE30F9F5FDC2F997" box="[417,612,1543,1565]" italics="true" pageId="50" pageNumber="554">Megaselia gregaria</emphasis>
