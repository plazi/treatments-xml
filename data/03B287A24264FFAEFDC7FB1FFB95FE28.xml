<taxonomicName id="4C1B4D374265FFACFE31F9F0FDCFF99D" baseAuthorityName="Wood" baseAuthorityYear="1910" box="[416,617,1538,1559]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="553" phylum="Arthropoda" rank="species" species="gregaria">Megaselia gregaria</taxonomicName>
