<collectingCountry id="D8D97DF1415E5222201A7AFFFE3376D1" box="[359,426,189,215]" name="China" pageId="8" pageNumber="212">China</collectingCountry>
(
<collectingRegion id="620AF383415E522220C47AFFFD8376DE" box="[441,538,189,216]" country="China" name="Sichuan" pageId="8" pageNumber="212">Sichuan</collectingRegion>
