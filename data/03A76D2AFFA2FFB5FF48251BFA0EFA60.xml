<number id="3DC6CE1BFFA2FFB5FB4C244EFB22FCC0" box="[1171,1200,784,807]" pageId="2" pageNumber="31" value="18.0">18</number>
); body length greater than 
<quantity id="4CF671D9FFA2FFB5FF24246EFEC2FCA0" box="[251,336,816,839]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="2" pageNumber="31" unit="mm" value="1.9">1.9 mm</quantity>
