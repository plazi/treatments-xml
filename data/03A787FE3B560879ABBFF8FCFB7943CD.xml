<emphasis id="B97AEAFA3B590879ABEFFF05FEF342D0" bold="true" box="[199,345,150,176]" pageId="9" pageNumber="460">Distribution</emphasis>
. 
<collectingCountry id="F31976783B590879AA4EFF04FE7A42D1" box="[358,464,151,177]" name="Australia" pageId="9" pageNumber="460">Australia</collectingCountry>
, 
<collectingRegion id="49CAF80A3B590879AAF3FF04FDC842D1" box="[475,610,151,177]" country="Australia" name="Queensland" pageId="9" pageNumber="460">Queensland</collectingRegion>
