<heading id="37408184552E560781DCD2B6F09716F9" bold="true" box="[151,393,1935,1960]" fontSize="10" level="2" pageId="32" pageNumber="33" reason="2">
<emphasis id="5EC3EAFA552E560781DCD2B6F09716F9" bold="true" box="[151,393,1935,1960]" pageId="32" pageNumber="33">
<taxonomicName id="ABB74D6B552E560781DCD2B6F01F16F9" box="[151,257,1935,1960]" class="Insecta" family="Curculionidae" genus="Irenimus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="genus">
