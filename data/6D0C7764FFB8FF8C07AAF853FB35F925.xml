<taxonomicName id="22A5BDF1FFBFFF8C0636FB87FE0AFBB9" authorityName="Pocock" authorityYear="1901" box="[267,423,1123,1149]" class="Arachnida" family="Theraphosidae" genus="Aphonopelma" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
