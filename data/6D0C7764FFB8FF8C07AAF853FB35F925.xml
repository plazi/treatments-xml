<taxonomicName id="22A5BDF1FFBFFF8C0645F9BFFD8CF9B2" box="[376,545,1627,1654]" class="Arachnida" family="Theraphosidae" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="family">Theraphosidae</taxonomicName>
