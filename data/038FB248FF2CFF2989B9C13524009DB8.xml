<taxonomicName id="4C2678DDFF2CFF2B882BC17E213399F3" box="[261,377,1901,1923]" class="Secernentea" family="Cucullanidae" genus="Cucullanus" kingdom="Animalia" order="Ascaridida" pageId="157" pageNumber="158" phylum="Nematoda" rank="genus">
