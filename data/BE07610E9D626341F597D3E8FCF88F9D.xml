<number id="8066C23F9D626343F5F8D587FECF89E8" box="[248,365,1981,2007]" pageId="5" pageNumber="6" value="1.725" valueMax="1.77" valueMin="1.68">1.68–1.77</number>
(
<number id="8066C23F9D626343F47DD587FE1789E8" box="[381,437,1981,2007]" pageId="5" pageNumber="6" value="1.71">1.71</number>
