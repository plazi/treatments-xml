<collectingMunicipality id="6BDFAC80EE750F67FD46FB6C058AFB0A" box="[666,977,1166,1193]" pageId="17" pageNumber="18">Bastak State Nature Reserve</collectingMunicipality>
(
<location id="8EDB6021EE750F67FC01FB6C023EFB0B" LSID="urn:lsid:plazi:treatment:03AD87ECEE6B0F67FF54FA3605CCFB53:8EDB6021EE750F67FC01FB6C023EFB0B" box="[989,1125,1166,1192]" county="The" latitude="48.933334" longLatPrecision="1107" longitude="133.11667" municipality="Bastak State Nature Reserve" name="East Siberia" pageId="17" pageNumber="18" stateProvince="Yevrey">East Siberia</location>
