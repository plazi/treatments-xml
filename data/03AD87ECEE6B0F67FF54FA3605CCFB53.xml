<location id="8EDB6021EE750F67FB60FB6C03FFFB0A" LSID="urn:lsid:plazi:treatment:03AD87ECEE6B0F67FF54FA3605CCFB53:8EDB6021EE750F67FB60FB6C03FFFB0A" box="[1212,1444,1166,1193]" county="The" latitude="48.933334" longLatPrecision="1107" longitude="133.11667" municipality="Bastak State Nature Reserve" name="Autonomous Region" pageId="17" pageNumber="18" stateProvince="Yevrey">Autonomous Region</location>
