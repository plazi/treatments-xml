<taxonomicName id="5F824D5EFFF42514FC29FFA7FC1EFF80" authorityName="MacLeay, MacLeay" authorityYear="1838" box="[918,1017,78,95]" class="Malacostraca" family="Goneplacidae" kingdom="Animalia" order="Decapoda" pageId="118" pageNumber="727" phylum="Arthropoda" rank="subFamily" subFamily="Goneplacinae">Goneplacinae</taxonomicName>
