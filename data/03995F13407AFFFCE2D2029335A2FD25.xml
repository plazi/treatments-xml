<heading id="D0C75969407AFFFCE2D20293352DFF14" bold="true" box="[151,519,152,178]" fontSize="11" level="1" pageId="33" pageNumber="184" reason="1">
<taxonomicName id="4C309586407AFFFCE2D20293352DFF14" authority="Wulfert, 1942" authorityName="Wulfert" authorityYear="1942" box="[151,519,152,178]" class="Eurotatoria" family="Philodinidae" genus="Rotaria" kingdom="Animalia" order="Bdelloidea" pageId="33" pageNumber="184" phylum="Rotifera" rank="species" species="laticeps">
<emphasis id="B9443217407AFFFCE2D20293352DFF14" bold="true" box="[151,519,152,178]" pageId="33" pageNumber="184">
<emphasis id="B9443217407AFFFCE2D20293367EFF14" bold="true" box="[151,340,152,178]" italics="true" pageId="33" pageNumber="184">Rotaria laticeps</emphasis>
<bibRefCitation id="EFA193F4407AFFFCE31E0293352DFF14" author="Wulfert" box="[347,519,152,178]" pageId="33" pageNumber="184" refString="Wulfert, K. (1942) Uber die Meeres- und Brackwasser-Rotatorien in der Umgebung von Rovigno d'Istria. Thalassia - Istituto Italo-Germanico di Biologia Marina di Rovigno d'Istria, Venezia, 4, 3 - 26, pls. 1 - 6." type="journal article" year="1942">
Wulfert, 
<number id="3DF8FC22407AFFFCE3890293352DFF14" box="[460,519,152,178]" pageId="33" pageNumber="184" value="1942.0">1942</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
