</quantity>
, 
<collectorName id="23315330FF899014FF5D6057FF4FF9A1" box="[151,254,1548,1573]" pageId="52" pageNumber="53">J. Pinzón</collectorName>
col. (
<collectionCode id="E8D5AE23FF899014FEF76057FE25F9A1" box="[317,404,1549,1573]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34802" name="Natural History Museum of Los Angeles County" pageId="52" pageNumber="53">LACM</collectionCode>
