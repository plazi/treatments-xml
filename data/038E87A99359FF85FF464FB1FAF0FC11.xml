<taxonomicName id="4C274D3C9358FF85FD054E4EFCA7FE64" box="[724,882,360,384]" class="Adenophorea" family="Leptolaimidae" genus="Antomicron" kingdom="Animalia" order="Araeolaimida" pageId="5" pageNumber="44" phylum="Nematoda" rank="species" species="alveolatum">
