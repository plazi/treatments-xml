<taxonomicName id="4C274D3C9358FF85FD634E8BFCE0FE21" box="[690,821,429,453]" class="Adenophorea" family="Leptolaimidae" genus="Antomicron" kingdom="Animalia" order="Araeolaimida" pageId="5" pageNumber="44" phylum="Nematoda" rank="genus">
