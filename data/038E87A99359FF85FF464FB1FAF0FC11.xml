<number id="3DEF24989358FF85FCE74EACFC90FE46" box="[822,837,394,418]" pageId="5" pageNumber="44" value="9.0">9</number>
<emphasis id="B953EAAD9358FF85FC9D4EAAFCBFFE47" box="[844,874,396,419]" italics="true" pageId="5" pageNumber="44">vs.</emphasis>
<number id="3DEF24989358FF85FCA34EACFC5AFE46" box="[882,911,394,418]" pageId="5" pageNumber="44" value="35.0">35</number>
