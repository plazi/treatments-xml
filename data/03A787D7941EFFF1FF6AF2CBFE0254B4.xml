<taxonomicName id="4C0E4D42941EFFF0FA80F506FE7E5089" authority="Linnavuori, 1986" authorityName="Linnavuori" authorityYear="1986" class="Insecta" family="Miridae" genus="Megacoelum" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="169" phylum="Arthropoda" rank="species" species="salsolae">
<emphasis id="B97AEAD3941EFFF0FA80F506FF4A5089" italics="true" pageId="18" pageNumber="169">M. salsolae</emphasis>
Linnavuori, 
<number id="3DC624E6941EFFF0FE77F5E3FE7E5089" box="[394,455,1436,1461]" pageId="18" pageNumber="169" value="1986.0">1986</number>
</taxonomicName>
*, 
<taxonomicName id="4C0E4D42941EFFF0FE1FF5E2FCAC5088" authority="Reuter, 1904" authorityName="Reuter" authorityYear="1904" box="[482,789,1436,1461]" class="Insecta" family="Miridae" genus="Megacoelum" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="169" phylum="Arthropoda" rank="species" species="sordidum">
<emphasis id="B97AEAD3941EFFF0FE1FF5E2FDCA5089" box="[482,627,1436,1461]" italics="true" pageId="18" pageNumber="169">M. sordidum</emphasis>
Reuter, 
<number id="3DC624E6941EFFF0FD25F5E3FCAC5088" box="[728,789,1436,1460]" pageId="18" pageNumber="169" value="1904.0">1904</number>
</taxonomicName>
*, 
<taxonomicName id="4C0E4D42941EFFF0FCD2F5E2FBE15089" authority="Wagner, 1958" authorityName="Wagner" authorityYear="1958" box="[815,1112,1436,1461]" class="Insecta" family="Miridae" genus="Waucoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="169" phylum="Arthropoda" rank="species" species="tricolor">
<emphasis id="B97AEAD3941EFFF0FCD2F5E2FC155089" box="[815,940,1436,1461]" italics="true" pageId="18" pageNumber="169">M. tricolor</emphasis>
Wagner, 
<number id="3DC624E6941EFFF0FBE0F5E3FBE15089" box="[1053,1112,1436,1461]" pageId="18" pageNumber="169" value="1958.0">1958</number>
