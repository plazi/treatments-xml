<taxonomicName id="859684E8813C0B37FEFEC42EFE68D427" authority="Fraser 1972" authorityName="Fraser" authorityYear="1972" box="[273,477,582,604]" class="Actinopterygii" family="Apogonidae" genus="Verulux" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="36" pageNumber="187" phylum="Chordata" rank="genus">
