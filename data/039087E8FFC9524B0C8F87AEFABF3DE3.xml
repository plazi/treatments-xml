<number id="3DF124D9FFCA524A0F748343FC5B3C2B" box="[876,929,295,319]" pageId="6" pageNumber="45" value="0.35">0.35</number>
); pronotum length 
<number id="3DF124D9FFCA524A089B8343FB533C2B" box="[1155,1193,295,319]" pageId="6" pageNumber="45" value="3.1">3.1</number>
; prozona length 2.0; metazona length 1.0– 
<number id="3DF124D9FFCA524A0D918328FE503C70" box="[393,426,332,356]" pageId="6" pageNumber="45" value="1.1">1.1</number>
(
<number id="3DF124D9FFCA524A0DA28328FE153C70" box="[442,495,332,356]" pageId="6" pageNumber="45" value="1.08">1.08</number>
