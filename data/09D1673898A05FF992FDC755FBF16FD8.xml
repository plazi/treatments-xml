<paragraph id="A6330F1F09C97A217B57CBC7E88176BD" pageId="66" pageNumber="67">
<taxonomicName id="292D4DCE35687C8321BC54A0B9F4F007" authorityName="Shi &amp; Zhou &amp; Liang" authorityYear="2013" baseAuthorityName="Habu" baseAuthorityYear="1979" class="Insecta" family="Carabidae" genus="Dasiosoma" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="bellum" status="comb. n.">Dasiosoma bellum</taxonomicName>
