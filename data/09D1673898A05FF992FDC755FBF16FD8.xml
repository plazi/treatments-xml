<taxonomicName id="26731D17D27B3760661F0AAEC11989C0" authorityName="Shi &amp; Zhou &amp; Liang" authorityYear="2013" baseAuthorityName="Shi &amp; Zhou &amp; Liang" baseAuthorityYear="2013" class="Insecta" family="Carabidae" genus="Teradaia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="genus" status="comb. n.">
