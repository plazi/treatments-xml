(
<taxonomicName id="2E9619E1FF8ACA73FD60A4AB7C10C709" authorityName="Bernhauer" authorityYear="1929" box="[641,783,1003,1024]" class="Insecta" family="Staphylinidae" genus="Hygropetrophila" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="143" phylum="Arthropoda" rank="genus">
<emphasis id="DBE2BE70FF8ACA73FD60A4AB7C10C709" box="[641,783,1003,1024]" italics="true" pageId="18" pageNumber="143">Hygropetrophila</emphasis>
