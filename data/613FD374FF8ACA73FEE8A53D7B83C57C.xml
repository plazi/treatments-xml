(
<taxonomicName id="2E9619E1FF8ACA73FD64A3477C0CC715" authorityName="Bernhauer" authorityYear="1929" box="[645,787,1031,1052]" class="Insecta" family="Staphylinidae" genus="Hygropetrophila" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="143" phylum="Arthropoda" rank="genus">
