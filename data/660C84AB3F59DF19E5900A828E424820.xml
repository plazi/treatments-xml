<paragraph id="C65AE7D87C23374233C55437F8E86330" pageId="5" pageNumber="54">
<taxonomicName id="8225D80B6E0B6D247290B58AA3F8E499" authority="Scopoli, 1777" authorityName="Scopoli" authorityYear="1777" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bulimus atricallosus" order="Stylommatophora" pageId="5" pageNumber="54" phylum="Mollusca" rank="species" species="atricallosus">Bulimus atricallosus</taxonomicName>
