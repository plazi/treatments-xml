<taxonomicName id="4C5E14FCB052FFF3FDFFFB12FBC884A6" authority="K. Viets, 1930" authorityName="K. Viets" authorityYear="1930" box="[513,1047,1209,1235]" class="Arachnida" family="Eylaidae" genus="Eylais" kingdom="Animalia" order="Trombidiformes" pageId="25" pageNumber="60" phylum="Arthropoda" rank="subSpecies" species="rimosa" subSpecies="proceripalpis">
<emphasis id="B92AB36DB052FFF3FDFFFB12FC8B84A7" box="[513,852,1209,1234]" italics="true" pageId="25" pageNumber="60">Eylais rimosa proceripalpis</emphasis>
<bibRefCitation id="EFCF128EB052FFF3FC9FFB12FBC884A6" author="Viets" box="[865,1047,1209,1235]" lastPageId="25" lastPageNumber="64" pageId="25" pageNumber="63" refString="Viets, K. (1930) Zur Kenntnis der Hydracarinen-Fauna von Spanien. Archiv fur Hydrobiologie, 21, 175 - 240, 21, 359 - 446." type="journal article" year="1930">
