<collectingCountry id="F3547655F55FFFDBFED5FC91ADCC6B5A" box="[359,426,769,794]" name="China" pageId="4" pageNumber="565">China</collectingCountry>
(
<collectingRegion id="4987F827F55FFFDBFE0BFC91AE246B5A" box="[441,578,769,794]" country="China" name="Guangdong" pageId="4" pageNumber="565">Guangdong</collectingRegion>
