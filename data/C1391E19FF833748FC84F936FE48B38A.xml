<taxonomicName id="8E90D48CFF843748FE86FF1CFE72B2EE" authorityName="Verrill" authorityYear="1909" box="[368,490,229,250]" class="Asteroidea" family="Goniasteridae" genus="Hippasteria" kingdom="Animalia" order="Valvatida" pageId="20" pageNumber="286" phylum="Echinodermata" rank="species" species="spinosa">
