<treatmentCitation id="0ABD6908FF957833FEEAF8ACFDF7F8FA" author="Walker" box="[373,583,1792,1815]" page="121" pageId="5" pageNumber="6" year="1850">
<bibRefCitation id="EF8D32E8FF957833FEEAF8ACFDBCF8FA" author="Walker" box="[373,524,1792,1815]" pageId="5" pageNumber="21" refString="Walker, F. (1850) List of the Specimens of Homopterous Insects in the Collection of the British Museum, British Museum Trustees, London, 1, 1 - 260." type="journal article" year="1850">
Walker, 
<number id="3DD45D3EFF957833FE4CF8ACFDBCF8FA" box="[467,524,1792,1815]" pageId="5" pageNumber="6" value="1850.0">1850</number>
</bibRefCitation>
: 
<number id="3DD45D3EFF957833FD83F8ACFDF7F8FA" box="[540,583,1792,1815]" pageId="5" pageNumber="6" value="121.0">121</number>
</treatmentCitation>
</taxonomicName>
