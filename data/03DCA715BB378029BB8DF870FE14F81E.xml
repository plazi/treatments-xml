<caption id="DF0A468BBB368029BB8DFB17FCF2FA85" ID-DOI="http://doi.org/10.5281/zenodo.5371932" ID-Zenodo-Dep="5371932" box="[136,793,1268,1290]" httpUri="https://zenodo.org/record/5371932/files/figure.png" pageId="1" pageNumber="290" startId="1.[136,229,1268,1290]" targetBox="[182,1412,339,1220]" targetPageId="1">
<paragraph id="8BCA1603BB368029BB8DFB17FCF2FA85" blockId="1.[136,793,1268,1290]" box="[136,793,1268,1290]" pageId="1" pageNumber="290">
