<specimenCode id="CF3B9EC3FF920740FF8A7624FF36FEFA" box="[93,217,301,323]" collectionCode="UF" country="USA" lsid="urn:lsid:biocol.org:col:34858" name="Florida Museum of Natural History- Zoology, Paleontology and Paleobotany" pageId="73" pageNumber="74" type="Museum">UF 18061</specimenCode>
