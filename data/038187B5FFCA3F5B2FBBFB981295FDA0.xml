<taxonomicName id="4C284D20FFCA3F582B5BFB0515B3FB6B" box="[1143,1277,1220,1244]" class="Insecta" family="Diaspididae" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="228" phylum="Arthropoda" rank="family">Diaspididae</taxonomicName>
