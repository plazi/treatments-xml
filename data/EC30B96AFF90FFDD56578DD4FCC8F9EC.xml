<taxonomicName id="A39973FFFF90FFDD57FF8D9FFDDDFC98" authority="(C. F. Ludw.) Desf., Tableau de l'Ecole de Botanique" baseAuthorityName="C. F. Ludw." class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="quercifolia">
