<subSubSection id="C3296538922D2A6CFF016E61FC3DF9EE" pageId="4" pageNumber="85" type="etymology">
<paragraph id="8B8C36B3922D2A6CFF016E61FC4CFA56" blockId="4.[151,1437,1009,1538]" pageId="4" pageNumber="85">
<emphasis id="B947EAA1922D2A6CFF016E61FE9AFA7A" bold="true" box="[199,337,1405,1430]" pageId="4" pageNumber="85">Etymology:</emphasis>
