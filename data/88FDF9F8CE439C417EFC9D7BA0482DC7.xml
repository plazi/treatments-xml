, 
<taxonomicName id="4587B6CF29F35609F10DD893FBB4D2A4" baseAuthorityName="Morelet" baseAuthorityYear="1863" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="15" pageNumber="16" phylum="Mollusca" rank="species" species="andoicus">
