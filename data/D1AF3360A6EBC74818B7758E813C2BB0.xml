<geoCoordinate id="60ABF1C9BC2AE1EC457220F62AB6126C" direction="south" orientation="latitude" precision="555" value="-28.33">28.33S</geoCoordinate>
<geoCoordinate id="35C7544E7A8B5267352195FFF2F8D6D9" direction="east" orientation="longitude" precision="555" value="152.88">152.88E</geoCoordinate>
