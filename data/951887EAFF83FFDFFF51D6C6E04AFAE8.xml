<paragraph id="1D0E36FCFF83FFDEFF51D579E72FF9A2" blockId="64.[151,879,1602,1657]" box="[151,879,1633,1657]" pageId="64" pageNumber="300">
<taxonomicName id="DAB14D7FFF83FFDEFF51D579E652F9A2" authority="Sars, 1911 f" authorityName="sensu Sars" authorityYear="1911" box="[151,530,1633,1657]" class="Hexanauplia" family="Miraciidae" genus="Microthalestris" kingdom="Animalia" order="Harpacticoida" pageId="64" pageNumber="300" phylum="Arthropoda" rank="species" species="littoralis">
