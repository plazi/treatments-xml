<taxonomicName id="52D5EFF1F42AFFF260EB76E14A14FF05" baseAuthorityName="Holthuis" baseAuthorityYear="1986" box="[989,1103,198,219]" class="Malacostraca" family="Atyidae" genus="Typhlatya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="792" phylum="Arthropoda" rank="genus">
