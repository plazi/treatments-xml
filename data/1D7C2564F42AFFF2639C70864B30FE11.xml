<collectionCode id="F3C40CB7F42AFFF2666477234B3FFEC7" box="[1362,1380,260,281]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="6" pageNumber="792" type="Museum">V</collectionCode>
