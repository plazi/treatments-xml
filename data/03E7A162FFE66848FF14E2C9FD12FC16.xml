<taxonomicName id="4C4E6BF7FFE66848FCBAE31CFC46FC32" authorityName="Leclerc" authorityYear="1989" box="[825,957,871,897]" class="Arachnida" family="Opilioacaridae" genus="Siamacarus" kingdom="Animalia" order="Opilioacarida" pageId="21" pageNumber="353" phylum="Arthropoda" rank="genus">
