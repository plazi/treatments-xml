, * 
<taxonomicName id="4C7B4D7AFF80E46AAA8551BB452BFC0A" authority="Michx." authorityName="Michx." box="[759,1064,978,1003]" class="Magnoliopsida" family="Lamiaceae" genus="Lycopus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="uniflorus">
