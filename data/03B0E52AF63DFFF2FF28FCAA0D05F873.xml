<taxonomicName id="4C192FBFF63DFFF2FEB7F96F0B78F94A" authority="Kormilev, 1956" authorityName="Kormilev" authorityYear="1956" box="[264,601,1758,1783]" class="Insecta" family="Aradidae" genus="Parapictinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="427" phylum="Arthropoda" rank="genus">
