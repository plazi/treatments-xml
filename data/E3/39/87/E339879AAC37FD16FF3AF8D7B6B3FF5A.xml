<emphasis bold="true" box="[151,340,1689,1711]" pageId="8" pageNumber="253">FIGURES 46–54.</emphasis>
<taxonomicName box="[347,537,1689,1710]" class="Insecta" family="Cicadellidae" genus="Sobrala" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="253" phylum="Arthropoda" rank="species" species="lamellaris" status="sp. nov.">
<emphasis box="[347,537,1689,1710]" italics="true" pageId="8" pageNumber="253">Sobrala lamellaris</emphasis>
</taxonomicName>
<emphasis bold="true" box="[544,625,1689,1711]" pageId="8" pageNumber="253">
<taxonomicNameLabel box="[544,625,1689,1711]" pageId="8" pageNumber="253" rank="species">sp. nov.</taxonomicNameLabel>
