<emphasis bold="true" box="[151,605,1484,1510]" pageId="29" pageNumber="30">
<taxonomicName box="[151,505,1484,1510]" class="Insecta" family="Cicadellidae" genus="Lamelliassus" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="chiangmaiensis" status="sp. nov.">
<emphasis bold="true" box="[151,505,1484,1510]" italics="true" pageId="29" pageNumber="30">Lamelliassus chiangmaiensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[512,605,1484,1510]" pageId="29" pageNumber="30" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
