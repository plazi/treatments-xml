<paragraph pageId="268" pageNumber="269">
<taxonomicName class="Insecta" family="Carabidae" genus="Homophron" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="268" pageNumber="269" phylum="Arthropoda" rank="subSpecies" species="tanneri" subSpecies="proximum">Homophron tanneri proximum</taxonomicName>
