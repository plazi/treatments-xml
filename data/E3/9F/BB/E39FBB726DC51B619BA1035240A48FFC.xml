<paragraph pageId="268" pageNumber="269">
<taxonomicName authorityName="Casey" authorityYear="1920" class="Hexapoda" family="Carabidae" genus="Omophron" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="268" pageNumber="269" phylum="Arthropoda" rank="species" species="lengi">Omophron lengi</taxonomicName>
