<number box="[864,983,1079,1105]" pageId="4" pageNumber="5" value="1.33" valueMax="1.45" valueMin="1.21">1.21–1.45</number>
, 
<quantity box="[996,1104,1079,1105]" metricMagnitude="-3" metricUnit="m" metricValue="1.31" pageId="4" pageNumber="5" unit="mm" value="1.31">1.31 mm</quantity>
