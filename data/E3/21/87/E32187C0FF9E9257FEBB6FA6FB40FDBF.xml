<number box="[460,580,1079,1105]" pageId="4" pageNumber="5" value="2.87" valueMax="3.34" valueMin="2.4">2.40–3.34</number>
, 
<quantity box="[592,700,1079,1105]" metricMagnitude="-3" metricUnit="m" metricValue="2.73" pageId="4" pageNumber="5" unit="mm" value="2.73">2.73 mm</quantity>
