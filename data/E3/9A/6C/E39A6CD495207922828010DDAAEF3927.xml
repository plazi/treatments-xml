<taxonomicName family="Colletinae" lsidName="" pageId="57" pageNumber="58" rank="family">Colletinae</taxonomicName>
(
<taxonomicName class="Insecta" family="Colletidae" genus="Colletes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Colletes" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">Colletes</taxonomicName>
