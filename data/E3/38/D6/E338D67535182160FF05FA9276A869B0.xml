and 
<collectingCountry box="[1111,1162,1870,1893]" name="Peru" pageId="6" pageNumber="436">Peru</collectingCountry>
(
<bibRefCitation author="Metcalf" box="[1176,1331,1869,1894]" pageId="6" pageNumber="436" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
Metcalf 
<number box="[1272,1331,1869,1894]" pageId="6" pageNumber="436" value="1963.0">1963</number>
</bibRefCitation>
; 
<bibRefCitation author="Sanborn" pageId="6" pageNumber="436" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book" year="2013">
Sanborn 
<number box="[151,212,1905,1929]" pageId="6" pageNumber="436" value="2013.0">2013</number>
