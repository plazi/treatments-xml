<collectingCountry box="[1168,1253,151,176]" name="Mexico" pageId="20" pageNumber="450">Mexico</collectingCountry>
(
<bibRefCitation author="Metcalf" box="[1272,1431,151,176]" pageId="20" pageNumber="450" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
Metcalf 
<number box="[1372,1431,151,176]" pageId="20" pageNumber="450" value="1963.0">1963</number>
</bibRefCitation>
; 
<bibRefCitation author="Sanborn" box="[151,324,188,213]" pageId="20" pageNumber="450" refString="Sanborn, A. F. (2007 b) New species, new records and checklist of cicadas from Mexico (Hemiptera: Cicadomorpha: Cicadidae). Zootaxa, 1651, 1 - 42." type="journal article" year="2007" yearSuffix="b">
Sanborn 
<number box="[251,310,188,213]" pageId="20" pageNumber="450" value="2007.0">2007</number>
b
</bibRefCitation>
; 
<bibRefCitation author="Sanborn" box="[336,397,188,212]" pageId="20" pageNumber="450" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book" year="2013">
