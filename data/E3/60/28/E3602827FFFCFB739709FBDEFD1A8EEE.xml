<paragraph blockId="10.[598,1034,1088,1118]" box="[598,1034,1088,1118]" pageId="10" pageNumber="11">
<heading bold="true" box="[598,1034,1088,1118]" centered="true" fontSize="12" level="1" pageId="10" pageNumber="11" reason="0">
<taxonomicName authority="Waldock 2013" authorityName="Waldock" authorityYear="2013" box="[598,1034,1088,1118]" class="Arachnida" family="Salticidae" genus="Maratus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="karrie">
<emphasis bold="true" box="[598,1034,1088,1118]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[598,817,1088,1118]" italics="true" pageId="10" pageNumber="11">Maratus karrie</emphasis>
<bibRefCitation author="Waldock, J. M." box="[824,1034,1089,1118]" journalOrPublisher="Records of the Western Australian Museum" pageId="10" pageNumber="11" pagination="66 - 81" part="28" refId="ref9122" refString="Waldock, J. M. 2013. A review of the peacock spiders of the Maratus mungaich species-group (Araneae: Salticidae), with descriptions of four new species. Records of the Western Australian Museum 28 (1): 66 - 81." title="A review of the peacock spiders of the Maratus mungaich species-group (Araneae: Salticidae), with descriptions of four new species" type="journal article" year="2013">Waldock 2013</bibRefCitation>
</emphasis>
</taxonomicName>
