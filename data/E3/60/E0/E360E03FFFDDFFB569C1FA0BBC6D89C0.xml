<quantity box="[412,476,1445,1471]" metricMagnitude="2" metricUnit="m" metricValue="6.1" pageId="4" pageNumber="49" unit="m" value="610.0">
<elevation box="[412,476,1445,1471]" metricMagnitude="2" metricUnit="m" metricValue="6.1" pageId="4" pageNumber="49" unit="m" value="610.0">610 m</elevation>
</quantity>
