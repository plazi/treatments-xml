<taxonomicName class="Gastropoda" family="Alacuppidae" genus="Roxania" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Roxania" order="Cephalaspidea" pageId="0" pageNumber="505" phylum="Mollusca" rank="genus">Roxania</taxonomicName>
