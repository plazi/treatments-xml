<subSubSection pageId="12" pageNumber="247" type="diagnosis">
<paragraph blockId="12.[151,1437,541,1070]" box="[151,635,541,566]" pageId="12" pageNumber="247">
<emphasis bold="true" box="[151,266,541,566]" pageId="12" pageNumber="247">Diagnosis</emphasis>
. Body 
<quantity box="[348,416,542,566]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="12" pageNumber="247" unit="mm" value="8.0">8 mm</quantity>
; fore wing 
<quantity box="[542,631,541,566]" metricMagnitude="-3" metricUnit="m" metricValue="6.2" pageId="12" pageNumber="247" unit="mm" value="6.2">6.2 mm</quantity>
