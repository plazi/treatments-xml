, 
<emphasis bold="true" box="[708,809,1218,1243]" pageId="2" pageNumber="555">Yunnan,</emphasis>
Lanping County, Tu’e Town, near Dahua Hydropower Plant (
<geoCoordinate box="[223,322,1254,1279]" direction="north" orientation="latitude" pageId="2" pageNumber="555" precision="555" value="26.34">
<number box="[223,291,1254,1279]" pageId="2" pageNumber="555" value="26.34">26.34</number>
°N
</geoCoordinate>
, 
<geoCoordinate box="[333,427,1255,1279]" direction="east" orientation="longitude" pageId="2" pageNumber="555" precision="555" value="99.14">
<number box="[333,400,1255,1279]" pageId="2" pageNumber="555" value="99.14">99.14</number>
°E
</geoCoordinate>
, alt. 
<quantity box="[472,562,1254,1279]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="2" pageNumber="555" unit="m" value="1500.0">1,500m</quantity>
), 
<date box="[582,711,1254,1279]" pageId="2" pageNumber="555" value="2013-07-25">
<number box="[582,611,1254,1279]" pageId="2" pageNumber="555" value="25.0">25</number>
.vii. 
<number box="[650,711,1254,1279]" pageId="2" pageNumber="555" value="2013.0">2013</number>
</date>
