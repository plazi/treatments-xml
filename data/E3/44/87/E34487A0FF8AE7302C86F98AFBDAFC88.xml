<taxonomicName authorityName="Parks" authorityYear="1922" class="Reptilia" family="Lambeosauridae" genus="Parasaurolophus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ornithischia" pageId="5" pageNumber="66" phylum="Chordata" rank="species" species="walkeri">
