<taxonomicName baseAuthorityName="Hooley" baseAuthorityYear="1925" box="[1062,1371,1474,1496]" class="Reptilia" family="Iguanodontidae" genus="Mantellisaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ornithischia" pageId="5" pageNumber="66" phylum="Chordata" rank="species" species="atherfieldensis">
