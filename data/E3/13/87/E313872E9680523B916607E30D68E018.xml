<taxonomicName class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" lsidName="Pinus armandii" order="Pinales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="armandii">
<emphasis italics="true" pageId="0" pageNumber="1">Pinus armandii</emphasis>
</taxonomicName>
