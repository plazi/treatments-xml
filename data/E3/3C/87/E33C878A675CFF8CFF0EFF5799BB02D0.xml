<number box="[1115,1128,690,714]" pageId="21" pageNumber="22" value="2.0">2</number>
and r 
<number box="[1198,1234,690,714]" pageId="21" pageNumber="22" value="4.5" valueMax="5.0" valueMin="4.0">4-5</number>
inserted dorsally, setae r 
<number box="[227,238,726,750]" pageId="21" pageNumber="22" value="1.0">1</number>
and r 
<number box="[305,318,726,750]" pageId="21" pageNumber="22" value="3.0">3</number>
