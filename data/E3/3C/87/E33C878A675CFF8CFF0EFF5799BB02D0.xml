<number box="[924,962,690,714]" pageId="21" pageNumber="22" value="3.5" valueMax="6.0" valueMin="1.0">1-6</number>
, z 
<number box="[989,1028,690,714]" pageId="21" pageNumber="22" value="4.0" valueMax="6.0" valueMin="2.0">2-6</number>
, s 
<number box="[1053,1091,690,714]" pageId="21" pageNumber="22" value="3.5" valueMax="6.0" valueMin="1.0">1-6</number>
