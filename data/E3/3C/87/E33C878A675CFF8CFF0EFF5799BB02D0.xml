gdZ 
<number box="[725,736,1086,1110]" pageId="21" pageNumber="22" value="1.0">1</number>
</emphasis>
(Po 
<number box="[786,796,1086,1110]" pageId="21" pageNumber="22" value="1.0">1</number>
) situated anteromedial to insertions of Z 
<number box="[1254,1265,1086,1110]" pageId="21" pageNumber="22" value="1.0">1</number>
; 
