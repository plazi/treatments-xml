<taxonomicName authority="Adorador &amp; Fernando" authorityName="Adorador &amp; Fernando, Mt. Cagua" authorityYear="2022" box="[270,704,1850,1872]" class="Liliopsida" family="Arecaceae" genus="Pinanga" kingdom="Plantae" order="Arecales" pageId="3" pageNumber="186" phylum="Tracheophyta" rank="species" species="leonardcoi">
