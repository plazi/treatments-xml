<paragraph id="CDBEB78A88C9A2CDEF483CA642793CF1" pageId="0" pageNumber="1">
<taxonomicName id="9C0551449FC4D24B26BF7C8A92CD3D82" LSID="http://zoobank.org/F2AB2C7F-6930-49F5-86E5-8400492F4887" authority="Lonsdale, 2021" authorityName="Lonsdale" authorityYear="2021" class="Insecta" family="Agromyzidae" genus="Ophiomyia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ophiomyia laticolis" order="Diptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="laticolis" status="sp. nov.">Ophiomyia laticolis</taxonomicName>
