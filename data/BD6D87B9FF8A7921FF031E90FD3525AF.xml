<collectingDate id="513EE987FF8B792FFC101D5AFBB72130" box="[923,1027,1616,1638]" pageId="7" pageNumber="430" value="2003-08-04">4.viii.2003</collectingDate>
</date>
, leg. 
<collectorName id="98315379FF8B792FFF1C1D60FF4A212A" box="[151,254,1641,1663]" pageId="7" pageNumber="430">H. Mendel</collectorName>
