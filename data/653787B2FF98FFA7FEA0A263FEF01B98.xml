<collectingCountry id="95897634FF99FFA7FD72A31BFCFC1D48" box="[730,865,604,630]" name="Philippines" pageId="27" pageNumber="28">Philippines</collectingCountry>
, 
<collectingCountry id="95897634FF99FFA7FCDCA31BFC731D48" box="[884,1006,604,630]" name="Singapore" pageId="27" pageNumber="28">Singapore</collectingCountry>
