<subSubSection id="C30763C49A6BB139FF49FDD55105FDE5" box="[151,609,535,561]" pageId="2" pageNumber="51" type="nomenclature">
<paragraph id="8BA2304F9A6BB139FF49FDD55105FDE5" blockId="2.[151,609,535,561]" box="[151,609,535,561]" pageId="2" pageNumber="51">
<heading id="D0EA87239A6BB139FF49FDD55105FDE5" bold="true" box="[151,609,535,561]" fontSize="11" level="1" pageId="2" pageNumber="51" reason="1">
<emphasis id="B969EC5D9A6BB139FF49FDD55105FDE5" bold="true" box="[151,609,535,561]" pageId="2" pageNumber="51">
Subfamily 
<taxonomicName id="4C1D4BCC9A6BB139FEC1FDD55105FDE5" authority="Stach, 1935" authorityName="Stach" authorityYear="1935" box="[287,609,535,561]" class="Gymnolaemata" family="Catenicellidae" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="51" phylum="Bryozoa" rank="subFamily" subFamily="Ditaxiporinae">
Ditaxiporinae 
<bibRefCitation id="EF8C4DBE9A6BB139FE0DFDD55105FDE5" author="Stach, L. W." box="[467,609,535,561]" pageId="2" pageNumber="51" pagination="389 - 396" refId="ref4555" refString="Stach, L. W. (1935) The genera of Catenicellidae. Proceedings of the Royal Society of Victoria, n. s., 47, 389 - 396." type="journal article" year="1935">Stach, 1935</bibRefCitation>
</taxonomicName>
</emphasis>
