<taxonomicName id="4C22EDA4C125831E13708E9890DCFC78" authorityName="Runyon &amp; Capellari" authorityYear="2018" box="[880,973,941,966]" class="Insecta" family="Dolichopodidae" genus="Chrysotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="581" phylum="Arthropoda" rank="species" species="keyensis">
