10. 
<taxonomicName id="68D7756EBBD37C5F28E5346F7CE6B748" LSID="BA1253DA-75F8-5971-98F0-038BDCF0CE19" authority="J. Compton, Mattapha, Sirich. &amp; Schrire" authorityName="J. Compton, Mattapha, Sirich. &amp; Schrire" authorityYear="2019" class="Magnoliopsida" family="Fabaceae" genus="Kanburia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Kanburia" order="Fabales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="genus" status="gen. nov.">Kanburia J.Compton, Mattapha, Sirich. &amp; Schrire</taxonomicName>
