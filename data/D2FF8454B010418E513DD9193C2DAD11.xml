<caption id="56879163425CA393FF8E5A7BC0457566" ID-Zenodo-Dep="3252283" doi="10.3897/dez.66.34309.figures30-35" httpUri="https://binary.pensoft.net/fig/310316" pageId="6" pageNumber="91" start="Figures 30–35" startId="F7">
<paragraph id="C875651DCDE7B7888E1FBF4E037E3F81" pageId="6" pageNumber="91">
<emphasis id="5DAEF3A46BA7160BE237431DE9A96ECC" bold="true" pageId="6" pageNumber="91">Figures 30-35.</emphasis>
<taxonomicName id="A5541997CB3A7DEEF62A927A9356E6E9" authority="Le Peletier &amp; Audinet-Serville, 1828" authorityName="Le Peletier &amp; Audinet-Serville" authorityYear="1828" class="Insecta" family="Tenthredinidae" genus="Empria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Empria" order="Hymenoptera" pageId="6" pageNumber="91" phylum="Arthropoda" rank="genus">
<emphasis id="309CEB1E708F567ABDCA5A84CCFDB9E9" italics="true" pageId="6" pageNumber="91">Empria</emphasis>
</taxonomicName>
species. 
<emphasis id="145F7C1781AA35C43CF3209C8C222395" bold="true" pageId="6" pageNumber="91">30</emphasis>
<taxonomicName id="7DFA2CD6639A52E68FF0507A46479AEA" authority="Le Peletier &amp; Audinet-Serville, 1828" authorityName="Le Peletier &amp; Audinet-Serville" authorityYear="1828" baseAuthorityName="Audinet-Serville" baseAuthorityYear="1823" class="Insecta" family="Tenthredinidae" genus="Empria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Empria sexpunctata" order="Hymenoptera" pageId="6" pageNumber="91" phylum="Arthropoda" rank="species" species="sexpunctata">
<emphasis id="0A0C9D9F48CD3AD2342D16D6F6DB70E6" italics="true" pageId="6" pageNumber="91">Empria sexpunctata</emphasis>
