<specimenCount id="08ABFD1C0EA4FF06FED3A8D3FE96FC3E" box="[346,363,963,979]" pageId="159" pageNumber="300" type="male">♂</specimenCount>
, here designated, 
<collectionCode id="78BCAE500EA4FF06FD87A8D2FDCBFC3F" box="[526,566,962,978]" pageId="159" pageNumber="300">CSM</collectionCode>
<collectingCountry id="66BA76050EA4FF06FDC8A8D2FD76FC3F" box="[577,651,962,978]" name="Germany" pageId="159" pageNumber="300">Germany</collectingCountry>
, 
<location id="1B72604E0EA4FF06FD10A8D2FEE8FC04" LSID="urn:lsid:plazi:treatment:960487830EA4FF06FF30A9FBFB2CF89D:1B72604E0EA4FF06FD10A8D2FEE8FC04" country="Germany" county="State" municipality="Black Forest" name="Black Forest Krummenbach" pageId="159" pageNumber="300">
<collectingMunicipality id="FE76ACEF0EA4FF06FD10A8D2FCFEFC3F" box="[665,771,962,978]" pageId="159" pageNumber="300">Black Forest</collectingMunicipality>
Krummenbach
</location>
, hyp. Grundw., 1960; 
<taxonomicName id="D9AD4D160EA4FF06FE44A8C9FD95FC04" authorityName="SCHWOERBEL" authorityYear="1961" box="[461,616,985,1001]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="159" pageNumber="300" phylum="Arthropoda" rank="species" species="longipes">
