<materialsCitation id="AEC53CC80EA4FF06FE88A8D2FE4FFBC0" collectionCode="CSM" country="Germany" county="State" location="Black Forest Krummenbach" municipality="Black Forest" pageId="159" pageNumber="300" specimenCount="2" specimenCount-male="2" typeStatus="lectotype">
