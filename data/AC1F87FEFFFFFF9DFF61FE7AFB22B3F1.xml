<taxonomicName id="E3B64D6BFFFFFF9DFF61FE7AFD72B69F" authority="(Hartig, 1843)" baseAuthorityName="Hartig" baseAuthorityYear="1843" box="[151,712,402,428]" class="Insecta" family="Cynipidae" genus="Pseudoneuroterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="478" phylum="Arthropoda" rank="species" species="macropterus">
