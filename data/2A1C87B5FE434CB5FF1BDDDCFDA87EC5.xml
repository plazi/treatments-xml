<quantity id="654D9B46FE4E4CB5FDA1DA52FD4D7E3F" box="[592,642,1313,1337]" metricMagnitude="1" metricUnit="m" metricValue="5.0927" pageId="472" pageNumber="473" unit="in" value="2005.0">2005</quantity>
