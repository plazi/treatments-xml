<taxonomicName id="8B984D2C4B61004774167AD5FD439ABA" authorityName="Drake" authorityYear="1927" box="[527,672,1683,1709]" class="Insecta" family="Tingidae" genus="Alloiothucha" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="370" phylum="Arthropoda" rank="genus">
<emphasis id="7EECEABD4B61004774167AD5FD439ABA" box="[527,672,1683,1709]" italics="true" pageId="1" pageNumber="370">Alloiothucha</emphasis>
</taxonomicName>
and 
<taxonomicName id="8B984D2C4B61004774C37AD5FC809ABA" authorityName="Drake &amp; Ruhoff" authorityYear="1965" box="[730,867,1683,1709]" class="Insecta" family="Tingidae" genus="Cetiothucha" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="370" phylum="Arthropoda" rank="genus">
