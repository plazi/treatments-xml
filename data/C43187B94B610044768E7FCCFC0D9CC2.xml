; 
<taxonomicName id="8B984D2C4B61004777567B2DFE099B93" authorityName="Drake" authorityYear="1927" box="[335,490,1899,1925]" class="Insecta" family="Tingidae" genus="Alloiothucha" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="370" phylum="Arthropoda" rank="species" species="necopinata">
