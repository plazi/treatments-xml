<taxonomicName id="2DBE7AB2FFAAC53EE7F4F923A6FBF90A" authorityName="Platnick &amp; Duperre" authorityYear="2010" class="Arachnida" family="Oonopidae" genus="Scaphios" kingdom="Animalia" order="Araneae" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="jatun">
