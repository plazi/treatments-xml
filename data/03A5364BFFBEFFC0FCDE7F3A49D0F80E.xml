<date id="FFB2A19DFFBEFFC2FAD57FBC48D3F817" box="[1337,1439,1922,1942]" pageId="28" pageNumber="29" value="1987-07-18">18.vii.1987</date>
, QM Berlesate No. 782, 
<geoCoordinate id="EE38E19AFFBEFFC2FBAC7FA149CAF832" box="[1088,1158,1951,1971]" degrees="16" direction="south" minutes="48" orientation="latitude" pageId="28" pageNumber="29" precision="925" value="-16.8">16°48′S</geoCoordinate>
, 
<geoCoordinate id="EE38E19AFFBEFFC2FB7B7FA149A6F832" box="[1175,1258,1951,1971]" degrees="179" direction="east" minutes="26" orientation="longitude" pageId="28" pageNumber="29" precision="925" value="179.43333">179°26′E</geoCoordinate>
, 
<specimenCount id="9D0A4CD4FFBEFFC2FB177FA14875F832" box="[1275,1337,1951,1971]" pageId="28" pageNumber="29" type="generic">
<quantity id="4CF42AB8FFBEFFC2FB177FA14879F832" box="[1275,1333,1951,1971]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="28" pageNumber="29" unit="m" value="20.0">20 m</quantity>
