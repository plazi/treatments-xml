<number id="3D8124900B51FF9CFB3AF97BFB537AD2" box="[1217,1228,1758,1784]" pageId="4" pageNumber="55" value="1.0">1</number>
</figureCitation>
A, 
<number id="3D8124900B51FF9CFB0FF97BFA9F7AD2" box="[1268,1280,1758,1784]" pageId="4" pageNumber="55" value="1.0">1</number>
B, 
<number id="3D8124900B51FF9CFADEF97BFAA97AD2" box="[1317,1334,1758,1784]" pageId="4" pageNumber="55" value="2.0">2</number>
