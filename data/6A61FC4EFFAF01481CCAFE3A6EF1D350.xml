<caption id="B6B71DD0FFAF01481F6FFCC06F9CDD04" ID-DOI="http://doi.org/10.5281/zenodo.5714406" ID-Zenodo-Dep="5714406" httpUri="https://zenodo.org/record/5714406/files/figure.png" inLine="true" pageId="2" pageNumber="525" targetBox="[155,740,574,983]" targetPageId="2">
<paragraph id="E2774D58FFAF01481F6FFCC06F9CDD04" blockId="2.[772,1358,568,990]" lastBlockId="2.[152,1357,1003,2680]" pageId="2" pageNumber="525">
<emphasis id="D0BC914AFFAF01481F6FFCC069E5DA40" box="[772,1172,843,872]" pageId="2" pageNumber="525">Subspecies and Distribution.</emphasis>
