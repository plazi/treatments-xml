<paragraph id="471ADA6642DBC86532251AA4B9CF29C4" pageId="0" pageNumber="1">
<taxonomicName id="358104D6D352646327A0E351EC03187D" genus="Archaea" lsidName="Archaea gracilicollis" pageId="0" pageNumber="1" rank="species" species="gracilicollis">Archaea gracilicollis</taxonomicName>
