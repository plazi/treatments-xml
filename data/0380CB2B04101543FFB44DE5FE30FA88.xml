<taxonomicName id="4C2901BE04101543FB404B14FA8DFAA3" authorityName="Deshayes" authorityYear="1863" box="[1123,1382,1341,1367]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="25" pageNumber="26" phylum="Mollusca" rank="species" species="angustissima">
