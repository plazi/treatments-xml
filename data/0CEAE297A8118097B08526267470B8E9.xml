<taxonomicName id="799BDC1399616FAF0ADC855F56505968" class="Insecta" family="Chrysomelidae" genus="Charidotella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Charidotella" order="Coleoptera" pageId="2" pageNumber="63" phylum="Arthropoda" rank="genus">Charidotella</taxonomicName>
