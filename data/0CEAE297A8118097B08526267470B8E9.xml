<taxonomicName id="A3877DB966D2C3964BD81A15E89AA92D" authority="Spaeth, 1913" authorityName="Spaeth" authorityYear="1913" class="Insecta" family="Chrysomelidae" genus="Philaspis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Philaspis" order="Coleoptera" pageId="2" pageNumber="63" phylum="Arthropoda" rank="genus">
