<taxonomicName id="4C70A5BCFFB7FF92FB4BFD98FAED0C24" authorityName="A.B.Williams" authorityYear="1988" box="[1185,1342,548,569]" class="Malacostraca" family="Alvinocarididae" genus="Alvinocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="18" pageNumber="463" phylum="Arthropoda" rank="species" species="stactophila">
