<date id="E40EFFC1FFFDFFCFFF4FFF5AFEA2FF1C" box="[188,301,188,213]" pageId="3" pageNumber="4">
<collectingDate id="F44A0629FFFDFFCFFF4FFF5AFEA2FF1C" box="[188,301,188,213]" pageId="3" pageNumber="4">1 October</collectingDate>
</date>
on 
<collectingCounty id="796EA18DFFFDFFCFFEAFFF5AFE61FF1C" box="[348,494,188,213]" pageId="3" pageNumber="4">Talim Island</collectingCounty>
</materialsCitation>
. 
<materialsCitation id="20D8D35CFFFDFFCFFE08FF5BFB16FF1D" ID-GBIF-Occurrence="1455395212" box="[507,1177,188,213]" country="Iceland" location="Luzon" pageId="3" pageNumber="4" specimenCount="1" typeStatus="holotype">
