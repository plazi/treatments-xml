<geoCoordinate id="F584BFC6FFFDFFCFFF64FF06FE89FF31" box="[151,262,224,248]" direction="east" orientation="longitude" pageId="3" pageNumber="4" precision="555" value="121.23">121.23°E</geoCoordinate>
].
