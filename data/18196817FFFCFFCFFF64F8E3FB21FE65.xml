<typeStatus id="4F0B67A3FFFDFFCFFD0AFF5BFCA6FF1C" box="[761,809,189,213]" pageId="3" pageNumber="4">type</typeStatus>
locality as Talim 
<collectingCountry id="E8A79991FFFDFFCFFC09FF5AFBCEFF1C" box="[1018,1089,188,213]" name="Iceland" pageId="3" pageNumber="4">Island</collectingCountry>
, 
<location id="956F8FDAFFFDFFCFFBBDFF5BFB16FF1D" LSID="urn:lsid:plazi:treatment:18196817FFFCFFCFFF64F8E3FB21FE65:956F8FDAFFFDFFCFFBBDFF5BFB16FF1D" box="[1102,1177,189,212]" country="Iceland" name="Luzon" pageId="3" pageNumber="4">Luzon</location>
</materialsCitation>
, 
<materialsCitation id="20D8D35CFFFDFFCFFB55FF5AFE9EFF31" ID-GBIF-Occurrence="1455395213" country="Philippines" latitude="14.36" location="Philippines" longLatPrecision="773" longitude="121.23" pageId="3" pageNumber="4" specimenCount="1" typeStatus="holotype">
<collectingCountry id="E8A79991FFFDFFCFFB55FF5AFAABFF1C" box="[1190,1316,188,213]" name="Philippines" pageId="3" pageNumber="4">Philippines</collectingCountry>
[
<geoCoordinate id="F584BFC6FFFDFFCFFAC7FF5AFA18FF1C" box="[1332,1431,188,213]" direction="north" orientation="latitude" pageId="3" pageNumber="4" precision="555" value="14.36">14.36°N</geoCoordinate>
