<taxonomicName id="4C314D29FFC8FFA0FE041F53CC41FCB7" box="[492,594,844,868]" class="Insecta" family="Tipulidae" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="478" phylum="Arthropoda" rank="family">Tipulidae</taxonomicName>
