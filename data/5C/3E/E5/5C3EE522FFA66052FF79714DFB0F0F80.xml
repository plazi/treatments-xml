); II: 
<number box="[771,810,343,367]" pageId="9" pageNumber="56" value="2.5" valueMax="4.0" valueMin="1.0">1-4</number>
- 
<number box="[819,834,343,367]" pageId="9" pageNumber="56" value="3.0">3</number>
(
<number box="[844,855,343,367]" pageId="9" pageNumber="56" value="1.0">1</number>
)- 
<number box="[875,891,343,367]" pageId="9" pageNumber="56" value="5.0">5</number>
(
<number box="[901,912,343,367]" pageId="9" pageNumber="56" value="1.0">1</number>
), III: 
<number box="[980,996,343,367]" pageId="9" pageNumber="56" value="2.0">2</number>
- 
<number box="[1005,1021,343,367]" pageId="9" pageNumber="56" value="2.0">2</number>
- 
<number box="[1031,1047,343,367]" pageId="9" pageNumber="56" value="2.0">2</number>
(
<number box="[1057,1067,343,367]" pageId="9" pageNumber="56" value="1.0">1</number>
)- 
<number box="[1086,1102,343,367]" pageId="9" pageNumber="56" value="2.0">2</number>
(
<number box="[1112,1122,343,367]" pageId="9" pageNumber="56" value="1.0">1</number>
), IV: 
<number box="[1189,1207,343,367]" pageId="9" pageNumber="56" value="2.0">2</number>
- 
<number box="[1216,1227,343,367]" pageId="9" pageNumber="56" value="1.0">1</number>
- 
<number box="[1238,1249,343,367]" pageId="9" pageNumber="56" value="1.0">1</number>
(
