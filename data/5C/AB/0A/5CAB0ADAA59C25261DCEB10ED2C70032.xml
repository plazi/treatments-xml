<taxonomicName class="Liliopsida" family="Poaceae" genus="Zea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Zea mays" order="Poales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="mays">Zea mays</taxonomicName>
Linnaeus (
<taxonomicName genus="Panicoideae" lsidName="Panicoideae" pageId="0" pageNumber="1" rank="subfamily" subfamily="Panicoideae">Panicoideae</taxonomicName>
