<taxonomicName box="[653,786,1714,1735]" class="Insecta" family="Scirtidae" genus="Eurycyphon" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="21" pageNumber="472" phylum="Arthropoda" rank="species" species="castaneus" status="n. sp.">E. castaneus</taxonomicName>
,
</emphasis>
