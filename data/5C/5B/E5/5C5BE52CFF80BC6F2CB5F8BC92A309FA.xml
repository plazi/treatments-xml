<taxonomicName authority="Watts" authorityName="Watts" box="[335,505,1640,1662]" class="Insecta" family="Scirtidae" genus="Eurycyphon" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="18" pageNumber="469" phylum="Arthropoda" rank="species" species="aquilus">
