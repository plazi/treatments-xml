<taxonomicName class="Dothideomycetes" family="Mycosphaerellaceae" genus="Cercospora" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Cercospora tentaculifera" order="Mycosphaerellales" pageId="0" pageNumber="69" phylum="Ascomycota" rank="species" species="tentaculifera">
<emphasis italics="true" pageId="0" pageNumber="69">Cercospora tentaculifera</emphasis>
</taxonomicName>
on 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Vigna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vigna unguiculata" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="unguiculata">
