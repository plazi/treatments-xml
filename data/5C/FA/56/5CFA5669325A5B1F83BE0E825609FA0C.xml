<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Vigna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vigna unguiculata" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="unguiculata">
