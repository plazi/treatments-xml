<taxonomicName authorityName="Tryon" authorityYear="1867" class="Gastropoda" family="Bulimulidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="65" pageNumber="66" phylum="Mollusca" rank="family">Bulimulidae</taxonomicName>
, 
