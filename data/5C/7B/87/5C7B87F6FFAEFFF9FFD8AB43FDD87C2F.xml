<subSubSection pageId="8" pageNumber="237" type="discussion">
<paragraph pageId="8" pageNumber="237">
<emphasis bold="true" box="[155,262,1507,1526]" pageId="8" pageNumber="237">Comments.</emphasis>
<emphasis box="[268,463,1507,1525]" italics="true" pageId="8" pageNumber="237">
<taxonomicName box="[268,463,1507,1525]" class="Insecta" family="Piophilidae" genus="Parapiophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="237" phylum="Arthropoda" rank="species" species="vulgaris">Parapiophila vulgaris</taxonomicName>
</emphasis>
