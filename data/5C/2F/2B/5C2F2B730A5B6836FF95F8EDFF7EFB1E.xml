<taxonomicName authorityName="Rathbun" authorityYear="1909" box="[253,475,285,309]" class="Malacostraca" family="Pinnotheridae" genus="Pinnotheres" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="195" phylum="Arthropoda" rank="species" species="lanensis">
