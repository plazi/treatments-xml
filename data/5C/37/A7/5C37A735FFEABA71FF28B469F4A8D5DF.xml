<quantity box="[886,1044,1095,1120]" metricMagnitude="3" metricUnit="m" metricValue="3.45" metricValueMax="4.0" metricValueMin="2.9" pageId="16" pageNumber="17" unit="m" value="3450.0" valueMax="4000.0" valueMin="2900.0">2900–4000 m</quantity>
