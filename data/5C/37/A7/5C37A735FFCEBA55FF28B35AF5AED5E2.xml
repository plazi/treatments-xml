<collectionCode box="[468,526,1170,1193]" pageId="52" pageNumber="53">UVZ</collectionCode>
(not catalogued) from Nariño, La Rejoya, Buesaco [
<quantity box="[1114,1276,1169,1193]" metricMagnitude="3" metricUnit="m" metricValue="1.85" metricValueMax="2.2" metricValueMin="1.5" pageId="52" pageNumber="53" unit="m" value="1850.0" valueMax="2200.0" valueMin="1500.0">1500–2200 m</quantity>
