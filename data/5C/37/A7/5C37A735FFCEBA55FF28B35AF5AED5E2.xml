<taxonomicName class="Gastropoda" family="Orthalicidae" genus="Auris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="calliostoma">
<emphasis italics="true" pageId="52" pageNumber="53">P. calliostoma</emphasis>
</taxonomicName>
