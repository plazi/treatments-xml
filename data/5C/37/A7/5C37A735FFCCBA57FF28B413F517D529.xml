<emphasis bold="true" box="[151,602,879,905]" pageId="54" pageNumber="55">
“ 
<taxonomicName box="[165,320,879,905]" class="Gastropoda" family="Orthalicidae" genus="Plekocheilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="54" pageNumber="55" phylum="Mollusca" rank="genus">
<emphasis bold="true" box="[165,320,879,905]" italics="true" pageId="54" pageNumber="55">Plekocheilus</emphasis>
</taxonomicName>
” 
<taxonomicName authority="Pfeiffer, 1854" authorityName="Pfeiffer" authorityYear="1854" box="[342,602,879,905]" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="54" pageNumber="55" phylum="Mollusca" rank="species" species="victor">
<emphasis bold="true" box="[342,410,879,905]" italics="true" pageId="54" pageNumber="55">victor</emphasis>
(
<bibRefCitation author="Pfeiffer" box="[425,594,879,905]" pageId="54" pageNumber="55" refString="Pfeiffer, L. (1854 ─ 1860). Novitates conchologicae ..., vol. 1. Th. Fischer, Cassel, 138 pp., 36 pls." type="book" year="1854">
