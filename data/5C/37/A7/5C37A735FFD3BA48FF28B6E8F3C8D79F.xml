<quantity box="[845,974,1292,1316]" metricMagnitude="3" metricUnit="m" metricValue="1.2349999999999999" metricValueMax="2.46" metricValueMin="0.01" pageId="41" pageNumber="42" unit="m" value="1235.0" valueMax="2460.0" valueMin="10.0">10–2460 m</quantity>
