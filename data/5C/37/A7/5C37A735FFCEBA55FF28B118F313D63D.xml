</taxonomicName>
(
<taxonomicName box="[323,419,1636,1662]" class="Mollusca" family="Mollusca" genus="Eurytus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca" pageId="52" pageNumber="53" phylum="Mollusca" rank="genus">
