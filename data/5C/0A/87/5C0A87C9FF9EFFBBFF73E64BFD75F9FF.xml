<number box="[1177,1239,404,428]" pageId="3" pageNumber="64" value="11.56">11.56</number>
(
<number box="[1253,1306,404,428]" pageId="3" pageNumber="64" value="3.1">3.10</number>
, 
<number box="[1317,1369,404,428]" pageId="3" pageNumber="64" value="1.2">1.20</number>
, 
<number box="[1380,1432,404,428]" pageId="3" pageNumber="64" value="2.55">2.55</number>
, 
<number box="[151,203,439,463]" pageId="3" pageNumber="64" value="2.83">2.83</number>
, 
<number box="[214,267,439,463]" pageId="3" pageNumber="64" value="1.88">1.88</number>
); III 
<number box="[322,385,439,463]" pageId="3" pageNumber="64" value="10.64">10.64</number>
(
<number box="[399,452,439,463]" pageId="3" pageNumber="64" value="2.6">2.60</number>
, 
<number box="[463,515,439,463]" pageId="3" pageNumber="64" value="1.14">1.14</number>
