<location LSID="urn:lsid:plazi:treatment:5C618789FFDB081EFF72FF193EFB99A8:D1176044FFDB081EFC4AFD2D38839D20" box="[1016,1178,745,769]" country="Philippines" municipality="Luzon" name="Mt. Banahao" pageId="7" pageNumber="6">Mt. Banahao</location>
”
</materialsCitation>
.
</paragraph>
</subSubSection>
<subSubSection box="[192,978,798,822]" pageId="7" pageNumber="6" type="distribution">
<paragraph blockId="7.[192,978,798,822]" box="[192,978,798,822]" pageId="7" pageNumber="6">
<emphasis bold="true" box="[192,371,798,822]" pageId="7" pageNumber="6">Distribution.</emphasis>
<collectingCountry box="[378,520,798,822]" name="Philippines" pageId="7" pageNumber="6">Philippines</collectingCountry>
, Luzon (Prov. 
<collectingRegion box="[695,791,798,822]" country="Philippines" name="Quezon" pageId="7" pageNumber="6">Quezon</collectingRegion>
