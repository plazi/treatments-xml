<subSubSection box="[151,1070,292,314]" pageId="2" pageNumber="283" type="materials_examined">
<paragraph blockId="2.[151,1070,292,314]" box="[151,1070,292,314]" pageId="2" pageNumber="283">
<typeStatus box="[151,201,293,314]" pageId="2" pageNumber="283">Type</typeStatus>
