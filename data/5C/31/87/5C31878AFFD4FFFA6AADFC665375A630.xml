with 24 
<taxonomicName authority=", Cu" authorityName="Cu" box="[188,296,1864,1890]" class="Insecta" family="Chironomidae" genus="Lopescladius" kingdom="Animalia" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="setae">setae, Cu</taxonomicName>
with 17 
<taxonomicName authority=", Cu" authorityName="Cu" box="[402,510,1864,1890]" class="Insecta" family="Chironomidae" genus="Lopescladius" kingdom="Animalia" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="setae">setae, Cu</taxonomicName>
<subScript attach="left" box="[510,518,1880,1894]" fontSize="6" pageId="46" pageNumber="47">1</subScript>
