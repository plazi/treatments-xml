<quantity box="[1074,1242,1386,1412]" metricMagnitude="-2" metricUnit="m" metricValue="2.5625" metricValueMax="2.725" metricValueMin="2.4" pageId="33" pageNumber="34" unit="mm" value="25.625" valueMax="27.25" valueMin="24.0">24–27, 25 µm</quantity>
; of midtibia 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="2.5119999999999996" metricValueMax="2.7239999999999998" metricValueMin="2.3" pageId="33" pageNumber="34" unit="mm" value="25.119999999999997" valueMax="27.24" valueMin="23.0">23– 27, 24 µm</quantity>
; of hind tibia 
<quantity box="[436,603,1426,1452]" metricMagnitude="-2" metricUnit="m" metricValue="2.814" metricValueMax="2.928" metricValueMin="2.7" pageId="33" pageNumber="34" unit="mm" value="28.14" valueMax="29.28" valueMin="27.0">27–29, 28 µm</quantity>
. Comb with 12–13, 13 setae; longest 
<quantity box="[1041,1209,1426,1452]" metricMagnitude="-2" metricUnit="m" metricValue="2.8645" metricValueMax="3.229" metricValueMin="2.5" pageId="33" pageNumber="34" unit="mm" value="28.645" valueMax="32.29" valueMin="25.0">25–32, 29 µm</quantity>
; shortest 
