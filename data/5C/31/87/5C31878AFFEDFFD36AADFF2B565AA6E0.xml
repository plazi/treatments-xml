long, spurs of midtibia 
<quantity box="[839,918,2002,2028]" metricMagnitude="-2" metricUnit="m" metricValue="3.2" pageId="23" pageNumber="24" unit="mm" value="32.0">32 µm</quantity>
and 
<quantity box="[977,1055,2002,2028]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="23" pageNumber="24" unit="mm" value="20.0">20 µm</quantity>
long, spurs of hind tibia 
<quantity box="[1358,1436,2002,2028]" metricMagnitude="-2" metricUnit="m" metricValue="4.8" pageId="23" pageNumber="24" unit="mm" value="48.0">48 µm</quantity>
</paragraph>
<paragraph blockId="24.[151,1436,152,218]" pageId="24" pageNumber="25">
and 
<quantity box="[203,281,152,178]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="24" pageNumber="25" unit="mm" value="20.0">20 µm</quantity>
