<typeStatus box="[199,258,1876,1900]" pageId="15" pageNumber="16">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Cuvier, 1829" authorityName="Cuvier" authorityYear="1829" box="[361,777,1876,1900]" class="Reptilia" family="Gekkonidae" genus="Ascalabotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="leachianus">
