<number box="[971,1087,1858,1884]" pageId="7" pageNumber="60" value="20.085" valueMax="28.17" valueMin="12.0">12–28, 17</number>
(
<number box="[1104,1119,1858,1884]" pageId="7" pageNumber="60" value="5.0">5</number>
