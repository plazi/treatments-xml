<subSubSection pageId="1" pageNumber="54" type="materials_examined">
<paragraph blockId="1.[264,1323,1113,1870]" pageId="1" pageNumber="54">
<emphasis bold="true" box="[264,516,1113,1139]" pageId="1" pageNumber="54">Material examined:</emphasis>
<typeStatus box="[530,639,1113,1139]" pageId="1" pageNumber="54">Holotype</typeStatus>
