<subSubSection id="1793B4CA8450948EFFEE2C29CC3E66DD" pageId="250" pageNumber="251" type="type species">
<paragraph id="AC12C84326D57775D32B0BA07B84F028" pageId="250" pageNumber="251">Type species.</paragraph>
<paragraph id="8AC81742A67F04BB1807AAAA64EF3E25" pageId="250" pageNumber="251">
<taxonomicName id="88C0E637CD65BA8B7C79EC5220049AEB" class="Magnoliopsida" family="Vivianiaceae" genus="Viviania" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Viviania aureofasciata" order="Geraniales" pageId="250" pageNumber="251" phylum="Tracheophyta" rank="species" species="aureofasciata">Viviania aureofasciata</taxonomicName>
