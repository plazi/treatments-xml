<taxonomicName id="558F0FD7FF044970FF3D8C34D5C1FE7F" authority="Chittenden." authorityName="Chittenden." box="[173,478,371,392]" class="Insecta" family="Cerambycidae" genus="Oberea" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="749" phylum="Arthropoda" rank="species" species="ulmicola">
<emphasis id="A0FBA846FF044970FF3D8C34D5C1FE7F" bold="true" box="[173,478,371,392]" pageId="11" pageNumber="749">
<emphasis id="A0FBA846FF044970FF3D8C34D545FE7F" bold="true" box="[173,346,371,392]" italics="true" pageId="11" pageNumber="749">Oberea ulmicola</emphasis>
Chittenden.
</emphasis>
</taxonomicName>
New larval host
</heading>
</paragraph>
<paragraph id="92307454FF044970FF578CCAD514FE55" blockId="11.[173,660,371,661]" box="[199,267,397,418]" pageId="11" pageNumber="749">record.</paragraph>
