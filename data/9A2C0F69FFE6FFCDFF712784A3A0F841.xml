<taxonomicName id="D585C5FCFFE6FFCDFEDE220FA247FA99" authority="Matile" authorityName="Matile" box="[312,583,1308,1334]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="ctenophora">
