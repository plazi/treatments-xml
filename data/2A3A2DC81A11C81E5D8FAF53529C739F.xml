<paragraph id="C98E5317CB6039560160D1EBAC665116" pageId="0" pageNumber="218" type="mainText">
<taxonomicName id="52874D3392A443A233478E4AE458B61A" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="218" phylum="Arthropoda" rank="species" species="segregans">
<pageBreakToken id="51462EDE92DC706C726C57B9FC8A3B39" pageId="0" pageNumber="218" start="start">Eupolybothrus</pageBreakToken>
segregans
</taxonomicName>
<taxonomicNameLabel id="A56A181F5D6A551CDE89620CBD04C628" pageId="0" pageNumber="218" rank="species">sp. n.</taxonomicNameLabel>
</paragraph>
