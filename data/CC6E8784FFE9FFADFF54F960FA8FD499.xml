<collectingCountry id="3CD07602FFEEFFACFD40FBC8FB17D1E1" box="[796,889,1132,1158]" name="Mexico" pageId="20" pageNumber="21">Mexico</collectingCountry>
; 
<collectingCountry id="3CD07602FFEEFFACFDDBFBC8FB84D1E1" box="[903,1002,1132,1158]" name="Grenada" pageId="20" pageNumber="21">Grenada</collectingCountry>
(Henderson 
<number id="F20F24B5FFEEFFACFAD8FBC8FCADD1E1" box="[1156,1219,1132,1158]" pageId="20" pageNumber="21" value="1920.0">1920</number>
); 
<collectingCountry id="3CD07602FFEEFFACFA85FBC8FD4BD1E1" box="[1241,1317,1132,1158]" name="Brazil" pageId="20" pageNumber="21">Brazil</collectingCountry>
: Ceará (Henderson 
<number id="F20F24B5FFEEFFACFFBFFB30FA4FD1C9" box="[483,545,1172,1198]" pageId="20" pageNumber="21" value="1920.0">1920</number>
), Bahia and Rio de Janeiro (this study). Living 
<quantity id="833F9B77FFEEFFACFA11FB30FC81D1C9" box="[1101,1263,1172,1198]" metricMagnitude="3" metricUnit="m" metricValue="1.3" metricValueMax="1.6" metricValueMin="1.0" pageId="20" pageNumber="21" unit="m" value="1300.0" valueMax="1600.0" valueMin="1000.0">1000–1600 m</quantity>
