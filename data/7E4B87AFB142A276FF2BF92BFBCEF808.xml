<emphasis id="C496EAABB142A276FF2BF8BDFEA6F850" bold="true" box="[151,304,1905,1930]" pageId="116" pageNumber="117">Distribution.</emphasis>
<collectingCountry id="8EF57629B142A276FE8BF8BDFE0BF850" box="[311,413,1905,1930]" name="Thailand" pageId="116" pageNumber="117">Thailand</collectingCountry>
, 
<collectingCountry id="8EF57629B142A276FE15F8BDFE7AF850" box="[425,492,1905,1930]" name="China" pageId="116" pageNumber="117">China</collectingCountry>
(
<collectingRegion id="3426F85BB142A276FE46F8BDFDD8F850" box="[506,590,1905,1930]" country="China" name="Hainan" pageId="116" pageNumber="117">Hainan</collectingRegion>
) and 
<collectingCountry id="8EF57629B142A276FD31F8BDFD7AF850" box="[653,748,1905,1930]" name="Vietnam" pageId="116" pageNumber="117">Vietnam</collectingCountry>
(
<collectingRegion id="3426F85BB142A276FD46F8BEFCE3F850" box="[762,885,1905,1930]" country="Vietnam" name="Bac Giang" pageId="116" pageNumber="117">Bac Giang</collectingRegion>
, Bac Thai, 
<collectingRegion id="3426F85BB142A276FC48F8BEFB83F853" box="[1012,1045,1906,1929]" country="Bhutan" name="Ha" pageId="116" pageNumber="117">Ha</collectingRegion>
Tay, 
<collectingRegion id="3426F85BB142A276FBEEF8BEFB28F850" box="[1106,1214,1906,1930]" country="Vietnam" name="Lang Son" pageId="116" pageNumber="117">Lang Son</collectingRegion>
and 
<collectingRegion id="3426F85BB142A276FB48F8BEFAC4F850" box="[1268,1362,1905,1930]" country="Vietnam" name="Yen Bai" pageId="116" pageNumber="117">Yen Bai</collectingRegion>
). 
