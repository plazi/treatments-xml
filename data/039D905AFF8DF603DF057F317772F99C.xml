<quantity id="4CCC8CA9FF8DF606D8987ECB7175F802" box="[1212,1298,1931,1956]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="1" pageNumber="220" unit="mm" value="1.5" valueMax="2.0" valueMin="1.0">1–2 mm</quantity>
longo), lamina
</emphasis>
<emphasis id="B940FD5EFF8FF604DCB378507511FE89" box="[151,374,278,303]" italics="true" pageId="3" pageNumber="222">foliari ampliore 6.5–8</emphasis>
× 
<emphasis id="B940FD5EFF8FF604DDB47850760FFE89" box="[400,616,278,303]" italics="true" pageId="3" pageNumber="222">
<quantity id="4CCC8CA9FF8FF604DDB478507588FE89" box="[400,495,278,303]" metricMagnitude="-2" metricUnit="m" metricValue="4.75" metricValueMax="5.5" metricValueMin="4.0" pageId="3" pageNumber="222" unit="cm" value="4.75" valueMax="5.5" valueMin="4.0">4–5.5 cm</quantity>
(non 4.5–6
</emphasis>
× 
<emphasis id="B940FD5EFF8FF604DEA778507552FDA9" italics="true" pageId="3" pageNumber="222">
<quantity id="4CCC8CA9FF8FF604DEA778507693FE88" box="[643,756,278,303]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" metricValueMax="2.5" metricValueMin="1.5" pageId="3" pageNumber="222" unit="cm" value="2.0" valueMax="2.5" valueMin="1.5">1.5–2.5 cm</quantity>
