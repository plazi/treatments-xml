<quantity id="4CCC8CA9FF8FF604DE817830769CFE29" box="[677,763,374,399]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" metricValueMax="9.0" metricValueMin="5.0" pageId="3" pageNumber="222" unit="mm" value="7.0" valueMax="9.0" valueMin="5.0">5–9 mm</quantity>
