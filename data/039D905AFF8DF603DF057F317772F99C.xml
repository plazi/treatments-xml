<quantity id="4CCC8CA9FF8DF606D9107E2B710AF822" box="[1332,1389,1900,1924]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="220" unit="mm" value="1.0">1 mm</quantity>
