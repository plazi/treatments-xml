<taxonomicName id="6BE94D36FF86FFE3EF90FDA55722FD22" box="[656,798,619,643]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="178" phylum="Arthropoda" rank="genus">
