(mean 
<quantity id="4C86D2CC3447F32CAD70600CFA7EF8EE" box="[1320,1424,1838,1863]" metricMagnitude="-4" metricUnit="m" metricValue="4.6" pageId="8" pageNumber="273" unit="mm" value="0.46">0.46 mm</quantity>
), EW 
