<taxonomicName id="DC9710D9B961FFD9CE1CF9ECFA7D3E7A" authorityName="Dankittipakul &amp; Jocqué &amp; Singtripop" authorityYear="2012" baseAuthorityName="Thorell" baseAuthorityYear="1890" box="[1353,1426,1625,1649]" class="Arachnida" family="Zodariidae" genus="Mallinella" kingdom="Animalia" order="Araneae" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="hilaris">
