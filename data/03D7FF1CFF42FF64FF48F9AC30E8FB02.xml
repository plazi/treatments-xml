<taxonomicName id="4C7E3589FF42FF64FE56FFD733B7FB21" authorityName="Laporte" authorityYear="1840" box="[478,715,1209,1233]" class="Insecta" family="Rutelidae" genus="Pelidnota" kingdom="Animalia" order="Coleoptera" pageId="137" pageNumber="136" phylum="Arthropoda" rank="species" species="metallica">
<emphasis id="B90A9218FF42FF64FE56FFD733B7FB21" box="[478,715,1209,1233]" italics="true" pageId="137" pageNumber="136">Pelidnota metallica</emphasis>
</taxonomicName>
