<taxonomicName id="D6AD923DC4E727670A12770F93731C93" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Pinales" pageId="23" pageNumber="36" phylum="Coniferophyta" rank="genus">
