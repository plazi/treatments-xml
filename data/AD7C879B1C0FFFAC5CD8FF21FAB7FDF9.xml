<emphasis id="17A1EA9F1C0FFFAC5CD8FF21FEF9FF72" bold="true" box="[151,339,151,177]" italics="true" pageId="14" pageNumber="56">Vespa wilemani</emphasis>
<bibRefCitation id="41444B7C1C0FFFAC5D15FF21FDE0FF72" author="Meade-Waldo" box="[346,586,151,177]" pageId="14" pageNumber="56" refString="Meade-Waldo, G. (1911) New species of Diploptera in the collection of the British Museum. Part III. Annals and Magazine of Natural History (8) 7, 98 - 113." type="journal article" year="1911">
Meade-Waldo 
<number id="931D24AA1C0FFFAC5E5EFF21FDE0FF72" box="[529,586,151,177]" pageId="14" pageNumber="56" value="1911.0">1911</number>
</bibRefCitation>
</taxonomicName>
[
<taxonomicName id="E2D54D0E1C0FFFAC5E14FF21FCD5FF72" authority="Smith 1870" authorityName="Smith" authorityYear="1870" box="[603,895,151,177]" class="Insecta" family="Vespidae" genus="Vespa" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="56" phylum="Arthropoda" rank="species" species="vivax">
