<treatmentCitationGroup id="AB4511ABFFDD6F67846AFE20C47A1C9E" box="[264,639,398,422]" pageId="12" pageNumber="13">
<taxonomicName id="4C554D06FFDD6F67846AFE20C47F1C9E" authority="Mosely 1954: 337" authorityName="Mosely" authorityPageNumber="337" authorityYear="1954" box="[264,634,398,422]" class="Insecta" family="Glossosomatidae" genus="Culoptila" kingdom="Animalia" order="Trichoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="aluca">
