<treatmentCitation id="0AF41094FFDD6F6784D7FE21C47F1C9E" author="Mosely" box="[437,634,398,421]" page="337" pageId="12" pageNumber="13" year="1954">
<bibRefCitation id="EFC44B74FFDD6F6784D7FE21C4401C9E" author="Mosely" box="[437,581,398,421]" pageId="12" pageNumber="51" refString="Mosely, M. E. (1954) The Protoptila group of the Glossosomatinae (Trichoptera: Rhyacophilidae). Bulletin of the British Museum (Natural History) Entomology, 3, 317 - 346." type="journal article" year="1954">
Mosely 
<number id="3D9D24A2FFDD6F67876EFE21C4401C9E" box="[524,581,398,421]" pageId="12" pageNumber="13" value="1954.0">1954</number>
</bibRefCitation>
