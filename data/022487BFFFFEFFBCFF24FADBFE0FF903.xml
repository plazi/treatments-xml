<collectingCountry id="F29A7639FFFEFFBCFCA6F99EFC2CF97B" box="[789,922,1663,1689]" name="Hong Kong" pageId="35" pageNumber="236">Hong Kong</collectingCountry>
) [
<collectingCountry id="F29A7639FFFEFFBCFC0BF99FFBB5F97B" box="[952,1027,1662,1689]" name="China" pageId="35" pageNumber="236">China</collectingCountry>
