<emphasis id="22D0AF6BE30EFFE0FF6DFE93931CFED2" bold="true" box="[192,368,353,378]" pageId="37" pageNumber="36">
<typeStatus id="CF1FCDDBE30EFFE0FF6DFE939369FED1" box="[192,261,354,378]" pageId="37" pageNumber="36">Type</typeStatus>
