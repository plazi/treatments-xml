<taxonomicName id="9AA44D14FFC6FFEFFE59E5D51FA8F81B" box="[389,447,1936,1957]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" order="Pinales" pageId="9" pageNumber="50" phylum="Tracheophyta" rank="genus">Pinus</taxonomicName>
yunnana
</emphasis>
</taxonomicName>
, Lancang
</taxonomicName>
, Prov
</taxonomicName>
