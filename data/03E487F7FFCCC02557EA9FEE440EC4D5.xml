<accessionNumber id="941EAB02FFCCC02555069D4644CDC156" box="[635,735,575,599]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/M00992" pageId="5" pageNumber="131">M00992</accessionNumber>
); Ichunjya-shita, scuba diving, 
<quantity id="4CB59B04FFCCC025533D9D46426EC156" box="[1088,1148,575,599]" metricMagnitude="1" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="131" unit="m" value="16.0">
<elevation id="0060D1D2FFCCC025533D9D46426EC156" box="[1088,1148,575,599]" metricMagnitude="1" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="131" unit="m" value="16.0">16 m</elevation>
