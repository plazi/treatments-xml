<typeStatus id="54DB2A88FFC8FF86FE37FD11FE5FB6EA" box="[414,500,686,710]" pageId="7" pageNumber="798">holotype</typeStatus>
et 
<specimenCount id="9D665FA3FFC8FF86FDBEFD11FD2CB6EA" box="[535,647,686,710]" pageId="7" pageNumber="798" type="generic" typeStatus="paratypes">4 paratypes</specimenCount>
(MNHN), 
<specimenCount id="9D665FA3FFC8FF86FF4AFD77FEE2B6CC" box="[227,329,712,736]" pageId="7" pageNumber="798" type="generic" typeStatus="paratype">1 paratype</specimenCount>
