<emphasis id="7392EAC90127FFB11B03FC6AFE7AFC7C" bold="true" box="[375,391,999,1017]" italics="true" pageId="10">¼</emphasis>
<quantity id="861E9B3E0127FFB11BF8FC6BFE2CFC7C" box="[396,465,998,1017]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="10" unit="mm" value="0.5">0.5 mm</quantity>
