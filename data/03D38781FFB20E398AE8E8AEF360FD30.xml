<collectionCode id="ED6BAE52FFB20E268803EF05F2F0FB9F" box="[636,739,1075,1101]" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="15" pageNumber="16">NHMW</collectionCode>
