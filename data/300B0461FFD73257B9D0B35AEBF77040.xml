<taxonomicName id="7FA2CEF4FFD73257BCC6B50BEE4C7040" authorityName="Puthz" authorityYear="2000" class="Insecta" family="Staphylinidae" genus="Dianous" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="283" phylum="Arthropoda" rank="species" species="gonggamontis">
