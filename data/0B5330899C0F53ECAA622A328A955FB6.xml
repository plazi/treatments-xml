<taxonomicName id="1C6574CC8A480682236B6AD005CF03A4" authorityName="Robineau-Desvoidy" authorityYear="1830" class="Insecta" family="Ephydridae" genus="Hydrellia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydrellia" order="Diptera" pageId="0" pageNumber="287" phylum="Arthropoda" rank="genus">
