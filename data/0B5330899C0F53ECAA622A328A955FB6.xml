<taxonomicName id="F90C6E4436F4C73C14C7EA7B3B81A946" class="Insecta" family="Braconidae" genus="Chaenusa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chaenusa aurantium" order="Hymenoptera" pageId="0" pageNumber="287" phylum="Arthropoda" rank="species" species="aurantium">
