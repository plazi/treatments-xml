<paragraph id="849EC3613EC6EA9BE1F460AEFB721B49" pageId="2" pageNumber="13">Not examined.</paragraph>
</subSubSection>
<subSubSection id="307524D58B96DFF67C9788289A27EEAE" pageId="2" pageNumber="13" type="material examined">
