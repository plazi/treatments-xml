<taxonomicName id="96A76BC3FFEBFFECA8E1FBFD7A3B601F" authorityName="EBMER &amp; SAKAGAMI" authorityYear="1994" box="[392,570,1060,1082]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="563" phylum="Arthropoda" rank="species" species="virideglaucum">
<emphasis id="63D3CC52FFEBFFECA8E1FBFD7A3B601F" bold="true" box="[392,570,1060,1082]" italics="true" pageId="22" pageNumber="563">L. virideglaucum</emphasis>
