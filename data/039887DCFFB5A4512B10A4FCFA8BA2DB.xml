<taxonomicName id="4C314D49FFB5A4512990A5AAFE30A7CA" authorityName="Bolton" authorityYear="1980" box="[189,460,951,976]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="pogonion">
