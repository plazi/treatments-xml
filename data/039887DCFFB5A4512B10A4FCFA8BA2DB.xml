<taxonomicName id="4C314D49FFB5A4512BD0A5AAFC8BA7CA" authorityName="Emery" authorityYear="1886" box="[765,887,950,976]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="solidum">
