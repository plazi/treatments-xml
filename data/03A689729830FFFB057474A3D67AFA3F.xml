<specimenCode id="DBA9901F983FFFFA07327425D385FAD6" box="[333,502,1337,1361]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="40" pageNumber="41" type="Museum">USNM 178709</specimenCode>
