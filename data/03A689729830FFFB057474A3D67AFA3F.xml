<materialsCitation id="3B673239983FFFFA06CF754ED38EFAD6" ID-GBIF-Occurrence="3059923302" collectingDate="1912-03-14" collectionCode="TYPE, MATERIAL" collectorName="E. A. Goldman" country="Panama" location="Darien province" pageId="40" pageNumber="41" specimenCode="USNM 178708, USNM 178709" specimenCount="3" stateProvince="Darien" typeStatus="holotype">
