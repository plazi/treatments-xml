<typeStatus id="54AE87B5FF90FF8BCCE7FA02FD98FA1C" box="[511,597,1443,1464]" pageId="3" pageNumber="100">holotype</typeStatus>
, 
<date id="FFAB1FD7FF90FF8BCF46FA02FD74FA1C" box="[606,697,1443,1464]" pageId="3" pageNumber="100" value="1992-12">XII-1992</date>
; 1
<emphasis id="B961E505FF90FF8BCFCDFA03FD24FA1C" bold="true" box="[725,745,1442,1464]" pageId="3" pageNumber="100">?</emphasis>
(NMNH), 1 
<emphasis id="B961E505FF90FF8BCE7DFA03FCB4FA1C" bold="true" box="[869,889,1442,1464]" pageId="3" pageNumber="100">/</emphasis>
