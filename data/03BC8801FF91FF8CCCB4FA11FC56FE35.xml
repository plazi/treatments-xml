<collectingCountry id="F3027987FF90FF8BCD8CFA1CFEEFFA76" box="[148,290,1469,1490]" name="Costa Rica" pageId="3" pageNumber="100">COSTA RICA</collectingCountry>
, 
<collectingRegion id="49D1F7F5FF90FF8BCC34FA1CFEB3FA76" box="[300,382,1469,1490]" country="Costa Rica" name="Alajuela" pageId="3" pageNumber="100">Alajuela</collectingRegion>
