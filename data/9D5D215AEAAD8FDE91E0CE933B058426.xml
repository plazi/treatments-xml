<taxonomicName id="626D860D423F44AF1125968593F75417" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181896" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole mackayi Wilson" lsidName-HNS="Pheidole mackayi Wilson" order="Hymenoptera" pageNumber="515" phylum="Arthropoda" rank="species" species="mackayi">mackayi</taxonomicName>
