<taxonomicName id="A579F896D0699A09ABB67E9336D71E8A" genus="Q." lsidName="Q. yini" pageId="0" pageNumber="51" rank="species" species="yini">
<emphasis id="7C089EC8A0AED617BF44A97415DE2B54" italics="true" pageId="0" pageNumber="51">Q. yini</emphasis>
</taxonomicName>
