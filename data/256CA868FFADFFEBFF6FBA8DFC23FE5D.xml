<collectorName id="00307CA8FFA2FFEBFA99BCE5FADFFF3D" box="[1377,1400,151,175]" pageId="56" pageNumber="57">St</collectorName>
<collectionCode id="CBD481BBFFA2FFEBFA86BCEAFA3CFF3D" box="[1406,1435,152,175]" pageId="56" pageNumber="57">IV</collectionCode>
