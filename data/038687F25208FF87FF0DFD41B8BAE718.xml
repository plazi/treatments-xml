<taxonomicName id="4C2F4D675208FF89FE84F888BE7AE418" authority="Campos &amp; Souza-Dias, 2021" authorityName="Campos &amp; Souza-Dias" authorityYear="2021" box="[286,525,1836,1860]" class="Insecta" family="Gryllidae" genus="Neometrypus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="458" phylum="Arthropoda" rank="species" species="carvalhoi" status="n. sp.">
