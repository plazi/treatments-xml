<taxonomicName id="25F34D3DFFF255455598FAB9FD86FAA6" box="[495,581,1331,1353]" class="Insecta" family="Gasteruptiidae" genus="Gasteruption" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="402" phylum="Arthropoda" rank="species" species="lacoulee" status="sp. nov.">lacoulee</taxonomicName>
</emphasis>
<taxonomicNameLabel id="CBB457D7FFF25545563BFABEFD5FFAA6" box="[588,668,1332,1353]" pageId="5" pageNumber="402" rank="species">
<emphasis id="D087EAACFFF25545563BFABEFD5FFAA6" bold="true" box="[588,668,1332,1353]" pageId="5" pageNumber="402">sp. nov.</emphasis>
</taxonomicNameLabel>
(7), 
<emphasis id="D087EAACFFF2554556B9FABEFCF0FAA6" box="[718,819,1332,1353]" italics="true" pageId="5" pageNumber="402">
G. 
<taxonomicName id="25F34D3DFFF25545569DFABEFCF0FAA6" box="[746,819,1332,1353]" class="Insecta" family="Gasteruptiidae" genus="Gasteruption" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="402" phylum="Arthropoda" rank="species" species="maquis" status="sp. nov.">maquis</taxonomicName>
</emphasis>
<taxonomicNameLabel id="CBB457D7FFF25545574DFABEFC49FAA6" box="[826,906,1332,1353]" pageId="5" pageNumber="402" rank="species">
<emphasis id="D087EAACFFF25545574DFABEFC49FAA6" bold="true" box="[826,906,1332,1353]" pageId="5" pageNumber="402">sp. nov.</emphasis>
</taxonomicNameLabel>
(9) and 
<emphasis id="D087EAACFFF255455796FABEFB9DFAA6" box="[993,1118,1332,1353]" italics="true" pageId="5" pageNumber="402">
G. 
<taxonomicName id="25F34D3DFFF25545578AFABEFB9DFAA6" box="[1021,1118,1332,1353]" class="Insecta" family="Gasteruptiidae" genus="Gasteruption" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="402" phylum="Arthropoda" rank="species" species="sarramea" status="sp. nov.">sarramea</taxonomicName>
</emphasis>
<taxonomicNameLabel id="CBB457D7FFF255455013FABEFB76FAA6" box="[1124,1205,1332,1353]" pageId="5" pageNumber="402" rank="species">
<emphasis id="D087EAACFFF255455013FABEFB76FAA6" bold="true" box="[1124,1205,1332,1353]" pageId="5" pageNumber="402">sp. nov.</emphasis>
