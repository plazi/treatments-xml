<quantity id="4CF99B59FF95EC72A2B1FA6A8D39F9FB" box="[452,556,1529,1552]" metricMagnitude="-2" metricUnit="m" metricValue="4.571999999999999" metricValueMax="5.842" metricValueMin="3.302" pageId="1" pageNumber="206" unit="in" value="1.7999999999999998" valueMax="2.3" valueMin="1.3">1.3­2.3 in</quantity>
