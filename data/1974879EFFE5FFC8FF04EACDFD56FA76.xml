<typeStatus id="4E66882AFFE5FFC9FCF2EB25FC41FEE2" box="[836,937,304,324]" pageId="16" pageNumber="1495">Lectotypus</typeStatus>
<taxonomicName id="56DD4D0BFFE5FFC9FC78EB25FBD5FEE2" box="[974,1085,304,324]" pageId="16" pageNumber="1495">
