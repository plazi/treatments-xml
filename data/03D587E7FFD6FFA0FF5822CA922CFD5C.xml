<collectingCountry id="F36B7661FFD4FFA0FDD320609464FE43" box="[519,594,378,404]" name="Kenya" pageId="6" pageNumber="7">Kenya</collectingCountry>
, 
<collectingCountry id="F36B7661FFD4FFA0FD89206094C5FE43" box="[605,755,378,404]" name="Mozambique" pageId="6" pageNumber="7">Mozambique</collectingCountry>
