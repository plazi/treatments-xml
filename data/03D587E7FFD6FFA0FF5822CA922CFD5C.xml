<taxonomicName id="4C7C4D72FFD4FFA0FF0120609773FE43" box="[213,325,378,404]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="chembe">
