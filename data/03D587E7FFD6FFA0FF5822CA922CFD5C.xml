<subSubSection id="C366657AFFD4FFA0FF6D231D96CEFDBF" pageId="6" pageNumber="7" type="etymology">
<paragraph id="8BC336F1FFD4FFA0FF6D231D96CEFDBF" blockId="6.[140,1108,167,651]" pageId="6" pageNumber="7">
<emphasis id="B908EAE3FFD4FFA0FF6D231D9776FDF6" bold="true" box="[185,320,519,545]" pageId="6" pageNumber="7">
<emphasis id="B908EAE3FFD4FFA0FF6D231D970DFDF6" box="[185,315,519,545]" inLineHeading="true" pageId="6" pageNumber="7" reason="1">Etymology</emphasis>
