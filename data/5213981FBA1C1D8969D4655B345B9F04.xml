<taxonomicName id="B03C6773F01DE557584AD71F77E57B7F" class="Insecta" family="Dytiscidae" genus="Hydroglyphus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydroglyphus orientalis" order="Coleoptera" pageId="5" pageNumber="142" phylum="Arthropoda" rank="species" species="orientalis">Hydroglyphus orientalis</taxonomicName>
(Clark, 1863) and 
<taxonomicName id="E16D7559D149FDF448FEB0AB2BBC146B" class="Insecta" family="Dytiscidae" genus="Sandracottus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sandracottus maculatus" order="Coleoptera" pageId="5" pageNumber="142" phylum="Arthropoda" rank="species" species="maculatus">Sandracottus maculatus</taxonomicName>
