<taxonomicName id="ECBCD66870FB1BAE243C0483B5E9B176" LSID="http://zoobank.org/DBB8F27A-3FB4-4740-8719-FF760BC6E150" authority="Fisher" authorityName="Fisher" authorityYear="2015" class="Insecta" family="Braconidae" genus="Colemanus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Colemanus" order="Hymenoptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="genus">Colemanus Fisher</taxonomicName>
<taxonomicNameLabel id="F2E4A5622F3084F42E0D1D0F22E3A161" pageId="4" pageNumber="61">gen. n.</taxonomicNameLabel>
