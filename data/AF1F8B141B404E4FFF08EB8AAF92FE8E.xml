<taxonomicName id="E0B641811B404E4EFF08EA18ADE4FE90" authority="Roder, 1885: 192" authorityName="Roder" authorityPageNumber="192" authorityYear="1885" box="[151,482,261,284]" class="Insecta" family="Asilidae" genus="Laphria" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="rufa">
<emphasis id="15C2E6101B404E4EFF08EA18AD26FE90" box="[151,288,261,284]" italics="true" pageId="18" pageNumber="19">Laphria rufa</emphasis>
<treatmentCitation id="A6171C131B404E4EFEB9EA18ADE4FE90" author="Roder" box="[294,482,261,284]" page="192" pageId="18" pageNumber="19" year="1885">
<bibRefCitation id="432747F31B404E4EFEB9EA18ADABFE90" author="Roder" box="[294,429,261,284]" pageId="18" pageNumber="23" refString="Roder, V. Von (1885) Ueber Dasypogon japonicum Bigot und Laphria rufa n. Sp. Aus Japan. Mitteilungen der schweizerischen entomologischen Gesellschaft, 7, 192 - 193." type="journal article" year="1885">
Röder, 
<number id="917E28251B404E4EFEEBEA18ADABFE90" box="[372,429,261,284]" pageId="18" pageNumber="19" value="1885.0">1885</number>
</bibRefCitation>
