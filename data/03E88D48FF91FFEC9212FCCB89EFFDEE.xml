<heading id="D0B68B32FF91FFED9212FCCB89B6FCAE" bold="true" box="[359,918,827,852]" centered="true" fontSize="8" level="2" pageId="4" pageNumber="147" reason="3">
<taxonomicName id="4C4147DDFF91FFED9212FCCB89B6FCAE" authority="(Blackburn, 1899)" baseAuthorityName="Blackburn" baseAuthorityYear="1899" box="[359,918,827,852]" class="Insecta" family="Ripiphoridae" genus="Macrosiagon" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="147" phylum="Arthropoda" rank="species" species="punctulaticeps">
