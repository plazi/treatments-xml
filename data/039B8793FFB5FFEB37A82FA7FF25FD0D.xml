<heading id="D0C581E9FFB5FFE637A82FA7FCF0F94A" bold="true" box="[151,809,1734,1760]" fontSize="11" level="1" pageId="7" pageNumber="457" reason="1">
<emphasis id="B946EA97FFB5FFE637A82FA7FCF0F94A" bold="true" box="[151,809,1734,1760]" pageId="7" pageNumber="457">
<taxonomicName id="4C324D06FFB5FFE637A82FA7FD99F94A" authority="Stal, 1862" authorityName="Stal" authorityYear="1862" box="[151,576,1734,1760]" class="Insecta" family="Cicadellidae" genus="Subrasaca" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="457" phylum="Arthropoda" rank="species" species="curvovittata">
<emphasis id="B946EA97FFB5FFE637A82FA7FE72F94A" bold="true" box="[151,427,1734,1760]" italics="true" pageId="7" pageNumber="457">Subrasaca curvovittata</emphasis>
(Stål, 
<number id="3DFA24A2FFB5FFE636C52FA7FDEEF94A" box="[506,567,1734,1760]" pageId="7" pageNumber="457" value="1862.0">1862</number>
)
</taxonomicName>
