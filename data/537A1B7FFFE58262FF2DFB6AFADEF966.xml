<number id="6D1BB84EFFE58262FE41FA44FDD1F9D3" box="[507,521,1528,1553]" pageId="12" pageNumber="486" value="6.0">6</number>
+ 
<number id="6D1BB84EFFE58262FDA3FA44FDFFF9D3" box="[537,551,1528,1553]" pageId="12" pageNumber="486" value="6.0">6</number>
(I, M, B 
<number id="6D1BB84EFFE58262FD32FA45FD40F9D3" box="[648,664,1529,1553]" pageId="12" pageNumber="486" value="4.0">4</number>
, B 
<number id="6D1BB84EFFE58262FD02FA44FD1EF9D3" box="[696,710,1528,1553]" pageId="12" pageNumber="486" value="5.0">5</number>
, A 
<number id="6D1BB84EFFE58262FD52FA45FD2FF9D3" box="[744,759,1529,1553]" pageId="12" pageNumber="486" value="4.0">4</number>
, A 
<number id="6D1BB84EFFE58262FCA2FA44FCFEF9D3" box="[792,806,1528,1553]" pageId="12" pageNumber="486" value="6.0">6</number>
) central and 
<number id="6D1BB84EFFE58262FC0DFA44FC1DF9D3" box="[951,965,1528,1553]" pageId="12" pageNumber="486" value="5.0">5</number>
+ 
<number id="6D1BB84EFFE58262FC6FFA44FC3BF9D3" box="[981,995,1528,1553]" pageId="12" pageNumber="486" value="5.0">5</number>
(F 
<number id="6D1BB84EFFE58262FBBDFA45FBCAF9D3" box="[1031,1042,1529,1553]" pageId="12" pageNumber="486" value="1.0">1</number>
, E 
<number id="6D1BB84EFFE58262FB8AFA45FB84F9D3" box="[1072,1116,1529,1553]" pageId="12" pageNumber="486" value="3.0" valueMax="4.0" valueMin="2.0">2–4</number>
