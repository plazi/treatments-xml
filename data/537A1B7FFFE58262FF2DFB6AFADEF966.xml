<date id="AF6D8CA9FFE58262FAADFA12FAEBFA08" box="[1303,1331,1454,1482]" pageId="12" pageNumber="486">
X 
<number id="6D1BB84EFFE58262FA91FA00FAEBFA08" box="[1323,1331,1468,1482]" pageId="12" pageNumber="486" value="2.0">2</number>
</date>
