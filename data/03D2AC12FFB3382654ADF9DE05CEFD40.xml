<taxonomicName id="4C7B6687FFB1382654ADFF6905BDFF3C" authority="Gisin &amp; Gama (1972)" authorityName="Gisin &amp; Gama" authorityYear="1972" box="[151,653,192,218]" class="Entognatha" family="Entomobryidae" genus="Pseudosinella" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="sexoculata">
<emphasis id="B90FC116FFB1382654ADFF69064CFF3F" box="[151,380,192,217]" italics="true" pageId="6" pageNumber="65">P. sexoculata sensu</emphasis>
<bibRefCitation id="EFEA60F5FFB1382655BFFF6905BDFF3C" author="Gisin" box="[389,653,192,218]" pageId="6" pageNumber="68" refString="Gisin, H. &amp; Gama, da M. M. (1972) Pseudosinella cavernicoles d'Espagne (Insecta: Collembola). Revue suisse de Zoologie, 79, 261 - 278." type="journal article" year="1972">
Gisin &amp; Gama (
<number id="3DB30F23FFB13826567FFF6905B4FF3C" box="[581,644,192,218]" pageId="6" pageNumber="65" value="1972.0">1972</number>
)
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C7B6687FFB1382656A1FF69035CFF3C" authority="Wang et al. (2004)" authorityName="Wang et al." authorityYear="2004" box="[667,1132,192,218]" class="Entognatha" family="Entomobryidae" genus="Pseudosinella" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="sexoculata">
<emphasis id="B90FC116FFB1382656A1FF6904B1FF3F" box="[667,897,192,217]" italics="true" pageId="6" pageNumber="65">P. sexoculata sensu</emphasis>
<bibRefCitation id="EFEA60F5FFB1382657B0FF69035CFF3C" box="[906,1132,192,218]" pageId="6" pageNumber="68" refString="Wang, F., Chen, J. X. &amp; Christiansen, K. (2004) A survey of the genus Pseudosinella (Collembola: Entomobryidae) from East Asia. Annales of the Entomological society of America, 97, 364 - 385." type="journal article">
Wang 
<emphasis id="B90FC116FFB1382657ECFF690323FF3F" box="[982,1043,192,217]" italics="true" pageId="6" pageNumber="65">et al.</emphasis>
(
<number id="3DB30F23FFB13826501EFF690354FF3C" box="[1060,1124,192,218]" pageId="6" pageNumber="65" value="2004.0">2004</number>
)
</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="4C7B6687FFB138265097FF690784FEE4" authority="Jordana et al." authorityName="Jordana et al." class="Entognatha" family="Entomobryidae" genus="Pseudosinella" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="simoni">
<emphasis id="B90FC116FFB138265097FF69022AFF3F" box="[1197,1306,192,217]" italics="true" pageId="6" pageNumber="65">P. simoni</emphasis>
