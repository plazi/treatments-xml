<subSubSection id="8FC8D304C3ADF98420E7BBD4B4FDEAC0" type="nomenclature">
<paragraph id="646EFAF0F68920FDDAD665281F7D3DC6" pageNumber="561">
<taxonomicName id="9FB196F186233E8E869711CE8CB60C4A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:236918" class="Insecta" family="Formicidae" genus="Axinidris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Axinidris icipe" order="Hymenoptera" pageNumber="561" phylum="Arthropoda" rank="species" species="icipe">Axinidris icipe</taxonomicName>
