<taxonomicName id="C8D33C8C269B25E430571CE01FC02A52" authorityName="Linnaeus" authorityYear="1753" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" phylum="Tracheophyta" rank="species" species="hirta">Carex hirta</taxonomicName>
