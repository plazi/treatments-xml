<materialsCitation id="7FFE3CA0EB73FFFEFE422AF8FD04FB3C" ID-GBIF-Occurrence="2147525165" box="[460,648,1230,1255]" collectingDate="2011-09-30" country="China" location="Jinghong City" pageId="5" pageNumber="84" specimenCount="1" stateProvince="Yunnan">
<collectingCountry id="B781766DEB73FFFEFE422AF8FD9BFB3C" box="[460,535,1230,1255]" name="China" pageId="5" pageNumber="84">China</collectingCountry>
, 
<collectingRegion id="0D52F81FEB73FFFEFDAD2AF8FD0FFB3C" box="[547,643,1230,1255]" country="China" name="Yunnan" pageId="5" pageNumber="84">Yunnan</collectingRegion>
,
</materialsCitation>
</emphasis>
<location id="CA496026EB73FFFEFD002AF8FCA1FB3C" LSID="urn:lsid:plazi:treatment:473F87EBEB73FFFEFF192DBAFE26FAD0:CA496026EB73FFFEFD002AF8FCA1FB3C" box="[654,813,1230,1255]" country="China" name="Jinghong City" pageId="5" pageNumber="84" stateProvince="Yunnan">Jinghong City</location>
, 1 macropterous female, 2 apterous males from grasses, 
<date id="BB28103DEB73FFFEFF192AC4FE82FAD0" box="[151,270,1266,1291]" pageId="5" pageNumber="84" value="2011-09-30">
