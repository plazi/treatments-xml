</taxonomicName>
and 
<taxonomicName id="F7FB69A0FFAEFFF4FD31FB61FC3A657D" box="[759,971,1276,1301]" class="Mammalia" family="Muridae" genus="Niviventer" kingdom="Animalia" order="Rodentia" pageId="6" pageNumber="54" phylum="Chordata" rank="subSpecies" species="andersoni" subSpecies="pianmaensis">
<emphasis id="028FCE31FFAEFFF4FD31FB61FC3A657D" box="[759,971,1276,1301]" italics="true" pageId="6" pageNumber="54">N. a. pianmaensis</emphasis>
</taxonomicName>
