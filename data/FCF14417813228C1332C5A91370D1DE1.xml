<taxonomicName id="0E1DB27BF3C3A0C8A2C8DB9518C425FE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138841" class="Insecta" family="Formicidae" genus="Ponera" genus.outerRound="1" kingdom="Animalia" lsidName="Ponera johannae Forel" lsidName-HNS="Ponera johannae Forel" order="Hymenoptera" pageNumber="114" phylum="Arthropoda" rank="species" species="johannae">P. johannae</taxonomicName>
