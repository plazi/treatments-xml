<taxonomicName id="4C7A4D1FB7FFFF4A640C792EDDA7FBAD" authorityName="Latreille" authorityYear="1802" box="[344,471,1052,1078]" class="Insecta" family="Carabidae" genus="Bembidion" kingdom="Animalia" order="Coleoptera" pageId="174" pageNumber="175" phylum="Arthropoda" rank="genus">
