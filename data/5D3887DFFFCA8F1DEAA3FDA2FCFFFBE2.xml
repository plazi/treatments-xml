<paragraph id="D52E36C9FFCA8F1AEB03FD2DFEB0FD46" blockId="67.[125,366,704,728]" box="[125,366,704,728]" pageId="67" pageNumber="2164">
<emphasis id="E7E5EADBFFCA8F1AEB03FD2DFEB0FD46" box="[125,366,704,728]" italics="true" pageId="67" pageNumber="2164">
Description (
<typeStatus id="0A2A886BFFCA8F1AEA79FD2DFEB8FD46" box="[263,358,704,728]" pageId="67" pageNumber="2164">holotype</typeStatus>
)
</emphasis>
