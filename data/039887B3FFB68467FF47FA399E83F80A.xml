<collectionCode id="ED20AE60FFB68467FC61F8AE9FB0F8BD" box="[945,1003,1896,1919]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14820" name="Yuzhou University" pageId="12" pageNumber="344">YZU</collectionCode>
from 
<emphasis id="B945EAB7FFB68467FBE1F8AF9951F842" box="[1073,1290,1895,1920]" italics="true" pageId="12" pageNumber="344">
<taxonomicName id="4C314D26FFB68467FBE1F8AF995DF842" box="[1073,1286,1895,1920]" class="Magnoliopsida" family="Lauraceae" genus="Litsea" kingdom="Plantae" order="Laurales" pageId="12" pageNumber="344" phylum="Tracheophyta" rank="species" species="monopetala">Litsea monopetala</taxonomicName>
