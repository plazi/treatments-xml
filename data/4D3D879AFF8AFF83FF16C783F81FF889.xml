<subSubSection id="8D8E6507FF8AFF83FF46C1DDF889F971" box="[199,420,1734,1759]" pageId="44" pageNumber="45" type="biology_ecology">
<paragraph id="C52B368CFF8AFF83FF46C1DDF889F971" blockId="44.[151,1437,258,1831]" box="[199,420,1734,1759]" pageId="44" pageNumber="45">
