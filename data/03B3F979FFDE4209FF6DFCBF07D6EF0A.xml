<emphasis id="B96E947DFFDC4209FC87FBC406ABEF43" bold="true" box="[893,958,1052,1077]" pageId="47" pageNumber="48">n. sp.</emphasis>
and 
<taxonomicName id="4C1A33ECFFDC4209FC0DFBC50029EF42" authority="De Man 1902" authorityName="De Man" authorityYear="1902" box="[1015,1340,1052,1077]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="consobrina">
