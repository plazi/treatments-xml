<paragraph id="8BA5486FFFDC4209FF3DFBBC07D6EF0A" blockId="47.[151,1437,151,1149]" box="[199,707,1124,1149]" pageId="47" pageNumber="48">
<emphasis id="B96E947DFFDC4209FF3DFBBC044CEF0B" bold="true" box="[199,345,1124,1149]" pageId="47" pageNumber="48">Distribution</emphasis>
. Western 
<collectingCountry id="F30D08FFFFDC4209FE33FBBC0726EF0B" box="[457,563,1124,1149]" name="Australia" pageId="47" pageNumber="48">Australia</collectingCountry>
, 
<quantity id="4CE2E58AFFDC4209FDC4FBBC07A9EF0A" box="[574,700,1124,1148]" metricMagnitude="2" metricUnit="m" metricValue="1.005" metricValueMax="1.01" metricValueMin="1.0" pageId="47" pageNumber="48" unit="m" value="100.5" valueMax="101.0" valueMin="100.0">
100– 
