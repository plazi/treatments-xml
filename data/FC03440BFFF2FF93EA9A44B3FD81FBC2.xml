<taxonomicName id="B3AA8E9EFFF2FF92E7524B9BF770F6D2" authorityName="Matschie" authorityYear="1902" box="[2121,2211,2300,2329]" class="Mammalia" family="Viverridae" genus="Genetta" kingdom="Animalia" order="Carnivora" pageId="16" pageNumber="223" phylum="Chordata" rank="species" species="thierryi">thierryi</taxonomicName>
