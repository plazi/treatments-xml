<quantity id="45411FB829750D607999FB746880FB58" box="[1225,1272,1212,1239]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="42" pageNumber="243" unit="m" value="5.0">5 m</quantity>
