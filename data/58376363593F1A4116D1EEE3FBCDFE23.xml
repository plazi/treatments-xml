<number id="6656C052593F1A421262ED4AFBCFFCD2" box="[1060,1096,830,854]" pageId="5" pageNumber="364" value="2.4">2.4</number>
) ± 
<quantity id="17667F90593F1A421237ED4AFB5CFCD1" box="[1137,1243,830,854]" metricMagnitude="-4" metricUnit="m" metricValue="1.2" pageId="5" pageNumber="364" unit="mm" value="0.12">0.12 mm</quantity>
; forewing length 
<number id="6656C052593F1A4216D1ED15FF3DFCFE" box="[151,186,865,890]" pageId="5" pageNumber="364" value="5.6">5.6</number>
(
<number id="6656C052593F1A42168FED15FE98FCFE" box="[201,287,865,890]" pageId="5" pageNumber="364" value="5.449999999999999" valueMax="6.1" valueMin="4.8">4.8–6.1</number>
