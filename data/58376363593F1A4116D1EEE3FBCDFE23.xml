<number id="6656C052593F1A421420ED49FD50FCD2" box="[614,727,829,854]" pageId="5" pageNumber="364" value="16.6" valueMax="17.5" valueMin="15.7">15.7–17.5</number>
) ± 
<quantity id="17667F90593F1A421547ED49FCEFFCD1" box="[769,872,829,854]" metricMagnitude="-4" metricUnit="m" metricValue="6.4" pageId="5" pageNumber="364" unit="mm" value="0.64">0.64 mm</quantity>
, width 
<number id="6656C052593F1A4215FBED4AFC67FCD2" box="[957,992,830,854]" pageId="5" pageNumber="364" value="2.2">2.2</number>
