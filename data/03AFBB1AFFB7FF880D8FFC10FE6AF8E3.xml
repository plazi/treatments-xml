<treatment id="03AFBB1AFFB7FF880D8FFC10FE6AF8E3" ID-DOI="http://doi.org/10.5281/zenodo.6071955" ID-GBIF-Taxon="124997061" ID-Zenodo-Dep="6071955" LSID="urn:lsid:plazi:treatment:03AFBB1AFFB7FF880D8FFC10FE6AF8E3" httpUri="http://treatment.plazi.org/id/03AFBB1AFFB7FF880D8FFC10FE6AF8E3" lastPageNumber="371" pageId="16" pageNumber="371">
