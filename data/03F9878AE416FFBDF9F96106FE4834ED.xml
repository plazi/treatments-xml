<emphasis id="B924EA8EE416FFBCF88463CBFD48322B" box="[490,734,1779,1801]" italics="true" pageId="16" pageNumber="17">
<taxonomicName id="4C504D1FE416FFBCF88463CBFD4C322B" box="[490,730,1779,1801]" class="Insecta" family="Noctuidae" genus="Hemeroplanis" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="habitalis">Hemeroplanis habitalis</taxonomicName>
