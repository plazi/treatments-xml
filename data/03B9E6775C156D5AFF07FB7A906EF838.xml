(Figs 
<number id="3DD845465C156D5AFF47FB5592D6FB4C" box="[215,252,1193,1218]" pageId="15" pageNumber="16" value="2.5">2.5</number>
, 
<number id="3DD845465C156D5AFE98FB569316FB4C" box="[264,316,1194,1218]" pageId="15" pageNumber="16" value="10.2">10.2</number>
