<emphasis id="B9542C073811FFBFFF3CF8AE2BA1018C" bold="true" box="[136,251,1908,1932]" pageId="86" pageNumber="87">Remarks.</emphasis>
<taxonomicName id="4C208B963811FFBFFEBCF8AE2AFD018B" baseAuthorityName="Ihering" baseAuthorityYear="1898" box="[264,423,1907,1931]" class="Actinopterygii" family="Pimelodidae" genus="Zungaro" kingdom="Animalia" order="Siluriformes" pageId="86" pageNumber="87" phylum="Chordata" rank="species" species="jahu">
