<taxonomicName id="16AECEBCC377BA37FD24FE8B8537FE81" authority="Kerner, 1778" authorityName="Kerner" authorityYear="1778" box="[683,1067,309,335]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="bromeliae">
