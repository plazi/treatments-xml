<taxonomicName id="16AECEBCC377BA37FACAFEE6806AFE55" authority="Bouche." authorityName="Bouche." class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="bromeliae">
