4e RACE: 
<taxonomicName id="33D76E067B19D71F30E41EADFEBDB8F8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:239307" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus maculatus subsp. BOIVINI" order="Hymenoptera" pageNumber="34" phylum="Arthropoda" rank="subGenus" species="maculatus" status="n. st." subSpecies="BOIVINI">CAMPONOTUS BOIVINI</taxonomicName>
