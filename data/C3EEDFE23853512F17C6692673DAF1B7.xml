<paragraph id="9A2E63637CE4766E0E9C1167B16C6B8C" pageNumber="773">
Etymology L 
<taxonomicName id="15FA501C1CB8B71E381905A8D9E19E17" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34077" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole ursus Mayr" lsidName-HNS="Pheidole ursus Mayr" order="Hymenoptera" pageNumber="773" phylum="Arthropoda" rank="species" species="ursus">ursus</taxonomicName>
