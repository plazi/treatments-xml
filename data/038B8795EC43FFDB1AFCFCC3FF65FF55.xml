<number id="3DEA24A4EC43FFD91E60F9E6FBA6F9C3" box="[1035,1079,1586,1612]" pageId="8" pageNumber="59" value="351.0">351</number>
(
<number id="3DEA24A4EC43FFD91E23F9E6FB20F9C3" box="[1096,1201,1586,1612]" pageId="8" pageNumber="59" value="332.5" valueMax="345.0" valueMin="320.0">320–345</number>
