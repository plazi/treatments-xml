<emphasis id="09C665072422FF99FF4FC56FAB30F8CE" bold="true" box="[151,339,1854,1879]" pageId="4" pageNumber="432">FIGURES 13–15.</emphasis>
<taxonomicName id="FCB2C2962422FF99FE80C56EA865F8CF" authorityName="Erichson" authorityYear="1847" box="[344,518,1855,1878]" class="Insecta" family="Dynastidae" genus="Tomarus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="432" phylum="Arthropoda" rank="species" species="maimon">
<emphasis id="09C665072422FF99FE80C56EA865F8CF" box="[344,518,1855,1878]" italics="true" pageId="4" pageNumber="432">Tomarus maimon</emphasis>
</taxonomicName>
