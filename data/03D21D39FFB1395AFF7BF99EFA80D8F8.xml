<taxonomicName id="4C7BD7ACFFB1395AFC03F90EFB85D8D4" authorityName="Gray" authorityYear="1847" box="[1007,1141,1725,1751]" class="Polyplacophora" family="Leptochitonidae" genus="Leptochiton" kingdom="Animalia" order="Lepidopleurida" pageId="3" pageNumber="404" phylum="Mollusca" rank="genus">
