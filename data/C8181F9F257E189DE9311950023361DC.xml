<taxonomicName id="EB6802FB7280BD1AC6A8A8B9E6FC4FFC" authority="Greeff, 1879" authorityName="Greeff" authorityYear="1879" class="Polychaeta" family="Iospilidae" genus="Phalacrophorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phalacrophorus pictus" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="pictus">Phalacrophorus pictus Greeff, 1879</taxonomicName>
