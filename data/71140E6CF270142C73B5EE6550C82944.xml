<quantity id="3E45129FF270142F7209EDC555792F9D" box="[299,348,1921,1947]" metricMagnitude="-4" metricUnit="m" metricValue="2.6" pageId="25" pageNumber="26" unit="mm" value="0.26">0.26</quantity>
