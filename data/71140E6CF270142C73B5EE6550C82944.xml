<subSubSection id="B1A7ECF1F273142C73E5EB6D50C82944" box="[199,1261,296,323]" pageId="26" pageNumber="27" type="etymology">
<paragraph id="F902BF7AF273142C73E5EB6D50C82944" blockId="26.[151,1436,152,323]" box="[199,1261,296,323]" pageId="26" pageNumber="27">
<emphasis id="CBC96368F273142C73E5EB6D556B2945" bold="true" box="[199,334,297,323]" pageId="26" pageNumber="27">Etymology.</emphasis>
