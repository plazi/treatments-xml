<taxonomicName id="E6E04D75223DFF808F06FCC47FA6FC48" box="[350,757,884,909]" class="Insecta" family="Asilidae" genus="Stichopogon" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="53" phylum="Arthropoda" rank="subSpecies" species="elegantulus" subSpecies="orientalis">
