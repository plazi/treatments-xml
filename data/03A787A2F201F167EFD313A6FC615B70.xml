<location id="8ED1606FF201F167EB4C178CFBEC5C86" LSID="urn:lsid:plazi:treatment:03A787A2F201F167EFD313A6FC615B70:8ED1606FF201F167EB4C178CFBEC5C86" box="[1046,1133,480,501]" county="Type'" latitude="-16.46" longLatPrecision="769" longitude="145.37" municipality="Mathews" name="Barron" pageId="53" pageNumber="53" stateProvince="Rivers">Barron</location>
