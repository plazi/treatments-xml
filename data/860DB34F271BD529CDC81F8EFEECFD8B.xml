<taxonomicName id="C9A479DA271CD52BCDFD1A3BFDEBF99F" baseAuthorityName="Holt and W. Tattersall" baseAuthorityYear="1905" box="[501,584,1551,1570]" class="Malacostraca" family="Mysidae" genus="Meterythrops" kingdom="Animalia" order="Mysida" pageId="29" pageNumber="1668" phylum="Arthropoda" rank="species" species="pictus">
