<taxonomicName id="4C2D018EA449C96793742FD2FEC2FF01" authorityName="Bezzi" authorityYear="1909" class="Insecta" family="Hybotidae" genus="Syneches" kingdom="Animalia" order="Diptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="repletus">
