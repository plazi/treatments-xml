<paragraph blockId="10.[151,385,1519,1546]" box="[151,385,1519,1546]" pageId="10" pageNumber="167">
<emphasis box="[151,270,1520,1545]" italics="true" pageId="10" pageNumber="167">Diagnosis</emphasis>
<emphasis bold="true" box="[277,287,1519,1545]" pageId="10" pageNumber="167">(</emphasis>
<emphasis box="[287,375,1520,1546]" italics="true" pageId="10" pageNumber="167">Female</emphasis>
