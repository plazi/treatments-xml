(Costa) (
<taxonomicName authority="" authorityName="" class="Insecta" family="Coccinellidae" genus="Coccinelloidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Coccinelloidea" order="Hemiptera" pageId="3" pageNumber="40" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
