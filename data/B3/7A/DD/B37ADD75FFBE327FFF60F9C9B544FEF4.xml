, by monotypy: 
<taxonomicName authorityName="Linnaeus" authorityYear="1766" box="[373,550,1664,1687]" class="Aves" family="Emberizidae" genus="Tanagra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="0" pageNumber="593" phylum="Chordata" rank="species" species="cristata">
<emphasis box="[373,550,1664,1687]" italics="true" pageId="0" pageNumber="593">Tanagra cristata</emphasis>
</taxonomicName>
“GL” = 
<taxonomicName authority="Gmelin, 1789" authorityName="Gmelin" authorityYear="1789" box="[645,975,1663,1687]" class="Aves" family="Emberizidae" genus="Tanagra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="0" pageNumber="593" phylum="Chordata" rank="species" species="cristata">
<emphasis box="[645,822,1664,1687]" italics="true" pageId="0" pageNumber="593">Tanagra cristata</emphasis>
<bibRefCitation author="Gmelin, J. F." box="[829,975,1663,1687]" pageId="0" pageNumber="593" pagination="501 - 1032" refId="ref1105" refString="Gmelin, J. F. (1789) Systema Naturae per Regna tria Naturae Vol. 1. 13 th Edition. Part 2. Impensis Georg. Emanuel. Beer, Lipsiae [Leipzig], 532 pp. [pp. 501 - 1032] https: // doi. org / 10.5962 / bhl. title. 36932" type="book chapter" year="1789">Gmelin, 1789</bibRefCitation>
</taxonomicName>
= 
<taxonomicName authority="Linnaeus, 1766" authorityName="Linnaeus" authorityYear="1766" box="[1004,1352,1663,1687]" class="Aves" family="Emberizidae" genus="Tanagra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="0" pageNumber="593" phylum="Chordata" rank="species" species="cristata">
<emphasis box="[1004,1181,1664,1687]" italics="true" pageId="0" pageNumber="593">Tanagra cristata</emphasis>
