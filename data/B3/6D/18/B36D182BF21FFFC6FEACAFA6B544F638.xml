<taxonomicName authorityName="Emmons" authorityYear="1988" box="[220,325,1590,1613]" class="Mammalia" family="Echimyidae" genus="Olallamys" kingdom="Animalia" order="Rodentia" pageId="0" pageNumber="18" phylum="Chordata" rank="genus">
