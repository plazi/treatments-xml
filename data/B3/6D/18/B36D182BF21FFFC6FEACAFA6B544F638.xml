<taxonomicName baseAuthorityName="Gunther" baseAuthorityYear="1879" box="[523,666,1590,1613]" class="Mammalia" family="Echimyidae" genus="Olallamys" kingdom="Animalia" order="Rodentia" pageId="0" pageNumber="18" phylum="Chordata" rank="species" species="albicaudus">
