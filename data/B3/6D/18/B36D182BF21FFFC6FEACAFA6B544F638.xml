<taxonomicName baseAuthorityName="Thomas" baseAuthorityYear="1916" box="[713,792,1590,1613]" class="Mammalia" family="Echimyidae" genus="Olallamys" kingdom="Animalia" order="Rodentia" pageId="0" pageNumber="18" phylum="Chordata" rank="species" species="edax">
