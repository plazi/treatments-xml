<taxonomicName authority="Linnaeus, 1758" authorityName="Graham" authorityYear="1957" class="Insecta" family="Malvaceae" genus="Hymenoptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="genus">Hymenoptera</taxonomicName>
(
<taxonomicName authorityName="Latreille" authorityYear="1802" class="Insecta" family="Sphecidae" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="family">Sphecidae</taxonomicName>
: 
<taxonomicName authorityName="Shuckard" authorityYear="1838" class="Magnoliopsida" family="Fagaceae" genus="Spilomena" kingdom="Plantae" lsidName="Spilomena" order="Fagales" pageId="0" pageNumber="77092" phylum="Tracheophyta">
<emphasis italics="true" pageId="0" pageNumber="77092">Spilomena</emphasis>
</taxonomicName>
sp.) in dead wood of 
<taxonomicName authorityName="Shuckard" authorityYear="1838" class="Magnoliopsida" family="Fagaceae" genus="Fagaceae" kingdom="Plantae" lsidName="" order="Fagales" pageId="0" pageNumber="77092" phylum="Tracheophyta">Fagaceae</taxonomicName>
(
<taxonomicName class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" lsidName="Quercus robur" order="Fagales" pageId="0" pageNumber="77092" phylum="Tracheophyta" species="robur">
