<emphasis bold="true" box="[151,540,151,178]" pageId="6">
<emphasis bold="true" box="[151,345,151,177]" italics="true" pageId="6">Alima orientalis</emphasis>
Manning, 
<number box="[481,540,152,178]" pageId="6" value="1978.0">1978</number>
</emphasis>
</taxonomicName>
