; 
<collectionCode box="[151,220,1051,1072]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33726" lsid="urn:lsid:biocol.org:col:33726" name="Louisiana State Arthropod Museum" pageId="6" pageNumber="526">LSAM</collectionCode>
