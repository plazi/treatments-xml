<subSubSection pageId="85" pageNumber="86" type="type specimens">
<paragraph pageId="85" pageNumber="86">Type specimens.</paragraph>
<paragraph pageId="85" pageNumber="86">
<taxonomicName class="Insecta" family="Scarabaeidae" genus="Deltochilum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Deltochilum mexicanum" order="Coleoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="mexicanum">Deltochilum mexicanum</taxonomicName>
