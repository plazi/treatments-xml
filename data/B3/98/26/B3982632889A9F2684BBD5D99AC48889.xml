<taxonomicName class="Insecta" family="Cicadellidae" genus="Parandanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parandanus ornatus" order="Hemiptera" pageId="1" pageNumber="74" phylum="Arthropoda" rank="species" species="ornatus">Parandanus ornatus</taxonomicName>
