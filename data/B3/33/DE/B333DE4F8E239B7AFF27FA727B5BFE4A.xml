<emphasis bold="true" box="[197,347,338,364]" pageId="28" pageNumber="29">Etymology.</emphasis>
<taxonomicName box="[356,442,338,363]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="hailan" status="new species">
<emphasis box="[356,442,338,363]" italics="true" pageId="28" pageNumber="29">Hailan</emphasis>
</taxonomicName>
, from Vietnamese “ 
<taxonomicName box="[695,736,338,364]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="hai">hai</taxonomicName>
