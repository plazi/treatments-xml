<taxonomicName box="[733,899,798,822]" class="Eutardigrada" family="Hypsibiidae" kingdom="Animalia" order="Parachela" pageId="9" pageNumber="60" phylum="Tardigrada" rank="subFamily" subFamily="Diphasconinae">Diphasconinae</taxonomicName>
and 
<taxonomicName box="[954,1126,798,822]" class="Eutardigrada" family="Hypsibiidae" kingdom="Animalia" order="Parachela" pageId="9" pageNumber="60" phylum="Tardigrada" rank="subFamily" subFamily="Itaquasconinae">Itaquasconinae</taxonomicName>
