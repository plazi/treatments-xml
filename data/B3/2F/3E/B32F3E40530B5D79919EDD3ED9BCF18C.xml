<taxonomicName authority="J.E.Smith, 1798" authorityName="J.E.Smith" authorityYear="1798" class="Magnoliopsida" family="Goodeniaceae" genus="Velleia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Velleia pubescens" order="Asterales" pageId="0" pageNumber="27" phylum="Tracheophyta" rank="species" species="pubescens">Velleia pubescens</taxonomicName>
