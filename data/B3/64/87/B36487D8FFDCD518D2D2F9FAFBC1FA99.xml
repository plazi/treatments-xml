, paratype ♂: 
<emphasis bold="true" box="[580,593,1286,1303]" pageId="30" pageNumber="447">A</emphasis>
, antenna; 
<emphasis bold="true" box="[682,695,1286,1303]" pageId="30" pageNumber="447">B</emphasis>
, leg 1, frontal view; 
<emphasis bold="true" box="[865,879,1286,1303]" pageId="30" pageNumber="447">C</emphasis>
, leg 2, caudal view; 
<emphasis bold="true" box="[1053,1067,1286,1303]" pageId="30" pageNumber="447">D</emphasis>
