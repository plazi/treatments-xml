FIG. 10. — 
<taxonomicName authority="Golovatch &amp; Geoffroy &amp; Mauriès &amp; Spiegel, 2007" authorityName="Golovatch &amp; Geoffroy &amp; Mauriès &amp; Spiegel" authorityYear="2007" box="[203,408,1077,1094]" class="Diplopoda" family="Cambalopsidae" genus="Glyphiulus" kingdom="Animalia" order="Spirostreptida" pageId="15" pageNumber="432" phylum="Arthropoda" rank="species" species="obliteratoides" status="n. sp.">
<emphasis box="[203,408,1077,1094]" italics="true" pageId="15" pageNumber="432">Glyphiulus obliteratoides</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[413,463,1077,1094]" pageId="15" pageNumber="432" rank="species">n. sp.</taxonomicNameLabel>
, paratype ♀: 
<emphasis bold="true" box="[571,584,1077,1094]" pageId="15" pageNumber="432">A</emphasis>
, telson, ventral view; 
<emphasis bold="true" box="[763,776,1077,1094]" pageId="15" pageNumber="432">B</emphasis>
, midbody segment section; 
<emphasis bold="true" box="[1013,1027,1077,1094]" pageId="15" pageNumber="432">C</emphasis>
