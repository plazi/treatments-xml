<taxonomicName id="E3A7FE88FFFF830FFDEF5EB4FD6CF917" authorityName="Hedge &amp; J. Leonard" authorityYear="1986" baseAuthorityName="Desf." box="[555,764,1748,1772]" class="Magnoliopsida" family="Brassicaceae" genus="Neotorularia" kingdom="Plantae" order="Brassicales" pageId="66" pageNumber="66" phylum="Tracheophyta" rank="species" species="torulosa">
