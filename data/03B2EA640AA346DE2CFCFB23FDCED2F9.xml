species: 
<taxonomicName id="4C1B20F10AA346DE2D4DFB7FFD40D24E" authority="Cockerell" authorityName="Cockerell" box="[294,651,1264,1289]" class="Insecta" family="Phoenicococcidae" genus="Phoenicococcus" kingdom="Animalia" order="Hemiptera" pageId="140" pageNumber="141" phylum="Arthropoda" rank="species" species="marlatti">
