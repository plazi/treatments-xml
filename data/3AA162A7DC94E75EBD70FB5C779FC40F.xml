<taxonomicName id="34086D647D4531768BF9E8FA7C1469A5" class="Magnoliopsida" family="Compositae" genus="Dorotheanthus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Asterales" pageId="24" pageNumber="25" phylum="Magnoliophyta" rank="genus">Dorotheanthus</taxonomicName>
, 
<taxonomicName id="F68265DF2D1733A3F55E63E3A95957D2" class="Magnoliopsida" family="Aizoaceae" genus="Leipoldtia" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Caryophyllales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="genus">Leipoldtia</taxonomicName>
