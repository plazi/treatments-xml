<taxonomicName id="A8CBD63FE687ED22776B28DDA5A45663" genus="Aizoaceae" lsidName="" pageId="24" pageNumber="25" rank="genus">Aizoaceae</taxonomicName>
: 
<taxonomicName id="2669529EE2E5164165BC55B041DB3DE3" genus="Mesembryanthema" lsidName="" pageId="24" pageNumber="25" rank="genus">Mesembryanthema</taxonomicName>
