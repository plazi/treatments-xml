<subSubSection id="98BE653E145E636B6493FAF7FC674640" box="[146,898,1362,1384]" pageId="1" pageNumber="560" type="materials_examined">
<paragraph id="D01B36B5145E636B6493FAF7FC674640" blockId="1.[146,898,1362,1384]" box="[146,898,1362,1384]" pageId="1" pageNumber="560">
<typeStatus id="0F1F8817145E636B6493FAF7FF234640" box="[146,198,1362,1384]" pageId="1" pageNumber="560">Type</typeStatus>
