and are probably now in 
<collectionCode id="ED1FAE71F210F176EEFB1397FE7C5B63" box="[417,509,1531,1552]" country="France" httpUri="http://grbio.org/cool/a5xg-x4h4" name="Museum d'Histoire Naturelle Perpignan" pageId="36" pageNumber="36">MHNP</collectionCode>
</materialsCitation>
; 
<materialsCitation id="3B663CE9F210F176ED541397FE0A5BAC" collectingDate="1936-07" collectionCode="BMNH" location="Lomlom" municipality="Disappointment" pageId="36" pageNumber="36" specimenCode="AMNH 218020, AMNH 214346, AMNH 214368, 214370" specimenCount="1" typeStatus="paratype">
<specimenCode id="DBA89ECFF210F176ED541397FF2E5B5E" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="36" pageNumber="36">AMNH 218020</specimenCode>
