), 
<specimenCode id="DBA89ECFF210F176EFAA10C0FE395BB2" box="[240,440,1707,1729]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="36" pageNumber="36">AMNH 214368</specimenCode>
, 
