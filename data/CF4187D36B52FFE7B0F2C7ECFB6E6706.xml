<collectingCountry id="3FFF76556B52FFE7B50DC6AAFABA65C2" box="[1384,1432,465,491]" name="Iran" pageId="7" pageNumber="110">Iran</collectingCountry>
, 
<collectingCountry id="3FFF76556B52FFE7B0F2C68FFFDB6627" box="[151,249,500,526]" name="Pakistan" pageId="7" pageNumber="110">Pakistan</collectingCountry>
, 
<collectingCountry id="3FFF76556B52FFE7B161C68FFE576627" box="[260,373,500,526]" name="Sri Lanka" pageId="7" pageNumber="110">Sri Lanka</collectingCountry>
