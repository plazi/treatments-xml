<emphasis id="B9426259FF92FFAE7DB4C73BFEEA2D5D" bold="true" box="[197,372,1891,1915]" pageId="8" pageNumber="9">Measurements</emphasis>
(n = 
<number id="3DFEAC6CFF92FFAE7CC1C73AFE5F2D5C" box="[432,449,1890,1914]" pageId="8" pageNumber="9" value="4.0">4</number>
): HB = 
<number id="3DFEAC6CFF92FFAE7F6AC73AFD1E2D5C" box="[539,640,1890,1914]" pageId="8" pageNumber="9" value="200.0" valueMax="215.0" valueMin="185.0">185–215</number>
, T = 
<number id="3DFEAC6CFF92FFAE7FC8C73AFC802D5C" box="[697,798,1890,1914]" pageId="8" pageNumber="9" value="256.0" valueMax="279.0" valueMin="233.0">233–279</number>
, HF = 
<number id="3DFEAC6CFF92FFAE7E1AC73AFC2A2D5C" box="[875,948,1890,1914]" pageId="8" pageNumber="9" value="33.5" valueMax="35.0" valueMin="32.0">32–35</number>
, E = 
<number id="3DFEAC6CFF92FFAE7E9FC73AFBA92D5C" box="[1006,1079,1890,1914]" pageId="8" pageNumber="9" value="34.0" valueMax="36.0" valueMin="32.0">32–36</number>
, W = 
