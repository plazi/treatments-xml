(n = 
<number id="3DFEAC6CFF90FFAC7C01C114FE1E2B42" box="[368,384,332,356]" pageId="10" pageNumber="11" value="1.0">1</number>
: skull): 
<collectionCode id="ED27268EFF90FFAC7CAAC114FDBE2B42" box="[475,544,332,356]" httpUri="http://grbio.org/cool/n6nq-8nqh" name="Universidade Federal do Espirito Santo" pageId="10" pageNumber="11">UFES</collectionCode>
<number id="3DFEAC6CFF90FFAC7F57C114FDFC2B42" box="[550,610,332,356]" pageId="10" pageNumber="11" value="1414.0">1414</number>
