<taxonomicName id="5CA34D11FFFEFFAFB895FE77CB2F315B" box="[1233,1342,500,525]" class="Malacostraca" family="Aphanodactylidae" genus="Gustavus" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="35" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="A9D7EA80FFFEFFAFB895FE77CB2F315B" box="[1233,1342,500,525]" italics="true" pageId="2" pageNumber="35">Gustavus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="B2E457FBFFFEFFAFB902FE77CB863158" box="[1350,1431,500,526]" pageId="2" pageNumber="35" rank="genus">n. gen.</taxonomicNameLabel>
, and 
<taxonomicName id="5CA34D11FFFEFFAFBC8CFD99CC4E3162" authority="Naruse, Fujita &amp; Ng, 2009" authorityName="Naruse, Fujita &amp; Ng" authorityYear="2009" box="[200,607,538,564]" class="Malacostraca" family="Pinnotheridae" genus="Uruma" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="35" phylum="Arthropoda" rank="genus">
