Tesch, 
<number id="2D6B24B5FFFEFFAFBEF2FE77CCE03158" box="[694,753,500,526]" pageId="2" pageNumber="35" value="1918.0">1918</number>
</bibRefCitation>
</taxonomicName>
[
<typeStatus id="44188830FFFEFFAFBEBAFE77CD223158" box="[766,819,500,526]" pageId="2" pageNumber="35">type</typeStatus>
genus], 
<taxonomicName id="5CA34D11FFFEFFAFBFDCFE77CAD43158" authority="Kammerer, 2006" authorityName="Kammerer" authorityYear="2006" box="[920,1221,500,526]" class="Malacostraca" family="Aphanodactylidae" genus="Gandoa" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="35" phylum="Arthropoda" rank="genus">
<emphasis id="A9D7EA80FFFEFFAFBFDCFE77CDE7315B" box="[920,1014,500,525]" italics="true" pageId="2" pageNumber="35">Gandoa</emphasis>
