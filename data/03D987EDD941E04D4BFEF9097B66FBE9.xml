<collectingCountry id="F367766BD943E04D4A0EFC60798DFC31" box="[359,413,979,1005]" name="Laos" pageId="3" pageNumber="308">Laos</collectingCountry>
(
<collectingRegion id="49B4F819D943E04D4AC4FC607A51FC32" box="[429,577,979,1006]" country="Laos" name="Bolikhamxai" pageId="3" pageNumber="308">Bolikhamsai</collectingRegion>
