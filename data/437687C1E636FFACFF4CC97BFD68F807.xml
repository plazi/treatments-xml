<taxonomicName id="0CDF4D54E637FFACFEB9CAC1FD6CF807" authority="(Fain 1967)" baseAuthorityName="Fain" baseAuthorityYear="1967" box="[354,664,1989,2015]" class="Arachnida" family="Gastronyssidae" genus="Rodhainyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
