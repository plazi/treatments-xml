, 
<collectingCountry id="E99BF29DFF923E20A0E0B354111EF913" box="[460,629,1642,1667]" name="French Guiana" pageId="13" pageNumber="164">French Guiana</collectingCountry>
, 
<collectingCountry id="E99BF29DFF923E20A3ACB35411A2F913" box="[640,713,1642,1667]" name="Brazil" pageId="13" pageNumber="164">Brazil</collectingCountry>
