. 
<taxonomicName id="D9A64D3FFFA5FFC98845FF1DE5FB839A" authorityName="Stal" authorityYear="1866" box="[362,664,223,248]" class="Insecta" family="Reduviidae" genus="Pyrrhosphodrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="397" phylum="Arthropoda" rank="species" species="amazonus">
