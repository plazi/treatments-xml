<emphasis id="25B9E51AFFF11016FEDDF8EEFDA6F132" box="[366,604,1855,1878]" italics="true" pageId="4" pageNumber="47">Dalpada smaragdina</emphasis>
(Walker)
</taxonomicName>
, 
<taxonomicName id="D0CD428BFFF11016FD66F8EEFCD0F132" box="[725,810,1855,1878]" class="Agaricomycetes" family="Phallaceae" genus="Phallus" kingdom="Fungi" order="Phallales" pageId="4" pageNumber="51" phylum="Basidiomycota" rank="genus">phallus</taxonomicName>
. 
<emphasis id="25B9E51AFFF11016FC85F8EFFCA8F132" bold="true" box="[822,850,1854,1878]" pageId="4" pageNumber="47">16</emphasis>
, dorsal view; 
<emphasis id="25B9E51AFFF11016FC40F8EFFBF5F132" bold="true" box="[1011,1039,1854,1878]" pageId="4" pageNumber="47">17</emphasis>
, lateral view; 
<emphasis id="25B9E51AFFF11016FB00F8EFFB35F132" bold="true" box="[1203,1231,1854,1878]" pageId="4" pageNumber="47">18</emphasis>
