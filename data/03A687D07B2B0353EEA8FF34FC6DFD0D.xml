<taxonomicName id="4C0F4D457B2C0352EF1AF82BFDB1F860" authority="(Green)" authorityName="Green" baseAuthorityName="Green" baseAuthorityYear="1904" box="[293,631,1927,1949]" class="Insecta" family="Coccidae" genus="Paralecanium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="calophylli">
