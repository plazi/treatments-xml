<heading id="205A81E1FFC4FF93FF35FC1A28B2FBCB" bold="true" box="[151,689,1003,1030]" fontSize="11" level="1" pageId="10" pageNumber="461" reason="1">
<taxonomicName id="BCAD4D0EFFC4FF93FF35FC1A28B2FBCB" authority="Stimpson, 1859" authorityName="Stimpson" authorityYear="1859" box="[151,689,1003,1030]" class="Malacostraca" family="Cryptochiridae" genus="Hapalocarcinus" kingdom="Animalia" order="Decapoda" pageId="10" pageNumber="461" phylum="Arthropoda" rank="species" species="marsupialis">
