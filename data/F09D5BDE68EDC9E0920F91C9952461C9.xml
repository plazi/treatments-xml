<taxonomicName id="7CD744563FBBB19D3266F59659BCC2B7" class="Insecta" family="Carabidae" genus="Pterostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pterostichus adelphus" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="adelphus">Pterostichus adelphus</taxonomicName>
