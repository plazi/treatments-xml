<taxonomicName id="FFF34D4AA928FF8407CEF95BCA77F91D" authority="(Chambers)" baseAuthorityName="Chambers" baseAuthorityYear="1871" box="[151,526,1742,1767]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="species" species="celtisella">
