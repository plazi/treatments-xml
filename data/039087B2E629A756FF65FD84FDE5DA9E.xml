<taxonomicName id="4C394D27E629A756FDF6FDDBFD11DA1E" box="[516,644,600,623]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="jujuyense">
