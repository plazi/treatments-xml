<taxonomicName id="4C394D27E629A756FE1FFDFBFD19DAFE" box="[493,652,632,655]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="auripellitum">
