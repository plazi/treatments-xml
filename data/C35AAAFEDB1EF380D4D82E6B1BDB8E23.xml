<emphasis id="D765B17449F5907D0A291832565BB759" italics="true" pageId="14" pageNumber="15">Pampsilota afer</emphasis>
</taxonomicName>
, 
<taxonomicName id="FC682E8BB9A40F4F866167BF1A9A1270" authority="Konow, 1899" authorityName="Konow" authorityYear="1899" baseAuthorityName="Mocsary" baseAuthorityYear="1909" class="Insecta" family="Argidae" genus="Pampsilota" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pampsilota africanus" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="africanus">
