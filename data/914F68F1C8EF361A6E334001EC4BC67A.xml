<taxonomicName id="4D348E1F17125FD34769231887C3AFFA" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia discoidalis" order="Coleoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="discoidalis">Cephaloleia discoidalis</taxonomicName>
, 
<taxonomicName id="97BFEC6477661F85003EB58C7988EFC9" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia dorsalis" order="Coleoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="dorsalis">Cephaloleia dorsalis</taxonomicName>
, 
<taxonomicName id="0E62203F74A2D996301006238AEB9034" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia linkei" order="Coleoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="linkei">Cephaloleia linkei</taxonomicName>
, and 
<taxonomicName id="E9492B518C12B81521CF92CDD1D21E1C" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia suturalis" order="Coleoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="suturalis">Cephaloleia suturalis</taxonomicName>
