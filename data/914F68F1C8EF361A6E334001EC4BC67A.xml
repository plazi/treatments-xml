<taxonomicName id="4219EB95DA881CCFFE32FEC50E270625" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia balyi" order="Coleoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="balyi">Cephaloleia balyi</taxonomicName>
