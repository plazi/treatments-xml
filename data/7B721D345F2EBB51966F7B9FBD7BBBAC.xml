<subSubSection id="71A0F1A8E421CC4716E905330471EC66" pageId="18" pageNumber="157" type="nomenclature">
<paragraph id="401DF1FC0A5A3EFF5EC5A3B5AF5ECEB2" pageId="18" pageNumber="157">
<taxonomicName id="4472216E60A1DFEAE19CBF9F44F1465F" LSID="http://zoobank.org/81DE3680-4B3E-456B-B7E9-D2FA578487CA" authority="Lafontaine, Walsh &amp; Ferris" authorityName="Lafontaine, Walsh &amp; Ferris" authorityYear="2014" class="Insecta" family="Noctuidae" genus="Nudorthodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Nudorthodes" order="Lepidoptera" pageId="18" pageNumber="157" phylum="Arthropoda" rank="genus">Nudorthodes Lafontaine, Walsh &amp; Ferris</taxonomicName>
