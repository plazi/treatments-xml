<emphasis id="B954EAB5DF18FF89C1D7F931FE6C58D9" bold="true" box="[151,470,1701,1727]" pageId="9" pageNumber="10">
Genus 
<taxonomicName id="4C204D24DF18FF89C1AEF931FE6C58D9" authority="Baird, 1850" authorityName="Baird" authorityYear="1850" box="[238,470,1701,1727]" class="Branchiopoda" family="Moinidae" genus="Moina" kingdom="Animalia" order="Diplostraca" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
