<taxonomicName id="B4E3AD0BB114F4B835625B5956BB75AF" class="Insecta" family="Geometridae" genus="Gymnoscelis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnoscelis pseudotibialis" order="Lepidoptera" pageId="0" pageNumber="7938" phylum="Arthropoda" rank="species" species="pseudotibialis">Gymnoscelis pseudotibialis</taxonomicName>
