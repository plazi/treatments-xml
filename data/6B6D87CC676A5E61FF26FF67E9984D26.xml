number added later]”; 
<number id="550C24FD676A5E61FBA0FDF3EF6F4B42" box="[1041,1058,515,539]" pageId="18" pageNumber="219" value="2.0">2</number>
. “ 
<taxonomicName id="24C44D59676A5E61FB88FDF4EB964B66" authority="Det. De Santis" authorityName="Det. De Santis" class="Insecta" family="Encyrtidae" genus="Leptomastix" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="malenotus">
<emphasis id="D1B0EAC8676A5E61FB88FDF4EE704B42" box="[1081,1341,514,539]" italics="true" pageId="18" pageNumber="219">Leptomastix malenotus</emphasis>
Det. 
<collectingCountry id="9BD3764A676A5E61FACAFDF3EED14B43" box="[1403,1436,515,538]" name="South Georgia and the South Sandwich Islands" pageId="18" pageNumber="219">De</collectingCountry>
Santis
</taxonomicName>
HOLOTIPO ♀ 
<collectionCode id="85D5AE1F676A5E61FE11FDD7E94F4B66" box="[416,514,551,575]" pageId="18" pageNumber="219">MUSEO</collectionCode>
<collectingCountry id="9BD3764A676A5E61FDBFFDD7E97E4B67" box="[526,563,551,574]" name="South Georgia and the South Sandwich Islands" pageId="18" pageNumber="219">DE</collectingCountry>
LA 
<collectionCode id="85D5AE1F676A5E61FDC1FDD7E9864B66" box="[624,715,551,575]" pageId="18" pageNumber="219">PLATA</collectionCode>
”. The 
<typeStatus id="3C7F8878676A5E61FCADFDD6E8334B66" box="[796,894,550,575]" pageId="18" pageNumber="219">holotype</typeStatus>
(
<figureCitation id="7BFF2A5F676A5E61FC23FDD6E8A54B66" box="[914,1000,550,575]" captionStart="FIGURES 27 – 29" captionStartId="17.[151,264,1936,1959]" captionTargetBox="[151,1435,267,1914]" captionTargetId="figure@17.[151,1435,267,1914]" captionTargetPageId="17" captionText="FIGURES 27 – 29. Anagyrus malenotus ♀ (holotype): 27, slide; 28, antenna; 29, habitus." httpUri="https://zenodo.org/record/250617/files/figure.png" pageId="18" pageNumber="219">
Fig. 
<number id="550C24FD676A5E61FC7AFDD7E8A54B66" box="[971,1000,551,575]" pageId="18" pageNumber="219" value="29.0">29</number>
