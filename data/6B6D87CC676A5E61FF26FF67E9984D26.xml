-V- 
<number id="550C24FD676A5E61FEC8FDF3EAFF4B42" box="[377,434,515,539]" pageId="18" pageNumber="219" value="1932.0">1932</number>
</date>
<number id="550C24FD676A5E61FE08FDF2EAB84B42" box="[441,501,514,539]" pageId="18" pageNumber="219" value="2682.0">2682</number>
/ 
<number id="550C24FD676A5E61FE4FFDF2E94A4B42" box="[510,519,514,539]" pageId="18" pageNumber="219" value="1.0">1</number>
[in pencil, 
<collectionCode id="85D5AE1F676A5E61FD39FDF3E99E4B42" box="[648,723,515,539]" pageId="18" pageNumber="219">MLPA</collectionCode>
