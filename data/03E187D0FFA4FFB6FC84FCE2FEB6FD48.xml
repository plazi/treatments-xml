<taxonomicName id="4C484D45FFA6FFB7FF62F9D4FE24F9D4" authorityName="ROUGIER &amp; MARTINELLI &amp; FORASIEPI &amp; NOVACEK" authorityYear="2007" box="[241,417,1599,1620]" class="Mammalia" family="Henosferidae" genus="Henosferus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Australosphenida incertae sedis" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="molus">
