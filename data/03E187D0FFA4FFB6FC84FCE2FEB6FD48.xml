<taxonomicName id="4C484D45FFA4FFB5FC84FCE2FC58FCA1" authority="ROUGIER &amp; MARTINELLI &amp; FORASIEPI &amp; NOVACEK, 2007" authorityName="ROUGIER &amp; MARTINELLI &amp; FORASIEPI &amp; NOVACEK" authorityYear="2007" box="[791,989,777,801]" class="Mammalia" family="Henosferidae" genus="Henosferus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Australosphenida incertae sedis" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="molus" status="new species">
<emphasis id="B93CEAD4FFA4FFB5FC84FCE2FC58FCA1" bold="true" box="[791,989,777,801]" italics="true" pageId="4" pageNumber="5">Henosferus molus</emphasis>
