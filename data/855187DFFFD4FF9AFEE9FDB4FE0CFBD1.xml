<number id="BB3024EEFFD7FF9BFC8EF88AFC71F8E1" box="[879,919,1844,1870]" pageId="2" pageNumber="3" value="0.6">0.6</number>
; flagellomere 
<number id="BB3024EEFFD7FF9BFBB3F88AFB87F8E1" box="[1106,1121,1844,1870]" pageId="2" pageNumber="3" value="1.0">1</number>
length (F 
<number id="BB3024EEFFD7FF9BFB08F88AFB15F8E1" box="[1257,1267,1844,1870]" pageId="2" pageNumber="3" value="1.0">1</number>
L) / flagellomere 
