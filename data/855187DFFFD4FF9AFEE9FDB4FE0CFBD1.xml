<taxonomicName id="CAF84D4AFFD4FF98FE5CF9B3FD07F98A" authority="Shaw" authorityName="Shaw" box="[445,737,1549,1573]" class="Insecta" family="Megalyridae" genus="Cryptalyra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="colombia" status="NEW SPECIES">
