<paragraph id="8B9636A2FFFF8920FF02FACE766C9CFC" blockId="85.[151,1400,1372,1424]" box="[151,1400,1402,1424]" pageId="85" pageNumber="86">
1951 
<taxonomicName id="4C294D21FFFF8920FF45FACE71089CFC" authority="(A. Agassiz)" authorityName="A. Agassiz" baseAuthorityName="A. Agassiz" baseAuthorityYear="1863" box="[208,540,1402,1424]" class="Echinoidea" family="Maretiidae" genus="Pseudomaretia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spatangoida" pageId="85" pageNumber="86" phylum="Echinodermata" rank="species" species="alta">
