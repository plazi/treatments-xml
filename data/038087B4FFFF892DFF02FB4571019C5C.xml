<taxonomicName id="4C294D21FFFF8920FF45FAE972D39C1E" authority="A. Agassiz" authorityName="A. Agassiz" box="[208,455,1372,1394]" class="Echinoidea" family="Maretiidae" genus="Maretia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spatangoida" pageId="85" pageNumber="86" phylum="Echinodermata" rank="species" species="alta">
