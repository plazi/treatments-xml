<quantity id="6F359B7DFFBBC6296488FEE7FBF1FEB1" box="[992,1055,359,381]" metricMagnitude="3" metricUnit="m" metricValue="2.1559999999999997" pageId="23" pageNumber="788" unit="m" value="2156.0">2156m</quantity>
