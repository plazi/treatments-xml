. 
<taxonomicName id="D8DBC4B8FFDFD628CA4DBC52067FF48E" authority="Boxshall &amp; O’Reilly &amp; Sikorski &amp; Summerfield, 2019" authorityName="Boxshall &amp; O’Reilly &amp; Sikorski &amp; Summerfield" authorityYear="2019" box="[301,531,1907,1929]" class="Maxillopoda" family="Saccopsidae" genus="Euchonicola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="caudatus" status="gen. et sp. nov.">
<emphasis id="2DAF6329FFDFD628CA4DBC52067FF48E" box="[301,531,1907,1929]" italics="true" pageId="52" pageNumber="53">Euchonicola caudatus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="369CDE52FFDFD628C97CBC5206A9F48E" box="[540,709,1907,1929]" pageId="52" pageNumber="53" rank="species">
