<subSubSection id="C315653F8746FF815897C4A4F836F8BF" pageId="18" pageNumber="19" type="discussion">
<paragraph id="8BB036B48746FF815897C4A4F836F8BF" blockId="18.[151,1437,151,1905]" pageId="18" pageNumber="19">
<emphasis id="B97BEAA68746FF815897C4A4FD50F92E" bold="true" box="[199,335,1734,1760]" pageId="18" pageNumber="19">Comments.</emphasis>
<taxonomicName id="4C0F4D378746FF815906C4A5FE95F92F" authority="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz, 2020" authorityName="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz" authorityYear="2020" box="[342,650,1735,1761]" class="Insecta" family="Psyllidae" genus="Mitrapsylla" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="aeschynomenis" status="sp. nov.">
<emphasis id="B97BEAA68746FF815906C4A5FE95F92F" box="[342,650,1735,1761]" italics="true" pageId="18" pageNumber="19">Mitrapsylla aeschynomenis</emphasis>
</taxonomicName>
<emphasis id="B97BEAA68746FF815AC1C4A5FEF5F92F" bold="true" box="[657,746,1735,1761]" pageId="18" pageNumber="19">
<taxonomicNameLabel id="A24857DD8746FF815AC1C4A5FEF5F92F" box="[657,746,1735,1761]" pageId="18" pageNumber="19" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
resembles 
