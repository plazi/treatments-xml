<taxonomicName id="2585D190E94D18014BFA42683F35C5B8" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="501" phylum="Tracheophyta" rank="species" species="viscosus">
