<taxonomicName id="8BEDEAA46148E290FF59F16D2C919F4E" class="Filicopsida" family="Gleicheniaceae" genus="Dicranopteris" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Dicranopteris linearis" order="Gleicheniales" pageId="2" pageNumber="95" phylum="Pteridophyta" rank="species" species="linearis">
<emphasis id="4B23ECA23EC57B97C9F89CEC829766E8" italics="true" pageId="2" pageNumber="95">Dicranopteris linearis</emphasis>
</taxonomicName>
(Burm. f.) Underw., 
<taxonomicName id="4B7367262590AF2A3CDFA8AED8EA14CC" authorityName="Kaulfuss" authorityYear="1820" class="Filicopsida" family="Dicksoniaceae" genus="Cibotium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Cibotium" order="Polypodiales" pageId="2" pageNumber="95" phylum="Pteridophyta" rank="genus">
<emphasis id="F800C2E4FFCBAEFA623FF46E270CF62A" italics="true" pageId="2" pageNumber="95">Cibotium</emphasis>
