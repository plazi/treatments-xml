<number id="3DF924AFFFFBFFC353AE5DA0FE60F957" box="[439,449,1670,1693]" pageId="11" pageNumber="32" value="1.0">1</number>
/ 
<number id="3DF924AFFFFBFFC353D25DA0FE78F957" box="[459,473,1670,1693]" pageId="11" pageNumber="32" value="3.0">3</number>
