<taxonomicName id="4C344D7AAA68FF8BFDB1F90BFCB4F8BE" authority="Nabelek (1929: 9)" authorityName="Nabelek" authorityPageNumber="9" authorityYear="1929" box="[519,875,1784,1810]" class="Liliopsida" family="Poaceae" genus="Pentatherum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="0" pageNumber="136" phylum="Tracheophyta" rank="genus">
