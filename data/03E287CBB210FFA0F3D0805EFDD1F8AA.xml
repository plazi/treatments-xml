<taxonomicName id="4C4B4D5EB210FFA0F7968255FEEAF8AA" authority="(Fitch, 1859)" baseAuthorityName="Fitch" baseAuthorityYear="1859" class="Insecta" family="Cynipidae" genus="Ceroptres" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="quercusarbos">
<emphasis id="B93FEACFB210FFA0F7968255FF73F8AA" italics="true" pageId="59" pageNumber="60">Ceroptres quercusarbos</emphasis>
