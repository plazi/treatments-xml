<taxonomicName id="4C70A5BCFFABFF8EFB07F9FCFC5F0869" authorityName="Williams" authorityYear="1988" class="Malacostraca" family="Alvinocarididae" genus="Alvinocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="459" phylum="Arthropoda" rank="species" species="muricola">
