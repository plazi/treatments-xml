<taxonomicName id="3E6F96717AF1506B6FB1B982A3703505" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142329" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica gracilescens Smith" order="Hymenoptera" pageId="7" pageNumber="74" phylum="Arthropoda" rank="species" species="gracilescens">Myrmica gracilescens</taxonomicName>
