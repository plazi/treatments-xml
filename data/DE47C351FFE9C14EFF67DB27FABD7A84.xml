<subSubSection id="1EF421CCFFEEC14EFF37DFCFFA637AF8" box="[199,1429,402,429]" pageId="116" pageNumber="117" type="etymology">
<paragraph id="56517247FFEEC14EFF37DFCFFA637AF8" blockId="116.[151,1437,150,465]" box="[199,1429,402,429]" pageId="116" pageNumber="117">
<emphasis id="649AAE55FFEEC14EFF37DFCFFEB87AF8" bold="true" box="[199,334,402,428]" pageId="116" pageNumber="117">Etymology.</emphasis>
