<emphasis id="B9139A0DFFD6121772E60B319A98F923" box="[733,745,1661,1684]" italics="true" pageId="10" pageNumber="55">c</emphasis>
<emphasis id="B9139A0DFFD6121772D10BC79A83F92F" box="[746,754,1675,1688]" italics="true" pageId="10" pageNumber="55">
<number id="3DAF5438FFD6121772D10BC79A83F92F" box="[746,754,1675,1688]" pageId="10" pageNumber="55" value="2.0">2</number>
</emphasis>
and 
<emphasis id="B9139A0DFFD6121773110B319B47F923" box="[810,822,1661,1684]" italics="true" pageId="10" pageNumber="55">c</emphasis>
