<emphasis id="B9139A0DFFD6121770E30A709885F8E4" box="[216,244,1852,1875]" italics="true" pageId="10" pageNumber="55">an</emphasis>
<emphasis id="B9139A0DFFD6121770CF0A06988DF8E0" box="[244,252,1866,1879]" italics="true" pageId="10" pageNumber="55">
<number id="3DAF5438FFD6121770CF0A06988DF8E0" box="[244,252,1866,1879]" pageId="10" pageNumber="55" value="1.0">1</number>
</emphasis>
and 
<emphasis id="B9139A0DFFD61217710E0A709920F8E4" box="[309,337,1852,1875]" italics="true" pageId="10" pageNumber="55">an</emphasis>
<emphasis id="B9139A0DFFD61217716A0A069928F8E0" box="[337,345,1866,1879]" italics="true" pageId="10" pageNumber="55">
<number id="3DAF5438FFD61217716A0A069928F8E0" box="[337,345,1866,1879]" pageId="10" pageNumber="55" value="2.0">2</number>
