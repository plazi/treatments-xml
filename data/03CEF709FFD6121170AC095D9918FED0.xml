<emphasis id="B9139A0DFFD6121772DF0A5A9A83F89A" box="[740,754,1814,1837]" italics="true" pageId="10" pageNumber="55">g</emphasis>
<emphasis id="B9139A0DFFD6121772C90A689A8BF886" box="[754,762,1828,1841]" italics="true" pageId="10" pageNumber="55">
<number id="3DAF5438FFD6121772C90A689A8BF886" box="[754,762,1828,1841]" pageId="10" pageNumber="55" value="8.0">8</number>
</emphasis>
