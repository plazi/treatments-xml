<paragraph id="F74C3691BF6FFFABFF5EFE729883FD9C" blockId="44.[151,1436,511,1498]" box="[199,662,511,538]" pageId="44" pageNumber="45">
<materialsCitation id="479B3CCCBF6FFFABFF5EFE729887FD9C" box="[199,658,511,538]" country="Antarctica" location="Weddell Sea" pageId="44" pageNumber="45" specimenCount="1" typeStatus="holotype">
