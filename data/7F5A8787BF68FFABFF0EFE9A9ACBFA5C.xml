<location id="F22C604ABF6FFFABFEEAFD8D981DFD9C" LSID="urn:lsid:plazi:treatment:7F5A8787BF68FFABFF0EFE9A9ACBFA5C:F22C604ABF6FFFABFEEAFD8D981DFD9C" box="[371,520,512,538]" country="Antarctica" name="Weddell Sea" pageId="44" pageNumber="45">Weddell Sea</location>
, 
<collectingCountry id="8FE47601BF6FFFABFD8DFD8D9887FD9C" box="[532,658,512,538]" name="Antarctica" pageId="44" pageNumber="45">Antarctica</collectingCountry>
