<geoCoordinate id="EE32DF84FFD2FF9A5600EA4022EAF885" box="[597,721,1839,1863]" direction="north" orientation="latitude" pageId="6" pageNumber="219" precision="15" value="2.3277776">2°19'40&quot;N</geoCoordinate>
