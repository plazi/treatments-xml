<taxonomicName id="D084F4CF6A29FFF6FF0BFE787D40BCC4" authority="Burmeister, 1873: 407" authorityName="Burmeister" authorityPageNumber="407" authorityYear="1873" box="[175,609,428,449]" class="Insecta" family="Scarabaeidae" genus="Glyphoderus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="303" phylum="Arthropoda" rank="species" species="centralis">
<emphasis id="25F0535E6A29FFF6FF0BFE787EA4BCC4" box="[175,389,428,449]" italics="true" pageId="8" pageNumber="303">Glyphoderus centralis</emphasis>
