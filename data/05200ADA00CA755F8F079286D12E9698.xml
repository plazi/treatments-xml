<taxonomicName id="8C95D8CCF0F7082F69FD15FD270FBFE9" class="Dicotyledoneae" family="Apocynaceae" genus="Calotropis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Calotropis procera" order="Gentianales" pageId="34" pageNumber="35" phylum="Angiospermae" rank="species" species="procera">
