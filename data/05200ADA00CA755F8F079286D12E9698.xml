<taxonomicName id="8B834D6AEEDF6CC42509B30305AE15F9" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia ehrenbergiana" order="Fabales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="ehrenbergiana">
