<taxonomicName id="04654FA1B09184EFF47B4B929861CB4A" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="gerrardii">
