<taxonomicName id="C0DD5DD1FF9DFFF0FBC1F9D6FB13FC69" box="[1090,1174,918,944]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="642" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
