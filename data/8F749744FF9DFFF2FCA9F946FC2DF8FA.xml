<typeStatus id="D86698F0FF9DFFF0FCA9F932FC1CFC55" box="[810,921,882,908]" pageId="5" pageNumber="642">Holotype</typeStatus>
.
</emphasis>
<collectionCode id="61CCBE97FF9DFFF0FC24F932FC6DFC55" box="[935,1000,882,908]" pageId="5" pageNumber="642">KAM</collectionCode>
No. 
<specimenCode id="577B8E29FF9DFFF0FBA7F932FBDAFC55" box="[1060,1119,882,908]" pageId="5" pageNumber="642">7468</specimenCode>
, 
<specimenCount id="11DBEDDBFF9DFFF0FBEFF932FB07FC55" box="[1132,1154,882,908]" pageId="5" pageNumber="642" type="female">♀</specimenCount>
