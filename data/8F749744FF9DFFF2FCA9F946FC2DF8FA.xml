<materialsCitation id="B7B52C0FFF9DFFF0FB39FF96FCFDF985" ID-GBIF-Occurrence="3418289306" country="Russia" location="Upper Eocene" pageId="5" pageNumber="642" specimenCount="1" stateProvince="Kaliningrad">
