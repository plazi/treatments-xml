<taxonomicName id="11EFCCDFA330DC3DB91FF913FA20F910" authority="Linnaeus" authorityName="Linnaeus" box="[1102,1431,1763,1787]" class="Gastropoda" family="Lymnaeidae" genus="Lymnaea" kingdom="Animalia" order="Hygrophila" pageId="0" pageNumber="588" phylum="Mollusca" rank="species" species="stagnalis">
