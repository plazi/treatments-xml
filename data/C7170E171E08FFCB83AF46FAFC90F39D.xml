<subSubSection id="07A4EC8A1E08FFCB83AF473FFC90F39D" pageId="8" pageNumber="95" type="reference_group">
<paragraph id="4F01BF011E08FFCB83AF473FFDA8F571" blockId="8.[151,561,831,856]" box="[151,561,831,856]" pageId="8" pageNumber="95">
<treatmentCitationGroup id="6FAE982F1E08FFCB83AF473FFDA8F571" box="[151,561,831,856]" pageId="8" pageNumber="95">
<treatmentCitation id="CE1F99101E08FFCB83AF473FFDB4F571" author="Berg, C." box="[151,557,831,856]" page="137" pageId="8" pageNumber="95" year="1879">
<taxonomicName id="88BEC4821E08FFCB83AF473FFDB4F571" authority="Berg 1879: 137" authorityName="Berg" authorityPageNumber="137" authorityYear="1879" box="[151,557,831,856]" class="Insecta" family="Cicadidae" genus="Tettigades" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="95" phylum="Arthropoda" rank="species" species="cinnabarina">
<emphasis id="7DCA63131E08FFCB83AF473FFE18F571" box="[151,385,831,855]" italics="true" pageId="8" pageNumber="95">Tettigades cinnabarina</emphasis>
<bibRefCitation id="2B2FC2F01E08FFCB82BF4740FDB4F571" author="Berg, C." box="[391,557,831,856]" pageId="8" pageNumber="95" pagination="135 - 144" refId="ref7089" refString="Berg, C. (1879) Hemiptera Argentina (Continuacion.) Hemiptera Homoptera Latr. Anales de la Sociedad Cientifica Argentina, 8, 135 - 144." type="journal article" year="1879">Berg 1879: 137</bibRefCitation>
</taxonomicName>
