<caption id="1EB36633FF84FF91B38AF90DF3F5F8BB" httpUri="https://zenodo.org/record/3066709/files/figure.png" pageId="13" pageNumber="446" startId="13.[151,250,1709,1734]" targetBox="[151,1435,162,1674]" targetPageId="13">
<paragraph id="4A7336BBFF84FF91B38AF90DF3F5F8BB" blockId="13.[151,1437,1709,1799]" pageId="13" pageNumber="446">
