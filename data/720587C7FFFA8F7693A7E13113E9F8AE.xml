<collectingRegion id="3868F833FFFA8F76902AE1C41290F966" box="[794,901,1753,1778]" country="Argentina" name="Mendoza" pageId="79" pageNumber="80">Mendoza</collectingRegion>
, 
<collectingRegion id="3868F833FFFA8F7690A6E1C412C4F966" box="[918,977,1753,1778]" country="Argentina" name="Salta" pageId="79" pageNumber="80">Salta</collectingRegion>
