<taxonomicName id="DBA8162D1C73A7E158990A736C5385DB" authorityName="L., Sp. Pl. 1: 153. 1753. (Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus" order="Solanales" pageId="118" pageNumber="119" phylum="Tracheophyta" rank="genus">
