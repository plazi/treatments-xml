species: 
<taxonomicName id="4C3A04B3FFCAFFC87D418AD1FE17FD02" authorityName="Martynov" authorityYear="1935" box="[301,502,720,743]" class="Insecta" family="Polycentropodidae" genus="Cyrnopsis" kingdom="Animalia" order="Trichoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="palpalis">
