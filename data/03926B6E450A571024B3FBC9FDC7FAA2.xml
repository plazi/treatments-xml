<collectingCountry id="F32C9AE8450A571126DEFB13FCE6FB56" box="[735,819,1188,1210]" name="Thailand" pageId="20" pageNumber="725">Thailand</collectingCountry>
: Larnsang National Park bei 
<collectingRegion id="49FF149A450A571124DEFB0BFED0FB3E" box="[223,261,1212,1234]" country="Thailand" name="Tak" pageId="20" pageNumber="725">Tak</collectingRegion>
, 
<date id="FF85FCB8450A5711250FFB0BFEAAFB3D" box="[270,383,1212,1233]" pageId="20" pageNumber="725" value="1987-12-27">27.XII.1987</date>
