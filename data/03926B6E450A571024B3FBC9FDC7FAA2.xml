<typeStatus id="548064DA450A57112559FB13FE62FB56" box="[344,439,1188,1210]" pageId="20" pageNumber="725">Holotypus</typeStatus>
() und 2, 1 - 
<typeStatus id="548064DA450A57112674FB13FD00FB55" box="[629,725,1188,1209]" pageId="20" pageNumber="725">Paratypen</typeStatus>
