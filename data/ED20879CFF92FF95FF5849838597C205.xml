<taxonomicName id="A2894D09FF92FF95FF584E14865CC36F" authority="Princis, 1950: 163" authorityName="Princis" authorityPageNumber="163" authorityYear="1950" box="[151,615,1025,1047]" class="Insecta" family="Blaberidae" genus="Diploptera" kingdom="Animalia" order="Blattodea" pageId="17" pageNumber="52" phylum="Arthropoda" rank="species" species="erythrocephala">
