<taxonomicName id="1AD04D44A76CFFA7FF7CF8B2FF542C4E" box="[151,238,1818,1847]" class="Insecta" family="Lonchaeidae" genus="Helina" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="236" phylum="Arthropoda" rank="genus">
<emphasis id="EFA4EAD5A76CFFA7FF7CF8B2FF542C4E" box="[151,238,1818,1847]" italics="true" pageId="3" pageNumber="236">Helina</emphasis>
</taxonomicName>
