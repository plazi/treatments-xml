<taxonomicName id="1AD04D44A76CFFA7FE77F8B2FDA12C4E" box="[412,539,1818,1847]" class="Insecta" family="Muscidae" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="236" phylum="Arthropoda" rank="family">Muscidae</taxonomicName>
