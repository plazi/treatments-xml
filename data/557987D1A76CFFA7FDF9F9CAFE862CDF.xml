<taxonomicName id="1AD04D44A76CFFA7FEC1F8B2FE2B2C4E" box="[298,401,1818,1847]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="236" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
