<materialsCitation id="0FFE3C9CFFF3FFE2FF0FAAF9FF5643A6" ID-GBIF-Occurrence="1425219618" location="Northeastern Atlantic" pageId="108" pageNumber="109" specimenCount="1" stateProvince="Aquitaine">
