<taxonomicName id="4C475EA4FF802D6DFD18F920BD66FFC6" box="[661,782,1727,1752]" class="Insecta" family="Miridae" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="tribe" tribe="Herdoniini">Herdoniini</taxonomicName>
