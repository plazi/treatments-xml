<quantity id="4CDF9B3F294EFFECFD40E45FFCC04F36" box="[662,781,591,620]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" pageId="14" pageNumber="13" unit="mm" value="4.9">4.90 mm</quantity>
