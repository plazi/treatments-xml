<quantity id="4CDF9B3F294EFFECFE4FE463FDD44FCD" box="[409,537,627,656]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" pageId="14" pageNumber="13" unit="mm" value="2.65">2.65 mm</quantity>
