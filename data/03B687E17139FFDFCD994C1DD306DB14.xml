, 
<collectingCountry id="F30876677139FFDFCF874BE1D088DA84" box="[649,734,1161,1185]" name="Taiwan" pageId="18" pageNumber="19">Taiwan</collectingCountry>
, Peninsular 
<collectingCountry id="F30876677139FFDFCE654BE1D182DA84" box="[875,980,1161,1185]" name="Malaysia" pageId="18" pageNumber="19">Malaysia</collectingCountry>
, 
<collectingCountry id="F30876677139FFDFCEED4BE1D613DA84" box="[995,1093,1161,1185]" name="Vietnam" pageId="18" pageNumber="19">Vietnam</collectingCountry>
, 
<collectingCountry id="F30876677139FFDFC95C4BE1D695DA84" box="[1106,1219,1161,1185]" name="Indonesia" pageId="18" pageNumber="19">Indonesia</collectingCountry>
