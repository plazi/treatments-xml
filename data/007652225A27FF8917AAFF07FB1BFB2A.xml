<collectingCountry id="F0C8A3A45A27FF89157EFCB8FD42FCA5" box="[579,693,807,832]" name="Argentina" pageId="6" pageNumber="307">Argentina</collectingCountry>
, 
<elevation id="03F204075A27FF8915FCFCB8FCEEFCDA" box="[705,793,807,831]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="6" pageNumber="307" unit="m" value="1700.0">
<quantity id="4F274ED15A27FF8915FCFCB8FCEEFCDA" box="[705,793,807,831]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="6" pageNumber="307" unit="m" value="1700.0">1700 m</quantity>
