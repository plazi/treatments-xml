<quantity id="4F274ED15A27FF8916AAFE66FE0CFDF4" box="[407,507,505,530]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="6" pageNumber="307" unit="mm" value="3.6">3.60 mm</quantity>
