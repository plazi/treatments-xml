<taxonomicName id="B7A603A6FF94D207510CFBDE96DCFB36" authorityName="Spinola" authorityYear="1811" box="[343,470,1151,1177]" class="Insecta" family="Torymidae" genus="Podagrion" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="783" phylum="Arthropoda" rank="genus">
