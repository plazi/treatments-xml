<paragraph id="8BF23685FFE1FF8EFF30F85BB8DDBF8C" blockId="12.[136,651,1852,1988]" box="[136,459,1924,1952]" pageId="12" pageNumber="213">
<taxonomicName id="4C4D4D06FFE1FF8EFF30F85BB8DDBF8C" authority="(Benth.) Barneby" authorityName="(Benth.) Barneby" box="[136,459,1924,1952]" pageId="12" pageNumber="213" rank="variety" variety="vestita">
