<quantity id="4C819B0BFF9DFF88ABD2F81FFE7CF809" box="[408,486,1989,2015]" metricMagnitude="2" metricUnit="m" metricValue="4.8" pageId="14" pageNumber="15" unit="m" value="480.0">480 m</quantity>
