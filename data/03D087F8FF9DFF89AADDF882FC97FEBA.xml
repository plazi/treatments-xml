<quantity id="4C819B0BFF9DFF88A915F87AFC4EF86C" box="[863,980,1952,1978]" metricMagnitude="-2" metricUnit="m" metricValue="5.36" pageId="14" pageNumber="15" unit="mm" value="53.6">53.6 mm</quantity>
