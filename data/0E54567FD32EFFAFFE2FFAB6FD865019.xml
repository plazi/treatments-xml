<collectionCode id="E0EC7FACD32EFFAEFDB4FA65FD0057A8" box="[616,681,1487,1506]" country="Czech Republic" httpUri="http://grbio.org/cool/hj12-2xm7" name="National Museum Prague" pageId="16" pageNumber="71">NMPC</collectionCode>
)
