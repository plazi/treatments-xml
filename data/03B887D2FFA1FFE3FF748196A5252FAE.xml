<taxonomicName id="4C114D47FFA1FFE3FE2783BFA29C288B" authority="Cid-Arcos &amp; Campodonico, 2020" authorityName="Cid-Arcos &amp; Campodonico" authorityYear="2020" box="[494,668,920,944]" class="Insecta" family="Buprestidae" genus="Lasionota" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="652" phylum="Arthropoda" rank="species" species="aonikenk" status="sp. nov." subGenus="Arqueozodes">
