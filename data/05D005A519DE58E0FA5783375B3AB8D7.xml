<taxonomicName id="58975409A07BFED94A8C7AA425E156EE" LSID="http://zoobank.org/3704DA92-9DC3-4DD2-B4D7-6D08B35896F7" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus reinhardi" order="Hymenoptera" pageId="147" pageNumber="148" phylum="Arthropoda" rank="species" species="reinhardi">Heterospilus reinhardi Marsh</taxonomicName>
<taxonomicNameLabel id="7AAD2646D71FD182FD39D169A49429BA" pageId="147" pageNumber="148">sp. n.</taxonomicNameLabel>
