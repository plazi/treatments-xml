<taxonomicName id="59E304AABE1DFFA9FEB7FBC4FDADB17C" authority="Tomikawa 2007" authorityName="Tomikawa" authorityYear="2007" box="[299,618,1049,1073]" class="Malacostraca" family="Luciobliviidae" genus="Lucioblivio" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
