, 
<taxonomicName id="4C064D55FFCD5540FE3D3E9895F275DD" authority="Malloch" authorityName="Malloch" box="[444,671,151,176]" class="Insecta" family="Sphaeroceridae" genus="Minilimosina" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="parva">
