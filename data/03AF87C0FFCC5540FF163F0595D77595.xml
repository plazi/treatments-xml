and 
<taxonomicName id="4C064D55FFCD5540FD5A3E98936E75DD" authority="Stenhammar" authorityName="Stenhammar" box="[731,1027,151,176]" class="Insecta" family="Sphaeroceridae" genus="Minilimosina" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="parvula">
