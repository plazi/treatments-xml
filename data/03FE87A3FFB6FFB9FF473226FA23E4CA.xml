<typeStatus id="54EC8817FFB6FFB9FF4732D3FF73E0F3" box="[151,204,698,721]" pageId="36" pageNumber="37">Type</typeStatus>
species: 
<taxonomicName id="4C574D36FFB6FFB9FEFD32D3FD5AE0F3" authority="Chretien, 1899" authorityName="Chretien" authorityYear="1899" box="[301,741,698,721]" class="Insecta" family="Gelechiidae" genus="Schistophila" kingdom="Animalia" order="Lepidoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="laurocistella">
