species. 
<taxonomicName id="4C17FB9DFFE5FF9964618DCAFD7CABD8" authorityName="Mound" authorityYear="1970" box="[571,674,1730,1752]" class="Insecta" family="Phlaeothripidae" genus="Mystrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="354" phylum="Arthropoda" rank="species" species="dilatus">
