<taxonomicName id="4C17FB9DFFE5FF9962C68DCAFAC3ABD8" authorityName="Shin &amp; Woo" authorityYear="1999" box="[1180,1309,1731,1752]" class="Insecta" family="Phlaeothripidae" genus="Mystrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="354" phylum="Arthropoda" rank="species" species="reteanum">
<emphasis id="B9635C0CFFE5FF9962C68DCAFAC3ABD8" box="[1180,1309,1731,1752]" italics="true" pageId="7" pageNumber="354">M. reteanum</emphasis>
