<taxonomicName id="41F24D424D54EE2CB309F7DCF8506F5F" authority="Nicholls, 1944" authorityName="Nicholls" authorityYear="1944" box="[1548,2029,2133,2172]" class="Malacostraca" family="Phreatoicidae" genus="Crenoicus" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="10" phylum="Arthropoda" rank="species" species="mixtus">
