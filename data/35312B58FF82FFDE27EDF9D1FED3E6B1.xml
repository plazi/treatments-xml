(
<collectingRegion id="7F5C54ACFF83FFDE257AFC78FDB8E6D1" box="[338,541,925,948]" country="Laos" name="Salavan" pageId="11" pageNumber="302">Saravane Province</collectingRegion>
); 
<collectingCountry id="C58FDADEFF83FFDE2606FC78FD2CE6D1" box="[558,649,925,948]" name="Vietnam" pageId="11" pageNumber="302">Vietnam</collectingCountry>
(
<collectingRegion id="7F5C54ACFF83FFDE26BEFC78FD48E6D1" box="[662,749,925,948]" country="Vietnam" name="Ha Tinh" pageId="11" pageNumber="302">Ha Tinh</collectingRegion>
and 
<collectingRegion id="7F5C54ACFF83FFDE24B6FC58FF50E6B1" box="[158,245,957,980]" country="Vietnam" name="Lao Cai" pageId="11" pageNumber="302">Lao Cai</collectingRegion>
provinces).
</paragraph>
