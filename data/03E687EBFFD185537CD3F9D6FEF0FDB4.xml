<quantity id="4CB79B18FFD1855C7847F919FBE7F959" box="[1027,1119,1758,1782]" metricMagnitude="3" metricUnit="m" metricValue="3.1" pageId="7" pageNumber="330" unit="m" value="3100.0">3100 m</quantity>
