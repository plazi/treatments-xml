<taxonomicName id="9F4ABD022ED13ACAD7AC1EED17C46E62" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33840" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole longiscapa Forel" order="Hymenoptera" pageNumber="205" phylum="Arthropoda" rank="species" species="longiscapa">longiscapa</taxonomicName>
