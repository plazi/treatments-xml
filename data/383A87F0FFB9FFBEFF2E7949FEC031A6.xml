<taxonomicName id="77934D65FFB9FFB9FF2E7F00FE7A3512" authority="LeConte" authorityName="LeConte" box="[148,450,1366,1387]" class="Insecta" family="Corylophidae" genus="Orthoperus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="108" phylum="Arthropoda" rank="species" species="suturalis">
<emphasis id="82E7EAF4FFB9FFB9FF2E7F00FEDA3512" bold="true" box="[148,354,1366,1387]" italics="true" pageId="3" pageNumber="108">Orthoperus suturalis</emphasis>
