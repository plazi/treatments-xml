<keyLead id="523895BEFF9C6154FF75FEF1FA4D2874" pageId="3" pageNumber="454">
<number id="5F0124ACFF9C6154FF75FEF1FF732816" box="[151,165,298,319]" pageId="3" pageNumber="454" value="8.0">8</number>
