<taxonomicName id="2EC94D08FF9C6154FACDFE93FA4D2874" box="[1327,1435,327,349]" class="Insecta" family="Cicadidae" genus="Melampsalta" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="454" phylum="Arthropoda" rank="species" species="radiator">
