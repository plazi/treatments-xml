<taxonomicName id="8ECE4D24FFF0FFF0FDFAF493FD7D2CBF" authorityName="Stainton" authorityYear="1854" box="[548,696,960,983]" class="Insecta" family="Gracillariidae" kingdom="Animalia" order="Lepidoptera" pageId="50" pageNumber="395" phylum="Arthropoda" rank="family">Gracillariidae</taxonomicName>
