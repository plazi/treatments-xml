<paragraph id="59CF17C0ECA3D537AD8C0815208B5DBF" pageId="null" pageNumber="739">
<pageBreakToken id="5715D56A91D7A0B2EFD328A1196B7031" pageId="null" pageNumber="739" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="0866C523DC5C4B9C2CAF58F7933452B8" authority="L." class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="739" phylum="Tracheophyta" rank="species" species="aviculare">
Polygonum 
<normalizedToken id="6BA17B7D9B372C33D94A5ADCA803284A" originalValue="aviculáre" pageId="null" pageNumber="739">aviculare</normalizedToken>
<authorityName id="EB333DF3A06315A927CEDCB7AF976E89" pageId="null" pageNumber="739">L.</authorityName>
</taxonomicName>
</paragraph>
</subSubSection>
