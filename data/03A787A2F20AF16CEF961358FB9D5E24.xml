<taxonomicName id="4C0E4D37F20AF16CEEFA13D8FD8058BB" box="[416,513,1460,1480]" class="Aves" family="Aegithalidae" genus="Psaltriparus" kingdom="Animalia" order="Passeriformes" pageId="62" pageNumber="62" phylum="Chordata" rank="species" species="melanotis">
