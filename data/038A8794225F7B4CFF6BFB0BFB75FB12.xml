<quantity id="4CDB9B67225D7B4CFB96FED1FB75FE90" box="[1130,1226,271,297]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="26" pageNumber="27" unit="mm" value="0.3">0.3 mm</quantity>
