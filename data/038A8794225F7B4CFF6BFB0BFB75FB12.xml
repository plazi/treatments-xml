<quantity id="4CDB9B67225D7B4CFBB1FEE6FB12FEEB" box="[1101,1197,312,338]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="26" pageNumber="27" unit="mm" value="1.6">1.6 mm</quantity>
; ultimate section of CuA 
<number id="3DEB24A5225D7B4CFF32FEACFF69FE39" box="[206,214,370,384]" pageId="26" pageNumber="27" value="1.0">1</number>
about 
<number id="3DEB24A5225D7B4CFEDAFEBCFE8FFEC5" box="[294,304,354,380]" pageId="26" pageNumber="27" value="1.0">1</number>
/ 
<number id="3DEB24A5225D7B4CFEC7FEBCFEF4FEC5" box="[315,331,354,380]" pageId="26" pageNumber="27" value="6.0">6</number>
