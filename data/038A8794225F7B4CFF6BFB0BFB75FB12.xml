<quantity id="4CDB9B67225D7B4CFC83FED1FC64FE90" box="[895,987,271,297]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="26" pageNumber="27" unit="mm" value="1.9">1.9 mm</quantity>
: 
<quantity id="4CDB9B67225D7B4CFC08FED1FBEEFE90" box="[1012,1105,271,297]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="26" pageNumber="27" unit="mm" value="0.6">0.6 mm</quantity>
