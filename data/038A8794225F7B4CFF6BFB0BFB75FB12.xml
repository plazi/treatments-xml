<quantity id="4CDB9B67225D7B4CFC2BFEE6FB8CFEEB" box="[983,1075,312,338]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="26" pageNumber="27" unit="mm" value="1.2">1.2 mm</quantity>
