<quantity id="4CDB9B67225D7B4CFD01FE96FC99FEEB" box="[765,806,312,342]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="26" pageNumber="27" unit="in" value="1.0">1 in</quantity>
