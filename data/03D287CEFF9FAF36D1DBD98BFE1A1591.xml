<subSubSection id="C3616553FF9FAF31D1DBD98BFE541441" box="[151,498,1791,1817]" pageId="43" pageNumber="44" type="nomenclature">
<paragraph id="8BC436D8FF9FAF31D1DBD98BFE541441" blockId="43.[151,498,1791,1817]" box="[151,498,1791,1817]" pageId="43" pageNumber="44">
<heading id="D08C81B4FF9FAF31D1DBD98BFE541441" bold="true" box="[151,498,1791,1817]" fontSize="11" level="1" pageId="43" pageNumber="44" reason="1">
<emphasis id="B90FEACAFF9FAF31D1DBD98BFE541441" bold="true" box="[151,498,1791,1817]" pageId="43" pageNumber="44">
16. 
<emphasis id="B90FEACAFF9FAF31D18FD98BFECC1441" bold="true" box="[195,362,1791,1817]" italics="true" pageId="43" pageNumber="44">
<taxonomicName id="4C7B4D5BFF9FAF31D18FD98BFEC21441" authority="Vaz-De-Mello, 2008" authorityName="Vaz-De-Mello" authorityYear="2008" box="[195,356,1791,1817]" class="Insecta" family="Scarabaeidae" genus="Trichillidium" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus" status="new genus">Trichillidium</taxonomicName>
