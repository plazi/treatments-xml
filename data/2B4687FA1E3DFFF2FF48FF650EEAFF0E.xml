<quantity id="64179B091E3DFFF1FF3EFA830A00FA52" box="[254,320,1400,1422]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="246" unit="mm" value="10.0">10 mm</quantity>
