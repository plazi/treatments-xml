<taxonomicName id="4C324D33B44FFFE4AD26D5C7FB3CA0D1" box="[1117,1224,1238,1262]" class="Insecta" family="Cicadellidae" genus="Agallia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="onukii">
