36. 
<taxonomicName id="EE35EB04426D17E9F228CEA9A91C673C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32432" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica ruginodis Nylander" order="Hymenoptera" pageNumber="66" phylum="Arthropoda" rank="species" species="ruginodis">Myrm. ruginodis</taxonomicName>
