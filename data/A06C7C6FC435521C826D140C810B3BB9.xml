<taxonomicName id="BA857EFFDEB640CB9664C4C14966A0E7" LSID="A06C7C6F-C435-521C-826D-140C810B3BB9" authority="- group" class="Arachnida" family="Telemidae" genus="Pinelema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="adunca" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="adunca">adunca -group</taxonomicName>
