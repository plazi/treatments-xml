(
<collectingRegion id="D45CE2D8E46DFF9BFE6AC0D9FD05B9A1" box="[451,660,1755,1781]" country="China" name="Yunnan" pageId="2" pageNumber="61">Yunnan Province</collectingRegion>
).
</paragraph>
