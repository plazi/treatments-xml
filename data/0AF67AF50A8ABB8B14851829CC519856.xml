<location id="A1CC46B9ACE7C6ED39CEDE01C2FADCC8" LSID="urn:lsid:plazi:treatment:0AF67AF50A8ABB8B14851829CC519856:A1CC46B9ACE7C6ED39CEDE01C2FADCC8" country="Germany" county="Upper Palatinate" latitude="49.015" longitude="12.09556" name="Regensburg" stateProvince="Bavaria">In hiesiger Gegend</location>
