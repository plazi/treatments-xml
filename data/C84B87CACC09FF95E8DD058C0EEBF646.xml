<quantity id="871A9B39CC09FFAAEC6E07F00B69F29D" box="[1060,1142,1975,1999]" metricMagnitude="-2" metricUnit="m" metricValue="8.0" pageId="31" pageNumber="32" unit="mm" value="80.0">80 mm</quantity>
in diameter, with female gonothecae, basibiont of 
<taxonomicName id="87E24D5FCC09FFAAE9FB079B0D94F2A0" box="[433,651,2012,2035]" class="Hydrozoa" family="Bougainvilliidae" genus="Bimeria" kingdom="Animalia" order="Anthoathecata" pageId="31" pageNumber="32" phylum="Cnidaria" rank="species" species="corynopsis">
<emphasis id="7296EACECC09FFAAE9FB079B0D94F2A0" box="[433,651,2012,2035]" italics="true" pageId="31" pageNumber="32">Bimeria corynopsis</emphasis>
</taxonomicName>
, 
<taxonomicName id="87E24D5FCC09FFAAEAD0079D0C6BF2A0" box="[666,884,2010,2035]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="31" pageNumber="32" phylum="Cnidaria" rank="species" species="tangaroa" status="sp. nov.">
<emphasis id="7296EACECC09FFAAEAD0079D0C6BF2A0" box="[666,884,2010,2035]" italics="true" pageId="31" pageNumber="32">Halecium tangaroa</emphasis>
</taxonomicName>
<taxonomicNameLabel id="69A557B5CC09FFAAEB31079D0CCBF2A0" box="[891,980,2010,2035]" pageId="31" pageNumber="32" rank="species">
<emphasis id="7296EACECC09FFAAEB31079D0CCBF2A0" bold="true" box="[891,980,2010,2035]" pageId="31" pageNumber="32">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="87E24D5FCC09FFAAEC41079D0BB4F2A0" box="[1035,1195,2010,2035]" class="Hydrozoa" family="Hebellidae" genus="Hebella" kingdom="Animalia" order="Leptothecata" pageId="31" pageNumber="32" phylum="Cnidaria" rank="species" species="plana">
