species: 
<taxonomicName id="4C7A4D1FB7A7FF1265B878CADD83F98A" authority="Thaxt." box="[236,499,1527,1554]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Corethromyces" kingdom="Fungi" order="Laboulbeniales" pageId="246" pageNumber="247" phylum="Ascomycota" rank="species" species="conosomatis">
