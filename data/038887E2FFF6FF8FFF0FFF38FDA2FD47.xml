<paragraph id="8B9E36F4FFF7FF8CFF3AFB74FD69FAC9" blockId="13.[140,1108,1011,1671]" box="[185,667,1258,1285]" pageId="13" pageNumber="52">
<emphasis id="B955EAE6FFF7FF8CFF3AFB74FEFCFAC9" bold="true" box="[185,270,1258,1284]" inLineHeading="true" pageId="13" pageNumber="52" reason="1">Length</emphasis>
(1): body, 
<quantity id="4CD99B11FFF7FF8CFE08FB74FE1AFAC9" box="[395,488,1258,1285]" metricMagnitude="-3" metricUnit="m" metricValue="5.7" pageId="13" pageNumber="52" unit="mm" value="5.7">5.7 mm</quantity>
