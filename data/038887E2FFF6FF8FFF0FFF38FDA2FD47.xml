<quantity id="4CD99B11FFF7FF8CFDB9FB74FD6AFAC9" box="[570,664,1258,1284]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="13" pageNumber="52" unit="mm" value="4.2">4.2 mm</quantity>
