<geoCoordinate id="EE665045FFA8B30AE927093BFE04FBD6" box="[264,409,1039,1065]" degrees="34" direction="north" minutes="41.40" orientation="latitude" pageId="20" pageNumber="21" precision="9" value="34.69">34° 41.40´N</geoCoordinate>
