<collectingCountry id="F29317D6D92BFFB30643FCB0FE99FCA1" box="[265,364,796,819]" name="Thailand" pageId="17" pageNumber="122">Thailand</collectingCountry>
: 
<collectingRegion id="484099A4D92BFFB3063CFCB0FE00FCA1" box="[374,501,796,819]" country="Thailand" name="Chiang Mai" pageId="17" pageNumber="122">Chiang Mai</collectingRegion>
(
