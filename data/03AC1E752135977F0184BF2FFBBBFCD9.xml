<taxonomicName id="4C05D4E02136977F0754B8B2FBB8FD1A" authorityName="Silvestri" authorityYear="1948" box="[1027,1134,656,680]" class="Diplopoda" family="Polyxenidae" genus="Chilexenus" kingdom="Animalia" order="Polyxenida" pageId="2" pageNumber="831" phylum="Arthropoda" rank="genus">
