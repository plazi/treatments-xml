<paragraph id="089E45324A54F52DB3641F2260E189AC" lastPageId="3" lastPageNumber="92" pageId="2" pageNumber="91">
<bibRefCitation id="3FDBE60807201289C2CD8FD51D024982" author="Fernandez, F" journalOrPublisher="Biota Colombiana" pageId="5" pageNumber="94" pagination="3 - 24" refId="B9" refString="Fernandez, F, 2000. Avispas Cazadoras de Aranas (Hymenoptera: Pompilidae) de la Region Neotropical. Biota Colombiana 1: 3 - 24" title="Avispas Cazadoras de Aranas (Hymenoptera: Pompilidae) de la Region Neotropical." volume="1" year="2000">
<normalizedToken id="B5A72A3D6D760EFFA9E7B5B9E69822DD" originalValue="Fernández">Fernandez</normalizedToken>
