<taxonomicName id="906F39486869CC781ED0EC2B07FABD99" authorityName="Dreisbach" authorityYear="1950" class="Hexapoda" family="Pompilidae" genus="Priochilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Priochilus formosum subsp. hondurense" order="Hymenoptera" pageId="2" pageNumber="91" phylum="Arthropoda" rank="subSpecies" species="formosum" subSpecies="hondurense">
<emphasis id="5FDE100ED1884F6DFE0E6034E0B23B4B" italics="true" pageId="2" pageNumber="91">Priochilus formosum hondurense</emphasis>
</taxonomicName>
