<taxonomicName id="4C5F4D6FC544C258FE6AFA6A07C3F8B6" box="[451,655,1798,1822]" class="Gastropoda" family="Palaeozygopleuridae" genus="Palaeozygopleura" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Murchisoniina" pageId="8" pageNumber="477" phylum="Mollusca" rank="genus">
