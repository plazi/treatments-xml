<emphasis id="B92BEAFEC544C258FAFFFBD2046EF953" italics="true" pageId="8" pageNumber="477">
<taxonomicName id="4C5F4D6FC544C258FAFFFBD200D0F97E" box="[1366,1436,1726,1750]" class="Gastropoda" family="Palaeozygopleuridae" genus="Palaeozygopleura" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Murchisoniina" pageId="8" pageNumber="477" phylum="Mollusca" rank="species" species="alinae">alinae</taxonomicName>
