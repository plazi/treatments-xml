<taxonomicName id="7C138F983C46C4E79D34487E5D32F577" class="Insecta" family="Cerambycidae" genus="Strongylurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Strongylurus" order="Coleoptera" pageId="209" pageNumber="210" phylum="Arthropoda" rank="genus">Strongylurus</taxonomicName>
