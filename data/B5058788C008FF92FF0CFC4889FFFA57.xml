<taxonomicName id="FAAC4D1DC008FF92FB7FFB9A8ECEFBFF" box="[1252,1436,1118,1141]" class="Magnoliopsida" family="Brassicaceae" genus="Berteroa" kingdom="Plantae" order="Brassicales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="incana">
<emphasis id="0FD8EA8CC008FF92FB7FFB9A8ECEFBFF" box="[1252,1436,1118,1141]" italics="true" pageId="9" pageNumber="56">Berteroa incana</emphasis>
