<taxonomicName id="FAAC4D1DC008FF92FE9CFB468A9EFB13" box="[263,460,1153,1177]" class="Magnoliopsida" family="Brassicaceae" genus="Capsella" kingdom="Plantae" order="Brassicales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="bursa-pastoris">
<emphasis id="0FD8EA8CC008FF92FE9CFB468A9EFB13" box="[263,460,1153,1177]" italics="true" pageId="9" pageNumber="56">C. bursa-pastoris</emphasis>
