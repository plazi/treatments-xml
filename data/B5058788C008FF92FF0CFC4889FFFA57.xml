<taxonomicName id="FAAC4D1DC008FF92FE40FB468912FB13" box="[475,576,1153,1177]" class="Magnoliopsida" family="Brassicaceae" genus="Lepidium" kingdom="Plantae" order="Brassicales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="draba">
<emphasis id="0FD8EA8CC008FF92FE40FB468912FB13" box="[475,576,1153,1177]" italics="true" pageId="9" pageNumber="56">C. draba</emphasis>
</taxonomicName>
, 
<taxonomicName id="FAAC4D1DC008FF92FDD4FB4689A7FB13" box="[591,757,1154,1177]" class="Magnoliopsida" family="Apiaceae" genus="Daucus" kingdom="Plantae" order="Apiales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="carota">
<emphasis id="0FD8EA8CC008FF92FDD4FB4689A7FB13" box="[591,757,1154,1177]" italics="true" pageId="9" pageNumber="56">Daucus carota</emphasis>
</taxonomicName>
L. (family Umbelliferae), 
<taxonomicName id="FAAC4D1DC008FF92FBBFFB468FC0FB13" box="[1060,1170,1153,1177]" class="Magnoliopsida" family="Brassicaceae" genus="Sisymbrium" kingdom="Plantae" order="Brassicales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="sophia">
<emphasis id="0FD8EA8CC008FF92FBBFFB468FC0FB13" box="[1060,1170,1153,1177]" italics="true" pageId="9" pageNumber="56">D. sophia</emphasis>
</taxonomicName>
(previously listed as 
<taxonomicName id="FAAC4D1DC008FF92FA1CFB468BB0FB37" class="Magnoliopsida" family="Brassicaceae" genus="Sisymbrium" kingdom="Plantae" order="Brassicales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="sophia">
<emphasis id="0FD8EA8CC008FF92FA1CFB468BB0FB37" italics="true" pageId="9" pageNumber="56">S. sophia</emphasis>
