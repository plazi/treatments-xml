<number id="3DF87A1EFFF23B74C599FA1EFD75FA05" box="[656,686,1479,1505]" pageId="1" pageNumber="48" value="30.0">30</number>
<collectionCode id="ED21F0FCFFF23B74C5A4FA1EFD3EFA05" box="[685,741,1479,1505]" httpUri="http://grbio.org/cool/jq83-nt2d" name="Shimonoseki University of Fisheries" pageId="1" pageNumber="48">SUF</collectionCode>
