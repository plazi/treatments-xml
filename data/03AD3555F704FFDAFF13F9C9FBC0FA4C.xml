<collectingCountry id="F313C4D3F704FFD5FD60FCCDFCBCFCFF" box="[740,812,779,803]" name="Democratic Republic of the Congo" pageId="23" pageNumber="120">Congo</collectingCountry>
(Kinshasa), 
<collectingMunicipality id="6BDF1E39F704FFD5FC3CFCCCFBAEFCFE" box="[952,1086,778,802]" pageId="23" pageNumber="120">Bena-Longo</collectingMunicipality>
, fr. 
<date id="FFBAA283F704FFD5FBE9FCCCFB4FFCFF" box="[1133,1247,778,803]" pageId="23" pageNumber="120" value="1959-12">
<collectingDate id="EFFE5B6BF704FFD5FBE9FCCCFB4FFCFF" box="[1133,1247,778,803]" pageId="23" pageNumber="120" value="1959-12">Dec. 1959</collectingDate>
