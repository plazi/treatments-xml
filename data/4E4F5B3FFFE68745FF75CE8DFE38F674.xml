<collectingCountry id="BEF1AAB9FFE68745FD62CC71FD23F138" box="[640,756,837,863]" name="Argentina" pageId="29" pageNumber="30">Argentina</collectingCountry>
: 
<collectingRegion id="042224CBFFE68745FCE0CC71FC83F138" box="[770,852,837,863]" country="Argentina" name="Chubut" pageId="29" pageNumber="30">Chubut</collectingRegion>
