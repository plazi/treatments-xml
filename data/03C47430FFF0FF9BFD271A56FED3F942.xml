<typeStatus id="54D67B84FFF1FF9BFEF81A43FEC7F9BF" box="[300,374,1575,1596]" pageId="17" pageNumber="370">syntype</typeStatus>
of 
<taxonomicName id="4C6DBEA5FFF1FF9BFE4F1A4CFDDCF9BF" authority=", BMNH" authorityName="BMNH" box="[411,621,1575,1597]" class="Insecta" family="Cerambycidae" genus="Prosternodes" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="370" phylum="Arthropoda" rank="species" species="oberthuri">
<emphasis id="B9191934FFF1FF9BFE4F1A4CFDA3F9BE" box="[411,530,1576,1597]" italics="true" pageId="17" pageNumber="370">P. oberthuri</emphasis>
, BMNH
</taxonomicName>
]); 
