<taxonomicName id="6D80CD15EE08C1CAC1B527CF7674EBE5" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" baseAuthorityName="Kriebel" baseAuthorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia dissitiflora" order="Myrtales" pageId="162" pageNumber="163" phylum="Tracheophyta" rank="species" species="dissitiflora" status="comb. nov.">
