<taxonomicName id="4C214D415C589F7AFED0FA4DFE77FA0D" box="[288,412,1442,1465]" class="Insecta" family="Corixidae" genus="Cenocorixa" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="39" phylum="Arthropoda" rank="genus">Cenocorixa</taxonomicName>
