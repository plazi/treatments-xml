and 
<taxonomicName id="82ECD2933EA149D3283B2A8700133BF2" class="Actinopterygii" family="Loricariidae" genus="Pseudancistrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudancistrus asurini" order="Siluriformes" pageId="2" pageNumber="23" phylum="Chordata" rank="species" species="asurini">Pseudancistrus asurini</taxonomicName>
