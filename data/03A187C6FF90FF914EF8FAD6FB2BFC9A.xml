<taxonomicName id="4C084D53FF93FF914C19FD6CFD21FD72" baseAuthorityName="McAlpine" baseAuthorityYear="1981" box="[630,718,712,738]" kingdom="Plantae" order="Diptera" pageId="23" pageNumber="24" rank="order">Diptera</taxonomicName>
