<taxonomicName id="4C084D53FF93FF914C7FFD54FD87FC9A" baseAuthorityName="McAlpine" baseAuthorityYear="1981" box="[528,616,752,778]" kingdom="Plantae" order="Diptera" pageId="23" pageNumber="24" rank="order">Diptera</taxonomicName>
