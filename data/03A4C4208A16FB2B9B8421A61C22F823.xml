<materialsCitation id="3B657F6B8A16FB2B9ADE266F1C36F823" ID-GBIF-Occurrence="3026613330" collectingDate="2020-11-12" country="Mexico" county="Toltec" location="Saussure" municipality="Mesoamerican" pageId="103" pageNumber="104" specimenCount="1" stateProvince="Hidalgo">
