vel 
<taxonomicName id="4C0D0EB58A16FB2B9E74202F1E1EFE92" class="Insecta" family="Acridiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="family">Acridiidae</taxonomicName>
