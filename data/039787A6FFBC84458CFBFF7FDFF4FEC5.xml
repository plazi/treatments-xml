<taxonomicName id="4C3E4D33FFBC84458EA3FEABDFF8FEC5" authority="Needham &amp; Murphy 1924" authorityName="Needham &amp; Murphy" authorityYear="1924" box="[719,1222,333,359]" class="Insecta" family="Leptophlebiidae" genus="Thraulodes" kingdom="Animalia" order="Ephemeroptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="telegraphicus">
