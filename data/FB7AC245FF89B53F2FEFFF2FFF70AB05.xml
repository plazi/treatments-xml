<taxonomicName id="B4D308D0FF88B53F2D26FDF0FCE1AB27" authorityName="Talwar" authorityYear="1973" box="[606,862,584,606]" class="Actinopterygii" family="Cepolidae" genus="Sphenanthias" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="67" phylum="Chordata" rank="species" species="whiteheadi">
<emphasis id="41A7AF41FF88B53F2D26FDF0FCE1AB27" box="[606,862,584,606]" italics="true" pageId="3" pageNumber="67">Sphenanthias whiteheadi</emphasis>
