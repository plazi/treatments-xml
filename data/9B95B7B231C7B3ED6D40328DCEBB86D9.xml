<subSubSection id="3C2399522604AB4B2EEB986E277B98AF" pageId="20" pageNumber="21" type="reference_group">
<paragraph id="38BA962C585442587534FD59A95F3D56" pageId="20" pageNumber="21">
<taxonomicName id="4EE48C139DF82DA5DF9E1A64BB7D63C1" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina dofleini" order="Stylommatophora" pageId="20" pageNumber="21" phylum="Mollusca" rank="subSpecies" species="dofleini" subSpecies="sinosi">Montenegrina dofleini sinosi</taxonomicName>
<normalizedToken id="58F0E17D378EFCEFD5E035FF74AB218B" originalValue="Páll-Gergely">Pall-Gergely</normalizedToken>
