<figureCitation id="89AD2A220371FFD2FD14CBCAFD07EDA4" box="[640,749,1468,1494]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-2="FIGURES 3 – 7" captionStartId-0="2.[264,367,1756,1780]" captionStartId-1="3.[264,367,1768,1792]" captionStartId-2="4.[264,382,1787,1811]" captionTargetBox-1="[297,1174,317,1768]" captionTargetBox-2="[277,1316,1019,1743]" captionTargetId-1="figure@3.[295,1319,299,1768]" captionTargetId-2="figure@4.[267,1316,1009,1750]" captionTargetPageId-1="3" captionTargetPageId-2="4" captionText-0="FIGURE 1: Antennoseius orientalis sp. n., female. 1. Dorsal view." captionText-1="FIGURE 2: Antennoseius orientalis sp. n., female. 2. Ventral view." captionText-2="FIGURES 3 – 7: Antennoseius orientalis sp. n., female. 3. Tectum, 4. Chelicera, 5. Tarsus I, 6. leg II (partial view), 7. Leg I (partial view)." httpUri-1="https://zenodo.org/record/156355/files/figure.png" httpUri-2="https://zenodo.org/record/156356/files/figure.png" pageId="1" pageNumber="2">
