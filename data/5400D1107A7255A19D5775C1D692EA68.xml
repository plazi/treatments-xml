</taxonomicName>
, 
<taxonomicName id="8E1FA52800C2295E40607A1A79DEBA19" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
