<collectingRegion box="[1003,1199,1040,1067]" country="China" name="Yunnan" pageId="9" pageNumber="520">Yunnan Province</collectingRegion>
of 
<collectingCountry box="[1244,1311,1040,1066]" name="China" pageId="9" pageNumber="520">China</collectingCountry>
(
<geoCoordinate box="[1331,1431,1040,1066]" degrees="27" direction="north" minutes="00" orientation="latitude" pageId="9" pageNumber="520" precision="925" value="27.0">27°00’N</geoCoordinate>
; 
<geoCoordinate box="[151,262,1076,1102]" degrees="100" direction="east" minutes="12" orientation="longitude" pageId="9" pageNumber="520" precision="925" value="100.2">100°12’E</geoCoordinate>
).
</paragraph>
</subSubSection>
