<collectingCountry box="[1106,1176,1554,1578]" name="China" pageId="4" pageNumber="5">China</collectingCountry>
, 
<collectingCountry box="[1192,1277,1554,1578]" name="Taiwan" pageId="4" pageNumber="5">Taiwan</collectingCountry>
, 
<collectingCountry box="[1293,1432,1555,1579]" name="Hong Kong" pageId="4" pageNumber="5">Hong Kong</collectingCountry>
, Raos, 
<collectingCountry box="[221,323,1590,1614]" name="Thailand" pageId="4" pageNumber="5">Thailand</collectingCountry>
, 
<collectingCountry box="[334,432,1590,1614]" name="Vietnam" pageId="4" pageNumber="5">Vietnam</collectingCountry>
