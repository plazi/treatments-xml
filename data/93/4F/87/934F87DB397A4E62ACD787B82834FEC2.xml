<number box="[1054,1197,1473,1497]" pageId="12" pageNumber="47" value="0.7929999999999999" valueMax="0.836" valueMin="0.75">0.750–0.836</number>
, SL 
<number box="[1248,1391,1473,1497]" pageId="12" pageNumber="47" value="0.3725" valueMax="0.401" valueMin="0.344">0.344–0.401</number>
, EL 
<number box="[151,292,1509,1533]" pageId="12" pageNumber="47" value="0.38749999999999996" valueMax="0.411" valueMin="0.364">0.364–0.411</number>
, ML 
<number box="[353,496,1509,1533]" pageId="12" pageNumber="47" value="1.4655" valueMax="1.553" valueMin="1.378">1.378–1.553</number>
