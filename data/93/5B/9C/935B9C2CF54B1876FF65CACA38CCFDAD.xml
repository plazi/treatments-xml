<taxonomicName baseAuthorityName="Reiche" baseAuthorityYear="1862" box="[602,798,540,561]" class="Insecta" family="Glaphyridae" genus="Eulasia" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="1508" phylum="Arthropoda" rank="species" species="nitidicollis">
