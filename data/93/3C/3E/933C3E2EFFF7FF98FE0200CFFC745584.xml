<taxonomicName authority="Pilsbry, 1918" authorityName="Pilsbry" authorityYear="1918" box="[529,943,1195,1219]" class="Gastropoda" family="Gastrocoptidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="18" pageNumber="1749" phylum="Mollusca" rank="family">
<emphasis bold="true" box="[529,787,1195,1219]" pageId="18" pageNumber="1749">GASTROCOPTIDAE</emphasis>
