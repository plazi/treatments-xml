<taxonomicName authorityName="Hubbs" authorityYear="1936" box="[468,757,490,514]" class="Actinopterygii" family="Cyprinodontidae" genus="Floridichthys" kingdom="Animalia" order="Cyprinodontiformes" pageId="32" pageNumber="1763" phylum="Chordata" rank="species" species="polyommus">
