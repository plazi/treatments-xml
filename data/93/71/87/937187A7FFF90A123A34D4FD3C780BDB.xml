</taxonomicName>
from 
<collectingRegion box="[93,157,921,944]" country="United States of America" name="Texas" pageId="9" pageNumber="1740">Texas</collectingRegion>
, 
<collectingCountry box="[183,240,920,944]" name="United States of America" pageId="9" pageNumber="1740">USA</collectingCountry>
(
<bibRefCitation author="LUMSDEN, R. D." box="[269,453,920,944]" pageId="9" pageNumber="1740" pagination="293" refId="ref20344" refString="LUMSDEN, R. D., 1963, A new heterophyid trematode of the Ascocotyle complex of species encysted in poeciliid and cyprinodont ® shes of southeast Texas, Proceedings of the Helminthological Society of Washington, 30, 293 ± 296." type="journal article" year="1963">Lumsden, 1963</bibRefCitation>
