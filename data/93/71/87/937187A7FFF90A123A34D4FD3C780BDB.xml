<taxonomicName authorityName="Miller" authorityYear="1955" box="[429,643,577,601]" class="Actinopterygii" family="Fundulidae" genus="Fundulus" kingdom="Animalia" order="Cyprinodontiformes" pageId="9" pageNumber="1740" phylum="Chordata" rank="species" species="persimilis">
<emphasis box="[429,643,577,601]" italics="true" pageId="9" pageNumber="1740">Fundulus persimilis</emphasis>
</taxonomicName>
(
<taxonomicName box="[659,856,577,601]" class="Actinopterygii" family="Cyprinodontidae" kingdom="Animalia" order="Cyprinodontiformes" pageId="9" pageNumber="1740" phylum="Chordata" rank="family">Cyprinodontidae</taxonomicName>
