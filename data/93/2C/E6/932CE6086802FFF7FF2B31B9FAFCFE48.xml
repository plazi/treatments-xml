</taxonomicName>
and 
<taxonomicName box="[954,1127,223,249]" class="Magnoliopsida" family="Lauraceae" genus="Litsea" kingdom="Plantae" order="Laurales" pageId="9" pageNumber="163" phylum="Tracheophyta" rank="species" species="elongata">
