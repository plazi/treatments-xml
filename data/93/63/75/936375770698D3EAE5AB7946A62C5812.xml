3*. 
<normalizedToken originalValue="Kelchblätter" pageId="null" pageNumber="825">Kelchblaetter</normalizedToken>
lanzettlich; 
<normalizedToken originalValue="Blütenstiele" pageId="null" pageNumber="825">Bluetenstiele</normalizedToken>
