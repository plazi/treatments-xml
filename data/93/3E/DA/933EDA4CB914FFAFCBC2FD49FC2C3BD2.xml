<typeStatus box="[608,656,962,986]" pageId="50" pageNumber="51">type</typeStatus>
locality (central 
<collectingCountry box="[846,951,961,985]" name="Thailand" pageId="50" pageNumber="51">Thailand</collectingCountry>
).
</paragraph>
