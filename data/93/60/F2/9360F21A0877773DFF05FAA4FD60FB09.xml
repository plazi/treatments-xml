, farmland, 
<geoCoordinate box="[975,1144,886,912]" degrees="0" direction="south" minutes="51" orientation="latitude" pageId="22" pageNumber="23" precision="1" seconds="39.89" value="-0.8610806">0°51’39.89”S</geoCoordinate>
, 
