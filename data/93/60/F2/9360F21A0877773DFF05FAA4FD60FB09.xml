. 
<collectingRegion box="[728,874,848,874]" country="Ecuador" name="Galapagos" pageId="22" pageNumber="23">GALAPAGOS</collectingRegion>
: 
<emphasis bold="true" box="[893,1352,848,874]" pageId="22" pageNumber="23">
<collectingCounty box="[893,1352,848,874]" pageId="22" pageNumber="23">Isabela Island, Volcán Sierra Negra</collectingCounty>
</emphasis>
