<paragraph blockId="14.[151,787,1302,1328]" box="[151,787,1302,1328]" pageId="14" pageNumber="15">
<heading box="[151,787,1302,1328]" fontSize="11" level="2" pageId="14" pageNumber="15" reason="3">
<treatmentCitationGroup box="[151,787,1302,1328]" pageId="14" pageNumber="15">
