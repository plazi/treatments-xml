(
<taxonomicName box="[442,588,1970,1995]" class="Magnoliopsida" family="Sapindaceae" kingdom="Plantae" order="Sapindales" pageId="16" pageNumber="517" phylum="Tracheophyta" rank="family">Sapindaceae</taxonomicName>
