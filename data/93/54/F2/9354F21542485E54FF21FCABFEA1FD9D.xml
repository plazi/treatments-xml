<taxonomicName box="[910,1146,223,248]" class="Magnoliopsida" family="Salicaceae" genus="Casearia" kingdom="Plantae" order="Malpighiales" pageId="14" pageNumber="515" phylum="Tracheophyta" rank="species" species="corymbosa">
<emphasis box="[910,1146,223,248]" italics="true" pageId="14" pageNumber="515">Casearia corymbosa</emphasis>
</taxonomicName>
(
<taxonomicName box="[1163,1333,223,248]" class="Magnoliopsida" family="Flacourtiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="14" pageNumber="515" phylum="Magnoliophyta" rank="family">Flacourtiaceae</taxonomicName>
); 
<taxonomicName class="Insecta" family="Limacodidae" genus="Parasa" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="wellesca">
<emphasis italics="true" pageId="14" pageNumber="515">Parasa wellesca</emphasis>
</taxonomicName>
(
<taxonomicName box="[261,411,260,285]" class="Insecta" family="Limacodidae" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="family">Limacodidae</taxonomicName>
) feeding on 
<taxonomicName box="[550,752,260,285]" class="Magnoliopsida" family="Annonaceae" genus="Annona" kingdom="Plantae" order="Magnoliales" pageId="14" pageNumber="515" phylum="Tracheophyta" rank="species" species="reticulata">
<emphasis box="[550,752,260,285]" italics="true" pageId="14" pageNumber="515">Annona reticulata</emphasis>
</taxonomicName>
(
<taxonomicName box="[766,911,260,284]" class="Magnoliopsida" family="Annonaceae" kingdom="Plantae" order="Magnoliales" pageId="14" pageNumber="515" phylum="Tracheophyta" rank="family">Annonaceae</taxonomicName>
); 
<taxonomicName box="[930,1140,260,285]" class="Insecta" family="Limacodidae" genus="Vipsania" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="rosabella">
<emphasis box="[930,1140,260,285]" italics="true" pageId="14" pageNumber="515">Vipsania rosabella</emphasis>
</taxonomicName>
(
<taxonomicName box="[1153,1303,260,285]" class="Insecta" family="Limacodidae" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="family">Limacodidae</taxonomicName>
) feeding on 
<taxonomicName box="[151,370,296,320]" class="Magnoliopsida" family="Combretaceae" genus="Conocarpus" kingdom="Plantae" order="Myrtales" pageId="14" pageNumber="515" phylum="Tracheophyta" rank="species" species="erecta">
<emphasis box="[151,370,296,320]" italics="true" pageId="14" pageNumber="515">Conocarpus erecta</emphasis>
</taxonomicName>
(
<taxonomicName box="[390,557,295,320]" class="Magnoliopsida" family="Combretaceae" kingdom="Plantae" order="Myrtales" pageId="14" pageNumber="515" phylum="Tracheophyta" rank="family">Combretaceae</taxonomicName>
); and male reared from limacodid larva feeding on 
<taxonomicName box="[1186,1435,297,320]" class="Magnoliopsida" family="Rubiaceae" genus="Exostema" kingdom="Plantae" order="Gentianales" pageId="14" pageNumber="515" phylum="Tracheophyta" rank="species" species="mexicanum">
<emphasis box="[1186,1435,297,320]" italics="true" pageId="14" pageNumber="515">Exostema mexicanum</emphasis>
</taxonomicName>
