, 12 (10–13), 
<emphasis box="[294,316,1339,1362]" italics="true" pageId="21" pageNumber="62">
j 
<date box="[304,316,1339,1362]" pageId="21" pageNumber="62" value="2010-05-10">5</date>
</emphasis>
10, 
<emphasis bold="true" box="[370,398,1338,1363]" pageId="21" pageNumber="62">10</emphasis>
(9–11), 
<emphasis box="[498,520,1338,1363]" italics="true" pageId="21" pageNumber="62">
j 
<date box="[508,520,1338,1363]" pageId="21" pageNumber="62" value="1930-06-31">6</date>
</emphasis>
<emphasis bold="true" box="[530,558,1338,1363]" pageId="21" pageNumber="62">31</emphasis>
, 30 (30–31), 
<emphasis box="[717,744,1339,1362]" italics="true" pageId="21" pageNumber="62">J5</emphasis>
<emphasis bold="true" box="[754,768,1338,1363]" pageId="21" pageNumber="62">4</emphasis>
, 4 (4–5), 
<emphasis box="[885,910,1339,1363]" italics="true" pageId="21" pageNumber="62">
z 
<date box="[897,910,1339,1363]" pageId="21" pageNumber="62" value="1928-02-29">2</date>
</emphasis>
<emphasis bold="true" box="[920,948,1338,1363]" pageId="21" pageNumber="62">29</emphasis>
, 28 (25–31), 
<emphasis box="[1108,1133,1339,1363]" italics="true" pageId="21" pageNumber="62">
z 
<date box="[1119,1133,1339,1363]" pageId="21" pageNumber="62" value="1930-04-31">4</date>
</emphasis>
<emphasis bold="true" box="[1142,1170,1338,1363]" pageId="21" pageNumber="62">31</emphasis>
, 30 (28–32), 
<emphasis box="[1330,1355,1339,1362]" italics="true" pageId="21" pageNumber="62">
z 
<date box="[1341,1355,1339,1362]" pageId="21" pageNumber="62" value="2012-05-11">5</date>
</emphasis>
<emphasis bold="true" box="[1365,1390,1338,1363]" pageId="21" pageNumber="62">11</emphasis>
, 12 (11–13), 
<emphasis box="[252,282,1375,1399]" italics="true" pageId="21" pageNumber="62">
Z 
<date box="[271,282,1375,1399]" pageId="21" pageNumber="62" value="1929-01-29">1</date>
</emphasis>
<emphasis bold="true" box="[290,318,1374,1399]" pageId="21" pageNumber="62">29</emphasis>
, 29 (28–30), 
<emphasis box="[470,500,1375,1399]" italics="true" pageId="21" pageNumber="62">Z4</emphasis>
<emphasis bold="true" box="[507,535,1374,1399]" pageId="21" pageNumber="62">42</emphasis>
, 41 (40–42), 
<emphasis box="[687,717,1375,1398]" italics="true" pageId="21" pageNumber="62">Z5</emphasis>
<emphasis bold="true" box="[724,752,1374,1399]" pageId="21" pageNumber="62">44</emphasis>
, 45 (43–46), 
<geoCoordinate box="[905,965,1374,1399]" direction="south" orientation="latitude" pageId="21" pageNumber="62" precision="925" value="-4.5833335">
<emphasis box="[905,930,1375,1399]" italics="true" pageId="21" pageNumber="62">s4</emphasis>
