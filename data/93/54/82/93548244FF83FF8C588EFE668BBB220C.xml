<emphasis box="[966,1009,1868,1892]" italics="true" pageId="17" pageNumber="58">JV1</emphasis>
, 
<emphasis box="[1025,1069,1868,1892]" italics="true" pageId="17" pageNumber="58">JV2</emphasis>
, and 
<emphasis box="[1132,1179,1868,1892]" italics="true" pageId="17" pageNumber="58">ZV2</emphasis>
