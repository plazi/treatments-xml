<paragraph blockId="6.[151,1437,1361,2034]" pageId="6" pageNumber="47">
<emphasis bold="true" box="[151,238,1361,1386]" pageId="6" pageNumber="47">Female</emphasis>
(n = 10). 
<emphasis box="[377,467,1362,1385]" italics="true" pageId="6" pageNumber="47">Dorsum</emphasis>
