<emphasis box="[920,948,1506,1530]" italics="true" pageId="6" pageNumber="47">S4</emphasis>
10
</geoCoordinate>
(9–11), 
<geoCoordinate box="[1076,1124,1506,1530]" direction="south" orientation="latitude" pageId="6" pageNumber="47" precision="925" value="-5.133333">
<emphasis box="[1076,1104,1506,1529]" italics="true" pageId="6" pageNumber="47">S5</emphasis>
8
</geoCoordinate>
