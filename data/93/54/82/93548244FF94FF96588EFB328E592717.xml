<emphasis box="[386,408,1469,1494]" italics="true" pageId="6" pageNumber="47">j6</emphasis>
10 (10–11), 
<emphasis box="[557,584,1470,1494]" italics="true" pageId="6" pageNumber="47">J2</emphasis>
12 (10–13), 
<emphasis box="[744,771,1470,1493]" italics="true" pageId="6" pageNumber="47">J5</emphasis>
8 (7–9), 
<emphasis box="[888,913,1470,1494]" italics="true" pageId="6" pageNumber="47">z2</emphasis>
11 (10–12), 
<emphasis box="[1063,1088,1470,1494]" italics="true" pageId="6" pageNumber="47">z4</emphasis>
11 (11–12), 
<emphasis box="[1236,1262,1470,1493]" italics="true" pageId="6" pageNumber="47">z5</emphasis>
8 (7–9), 
<emphasis box="[1370,1400,1470,1494]" italics="true" pageId="6" pageNumber="47">Z1</emphasis>
11 (9–13), 
<emphasis box="[239,269,1506,1530]" italics="true" pageId="6" pageNumber="47">Z4</emphasis>
25 (25–26), 
<emphasis box="[411,441,1506,1529]" italics="true" pageId="6" pageNumber="47">Z5</emphasis>
69 (68–70), 
<geoCoordinate box="[584,643,1505,1530]" direction="south" orientation="latitude" pageId="6" pageNumber="47" precision="925" value="-4.266667">
