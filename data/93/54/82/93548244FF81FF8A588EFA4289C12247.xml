<taxonomicName box="[391,517,692,716]" class="Magnoliopsida" family="Asteraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="21" pageNumber="62" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
) at Kakdwip: 21° 87' 60&quot; N, 88° 18' 53&quot; E, 
<quantity box="[1023,1068,692,716]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="21" pageNumber="62" unit="m" value="3.0">3 m</quantity>
