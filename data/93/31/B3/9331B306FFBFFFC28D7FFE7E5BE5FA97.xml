<taxonomicName authority="(Quate, 1965)" authorityName="Ježek &amp; Harten" authorityYear="2005" baseAuthorityName="Quate" baseAuthorityYear="1965" box="[710,997,1193,1217]" class="Insecta" family="Psychodidae" genus="Psychana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="207" phylum="Arthropoda" rank="species" species="pellucida" status="comb. nov.">
<emphasis box="[710,838,1193,1217]" italics="true" pageId="8" pageNumber="207">P. pellucida</emphasis>
(
<bibRefCitation author="QUATE L. W." box="[855,990,1193,1217]" pageId="8" pageNumber="207" pagination="815 - 902" refId="ref10464" refString="QUATE L. W. 1965: A taxonomic study of Philippine Psychodidae (Diptera). Pacific Insects 7: 815 - 902." type="journal article" year="1965">Quate, 1965</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel box="[1007,1127,1193,1217]" pageId="8" pageNumber="207" rank="species">comb. nov.</taxonomicNameLabel>
(from 
<taxonomicName authorityName="Latreille" authorityYear="1794" box="[222,325,1225,1249]" class="Insecta" family="Psychodidae" genus="Psychoda" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="207" phylum="Arthropoda" rank="genus">
