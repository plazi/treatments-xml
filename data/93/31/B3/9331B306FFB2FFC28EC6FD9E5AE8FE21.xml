<collectingCountry box="[504,571,319,343]" name="Guam" pageId="8" pageNumber="207">Guam</collectingCountry>
, the 
<collectingCountry box="[620,742,319,343]" name="Philippines" pageId="8" pageNumber="207">Philippines</collectingCountry>
