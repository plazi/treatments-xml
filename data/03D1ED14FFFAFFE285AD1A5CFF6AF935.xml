<taxonomicName id="4C782781FFFAFFE6860C1AD3FB97F928" authority="Expedition" authorityName="Expedition" box="[783,1033,1708,1730]" class="Arachnida" family="Oonopidae" genus="Niarchos" kingdom="Animalia" order="Araneae" pageId="98" pageNumber="98" phylum="Arthropoda" rank="genus">Niarchos Expedition</taxonomicName>
