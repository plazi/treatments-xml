<treatmentCitationGroup id="AB02F058FFA4FF88FF0FFD8EC5BCFD60" pageId="8" pageNumber="46">
<treatmentCitation id="0AB3F167FFA4FF88FF0FFD8EC51BFD83" author="Franz, H." box="[199,781,598,622]" page="284" pageId="8" pageNumber="46" year="1975">
<taxonomicName id="4C12ACF5FFA4FF88FF0FFD8EC51BFD83" authority="Franz, 1975 b: 284" authorityName="Franz" authorityPageNumber="284" authorityYear="1975" box="[199,781,598,622]" class="Insecta" family="Staphylinidae" genus="Scydmaenus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="46" phylum="Arthropoda" rank="species" species="reunionis" subGenus="Mascarensia">
<emphasis id="B9660B64FFA4FF88FF0FFD8EC752FD83" box="[199,324,598,622]" italics="true" pageId="8" pageNumber="46">Scydmaenus</emphasis>
