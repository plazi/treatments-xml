x
</date>
<quantity id="4CB79B3DEA14FFF7FB6BFD854D12FDC3" box="[1271,1337,547,571]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="2" pageNumber="571" unit="mm" value="4.0">4 mm</quantity>
