<geoCoordinate id="EE74D5E42773FF918B51EE59FC00FB87" box="[818,967,1048,1075]" degrees="31" direction="south" minutes="52" orientation="latitude" pageId="28" pageNumber="29" precision="15" seconds="37" value="-31.876945">31º 52’ 37”S</geoCoordinate>
<geoCoordinate id="EE74D5E42773FF918BADEE59FBB2FB87" box="[974,1141,1048,1075]" degrees="115" direction="east" minutes="47" orientation="longitude" pageId="28" pageNumber="29" precision="15" seconds="18" value="115.78833">115º 47’ 18”E</geoCoordinate>
