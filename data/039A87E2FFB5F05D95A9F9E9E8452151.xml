<geoCoordinate id="EE075033FFB5F05D9154F94BE9E020B1" box="[1085,1233,1674,1699]" degrees="23" direction="south" minutes="48" orientation="latitude" pageId="3" pageNumber="2" precision="1" seconds="43.0" value="-23.811943">
S23 
<superScript id="7C469BBCFFB5F05D9105F94AE945208A" attach="none" box="[1132,1140,1674,1688]" fontSize="6" pageId="3" pageNumber="2">o</superScript>
