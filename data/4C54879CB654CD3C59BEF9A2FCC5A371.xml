<taxonomicName id="03FD4D09B654CD3C5A42F911FC28A34A" authorityName="Distant" authorityYear="1905" box="[875,989,1777,1803]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="subTribe" subTribe="Fidicinina">Fidicinina</taxonomicName>
