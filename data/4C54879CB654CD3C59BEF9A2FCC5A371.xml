<taxonomicName id="03FD4D09B654CD3C5DDCF911FA9FA34A" authorityName="Boulard &amp; Martinelli" authorityYear="1996" box="[1269,1386,1777,1803]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="subTribe" subTribe="Guyalnina">Guyalnina</taxonomicName>
