<taxonomicName id="A5EB856201012D9F2F377D80BE0EA051" authority="Mann" authorityName="Mann" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" phylum="Arthropoda" rank="subSpecies" species="cato" subSpecies="isolatus">
