<emphasis id="23189EC20C34123E9AFFB41484CF4B14" italics="true" pageId="0" pageNumber="79">Excentrodendron hsiemmu</emphasis>
</taxonomicName>
, 
<taxonomicName id="9ACF70B4BBBBB2360C5D1F09627A0C92" authorityName="D. Don" authorityYear="1823" class="Dicotyledoneae" family="Melastomataceae" genus="Melastoma" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Melastoma candidum" order="Myrtiflorae" pageId="0" pageNumber="79" phylum="Angiospermae" rank="species" species="candidum">
