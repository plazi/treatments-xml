<taxonomicName id="26F14D1AE176F56DFE345A91FDB1FB5C" authorityName="Mendes &amp; Rafael" authorityYear="2021" box="[479,546,1203,1228]" class="Insecta" family="Tettigoniidae" genus="Tukunha" kingdom="Animalia" order="Orthoptera" pageId="13" pageNumber="566" phylum="Arthropoda" rank="species" species="minor">
<emphasis id="D385EA8BE176F56DFE345A91FDB1FB5C" box="[479,546,1203,1228]" italics="true" pageId="13" pageNumber="566">minor</emphasis>
</taxonomicName>
