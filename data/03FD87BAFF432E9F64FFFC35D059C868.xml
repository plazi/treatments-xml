<taxonomicName id="4C544D2FFF582E9F60A1FE46D725C82F" authorityName="sensu Sars" authorityYear="1887" box="[1225,1386,511,536]" class="Malacostraca" family="Lampropidae" genus="Paralamprops" kingdom="Animalia" order="Cumacea" pageId="180" pageNumber="181" phylum="Arthropoda" rank="genus">
<emphasis id="B920EABEFF582E9F60A1FE46D725C82F" box="[1225,1386,511,536]" italics="true" pageId="180" pageNumber="181">Paralamprops</emphasis>
