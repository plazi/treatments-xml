<taxonomicName id="C6863C90FFD8E10D200DEAB6FDF866D6" authorityName="Risbec" authorityYear="1951" box="[383,528,662,688]" class="Insecta" family="Eupelmidae" genus="Anastatus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="464" phylum="Arthropoda" rank="species" species="rhynchitidi">
