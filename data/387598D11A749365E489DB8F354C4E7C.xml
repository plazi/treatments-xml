(
<bibRefCitation id="28EAB5926DE240B1240EDD330A7736BE" author="Yamaguti, S" journalOrPublisher="University of Hawaii, Honolulu" pageId="19" pageNumber="20" title="Monogenetic trematodes of Hawaiian fishes." url="https://doi.org/10.2307/3277429" year="1968">Yamaguti 1968</bibRefCitation>
