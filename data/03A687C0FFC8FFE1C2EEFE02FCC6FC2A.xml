<quantity id="4CF79B33FFC8FFE1C313FDF1FE3DFDD0" box="[362,427,552,572]" metricMagnitude="3" metricUnit="m" metricValue="2.15" pageId="39" pageNumber="194" unit="m" value="2150.0">2150m</quantity>
