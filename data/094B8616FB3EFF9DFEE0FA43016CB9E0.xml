</emphasis>
<taxonomicName id="46E24C83FB3EFF9EFE5CF89103FEBD00" authority="A. Milne-Edwards, 1880" authorityName="A. Milne-Edwards" authorityYear="1880" box="[436,996,1828,1854]" class="Malacostraca" family="Paguridae" genus="Catapagurus" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="sharreri">
