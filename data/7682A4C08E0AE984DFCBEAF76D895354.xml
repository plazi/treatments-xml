<taxonomicName id="35C4CD4CD9B64E1CC1073E2B38C7E047" LSID="urn:lsid:zoobank.org:act:8F89524D-85C5-4332-86AC-D278AB724C1D" class="Insecta" family="Therevidae" genus="Acupalpa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acupalpa boharti" order="Diptera" pageId="9" pageNumber="38" phylum="Arthropoda" rank="species" species="boharti">Acupalpa boharti</taxonomicName>
