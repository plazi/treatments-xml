<taxonomicName id="A2B90EBDFFF7230DFF710B57FE54F99E" box="[182,384,1575,1601]" class="Hydrozoa" family="Campanulariidae" genus="Campanularia" kingdom="Animalia" order="Leptothecata" pageId="5" pageNumber="326" phylum="Cnidaria" rank="species" species="undetermined">
<emphasis id="57CDA92CFFF7230DFF710B57FE8DF99E" box="[182,345,1575,1601]" italics="true" pageId="5" pageNumber="326">Campanularia</emphasis>
