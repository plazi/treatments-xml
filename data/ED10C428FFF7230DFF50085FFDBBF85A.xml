<quantity id="A241D8DBFFF7230DFC160B73FBF7F9C2" box="[977,1059,1539,1565]" metricMagnitude="-2" metricUnit="m" metricValue="9.0" pageId="5" pageNumber="326" unit="mm" value="90.0">90 mm</quantity>
