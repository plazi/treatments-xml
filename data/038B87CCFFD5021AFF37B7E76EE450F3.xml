<quantity id="4CDA9B3FFFD2021AFF64B0836C6B52EB" box="[196,361,1206,1233]" metricMagnitude="-4" metricUnit="m" metricValue="8.75" metricValueMax="9.5" metricValueMin="8.0" pageId="4" pageNumber="278" unit="mm" value="0.875" valueMax="0.95" valueMin="0.8">0.80–0.95 mm</quantity>
(mean 
<quantity id="4CDA9B3FFFD2021AFE62B0836F2F52EB" box="[450,557,1206,1232]" metricMagnitude="-4" metricUnit="m" metricValue="8.6" pageId="4" pageNumber="278" unit="mm" value="0.86">0.86 mm</quantity>
); HL 
<quantity id="4CDA9B3FFFD2021AFDD1B0836E1452EB" box="[625,790,1206,1233]" metricMagnitude="-4" metricUnit="m" metricValue="1.65" metricValueMax="1.7999999999999998" metricValueMin="1.5" pageId="4" pageNumber="278" unit="mm" value="0.16499999999999998" valueMax="0.18" valueMin="0.15">0.15–0.18 mm</quantity>
(mean 
<quantity id="4CDA9B3FFFD2021AFCCFB0836ED852EB" box="[879,986,1206,1232]" metricMagnitude="-4" metricUnit="m" metricValue="1.6" pageId="4" pageNumber="278" unit="mm" value="0.16">0.16 mm</quantity>
), HW 
<quantity id="4CDA9B3FFFD2021AFB87B08369CF52EB" box="[1063,1229,1206,1232]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" metricValueMax="1.6" metricValueMin="1.4" pageId="4" pageNumber="278" unit="mm" value="0.15" valueMax="0.16" valueMin="0.14">0.14–0.16 mm</quantity>
(mean 
<quantity id="4CDA9B3FFFD2021AFA86B083689252EB" box="[1318,1424,1206,1233]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="4" pageNumber="278" unit="mm" value="0.15">0.15 mm</quantity>
), AnL 
