<taxonomicName id="F8E6CB678E18CF399D4E1D5974BA0A8D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181711" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole dolon Wilson" lsidName-HNS="Pheidole dolon Wilson" order="Hymenoptera" pageNumber="254" phylum="Arthropoda" rank="species" species="dolon">Pheidole dolon</taxonomicName>
<taxonomicNameLabel id="113E879902D1565E9D03EC6D832B53CE" rank="species">new species</taxonomicNameLabel>
</paragraph>
