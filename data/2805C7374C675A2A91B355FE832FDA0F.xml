<paragraph id="956EC117BAFB858531BA9FA15D5D674E" pageId="0" pageNumber="17">
<taxonomicName id="D94F4A8F3AE7A5A71F6313C1C545908B" authorityName="Simon" authorityYear="1889" class="Arachnida" family="Hypochilidae" genus="Hypochilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypochilus davidi" order="Araneae" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="davidi">
