<caption id="D39866257B3DFFB1FF22F837A47FF814" httpUri="https://zenodo.org/record/251556/files/figure.png" pageId="12" pageNumber="251" targetBox="[184,1402,719,1924]" targetPageId="12">
<paragraph id="875836AD7B3DFFB1FF22F837A47FF814" blockId="12.[151,1435,1946,2032]" pageId="12" pageNumber="251">
