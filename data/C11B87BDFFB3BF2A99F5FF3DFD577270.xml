<paragraph id="490D36ABFFB3BF2A99CEFC67FD577270" blockId="17.[159,771,867,1028]" pageId="17" pageNumber="429">
<taxonomicName id="8EB24D28FFB3BF2A99CEFC67FE0E75EB" authorityName="Horsfield" authorityYear="1823" box="[197,426,901,927]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="17" pageNumber="429" phylum="Chordata" rank="species" species="affinis">
