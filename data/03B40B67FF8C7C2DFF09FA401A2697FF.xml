<taxonomicName id="4C1DC1F2FF8C7C2DFE86F9F619B19686" box="[280,492,1651,1675]" class="Insecta" family="Ichneumonidae" genus="Temelucha" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pestifer">
