<subSubSection id="6FF1652CFF87FFCAFF2BFB0CFD4378C4" box="[151,743,1229,1252]" pageId="2" pageNumber="3" type="type_taxon">
<paragraph id="275436A7FF87FFCAFF2BFB0CFD4378C4" blockId="2.[151,743,1194,1252]" box="[151,743,1229,1252]" pageId="2" pageNumber="3">
<emphasis id="159FEAB5FF87FFCAFF2BFB0CFEB878C3" bold="true" box="[151,284,1229,1252]" pageId="2" pageNumber="3">
<typeStatus id="F8508805FF87FFCAFF2BFB0CFF6A78C4" box="[151,206,1230,1251]" pageId="2" pageNumber="3">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="E0EB4D24FF87FFCAFE96FB0FFD4778C4" authority="Banks, 1895, p. 8" authorityName="Banks" authorityPageNumber="8" authorityYear="1895" box="[298,739,1229,1251]" class="Arachnida" family="Oribatellidae" genus="Oribatella" kingdom="Animalia" order="Sarcoptiformes" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="quadridentata">
