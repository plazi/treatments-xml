<emphasis id="8596257F5556FFC9FF43FB4F9AD1FB5E" bold="true" box="[136,247,1200,1222]" pageId="4" pageNumber="158">FIGURE 2</emphasis>
. Distribution of 
<taxonomicName id="70E282EE5556FFC9FE5CFB4F9859FB5D" box="[407,639,1200,1222]" class="Magnoliopsida" family="Rubiaceae" genus="Rudgea" kingdom="Plantae" order="Gentianales" pageId="4" pageNumber="158" phylum="Tracheophyta" rank="species" species="approuaguensis">
<emphasis id="8596257F5556FFC9FE5CFB4F9859FB5D" box="[407,639,1200,1222]" italics="true" pageId="4" pageNumber="158">Rudgea approuaguensis</emphasis>
</taxonomicName>
(triangles), 
<taxonomicName id="70E282EE5556FFC9FD3FFB4E99BBFB5D" authorityName="Steyermark" authorityYear="1967" box="[756,925,1201,1222]" class="Magnoliopsida" family="Rubiaceae" genus="Rudgea" kingdom="Plantae" order="Gentianales" pageId="4" pageNumber="158" phylum="Tracheophyta" rank="species" species="ayangannensis">
<emphasis id="8596257F5556FFC9FD3FFB4E99BBFB5D" box="[756,925,1201,1222]" italics="true" pageId="4" pageNumber="158">R. ayangannensis</emphasis>
</taxonomicName>
(stars) and 
<taxonomicName id="70E282EE5556FFC9FBC5FB4E9EB5FB5E" box="[1038,1171,1200,1222]" class="Magnoliopsida" family="Rubiaceae" genus="Rudgea" kingdom="Plantae" order="Gentianales" pageId="4" pageNumber="158" phylum="Tracheophyta" rank="species" species="graniticola">
<emphasis id="8596257F5556FFC9FBC5FB4E9EB5FB5E" box="[1038,1171,1200,1222]" italics="true" pageId="4" pageNumber="158">R. graniticola</emphasis>
</taxonomicName>
