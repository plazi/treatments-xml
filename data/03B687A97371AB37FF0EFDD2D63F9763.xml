<number id="3DD724987371AB34FBE1FBBED3A39257" box="[1144,1214,1069,1095]" pageId="25" pageNumber="26" value="19.02">19.02</number>
. 
<number id="3DD724987371AB34FB5BFBBED3E09257" box="[1218,1277,1069,1095]" pageId="25" pageNumber="26" value="1991.0">1991</number>
</date>
