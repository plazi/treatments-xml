<normalizedToken id="44C2C4F7242CFB53CFB280F878A835A9" originalValue="Blätter" pageId="null" pageNumber="720">Blaetter</normalizedToken>
bis 16 je Quirl, sehr schmal und 
<normalizedToken id="1A0DC8DA4B08B0572D10A2B76A29F5D9" originalValue="grasähnlich" pageId="null" pageNumber="720">grasaehnlich</normalizedToken>
; 
