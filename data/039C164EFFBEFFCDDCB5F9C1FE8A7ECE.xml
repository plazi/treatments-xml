<subSubSection id="C32FF4D3FFBEFFB2DCB5F9C1FDAA7EF1" box="[135,560,1238,1261]" pageId="63" pageNumber="64" type="nomenclature">
<paragraph id="8B8AA758FFBEFFB2DCB5F9C1FDAA7EF1" blockId="63.[135,560,1238,1261]" box="[135,560,1238,1261]" pageId="63" pageNumber="64">
<heading id="D0C21034FFBEFFB2DCB5F9C1FDAA7EF1" box="[135,560,1238,1261]" centered="true" fontSize="9" level="2" pageId="63" pageNumber="64" reason="2">
<taxonomicName id="4C35DCDBFFBEFFB2DCB5F9C1FE037EF1" box="[135,409,1238,1261]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="jacksoni" status="new species">
<emphasis id="B9417B4AFFBEFFB2DCB5F9C1FE037EF1" bold="true" box="[135,409,1238,1261]" italics="true" pageId="63" pageNumber="64">Paramblynotus jacksoni</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFBEFFB2DD9BF9C0FDAA7EF1" box="[425,560,1239,1261]" pageId="63" pageNumber="64" rank="species">new species</taxonomicNameLabel>
