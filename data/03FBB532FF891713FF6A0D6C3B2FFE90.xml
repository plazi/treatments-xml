<taxonomicName id="4C527FA7FF88170EFD580E0A38D0FA86" authorityName="Huber" authorityYear="2012" box="[677,762,1373,1394]" class="Arachnida" family="Pholcidae" genus="Smeringopus" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="ruhiza">
<emphasis id="B926D836FF88170EFD580E0A38D0FA86" box="[677,762,1373,1394]" italics="true" pageId="13" pageNumber="14">S. ruhiza</emphasis>
