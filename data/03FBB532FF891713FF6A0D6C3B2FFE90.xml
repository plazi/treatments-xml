<taxonomicName id="4C527FA7FF88170EFEC20E0A3BB3FA86" authorityName="Huber" authorityYear="2012" box="[319,409,1373,1394]" class="Arachnida" family="Pholcidae" genus="Smeringopus" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="bwindi">
