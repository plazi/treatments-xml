<quantity id="4CAAA9C1FF8A170CFD620C0D38E7F886" box="[671,717,1882,1906]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="15" pageNumber="16" unit="in" value="1.0">1 in</quantity>
