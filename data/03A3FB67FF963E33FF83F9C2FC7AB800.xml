<taxonomicName id="4C0A31F2FF973E33FE19FEB4FDFCB803" authority="Schmidt 1848" authorityName="Schmidt" authorityYear="1848" box="[269,593,295,321]" class="Rhabditophora" family="Macrostomidae" genus="Macrostomum" kingdom="Animalia" order="Macrostomida" pageId="5" pageNumber="520" phylum="Platyhelminthes" rank="genus">
