<taxonomicName id="4C0A31F2FF973E33FC30FEB4FC66B800" authorityName="Karling" authorityYear="1940" box="[804,971,295,322]" class="Rhabditophora" kingdom="Animalia" order="Macrostomida" pageId="5" pageNumber="520" phylum="Platyhelminthes" rank="order">Macrostomida</taxonomicName>
