<taxonomicName id="EDF94D788D3CFFC500C2FB5BFBFFC5F1" authorityName="Moron" authorityYear="2013" box="[948,1055,1166,1184]" class="Insecta" family="Scarabaeidae" genus="Phyllophaga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="172" phylum="Arthropoda" rank="species" species="cazahuata">
