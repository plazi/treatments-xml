<emphasis id="B946EAD9FFDCFF8B7852F902FCC678B1" box="[868,894,1700,1724]" italics="true" pageId="2">
<date id="FF8C100BFFDCFF8B7852F902FCC678B1" box="[868,894,1700,1724]" pageId="2">
v 
<number id="3DFA24ECFFDCFF8B7846F902FCC678B1" box="[880,894,1700,1724]" pageId="2" value="4.0">4</number>
