<collectionCode id="7AF9AE45FFD6FFAB67E0FE16FE376AF1" box="[384,453,430,451]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34264" name="University of Kansas - Biodiversity Institute" pageId="15" pageNumber="113" type="Museum">SEMC</collectionCode>
