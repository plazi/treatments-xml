<taxonomicName id="05DAD9E800CE8B2C09D3DD23CBB1834B" class="Insecta" family="Leiodidae" genus="Ptomaphaginus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ptomaphaginus troglodytes" order="Coleoptera" pageId="2" pageNumber="137" phylum="Arthropoda" rank="species" species="troglodytes">Ptomaphaginus troglodytes</taxonomicName>
