<paragraph id="9C39464B10B271ADD56AFABD1F3A6645" pageId="0" pageNumber="1">Hosts.</paragraph>
<paragraph id="ED8DAFD594DBF62C14D8B5126D4DACD1" pageId="0" pageNumber="1">
<taxonomicName id="6D1C4F9905E3C49B31842CB468C18DEF" family="Amaranthaceae" genus="A." kingdom="Animalia" lsidName="" order="Diptera" pageId="0" pageNumber="1" rank="family">Amaranthaceae</taxonomicName>
