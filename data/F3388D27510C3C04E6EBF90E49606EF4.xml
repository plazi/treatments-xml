<quantity id="BC6991D4510C3C05E4DBF8C349616830" box="[679,779,1872,1897]" metricMagnitude="-4" metricUnit="m" metricValue="3.5999999999999996" pageId="32" pageNumber="183" unit="mm" value="0.36">0.36 mm</quantity>
