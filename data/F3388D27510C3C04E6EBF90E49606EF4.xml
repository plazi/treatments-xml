<quantity id="BC6991D4510C3C05E5D3F8E74E7968D4" box="[943,1043,1908,1933]" metricMagnitude="-4" metricUnit="m" metricValue="5.4" pageId="32" pageNumber="183" unit="mm" value="0.54">0.54 mm</quantity>
