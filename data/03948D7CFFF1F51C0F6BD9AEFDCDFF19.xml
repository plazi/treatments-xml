<paragraph id="8B823C6AFFF1F5200F6BD9AEFB14FA56" blockId="28.[810,1194,1503,1527]" box="[810,1194,1503,1527]" pageId="28" pageNumber="535">
<heading id="D0CA8B06FFF1F5200F6BD9AEFB14FA56" box="[810,1194,1503,1527]" fontSize="10" level="2" pageId="28" pageNumber="535" reason="2">
<taxonomicName id="4C3D47E9FFF1F5200F6BD9AEFB14FA56" authority="Nees ex Trin." authorityName="Trin." box="[810,1194,1503,1527]" class="Liliopsida" family="Poaceae" genus="Paspalum" kingdom="Plantae" order="Poales" pageId="28" pageNumber="535" phylum="Tracheophyta" rank="species" species="eucomum">
<emphasis id="B949E078FFF1F5200F6BD9AEFB14FA56" bold="true" box="[810,1194,1503,1527]" inLineHeading="true" pageId="28" pageNumber="535" reason="1">
<emphasis id="B949E078FFF1F5200F6BD9AEFBB3FA56" bold="true" box="[810,1037,1503,1527]" italics="true" pageId="28" pageNumber="535">Paspalum eucomum</emphasis>
