<taxonomicName id="49C44D65FF989005FBDD6020FB2FF916" box="[1047,1182,1658,1682]" class="Insecta" family="Phoridae" genus="Coniceromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="bilineata">
