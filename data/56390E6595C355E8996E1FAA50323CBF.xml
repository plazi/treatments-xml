<paragraph id="33072E2655DEB52CDECBD509BD5C3C0C" pageId="10" pageNumber="477">
<taxonomicName id="1F47AD175C2812D69DCA51B1A633829B" class="Aves" family="Muscicapidae" genus="Saxicola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Saxicola caprata subsp. pyrrhonota" order="Passeriformes" pageId="10" pageNumber="477" phylum="Chordata" rank="subSpecies" species="caprata" subSpecies="pyrrhonota">Saxicola caprata pyrrhonota</taxonomicName>
