<taxonomicName id="4C7F4D34BE372B6A29DBD77CFB1B7766" baseAuthorityName="Fabricius" baseAuthorityYear="1782" box="[1001,1184,596,620]" class="Insecta" family="Formicidae" genus="Eciton" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="2622" phylum="Arthropoda" rank="species" species="hamatum">
<emphasis id="B90BEAA5BE372B6A29DBD77CFB1B7766" box="[1001,1184,596,620]" italics="true" pageId="8" pageNumber="2622">Eciton hamatum</emphasis>
</taxonomicName>
colony EC #28. Total bivouac sampling. A. Tishechkin’/‘ 
<typeStatus id="54C48815BE372B6A296FD75CFC4E7786" box="[861,1013,628,652]" pageId="8" pageNumber="2622">PARATYPE</typeStatus>
