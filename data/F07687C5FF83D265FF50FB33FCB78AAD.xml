<collectingRegion id="BA1BF831FF83D265FCDAFA83FC048935" box="[797,949,1377,1403]" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="28" pageNumber="329">Buenos Aires</collectingRegion>
, 
<collectingCountry id="00C87643FF83D265FC78FA83FB808935" box="[959,1073,1377,1403]" name="Argentina" pageId="28" pageNumber="329">Argentina</collectingCountry>
, and 
<collectingCountry id="00C87643FF83D265FBACFA83FB1E8935" box="[1131,1199,1377,1403]" name="Brazil" pageId="28" pageNumber="329">Brazil</collectingCountry>
(
<collectingRegion id="BA1BF831FF83D265FB79FA83FAA48935" box="[1214,1301,1377,1403]" country="Brazil" name="Sergipe" pageId="28" pageNumber="329">Sergipe</collectingRegion>
