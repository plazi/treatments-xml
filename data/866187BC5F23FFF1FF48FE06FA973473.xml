<collectingCountry id="76DF763A5F23FFF1FECEFC3CFE9333AF" box="[273,361,963,989]" name="Mexico" pageId="3" pageNumber="538">Mexico</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FEABFC3CFE1033AF" box="[372,490,963,989]" name="Nicaragua" pageId="3" pageNumber="538">Nicaragua</collectingCountry>
