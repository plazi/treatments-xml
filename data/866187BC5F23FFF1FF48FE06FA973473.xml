<collectingCountry id="76DF763A5F23FFF1FF0CFC60FEBF33CB" box="[211,325,927,953]" name="Argentina" pageId="3" pageNumber="538">Argentina</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FE8EFC60FE5C33CB" box="[337,422,927,953]" name="Bolivia" pageId="3" pageNumber="538">Bolivia</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FE6EFC60FE0233CB" box="[433,504,927,953]" name="Brazil" pageId="3" pageNumber="538">Brazil</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FDDBFC60FD8F33CB" box="[516,629,927,953]" name="Colombia" pageId="3" pageNumber="538">Colombia</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FD5FFC60FD0733CB" box="[640,765,927,953]" name="Costa Rica" pageId="3" pageNumber="538">Costa Rica</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FCD7FC60FC9D33CB" box="[776,871,927,953]" name="Ecuador" pageId="3" pageNumber="538">Ecuador</collectingCountry>
, 
<collectingCountry id="76DF763A5F23FFF1FCACFC60FC0233CB" box="[883,1016,927,953]" name="El Salvador" pageId="3" pageNumber="538">El Salvador</collectingCountry>
, French Guiana, 
<collectingCountry id="76DF763A5F23FFF1FB68FC60FAC933CB" box="[1207,1331,927,953]" name="Guatemala" pageId="3" pageNumber="538">Guatemala</collectingCountry>
