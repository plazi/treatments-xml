<taxonomicName id="22A5BDF1FF84FFB70519FE3FFD1BFE31" baseAuthorityName="Holmberg" baseAuthorityYear="1875" box="[548,694,475,501]" class="Arachnida" family="Sparassidae" genus="Polybetes" kingdom="Animalia" order="Araneae" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="pythagoricus">
