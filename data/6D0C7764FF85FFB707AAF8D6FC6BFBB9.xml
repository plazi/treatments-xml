; 
<collectorName id="4850A3A4FF84FFB7044EFDC7FBFAFDF9" box="[883,1111,547,573]" pageId="55" pageNumber="56">L. F. Corullon. Host</collectorName>
: 
<taxonomicName id="22A5BDF1FF84FFB7035FFDC7FAFBFDF9" authorityName="Lucas" authorityYear="1836" box="[1122,1366,547,573]" class="Arachnida" family="Lycosidae" genus="Lycosa" kingdom="Animalia" order="Araneae" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="erythrognatha">
