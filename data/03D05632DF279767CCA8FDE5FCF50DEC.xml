<emphasis id="B90D3B36DF279767CD49FCA1FE6D0DED" box="[348,461,893,919]" italics="true" pageId="22" pageNumber="23">A. oxiana</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23E864DDF279767CDC1FCA1FD8F0DEC" box="[468,559,893,919]" pageId="22" pageNumber="23" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="4C799CA7DF279767CE72FCA1FD540DEC" authority="Zonstein, 2018" authorityName="Zonstein" authorityYear="2018" box="[615,756,893,919]" class="Arachnida" family="Cyrtaucheniidae" genus="Anemesia" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="sogdiana" status="sp. nov.">
<emphasis id="B90D3B36DF279767CE72FCA1FD540DEC" box="[615,756,893,919]" italics="true" pageId="22" pageNumber="23">A. sogdiana</emphasis>
