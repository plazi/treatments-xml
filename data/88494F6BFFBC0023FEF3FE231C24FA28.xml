<heading id="5B174911FFBC0023FEF3FE231A65FE34" bold="true" box="[264,613,483,510]" fontSize="11" level="1" pageId="3" pageNumber="48" reason="1">
<treatmentCitationGroup id="20F0D953FFBC0023FEF3FE231A65FE34" box="[264,613,483,510]" pageId="3" pageNumber="48">
<taxonomicName id="C7E085FEFFBC0023FEF3FE231A65FE34" authority="LeConte, 1861: 130" authorityName="LeConte" authorityPageNumber="130" authorityYear="1861" box="[264,613,483,510]" class="Insecta" family="Lucanidae" genus="Nicagus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="3294226FFFBC0023FEF3FE231A65FE34" bold="true" box="[264,613,483,510]" pageId="3" pageNumber="48">
<emphasis id="3294226FFFBC0023FEF3FE23196BFE37" bold="true" box="[264,363,484,510]" italics="true" pageId="3" pageNumber="48">Nicagus</emphasis>
<treatmentCitation id="8141D86CFFBC0023FE88FE241A65FE34" author="LeConte" box="[371,613,483,509]" page="130" pageId="3" pageNumber="48" year="1861">
<bibRefCitation id="6471838CFFBC0023FE88FE241A2CFE34" author="LeConte" box="[371,556,483,509]" pageId="3" pageNumber="59" refString="LeConte, J. L. (1861) Classification of the Coleoptera of North America, Part 1. Smithsonian Miscellaneous Collections, 136, 1 - 208." type="journal article" year="1861">
LeConte, 
