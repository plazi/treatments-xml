<emphasis id="B94FEA9F7312AB40424DF9A1FEC8F911" box="[271,348,1649,1674]" italics="true" pageId="103" pageNumber="104">frigida</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3B4D0E7312AB4042D4F9A2FD5DF911" authority="(Pengelly 1955)" authorityName="Pengelly" authorityYear="1955" baseAuthorityName="Pengelly" baseAuthorityYear="1955" box="[406,713,1649,1674]" class="Insecta" family="Apidae" genus="Nomada" kingdom="Animalia" order="Hymenoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="texana">
