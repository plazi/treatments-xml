<taxonomicName id="4C3B4D0E7312AB40406DF945FB60F935" authority="(Rocha Filho &amp; Packer 2016)" authorityName="Rocha Filho &amp; Packer" authorityYear="2016" baseAuthorityName="Rocha Filho &amp; Packer" baseAuthorityYear="2016" box="[815,1268,1685,1710]" class="Insecta" family="Megachilidae" genus="Coelioxys" kingdom="Animalia" order="Hymenoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="genus">
