<taxonomicName id="4C3B4D0E7312AB40402DF9A2FB38F911" authority="(Hobbs 1968)" authorityName="Hobbs" authorityYear="1968" baseAuthorityName="Hobbs" baseAuthorityYear="1968" box="[879,1196,1649,1674]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="rotundata">
