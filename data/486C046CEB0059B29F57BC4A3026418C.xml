<paragraph id="794C8DD3E339286B89B053474FEED892" pageId="54" pageNumber="79">
<taxonomicName id="6C7A837DA4A7360DFBE21901470A42CD" class="Magnoliopsida" family="Asteraceae" genus="Kurziella" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="54" pageNumber="79" phylum="Tracheophyta" rank="species" species="gymnoclada">
<emphasis id="80331204DE8D9F23314FC0210671978E" italics="true" pageId="54" pageNumber="79">Kurziella gymnoclada</emphasis>
</taxonomicName>
