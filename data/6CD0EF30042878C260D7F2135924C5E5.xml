<taxonomicName id="EBDB930D8BB7418D6D73CEF9B8FE3AC1" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina skipetarica" order="Stylommatophora" pageId="55" pageNumber="56" phylum="Mollusca" rank="subSpecies" species="skipetarica" subSpecies="skipetarica">Montenegrina skipetarica skipetarica</taxonomicName>
.
</paragraph>
</subSubSection>
