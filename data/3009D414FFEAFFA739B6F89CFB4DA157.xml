<number id="0E687725FFEAFFA139B6F89CFF27A5C5" box="[151,182,1901,1927]" pageId="19" pageNumber="120" value="14.0">14</number>
. 
<taxonomicName id="7FA01E81FFEAFFA139E2F89CFCB6A5C5" authority="Bates, 1888" authorityName="Bates" authorityYear="1888" box="[195,807,1901,1927]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="gigantea" subGenus="Phyllophaga">
