<heading id="D0A85D4FFF80FFFE5BC9F89EFE32C62A" bold="true" box="[151,504,1874,1901]" fontSize="11" level="1" pageId="23" pageNumber="230" reason="1">
<taxonomicName id="4C5F91A0FF80FFFE5BC9F89EFE32C62A" authority="Lundblad, 1930" authorityName="Lundblad" authorityYear="1930" box="[151,504,1874,1901]" class="Arachnida" family="Aturidae" genus="Axonopsella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="23" pageNumber="230" phylum="Arthropoda" rank="genus">
<emphasis id="B92B3631FF80FFFE5BC9F89EFE32C62A" bold="true" box="[151,504,1874,1901]" pageId="23" pageNumber="230">
<emphasis id="B92B3631FF80FFFE5BC9F89EFEE7C62B" bold="true" box="[151,301,1874,1900]" italics="true" pageId="23" pageNumber="230">Axonopsella</emphasis>
Lundblad, 
<number id="3D97F804FF80FFFE5AE0F89FFE32C62A" box="[446,504,1875,1901]" pageId="23" pageNumber="230" value="1930.0">1930</number>
</emphasis>
