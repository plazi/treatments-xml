<emphasis id="B911EAD1B25FDF02FF66FDA6FEAF8E2E" bold="true" box="[199,315,533,559]" pageId="13" pageNumber="314">Remarks.</emphasis>
<taxonomicName id="4C654D40B25FDF02FEE0FDA7FDA18E2F" authorityName="Nel, Varenne &amp; Peslier" authorityYear="2020" box="[321,565,532,558]" class="Insecta" family="Tortricidae" genus="Ancylis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="314" phylum="Arthropoda" rank="species" species="paraobtusana">
