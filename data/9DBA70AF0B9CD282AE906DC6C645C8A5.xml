<geoCoordinate id="9C45C75F8CBAFE4E2BA15A5B214EE467" direction="north" orientation="latitude" precision="1" value="21.964716">21°57.883'N</geoCoordinate>
, 
<geoCoordinate id="5995F0DEA4BA7253480580A228BFB149" direction="east" orientation="longitude" precision="1" value="101.20245">101°12.147'E</geoCoordinate>
, elevation ca 839 m, 15.08.2011, primary tropical seasonal rain forest, fogging; 1♂, 
<geoCoordinate id="5E9EBB56C875343F3EEEF382F00A2734" direction="north" orientation="latitude" precision="1" value="21.957417">21°57.445'N</geoCoordinate>
