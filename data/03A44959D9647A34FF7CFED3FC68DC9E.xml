<taxonomicName id="4C0D83CCD9647A34FE91FE6FFCD3DFB6" authority="Gilmour, 1957" authorityName="Gilmour" authorityYear="1957" box="[378,840,404,431]" class="Insecta" family="Cerambycidae" genus="Trichonyssodrys" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="544" phylum="Arthropoda" rank="species" species="maculata">
