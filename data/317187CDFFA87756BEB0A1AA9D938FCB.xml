<taxonomicName id="7ED84D58FFA87756BD30A6EE9DD38ECF" authority="Kossack, 1911" authorityName="Kossack" authorityYear="1911" box="[791,1213,1229,1254]" class="Trematoda" family="Cyclocoelidae" genus="Hyptiasmus" kingdom="Animalia" order="Plagiorchiida" pageId="68" pageNumber="69" phylum="Platyhelminthes" rank="species" species="ominosus">
<emphasis id="8BACEAC9FFA87756BD30A6EE9D608ECC" box="[791,1038,1230,1253]" italics="true" pageId="68" pageNumber="69">Hyptiasmus ominosus</emphasis>
<bibRefCitation id="DD494B2AFFA87756BA30A6ED9DD38ECF" author="Kossack" box="[1047,1213,1229,1254]" pageId="68" pageNumber="69" refString="Kossack, W. F. K. (1911) Uber Monostomiden. Zoologische Jahrbucher Abteilung fur Systematik, Geographie und Biologie der Tiere, 31, 491 - 590." type="journal article" year="1911">
Kossack, 
<number id="0F1024FCFFA87756BAA1A6EE9DD38ECF" box="[1158,1213,1230,1254]" pageId="68" pageNumber="69" value="1911.0">1911</number>
</bibRefCitation>
</taxonomicName>
by 
<bibRefCitation id="DD494B2AFFA87756BACCA6ED9CF28ECF" author="Kossack" box="[1259,1436,1229,1254]" pageId="68" pageNumber="69" refString="Kossack, W. F. K. (1911) Uber Monostomiden. Zoologische Jahrbucher Abteilung fur Systematik, Geographie und Biologie der Tiere, 31, 491 - 590." type="journal article" year="1911">
Kossack (
<number id="0F1024FCFFA87756BB7DA6EE9CFD8ECF" box="[1370,1427,1230,1254]" pageId="68" pageNumber="69" value="1911.0">1911</number>
)
</bibRefCitation>
from materials labelled as “ 
<taxonomicName id="7ED84D58FFA87756BFF2A6D29AD88F23" authority="Creplin, 1829" authorityName="Creplin" authorityYear="1829" box="[469,950,1265,1290]" class="Trematoda" genus="Monostomum" higherTaxonomySource="CoL" kingdom="Animalia" pageId="68" pageNumber="69" phylum="Platyhelminthes" rank="species" species="microstomum">
