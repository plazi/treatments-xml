, 
<emphasis id="D891EAA1FFDA292EFD2013A74159FA6A" bold="true" box="[758,775,1459,1480]" pageId="7" pageNumber="524">E</emphasis>
, 
<emphasis id="D891EAA1FFDA292EFCC213A74176FA6B" bold="true" box="[788,808,1459,1481]" pageId="7" pageNumber="524">G</emphasis>
