<location id="6B20604B401BE24DFE21FA1E943A9C96" LSID="urn:lsid:plazi:treatment:E6568786401BE24DFF0CFC1592749D8D:6B20604B401BE24DFE21FA1E943A9C96" box="[443,584,1467,1491]" country="Gabon" municipality="Monts de Cristal in Gabon" name="Primary forest" pageId="7" pageNumber="44">Primary forest</location>
along streams at around 
<quantity id="A9079B75401BE24DFEBEFA7097169CA8" box="[292,356,1493,1517]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="7" pageNumber="44" unit="m" value="500.0">
<elevation id="E5D2D1A3401BE24DFEBEFA7097169CA8" box="[292,356,1493,1517]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="7" pageNumber="44" unit="m" value="500.0">500 m</elevation>
</quantity>
in 
