<paragraph id="C923AB7D743FFFB8FF655ABDFDBFB170" blockId="16.[151,586,978,1031]" box="[151,586,1009,1031]" pageId="16" pageNumber="17">
<taxonomicName id="0E9CD0FE743FFFB8FF655ABDFDBFB170" authority="Rebelo &amp; Moure, 1996" authorityName="Rebelo &amp; Moure" authorityYear="1996" box="[151,586,1009,1031]" class="Insecta" family="Apidae" genus="Euglossa" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="fimbriata">
