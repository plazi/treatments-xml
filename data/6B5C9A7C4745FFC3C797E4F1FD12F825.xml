<taxonomicName id="24F550E94745FFC3C659E115FD14F9F1" authority="Eggers" authorityName="Eggers" box="[345,705,1545,1570]" class="Insecta" family="Curculionidae" genus="Scolytus" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="species" species="mediterraneus">
