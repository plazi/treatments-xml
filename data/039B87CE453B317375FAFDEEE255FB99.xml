<taxonomicName id="4C324D5B453B3173764EFD60E2C6FD5D" box="[803,898,703,728]" class="Malacostraca" family="Nannastacidae" genus="Cumella" kingdom="Animalia" order="Cumacea" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
