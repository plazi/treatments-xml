<taxonomicName id="0AD53B5C4E187CB46AF38447002F8DE8" authority="L. em. Koch" class="Magnoliopsida" family="Rosaceae" genus="Potentilla" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="402" phylum="Tracheophyta" rank="species" species="verna">
