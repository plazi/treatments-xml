<taxonomicName id="D9A950B52A04FFB915A8FAFD6A3AFA72" box="[900,1051,1450,1475]" class="Arachnida" family="Harpirhynchidae" genus="Perharpyrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="charadrius" status="sp. nov.">
