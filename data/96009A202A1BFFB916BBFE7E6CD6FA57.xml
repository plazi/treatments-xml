<taxonomicNameLabel id="37EE4A5F2A04FFB9120FFAFC6A5CFA73" box="[1059,1149,1450,1475]" pageId="80" pageNumber="81" rank="species">
<emphasis id="2CDDF7242A04FFB9120FFAFC6A56FA72" bold="true" box="[1059,1143,1450,1475]" pageId="80" pageNumber="81">sp. nov</emphasis>
