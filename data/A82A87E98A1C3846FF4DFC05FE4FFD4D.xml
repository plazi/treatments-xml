<quantity id="E77B9B1A8A1C3847FD59F8EFFCEAF8DC" box="[643,808,1821,1848]" metricMagnitude="-2" metricUnit="m" metricValue="2.21" metricValueMax="2.51" metricValueMin="1.91" pageId="10" pageNumber="11" unit="mm" value="22.1" valueMax="25.1" valueMin="19.1">19.1–25.1 mm</quantity>
, 
<collectionCode id="4692AE3A8A1C3847FCEEF8ECFCA7F8DC" box="[820,869,1822,1848]" country="Germany" name="Museum Donaueschingen" pageId="10" pageNumber="11">MD</collectionCode>
