<quantity id="E77B9B1A8A1C3847FC79F8EFFC38F8DC" box="[931,1018,1821,1848]" metricMagnitude="-3" metricUnit="m" metricValue="9.1" pageId="10" pageNumber="11" unit="mm" value="9.1">9.1 mm</quantity>
, AA = 31–34°, SL/ 
<collectionCode id="4692AE3A8A1C3847FB0EF8EFFAC1F8D3" box="[1236,1283,1821,1847]" country="Germany" name="Museum Donaueschingen" pageId="10" pageNumber="11">MD</collectionCode>
