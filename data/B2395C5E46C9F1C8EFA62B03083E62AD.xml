<paragraph id="67A87268DD3936F9F6CDB8B0C411518A" pageId="null" pageNumber="704">
<taxonomicName id="E0F7D7CE528738BD331AC2004CD7D060" authority="L." class="Magnoliopsida" family="Cannabaceae" genus="Cannabis" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="704" phylum="Tracheophyta" rank="species" species="sativa">
Cannabis 
<normalizedToken id="3B5E5E677840E86A7FFF5461531A5F9B" originalValue="satíva" pageId="null" pageNumber="704">sativa</normalizedToken>
<authorityName id="53C0E2788B69FFBB22D43DED83B0D740" pageId="null" pageNumber="704">L.</authorityName>
</taxonomicName>
</paragraph>
