<taxonomicName id="71E84D3517594E03FB9CF8CFFB58F8C4" authorityName="Jennings &amp; Parslow &amp; Austin" authorityYear="2018" box="[1119,1224,1808,1832]" class="Insecta" family="Curculionidae" genus="Aulacus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="burnsi">
