<emphasis id="BF92B674FFBFFFA2B49C1D017A67206D" bold="true" box="[498,853,1820,1850]" pageId="120" pageNumber="121">
<taxonomicName id="4AE611E5FFBFFFA2B49C1D017B50206D" baseAuthorityName="Baroni Urbani" baseAuthorityYear="1901" box="[498,610,1820,1850]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="120" pageNumber="121" phylum="Arthropoda" rank="species" species="augusti">
