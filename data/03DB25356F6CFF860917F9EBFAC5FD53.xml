<taxonomicName id="4C72EFA06F6BFF860856FDC9FC0FFDDE" authority="Marshall, 2019" authorityName="Marshall" authorityYear="2019" box="[791,959,520,546]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="albitergum" status="sp. nov.">
