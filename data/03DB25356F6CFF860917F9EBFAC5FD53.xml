<paragraph id="8BCD94236F6BFF860BFCFDC9FAC5FD53" blockId="21.[189,1399,520,687]" pageId="21" pageNumber="22">
<taxonomicName id="4C72EFA06F6BFF860BFCFDC9FE6AFDDE" authority="Marshall, 2019" authorityName="Marshall" authorityYear="2019" box="[189,474,520,546]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="anchitarsus" status="sp. nov.">
<emphasis id="B90648316F6BFF860BFCFDC9FE6AFDDE" box="[189,474,520,546]" italics="true" pageId="21" pageNumber="22">Mesoconius anchitarsus</emphasis>
