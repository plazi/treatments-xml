<collectingCountry id="4CC976037E7AA978D4D0CA2FFCF5DF43" box="[723,783,1858,1882]" name="India" pageId="5" pageNumber="82">India</collectingCountry>
, 
<collectingCountry id="4CC976037E7AA978D51FCA2FFC77DF43" box="[796,909,1858,1882]" name="Sri Lanka" pageId="5" pageNumber="82">Sri Lanka</collectingCountry>
, 
<collectingCountry id="4CC976037E7AA978D59BCA2FFC17DF43" box="[920,1005,1858,1882]" name="Taiwan" pageId="5" pageNumber="82">Taiwan</collectingCountry>
, 
<collectingCountry id="4CC976037E7AA978D5FACA2EFBC6DF43" box="[1017,1084,1859,1882]" name="Japan" pageId="5" pageNumber="82">Japan</collectingCountry>
, 
<collectingCountry id="4CC976037E7AA978D24BCA2FFB42DF43" box="[1096,1208,1858,1882]" name="Indonesia" pageId="5" pageNumber="82">Indonesia</collectingCountry>
, and the 
<collectingCountry id="4CC976037E7AA978D31DCA2FFA66DF43" box="[1310,1436,1858,1882]" name="Philippines" pageId="5" pageNumber="82">Philippines</collectingCountry>
(
<bibRefCitation id="504F4B627E7AA978D69DCA0BFEBBDF67" author="Yu" box="[158,321,1894,1919]" pageId="5" pageNumber="82" refString="Yu, D. S., van Achterberg, K. &amp; Horstmann, K. (2005) World Ichneumonoidea 2004 - Taxonomy, Biology, Morphology and Distribution. DVD / CD. Taxapad, Vancouver, Canada." type="book" year="2005">
Yu 
<emphasis id="06AAEA817E7AA978D6C5CA05FF0DDF67" box="[198,247,1894,1919]" italics="true" pageId="5" pageNumber="82">et al</emphasis>
