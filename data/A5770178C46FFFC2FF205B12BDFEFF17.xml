<emphasis id="1FAA6C7CC46EFFC1FB685A9BBC20F914" bold="true" box="[1247,1289,1757,1780]" pageId="9" pageNumber="210">(24)</emphasis>
<taxonomicName id="EADECBEDC46EFFC1FAA35A98B9D9F8F2" class="Insecta" family="Phlaeothripidae" genus="Horistothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="species" species="australis">
<emphasis id="1FAA6C7CC46EFFC1FAA35A98B9D9F8F2" italics="true" pageId="9" pageNumber="210">Horistothrips australis</emphasis>
</taxonomicName>
