<emphasis id="1FAA6C7CC46EFFC1FAC55AF9BCB4F936" bold="true" box="[1394,1437,1727,1750]" pageId="9" pageNumber="210">(21)</emphasis>
<taxonomicName id="EADECBEDC46EFFC1FF205A98B8A7F914" box="[151,398,1758,1780]" class="Insecta" family="Phlaeothripidae" genus="Ecacanthothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="species" species="tibialis">
