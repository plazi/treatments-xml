<emphasis id="1FAA6C7CC46EFFC1FD955A9BBB65F914" bold="true" box="[546,588,1757,1780]" pageId="9" pageNumber="210">(22)</emphasis>
<taxonomicName id="EADECBEDC46EFFC1FDE15A98BA01F914" box="[598,808,1758,1780]" class="Insecta" family="Phlaeothripidae" genus="Habrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="species" species="curious">
<emphasis id="1FAA6C7CC46EFFC1FDE15A98BA01F914" box="[598,808,1758,1780]" italics="true" pageId="9" pageNumber="210">Habrothrips curious</emphasis>
</taxonomicName>
