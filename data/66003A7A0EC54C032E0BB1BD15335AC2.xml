<taxonomicName id="FEC1B4C9A653B7A65443144FCDE998B1" family="Scarabaeidae" genus="C." kingdom="Animalia" lsidName="C. testacea" order="Coleoptera" pageId="117" pageNumber="218" rank="species" species="testacea">
<emphasis id="78F6A380C8CB92C8E184A56B991F0B22" italics="true" pageId="117" pageNumber="218">C. testacea</emphasis>
