<taxonomicName id="A3B73FC7FAA89D686A599BF22A514790" family="Scarabaeidae" genus="C." kingdom="Animalia" lsidName="C. testacea" order="Coleoptera" pageId="117" pageNumber="218" rank="species" species="testacea">
