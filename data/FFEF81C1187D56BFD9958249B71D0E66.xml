<taxonomicName id="5D17011D0916B0D6FF433919419C423B" class="Malacostraca" family="Leucothoidae" genus="Leucothoe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leucothoe eltoni" order="Amphipoda" pageId="12" pageNumber="63" phylum="Arthropoda" rank="species" species="eltoni">Leucothoe eltoni</taxonomicName>
