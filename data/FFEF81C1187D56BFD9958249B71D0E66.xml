<taxonomicName id="79CAE2DB01454DBB2C9FF041C3BC73E4" class="Ascidiacea" family="Pyuridae" genus="Herdmania" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Herdmania" order="Stolidobranchia" pageId="3" pageNumber="54" phylum="Chordata" rank="genus">Herdmania</taxonomicName>
