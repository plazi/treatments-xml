50. - 
<taxonomicName id="AFB6FED3011A0D175BB83EF1ED765C81" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:144199" class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Strumigenys (Cephaloxys) bequaerti Santschi" lsidName-HNS="Strumigenys (Cephaloxys) bequaerti Santschi" order="Hymenoptera" pageNumber="286" phylum="Arthropoda" rank="species" species="bequaerti" subGenus="Xephaloxys">Strumigenys (Xephaloxys) bequaerti</taxonomicName>
