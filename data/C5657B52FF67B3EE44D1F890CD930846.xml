<taxonomicName id="8ACCB1C7FF61B3EE404BFD2FCD1709D2" baseAuthorityName="Johnson" baseAuthorityYear="1901" box="[1037,1297,742,766]" class="Polychaeta" family="Maldanidae" genus="Axiothella" kingdom="Animalia" pageId="253" pageNumber="254" phylum="Annelida" rank="species" species="rubrocincta">
<emphasis id="7FB81656FF61B3EE404BFD2FCD1709D2" box="[1037,1297,742,766]" italics="true" pageId="253" pageNumber="254">Axiothella rubrocincta</emphasis>
</taxonomicName>
, 
<taxonomicName id="8ACCB1C7FF61B3EE4160FD2EC9E3080E" authority="Hartman, 1941" authorityName="Hartman" authorityYear="1941" class="Polychaeta" family="Pectinariidae" genus="Pectinaria" kingdom="Chromista" order="Terebellida" pageId="253" pageNumber="254" phylum="Foraminifera" rank="species" species="californiensis">
<emphasis id="7FB81656FF61B3EE4160FD2EC936080F" italics="true" pageId="253" pageNumber="254">Pectinaria californiensis</emphasis>
Hartman, 1941
</taxonomicName>
; 
<taxonomicName id="8ACCB1C7FF61B3EE45B2FCC2CB4B080E" authority="(Linnaeus, 1757)" baseAuthorityName="Linnaeus" baseAuthorityYear="1757" box="[500,845,778,803]" class="Malacostraca" family="Pinnotheridae" genus="Scleroplax" kingdom="Animalia" order="Decapoda" pageId="253" pageNumber="254" phylum="Arthropoda" rank="species" species="granulata">
<emphasis id="7FB81656FF61B3EE45B2FCC2CA79080F" box="[500,639,779,803]" italics="true" pageId="253" pageNumber="254">P. granulata</emphasis>
