and 
<location id="38049EFCFFBA790CFB4E25D667468CF2" LSID="urn:lsid:plazi:treatment:B5727931FFBB790CFF3C233460F48D27:38049EFCFFBA790CFB4E25D667468CF2" box="[1253,1345,151,176]" municipality="Australasian" name="Oceania" pageId="11" pageNumber="12" stateProvince="Oriental">Oceania</location>
