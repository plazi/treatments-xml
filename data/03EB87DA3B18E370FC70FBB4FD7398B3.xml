<taxonomicName id="4C424D4F3B1EE373FF18F81BFE469AA1" authority="Rahayu &amp; Ng, 2014" authorityName="Rahayu &amp; Ng" authorityYear="2014" box="[222,453,2002,2023]" class="Malacostraca" family="Hexapodidae" genus="Mariaplax" kingdom="Animalia" order="Decapoda" pageId="37" pageNumber="433" phylum="Arthropoda" rank="species" species="cyrtophallus" status="new species">
