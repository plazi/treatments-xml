<paragraph id="CFE11BA5D12A70092CADC585684F687C" pageId="29" pageNumber="44">
<taxonomicName id="CBBFC99736BAEE49A43B4479F9078206" class="Insecta" family="Curculionidae" genus="Antarctobius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="29" pageNumber="44" phylum="Arthropoda" rank="species" species="lacunosus">
