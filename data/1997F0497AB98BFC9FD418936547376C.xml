<taxonomicName id="8E97EF053E19C2EF45EC4D06BDE69450" class="Bivalvia" family="Veneridae" genus="Venus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Venus gallus" order="Venerida" pageId="9" pageNumber="10" phylum="Mollusca" rank="species" species="gallus">Venus gallus</taxonomicName>
