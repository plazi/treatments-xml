<taxonomicName id="1DB54D64E048FFBB256B6658783EFD2C" authority="KAUP, 1832" authorityName="KAUP" authorityYear="1832" box="[1048,1310,630,654]" class="Mammalia" family="Castoridae" genus="Chalicomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="1" pageNumber="166" phylum="Chordata" rank="genus">
<emphasis id="E8C1EAF5E048FFBB256B6658783EFD2C" bold="true" box="[1048,1310,630,654]" pageId="1" pageNumber="166">
<emphasis id="E8C1EAF5E048FFBB256B665879B9FD2C" bold="true" box="[1048,1177,630,654]" italics="true" pageId="1" pageNumber="166">Chalicomys</emphasis>
<bibRefCitation id="BE244B16E048FFBB25D3665879FBFD2E" author="Kaup, J." box="[1184,1243,630,654]" pageId="1" pageNumber="166" pagination="992 - 996" refId="ref6199" refString="Kaup, J. (1832): Beschreibung dreyer Gattungen urweltlicher Nager des Zoologischen Museums zu Darmstadt welche von den jetzt lebenden Genera verschieden sind. - Isis, Leipzig, 1832 (9): 992 - 996." type="journal article" year="1832">KAUP</bibRefCitation>
