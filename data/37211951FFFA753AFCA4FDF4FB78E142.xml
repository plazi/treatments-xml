<taxonomicName id="7888D3C4FFFA753AFC88FDF4FB38E753" authority="Ruiz &amp; Pav." authorityName="Ruiz &amp; Pav." box="[882,1200,558,582]" class="Magnoliopsida" family="Rosaceae" genus="Acaena" kingdom="Plantae" order="Rosales" pageId="10" pageNumber="97" phylum="Tracheophyta" rank="species" species="ovalifolia">
