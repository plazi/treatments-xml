<quantity id="8512ADC7FF91FF90AE7C476CFD47784F" box="[583,670,1047,1069]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" metricValueMax="7.0" metricValueMin="6.0" pageId="25" pageNumber="1288" unit="mm" value="6.5" valueMax="7.0" valueMin="6.0">6-7 mm</quantity>
