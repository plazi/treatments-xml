<taxonomicName id="ED8A12F49E40FFCFFE56FA6F6749FA41" authority="Li et al., 2013" authorityName="Li et al." authorityYear="2013" box="[492,894,1477,1502]" class="Insecta" family="Staphylinidae" genus="Paederus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="435" phylum="Arthropoda" rank="species" species="parvidenticulatus">
