- Antennomeres 
<collectionCode id="3E9A72D4FFACFF92D73787F5094008BA" box="[356,373,1928,1949]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="3" pageNumber="26" type="Museum">V</collectionCode>
