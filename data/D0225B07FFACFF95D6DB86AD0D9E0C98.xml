disimilar, 
<collectionCode id="3E9A72D4FFACFF92D47087F50A0908BA" box="[547,572,1928,1949]" country="Norway" name="Mykotektet, National Veterinary Institute" pageId="3" pageNumber="26">VI</collectionCode>
