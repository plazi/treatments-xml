<paragraph id="5D0B3686FFCDFF89A394FC6C02E0FC2E" blockId="5.[178,438,957,979]" box="[178,438,957,979]" pageId="5" pageNumber="834">
<heading id="064381EAFFCDFF89A394FC6C02E0FC2E" box="[178,438,957,979]" fontSize="8" level="2" pageId="5" pageNumber="834" reason="2">
<emphasis id="6FC0EA94FFCDFF89A394FC6C02E0FC2E" bold="true" box="[178,438,957,979]" pageId="5" pageNumber="834">
<taxonomicName id="9AB44D05FFCDFF89A394FC6C0232FC2E" authority="Malicky &amp; I &amp; D, 2014" authorityName="Malicky &amp; I &amp; D" authorityYear="2014" box="[178,356,957,979]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="834" phylum="Arthropoda" rank="species" species="tecalto" status="nov.sp.">
<emphasis id="6FC0EA94FFCDFF89A394FC6C0232FC2E" bold="true" box="[178,356,957,979]" italics="true" pageId="5" pageNumber="834">Chimarra tecalto</emphasis>
</taxonomicName>
<taxonomicNameLabel id="74F357EFFFCDFF89A24CFC6C02E0FC2E" box="[362,438,957,979]" pageId="5" pageNumber="834" rank="species">nov.sp.</taxonomicNameLabel>
