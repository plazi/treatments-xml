galls, 
<date id="FF86DC832074FFE049EDE254FAADF98E" box="[1196,1309,1609,1633]" pageId="9" pageNumber="30" value="1968-10-11">
<number id="3DF0E8642074FFE049EDE254FB77F98E" box="[1196,1223,1609,1633]" pageId="9" pageNumber="30" value="11.0">11</number>
.x. 
<number id="3DF0E8642074FFE0499FE254FAADF98E" box="[1246,1309,1609,1633]" pageId="9" pageNumber="30" value="1968.0">1968</number>
