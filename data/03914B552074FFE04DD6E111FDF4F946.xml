<number id="3DF0E8642074FFE0482BE254FA27F98E" box="[1386,1431,1609,1633]" pageId="9" pageNumber="30" value="426.0">426</number>
, 
<collectionCode id="ED2962862074FFE04DD6E270FF61F96A" box="[151,209,1645,1669]" httpUri="http://grbio.org/cool/nwky-2b63" name="National Pusa Collection" pageId="9" pageNumber="30">NPC</collectionCode>
