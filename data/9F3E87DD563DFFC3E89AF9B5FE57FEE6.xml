<emphasis id="25E3EAD9563DFFC5E89AF9B5FD99F993" bold="true" box="[151,530,1584,1611]" pageId="19" pageNumber="20">
<taxonomicName id="D0974D48563DFFC5E89AF9B5FEF8F992" box="[151,371,1585,1611]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="nathani" status="new species">
<emphasis id="25E3EAD9563DFFC5E89AF9B5FEF8F992" bold="true" box="[151,371,1585,1611]" italics="true" pageId="19" pageNumber="20">Chimarra nathani</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="3ED057A2563DFFC5E98FF9B4FD99F993" box="[386,530,1584,1610]" pageId="19" pageNumber="20" rank="species">new species</taxonomicNameLabel>
