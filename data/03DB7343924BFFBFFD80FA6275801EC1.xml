<taxonomicName id="4C72B9D69275FFBEFD54FA7B75311A8D" authorityName="Freund &amp; Wagner" authorityYear="2003" baseAuthorityName="Bryant" baseAuthorityYear="1953" box="[726,851,1488,1512]" class="Insecta" family="Chrysomelidae" genus="Bonesioides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="1946" phylum="Arthropoda" rank="species" species="jacksoni">
