<taxonomicName id="4C72B9D69275FFBEFC13FA7B759C1A8D" authorityName="Freund &amp; Wagner" authorityYear="2003" baseAuthorityName="Jacoby" baseAuthorityYear="1899" box="[913,1022,1488,1512]" class="Insecta" family="Chrysomelidae" genus="Bonesioides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="1946" phylum="Arthropoda" rank="species" species="kirschi">
<emphasis id="B9061E479275FFBEFC13FA7B759C1A8D" box="[913,1022,1488,1512]" italics="true" pageId="32" pageNumber="1946">B. kirschi</emphasis>
</taxonomicName>
