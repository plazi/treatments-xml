<quantity id="4C9A6198575CFF8E1C14FBC2FE656A0C" box="[312,404,1032,1058]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="2.0" metricValueMin="0.1" pageId="62" pageNumber="63" unit="m" value="10.5" valueMax="20.0" valueMin="1.0">
