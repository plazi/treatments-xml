. 
<materialsCitation id="0EE83D21FF2B2EF2B996FCE0FC5EB0BD" collectingDate="1994-10-31" collectingDateMax="1994-11-10" collectingDateMin="1994-10-31" collectionCode="CNCI" country="Ecuador" elevation="270" location="Sacha Lodge" municipality="River" pageId="181" pageNumber="182" specimenCount="5" specimenCount-male="5" stateProvince="Sucumbios" typeStatus="holotype">
