<taxonomicName id="4C624D1CFF97FFD4FF6EFA7D98A5E4FA" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[229,428,1421,1447]" class="Insecta" family="Tachinidae" genus="Aplomya" kingdom="Animalia" order="Diptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="zonata">Aplomya zonata</taxonomicName>
