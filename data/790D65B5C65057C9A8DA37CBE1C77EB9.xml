<taxonomicName id="96CAD2258F1B67BF3D17833908031670" authorityName="Sharp" authorityYear="1882" class="Insecta" family="Hydrophilidae" genus="Chasmogenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chasmogenus" order="Coleoptera" pageId="0" pageNumber="25" phylum="Arthropoda" rank="genus">
