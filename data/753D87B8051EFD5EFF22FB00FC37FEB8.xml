<taxonomicName id="3A944D2D051FFD5DFE09FABDFC67FAFB" authority="Takeda &amp; Miyake, 1971" authorityName="Takeda &amp; Miyake" authorityYear="1971" box="[444,937,1352,1374]" class="Malacostraca" family="Hymenosomatidae" genus="Lucascinus" kingdom="Animalia" order="Decapoda" pageId="25" pageNumber="505" phylum="Arthropoda" rank="species" species="keijibabai">
