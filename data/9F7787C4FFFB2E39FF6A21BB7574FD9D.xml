<taxonomicName id="D0DE4D51FFFB2E34FBD9241D73A9F9B2" authorityName="Ghesquiere. A" authorityYear="1940" box="[1060,1205,1590,1615]" class="Insecta" family="Lecithoceridae" genus="Ptilothyris" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="206" phylum="Arthropoda" rank="species" species="porphyrea">
<emphasis id="25AAEAC0FFFB2E34FBD9241D73A9F9B2" box="[1060,1205,1590,1615]" italics="true" pageId="5" pageNumber="206">P. porphyrea</emphasis>
