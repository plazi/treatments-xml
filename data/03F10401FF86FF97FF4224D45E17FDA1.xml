MORPHOLOGY OF 
<taxonomicName id="4C58CE94FF86FF97FE8924D458E4FAEF" authorityName="Waldman &amp; Savage" authorityYear="1972" box="[379,712,1261,1284]" class="Mammalia" family="Docodontidae" genus="Borealestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Docodonta" pageId="13" pageNumber="1361" phylum="Chordata" rank="species" species="serendipitus">
<emphasis id="B92C6905FF86FF97FE8924D458E4FAEF" box="[379,712,1261,1284]" italics="true" pageId="13" pageNumber="1336">BOREALESTES SERENDIPITUS</emphasis>
</taxonomicName>
AND 
<taxonomicName id="4C58CE94FF86FF97FEC125355856FAC8" authority="Panciroli &amp; Benson &amp; Fernandez &amp; Butler &amp; Fraser &amp; Luo &amp; Walsh, 2021" box="[307,634,1292,1315]" class="Mammalia" family="Docodontidae" genus="Borealestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Docodonta" pageId="13" pageNumber="1336" phylum="Chordata" rank="species" species="cuillinensis">
<emphasis id="B92C6905FF86FF97FEC125355856FAC8" box="[307,634,1292,1315]" italics="true" pageId="13" pageNumber="1336">
<emphasis id="B92C6905FF86FF97FEC125355BF9FAC8" box="[307,469,1292,1315]" italics="true" pageId="13" pageNumber="1336">BOREALESTES</emphasis>
