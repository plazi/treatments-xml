<taxonomicName id="4C7D4D6D2128FFEDFC7AD94BB960F9C4" box="[924,1099,1555,1581]" class="Malacostraca" family="Homolidae" genus="Maloha" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="alcocki">
