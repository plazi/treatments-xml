<taxonomicName id="4C7D4D6D2128FFEDFF21D94BBCA5F9C4" baseAuthorityName="Stebbing" baseAuthorityYear="1920" box="[199,398,1555,1581]" class="Malacostraca" family="Homolidae" genus="Paromola" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="alcocki">
