<taxonomicName id="4C7D4D6D2128FFEDFE82DAB7BE91F9E0" authority="(Stebbing, 1922) (WORMS 2021)" authorityName="WORMS" authorityYear="2021" baseAuthorityName="Stebbing" baseAuthorityYear="1922" box="[356,954,1519,1545]" class="Malacostraca" family="Homolidae" genus="Thelxiope" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="alcocki">
