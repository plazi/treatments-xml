<paragraph id="203936837F03FFF1FF3E5F6AFCF5F9FD" blockId="5.[151,1436,1556,1976]" box="[151,843,1556,1581]" pageId="5" pageNumber="480">
<taxonomicName id="E7864D007F03FFF1FF3E5F6AFDCDF9FC" authority="(Dufour, 1833)" authorityName="Dufour" authorityYear="1833" baseAuthorityName="Dufour" baseAuthorityYear="1833" box="[151,627,1556,1581]" class="Insecta" family="Cixiidae" genus="Reptalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="480" phylum="Arthropoda" rank="species" species="quinquecostatus">
