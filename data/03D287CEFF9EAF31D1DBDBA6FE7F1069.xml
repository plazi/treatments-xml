<paragraph id="8BC436D8FF9EAF30D1DBDBA6FE1E17B4" blockId="42.[151,440,1234,1260]" box="[151,440,1234,1260]" pageId="42" pageNumber="43">
<heading id="D08C81B4FF9EAF30D1DBDBA6FE1E17B4" bold="true" box="[151,440,1234,1260]" fontSize="11" level="1" pageId="42" pageNumber="43" reason="1">
<emphasis id="B90FEACAFF9EAF30D1DBDBA6FE1E17B4" bold="true" box="[151,440,1234,1260]" pageId="42" pageNumber="43">
15. 
<emphasis id="B90FEACAFF9EAF30D18FDBA6FE8817B4" bold="true" box="[195,302,1234,1260]" italics="true" pageId="42" pageNumber="43">
<taxonomicName id="4C7B4D5BFF9EAF30D18FDBA6FE8F17B4" authority="Vaz-De-Mello, 2008" authorityName="Vaz-De-Mello" authorityYear="2008" box="[195,297,1234,1260]" class="Insecta" family="Scarabaeidae" genus="Silvinha" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus" status="new genus">Silvinha</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A23C57B1FF9EAF30D07ADBA6FE1E17B4" box="[310,440,1234,1260]" pageId="42" pageNumber="43" rank="genus">new genus</taxonomicNameLabel>
