<quantity id="366B9B745C016017FBC7F996FB49CD52" box="[1070,1157,1546,1568]" metricMagnitude="3" metricUnit="m" metricValue="5.26" pageId="9" pageNumber="388" unit="m" value="5260.0">5260 m</quantity>
