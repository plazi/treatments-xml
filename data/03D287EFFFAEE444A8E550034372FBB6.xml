or 
<taxonomicName id="4C7B4D7AFFAEE444A91B56504372FBB6" authority="Spencer." authorityName="Spencer." box="[361,625,1087,1111]" class="Insecta" family="Agromyzidae" genus="Agromyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="virginiensis">
