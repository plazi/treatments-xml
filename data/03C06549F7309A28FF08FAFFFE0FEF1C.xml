<taxonomicName id="4C69AFDCF7309A27FF08FAFFFCD7EB41" authority="Foissner, 2004" authorityName="Foissner" authorityYear="2004" box="[151,832,1318,1344]" class="Hypotrichea" family="Oxytrichidae" genus="Cyrtohymena" kingdom="Protozoa" order="Oxytrichida" pageId="7" pageNumber="458" phylum="Ciliophora" rank="species" species="australis" subGenus="Cyrtohymenides">
