</taxonomicName>
and 
<number id="A86139112A4EFFF314C4FEE16D2AFE61" box="[744,779,439,464]" pageId="26" pageNumber="27" value="1.5">1.5</number>
times shorter in 
<taxonomicName id="D9A950B52A4EFFF315E7FEEF6A4DFE61" box="[971,1132,439,464]" class="Arachnida" family="Harpirhynchidae" genus="Harpyrhynchoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="phalaropus">
