<taxonomicName id="4C37D68BFFA6FF93FBF9B40BFB273741" box="[1036,1160,1375,1401]" class="Demospongiae" family="Geodiidae" genus="Geodia" kingdom="Animalia" order="Tetractinellida" pageId="7" pageNumber="108" phylum="Porifera" rank="species" species="undetermined">
