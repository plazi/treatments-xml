species: 
<taxonomicName id="DBCF7CB4FFB6A65EFC04D0DAFAD2FA46" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[1012,1390,1488,1515]" class="Insecta" family="Cimicidae" genus="Cimex" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="727" phylum="Arthropoda" rank="species" species="littoralis">
<emphasis id="2EBBDB25FFB6A65EFC04D0DAFB10FA46" box="[1012,1196,1488,1515]" italics="true" pageId="0" pageNumber="727">Cimex littoralis</emphasis>
Linnaeus, 1758
</taxonomicName>
