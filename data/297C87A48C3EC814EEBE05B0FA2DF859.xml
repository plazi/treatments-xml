<taxonomicName id="66D54D318C3FC814EDAD027BFC51F8BC" box="[900,1017,1882,1907]" class="Adenophorea" family="Tripylidae" genus="Tripylella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="11" pageNumber="209" phylum="Nematoda" rank="species" species="iucunda">
