<taxonomicName id="5BD230D9FFDF8D393CCBFC34FCC2FBD9" baseAuthorityName="d'Orbigny" baseAuthorityYear="1850" box="[677,783,1009,1034]" class="Magnoliopsida" family="Plantaginaceae" genus="Globularia" kingdom="Plantae" order="Lamiales" pageId="6" pageNumber="309" phylum="Tracheophyta" rank="species" species="vapincana">
