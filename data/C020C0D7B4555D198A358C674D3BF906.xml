<paragraph id="1CD1F51D84B845778AE6CE3ACC835245" pageId="7" pageNumber="20">
<taxonomicName id="E0760427943A1636B3F4BAFFDF31C9F8" authorityName="Chamberlin" authorityYear="1914" class="Chilopoda" family="Scolopendridae" genus="Otostigmus" kingdom="Animalia" lsidName="Otostigmus tidius" order="Scolopendromorpha" pageId="7" pageNumber="20" phylum="Arthropoda" rank="species" species="tidius">
<emphasis id="CBE236A95FAFC36B0E67D94EF8889722" italics="true" pageId="7" pageNumber="20">Otostigmus tidius</emphasis>
</taxonomicName>
