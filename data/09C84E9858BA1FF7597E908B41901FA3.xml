<taxonomicName id="D0143AF8B7774053A20891778608085B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:243401" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" lsidName-HNS="Aphaenogaster testaceo-pilosa (Lucas)" order="Hymenoptera" pageNumber="71" phylum="Arthropoda" rank="species" species="testaceo-pilosa">A. testaceo-pilosa Lucas</taxonomicName>
et 
<taxonomicName id="EC0FE75D36517AD465C0123C550CD306" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25498" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" lsidName-HNS="Aphaenogaster sardoa Mayr" order="Hymenoptera" pageNumber="71" phylum="Arthropoda" rank="species" species="sardoa">A. sardoa Mayr</taxonomicName>
.
</paragraph>
