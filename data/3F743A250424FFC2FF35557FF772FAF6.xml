<taxonomicName id="70DDF0B00424FFC7FF35557FF055FBDC" authority="Ohler, Teynie &amp; David, 2004" authorityName="Ohler, Teynie &amp; David" authorityYear="2004" box="[151,814,1112,1139]" class="Amphibia" family="Megophryidae" genus="Leptobrachium" kingdom="Animalia" order="Anura" pageId="1" pageNumber="30" phylum="Chordata" rank="species" species="buchardi">
