<taxonomicName id="4C354D058C597A4CFE8BA328FE5AFE79" box="[326,495,1562,1583]" class="Arachnida" family="Telemidae" genus="Kinku" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="195" phylum="Arthropoda" rank="species" species="turumanya" status="n. sp.">
