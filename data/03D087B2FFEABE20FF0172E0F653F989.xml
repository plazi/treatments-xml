<paragraph id="8BC636A4FFEABE20FF2772BEF27DFB04" blockId="14.[212,1364,1233,1255]" box="[212,1364,1233,1255]" pageId="14" pageNumber="601">
<taxonomicName id="4C794D27FFEABE20FF2772BEF6E6FB05" authority="Taki, 1956: 47" authorityName="Taki" authorityPageNumber="47" authorityYear="1956" box="[212,463,1233,1255]" class="Gastropoda" family="Acteonidae" genus="Japonacteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="14" pageNumber="601" phylum="Mollusca" rank="genus">
