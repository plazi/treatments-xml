<quantity id="4CF89B6FFFD7F41547D734D97FEE3E97" box="[659,759,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" metricValueMax="7.0" metricValueMin="5.0" pageId="41" pageNumber="42" unit="mm" value="6.0" valueMax="7.0" valueMin="5.0">5–7 mm</quantity>
