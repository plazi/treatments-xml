<taxonomicName id="94C04D14FFE4FFF5FAA9F936FBD72A26" authority="Rzebik-Kowalska, 1988" authorityName="Rzebik-Kowalska" authorityYear="1988" class="Mammalia" family="Soricidae" genus="Crocidura" kingdom="Animalia" order="Soricomorpha" pageId="18" pageNumber="253" phylum="Chordata" rank="species" species="jaegeri">
