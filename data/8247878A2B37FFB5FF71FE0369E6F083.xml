<paragraph id="0A51369C2B37FFB5FEC9FD4E68E9F5E2" blockId="6.[151,746,626,713]" box="[303,746,690,713]" pageId="6" pageNumber="7">
<taxonomicName id="CDEE4D1F2B37FFB5FEC9FD4E68E9F5E2" authority="Gieysztor, 1938" authorityName="Gieysztor" authorityYear="1938" box="[303,746,690,713]" class="Rhabditophora" family="Dalyelliidae" genus="Microdalyellia" kingdom="Animalia" order="Rhabdocoela" pageId="6" pageNumber="7" phylum="Platyhelminthes" rank="species" species="cuspidata">
