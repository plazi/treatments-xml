<typeStatus id="54E205FDAA51FFDEFE715D23FDDCCA70" box="[436,538,1571,1596]" pageId="16" pageNumber="317">holotype</typeStatus>
, 
<figureCitation id="1362A7DAAA51FFDEFDEC5D23FD40CA70" box="[553,646,1571,1596]" captionStart="FIGURE 15" captionStartId="17.[151,250,1953,1975]" captionTargetBox="[355,1230,201,1922]" captionTargetId="figure@17.[346,1240,193,1932]" captionTargetPageId="17" captionText="FIGURE 15. Anharpyrhynchus elizae sp. nov., female. A—dorsal view; B—ventral view." httpUri="https://zenodo.org/record/230018/files/figure.png" pageId="16" pageNumber="317">
