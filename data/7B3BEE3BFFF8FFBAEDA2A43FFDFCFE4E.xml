<treatmentCitationGroup id="D3827803FFF8FFBBEDA2A4B6FCB6FA09" box="[151,856,1437,1459]" pageId="6" pageNumber="479">
<taxonomicName id="349224AEFFF8FFBBEDA2A4B6FCFDFA09" authority="Baker, 1924: 64" authorityName="Baker" authorityPageNumber="64" authorityYear="1924" box="[151,787,1437,1459]" class="Insecta" family="Cicadellidae" genus="Makilingia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="479" phylum="Arthropoda" rank="variety" species="banahaoensis" variety="montalbanensis">
