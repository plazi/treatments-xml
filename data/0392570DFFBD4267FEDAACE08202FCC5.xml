<taxonomicName id="4C3B9D98FFBB4267FD1BABD58003FD3E" authorityName="Marshall &amp; Cui" authorityYear="2005" box="[713,841,724,749]" class="Insecta" family="Sphaeroceridae" genus="Robustagramma" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="setilamella">
