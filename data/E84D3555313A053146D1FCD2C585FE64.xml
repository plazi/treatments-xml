<paragraph id="605B8443313905314681FF6AC6A3FEAC" blockId="6.[151,1436,151,392]" pageId="6" pageNumber="577">
<emphasis id="52905851313905314681FF6AC55EFF14" bold="true" box="[199,325,223,248]" italics="true" pageId="6" pageNumber="577">Terminalia</emphasis>
