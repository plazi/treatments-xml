<taxonomicName id="DAB7D5CEFFE3FF87FF11FA60366CFA39" authority="Herrich-Schaffer, 1868" authorityName="Herrich-Schaffer" authorityYear="1868" box="[140,549,1433,1460]" class="Insecta" family="Noctuidae" genus="Ponometia" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="139" phylum="Arthropoda" rank="genus">
