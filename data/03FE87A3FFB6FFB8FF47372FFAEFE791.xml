<subSubSection id="C34D653EFFB7FFB8FF473151FCF0E36D" box="[151,847,312,335]" pageId="37" pageNumber="38" type="reference_group">
<paragraph id="8BE836B5FFB7FFB8FF473151FCF0E36D" blockId="37.[151,1290,152,335]" box="[151,847,312,335]" pageId="37" pageNumber="38">
<treatmentCitationGroup id="AB47119BFFB7FFB8FF473151FCF0E36D" box="[151,847,312,335]" pageId="37" pageNumber="38">
<taxonomicName id="4C574D36FFB7FFB8FF473151FE28E36D" authority="Busck, 1903: 813" authorityName="Busck" authorityPageNumber="813" authorityYear="1903" box="[151,407,312,335]" class="Actinopterygii" family="Aphaniidae" genus="Tellia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyprinodontiformes" pageId="37" pageNumber="38" phylum="Chordata" rank="genus">
<emphasis id="B923EAA7FFB7FFB8FF473151FF6CE36D" box="[151,211,312,335]" italics="true" pageId="37" pageNumber="38">Tellia</emphasis>
