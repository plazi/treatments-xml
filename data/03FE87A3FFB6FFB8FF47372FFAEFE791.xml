Nel
</taxonomicName>
and 
<taxonomicName id="4C574D36FFB7FFB8FE323518FD10E7A9" authority="Stainton" authorityName="Stainton" box="[482,687,1393,1419]" class="Insecta" family="Gelechiidae" genus="Gelechia" kingdom="Animalia" order="Lepidoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="cisti">
