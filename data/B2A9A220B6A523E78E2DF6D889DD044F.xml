<taxonomicName id="CD96FCFCED36419101DF42D2C448970E" family="Platygastroidea" genus="T." kingdom="Animalia" lsidName="T. comperei" order="Hymenoptera" pageId="10" pageNumber="13" rank="species" species="comperei">
