<taxonomicName id="4C6D13AD34663F5EFBD7FD98FB51622A" authorityName="Lucena" authorityYear="1987" box="[1068,1178,1009,1033]" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="28" pageNumber="221" phylum="Chordata" rank="species" species="leticiae">
<emphasis id="B919B43C34663F5EFBD7FD98FB51622A" box="[1068,1178,1009,1033]" italics="true" pageId="28" pageNumber="221">C. leticiae</emphasis>
</taxonomicName>
). 
<taxonomicName id="4C6D13AD34663F5EFB54FD98FAA3622B" authorityName="Eigenmann" authorityYear="1912" box="[1199,1384,1009,1033]" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="28" pageNumber="221" phylum="Chordata" rank="species" species="rupununi">
