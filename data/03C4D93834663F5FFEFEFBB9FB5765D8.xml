</taxonomicName>
, 
<taxonomicName id="4C6D13AD34663F5EFA61FDB8FC5B622A" authorityName=", Lucena" authorityYear="1987" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="28" pageNumber="221" phylum="Chordata" rank="species" species="gibbosus">
<emphasis id="B919B43C34663F5EFA61FDB8FC5B622A" italics="true" pageId="28" pageNumber="221">C. gibbosus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6D13AD34663F5EFC67FD98FC3F622B" box="[924,1012,1009,1032]" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="28" pageNumber="221" phylum="Chordata" rank="species" species="niger">
<emphasis id="B919B43C34663F5EFC67FD98FC3F622B" box="[924,1012,1009,1032]" italics="true" pageId="28" pageNumber="221">C. niger</emphasis>
