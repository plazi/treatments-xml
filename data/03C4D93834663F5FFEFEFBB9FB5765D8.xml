</taxonomicName>
, 
<taxonomicName id="4C6D13AD34663F5EFA61FDD8FC5065CB" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="28" pageNumber="221" phylum="Chordata" rank="species" species="apurensis">
