</taxonomicName>
, 
<taxonomicName id="4C6D13AD34663F5EFAF8FDD8FA4065EA" authorityName="Lucena" authorityYear="1987" box="[1283,1419,945,969]" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="28" pageNumber="221" phylum="Chordata" rank="species" species="notulatus">
