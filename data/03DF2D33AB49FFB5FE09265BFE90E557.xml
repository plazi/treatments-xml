<heading id="D0812B49AB49FFB2FE09265BFC86E20A" box="[409,895,581,605]" centered="true" fontSize="10" level="2" pageId="11" pageNumber="2260" reason="5">
<taxonomicName id="4C76E7A6AB49FFB2FE09265BFCCCE20A" authority="Marino &amp; Spinelli, 2004" authorityName="Marino &amp; Spinelli" authorityYear="2004" box="[409,821,581,605]" class="Insecta" family="Ceratopogonidae" genus="Forcipomyia" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="2260" phylum="Arthropoda" rank="species" species="tehuelche" status="sp. n." subGenus="Trichohelea">
<emphasis id="B9024037AB49FFB2FE09265BFCCCE20A" bold="true" box="[409,821,581,605]" pageId="11" pageNumber="2260">
<emphasis id="B9024037AB49FFB2FE09265BFDDCE20A" bold="true" box="[409,549,581,605]" italics="true" pageId="11" pageNumber="2260">Forcipomyia</emphasis>
(
<emphasis id="B9024037AB49FFB2FDA8265BFD43E20A" bold="true" box="[568,698,581,605]" italics="true" pageId="11" pageNumber="2260">Trichohelea</emphasis>
