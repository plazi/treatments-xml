Subgenus 
<taxonomicName id="4C364D3A2471FF9CFD08FB69FC0EFB10" authority="Poisson, 1938" authorityName="Poisson" authorityYear="1938" box="[703,1006,1232,1258]" class="Insecta" genus="Mesonecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
