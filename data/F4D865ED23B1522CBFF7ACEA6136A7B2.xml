<paragraph id="2CA4DD23D87F4DD634A34705A023F1B1" pageId="0" pageNumber="123">
<emphasis id="25532DAF98F689689FB85A04EA7AC804" bold="true" italics="true" pageId="0" pageNumber="123">Metasoma</emphasis>
1.19
<normalizedToken id="3ADC46F3EDAEB24D0A307A3FFCD533FF" originalValue="×">x</normalizedToken>
