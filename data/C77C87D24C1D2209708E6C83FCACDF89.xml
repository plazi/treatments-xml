<subSubSection id="07CF654F4C18220970DE6FB2FCACDF89" box="[199,792,188,215]" pageId="12" pageNumber="348" type="distribution">
<paragraph id="4F6A36C44C18220970DE6FB2FCACDF89" blockId="12.[199,1209,152,215]" box="[199,792,188,215]" pageId="12" pageNumber="348">
<emphasis id="7DA1EAD64C18220970DE6FB2FED5DF89" bold="true" box="[199,353,188,215]" pageId="12" pageNumber="348">Distribution.</emphasis>
<collectingCountry id="37C276544C182209717E6FB3FE1EDF89" box="[359,426,189,215]" name="China" pageId="12" pageNumber="348">China</collectingCountry>
(
<collectingRegion id="8D11F8264C18220971A06FB3FDA7DF89" box="[441,531,189,215]" country="China" name="Yunnan" pageId="12" pageNumber="348">Yunnan</collectingRegion>
, 
