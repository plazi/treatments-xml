<pageBreakToken id="6C185C6837D18DA8D515D6B02384CBC9" pageId="0" pageNumber="93" start="start">2</pageBreakToken>
. 
<taxonomicName id="F1DAC48C12EA460D4F997B2154982171" authority="Latzel, 1880" authorityName="Latzel" authorityYear="1880" class="Chilopoda" family="Lithobiidae" genus="Polybothrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lithobiida" pageId="0" pageNumber="93" phylum="Arthropoda" rank="species" species="ochraceus">Polybothrus ochraceus</taxonomicName>
