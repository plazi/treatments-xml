<collectingCountry id="ACF5766AFFC5F269FD809A75FD78F7D3" box="[606,696,2066,2092]" name="China" pageId="4" pageNumber="166">CHINA</collectingCountry>
, 
<collectingRegion id="1626F818FFC5F269FD1D9A75FCE0F7D3" box="[707,800,2066,2092]" country="China" name="Sichuan" pageId="4" pageNumber="166">Sichuan</collectingRegion>
, 
<collectingCounty id="3D3C4E76FFC5F269FCF29A75FCB8F7D3" box="[812,888,2066,2092]" pageId="4" pageNumber="166">Dêqên</collectingCounty>
, 
<collectingMunicipality id="3439AC80FFC5F269FC5D9A75FC29F7D3" box="[899,1001,2066,2092]" pageId="4" pageNumber="166">Benzilan</collectingMunicipality>
, 
<location id="D13D6021FFC5F269FC2D9A75FBB8F7D3" LSID="urn:lsid:plazi:treatment:5C4B87ECFFC3F269FF5695F3FAE5F7D3:D13D6021FFC5F269FC2D9A75FBB8F7D3" box="[1011,1144,2066,2092]" country="China" county="Deqen" municipality="Benzilan" name="Yeri village" pageId="4" pageNumber="166" stateProvince="Sichuan">Yeri village</location>
, 
<quantity id="131A9B1FFFC5F269FB5D9A75FAE1F7D3" box="[1155,1313,2066,2092]" metricMagnitude="3" metricUnit="m" metricValue="2.5" metricValueMax="2.6" metricValueMin="2.4" pageId="4" pageNumber="166" unit="m" value="2500.0" valueMax="2600.0" valueMin="2400.0">
<elevation id="5FCFD1C9FFC5F269FB5D9A75FAE1F7D3" box="[1155,1313,2066,2092]" metricMagnitude="3" metricUnit="m" metricValue="2.5" metricValueMax="2.6" metricValueMin="2.4" pageId="4" pageNumber="166" unit="m" value="2500.0" valueMax="2600.0" valueMin="2400.0">2400–2600 m</elevation>
