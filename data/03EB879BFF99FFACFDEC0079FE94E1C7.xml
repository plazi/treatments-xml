<taxonomicName id="4C424D0EFF98FFAAFC3D016BFB10E466" box="[1013,1180,1894,1919]" class="Hydrozoa" family="Ptychogastriidae" genus="Tesserogastria" kingdom="Animalia" order="Trachymedusae" pageId="11" pageNumber="12" phylum="Cnidaria" rank="genus">Tesserogastria</taxonomicName>
