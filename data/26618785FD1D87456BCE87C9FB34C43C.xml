<subSubSection id="E6D26518FD1D87456BCE82EAFB34C43C" pageId="5" pageNumber="4" type="etymology">
<paragraph id="AE773693FD1D87456BCE82EAFB68C5BF" blockId="5.[192,1234,1250,1274]" box="[192,1234,1250,1274]" pageId="5" pageNumber="4">
<emphasis id="9CBCEA81FD1D87456BCE82EAFEE6C5BF" bold="true" box="[192,348,1250,1274]" pageId="5" pageNumber="4">Etymology.</emphasis>
