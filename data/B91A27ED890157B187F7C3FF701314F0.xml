<taxonomicName id="4C126A94D7D7873FE7E4EDF951FE230D" authorityName="M. P. S. Camara, M. E. Palm &amp; A. W. Ramaley, Mycol. Res. 107 (5): 519" authorityYear="2003" class="Dothideomycetes" family="Phaeosphaeriaceae" genus="Phaeosphaeriopsis" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Phaeosphaeriopsis" order="Pleosporales" pageId="0" pageNumber="59" phylum="Ascomycota" rank="genus">
<emphasis id="BA2F78DFFA1032BB01B5ADA7FD0CDDEA" italics="true" pageId="0" pageNumber="59">Phaeosphaeriopsis</emphasis>
</taxonomicName>
