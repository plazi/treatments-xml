<taxonomicName id="631DCD9642BF68C18A0F911252F3B79B" authorityName="M. P. S. Camara, M. E. Palm &amp; A. W. Ramaley, Mycol. Res. 107 (5): 519" authorityYear="2003" class="Dothideomycetes" family="Phaeosphaeriaceae" genus="Phaeosphaeriopsis" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Phaeosphaeriopsis" order="Pleosporales" pageId="0" pageNumber="59" phylum="Ascomycota" rank="genus">
<emphasis id="DAFE7E7DF7E7564E8D82109533C1C099" italics="true" pageId="0" pageNumber="59">Phaeosphaeriopsis</emphasis>
</taxonomicName>
