<taxonomicName id="16AECEBCC353BA13FE79F9848352F99D" authorityName="Linnaeus" authorityYear="1758" box="[502,590,1593,1619]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
