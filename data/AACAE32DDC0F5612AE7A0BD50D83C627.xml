<subSubSection id="903F9F3F260419B263F53B920A59CA87" pageId="0" pageNumber="1" type="materials_examined">
<paragraph id="57C72545203DA1D43AC4DFCFD3D844AC" pageId="0" pageNumber="1">Material examined.</paragraph>
<paragraph id="53DD91F4BE152032B459946034AE6B21" pageId="0" pageNumber="1">
