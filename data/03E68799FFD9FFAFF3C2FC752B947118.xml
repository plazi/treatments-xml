<figureCitation id="13742A0AFFD9FFAFF1BCFB2A2D4170D3" box="[745,841,1239,1260]" captionStart="FIGURE 189" captionStartId="244.[151,244,627,648]" captionTargetBox="[184,1402,207,594]" captionTargetId="figure-44@244.[178,1410,191,605]" captionTargetPageId="244" captionText="FIGURE 189. Muiralevu telum, holotype ♂." pageId="58" pageNumber="59">Fig. 189h</figureCitation>
