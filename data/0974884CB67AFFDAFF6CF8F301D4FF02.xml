<taxonomicName id="46DD42D9B679FFDBFEE4F9E300E9F98D" authority="(Green, 1909)" authorityName="Green" authorityYear="1909" baseAuthorityName="Green" baseAuthorityYear="1909" box="[287,674,1605,1627]" class="Insecta" family="Coccidae" genus="Drepanococcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="chiton">
