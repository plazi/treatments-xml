<taxonomicName id="D3B858051D2215B296B80E3ED875FBDB" authority="Li, 2002" authorityName="Li" authorityYear="2002" class="Insecta" family="Psocidae" genus="Pentablaste" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pentablaste obconica" order="Psocodea" pageId="6" pageNumber="33" phylum="Arthropoda" rank="species" species="obconica">Pentablaste obconica</taxonomicName>
