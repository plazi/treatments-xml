, 
<taxonomicName id="B2D7E04E78FB2A27E4907FA60816CE98" baseAuthorityName="L. Pfeiffer" baseAuthorityYear="1857" class="Gastropoda" family="Bulimulidae" genus="Leiostracus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leiostracus sarcochilus" order="Pulmonata" pageId="27" pageNumber="44" phylum="Mollusca" rank="species" species="sarcochilus">
