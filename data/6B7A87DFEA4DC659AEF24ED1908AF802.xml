<number id="551B24EEEA4DC659AEAB4FE1904BFE35" box="[206,272,423,449]" pageId="21" pageNumber="22" value="2004.0">2004</number>
</date>
, M. Hull, reared in Caernarfon or Leiden; Genitalia slides ɗ 
<number id="551B24EEEA4DC659AD9F4FE1956CFE35" box="[1018,1079,423,449]" pageId="21" pageNumber="22" value="3552.0">3552</number>
, Ψ 
<number id="551B24EEEA4DC659AA014FE195FAFE35" box="[1124,1185,423,449]" pageId="21" pageNumber="22" value="3626.0">3626</number>
, Ψ 
<number id="551B24EEEA4DC659AAAB4FE19457FE35" box="[1230,1292,423,449]" pageId="21" pageNumber="22" value="3642.0">3642</number>
, Wing slide 
<number id="551B24EEEA4DC659AEF24F88918FFE1C" box="[151,212,462,488]" pageId="21" pageNumber="22" value="3645.0">3645</number>
, Larval slides 
<collectionCode id="85C2AE0CEA4DC659AF184F88908EFE1C" LSID="urn:lsid:biocol.org:col:34992" box="[381,469,462,488]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="21" pageNumber="22">RMNH</collectionCode>
<collectionCode id="85C2AE0CEA4DC659AFB84F889357FE1C" box="[477,524,462,488]" pageId="21" pageNumber="22">INS</collectionCode>
no 
<number id="551B24EEEA4DC659AC5D4F889382FE1C" box="[568,729,462,488]" pageId="21" pageNumber="22" value="12393.0" valueMax="12394.0" valueMin="12392.0">12392–12394</number>
(
<collectionCode id="85C2AE0CEA4DC659AC8D4F88921EFE1C" LSID="urn:lsid:biocol.org:col:34992" box="[744,837,462,488]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="21" pageNumber="22">RMNH</collectionCode>
, 
<collectionCode id="85C2AE0CEA4DC659AD344F8892F7FE1C" box="[849,940,462,488]" pageId="21" pageNumber="22">BMNH</collectionCode>
