<paragraph id="0C21F27ABE290C5D32D0FD84F9FFFDC4" blockId="15.[144,664,557,620]" box="[144,664,596,620]" pageId="15" pageNumber="2324">
<collectingCountry id="7489B2EABE290C5D32D0FD84FA66FDC4" box="[144,257,596,620]" name="Indonesia" pageId="15" pageNumber="2324">Indonesia</collectingCountry>
