<emphasis id="96AAAE034438FFB6FF32F579FDD7DF37" bold="true" box="[151,536,1885,1911]" pageId="25" pageNumber="300">
Genus 
<taxonomicName id="63DE09924438FFB6FF4BF579FDD7DF37" authority="Uchida, 1978" authorityName="Uchida" authorityYear="1978" box="[238,536,1885,1911]" class="Polychaeta" family="Serpulidae" genus="Paraprotis" kingdom="Animalia" order="Sabellida" pageId="25" pageNumber="300" phylum="Annelida" rank="genus">
<emphasis id="96AAAE034438FFB6FF4BF579FEA2DF37" bold="true" box="[238,365,1885,1911]" italics="true" pageId="25" pageNumber="300">Paraprotis</emphasis>
<bibRefCitation id="C04F0FE04438FFB6FED1F579FDD7DF37" author="Uchida" box="[372,536,1885,1911]" pageId="25" pageNumber="300" refString="Uchida, H. (1978) Serpulid tube worms (Polychaeta, Sedentaria) from Japan with the systematic review of the group. Bulletin of the Marine Park Research Stations, 2, 1 - 98." type="journal article" year="1978">
Uchida, 
<number id="121660364438FFB6FE78F579FDD7DF37" box="[477,536,1885,1911]" pageId="25" pageNumber="300" value="1978.0">1978</number>
</bibRefCitation>
