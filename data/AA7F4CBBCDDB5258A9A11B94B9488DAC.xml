<taxonomicName id="10F9853CBE2800AE068ADF29B13112C2" authorityName="Bremek" authorityYear="1934" class="Magnoliopsida" family="Rubiaceae" genus="Pavetta" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pavetta bauchei" order="Gentianales" pageId="0" pageNumber="99" phylum="Tracheophyta" rank="species" species="bauchei">
<emphasis id="16186DA5E42345B929A22DC404FB1096" italics="true" pageId="0" pageNumber="99">Pavetta bauchei</emphasis>
</taxonomicName>
Bremek., 
<taxonomicName id="7C1EA14E7DBCACF6AD1A888C560F9832" authorityName="King &amp; Gamble" authorityYear="1904" class="Magnoliopsida" family="Rubiaceae" genus="Lasianthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lasianthus curtisii" order="Gentianales" pageId="0" pageNumber="99" phylum="Tracheophyta" rank="species" species="curtisii">
<emphasis id="CB69837A8B0709A850186F29A458212B" italics="true" pageId="0" pageNumber="99">Lasianthus curtisii</emphasis>
</taxonomicName>
King &amp; Gamble, 
<taxonomicName id="E2267ECD30629A0B32DC79D9AA5F7D2A" class="Magnoliopsida" family="Annonaceae" genus="Popowia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Popowia pisocarpa" order="Magnoliales" pageId="0" pageNumber="99" phylum="Tracheophyta" rank="species" species="pisocarpa">
<emphasis id="0430FA9AC4EB3D8388570F72B4E1E7FD" italics="true" pageId="0" pageNumber="99">Popowia pisocarpa</emphasis>
</taxonomicName>
(Blume) Endl. ex Walp., 
<taxonomicName id="E494D0C206815F8F04066FF9352E9D99" class="Magnoliopsida" family="Annonaceae" genus="Fissistigma" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fissistigma taynguyenense" order="Magnoliales" pageId="0" pageNumber="99" phylum="Tracheophyta" rank="species" species="taynguyenense">
<emphasis id="8C24D88ACA153E6DE32EEA8ED0434BD4" italics="true" pageId="0" pageNumber="99">Fissistigma taynguyenense</emphasis>
