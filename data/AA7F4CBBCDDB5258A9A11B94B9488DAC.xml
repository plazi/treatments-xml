<emphasis id="E15045B9AE817A5A0C01690C458AB41F" italics="true" pageId="0" pageNumber="99">Ardisia verbascifolia</emphasis>
</taxonomicName>
Mez and 
<taxonomicName id="5F96DB637CA6599935EDF2B8EE3841DD" class="Magnoliopsida" family="Lauraceae" genus="Litsea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Litsea clemensii" order="Laurales" pageId="0" pageNumber="99" phylum="Tracheophyta" rank="species" species="clemensii">
