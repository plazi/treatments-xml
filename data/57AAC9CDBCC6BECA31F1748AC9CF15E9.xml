<taxonomicName id="1C8DD84F5985D24C01ABC7BCA4BF08B3" LSID="http://zoobank.org/5CAE39E6-B047-4F0A-8DE0-679FDC1246F1" authority="Marsh" authorityName="Marsh, Wild &amp; Whitfield" authorityYear="2013" class="Insecta" family="Braconidae" genus="Paraheterospilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paraheterospilus eumekus" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="eumekus">Paraheterospilus eumekus Marsh</taxonomicName>
