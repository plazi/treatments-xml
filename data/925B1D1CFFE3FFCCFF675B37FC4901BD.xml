<location id="1F2DFAD1FFE3FFCCFBE359F7FB150181" LSID="urn:lsid:plazi:treatment:925B1D1CFFE3FFCCFF675B37FC4901BD:1F2DFAD1FFE3FFCCFBE359F7FB150181" box="[1043,1225,1972,1997]" country="Japan" name="central Honshu" pageId="6" pageNumber="207" stateProvince="Okinawa">central Honshu</location>
