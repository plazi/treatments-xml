<taxonomicName id="DDF2D789FFE3FFCCFDAB59F7FD6C0181" box="[603,688,1972,1997]" class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="6" pageNumber="207" phylum="Tracheophyta" rank="genus">
