<taxonomicName id="4C5B35DD81890D9C274AAC13FBEEF808" box="[875,1120,1901,1926]" class="Magnoliopsida" family="Meliaceae" genus="Dysoxylum" kingdom="Plantae" order="Sapindales" pageId="162" pageNumber="163" phylum="Tracheophyta" rank="species" species="spectabile">
