<typeStatus id="54E0F0FC81890D9C24B6AC10FF5CF80B" box="[151,210,1902,1925]" pageId="162" pageNumber="163">Type</typeStatus>
details. 
<collectingCountry id="F34C0ECE81890D9C2515AC10FD89F808" box="[308,519,1901,1926]" name="New Zealand" pageId="162" pageNumber="163">NEW ZEALAND</collectingCountry>
