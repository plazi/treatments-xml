<location id="661353D1FFA7E43C6940FCC0D48D96AF" LSID="urn:lsid:plazi:treatment:EB65B41CFFA7E43B6A4DFDDBD37D94A0:661353D1FFA7E43C6940FCC0D48D96AF" box="[517,748,879,905]" country="United Kingdom" county="Virginia" latitude="37.5" longLatPrecision="1181" longitude="-72.5" municipality="New" name="Northwest Atlantic" pageId="58" pageNumber="59">Northwest Atlantic</location>
