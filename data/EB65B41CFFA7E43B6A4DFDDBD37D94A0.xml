, off 
<collectingCounty id="8A127D86FFA7E43C6F4AFCC0D21096AF" box="[1039,1137,879,905]" pageId="58" pageNumber="59">Virginia</collectingCounty>
, approximately 
<geoCoordinate id="06F863CDFFA7E43C6A4DFC38D70C9697" box="[264,365,919,945]" degrees="37" direction="north" minutes="30" orientation="latitude" pageId="58" pageNumber="59" precision="925" value="37.5">37°30’N</geoCoordinate>
