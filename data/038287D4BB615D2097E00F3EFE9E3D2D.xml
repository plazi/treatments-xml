<taxonomicName id="4C2B4D41BB615D2094670FB8FC2C3FCD" authority="Hall, 1937" authorityName="Hall" authorityYear="1937" box="[784,1009,1236,1261]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="bomplandi">
