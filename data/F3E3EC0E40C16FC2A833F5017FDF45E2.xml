<taxonomicName id="0C86465EB64E415BEA91D6E68CCFFDE1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31741" class="Insecta" family="Formicidae" genus="Monomorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Monomorium termitobium Forel" lsidName-HNS="Monomorium termitobium Forel" order="Hymenoptera" phylum="Arthropoda" rank="species" species="termitobium">Monomorium termitobium</taxonomicName>
<taxonomicNameLabel id="586C2EC5ABDFD67C4922FF2986DA6771" rank="species">n. sp.</taxonomicNameLabel>
