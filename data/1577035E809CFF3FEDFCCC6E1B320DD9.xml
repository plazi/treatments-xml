<collectingCountry id="E5C9F2D8809CFF3FEC13CD3618920D44" box="[376,463,799,824]" name="Kiribati" pageId="152" pageNumber="153">Kiribati</collectingCountry>
(
<collectingRegion id="5F1A7CAA809CFF3FEC8ACD361B2D0D44" box="[481,624,799,824]" country="Kiribati" name="Line Islands" pageId="152" pageNumber="153">Line Islands</collectingRegion>
