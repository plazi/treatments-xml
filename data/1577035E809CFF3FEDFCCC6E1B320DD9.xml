<quantity id="5A261FAD809CFF3FE993CD4E1C130D03" box="[1272,1358,871,896]" metricMagnitude="1" metricUnit="m" metricValue="3.75" metricValueMax="7.5" metricValueMin="0.0" pageId="152" pageNumber="153" unit="m" value="37.5" valueMax="75.0" valueMin="0.0">0–75 m</quantity>
