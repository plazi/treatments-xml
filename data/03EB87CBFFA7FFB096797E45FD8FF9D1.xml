<quantity id="4CBA9B38FFA7FFB097477E85FCFCFB5E" box="[773,830,1165,1189]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="15" pageNumber="3287" unit="m" value="20.0">20 m</quantity>
