<taxonomicName authority="Schulz, 1907" authorityName="Schulz" authorityYear="1907" class="Insecta" family="Trigonalyidae" genus="Bareogonalos" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bareogonalos huisuni" order="Hymenoptera" pageId="2" pageNumber="19" phylum="Arthropoda" rank="species" species="huisuni">Bareogonalos huisuni</taxonomicName>
