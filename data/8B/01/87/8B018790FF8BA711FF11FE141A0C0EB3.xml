<quantity box="[1210,1376,799,823]" metricMagnitude="-4" metricUnit="m" metricValue="4.75" metricValueMax="4.8" metricValueMin="4.699999999999999" pageId="9" pageNumber="536" unit="mm" value="0.475" valueMax="0.48" valueMin="0.47">0.47–0.48 mm</quantity>
