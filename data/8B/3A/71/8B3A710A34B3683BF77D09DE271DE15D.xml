<title pageId="null" pageNumber="125">
<subSubSection pageId="null" pageNumber="125" type="nomenclature">
<paragraph pageId="null" pageNumber="125">
<taxonomicName authority="L." class="Magnoliopsida" family="Lamiaceae" genus="Lamium" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="125" phylum="Tracheophyta" rank="species" species="maculatum">
