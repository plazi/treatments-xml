<subSubSection pageId="0" pageNumber="1" type="nomenclature">
<paragraph pageId="0" pageNumber="1">
<taxonomicName LSID="8B4FD163-E57D-5E1A-8170-A8C244097CE9" authority="Elsayed &amp; Tokuda" authorityName="Elsayed &amp; Tokuda" authorityYear="2020" class="Lecanoromycetes" family="Cecidomyiidae" genus="Massalongia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Massalongia nakamuratetsui" order="Diptera" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="nakamuratetsui" status="sp. nov.">Massalongia nakamuratetsui Elsayed &amp; Tokuda</taxonomicName>
<taxonomicNameLabel pageId="0" pageNumber="1">sp. nov.</taxonomicNameLabel>
</paragraph>
