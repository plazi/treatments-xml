<taxonomicName authority="Peart, 2007" authorityName="Peart" authorityYear="2007" box="[276,387,1831,1856]" class="Malacostraca" family="Ampithoidae" genus="Ampithoe" kingdom="Animalia" order="Amphipoda" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="hyalos" status="sp. nov.">
<emphasis box="[276,387,1831,1856]" italics="true" pageId="76" pageNumber="77">A. hyalos</emphasis>
</taxonomicName>
<emphasis bold="true" box="[395,489,1830,1856]" pageId="76" pageNumber="77">
<taxonomicNameLabel box="[395,489,1830,1856]" pageId="76" pageNumber="77" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
Characters distinguishing 
<taxonomicName authorityName="Peart" authorityYear="2007" box="[805,885,1831,1856]" class="Malacostraca" family="Ampithoidae" genus="Ampithoe" kingdom="Animalia" order="Amphipoda" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="roly">
<emphasis box="[805,885,1831,1856]" italics="true" pageId="76" pageNumber="77">A. roly</emphasis>
</taxonomicName>
