— 
<collectingCountry box="[710,779,506,530]" name="China" pageId="7" pageNumber="494">China</collectingCountry>
(
<collectingRegion box="[794,880,506,530]" country="China" name="Jiangxi" pageId="7" pageNumber="494">Jiangxi</collectingRegion>
