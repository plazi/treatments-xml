<collectingCountry box="[710,779,575,599]" name="China" pageId="13" pageNumber="500">China</collectingCountry>
(
<collectingRegion box="[794,880,575,599]" country="China" name="Jiangxi" pageId="13" pageNumber="500">Jiangxi</collectingRegion>
