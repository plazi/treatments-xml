<location LSID="urn:lsid:plazi:treatment:8B57EE20FFFBB31CB4CC3DF842578CFF:062109EDFFFBB302B5343E9942E48ED5" box="[367,411,1750,1775]" country="Iceland" municipality="Yasawa Group" name="The" pageId="15" pageNumber="340">The</location>
