<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="7" pageNumber="328" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
