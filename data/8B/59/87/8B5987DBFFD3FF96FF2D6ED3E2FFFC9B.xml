<subSubSection pageId="2" type="key">
<keyStep pageId="2">
<paragraph blockId="2.[151,1436,222,772]" box="[151,1435,222,243]" pageId="2">
<keyLead box="[151,1435,222,243]" pageId="2">1. Glandiductor asymmetrical............................................................................. 2</keyLead>
</paragraph>
<paragraph blockId="2.[151,1436,222,772]" box="[151,1435,251,272]" pageId="2">
<keyLead box="[151,1435,251,272]" pageId="2">- Glandiductor symmetrical.............................................................................. 4</keyLead>
</paragraph>
</keyStep>
