<taxonomicNameLabel box="[782,839,1080,1101]" pageId="2" pageNumber="360" rank="species">
<emphasis bold="true" box="[782,839,1080,1101]" pageId="2" pageNumber="360">sp. n.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName box="[889,1010,1079,1101]" class="Secernentea" family="Molineidae" genus="Oswaldocruzia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Strongylida" pageId="2" pageNumber="360" phylum="Nematoda" rank="species" species="lacertica" status="sp. n.">
