<paragraph pageId="null" pageNumber="525">
<pageBreakToken pageId="null" pageNumber="525" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName authority="L." class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="525" phylum="Tracheophyta" rank="species" species="pratense">
Trifolium 
<normalizedToken originalValue="praténse" pageId="null" pageNumber="525">pratense</normalizedToken>
<authorityName pageId="null" pageNumber="525">L.</authorityName>
</taxonomicName>
</paragraph>
