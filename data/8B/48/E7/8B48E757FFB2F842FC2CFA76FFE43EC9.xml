species: 
<taxonomicName authorityName="Criscione &amp; Hallan &amp; Puillandre &amp; Fedosov" authorityYear="2021" box="[983,1281,1677,1699]" class="Gastropoda" family="Raphitomidae" genus="Glaciotomella" kingdom="Animalia" order="Neogastropoda" pageId="23" pageNumber="984" phylum="Mollusca" rank="species" species="investigator">Glaciotomella investigator</taxonomicName>
