, holotype AMS C.571611; B, 
<taxonomicName authorityName="Criscione &amp; Hallan &amp; Puillandre &amp; Fedosov" authorityYear="2021" class="Gastropoda" family="Raphitomidae" genus="Globodaphne" kingdom="Animalia" order="Neogastropoda" pageId="17" pageNumber="978" phylum="Mollusca" rank="species" species="pomum">
<emphasis italics="true" pageId="17" pageNumber="978">Globodaphne pomum</emphasis>
</taxonomicName>
, holotype AMS C.482283; C, 
<taxonomicName authorityName="Criscione" authorityYear="2020" box="[542,717,1368,1390]" class="Gastropoda" family="Raphitomidae" genus="Pagodibela" kingdom="Animalia" order="Neogastropoda" pageId="17" pageNumber="978" phylum="Mollusca" rank="species" species="maia">
