<quantity id="4CE4EAD8471CFF8EE299FB29A6D89A72" box="[140,214,1276,1300]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" pageId="8" pageNumber="838" unit="mm" value="45.0">45 mm</quantity>
