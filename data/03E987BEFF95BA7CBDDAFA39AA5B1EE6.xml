<collectingCountry id="F3577638FF95BA7CB91AF92DAD821E9E" box="[1111,1179,1666,1691]" name="Iceland" pageId="27" pageNumber="167">Island</collectingCountry>
near 
<collectingCountry id="F3577638FF95BA7CB992F92DAC3C1E9E" box="[1247,1317,1666,1691]" name="China" pageId="27" pageNumber="167">China</collectingCountry>
