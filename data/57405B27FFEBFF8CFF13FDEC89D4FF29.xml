<taxonomicName id="18E991B2FFEAFFB3FEB7F93B8827F8F5" authority="(Kieffer)" baseAuthorityName="Kieffer" baseAuthorityYear="1921" box="[307,840,1775,1799]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="scirpicola" subGenus="Pentapedilum">
