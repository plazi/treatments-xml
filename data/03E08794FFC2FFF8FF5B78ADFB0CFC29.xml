<typeStatus id="54F28820FFC2FFFFFF5B78F2FF28FACC" box="[151,201,1266,1290]" pageId="3" pageNumber="198">Type</typeStatus>
species: 
<taxonomicName id="4C494D01FFC2FFFFFEEA78F2FD6EFACC" authority="Riley, 1881" authorityName="Riley" authorityYear="1881" box="[294,655,1265,1290]" class="Insecta" family="Tortricidae" genus="Conchylis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="198" phylum="Arthropoda" rank="species" species="oenotherana">
<emphasis id="B93DEA90FFC2FFFFFEEA78F2FDF3FACC" box="[294,530,1266,1290]" italics="true" pageId="3" pageNumber="198">Conchylis oenotherana</emphasis>
