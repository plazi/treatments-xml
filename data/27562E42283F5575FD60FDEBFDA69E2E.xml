<materialsCitation id="1F979509283E5572FEFDFACFFEF098F2" ID-GBIF-Occurrence="2609528150" collectionCode="WAMC" pageId="19" pageNumber="20" specimenCode="WAMC55620" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
