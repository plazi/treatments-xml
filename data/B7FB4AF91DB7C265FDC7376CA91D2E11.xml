<paragraph id="8679944BAC6FB24A43E7EF927B995139" pageId="77" pageNumber="78">
<taxonomicName id="4190B022955679AB27249A9C789EA5D1" class="Arachnida" family="Nanhermanniidae" genus="Nanhermannia" kingdom="Animalia" order="Sarcoptiformes" pageId="77" pageNumber="78" phylum="Arthropoda" rank="genus">Nanhermannia</taxonomicName>
