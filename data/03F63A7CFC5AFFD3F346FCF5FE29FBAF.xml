<geoCoordinate id="EE6BEDADFC5AFFD3F683FCC4FB39FCA3" box="[1169,1244,818,838]" degrees="99" direction="east" minutes="45" orientation="longitude" pageId="27" pageNumber="1036" precision="925" value="99.75">99°45‘E</geoCoordinate>
