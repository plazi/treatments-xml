<number id="3D9E249EFFE7FFEEFDE5B7A55CBEFCCE" box="[562,683,732,758]" pageId="26" pageNumber="27" value="2.64" valueMax="2.72" valueMin="2.56">2.56–2.72</number>
, 
<quantity id="4CAE9B5CFFE7FFEEFD61B7A55D0AFCCE" box="[694,799,732,758]" metricMagnitude="-3" metricUnit="m" metricValue="2.68" pageId="26" pageNumber="27" unit="mm" value="2.68">2.68 mm</quantity>
long; tergite 
<number id="3D9E249EFFE7FFEEFC6CB7A55DDFFCCE" box="[955,970,732,758]" pageId="26" pageNumber="27" value="5.0">5</number>
