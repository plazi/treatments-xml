<quantity id="4CAE9B5CFFE7FFEEFB83B7A55AA8FCCE" box="[1108,1213,732,758]" metricMagnitude="-4" metricUnit="m" metricValue="7.3" pageId="26" pageNumber="27" unit="mm" value="0.73">0.73 mm</quantity>
