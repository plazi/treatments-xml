<number id="3D9E249EFFE7FFEEFDFFB1655C23FA0E" box="[552,566,1052,1078]" pageId="26" pageNumber="27" value="2.0">2</number>
yellow; tergite 
<number id="3D9E249EFFE7FFEEFD20B1655D10FA0E" box="[759,773,1052,1078]" pageId="26" pageNumber="27" value="4.0">4</number>
