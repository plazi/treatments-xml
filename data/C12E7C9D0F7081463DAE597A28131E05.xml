group: 
<taxonomicName id="E34E1F3B0853C8173C48B4B2A95FA698" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181999" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole gibbata Borgmeier" lsidName-HNS="Pheidole gibbata Borgmeier" order="Hymenoptera" pageNumber="645" phylum="Arthropoda" rank="species" species="gibbata">gibbata</taxonomicName>
