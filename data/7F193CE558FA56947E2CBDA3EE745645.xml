<taxonomicName id="82FE13B9DE46DA6601423543219C7CF4" authority="Hoppeanum Koch" class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="479" phylum="Tracheophyta" rank="genus">
