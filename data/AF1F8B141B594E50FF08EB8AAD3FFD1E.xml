<taxonomicName id="E0B641811B594E57FB5AED50A956F9E8" box="[1221,1360,1613,1636]" class="Insecta" family="Asilidae" genus="Laphria" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="mitsukurri">
