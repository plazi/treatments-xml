<taxonomicName id="4C2C71FEFFC69F2CFF45FB91FEA4A9C3" authorityName="Roxb., Flora Indica" authorityYear="1832" box="[189,361,723,750]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="luducca">
