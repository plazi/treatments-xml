, female, Jianshi National Forest Park, Tian’er County (
<geoCoordinate id="EE373DD9624AFFCFFBD3FE67FB07335C" box="[1091,1190,490,516]" degrees="25" direction="north" minutes="01" orientation="latitude" pageId="3" pageNumber="63" precision="925" value="25.016666">25°01'N</geoCoordinate>
, 
<geoCoordinate id="EE373DD9624AFFCFFB23FE67FA83335C" box="[1203,1314,490,516]" degrees="107" direction="east" minutes="16" orientation="longitude" pageId="3" pageNumber="63" precision="925" value="107.26667">107°16'E</geoCoordinate>
), 
<collectingRegion id="49C795FC624AFFCFFAA7FE67FF533373" country="China" name="Guangxi" pageId="3" pageNumber="63">Guangxi Zhuang</collectingRegion>
Autonomous Region, 
<collectingCountry id="F3141B8E624AFFCFFD96FD9CFDF03373" box="[518,593,529,555]" name="China" pageId="3" pageNumber="63">China</collectingCountry>
, 
<date id="FFBD7DDE624AFFCFFDCEFD9CFCAB3373" box="[606,778,529,555]" pageId="3" pageNumber="63" value="2006-08-18">18-Aug.-2006</date>
, from 
<taxonomicName id="4C03209D624AFFCFFCCAFD9CFC413373" box="[858,992,529,555]" class="Magnoliopsida" family="Rubiaceae" genus="Luculia" kingdom="Plantae" order="Gentianales" pageId="3" pageNumber="63" phylum="Tracheophyta" rank="species" species="undetermined">
