and 
<taxonomicName id="DE15F8981CF59393378E19E93A56A623" class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus tremula" order="Malpighiales" pageId="8" pageNumber="227" phylum="Tracheophyta" rank="species" species="tremula">Populus tremula</taxonomicName>
stand on N slope, 
<geoCoordinate id="914E9968B8FFCE6B567DF1DC45EB848A" direction="north" orientation="latitude" precision="1" value="51.359386">51°21'33.8&quot;N</geoCoordinate>
, 
<geoCoordinate id="3096CDED8007FEAA0603824839FB6F2E" direction="east" orientation="longitude" precision="1" value="83.623116">83°37'23.2&quot;E</geoCoordinate>
, 518 m a.s.l., 14.07.2015, leg. P.N.; 4 juv. (ASU), site 1 on S slope, soil sample 3 (0-10 cm deep), 1.06.2016; 2 ♀♀, 9 juv. (ASU), site 1 on S slope, soil sample 4 (0-10 cm deep), 1.06.2016; 1 juv. (ASU), site 1 on S slope, soil sample 5 (0-10 cm deep), 1.06.2016; 2 ♂♂, 1 ♀, 2 juv. (ASU), foot of S slope of mountain, 
<taxonomicName id="7E7283E9270B857669AEAF5C48D41DF0" authority="Turcz., 1843 (Mill.)" authorityName="Turcz., 1843 (Mill.)" class="Magnoliopsida" family="Rosaceae" genus="Padus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Padus avium" order="Rosales" pageId="8" pageNumber="227" phylum="Tracheophyta" rank="species" species="avium">Padus avium</taxonomicName>
and 
<taxonomicName id="C15A8B65EB0CE7819E350880DBBA9F49" class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus tremula" order="Malpighiales" pageId="8" pageNumber="227" phylum="Tracheophyta" rank="species" species="tremula">Populus tremula</taxonomicName>
