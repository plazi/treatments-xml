<td id="5C57A20FE961F6DBFE68FDFBFE563667" box="[401,460,525,549]" gridcol="2" gridrow="2" pageId="62" pageNumber="427">1-0</td>
<td id="5C57A20FE961F6DBFDD0FDFBFD7A3667" box="[553,736,525,549]" gridcol="3" gridrow="2" pageId="62" pageNumber="427">I-0; I-1; III, I, 5</td>
<td id="5C57A20FE961F6DBFCB3FDFBFC763667" box="[842,1004,525,549]" gridcol="4" gridrow="2" pageId="62" pageNumber="427">0-1; 0-2; III, 3</td>
</tr>
<tr id="1F86CB73E961F6DBFFB6FDDBFC763604" box="[79,1004,557,582]" gridrow="3" pageId="62" pageNumber="427">
<td id="5C57A20FE961F6DBFFB6FDDBFF0A3604" box="[79,144,557,582]" gridcol="0" gridrow="3" pageId="62" pageNumber="427">Leg 3</td>
<td id="5C57A20FE961F6DBFF01FDDBFEAE3604" box="[248,308,557,582]" gridcol="1" gridrow="3" pageId="62" pageNumber="427">0-1</td>
<td id="5C57A20FE961F6DBFE68FDDBFE563604" box="[401,460,557,582]" gridcol="2" gridrow="3" pageId="62" pageNumber="427">1-0</td>
<td id="5C57A20FE961F6DBFDD0FDDBFD7A3604" box="[553,736,557,582]" gridcol="3" gridrow="3" pageId="62" pageNumber="427">I-0; I-1; III, I, 5</td>
<td id="5C57A20FE961F6DBFCB3FDDBFC763604" box="[842,1004,557,582]" gridcol="4" gridrow="3" pageId="62" pageNumber="427">0-1; 0-2; III, 2</td>
</tr>
<tr id="1F86CB73E961F6DBFFB6FDBBFC763624" box="[79,1004,589,614]" gridrow="4" pageId="62" pageNumber="427">
<td id="5C57A20FE961F6DBFFB6FDBBFF0A3624" box="[79,144,589,614]" gridcol="0" gridrow="4" pageId="62" pageNumber="427">Leg 4</td>
<td id="5C57A20FE961F6DBFF01FDBBFEAE3624" box="[248,308,589,614]" gridcol="1" gridrow="4" pageId="62" pageNumber="427">0-1</td>
<td id="5C57A20FE961F6DBFE68FDBBFE563624" box="[401,460,589,614]" gridcol="2" gridrow="4" pageId="62" pageNumber="427">1-0</td>
