<taxonomicName id="C79F4D6A064AFF9E8AD733DFE56EFF2E" box="[450,611,151,177]" class="Insecta" family="Chironomidae" genus="Corynoneura" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="44" phylum="Arthropoda" rank="genus">
