<taxonomicName id="C79F4D6A064AFF9E8F593186E291FD7A" authority="Andersen et Saether" authorityName="Andersen et Saether" box="[1100,1436,718,741]" class="Insecta" family="Chironomidae" genus="Onconeura" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="44" phylum="Arthropoda" rank="genus">
