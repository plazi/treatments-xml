<quantity id="4CBECFA5FFD1FFD86DD074608F54D362" box="[440,533,786,812]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="10" pageNumber="11" unit="mm" value="0.8">0.8 mm</quantity>
