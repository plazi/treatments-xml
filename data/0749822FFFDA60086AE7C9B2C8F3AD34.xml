<taxonomicName id="48E048BAFFDA60086B4ECD8ECA31ACBC" box="[318,533,1699,1724]" class="Insecta" family="Ecnomidae" genus="Ecnomina" kingdom="Animalia" order="Trichoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="sentosa">
