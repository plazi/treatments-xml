<paragraph id="8B893687FFBEFF8BE4F8F970FA4CFA47" blockId="5.[151,1436,1705,2018]" pageId="5" pageNumber="118">
<emphasis id="B942EA95FFBEFF8BE4F8F970FE00FBAB" bold="true" box="[199,462,1705,1730]" pageId="5" pageNumber="118">Differential diagnosis.</emphasis>
<taxonomicName id="4C364D04FFBEFF8BE5E9F973FD5DFBAB" box="[470,659,1706,1730]" class="Insecta" family="Lycidae" genus="Ochinoeus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="118" phylum="Arthropoda" rank="species" species="huaphanensis" status="sp. nov.">
<emphasis id="B942EA95FFBEFF8BE5E9F973FD5DFBAB" box="[470,659,1706,1730]" italics="true" pageId="5" pageNumber="118">O. huaphanensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27157EEFFBEFF8BE6A4F970FD3BFBAB" box="[667,757,1705,1730]" pageId="5" pageNumber="118" rank="species">
