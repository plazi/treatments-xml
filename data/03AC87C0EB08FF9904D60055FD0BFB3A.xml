<taxonomicName id="4C054D55EB0BFF9905120691FE69FE65" authority="Bruand, 1846" authorityName="Bruand" authorityYear="1846" box="[339,434,423,448]" class="Insecta" family="Geometridae" genus="Nebula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="33" phylum="Arthropoda" rank="genus">Nebula</taxonomicName>
