and 
<emphasis id="B971EAC4EB0BFF9906750691FD35FE65" box="[564,750,423,448]" italics="true" pageId="10" pageNumber="33">
“ 
<taxonomicName id="4C054D55EB0BFF9906010691FD44FE65" authority="Bruand, 1846" authorityName="Bruand" authorityYear="1846" box="[576,671,423,448]" class="Insecta" family="Geometridae" genus="Nebula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="33" phylum="Arthropoda" rank="genus">Nebula</taxonomicName>
