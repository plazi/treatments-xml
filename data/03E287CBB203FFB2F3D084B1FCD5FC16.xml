<taxonomicName id="4C4B4D5EB202FFB2F7F5864DFF5EFC82" authority="Kell." authorityName="Kell." class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="41" pageNumber="42" phylum="Tracheophyta" rank="species" species="vaccinifolia">
