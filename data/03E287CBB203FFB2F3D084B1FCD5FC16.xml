<taxonomicName id="4C4B4D5EB202FFB2F1AA8629FB5FFCBD" authority="(Ashmead, 1896)" baseAuthorityName="Ashmead" baseAuthorityYear="1896" box="[749,1230,837,864]" class="Insecta" family="Cynipidae" genus="Disholcaspis" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="truckeensis">
