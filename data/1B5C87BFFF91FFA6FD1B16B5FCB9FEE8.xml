<taxonomicName id="54F54D2AFF95FFA8FE8014B3FE6CF8A1" authority="Joachim Bresseel &amp; Jérôme Constant, 2018" authorityName="Joachim Bresseel &amp; Jérôme Constant" authorityYear="2018" box="[279,422,1775,1799]" class="Insecta" family="Heteropterygidae" genus="Orestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phasmida" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="botot" status="sp. nov.">
