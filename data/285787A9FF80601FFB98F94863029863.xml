<quantity id="67069B5AFF80601FFA3EF84C677B97AB" box="[302,373,1994,2020]" metricMagnitude="0" metricUnit="m" metricValue="7.8232" pageId="28" pageNumber="29" unit="in" value="308.0">308 in</quantity>
