<taxonomicName id="9C2247B3EE36B18043BAD74096987262" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182095" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole zoster Wilson" lsidName-HNS="Pheidole zoster Wilson" order="Hymenoptera" pageNumber="699" phylum="Arthropoda" rank="species" species="zoster">zoster</taxonomicName>
