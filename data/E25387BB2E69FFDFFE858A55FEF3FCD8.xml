<taxonomicName id="ADFA4D2E2E69FFDFFD1589F1FE84FCC2" authority="Latham, 1801" authorityName="Latham" authorityYear="1801" class="Aves" family="Meropidae" genus="Merops" kingdom="Animalia" order="Coraciiformes" pageId="39" pageNumber="64" phylum="Chordata" rank="species" species="orientalis">
