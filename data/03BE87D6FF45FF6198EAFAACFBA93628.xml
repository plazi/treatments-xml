<quantity id="4CEF9B25FF45FF619D79FA1DFF0D360C" box="[1284,1356,1519,1545]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="212" pageNumber="213" unit="m" value="600.0">600 m</quantity>
