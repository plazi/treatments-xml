<treatmentCitationGroup id="AED411C8FFA2903FFF5D62B6FDA7FAA4" pageId="31" pageNumber="32">
<taxonomicName id="49C44D65FFA2903FFF5D62B6FD21FA86" authority="Borgmeier, 1950: 287" authorityName="Borgmeier" authorityPageNumber="287" authorityYear="1950" box="[151,656,1260,1282]" class="Insecta" family="Phoridae" genus="Coniceromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="disparivena">
<emphasis id="BCB0EAF4FFA2903FFF5D62B6FE15FA86" box="[151,420,1260,1282]" italics="true" pageId="31" pageNumber="32">Coniceromyia disparivena</emphasis>
<treatmentCitation id="0F6510F7FFA2903FFE6662B6FD21FA86" author="Borgmeier" box="[428,656,1260,1282]" page="287" pageId="31" pageNumber="32" year="1950">
<bibRefCitation id="EA554B17FFA2903FFE6662B6FDEAFA86" author="Borgmeier" box="[428,603,1260,1282]" pageId="31" pageNumber="32" refString="Borgmeier, T. (1950) Revision der Gattung Coniceromyia Borgmeier, nebst Beschreibung von sechs neuen Arten (Dipt. Phoridae). Revista de Entomologia, 21, 281 - 300." type="journal article" year="1950">Borgmeier, 1950</bibRefCitation>
