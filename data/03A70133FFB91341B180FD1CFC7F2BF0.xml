<subSubSection id="C314E3AEFFB81341B1D0F84DFC7F2BF0" pageId="7" pageNumber="568" type="etymology">
<paragraph id="8BB1B025FFB81341B1D0F84DFC7F2BF0" blockId="7.[151,1436,1890,1986]" pageId="7" pageNumber="568">
<emphasis id="B97A6C37FFB81341B1D0F84DFE842BAC" bold="true" box="[199,326,1926,1950]" pageId="7" pageNumber="568">Etymology</emphasis>
