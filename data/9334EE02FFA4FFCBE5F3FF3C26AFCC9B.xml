<taxonomicName id="DC9D2497FFA6FFCCE77DF88325A0CA74" authorityName="Lattke" authorityYear="1863" box="[537,750,1828,1852]" class="Insecta" family="Formicidae" genus="Gnamptogenys" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="469" phylum="Arthropoda" rank="species" species="lavra">
