): &quot; 
<collectingCountry id="F3687663B0769A5DBFE740F21018FD37" box="[673,779,654,678]" name="Australia" pageId="38" pageNumber="77">Australia</collectingCountry>
/ 
<date id="FFC11033B0769A5DBE6340F21096FD37" box="[805,901,654,678]" pageId="38" pageNumber="77" value="1912" valueMax="1922">
<number id="3DB724D4B0769A5DBE6340F2104CFD37" box="[805,863,654,678]" pageId="38" pageNumber="77" value="1912.0">1912</number>
