<taxonomicName id="3813E0CEF968C3B50661AC01748569F2" LSID="http://zoobank.org/C3192579-28C3-4B57-A4DE-72D5804904B1" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus neesi" order="Hymenoptera" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="neesi">Heterospilus neesi Marsh</taxonomicName>
<taxonomicNameLabel id="0D6488C9E4B48F154B8832D71CCCE7F6" pageId="125" pageNumber="126">sp. n.</taxonomicNameLabel>
