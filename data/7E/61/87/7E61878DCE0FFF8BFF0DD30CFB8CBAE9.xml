<subSubSection pageId="14" pageNumber="381" type="etymology">
<paragraph blockId="14.[151,1436,151,501]" pageId="14" pageNumber="381">
<emphasis bold="true" box="[199,334,404,429]" pageId="14" pageNumber="381">Etymology.</emphasis>
