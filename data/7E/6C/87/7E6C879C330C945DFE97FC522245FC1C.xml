<taxonomicName box="[545,753,924,950]" class="Insecta" family="Glyphipterigidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="family">Glyphipterygidae</taxonomicName>
, 
<taxonomicName box="[775,936,924,950]" class="Insecta" family="Nymphalidae" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="family">Nymphalidae</taxonomicName>
, 
<taxonomicName box="[959,1126,924,950]" class="Insecta" family="Pterophoridae" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="family">Pterophoridae</taxonomicName>
, and 
<taxonomicName box="[1210,1321,924,950]" class="Insecta" family="Pyralidae" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="family">Pyralidae</taxonomicName>
(
<bibRefCitation author="Arnaud" box="[272,430,964,990]" pageId="24" pageNumber="39" refString="Arnaud, P. H., Jr. (1978) A host-parasite catalog of North American Tachinidae (Diptera). United States Department of Agriculture, Miscellaneous Publication, 1319, 860 pp." type="book" year="1978">
Arnaud 
<number box="[369,430,964,990]" pageId="24" pageNumber="25" value="1978.0">1978</number>
</bibRefCitation>
, as 
<taxonomicName box="[474,603,964,989]" class="Insecta" family="Tachinidae" genus="Phorocera" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="tortricis">
