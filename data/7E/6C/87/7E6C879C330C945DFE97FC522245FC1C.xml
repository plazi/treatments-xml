ex. 
<taxonomicName box="[312,510,1553,1576]" class="Insecta" family="Tortricidae" genus="Archips" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="rosaceana">
