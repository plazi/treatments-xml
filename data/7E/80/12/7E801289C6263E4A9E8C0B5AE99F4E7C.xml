<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181947" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole subreticulata Emery" lsidName-HNS="Pheidole subreticulata Emery" order="Hymenoptera" pageNumber="386" phylum="Arthropoda" rank="species" species="subreticulata">subreticulata</taxonomicName>
, and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181953" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole tennantae Wilson" lsidName-HNS="Pheidole tennantae Wilson" order="Hymenoptera" pageNumber="386" phylum="Arthropoda" rank="species" species="tennantae">tennantae</taxonomicName>
