<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181851" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole caracalla Wilson" lsidName-HNS="Pheidole caracalla Wilson" order="Hymenoptera" pageNumber="386" phylum="Arthropoda" rank="species" species="caracalla">caracalla</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181901" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole micon Wilson" lsidName-HNS="Pheidole micon Wilson" order="Hymenoptera" pageNumber="386" phylum="Arthropoda" rank="species" species="micon">micon</taxonomicName>
