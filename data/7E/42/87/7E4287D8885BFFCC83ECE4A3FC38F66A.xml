<collectingCountry box="[227,314,1071,1096]" name="Taiwan" pageId="4" pageNumber="401">Taiwan</collectingCountry>
), 
<collectingCountry box="[332,399,1073,1096]" name="Japan" pageId="4" pageNumber="401">Japan</collectingCountry>
, 
<collectingCountry box="[410,481,1073,1096]" name="South Korea" pageId="4" pageNumber="401">Korea</collectingCountry>
