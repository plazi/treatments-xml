<number box="[1040,1095,1513,1539]" pageId="3" pageNumber="4" value="2.46">2.46</number>
; first flagellomere length/width = 
<number box="[443,498,1553,1579]" pageId="3" pageNumber="4" value="3.71">3.71</number>
