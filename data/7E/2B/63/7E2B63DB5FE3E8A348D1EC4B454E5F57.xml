<taxonomicName class="Insecta" family="Vespidae" genus="Icaria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Icaria cotonata" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cotonata">Icaria cotonata</taxonomicName>
