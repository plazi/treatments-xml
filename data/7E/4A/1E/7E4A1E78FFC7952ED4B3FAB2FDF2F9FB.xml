<subSubSection pageId="1" pageNumber="120" type="etymology">
<paragraph blockId="1.[151,1436,1476,2005]" box="[199,1044,1620,1645]" pageId="1" pageNumber="120">
<emphasis bold="true" box="[199,326,1620,1645]" pageId="1" pageNumber="120">Etymology</emphasis>
