<taxonomicName box="[963,1141,352,377]" class="Agaricomycetes" family="Agaricaceae" genus="Catastoma" kingdom="Animalia" order="Agaricales" pageId="16" pageNumber="17" phylum="Basidiomycota" rank="species" species="rhodogaster">
<emphasis box="[963,1141,352,377]" italics="true" pageId="16" pageNumber="17">G. rhodogaster</emphasis>
</taxonomicName>
(
<accessionNumber box="[1159,1305,352,378]" httpUri="http://www.uniprot.org/uniprot/SMF77413" pageId="16" pageNumber="17">
<collectionCode LSID="urn:lsid:biocol.org:col:34838" box="[1159,1220,352,378]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="16" pageNumber="17">SMF</collectionCode>
