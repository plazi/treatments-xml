, 
<geoCoordinate box="[1306,1398,1610,1634]" direction="east" orientation="longitude" pageId="7" pageNumber="8" precision="925" value="29.05">
<number box="[1306,1336,1610,1634]" pageId="7" pageNumber="8" value="29.0">29</number>
º03’E
