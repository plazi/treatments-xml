<geoCoordinate box="[616,708,1754,1778]" direction="south" orientation="latitude" pageId="7" pageNumber="8" precision="925" value="-29.166666">
<number box="[616,648,1754,1778]" pageId="7" pageNumber="8" value="29.0">29</number>
º 
<number box="[658,688,1754,1778]" pageId="7" pageNumber="8" value="10.0">10</number>
’S
</geoCoordinate>
, 
<geoCoordinate box="[720,812,1754,1778]" direction="east" orientation="longitude" pageId="7" pageNumber="8" precision="925" value="31.416666">
<number box="[720,748,1754,1778]" pageId="7" pageNumber="8" value="31.0">31</number>
º 
<number box="[758,788,1754,1778]" pageId="7" pageNumber="8" value="25.0">25</number>
