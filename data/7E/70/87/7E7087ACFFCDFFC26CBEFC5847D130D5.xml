<subSubSection pageId="25" pageNumber="26" type="discussion">
<paragraph blockId="25.[151,1436,1132,1507]" pageId="25" pageNumber="26">
<emphasis bold="true" box="[199,315,1373,1399]" pageId="25" pageNumber="26">Remarks.</emphasis>
<bibRefCitation author="Tavakilian, G. L." box="[324,524,1373,1399]" pageId="25" pageNumber="26" pagination="439 - 453" refId="ref50358" refString="Tavakilian, G. L. (1991) Notas sinonimicas e novas combinacoes em longicorneos sulamericanos (Coleoptera, Cerambycidae). Revista Brasileira de Entomologia, 35 (2), 439 - 453." type="journal article" year="1991">Tavakilian (1991)</bibRefCitation>
synonymized 
<taxonomicName authorityName="Villiers" authorityYear="1971" box="[691,900,1373,1399]" class="Insecta" family="Cerambycidae" genus="Urgleptes" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="halffteri">
<emphasis box="[691,900,1373,1399]" italics="true" pageId="25" pageNumber="26">Urgleptes halffteri</emphasis>
</taxonomicName>
with 
<taxonomicName authority="Gilmour, 1962" authorityName="Gilmour" authorityYear="1962" box="[967,1207,1373,1399]" class="Insecta" family="Cerambycidae" genus="Nyssodrysola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="stictica">
<emphasis box="[967,1207,1373,1399]" italics="true" pageId="25" pageNumber="26">Nyssodrysola stictica</emphasis>
</taxonomicName>
, and transferred the species to 
<taxonomicName authorityName="Casey" authorityYear="1913" box="[270,421,1409,1435]" class="Insecta" family="Cerambycidae" genus="Nyssodrysina" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis box="[270,421,1409,1435]" italics="true" pageId="25" pageNumber="26">Nyssodrysina</emphasis>
</taxonomicName>
. 
<bibRefCitation author="Monne, M. A. &amp; Giesbert, E. F." box="[436,730,1409,1435]" pageId="25" pageNumber="26" pagination="249 - 255" refId="ref48963" refString="Monne, M. A. &amp; Giesbert, E. F. (1992) Nomenclatural notes on Western Hemisphere Cerambycidae (Coleoptera). Insecta Mundi, 6 (2), 249 - 255." type="journal article" year="1992">Monné &amp; Giesbert (1992)</bibRefCitation>
synonymized 
<taxonomicName authority="Gilmour, 1962" authorityName="Gilmour" authorityYear="1962" box="[896,1136,1409,1435]" class="Insecta" family="Cerambycidae" genus="Nyssodrysola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="stictica">
<emphasis box="[896,1136,1409,1435]" italics="true" pageId="25" pageNumber="26">Nyssodrysola stictica</emphasis>
</taxonomicName>
with 
<taxonomicName baseAuthorityName="Bates" baseAuthorityYear="1864" box="[1201,1429,1409,1435]" class="Insecta" family="Cerambycidae" genus="Nyssodrysina" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="spreta">
<emphasis box="[1201,1429,1409,1435]" italics="true" pageId="25" pageNumber="26">Nyssodrysina spreta</emphasis>
</taxonomicName>
. 
<bibRefCitation author="Tavakilian, G. L. &amp; Chevillotte, H." box="[151,507,1445,1471]" pageId="25" pageNumber="26" refId="ref50395" refString="Tavakilian, G. L. &amp; Chevillotte, H. (2020) Titan: base de donnees internationales sur les Cerambycidae ou Longicornes. Available from: http: // titan. gbif. fr / index. html (accessed 30 June 2020)" type="url" year="2020">Tavakilian &amp; Chevillotte (2020)</bibRefCitation>
show these three species in 
<taxonomicName authorityName="Casey" authorityYear="1913" box="[816,967,1445,1471]" class="Insecta" family="Cerambycidae" genus="Nyssodrysina" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis box="[816,967,1445,1471]" italics="true" pageId="25" pageNumber="26">Nyssodrysina</emphasis>
</taxonomicName>
, but 
<taxonomicName authorityName="Villiers" authorityYear="1971" box="[1021,1144,1445,1471]" class="Insecta" family="Cerambycidae" genus="Urgleptes" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="halffteri">
<emphasis box="[1021,1144,1445,1471]" italics="true" pageId="25" pageNumber="26">U. halffteri</emphasis>
