<subSubSection box="[151,994,1277,1302]" pageId="52" pageNumber="53" type="distribution">
<paragraph blockId="52.[151,1437,1277,1482]" box="[151,994,1277,1302]" pageId="52" pageNumber="53">
<emphasis bold="true" box="[151,304,1277,1302]" pageId="52" pageNumber="53">Distribution.</emphasis>
South 
<collectingCountry box="[383,453,1277,1302]" name="China" pageId="52" pageNumber="53">China</collectingCountry>
, 
<collectingCountry box="[464,564,1277,1302]" name="Thailand" pageId="52" pageNumber="53">Thailand</collectingCountry>
