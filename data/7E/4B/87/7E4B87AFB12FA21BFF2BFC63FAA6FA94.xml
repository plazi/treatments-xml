<collectingRegion box="[699,929,1261,1286]" country="Vietnam" name="Lam Dong" pageId="25" pageNumber="26">
<collectingRegion box="[699,820,1262,1286]" inferredBySuffix="true" pageId="25" pageNumber="26">
