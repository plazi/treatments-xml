<collectingRegion box="[677,874,1297,1322]" country="Vietnam" name="Lao Cai" pageId="25" pageNumber="26">
Lao 
<collectingRegion box="[728,874,1297,1322]" inferredBySuffix="true" pageId="25" pageNumber="26">Cai Province</collectingRegion>
</collectingRegion>
