<emphasis box="[467,484,584,607]" italics="true" pageId="25" pageNumber="26">G</emphasis>
.) 
<emphasis box="[507,670,584,608]" italics="true" pageId="25" pageNumber="26">parahiyangum</emphasis>
</taxonomicName>
and 
<taxonomicName box="[725,847,584,608]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="unii" status="sp. nov." subGenus="Gomphostilbia">
<emphasis box="[725,739,584,607]" italics="true" pageId="25" pageNumber="26">S</emphasis>
