<collectingRegion box="[536,686,1660,1685]" inferredBySuffix="true" pageId="116" pageNumber="117">Son Province</collectingRegion>
</collectingRegion>
in northern 
<collectingCountry box="[823,918,1660,1685]" name="Vietnam" pageId="116" pageNumber="117">Vietnam</collectingCountry>
