<number box="[212,238,1862,1886]" pageId="19" pageNumber="220" value="14.0">14</number>
</figureCitation>
C–F and 
<number box="[342,413,1861,1886]" pageId="19" pageNumber="220" value="16.5" valueMax="18.0" valueMin="15.0">15–18</number>
