<taxonomicName authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[260,440,906,932]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="insula">
