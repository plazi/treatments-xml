<collectingCountry box="[316,468,941,968]" name="New Zealand" pageId="56" pageNumber="57">New Zealand</collectingCountry>
(
<taxonomicName authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[484,611,941,967]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="antipoda">
