<taxonomicName id="428EE8C29EA6E3DCFD3AF824D87F64A5" authorityName="Quicke &amp; Butcher" authorityYear="2012" box="[736,835,2013,2037]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="195" pageNumber="196" phylum="Arthropoda" rank="species" species="codon" status="sp. nov.">
<emphasis id="B7FA4F539EA6E3DCFD3AF824D87F64A5" box="[736,835,2013,2037]" italics="true" pageId="195" pageNumber="196">A. codon</emphasis>
</taxonomicName>
<emphasis id="B7FA4F539EA6E3DCFC91F824D89864A5" bold="true" box="[843,932,2013,2037]" pageId="195" pageNumber="196">
<taxonomicNameLabel id="ACC9F2289EA6E3DCFC91F824D89864A5" box="[843,932,2013,2037]" pageId="195" pageNumber="196" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
