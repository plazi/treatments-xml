<caption id="10BA66479755FF84429FFC3D3878FC1A" ID-DOI="http://doi.org/10.5281/zenodo.5419263" ID-Zenodo-Dep="5419263" httpUri="https://zenodo.org/record/5419263/files/figure.png" pageId="2" pageNumber="287" startId="2.[136,175,914,937]" targetBox="[220,1377,508,897]" targetPageId="2">
<paragraph id="447A36CF9755FF84429FFC3D3878FC1A" blockId="2.[136,1459,913,969]" pageId="2" pageNumber="287">
