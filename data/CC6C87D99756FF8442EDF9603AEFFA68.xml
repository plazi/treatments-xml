<taxonomicName id="83C54D4C9755FF844369FC3E3B93FC7A" authority=", MZUSP" authorityName="MZUSP" box="[382,702,913,937]" class="Actinopterygii" family="Pimelodidae" genus="Pimelodus" kingdom="Animalia" order="Siluriformes" pageId="2" pageNumber="287" phylum="Chordata" rank="species" species="ortmanni">
<emphasis id="76B1EADD9755FF844369FC3E3B74FC7A" box="[382,601,913,937]" italics="true" pageId="2" pageNumber="287">Pimelodus ortmanni</emphasis>
