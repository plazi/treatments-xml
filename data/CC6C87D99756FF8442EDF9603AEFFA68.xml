<taxonomicName id="83C54D4C9756FF87417EFD153D75FD02" authority="Valenciennes" authorityName="Valenciennes" authorityYear="1840" box="[873,1112,697,721]" class="Actinopterygii" family="Pimelodidae" genus="Pimelodus" kingdom="Animalia" order="Siluriformes" pageId="1" pageNumber="286" phylum="Chordata" rank="species" species="blochii">
