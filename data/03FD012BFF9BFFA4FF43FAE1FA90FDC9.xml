<collectingCountry id="F343F0ADFF9DFFA4FD76FDD4FCFAFDC8" box="[674,779,548,573]" name="Tanzania" pageId="5" pageNumber="154">Tanzania</collectingCountry>
: 
<collectingRegion id="49907EDFFF9DFFA4FCC2FDD4FCA6FDC8" box="[790,855,548,573]" country="Tanzania" name="Kusini Unguja" pageId="5" pageNumber="154">South</collectingRegion>
