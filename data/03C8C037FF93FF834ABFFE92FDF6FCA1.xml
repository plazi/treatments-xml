<materialsCitation id="3B097B7CFF93FF83490BFD0FFCEFFD61" collectingDate="1999-07-03" county="Ten" elevation="750" location="Pendolo" municipality="Boe" pageId="36" pageNumber="1498" specimenCount="1" stateProvince="Sulawesi Utara">
