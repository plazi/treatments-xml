. 
<taxonomicName id="4C2F4D10FF91FFE4FEAAFAEFFDBF7E2F" authorityName="Melika, Nicholls &amp; Stone" authorityYear="2021" box="[331,601,1292,1318]" class="Insecta" family="Cynipidae" genus="Andricus" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="coconinoensis">
