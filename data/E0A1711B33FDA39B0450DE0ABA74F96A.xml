<taxonomicName id="B291F89565F93BCF4D70C80857C4EC25" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138763" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" lsidName-HNS="Ponera abeillei André" order="Hymenoptera" pageNumber="30" phylum="Arthropoda" rank="species" species="abeillei">Ponera abeillei Andre</taxonomicName>
