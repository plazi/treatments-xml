and 
<emphasis id="B90370455111FF88FBDBFCFBFB281867" box="[1085,1205,863,888]" italics="true" pageId="49" pageNumber="50">Cypressus</emphasis>
<taxonomicName id="4C77D7D45111FF88FB5BFCFAFB711867" box="[1213,1260,862,888]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="species" species="spp">spp</taxonomicName>
.; in 
<collectingCountry id="F360ECC75111FF88FAFBFCFAFAD11867" box="[1309,1356,862,888]" name="Iraq" pageId="49" pageNumber="50">Iraq</collectingCountry>
