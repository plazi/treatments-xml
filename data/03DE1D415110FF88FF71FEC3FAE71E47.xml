<taxonomicName id="4C77D7D45111FF88FE8FFC23FE5618BF" box="[361,459,903,928]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="genus">
<emphasis id="B90370455111FF88FE8FFC23FE5618BF" box="[361,459,903,928]" italics="true" pageId="49" pageNumber="50">Quercus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C77D7D45111FF88FDE5FC23FCC718BF" authority="Wiltshire 1957: 44" authorityName="Wiltshire" authorityPageNumber="44" authorityYear="1957" box="[515,858,902,928]" class="Magnoliopsida" family="Anacardiaceae" genus="Pistacia" kingdom="Plantae" order="Sapindales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="genus">
<emphasis id="B90370455111FF88FDE5FC23FDFE18BF" box="[515,611,903,928]" italics="true" pageId="49" pageNumber="50">Pistacia</emphasis>
(Wiltshire 
<number id="3DBFBE705111FF88FD00FC22FCB818BF" box="[742,805,902,928]" pageId="49" pageNumber="50" value="1957.0">1957</number>
