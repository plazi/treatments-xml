<collectingCountry id="E5C9F2D88063FFC0EFA1CC9C1A500CB0" box="[714,781,693,716]" name="Japan" pageId="103" pageNumber="104">Japan</collectingCountry>
, south to 
<collectingRegion id="5F1A7CAA8063FFC0EE17CC9C1D1A0CB1" box="[892,1095,692,717]" country="Australia" name="Western Australia" pageId="103" pageNumber="104">Western Australia</collectingRegion>
at 
<geoCoordinate id="F8EAD48F8063FFC0E900CC9D1D990CB0" box="[1131,1220,692,716]" direction="south" orientation="latitude" pageId="103" pageNumber="104" precision="925" value="-14.066667">14°04'S</geoCoordinate>
, 
<collectingRegion id="5F1A7CAA8063FFC0E9B9CC9C1CC10CB1" box="[1234,1436,692,717]" country="Australia" name="New South Wales" pageId="103" pageNumber="104">New South Wales</collectingRegion>
(
<collectingCountry id="E5C9F2D88063FFC0EDF5CCFE18560C8C" box="[158,267,727,752]" name="Australia" pageId="103" pageNumber="104">Australia</collectingCountry>
