<quantity id="5A261FAD8063FFC0EFEACCD51B8B0D68" box="[641,726,764,789]" metricMagnitude="1" metricUnit="m" metricValue="2.4" metricValueMax="4.3" metricValueMin="0.5" pageId="103" pageNumber="104" unit="m" value="24.0" valueMax="43.0" valueMin="5.0">5–43 m</quantity>
