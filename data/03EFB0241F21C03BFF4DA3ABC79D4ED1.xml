<taxonomicName id="4C467AB11F22C03BFB44A7C5C7074CD2" box="[1153,1328,444,468]" class="Hydrozoa" family="Ptilocodiidae" genus="Tregouboviopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecata" pageId="2" pageNumber="238" phylum="Cnidaria" rank="genus">
