Genus 
<taxonomicName id="4C3BFDC2F16FFFFC2C62FDEEFCB6D37A" authority="Simon, 1876" authorityName="Simon" authorityYear="1876" box="[591,811,546,574]" class="Arachnida" family="Salticidae" genus="Cyrba" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="164" phylum="Arthropoda" rank="genus">
