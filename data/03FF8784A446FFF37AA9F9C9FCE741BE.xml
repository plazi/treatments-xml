<taxonomicName id="4C564D11A446FFF379FCFF32FB96414D" authorityName="E. Geoffroy Saint-Hilaire" authorityYear="1803" box="[968,1080,252,275]" class="Mammalia" family="Talpidae" genus="Scalopus" kingdom="Animalia" order="Soricomorpha" pageId="39" pageNumber="40" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A446FFF379FCFF32FB96414D" box="[968,1080,252,275]" italics="true" pageId="39" pageNumber="40">Scalopus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A446FFF37E65FF32FB66414D" box="[1105,1224,252,275]" class="Mammalia" family="Talpidae" genus="Scapanus" kingdom="Animalia" order="Soricomorpha" pageId="39" pageNumber="40" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A446FFF37E65FF32FB66414D" box="[1105,1224,252,275]" italics="true" pageId="39" pageNumber="40">Scapanus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A446FFF378F4FED7FC99416E" authorityName="Milne-Edwards" authorityYear="1872" box="[704,823,281,304]" class="Mammalia" family="Talpidae" genus="Scaptonyx" kingdom="Animalia" order="Soricomorpha" pageId="39" pageNumber="40" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A446FFF378F4FED7FC99416E" box="[704,823,281,304]" italics="true" pageId="39" pageNumber="40">Scaptonyx</emphasis>
</taxonomicName>
