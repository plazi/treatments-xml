<emphasis id="B922EA80A446FFF37E57FF11FD57414D" italics="true" pageId="39" pageNumber="40">Neurotrichus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A446FFF37926FF32FC00414D" authorityName="True" authorityYear="1894" box="[786,942,252,275]" class="Mammalia" family="Talpidae" genus="Parascalops" kingdom="Animalia" order="Soricomorpha" pageId="39" pageNumber="40" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A446FFF37926FF32FC00414D" box="[786,942,252,275]" italics="true" pageId="39" pageNumber="40">Parascalops</emphasis>
</taxonomicName>
