<taxonomicName id="4C564D11A446FFF379D1FF11FBFD40A8" authorityName="Guldenstadt" authorityYear="1777" box="[997,1107,223,246]" class="Mammalia" family="Talpidae" genus="Desmana" kingdom="Animalia" order="Soricomorpha" pageId="39" pageNumber="40" phylum="Chordata" rank="genus">
