<taxonomicName id="4C7F4D3DFA74AC6D9624C0ADFDBCE3BB" box="[376,541,1750,1771]" class="Insecta" family="Chrysididae" genus="Ipsiura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="oaxacae">
<emphasis id="B90BEAACFA74AC6D9624C0ADFDBCE3BB" box="[376,541,1750,1771]" italics="true" pageId="53" pageNumber="54">Ipsiura oaxacae</emphasis>
