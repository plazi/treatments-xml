<taxonomicName id="A1969B1DA8BAE900501F516A150FE795" class="Insecta" family="Leptoceridae" genus="Oecetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oecetis maritza" order="Trichoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="maritza">Oecetis maritza</taxonomicName>
