<subSubSection id="5BA97F3F68CBDB9257BAE52BADACF836" pageId="40" pageNumber="41" type="etymology">
<paragraph id="F496DB3C9F546CC98E317430D60F57B8" pageId="40" pageNumber="41">Etymology.</paragraph>
<paragraph id="2F1D867E753FD3E6E3C2BF3B5A3DE921" pageId="40" pageNumber="41">
