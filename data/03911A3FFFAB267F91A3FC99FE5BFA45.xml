<taxonomicName id="4C38D0AAFFAB267F903FFC99FE03FCCD" authorityName="Tobias &amp; Perepetchayenko" authorityYear="1995" box="[267,420,837,864]" class="Insecta" family="Braconidae" genus="Alysdacnusa" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="506" phylum="Arthropoda" rank="genus">
