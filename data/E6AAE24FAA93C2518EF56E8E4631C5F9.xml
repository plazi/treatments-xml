For. (= 
<taxonomicName id="61740E173BDA84E5E60759C76D1AB771" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136529" class="Insecta" family="Formicidae" genus="Cataulacus" kingdom="Animalia" lsidName="Cataulacus intrudens var. rugosus Forel" lsidName-HNS="Cataulacus intrudens var. rugosus Forel" order="Hymenoptera" pageNumber="129" phylum="Arthropoda" rank="variety" species="intrudens" variety="rugosus">C. intrudens var. rugosus</taxonomicName>
