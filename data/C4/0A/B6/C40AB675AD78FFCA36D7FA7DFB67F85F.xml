<paragraph blockId="3.[151,1436,1549,2018]" pageId="3" pageNumber="56">
Male: Body length 
<quantity box="[385,508,1549,1575]" metricMagnitude="-2" metricUnit="m" metricValue="4.25" metricValueMax="4.5" metricValueMin="4.0" pageId="3" pageNumber="56" unit="mm" value="42.5" valueMax="45.0" valueMin="40.0">40–45mm</quantity>
; abdomen length 
<quantity box="[720,843,1549,1575]" metricMagnitude="-2" metricUnit="m" metricValue="3.15" metricValueMax="3.3" metricValueMin="3.0" pageId="3" pageNumber="56" unit="mm" value="31.5" valueMax="33.0" valueMin="30.0">30–33mm</quantity>
; forewing length 
<quantity box="[1053,1176,1549,1575]" metricMagnitude="-2" metricUnit="m" metricValue="4.1" metricValueMax="4.5" metricValueMin="3.7" pageId="3" pageNumber="56" unit="mm" value="41.0" valueMax="45.0" valueMin="37.0">37–45mm</quantity>
; hindwing length 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="3.1" metricValueMax="3.3" metricValueMin="2.9" pageId="3" pageNumber="56" unit="mm" value="31.0" valueMax="33.0" valueMin="29.0">29– 33mm</quantity>
; antennae length 
<quantity box="[433,556,1589,1615]" metricMagnitude="-2" metricUnit="m" metricValue="2.8" metricValueMax="3.1" metricValueMin="2.5" pageId="3" pageNumber="56" unit="mm" value="28.0" valueMax="31.0" valueMin="25.0">25–31mm</quantity>
