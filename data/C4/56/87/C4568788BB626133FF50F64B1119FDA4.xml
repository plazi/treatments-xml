<taxonomicName authority=": Hayasaka" authorityName="Hayasaka" box="[263,605,312,334]" class="Rhynchonellata" family="Thecidellinidae" genus="Thecidellella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thecideida" pageId="13" pageNumber="494" phylum="Brachiopoda" rank="species" species="japonica">
