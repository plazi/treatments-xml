44. 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147759" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica nana Jerdon" lsidName-HNS="Formica nana Jerdon" order="Hymenoptera" pageNumber="125" phylum="Arthropoda" rank="species" species="nana">Formica nana</taxonomicName>
