<taxonomicName class="Insecta" family="Carabidae" genus="Oodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oodes" order="Coleoptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="genus">Oodes</taxonomicName>
