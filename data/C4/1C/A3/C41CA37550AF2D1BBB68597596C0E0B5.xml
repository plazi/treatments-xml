<taxonomicName class="Insecta" family="Carabidae" genus="Sphoerodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphoerodes" order="Coleoptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="genus">Sphoerodes</taxonomicName>
