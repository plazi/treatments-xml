<taxonomicName class="Sordariomycetes" family="Ceratostomataceae" genus="Sphaerodes" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Sphaerodes" order="Melanosporales" pageId="83" pageNumber="84" phylum="Ascomycota" rank="genus">Sphaerodes</taxonomicName>
