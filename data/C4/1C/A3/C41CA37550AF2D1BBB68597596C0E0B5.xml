<pageBreakToken pageId="83" pageNumber="84" start="start">latinized</pageBreakToken>
form by Bedel (1879: 52, as 
<taxonomicName lsidName="" pageId="83" pageNumber="84" rank="tribe" tribe="Oodini">Oodini</taxonomicName>
