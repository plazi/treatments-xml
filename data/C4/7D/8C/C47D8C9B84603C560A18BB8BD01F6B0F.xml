<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2468" class="Insecta" family="Formicidae" genus="Forelius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Forelius Emery" order="Hymenoptera" pageNumber="59" phylum="Arthropoda" rank="genus">Forelius</taxonomicName>
