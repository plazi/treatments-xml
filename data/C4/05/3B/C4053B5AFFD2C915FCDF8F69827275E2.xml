<geoCoordinate box="[948,1064,928,952]" degrees="24" direction="east" minutes="16" orientation="longitude" pageId="23" pageNumber="262" precision="15" seconds="55" value="24.281944">24°16'55&quot;E</geoCoordinate>
, 
<quantity box="[1075,1144,928,952]" metricMagnitude="2" metricUnit="m" metricValue="3.8" pageId="23" pageNumber="262" unit="m" value="380.0">
<elevation box="[1075,1144,928,952]" metricMagnitude="2" metricUnit="m" metricValue="3.8" pageId="23" pageNumber="262" unit="m" value="380.0">380 m</elevation>
</quantity>
, soil washing, 
<date box="[1299,1417,928,952]" pageId="23" pageNumber="262" value="2018-03-23">
<collectingDate box="[1299,1417,928,952]" pageId="23" pageNumber="262" value="2018-03-23">23.III.2018</collectingDate>
</date>
, 
<collectorName box="[816,916,960,984]" pageId="23" pageNumber="262">V. Assing</collectorName>
