<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181674" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole inversa Forel" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="inversa">inversa</taxonomicName>
