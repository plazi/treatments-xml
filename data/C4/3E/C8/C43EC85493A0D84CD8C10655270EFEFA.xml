<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33959" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole radoszkowskii Mayr" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="radoszkowskii">radoszkowskii</taxonomicName>
, and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34069" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole triconstricta Forel" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="triconstricta">triconstricta</taxonomicName>
