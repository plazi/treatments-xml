<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181662" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole bruesi Wheeler" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="bruesi">bruesi</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142247" class="Insecta" family="Formicidae" genus="Atta" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Atta diligens Smith" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="diligens">diligens</taxonomicName>
