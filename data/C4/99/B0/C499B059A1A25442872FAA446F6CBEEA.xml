<geoCoordinate degrees="17" direction="south" minutes="30" orientation="latitude" precision="15" seconds="33" value="-17.509167">17°30'33&quot;S</geoCoordinate>
; 
<geoCoordinate degrees="68" direction="west" minutes="20" orientation="longitude" precision="15" seconds="35" value="-68.343056">68°20'35&quot;W</geoCoordinate>
