<subSubSection pageId="8" pageNumber="25" type="diagnosis">
<paragraph blockId="8.[848,1487,621,677]" pageId="8" pageNumber="25">
<emphasis bold="true" box="[848,987,621,645]" pageId="8" pageNumber="25">
