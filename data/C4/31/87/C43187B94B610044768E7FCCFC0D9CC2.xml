) and 
<taxonomicName authorityName="Drake" authorityYear="1927" box="[907,1052,1863,1889]" class="Insecta" family="Tingidae" genus="Alloiothucha" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="370" phylum="Arthropoda" rank="genus">
<emphasis box="[907,1052,1863,1889]" italics="true" pageId="1" pageNumber="370">Alloiothucha</emphasis>
</taxonomicName>
(
<taxonomicName baseAuthorityName="Horvath" baseAuthorityYear="1926" box="[1067,1203,1863,1889]" class="Insecta" family="Tingidae" genus="Alloiothucha" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="370" phylum="Arthropoda" rank="species" species="artocarpi">
<emphasis box="[1067,1203,1863,1889]" italics="true" pageId="1" pageNumber="370">A. artocarpi</emphasis>
</taxonomicName>
, 
<figureCitation box="[1215,1321,1863,1889]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-2="FIGURE 3" captionStart-3="FIGURE 4" captionStartId-0="2.[151,250,2003,2027]" captionStartId-1="3.[151,250,1209,1233]" captionStartId-2="4.[151,250,1060,1084]" captionStartId-3="5.[151,250,1796,1820]" captionTargetBox-0="[159,1429,1093,1971]" captionTargetBox-1="[151,1436,190,1185]" captionTargetBox-2="[151,1436,181,1036]" captionTargetBox-3="[160,1423,321,1759]" captionTargetId-0="figure-331@2.[151,1436,1082,1977]" captionTargetId-1="figure-18@3.[151,1436,181,1185]" captionTargetId-2="figure-25@4.[151,1436,181,1036]" captionTargetId-3="figure-66@5.[151,1436,309,1773]" captionTargetPageId-0="2" captionTargetPageId-1="3" captionTargetPageId-2="4" captionTargetPageId-3="5" captionText-0="FIGURE 1. Habitus images of Alloiothucha artocarpi from Laos, dorsal and lateral views (A, B). Scale bar: 1.0 mm." captionText-1="FIGURE 2. Thoracic structures of Alloiothucha artocarpi: pronotum, dorsal and dorsolateral views (A, B); hemelytra, dorsolateral view (C). Scale bars: 0.2 mm." captionText-2="FIGURE 3. Rostrum and sternal laminae of Alloiothucha artocarpi, ventral view (A). Apical part of the abdomen of A. artocarpi, ventral view: male (B) and female (C). Scale bars: 0.2 mm." captionText-3="FIGURE 4. Male terminalia of Alloiothucha artocarpi, dorsal view: paramere (A) and pygophore (B). Ostiolar peritreme of A. artocarpi, lateral view (C). Scale bars: 0.1 mm." figureDoi-0="http://doi.org/10.5281/zenodo.6079092" figureDoi-1="http://doi.org/10.5281/zenodo.6079097" figureDoi-2="http://doi.org/10.5281/zenodo.6079101" figureDoi-3="http://doi.org/10.5281/zenodo.6079106" httpUri-0="https://zenodo.org/record/6079092/files/figure.png" httpUri-1="https://zenodo.org/record/6079097/files/figure.png" httpUri-2="https://zenodo.org/record/6079101/files/figure.png" httpUri-3="https://zenodo.org/record/6079106/files/figure.png" pageId="1" pageNumber="370">Figs. 1–4</figureCitation>
; 
