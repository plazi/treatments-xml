<pageStartToken pageNumber="64">posterior</pageStartToken>
part truncate (shared with 
<taxonomicName class="Actinopterygii" family="Ariidae" genus="Carlarius" kingdom="Animalia" order="Siluriformes" pageNumber="63" phylum="Chordata" rank="species" species="heudelotii">Carlarius heudelotii</taxonomicName>
