<paragraph blockId="2.[151,1436,1733,1903]" box="[625,1077,1733,1760]" pageId="2" pageNumber="595">
(currently 
<taxonomicName baseAuthorityName="Scriba" baseAuthorityYear="1791" box="[759,1067,1733,1760]" class="Insecta" family="Geotrupidae" genus="Anoplotrupes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="595" phylum="Arthropoda" rank="species" species="stercorosus">
<emphasis bold="true" box="[759,1067,1733,1760]" italics="true" pageId="2" pageNumber="595">Anoplotrupes stercorosus</emphasis>
</taxonomicName>
)
