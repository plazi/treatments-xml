<treatmentCitation author="Sriba" box="[334,618,1733,1760]" page="250" pageId="2" pageNumber="595" year="1791">
<taxonomicName authority="Scriba, 1791" authorityName="Scriba" authorityYear="1791" box="[334,618,1733,1760]" class="Insecta" family="Scarabaeidae" genus="Scarabaeus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="595" phylum="Arthropoda" rank="species" species="stercorosus">
