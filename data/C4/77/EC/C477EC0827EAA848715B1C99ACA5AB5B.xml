<taxonomicName authority="Roewer, 1930" authorityName="Roewer" authorityYear="1930" class="Arachnida" family="Gonyleptidae" genus="Gonazula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gonazula gibbosa" order="Opiliones" pageId="13" pageNumber="38" phylum="Arthropoda" rank="species" species="gibbosa">Gonazula gibbosa</taxonomicName>
