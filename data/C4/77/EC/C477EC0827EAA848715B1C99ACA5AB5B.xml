<paragraph pageId="13" pageNumber="38">
<taxonomicName authority="Roewer, 1913" authorityName="Roewer" authorityYear="1913" class="Arachnida" family="Gonyleptidae" genus="Eugonyleptes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eugonyleptes" order="Opiliones" pageId="13" pageNumber="38" phylum="Arthropoda" rank="genus">Eugonyleptes</taxonomicName>
