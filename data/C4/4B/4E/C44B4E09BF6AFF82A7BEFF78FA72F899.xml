<emphasis box="[1273,1418,410,431]" italics="true" pageId="2" pageNumber="303">
<taxonomicName box="[1273,1411,410,431]" class="Insecta" family="Mycetophilidae" genus="Calomycomya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">Calomycomya</taxonomicName>
