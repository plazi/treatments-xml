<emphasis id="E2AE89BEA312F3B7F6A04D0307FA7F9D" bold="true" pageId="0" pageNumber="95">A, B</emphasis>
) and 
<taxonomicName id="AB8FD4E2065CC33C85921716B7C05A47" authorityName="Zamani &amp; Marusik" authorityYear="2021" class="Arachnida" family="Philodromidae" genus="Rhysodromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhysodromus medes" order="Araneae" pageId="0" pageNumber="95" phylum="Arthropoda" rank="species" species="medes">
