<number id="3DD6248A1418AF45FC8BFA9BFC1AFA16" box="[876,904,1385,1409]" pageId="15" pageNumber="16" value="36.0">36</number>
º 
<number id="3DD6248A1418AF45FC76FA9BFC3CFA16" box="[913,942,1385,1409]" pageId="15" pageNumber="16" value="20.0">20</number>
′ 
<number id="3DD6248A1418AF45FC53FA9BFC77FA16" box="[948,997,1385,1409]" pageId="15" pageNumber="16" value="38.4">38.4</number>
″N, 
<number id="3DD6248A1418AF45FBF1FA9BFBD2FA16" box="[1046,1088,1385,1409]" pageId="15" pageNumber="16" value="129.0">129</number>
º 
<number id="3DD6248A1418AF45FBAEFA9BFBF6FA16" box="[1097,1124,1385,1409]" pageId="15" pageNumber="16" value="16.0">16</number>
