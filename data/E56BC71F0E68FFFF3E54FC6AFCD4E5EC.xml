<taxonomicName id="AAC20D8A0E68FFFF3FDBF82AFEE9E5CC" box="[151,295,1922,1945]" class="Actinopterygii" family="Cetopsidae" genus="Cetopsogiton" kingdom="Animalia" order="Siluriformes" pageId="95" pageNumber="222" phylum="Chordata" rank="genus">
