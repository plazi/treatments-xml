<quantity id="3B4A9B1D8731C65B0797FED1FBFCDEF2" box="[1016,1101,376,402]" metricMagnitude="3" metricUnit="m" metricValue="3.7" pageId="37" pageNumber="38" unit="km" value="3.7">3.7 km</quantity>
SSE Buena Vista, 
<typeStatus id="2309885A8731C65B0158FED1FA2DDEF2" box="[1335,1436,376,402]" pageId="37" pageNumber="38">paratype</typeStatus>
male, 
<typeStatus id="2309885A8731C65B04B0FE34FEF5DED7" box="[223,324,413,439]" pageId="37" pageNumber="38">paratype</typeStatus>
female, 
<date id="880C10388731C65B05C4FE34FE51DED7" box="[427,480,413,439]" pageId="37" pageNumber="38">
XI. 
<number id="4A7A24DF8731C65B05BDFE34FE51DED7" box="[466,480,413,439]" pageId="37" pageNumber="38" value="10.10005" valueMax="15.2001" valueMin="5.0">5</number>
