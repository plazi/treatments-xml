<paragraph id="E82536CE905CFF94E0A1A78414E9FC7D" blockId="11.[151,480,942,968]" box="[151,480,942,968]" pageId="11" pageNumber="586">
<heading id="B36D81A2905CFF94E0A1A78414E9FC7D" bold="true" box="[151,480,942,968]" fontSize="11" level="1" pageId="11" pageNumber="586" reason="1">
<taxonomicName id="2F9A4D4D905CFF94E0A1A78414E9FC7D" authority="Chace, 1985" authorityName="Chace" authorityYear="1985" box="[151,480,942,968]" class="Malacostraca" family="Pandalidae" genus="Plesionika" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="586" phylum="Arthropoda" rank="species" species="parvimartia">
