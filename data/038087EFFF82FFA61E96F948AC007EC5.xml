<typeStatus id="5492885BFF83FFA11FD9F8F3AB057D81" box="[472,573,1900,1923]" pageId="11" pageNumber="12">paratype</typeStatus>
); 
<number id="3DE124DEFF83FFA11C51F8F3AB577D81" box="[592,623,1900,1923]" pageId="11" pageNumber="12" value="22.0">22</number>
. 
<taxonomicName id="4C294D7AFF83FFA11C7AF8F3AA9B7D81" authority="Junnilainen" authorityName="Junnilainen" box="[635,931,1900,1923]" class="Insecta" family="Gelechiidae" genus="Megacraspedus" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="albovenata" status="sp. n.">
<emphasis id="B95DEAEBFF83FFA11C7AF8F3AA237D81" box="[635,795,1900,1923]" italics="true" pageId="11" pageNumber="12">M. albovenata</emphasis>
Junnilainen
</taxonomicName>
<taxonomicNameLabel id="A26E5790FF83FFA11DABF8F3AAD27D86" box="[938,1002,1900,1924]" pageId="11" pageNumber="12" rank="species">
<emphasis id="B95DEAEBFF83FFA11DABF8F3AAD27D86" bold="true" box="[938,1002,1900,1924]" pageId="11" pageNumber="12">sp. n.</emphasis>
</taxonomicNameLabel>
(male, 
<typeStatus id="5492885BFF83FFA11A3EF8F3AD9D7D81" box="[1087,1189,1900,1923]" pageId="11" pageNumber="12">holotype</typeStatus>
); 
<number id="3DE124DEFF83FFA11AB8F8F3ADE07D81" box="[1209,1240,1900,1923]" pageId="11" pageNumber="12" value="23.0">23</number>
. 
<taxonomicName id="4C294D7AFF83FFA11AE2F8F3A82A7DA1" authority="Junnilainen" authorityName="Junnilainen" class="Insecta" family="Gelechiidae" genus="Megacraspedus" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="longipalpella" status="sp. n.">
<emphasis id="B95DEAEBFF83FFA11AE2F8F3ACA57D81" box="[1251,1437,1900,1923]" italics="true" pageId="11" pageNumber="12">M. longipalpella</emphasis>
Junnilainen
</taxonomicName>
