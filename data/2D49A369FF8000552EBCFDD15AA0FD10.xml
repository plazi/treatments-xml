<treatmentCitationGroup id="85F03551FF8000542EBCFD4B5A24FD07" box="[151,1139,678,700]" pageId="8" pageNumber="281">
<treatmentCitation id="2441346EFF8000542EBCFD4B5C9AFD07" author="Mey, W." box="[151,717,678,700]" page="212" pageId="8" pageNumber="281" year="1995">
