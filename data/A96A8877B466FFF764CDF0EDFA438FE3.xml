<taxonomicName id="E6C342E2B466FFF764B1F0EDFEB58AD3" box="[235,370,475,501]" class="Insecta" family="Chrysididae" kingdom="Animalia" order="Hymenoptera" pageId="90" pageNumber="91" phylum="Arthropoda" rank="subFamily" subFamily="Amiseginae">Amiseginae</taxonomicName>
