<taxonomicName id="4C144D2DFF29FF32FAE4FC08CB341CF4" box="[1312,1396,980,1005]" class="Insecta" family="Carabidae" genus="Amara" kingdom="Animalia" order="Coleoptera" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="pindica">
