<treatmentCitationGroup id="3C92242AFFE2E236FF69DFCB727CFA7D" box="[151,1361,1482,1505]" pageId="31" pageNumber="32">
<taxonomicName id="DB827887FFE2E236FF69DFCB76FBFA7D" authority="Liebke, 1938: 48" authorityName="Liebke" authorityPageNumber="48" authorityYear="1938" box="[151,470,1482,1505]" class="Insecta" family="Carabidae" genus="Isocasnonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="genus">
<emphasis id="2EF6DF16FFE2E236FF69DFCB7635FA7D" box="[151,280,1482,1505]" italics="true" pageId="31" pageNumber="32">Isocasnonia</emphasis>
<treatmentCitation id="9D232515FFE2E236FEE0DFCB76FBFA7D" author="Liebke" box="[286,470,1482,1505]" page="48" pageId="31" pageNumber="32" year="1938">
<bibRefCitation id="78137EF5FFE2E236FEE0DFCB7682FA7D" author="Liebke" box="[286,431,1482,1505]" pageId="31" pageNumber="39" refString="Liebke, M. (1938) Denkschrift uber die Carabiden-Tribus Colliurini. Festschrift zum 60. Geburtstage von Professor Dr. Embrik Strand, 4, 37 - 141." type="journal article" year="1938">
Liebke, 
<number id="AA4A1123FFE2E236FE88DFCB7682FA7D" box="[374,431,1482,1505]" pageId="31" pageNumber="32" value="1938.0">1938</number>
