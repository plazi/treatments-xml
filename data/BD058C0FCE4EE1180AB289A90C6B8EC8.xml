<paragraph id="5E4AACB5F5616852B9379A9D263DCDB0" pageId="null" pageNumber="632">
<taxonomicName id="CBE30184AD1C9A2C57FEDD903D12FECE" authority="Jacq." class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="632" phylum="Tracheophyta" rank="species" species="palustris">
