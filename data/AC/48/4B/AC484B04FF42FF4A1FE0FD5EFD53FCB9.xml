</taxonomicName>
<taxonomicName authorityName="Timmermann &amp; Kuhlmann" authorityYear="2009" box="[437,560,1969,1992]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="ninae" subGenus="Chaetalictus">
(
<emphasis box="[445,476,1969,1992]" italics="true" pageId="168" pageNumber="169">Ch</emphasis>
.) 
<emphasis box="[501,560,1969,1992]" italics="true" pageId="168" pageNumber="169">ninae</emphasis>
