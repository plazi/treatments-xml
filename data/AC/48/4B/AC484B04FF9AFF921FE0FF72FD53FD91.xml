<emphasis box="[306,414,1477,1500]" italics="true" pageId="112" pageNumber="113">Patellapis</emphasis>
</taxonomicName>
<taxonomicName authorityName="Cockerell" authorityYear="1939" box="[423,628,1476,1500]" class="Insecta" family="Halictidae" genus="Halictus" kingdom="Animalia" order="Hymenoptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="pastiniformis" subGenus="Chaetalictus">
(
<emphasis box="[432,463,1477,1500]" italics="true" pageId="112" pageNumber="113">Ch</emphasis>
.) 
<emphasis box="[488,628,1477,1500]" italics="true" pageId="112" pageNumber="113">pastiniformis</emphasis>
