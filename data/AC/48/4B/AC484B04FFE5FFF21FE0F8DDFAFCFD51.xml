<subSubSection pageId="17" pageNumber="18" type="materials_examined">
<paragraph blockId="17.[151,1436,151,756]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[198,377,189,215]" pageId="17" pageNumber="18">Type material</emphasis>
(
<specimenCount box="[397,565,189,215]" pageId="17" pageNumber="18" type="generic">12 specimens</specimenCount>
). 
<typeStatus box="[589,701,189,215]" pageId="17" pageNumber="18">Holotype</typeStatus>
, female, 
<collectingCountry box="[815,973,189,215]" name="South Africa" pageId="17" pageNumber="18">South Africa</collectingCountry>
, 
<collectingRegion box="[989,1167,189,215]" country="South Africa" name="Western Cape" pageId="17" pageNumber="18">Cape Province</collectingRegion>
, Richtersveld N Park, Koeroegabvlakte, S28°11' / E17°03', 17.–21. and 24.ix.1995, leg. F.W., S.K. and 
<collectionCode box="[1114,1135,228,254]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="17" pageNumber="18">R</collectionCode>
