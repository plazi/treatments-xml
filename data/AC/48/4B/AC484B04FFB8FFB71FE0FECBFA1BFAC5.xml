<caption ID-DOI="http://doi.org/10.5281/zenodo.5345669" ID-Zenodo-Dep="5345669" httpUri="https://zenodo.org/record/5345669/files/figure.png" pageId="83" pageNumber="84" startId="83.[151,255,1705,1729]" targetBox="[283,1303,157,1680]" targetPageId="83">
<paragraph blockId="83.[151,1436,1705,1832]" pageId="83" pageNumber="84">
<emphasis bold="true" box="[151,307,1705,1729]" pageId="83" pageNumber="84">FIGURE 53.</emphasis>
<taxonomicName authorityName="Timmermann &amp; Kuhlmann" authorityYear="2009" box="[326,434,1705,1728]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="genus">
<emphasis box="[326,434,1705,1728]" italics="true" pageId="83" pageNumber="84">Patellapis</emphasis>
</taxonomicName>
<taxonomicName authorityName="Cockerell" authorityYear="1946" box="[453,661,1705,1728]" class="Insecta" family="Halictidae" genus="Halictus" kingdom="Animalia" order="Hymenoptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="capillipalpis" subGenus="Chaetalictus">
(
