</taxonomicName>
<taxonomicName authorityName="Timmermann &amp; Kuhlmann" authorityYear="2009" box="[453,698,1558,1582]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="species" species="gruenebergensis" subGenus="Chaetalictus">
(
<emphasis box="[461,492,1559,1582]" italics="true" pageId="167" pageNumber="168">Ch</emphasis>
.) 
<emphasis box="[523,698,1559,1582]" italics="true" pageId="167" pageNumber="168">gruenebergensis</emphasis>
</taxonomicName>
