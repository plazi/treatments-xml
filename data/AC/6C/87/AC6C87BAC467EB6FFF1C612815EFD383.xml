<taxonomicName baseAuthorityName="A.Berlese" baseAuthorityYear="1904" box="[1072,1149,1075,1096]" class="Arachnida" family="Alycidae" genus="Laminamichaelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acariformes" pageId="3" pageNumber="304" phylum="Arthropoda" rank="species" species="setigera">
<emphasis box="[1072,1149,1075,1096]" italics="true" pageId="3" pageNumber="304">setigera</emphasis>
