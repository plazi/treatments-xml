<subSubSection pageId="14" pageNumber="315" type="materials_examined">
<paragraph blockId="14.[151,1437,1497,2028]" pageId="14" pageNumber="315">
<emphasis bold="true" box="[199,368,1713,1739]" pageId="14" pageNumber="315">Type material.</emphasis>
