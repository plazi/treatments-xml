<paragraph pageId="null" pageNumber="433">
<pageBreakToken pageId="null" pageNumber="433" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName authority="Hoppe" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="433" phylum="Tracheophyta" rank="species" species="contigua">
Carex 
<normalizedToken originalValue="contígua" pageId="null" pageNumber="433">contigua</normalizedToken>
Hoppe
</taxonomicName>
</paragraph>
