<subSubSection pageId="null" pageNumber="433" type="vernacular_name">
<paragraph pageId="null" pageNumber="433">
<normalizedToken originalValue="Gedrängtährige" pageId="null" pageNumber="433">Gedraengtaehrige</normalizedToken>
Segge
</paragraph>
</subSubSection>
