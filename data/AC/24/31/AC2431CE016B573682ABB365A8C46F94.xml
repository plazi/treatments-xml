Lake Bolshoy Kharbey (
<geoCoordinate degrees="67" direction="north" minutes="32" orientation="latitude" precision="15" seconds="50" value="67.54722">67°32'50&quot;N</geoCoordinate>
, 
<geoCoordinate degrees="62" direction="east" minutes="52" orientation="longitude" precision="1" seconds="26.3" value="62.87397">62°52'26.3&quot;E</geoCoordinate>
