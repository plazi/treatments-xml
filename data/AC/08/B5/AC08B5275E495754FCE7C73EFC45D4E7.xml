<collectingRegion country="United States of America" name="North Carolina" pageId="1" pageNumber="112">North Carolina</collectingRegion>
, and 
<collectingRegion box="[998,1095,1647,1671]" country="United States of America" name="Virginia" pageId="1" pageNumber="112">Virginia</collectingRegion>
(
<bibRefCitation author="Grubbs, S. A. &amp; R. E. DeWalt" box="[1108,1378,1647,1671]" pageId="1" pageNumber="112" refId="ref1263" refString="Grubbs, S. A. &amp; R. E. DeWalt. 2008. Taxonomic and" type="book" year="2008">Grubbs &amp; DeWalt 2008</bibRefCitation>
