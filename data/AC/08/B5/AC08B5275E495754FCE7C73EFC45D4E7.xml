<collectingRegion box="[1363,1443,1540,1564]" country="United States of America" name="Illinois" pageId="1" pageNumber="112">Illinois</collectingRegion>
and 
<collectingRegion box="[892,982,1576,1600]" country="United States of America" name="Indiana" pageId="1" pageNumber="112">Indiana</collectingRegion>
