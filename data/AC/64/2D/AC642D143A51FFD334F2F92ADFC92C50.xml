<quantity box="[230,390,1846,1870]" metricMagnitude="3" metricUnit="m" metricValue="2.55" metricValueMax="4.3" metricValueMin="0.8" pageId="24" pageNumber="317" unit="m" value="2550.0" valueMax="4300.0" valueMin="800.0">800–4,300 m</quantity>
