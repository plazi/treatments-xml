<quantity box="[509,691,1846,1870]" metricMagnitude="3" metricUnit="m" metricValue="2.5" metricValueMax="3.0" metricValueMin="2.0" pageId="24" pageNumber="317" unit="m" value="2500.0" valueMax="3000.0" valueMin="2000.0">2,000–3,000 m</quantity>
