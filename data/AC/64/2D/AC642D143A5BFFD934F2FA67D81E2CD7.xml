<quantity box="[311,454,1941,1965]" metricMagnitude="-1" metricUnit="m" metricValue="2.75" metricValueMax="3.7" metricValueMin="1.8" pageId="18" pageNumber="311" unit="cm" value="27.5" valueMax="37.0" valueMin="18.0">18 to 37 cm</quantity>
