<collectingCountry box="[1354,1436,260,285]" name="Iceland" pageId="13" pageNumber="306">Iceland</collectingCountry>
to 
<collectingCountry box="[180,279,295,320]" name="Portugal" pageId="13" pageNumber="306">Portugal</collectingCountry>
