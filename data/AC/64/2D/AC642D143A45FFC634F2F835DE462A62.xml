<number box="[1241,1283,224,248]" pageId="13" pageNumber="306" value="300.0">300</number>
and 
<quantity box="[1342,1436,224,248]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="13" pageNumber="306" unit="m" value="2300.0">2,300 m</quantity>
