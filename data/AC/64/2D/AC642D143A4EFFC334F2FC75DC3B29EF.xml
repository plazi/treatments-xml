<paragraph blockId="7.[151,1436,1087,1256]" box="[151,844,1087,1112]" pageId="7" pageNumber="300">
<taxonomicName authority="Rafinesque, 1810" authorityName="Rafinesque" authorityYear="1810" box="[151,572,1087,1112]" class="Elasmobranchii" family="Scyliorhinidae" genus="Galeus" kingdom="Animalia" order="Carcharhiniformes" pageId="7" pageNumber="300" phylum="Chordata" rank="species" species="melastomus">
<emphasis box="[151,369,1087,1112]" italics="true" pageId="7" pageNumber="300">Galeus melastomus</emphasis>
Rafinesque, 
<number box="[516,572,1088,1112]" pageId="7" pageNumber="300" value="1810.0">1810</number>
</taxonomicName>
