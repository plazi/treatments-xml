<number box="[1198,1240,907,931]" pageId="26" pageNumber="319" value="241.0">241</number>
and 
<quantity box="[1294,1388,906,931]" metricMagnitude="-1" metricUnit="m" metricValue="2.66" pageId="26" pageNumber="319" unit="mm" value="266.0">266 mm</quantity>
