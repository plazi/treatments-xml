<number box="[730,772,907,931]" pageId="26" pageNumber="319" value="780.0">780</number>
and 
<quantity box="[826,922,906,931]" metricMagnitude="3" metricUnit="m" metricValue="1.315" pageId="26" pageNumber="319" unit="m" value="1315.0">1,315 m</quantity>
