<number box="[720,762,1267,1291]" pageId="6" pageNumber="299" value="822.0">822</number>
and 
<quantity box="[820,916,1267,1291]" metricMagnitude="3" metricUnit="m" metricValue="1.119" pageId="6" pageNumber="299" unit="m" value="1119.0">1,119 m</quantity>
