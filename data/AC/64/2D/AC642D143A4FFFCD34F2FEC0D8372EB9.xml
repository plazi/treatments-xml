<paragraph blockId="6.[151,1437,1230,1435]" pageId="6" pageNumber="299">
<specimenCount box="[199,353,1266,1291]" pageId="6" pageNumber="299" type="generic">
<number box="[199,227,1267,1291]" pageId="6" pageNumber="299" value="17.0">17</number>
specimens
