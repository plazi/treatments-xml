<paragraph blockId="29.[151,534,151,178]" box="[151,534,151,178]" pageId="29" pageNumber="480">
<heading bold="true" box="[151,534,151,178]" fontSize="11" level="1" pageId="29" pageNumber="480" reason="1">
<taxonomicName authority="Hartig, 1843" authorityName="Hartig" authorityYear="1843" box="[151,534,151,178]" class="Insecta" family="Cynipidae" genus="Synophrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="480" phylum="Arthropoda" rank="species" species="politus">
<emphasis bold="true" box="[151,534,151,178]" pageId="29" pageNumber="480">
<emphasis bold="true" box="[151,369,151,177]" italics="true" pageId="29" pageNumber="480">Synophrus politus</emphasis>
<bibRefCitation author="Hartig, T." box="[377,534,152,178]" pageId="29" pageNumber="480" pagination="395 - 422" refId="ref33881" refString="Hartig, T. (1843) Zweiter nachtrag zur naturgeschichte der Gallwespen. Zeitschrift fur Entomologie, 4, 395 - 422." type="journal article" year="1843">Hartig, 1843</bibRefCitation>
</emphasis>
</taxonomicName>
