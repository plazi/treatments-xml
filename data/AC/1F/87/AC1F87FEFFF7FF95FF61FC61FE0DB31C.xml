<taxonomicName authority="(Giraud)" authorityName="Giraud" baseAuthorityName="Giraud" baseAuthorityYear="1859" box="[681,921,975,1000]" class="Insecta" family="Cynipidae" genus="Aphelonyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="470" phylum="Arthropoda" rank="species" species="cerricola">
<emphasis box="[681,815,976,1000]" italics="true" pageId="19" pageNumber="470">A. cerricola</emphasis>
(Giraud)
</taxonomicName>
, 
<taxonomicName authority="Melika, Stone, Sadeghi" authorityName="Melika, Stone, Sadeghi &amp; Pujade-Villar" authorityYear="2004" box="[932,1314,975,1000]" class="Insecta" family="Cynipidae" genus="Aphelonyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="470" phylum="Arthropoda" rank="species" species="persica">
<emphasis box="[932,1047,976,999]" italics="true" pageId="19" pageNumber="470">A. persica</emphasis>
Melika, Stone, Sadeghi
</taxonomicName>
&amp; Pujade- Villar, and 
<taxonomicName authority="Melika" authorityName="Melika" box="[276,542,1012,1037]" class="Insecta" family="Cynipidae" genus="Aphelonyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="470" phylum="Arthropoda" rank="species" species="kordestanica">
