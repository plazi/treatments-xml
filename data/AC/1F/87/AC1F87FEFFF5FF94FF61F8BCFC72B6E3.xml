<paragraph blockId="17.[151,432,1876,1902]" box="[151,432,1876,1902]" pageId="17" pageNumber="468">
<heading bold="true" box="[151,432,1876,1902]" fontSize="11" level="1" pageId="17" pageNumber="468" reason="1">
<emphasis bold="true" box="[151,432,1876,1902]" pageId="17" pageNumber="468">
<taxonomicName authorityName="Hartig" authorityYear="1840" box="[151,262,1876,1902]" class="Insecta" family="Cynipidae" genus="Andricus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="468" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,262,1876,1902]" italics="true" pageId="17" pageNumber="468">Andricus</emphasis>
</taxonomicName>
sp. nr. 
<taxonomicName box="[352,432,1876,1902]" class="Insecta" family="Cynipidae" genus="Andricus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="468" phylum="Arthropoda" rank="species" species="istvani">
