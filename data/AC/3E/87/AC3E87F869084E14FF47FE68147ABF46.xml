<taxonomicName box="[655,783,620,644]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="longipes">A. longipes</taxonomicName>
, 
<taxonomicName box="[795,903,620,644]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="milloti">A. milloti</taxonomicName>
, 
<taxonomicName box="[914,1008,620,644]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="remyi">A. remyi</taxonomicName>
</emphasis>
and 
<taxonomicName box="[1063,1192,620,644]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="succinea" status="sp. nov.">
