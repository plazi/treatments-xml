, 
<taxonomicName box="[1140,1267,584,608]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="capitata">A. capitata</taxonomicName>
, 
<taxonomicName box="[1281,1398,584,608]" class="Arachnida" family="Triaenonychidae" genus="Triacumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="cowani">A. cowani</taxonomicName>
, 
<taxonomicName class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="flavispina">A. flavispina</taxonomicName>
, 
<taxonomicName box="[275,391,620,644]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="hispida">A. hispida</taxonomicName>
