<subSubSection pageId="8" pageNumber="48" type="materials_examined">
<paragraph blockId="8.[151,1436,342,956]" box="[151,1432,342,367]" pageId="8" pageNumber="48">
<emphasis bold="true" box="[151,268,343,367]" pageId="8" pageNumber="48">
