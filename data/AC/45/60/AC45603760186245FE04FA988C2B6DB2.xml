<td box="[913,1079,1567,1587]" gridcol="2" gridrow="7" pageId="9" pageNumber="236">156–179</td>
<td box="[1136,1148,1567,1587]" gridcol="3" gridrow="7" pageId="9" pageNumber="236">7</td>
</tr>
<tr box="[125,1148,1594,1614]" gridrow="8" pageId="9" pageNumber="236">
<td box="[125,446,1594,1614]" gridcol="0" gridrow="8" pageId="9" pageNumber="236">Diameter at base</td>
<td box="[647,852,1594,1614]" gridcol="1" gridrow="8" pageId="9" pageNumber="236">88.0¡9.1</td>
<td box="[913,1079,1594,1614]" gridcol="2" gridrow="8" pageId="9" pageNumber="236">78–101</td>
<td box="[1136,1148,1594,1614]" gridcol="3" gridrow="8" pageId="9" pageNumber="236">7</td>
</tr>
