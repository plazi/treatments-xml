. (
<taxonomicName authorityName="Pocock" authorityYear="1901" box="[515,620,1616,1640]" class="Chilopoda" family="Henicopidae" genus="Paralamyctes" kingdom="Animalia" order="Lithobiomorpha" pageId="13" pageNumber="39" phylum="Arthropoda" rank="subGenus" subGenus="Haasiella">
