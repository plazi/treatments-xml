<emphasis bold="true" box="[199,352,692,717]" pageId="1" pageNumber="175">Distribution.</emphasis>
<collectingCountry box="[359,426,692,717]" name="China" pageId="1" pageNumber="175">China</collectingCountry>
(
<collectingRegion box="[441,526,692,717]" country="China" name="Hainan" pageId="1" pageNumber="175">Hainan</collectingRegion>
, 
<collectingRegion box="[537,627,692,716]" country="China" name="Yunnan" pageId="1" pageNumber="175">Yunnan</collectingRegion>
