<subSubSection pageId="63" pageNumber="64" type="description">
<paragraph pageId="63" pageNumber="64">
<pageBreakToken pageId="63" pageNumber="64" start="start">WORKER</pageBreakToken>
. Measurements: HL 0.58
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.64, HW 0.46
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.52, HS 0.525
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.580, SL 0.40
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.45, PrW 0.34
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.40, WL 0.75
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.85, HFL 0.39
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.44, PeNL 0.16
<normalizedToken originalValue="–" pageId="63" pageNumber="64">-</normalizedToken>
0.19, PeH 0.34
