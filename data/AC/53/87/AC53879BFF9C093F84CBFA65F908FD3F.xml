<collectingCountry box="[944,1055,571,593]" name="Uzbekistan" pageId="6" pageNumber="1109">Uzbekistan</collectingCountry>
, 
<collectingCountry box="[216,312,594,616]" name="Tajikistan" pageId="6" pageNumber="1109">Tajikistan</collectingCountry>
