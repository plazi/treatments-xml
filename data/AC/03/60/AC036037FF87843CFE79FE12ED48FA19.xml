One deutonymph, Shushtar, Band-e Qir, 
<geoCoordinate box="[978,1141,699,722]" degrees="31" direction="north" minutes="39" orientation="latitude" pageId="14" pageNumber="337" precision="15" seconds="10" value="31.652777">31°39'10&quot; N</geoCoordinate>
, 
<geoCoordinate box="[1156,1315,699,722]" degrees="48" direction="east" minutes="53" orientation="longitude" pageId="14" pageNumber="337" precision="15" seconds="09" value="48.885834">48°53'09&quot; E</geoCoordinate>
, 
<quantity box="[1330,1390,699,722]" metricMagnitude="1" metricUnit="m" metricValue="2.6" pageId="14" pageNumber="337" unit="m" value="26.0">26m</quantity>
