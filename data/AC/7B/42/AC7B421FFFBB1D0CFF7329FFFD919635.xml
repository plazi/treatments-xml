<paragraph blockId="1.[151,1435,1245,1309]" pageId="1" pageNumber="66">
<emphasis bold="true" box="[151,324,1245,1269]" pageId="1" pageNumber="66">
FIGURES 
<number box="[278,319,1245,1269]" pageId="1" pageNumber="66" value="3.5" valueMax="6.0" valueMin="1.0">1–6</number>
