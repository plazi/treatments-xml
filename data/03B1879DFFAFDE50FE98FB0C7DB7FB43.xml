<emphasis id="B96CEA99FFAFDE52FE98FB0C7C06FB43" bold="true" box="[264,865,1244,1270]" pageId="24" pageNumber="25">
<taxonomicName id="4C184D08FFAFDE52FE98FB0C7DA5FB43" box="[264,706,1244,1270]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" kingdom="Animalia" order="Trichoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="circulata" status="new species" subGenus="Feropsyche">
<emphasis id="B96CEA99FFAFDE52FE98FB0C7ECEFB43" bold="true" box="[264,425,1244,1270]" italics="true" pageId="24" pageNumber="25">Helicopsyche</emphasis>
(
<emphasis id="B96CEA99FFAFDE52FE2AFB0C7D21FB43" bold="true" box="[442,582,1244,1270]" italics="true" pageId="24" pageNumber="25">Feropsyche</emphasis>
) 
<emphasis id="B96CEA99FFAFDE52FDC7FB0C7DA5FB43" bold="true" box="[599,706,1244,1270]" italics="true" pageId="24" pageNumber="25">circulata</emphasis>
</taxonomicName>
