<emphasis id="B932EADEFFA3FFF9FDF3F8B7FD32F858" bold="true" box="[550,663,1914,1940]" pageId="10" pageNumber="576">
<collectingCountry id="F351765CFFA3FFF9FDF3F8B7FD32F858" box="[550,663,1914,1940]" name="Taiwan" pageId="10" pageNumber="576">TAIWAN</collectingCountry>
</emphasis>
, 
<location id="8E996017FFA3FFF9FD73F8B7FCE5F858" LSID="urn:lsid:plazi:treatment:03EF87DAFFA3FFFFFF42FE7BFC21FE1D:8E996017FFA3FFF9FD73F8B7FCE5F858" box="[678,832,1914,1940]" country="Taiwan" municipality="Tsuifeng" name="Nantou-hsien" pageId="10" pageNumber="576">Nantou-hsien</location>
, 
