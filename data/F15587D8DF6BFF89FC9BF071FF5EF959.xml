<emphasis id="4B88EADCDF6AFF86FBC4F565FA1FF9B5" bold="true" box="[1133,1147,1628,1645]" pageId="7" pageNumber="14">C</emphasis>
, copulatory complex (ventral view); 
<emphasis id="4B88EADCDF6AFF86FEF0F548FF03F95A" bold="true" box="[345,359,1649,1666]" pageId="7" pageNumber="14">D</emphasis>
, hook; 
<emphasis id="4B88EADCDF6AFF86FE0DF548FFD4F95A" bold="true" box="[420,432,1649,1666]" pageId="7" pageNumber="14">E</emphasis>
, ventral bar; 
<emphasis id="4B88EADCDF6AFF86FDB5F548FC43F95A" bold="true" box="[540,551,1649,1666]" pageId="7" pageNumber="14">F</emphasis>
, dorsal bar; 
<emphasis id="4B88EADCDF6AFF86FD26F548FCF9F95A" bold="true" box="[655,669,1649,1666]" pageId="7" pageNumber="14">G</emphasis>
, ventral anchor; 
<emphasis id="4B88EADCDF6AFF86FC8EF548FD51F95A" bold="true" box="[807,821,1649,1666]" pageId="7" pageNumber="14">H</emphasis>
