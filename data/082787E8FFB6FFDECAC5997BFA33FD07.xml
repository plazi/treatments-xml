<taxonomicName id="478E4D7DFFB6FFDECAC5997BFB54FDF0" authority="Achard, 1920" authorityName="Achard" authorityYear="1920" box="[1016,1251,541,565]" class="Insecta" family="Staphylinidae" genus="Sapitia" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="92" phylum="Arthropoda" rank="genus">
<emphasis id="B2FAEAECFFB6FFDECAC5997BFB54FDF0" bold="true" box="[1016,1251,541,565]" pageId="17" pageNumber="92">
<emphasis id="B2FAEAECFFB6FFDECAC5997BFBF2FDF0" bold="true" box="[1016,1093,541,565]" italics="true" pageId="17" pageNumber="92">Sapitia</emphasis>
<bibRefCitation id="E41F4B0FFFB6FFDECD769978FB54FDF0" author="Achard J." box="[1099,1251,542,565]" pageId="17" pageNumber="92" pagination="123 - 136" refId="ref30774" refString="Achard J. 1920. Notes sur les Scaphidiidae de la faune indomalaise. Annales de la Societe Entomologique de Belgique 60: 123 - 136." type="journal article" year="1920">Achard, 1920</bibRefCitation>
