<emphasis id="C6BA916B2D0EFF95FF79FF79EE3E712D" bold="true" box="[198,358,1541,1567]" pageId="61" pageNumber="62">Distribution.</emphasis>
<taxonomicName id="33CE36FA2D0EFF95FECAFF79EE97712C" authorityName="Fibiger" authorityYear="2010" box="[373,463,1541,1566]" class="Insecta" family="Erebidae" genus="Asyprocessa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="wapi">
<emphasis id="C6BA916B2D0EFF95FECAFF79EE97712C" box="[373,463,1541,1566]" italics="true" pageId="61" pageNumber="62">A. wapi</emphasis>
</taxonomicName>
known only from South 
