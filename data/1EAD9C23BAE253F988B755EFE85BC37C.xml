<taxonomicName id="53E7FD5C79344CE8D30282C5BF1BCF67" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" kingdom="Plantae" lsidName="" order="Fagales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="family">Juglandaceae</taxonomicName>
