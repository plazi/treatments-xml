<taxonomicName id="098D9F4EDFE936AC3166D4ABFFC585E6" class="Magnoliopsida" family="Ulmaceae" genus="Ulmus" kingdom="Plantae" lsidName="Ulmus" order="Rosales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="genus">
