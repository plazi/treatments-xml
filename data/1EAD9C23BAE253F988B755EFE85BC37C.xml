<taxonomicName id="A6D33B4CE0A6FEF22D45CB5DCE29A755" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" kingdom="Plantae" lsidName="Juglans regia" order="Fagales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="species" species="regia">
