<paragraph id="06A8C85CE5307AE2181F7C42EE961BE6" pageId="11" pageNumber="12">
<taxonomicName id="7F3507B76A7D5B9DCE23ABF556567160" class="Hydrozoa" family="Stylasteridae" genus="Errinopora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Errinopora fisheri" order="Anthoathecata" pageId="11" pageNumber="12" phylum="Cnidaria" rank="species" species="fisheri">Errinopora fisheri</taxonomicName>
