<taxonomicName id="4C714D6DFC404F146B28FEF5FC7FFEF6" authority="Kuang, 1997" authorityName="Kuang" authorityYear="1997" box="[628,922,272,298]" class="Arachnida" family="Eriophyidae" genus="Abacarus" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="42" phylum="Arthropoda" rank="species" species="phalaris">
