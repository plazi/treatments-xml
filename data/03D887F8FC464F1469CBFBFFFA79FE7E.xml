<taxonomicName id="4C714D6DFC404F146DC2FE85FAC7FEA5" box="[1182,1314,352,377]" class="Arachnida" family="Eriophyidae" genus="Abacarus" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="42" phylum="Arthropoda" rank="species" species="phalaris">
