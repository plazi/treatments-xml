<taxonomicName id="53E7C0C7FFAB4939ECBBFE3E6589106F" authority="Barbour" authorityName="Barbour" authorityYear="1938" box="[972,1194,510,534]" class="Amphibia" family="Bufonidae" genus="Pelophryne" kingdom="Animalia" order="Anura" pageId="4" pageNumber="531" phylum="Chordata" rank="genus">
<emphasis id="A6936756FFAB4939ECBBFE3E6569106C" bold="true" box="[972,1098,510,534]" italics="true" pageId="4" pageNumber="531">Pelophryne</emphasis>
Barbour
</taxonomicName>
(
<taxonomicName id="53E7C0C7FFAB4939EBCDFE3E640E106F" authorityName="Gray" authorityYear="1825" box="[1210,1325,510,533]" class="Amphibia" family="Bufonidae" kingdom="Animalia" order="Anura" pageId="4" pageNumber="531" phylum="Chordata" rank="family">Bufonidae</taxonomicName>
)
</heading>
</paragraph>
