<taxonomicName id="428EE8C29EF6E38CFEEAF9C0DA40651F" authorityName="Quicke &amp; Butcher" authorityYear="2012" box="[304,380,1593,1615]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="147" pageNumber="148" phylum="Arthropoda" rank="species" species="opus" status="sp. nov.">
