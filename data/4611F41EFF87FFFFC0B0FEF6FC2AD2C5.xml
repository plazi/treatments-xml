<collectingCountry id="B6AF0598FF87FFFFC206FA14FCA5D18B" box="[545,794,1456,1482]" name="New Caledonia" pageId="13" pageNumber="58">
<collectionCode id="A8A9DDCDFF87FFFFC206FA14FDDBD18B" box="[545,612,1456,1482]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="13" pageNumber="58">NEW</collectionCode>
