<quantity id="0940E8EDFF87FFFFC3EAFA14FBA5D18B" box="[973,1050,1456,1482]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="13" pageNumber="58" unit="m" value="750.0">750 m</quantity>
, 
<date id="BA0663C8FF87FFFFC401FA14FB27D18B" box="[1062,1176,1456,1482]" pageId="13" pageNumber="58" value="1960-03-03">
<number id="7870572FFF87FFFFC401FA14FB8BD18B" box="[1062,1076,1456,1482]" pageId="13" pageNumber="58" value="3.0">3</number>
