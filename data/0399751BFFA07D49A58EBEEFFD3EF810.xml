<taxonomicName id="4C30BF8EFFA07D49A798BF33FD29F834" authorityName="Schmid" authorityYear="1959" box="[641,758,1914,1938]" class="Insecta" family="Stenopsychidae" genus="Stenopsyche" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="220" phylum="Arthropoda" rank="species" species="simplex">
