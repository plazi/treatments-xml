<collectingCountry id="63E70FC57A7B2C47FBF5F9C1AC83F9D6" box="[1106,1176,1554,1578]" name="China" pageId="4" pageNumber="5">China</collectingCountry>
, 
<collectingCountry id="63E70FC57A7B2C47FB0FF9C1ACE6F9D6" box="[1192,1277,1554,1578]" name="Taiwan" pageId="4" pageNumber="5">Taiwan</collectingCountry>
, 
<collectingCountry id="63E70FC57A7B2C47FAAAF9C0AD83F9D7" box="[1293,1432,1555,1579]" name="Hong Kong" pageId="4" pageNumber="5">Hong Kong</collectingCountry>
, Raos, 
<collectingCountry id="63E70FC57A7B2C47FF7AF9E5A958F9B2" box="[221,323,1590,1614]" name="Thailand" pageId="4" pageNumber="5">Thailand</collectingCountry>
, 
<collectingCountry id="63E70FC57A7B2C47FEE9F9E5A9ABF9B2" box="[334,432,1590,1614]" name="Vietnam" pageId="4" pageNumber="5">Vietnam</collectingCountry>
