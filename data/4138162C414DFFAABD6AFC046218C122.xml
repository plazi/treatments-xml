<taxonomicName id="0E91DCB9414DFFADBD6AFC046686C786" authority="Norton, Metz et Sharma, 1978" authorityName="Norton, Metz et Sharma" authorityYear="1978" box="[835,1420,922,947]" class="Arachnida" family="Lohmanniidae" genus="Lohmannia" kingdom="Animalia" order="Sarcoptiformes" pageId="3" pageNumber="249" phylum="Arthropoda" rank="species" species="banksi">
