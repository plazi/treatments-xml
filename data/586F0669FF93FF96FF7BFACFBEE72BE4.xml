<taxonomicName id="17C6CCFCFF93FF96FE51F950BEF52A84" authorityName="Malmgren" authorityYear="1866" box="[445,507,1697,1720]" class="Polychaeta" family="Pectinariidae" genus="Lagis" kingdom="Animalia" order="Terebellida" pageId="26" pageNumber="27" phylum="Annelida" rank="genus">
