<collectionCode id="57ADFEDBF226FFAEFCC1FC7CB5F8FBBE" box="[837,909,1004,1030]" country="India" httpUri="http://grbio.org/cool/dqz8-sfkc" name="Madras Museum" pageId="5" pageNumber="6">MAD</collectionCode>
