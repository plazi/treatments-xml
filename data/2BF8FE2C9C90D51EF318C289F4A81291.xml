, 
<taxonomicName id="29762605EC2219192038C9E47C5F229E" LSID-ZBK="urn:lsid:zoobank.org:act:129F5902-2C71-4C93-B999-FA124A15950C" class="Actinopterygii" family="Rivulidae" genus="Austrolebias" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Austrolebias jaegari Costa &amp; Cheffe 2002:84" order="Cyprinodontiformes" pageNumber="22" phylum="Chordata" rank="species" species="jaegari">A. jaegari</taxonomicName>
