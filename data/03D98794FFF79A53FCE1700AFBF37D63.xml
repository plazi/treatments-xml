<taxonomicName id="4C704D01FFF79A54FC9C7376FCDE7E66" authority="de Saint Laurent &amp; Cleva, 1981" authorityName="de Saint Laurent &amp; Cleva" authorityYear="1981" class="Malacostraca" family="Spongicolidae" genus="Paraspongicola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="43" pageNumber="130" phylum="Arthropoda" rank="species" species="pusillus">
