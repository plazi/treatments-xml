<materialsCitation id="F0CF0562FFE3FFD2FF25FED00C79FF4E" ID-GBIF-Occurrence="2028140045" box="[199,875,151,176]" collectionCode="AMS" pageId="36" pageNumber="37" specimenCount="1" typeStatus="holotype">
