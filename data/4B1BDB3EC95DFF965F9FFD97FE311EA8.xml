<location id="C66D3CF3C95DFF965EF1FBF2FD581E17" LSID="urn:lsid:plazi:treatment:4B1BDB3EC95DFF965F9FFD97FE311EA8:C66D3CF3C95DFF965EF1FBF2FD581E17" box="[505,689,1037,1061]" name="central Fromosa" pageId="63" pageNumber="64">central Fromosa</location>
