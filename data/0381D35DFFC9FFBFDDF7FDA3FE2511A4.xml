<taxonomicName id="4C2819C8FFC9FFBFDE21FAF0FD8B1183" authority="Mifsud &amp; Burckhardt, 2002" authorityName="Mifsud &amp; Burckhardt" authorityYear="2002" box="[515,628,1389,1412]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="70" pageNumber="1956" phylum="Arthropoda" rank="species" species="sinuata" status="sp. n.">
<emphasis id="B95CBE59FFC9FFBFDE21FAF0FD8B1183" box="[515,628,1389,1412]" italics="true" pageId="70" pageNumber="1956">P. sinuata</emphasis>
