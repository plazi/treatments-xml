<taxonomicName id="4C2819C8FFC9FFBFDECBFAD0FCC01163" authorityName="Russell" authorityYear="1943" box="[745,831,1357,1380]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="70" pageNumber="1956" phylum="Arthropoda" rank="species" species="gossypii">
