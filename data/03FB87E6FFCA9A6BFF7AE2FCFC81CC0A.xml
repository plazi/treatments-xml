<taxonomicName id="4C524D73FFCD9A6AFEC4E268FD47CDC2" authority="(Fischer, 1969)" authorityName="Fischer" authorityYear="1969" baseAuthorityName="Fischer" baseAuthorityYear="1969" box="[297,643,1652,1674]" class="Insecta" family="Braconidae" genus="Dinotrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="saileri">
