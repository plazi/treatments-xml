<vernacularName id="D9CC46B8FFDDFFCDFF31F8D36E0AE75F" box="[165,322,1832,1853]" pageId="8" pageNumber="313">Quistiti a camail</vernacularName>
/ 
<emphasis id="65BBEA84FFDDFFCDFEC3F8D36EFBE75F" box="[343,435,1832,1853]" pageId="8" pageNumber="313">German:</emphasis>
<vernacularName id="D9CC46B8FFDDFFCDFE2FF8D36D9FE75F" box="[443,727,1832,1853]" pageId="8" pageNumber="313">Weil3schulter-Seidenaffchen</vernacularName>
/ 
<emphasis id="65BBEA84FFDDFFCDFD79F8D36C00E75F" box="[749,840,1832,1853]" pageId="8" pageNumber="313">Spanish:</emphasis>
<vernacularName id="D9CC46B8FFDDFFCDFCC4F8D36CA4E75F" box="[848,1004,1832,1853]" pageId="8" pageNumber="313">Titi deSantarém</vernacularName>
