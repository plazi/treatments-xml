<geoCoordinate id="AE9A5011A807FFD8FC80FAB0FCD90C61" box="[789,868,1381,1403]" degrees="17.05" direction="south" orientation="latitude" pageId="31" pageNumber="31" precision="555" value="-17.05">17.05S</geoCoordinate>
