<geoCoordinate id="AE9A5011A807FFD8FCE5FAB0FC740C61" box="[880,969,1381,1403]" degrees="56.36" direction="west" orientation="longitude" pageId="31" pageNumber="31" precision="555" value="-56.36">56.36W</geoCoordinate>
(
<bibRefCitation id="AF3F4B27A807FFD8FC42FAB0FD4A0C83" author="Paynter, R. A., Jr &amp; M. A. Traylor, Jr." pageId="31" pageNumber="31" refId="ref100362" refString="Paynter, R. A., Jr, and M. A. Traylor, Jr. 1991. Ornithological gazetteer of Brazil, 2 volumes. Cambridge, MA: Museum of Comparative Zoology, viii + 789 pp., 2 maps." type="book" year="1991">Paynter and Traylor, 1991</bibRefCitation>
), 
<collectingRegion id="096AF834A807FFD8FC86FA56FC0A0C82" box="[787,951,1411,1432]" country="Brazil" name="Mato Grosso" pageId="31" pageNumber="31">Mato Grosso</collectingRegion>
/ 
<collectingRegion id="096AF834A807FFD8FC55FA56FB7A0C83" box="[960,1223,1411,1433]" country="Brazil" name="Mato Grosso do Sul" pageId="31" pageNumber="31">Mato Grosso do Sul</collectingRegion>
, 
<collectingCountry id="B3B97646A807FFD8FD28FA75FCB80CAC" box="[701,773,1440,1462]" name="Brazil" pageId="31" pageNumber="31">Brazil</collectingCountry>
