<subSubSection id="71D1655342043B1574C7884BEE9DBC7B" pageId="10" pageNumber="9" type="etymology">
<paragraph id="397436D842043B1574C7884BEE9DBC7B" blockId="10.[192,1440,195,347]" pageId="10" pageNumber="9">
<emphasis id="0BBFEACA42043B1574C7884BEB1ABC1B" bold="true" box="[192,349,291,315]" pageId="10" pageNumber="9">Etymology.</emphasis>
