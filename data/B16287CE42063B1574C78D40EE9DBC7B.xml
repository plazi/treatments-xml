<taxonomicName id="FECB4D5B42043B1574C7882BEB17BC7B" box="[192,336,323,347]" class="Insecta" kingdom="Animalia" order="Trichoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="order">Trichoptera</taxonomicName>
