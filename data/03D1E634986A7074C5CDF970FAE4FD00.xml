<quantity id="4C80FAC7986A7076C730F828FD2EF80F" box="[629,764,1962,1983]" metricMagnitude="3" metricUnit="m" metricValue="1.3" metricValueMax="1.4" metricValueMin="1.2" pageId="57" pageNumber="58" unit="m" value="1300.0" valueMax="1400.0" valueMin="1200.0">
<elevation id="0055B011986A7076C730F828FD2EF80F" box="[629,764,1962,1983]" metricMagnitude="3" metricUnit="m" metricValue="1.3" metricValueMax="1.4" metricValueMin="1.2" pageId="57" pageNumber="58" unit="m" value="1300.0" valueMax="1400.0" valueMin="1200.0">1200–1400 m</elevation>
