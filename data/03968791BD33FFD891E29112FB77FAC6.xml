<taxonomicName id="4C3F4D04BD33FFD8911C9403FC4CFBC8" authority="(Voss, 1962)" baseAuthorityName="Voss" baseAuthorityYear="1962" box="[630,923,1049,1076]" class="Cephalopoda" family="Sepiolidae" genus="Inioteuthis" higherTaxonomySource="CoL" kingdom="Animalia" order="Sepiida" pageId="41" pageNumber="42" phylum="Mollusca" rank="species" species="capensis">
