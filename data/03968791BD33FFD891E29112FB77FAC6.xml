<taxonomicName id="4C3F4D04BD33FFD893D79400FDEAFBCF" authority="(Naef, 1912)" baseAuthorityName="Naef" baseAuthorityYear="1912" box="[189,573,1049,1075]" class="Cephalopoda" family="Sepiolidae" genus="Rondeletiola" kingdom="Animalia" order="Sepiolida" pageId="41" pageNumber="42" phylum="Mollusca" rank="species" species="minor">
