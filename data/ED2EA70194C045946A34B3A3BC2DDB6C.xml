<taxonomicName id="C618E3CB66FF67BB92274ED5E69E81F2" authority="Foerster, 1869" authorityName="Foerster" authorityYear="1869" class="Insecta" family="Charipidae" genus="Phaenoglyphis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Phaenoglyphis" order="Hymenoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="genus">
