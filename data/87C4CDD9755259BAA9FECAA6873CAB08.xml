<paragraph id="A80D2184ACA48BAA2BFF9802A92306BC" pageId="128" pageNumber="129">
Genus 
<taxonomicName id="862A97457053379C2A9C3D1A2CEF0A97" LSID="87C4CDD9-7552-59BA-A9FE-CAA6873CAB08" authority="Ueno, 1955" authorityName="Ueno" authorityYear="1955" class="Insecta" family="Carabidae" genus="Nippononebria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Nippononebria" order="Coleoptera" pageId="128" pageNumber="129" phylum="Arthropoda" rank="genus">
Nippononebria 
<normalizedToken id="8058939EE5A0DBEFECDE895900B0296C" originalValue="Uéno">Ueno</normalizedToken>
