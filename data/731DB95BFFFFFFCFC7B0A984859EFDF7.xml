<taxonomicName id="3CB473CEFFFFFFCFC727A8C2845FFE17" authority="Balss, 1935" authorityName="Balss" authorityYear="1935" box="[293,597,480,505]" class="Malacostraca" family="Xanthidae" genus="Medaeus" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="922" phylum="Arthropoda" rank="species" species="rouxi">
