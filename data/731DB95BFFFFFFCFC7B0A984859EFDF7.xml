<taxonomicName id="3CB473CEFFFFFFCFC53DA8C2822BFE16" baseAuthorityName="Balss" baseAuthorityYear="1935" box="[831,1057,481,505]" class="Malacostraca" family="Pilumnidae" genus="Nanopilumnus" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="922" phylum="Arthropoda" rank="species" species="rouxi">
