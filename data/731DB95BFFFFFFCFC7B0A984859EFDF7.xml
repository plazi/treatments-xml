<taxonomicName id="3CB473CEFFFFFFCFC7C8A8E38463FE37" authorityName="Takeda" authorityYear="1974" box="[458,617,448,472]" class="Malacostraca" family="Pilumnidae" genus="Nanopilumnus" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="922" phylum="Arthropoda" rank="genus">
