<taxonomicName id="77E44D584134FFD7FF1F61BB9B633C6A" box="[158,326,1262,1288]" class="Magnoliopsida" family="Aquifoliaceae" kingdom="Plantae" order="Aquifoliales" pageId="9" pageNumber="62" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
