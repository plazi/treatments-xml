<taxonomicName id="77E44D584134FFD7FD22619D99113D80" box="[675,820,1224,1250]" class="Magnoliopsida" family="Araliaceae" kingdom="Plantae" order="Apiales" pageId="9" pageNumber="62" phylum="Tracheophyta" rank="family">Araliaceae</taxonomicName>
