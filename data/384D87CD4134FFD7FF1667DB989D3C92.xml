<taxonomicName id="77E44D584134FFD7FBFA619D9F023D83" box="[1147,1319,1224,1249]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="9" pageNumber="62" phylum="Tracheophyta" rank="species" species="anomala">
