<subSubSection id="C344652ED45EFFD6FF681073FE476B5D" pageId="18" pageNumber="19" type="biology_ecology">
<paragraph id="8BE136A5D45EFFD6FF681073FE476B5D" blockId="18.[140,1108,518,934]" pageId="18" pageNumber="19">
<emphasis id="B92AEAB7D45EFFD6FF681073FF236B1A" bold="true" box="[140,238,518,544]" pageId="18" pageNumber="19">
