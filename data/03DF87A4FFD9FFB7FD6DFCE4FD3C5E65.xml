<quantity id="4C8E9B57FFD5FFB7FE0FF8BBFD205E65" box="[489,654,488,515]" metricMagnitude="3" metricUnit="m" metricValue="2.775" metricValueMax="2.85" metricValueMin="2.7" pageId="26" pageNumber="27" unit="m" value="2775.0" valueMax="2850.0" valueMin="2700.0">2700–2850 m</quantity>
