, 
<taxonomicName id="724ED312D79364D5233F587F55A9E4A9" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1853" class="Gastropoda" family="Orthalicidae" genus="Porphyrobaphe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Porphyrobaphe (Porphyrobaphe) iris" order="Stylommatophora" pageId="15" pageNumber="32" phylum="Mollusca" rank="species" species="iris" subGenus="Porphyrobaphe">Porphyrobaphe (Porphyrobaphe) iris</taxonomicName>
