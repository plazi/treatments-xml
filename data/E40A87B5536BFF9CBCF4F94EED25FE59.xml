<taxonomicName id="ABA34D20536BFF9FB84CF866EACDF834" box="[1071,1203,1919,1944]" class="Arachnida" family="Salticidae" genus="Tauala" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="513" phylum="Arthropoda" rank="species" species="bilobatus">
<emphasis id="5ED7EAB1536BFF9FB84CF866EA42F83B" box="[1071,1084,1920,1943]" italics="true" pageId="12" pageNumber="513">T</emphasis>
. 
<emphasis id="5ED7EAB1536BFF9FB828F899EACDF834" box="[1099,1203,1919,1944]" italics="true" pageId="12" pageNumber="513">bilobatus</emphasis>
</taxonomicName>
