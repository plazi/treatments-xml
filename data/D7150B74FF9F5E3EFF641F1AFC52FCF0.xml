<taxonomicName id="98BCC1E1FF9E5E3BFADA1F94FA7EF827" box="[1321,1433,1959,1984]" class="Magnoliopsida" family="Combretaceae" genus="Terminalia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Myrtales" pageId="1" pageNumber="110" phylum="Magnoliopsida" rank="genus">Terminalia</taxonomicName>
