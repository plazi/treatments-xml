<emphasis id="D69514473BBFBB9B152F838528528176" bold="true" pageId="8" pageNumber="249">Figure 23.</emphasis>
<taxonomicName id="EE3421AD21673A8885673115BFAE7074" authorityName="Talamas" authorityYear="2016" class="Insecta" family="Platygastroidea" genus="Proteroscelio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Proteroscelio nexus" order="Hymenoptera" pageId="8" pageNumber="249" phylum="Arthropoda" rank="species" species="nexus">
<emphasis id="E19B161DADC65724FC07EB6AA01B0445" italics="true" pageId="8" pageNumber="249">Proteroscelio nexus</emphasis>
</taxonomicName>
