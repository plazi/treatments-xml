<elevation id="006DD1DFDD6F141CFAEB94AB00C9DF99" box="[1341,1431,151,177]" metricMagnitude="3" metricUnit="m" metricValue="2.18" pageId="9" pageNumber="382" unit="m" value="2180.0">2180 m</elevation>
</quantity>
, 
