from 
<taxonomicName id="2DC84D3CA65A6411FCAFC396FC2ACA0B" authorityName="Miller &amp; Hollister" authorityYear="1921" box="[825,986,2033,2055]" class="Mammalia" family="Muridae" genus="Eropeplus" kingdom="Animalia" order="Rodentia" pageId="3" pageNumber="650" phylum="Chordata" rank="species" species="canus">
