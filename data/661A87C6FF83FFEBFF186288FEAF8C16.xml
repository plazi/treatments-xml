<collectingCountry id="96A47640FF83FFE8FDCF652CFD098925" box="[576,678,1853,1879]" name="Russia" pageId="17" pageNumber="88">RUSSIA</collectingCountry>
, 
<collectingRegion id="2C77F832FF83FFE8FD3D652FFCD38925" box="[690,892,1853,1880]" country="Russia" name="Tyumen'" pageId="17" pageNumber="88">Tyumen Province</collectingRegion>
