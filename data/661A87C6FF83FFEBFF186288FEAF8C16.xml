<geoCoordinate id="8B875017FF83FFE8FB2C652DFAA88925" box="[1187,1287,1852,1879]" direction="north" orientation="latitude" pageId="17" pageNumber="88" precision="925" value="57.35">57°21’N</geoCoordinate>
