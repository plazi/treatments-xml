<location id="B07834A8FF81514CDB76FBD341E6FB8D" LSID="urn:lsid:plazi:treatment:3D0ED365FF81514CD9C2FC684168FACA:B07834A8FF81514CDB76FBD341E6FB8D" box="[521,686,1043,1067]" county="Harrison County" latitude="40.42016" longLatPrecision="1" longitude="-86.89705" municipality="In" name="Wabash River" pageId="13" pageNumber="44" stateProvince="Alabama">Wabash River</location>
