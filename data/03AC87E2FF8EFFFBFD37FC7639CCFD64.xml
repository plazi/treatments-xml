<location id="8EDA602FFF8EFFF8FD42FA7D3A08F9B4" LSID="urn:lsid:plazi:treatment:03AC87E2FF8EFFFBFD37FC7639CCFD64:8EDA602FFF8EFFF8FD42FA7D3A08F9B4" box="[701,879,1530,1552]" county="November" municipality="Coopers Camp" name="Melville Island" pageId="53" pageNumber="53">Melville Island</location>
