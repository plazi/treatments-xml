<date id="FFBB1034FF8EFFF8FB4FF99F3A07F9EF" pageId="53" pageNumber="53" value="1912-01-31">
<collectingDate id="EFFFE9DCFF8EFFF8FB4FF99F3A07F9EF" pageId="53" pageNumber="53" value="1912-01-31">31 January 1912</collectingDate>
</date>
). 
<typeStatus id="54BE8856FF8EFFF8FC89F9B13A8EF9EF" box="[886,1001,1590,1611]" pageId="53" pageNumber="53">Paratypes</typeStatus>
