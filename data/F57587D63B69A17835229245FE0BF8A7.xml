<quantity id="BA249B253B69A17A37929726FBA5F9E7" box="[952,1059,1559,1585]" metricMagnitude="-3" metricUnit="m" metricValue="3.92" pageId="4" pageNumber="323" unit="mm" value="3.92">3.92 mm</quantity>
(
<number id="CB1424E73B69A17A301E9726FBEFF9E7" box="[1076,1129,1559,1585]" pageId="4" pageNumber="323" value="1.08">1.08</number>
