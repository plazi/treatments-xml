<taxonomicName id="4C3395F3DD020463D3C0F911FF25A0C6" box="[1232,1389,1686,1709]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" sensu="lato" species="impetuosus">
