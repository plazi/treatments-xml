<quantity id="4CCB4395DD020463D46EF9AEFE51A02A" box="[894,1049,1577,1602]" metricMagnitude="3" metricUnit="m" metricValue="2.3525" metricValueMax="3.656" metricValueMin="1.049" pageId="54" pageNumber="55" unit="m" value="2352.5" valueMax="3656.0" valueMin="1049.0">1049–3656 m</quantity>
