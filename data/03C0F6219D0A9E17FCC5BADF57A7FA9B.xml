<taxonomicName id="4C693CB49D0A9E17FA9FBC3157AEFA9B" baseAuthorityName="Germar" baseAuthorityYear="1823" box="[1325,1429,1343,1366]" class="Insecta" family="Chrysomelidae" genus="Coptocycla" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="674" phylum="Arthropoda" rank="species" species="stigma">
