</emphasis>
<taxonomicName id="4C674D32FFB56945CE41FF3AFD60FF57" authority="MacLeay, 1819" authorityName="MacLeay" authorityYear="1819" box="[313,664,151,176]" class="Insecta" family="Passallidae" genus="Paxillus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="329" phylum="Arthropoda" rank="species" species="leachi">
