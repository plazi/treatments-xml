<quantity id="4CA59B109D5EFFE9FC65FDC9351DFDEF" box="[975,1062,551,575]" metricMagnitude="3" metricUnit="m" metricValue="1.128" pageId="34" pageNumber="685" unit="m" value="1128.0">1128 m</quantity>
