<taxonomicName id="4C56CBAD9A7C4343FB50F961FB4BF94A" box="[1159,1261,1699,1722]" class="Insecta" family="Chrysomelidae" genus="Prionodera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="jacobyi">
