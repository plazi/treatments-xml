<taxonomicName id="4C4D4D15FFACC255E5EAE37197813746" authority="Mello-Leitao 1943: 164" authorityName="Mello-Leitao" authorityPageNumber="164" authorityYear="1943" box="[736,1232,1566,1589]" class="Arachnida" family="Trechaleidae" genus="Demelodos" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="66" phylum="Arthropoda" rank="species" species="iheringi">
<emphasis id="B939EA84FFACC255E5EAE37190E83746" box="[736,953,1566,1589]" italics="true" pageId="0" pageNumber="66">Demelodos iheringi</emphasis>
<treatmentCitation id="0AEC1087FFACC255E4CEE37197813746" author="Mello-Leitao" box="[964,1232,1566,1589]" page="164" pageId="0" pageNumber="66" year="1943">
<bibRefCitation id="EFDC4B67FFACC255E4CEE37197C73746" author="Mello-Leitao" box="[964,1174,1566,1589]" pageId="0" pageNumber="66" refString="Mello-Leitao, C. (1943) Alguns pisauridas e tomisidas do Brasil. Revista Chilena de Historia Natural, 45, 164 - 172." type="journal article" year="1943">
Mello-Leitão 
<number id="3D8524B1FFACC255E357E37197C73746" box="[1117,1174,1566,1589]" pageId="0" pageNumber="66" value="1943.0">1943</number>
</bibRefCitation>
