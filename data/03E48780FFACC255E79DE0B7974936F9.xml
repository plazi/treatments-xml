<taxonomicName id="4C4D4D15FFACC255E79DE371917F3746" authority="Mello-Leitao, 1943: 164" authorityName="Mello-Leitao" authorityPageNumber="164" authorityYear="1943" box="[151,558,1566,1589]" class="Arachnida" family="Trechaleidae" genus="Demelodos" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="66" phylum="Arthropoda" rank="genus">
<emphasis id="B939EA84FFACC255E79DE371925E3746" box="[151,271,1566,1589]" italics="true" pageId="0" pageNumber="66">Demelodos</emphasis>
<treatmentCitation id="0AEC1087FFACC255E611E371917F3746" author="Mello-Leitao" box="[283,558,1566,1589]" page="164" pageId="0" pageNumber="66" year="1943">
<bibRefCitation id="EFDC4B67FFACC255E611E37192A23746" author="Mello-Leitao" box="[283,499,1566,1589]" pageId="0" pageNumber="66" refString="Mello-Leitao, C. (1943) Alguns pisauridas e tomisidas do Brasil. Revista Chilena de Historia Natural, 45, 164 - 172." type="journal article" year="1943">
Mello-Leitão, 
<number id="3D8524B1FFACC255E6B0E37192A23746" box="[442,499,1566,1589]" pageId="0" pageNumber="66" value="1943.0">1943</number>
</bibRefCitation>
