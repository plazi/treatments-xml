<subSubSection id="DA80657CFFCA3918B584FEE0FE04FD0E" pageId="9" pageNumber="238" type="diagnosis">
<paragraph id="922536F7FFCA3918B584FEE0FAA7FE09" blockId="9.[151,1437,259,682]" pageId="9" pageNumber="238">
<emphasis id="A0EEEAE5FFCA3918B584FEE0FEAEFE2D" bold="true" box="[199,314,367,393]" pageId="9" pageNumber="238">Diagnosis</emphasis>
