<materialsCitation id="3B283C95FFC8FFF1FEA38802FF30FA9B" ID-GBIF-Occurrence="3417104308" collectingDate="2010-05-02" collectionCode="P, TAN" collectorName="Aubriot &amp; Allorge &amp; Lehavana" country="Madagascar" latitude="-17.705278" location="Analalava" longLatPrecision="21" longitude="49.45528" municipality="Reg. Analanjirofo" pageId="2" pageNumber="141" specimenCount="1" stateProvince="Prov. Toamasina">
