<collectingCountry box="[364,453,1973,1999]" name="Mexico" pageId="10" pageNumber="28">Mexico</collectingCountry>
(
<collectingRegion box="[470,571,1973,1999]" country="Mexico" name="Hidalgo" pageId="10" pageNumber="28">Hidalgo</collectingRegion>
