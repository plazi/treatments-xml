<taxonomicName class="Insecta" family="Braconidae" genus="Hemichoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemichoma frankhovorei" order="Hymenoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="frankhovorei">
