<number box="[1227,1350,569,595]" pageId="11" pageNumber="12" value="975.5" valueMax="1066.0" valueMin="885.0">885–1066</number>
); mean breadth 
<number box="[246,290,606,632]" pageId="11" pageNumber="12" value="437.0">437</number>
(range 
<number box="[379,487,606,632]" pageId="11" pageNumber="12" value="446.5" valueMax="498.0" valueMin="395.0">395–498</number>
