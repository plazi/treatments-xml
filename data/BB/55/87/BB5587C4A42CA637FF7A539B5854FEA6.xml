<number box="[151,288,1418,1444]" pageId="8" pageNumber="9" value="1279.5" valueMax="1390.0" valueMin="1169.0">1169–1390</number>
</date>
); mean breadth 
<number box="[478,522,1418,1444]" pageId="8" pageNumber="9" value="615.0">615</number>
(range 
<number box="[614,722,1418,1444]" pageId="8" pageNumber="9" value="620.5" valueMax="672.0" valueMin="569.0">569–672</number>
); males (
<emphasis box="[830,845,1418,1443]" italics="true" pageId="8" pageNumber="9">n</emphasis>
