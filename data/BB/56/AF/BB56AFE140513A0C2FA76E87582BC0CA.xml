<subSubSection pageId="1" pageNumber="76" type="nomenclature">
<paragraph pageId="1" pageNumber="76">
<taxonomicName LSID="http://zoobank.org/0499D286-4426-4DF2-A379-02BC548E288F" class="Insecta" family="Staphylinidae" genus="Doryloxenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Doryloxenus aenictophilus" order="Coleoptera" pageId="1" pageNumber="76" phylum="Arthropoda" rank="species" species="aenictophilus">Doryloxenus aenictophilus</taxonomicName>
