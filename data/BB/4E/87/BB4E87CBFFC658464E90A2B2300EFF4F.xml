<taxonomicName box="[288,532,1393,1416]" class="Malacostraca" family="Melitidae" genus="Quadrimaera" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="chaelata" status="sp. nov.">
<emphasis box="[288,532,1393,1416]" italics="true" pageId="4" pageNumber="59">
Quadrimaera 
<taxonomicName box="[440,532,1393,1416]" pageId="4" pageNumber="59" rank="species" species="chaelata" status="sp. nov.">chaelata</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis bold="true" box="[538,623,1392,1416]" pageId="4" pageNumber="59">
<taxonomicNameLabel box="[538,623,1392,1416]" pageId="4" pageNumber="59" rank="species">sp. nov.</taxonomicNameLabel>
