<subSubSection pageId="4" pageNumber="19" type="type species">
<paragraph pageId="4" pageNumber="19">Type species.</paragraph>
<paragraph pageId="4" pageNumber="19">
<taxonomicName authority="Gillung &amp; Winterton, 2011" authorityName="Gillung &amp; Winterton" authorityYear="2011" class="Insecta" family="Acroceridae" genus="Quasi" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Quasi fisheri" order="Diptera" pageId="4" pageNumber="19" phylum="Arthropoda" rank="species" species="fisheri">Quasi fisheri</taxonomicName>
