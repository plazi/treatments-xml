, 
<collectingCountry box="[729,845,295,321]" name="Singapore" pageId="24" pageNumber="25">Singapore</collectingCountry>
, 
<collectingCountry box="[856,921,295,321]" name="Palau" pageId="24" pageNumber="25">Palau</collectingCountry>
, Marianas Is., 
<collectingCountry box="[1083,1189,295,321]" name="Australia" pageId="24" pageNumber="25">Australia</collectingCountry>
