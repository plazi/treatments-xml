<taxonomicName baseAuthorityName="Savigny" baseAuthorityYear="1816" box="[945,1097,295,321]" class="Ascidiacea" family="Ascidiidae" genus="Ascidia" kingdom="Animalia" order="Enterogona" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="nigra">
<emphasis box="[945,1097,295,321]" italics="true" pageId="15" pageNumber="16">Ascidia nigra</emphasis>
