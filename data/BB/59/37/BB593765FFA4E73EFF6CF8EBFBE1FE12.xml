<taxonomicName baseAuthorityName="Savigny" baseAuthorityYear="1816" class="Ascidiacea" family="Ascidiidae" genus="Ascidia" kingdom="Animalia" order="Enterogona" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="nigra">
