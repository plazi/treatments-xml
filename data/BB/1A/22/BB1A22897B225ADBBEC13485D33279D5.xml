<geoCoordinate degrees="22" direction="north" minutes="5.020" orientation="latitude" precision="1" value="22.083666">22°5.020'N</geoCoordinate>
, 
<geoCoordinate degrees="100" direction="east" minutes="22.087" orientation="longitude" precision="1" value="100.36812">100°22.087'E</geoCoordinate>
