<subSubSection box="[199,1110,260,285]" pageId="6" pageNumber="137" type="distribution">
<paragraph blockId="6.[151,1437,151,392]" box="[199,1110,260,285]" pageId="6" pageNumber="137">
<emphasis bold="true" box="[199,345,260,285]" pageId="6" pageNumber="137">Distribution</emphasis>
. 
<collectingCountry box="[359,417,260,285]" name="India" pageId="6" pageNumber="137">India</collectingCountry>
(*Andhra Pradesh, Kerala, *Odisha, Tamil Nadu), 
<collectingCountry box="[992,1106,260,285]" name="Sri Lanka" pageId="6" pageNumber="137">Sri Lanka</collectingCountry>
.
