<quantity box="[1286,1387,644,669]" metricMagnitude="-2" metricUnit="m" metricValue="1.06" pageId="19" pageNumber="210" unit="mm" value="10.6">10.6 mm</quantity>
and W: 
<quantity box="[191,281,679,704]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" pageId="19" pageNumber="210" unit="mm" value="7.5">7.5 mm</quantity>
