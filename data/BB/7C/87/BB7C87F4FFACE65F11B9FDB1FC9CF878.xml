<subSubSection pageId="19" pageNumber="210" type="materials_examined">
<paragraph blockId="19.[151,1437,644,1928]" pageId="19" pageNumber="210">
<emphasis bold="true" box="[151,326,644,669]" pageId="19" pageNumber="210">Type material.</emphasis>
