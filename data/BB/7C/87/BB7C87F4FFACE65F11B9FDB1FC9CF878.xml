<quantity metricMagnitude="-2" metricUnit="m" metricValue="1.1199999999999999" pageId="19" pageNumber="210" unit="mm" value="11.2">11.2 mm</quantity>
; W: 
<quantity box="[249,350,716,741]" metricMagnitude="-2" metricUnit="m" metricValue="1.46" pageId="19" pageNumber="210" unit="mm" value="14.6">14.6 mm</quantity>
) and TFMCFO-3 704 (H: 
<quantity box="[641,709,716,740]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="19" pageNumber="210" unit="mm" value="9.0">9 mm</quantity>
; W: 
<quantity box="[761,830,716,740]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="19" pageNumber="210" unit="mm" value="7.0">7 mm</quantity>
