and 
<taxonomicName class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="147" phylum="Tracheophyta" rank="species" species="yunnanensis">
<emphasis italics="true" pageId="6" pageNumber="147">P. yunnanensis</emphasis>
