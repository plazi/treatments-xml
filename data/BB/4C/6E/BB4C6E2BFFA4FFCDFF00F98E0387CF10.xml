<quantity box="[347,504,1774,1800]" metricMagnitude="3" metricUnit="m" metricValue="2.2344999999999997" metricValueMax="2.269" metricValueMin="2.2" pageId="6" pageNumber="147" unit="m" value="2234.5" valueMax="2269.0" valueMin="2200.0">2200–2269 m</quantity>
