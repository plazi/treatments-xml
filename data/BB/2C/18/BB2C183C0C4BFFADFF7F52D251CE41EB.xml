<quantity box="[402,502,1302,1323]" metricMagnitude="-3" metricUnit="m" metricValue="5.75" metricValueMax="6.5" metricValueMin="5.0" pageId="9" pageNumber="580" unit="mm" value="5.75" valueMax="6.5" valueMin="5.0">5–6.5 mm</quantity>
