<paragraph blockId="3.[189,521,1146,1172]" box="[189,521,1146,1172]" pageId="3" pageNumber="149">
<taxonomicName authority="Martynov, 1937" authorityName="Martynov" authorityYear="1937" box="[189,517,1146,1172]" class="Insecta" family="Xyelidae" genus="Liadoxyela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="149" phylum="Arthropoda" rank="genus">
