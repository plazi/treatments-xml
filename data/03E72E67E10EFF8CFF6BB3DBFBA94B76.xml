<quantity id="4CB63294E10EFF8DFD20B2FAFC9B4A48" box="[732,835,1014,1039]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="10" pageNumber="201" unit="mm" value="1.05">1.05 mm</quantity>
