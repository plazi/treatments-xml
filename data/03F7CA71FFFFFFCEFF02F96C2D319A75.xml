<taxonomicName id="4C5E00E4FFFCFFC9FED7F8202A749EC2" box="[322,527,1970,1992]" class="Insecta" family="Perlidae" genus="Neoperla" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="species" species="flavescens">
<emphasis id="B92AA775FFFCFFC9FED7F8202A749EC2" box="[322,527,1970,1992]" italics="true" pageId="2" pageNumber="59">
<taxonomicName id="4C5E00E4FFFCFFC9FED7F82029DB9EC2" box="[322,416,1970,1992]" class="Insecta" family="Perlidae" genus="Neoperla" higherTaxonomySource="CoL" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="genus">Neoperla</taxonomicName>
