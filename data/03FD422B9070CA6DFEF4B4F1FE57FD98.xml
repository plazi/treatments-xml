<taxonomicName id="4C5488BE9071CA60FE88B1A2FE4DFA69" authority="Shih &amp; Polaszek &amp; Dubey &amp; Chen &amp; Yang &amp; Ko, 2015" authorityName="Shih &amp; Polaszek &amp; Dubey &amp; Chen &amp; Yang &amp; Ko" authorityYear="2015" box="[259,447,1501,1524]" class="Insecta" family="Aphelinidae" genus="Eretmocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="382" phylum="Arthropoda" rank="species" species="lannae" status="sp. nov.">
