<taxonomicName id="4C524D57FF92FFD57D9EFE350418FE73" box="[486,625,428,452]" class="Actinopterygii" family="Peristediidae" genus="Satyrichthys" kingdom="Animalia" order="Scorpaeniformes" pageId="10" pageNumber="429" phylum="Chordata" rank="genus">
