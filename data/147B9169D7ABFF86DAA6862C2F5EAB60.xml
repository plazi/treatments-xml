<taxonomicName id="D5BE5A69B6EA7A0F87B46822193CF3E7" class="Insecta" family="Braconidae" genus="Adelurola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Adelurola kamtshatica" order="Hymenoptera" pageId="5" pageNumber="18" phylum="Arthropoda" rank="species" species="kamtshatica">Adelurola kamtshatica</taxonomicName>
