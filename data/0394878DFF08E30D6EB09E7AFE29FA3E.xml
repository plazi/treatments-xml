<subSubSection id="C3276510FF09E3126EE09F65FB32FDFF" lastPageId="15" lastPageNumber="16" pageId="13" pageNumber="14" type="description">
<paragraph id="8B82369BFF09E3106EE09F65FCBBF9EC" blockId="13.[151,1437,1597,2027]" pageId="13" pageNumber="14">
<emphasis id="B949EA89FF09E3106EE09F65FEA7F9C0" bold="true" box="[199,381,1597,1622]" pageId="13" pageNumber="14">Measurements.</emphasis>
BL = 
<quantity id="4CC59B7EFF09E3106FE19F66FDC4F9C3" box="[454,542,1598,1622]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="13" pageNumber="14" unit="mm" value="3.9">3.9 mm</quantity>
, BW = 
<quantity id="4CC59B7EFF09E3106C529F66FD17F9C3" box="[629,717,1598,1622]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="13" pageNumber="14" unit="mm" value="2.4">2.4 mm</quantity>
