<paragraph id="1861BE6DFFD5A930FF13CC44FD98349F" blockId="4.[151,1436,1853,1998]" pageId="4" pageNumber="105">
<emphasis id="2AAA627FFFD5A930FF13CC44FEAE3402" bold="true" box="[151,328,1853,1875]" pageId="4" pageNumber="105">FIGURES 7–20.</emphasis>
<taxonomicName id="DFDEC5EEFFD5A930FECBCC44FDCE3402" box="[335,552,1853,1875]" class="Insecta" family="Issidae" genus="Mongoliana" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="species" species="bistriata" status="sp. nov.">
<emphasis id="2AAA627FFFD5A930FECBCC44FDCE3402" box="[335,552,1853,1875]" italics="true" pageId="4" pageNumber="105">Mongoliana bistriata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="3199DF04FFD5A930FDB4CC44FD673402" box="[560,641,1853,1875]" pageId="4" pageNumber="105" rank="species">
