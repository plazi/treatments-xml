(
<figureCitation id="EBE168DE5507FFFE8EA0F435FBCE9878" box="[965,1056,556,577]" captionStart="Figures 32–43" captionStartId="17.[144,234,1374,1393]" captionTargetBox="[305,1261,195,1342]" captionTargetId="figure-177@17.[303,1263,195,1344]" captionTargetPageId="17" captionText="Figures 32–43. Adults: 32, male, Magulacra niveogrisea; 33, female, Magulacra niveogrisea; 34, male, Cossula arpi; 35, female, Cossula arpi; 36, male, Cossula buspina; 37, male, Cossula coerulescens; 38, female, Cossula coerulescens; 39, male, Cossula cossuloides; 40, female, Cossula cossuloides; 41, male, Cossula duplex; 42, female, Cossula duplex; 43, male, Cossula duplexata. Scale bar = 10.0 mm." figureDoi="http://doi.org/10.5281/zenodo.5446391" httpUri="https://zenodo.org/record/5446391/files/figure.png" pageId="47" pageNumber="269">FIGS 43</figureCitation>
, 
<figureCitation id="EBE168DE5507FFFE894BF435FB959878" box="[1070,1147,556,577]" captionStart="Figures 77–88" captionStartId="21.[144,234,1387,1406]" captionTargetBox="[303,1261,196,1355]" captionTargetId="figure-145@21.[303,1263,196,1358]" captionTargetPageId="21" captionText="Figures 77–88. Male genitalia. Cossula cossuloides: 77, genital capsule; 78, aedoeagus. Cossula duplex: 79, genital capsule; 80, aedoeagus. Cossula duplexata: 81, genital capsule; 82, aedoeagus. Cossula gaudeator: 83, genital capsule; 84, aedoeagus. Cossula longirostrum: 85, genital capsule; 86, aedoeagus. Cossula minutiloba: 87, genital capsule; 88, aedoeagus." figureDoi="http://doi.org/10.5281/zenodo.5446400" httpUri="https://zenodo.org/record/5446400/files/figure.png" pageId="47" pageNumber="269">81, 82</figureCitation>
