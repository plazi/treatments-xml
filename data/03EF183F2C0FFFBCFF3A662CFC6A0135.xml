, 
<collectingCountry id="F351E9B92C0EFFBCFE8B602FFEE20111" box="[294,372,295,321]" name="Jordan" pageId="23" pageNumber="24">Jordan</collectingCountry>
, 
<collectingCountry id="F351E9B92C0EFFBCFED3602FFE700111" box="[382,486,295,321]" name="Palestine" pageId="23" pageNumber="24">Palestine</collectingCountry>
