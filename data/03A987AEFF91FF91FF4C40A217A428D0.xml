<taxonomicName id="4C004D3BFF91FF91FE9A40A2140029F2" baseAuthorityName="de Saussure" baseAuthorityYear="1854" box="[321,561,1669,1695]" class="Insecta" family="Vespidae" genus="Protopolybia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="88" phylum="Arthropoda" rank="species" species="sedula">
