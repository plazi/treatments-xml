, or potentially on the related, native 
<taxonomicName id="A0AAC094E451FFC22701FC7DFB154283" box="[1074,1225,971,997]" class="Magnoliopsida" family="Rosaceae" genus="Sorbus" kingdom="Plantae" order="Rosales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="intermedia">
<emphasis id="55DE6705E451FFC22701FC7DFB154283" box="[1074,1225,971,997]" italics="true" pageId="11" pageNumber="12">S. intermedia</emphasis>
</taxonomicName>
, 
<taxonomicName id="A0AAC094E451FFC227EAFC7DFA8B4283" box="[1241,1367,971,997]" class="Magnoliopsida" family="Rosaceae" genus="Sorbus" kingdom="Plantae" order="Rosales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="rupicola">
