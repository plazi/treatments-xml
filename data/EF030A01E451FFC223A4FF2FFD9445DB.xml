<taxonomicName id="A0AAC094E451FFC222CCFC7DFD594283" baseAuthorityName="Pschorn-Walcher &amp; Altenhofer" baseAuthorityYear="2000" box="[511,645,971,997]" class="Magnoliopsida" family="Rosaceae" genus="Sorbus" kingdom="Plantae" order="Rosales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="aria">
<emphasis id="55DE6705E451FFC222CCFC7DFD594283" box="[511,645,971,997]" italics="true" pageId="11" pageNumber="12">Sorbus aria</emphasis>
