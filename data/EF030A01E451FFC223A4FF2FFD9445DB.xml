<materialsCitation id="D7C2B14AE451FFC222C5FF68FD374190" ID-GBIF-Occurrence="2847254406" box="[502,747,221,246]" country="United Kingdom" location="England" pageId="11" pageNumber="12" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
