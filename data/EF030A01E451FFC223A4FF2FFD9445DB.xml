<taxonomicName id="A0AAC094E451FFC226B4FC7DFEDB456F" authorityName="Hedl" authorityYear="1914" class="Magnoliopsida" family="Rosaceae" genus="Sorbus" kingdom="Plantae" order="Rosales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="norvegica">
<emphasis id="55DE6705E451FFC226B4FC7DFEDB456F" italics="true" pageId="11" pageNumber="12">S. norvegica</emphasis>
