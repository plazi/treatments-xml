<taxonomicName id="068786A044ED1D6CDC231E48FED5609D" authority="Tiegh." authorityName="Tiegh." class="Magnoliopsida" family="Ochnaceae" genus="Ouratea" kingdom="Plantae" order="Malpighiales" phylum="Tracheophyta" rank="species" species="plumieri">
<emphasis id="775585BC955E2DE69C8ABC13F93E3926" italics="true" pageNumber="696">
<emphasis id="AB1C54E47B1AC31EEB68679B5D7BA49F" bold="true" italics="true" pageNumber="696">Ouratea plumieri</emphasis>
</emphasis>
Tiegh.
</taxonomicName>
(
<taxonomicName id="07E4D979B00AE7B45AB0A276FA90C080" class="Magnoliopsida" family="Ochnaceae" kingdom="Plantae" order="Malpighiales" phylum="Tracheophyta" rank="family">Ochnaceae</taxonomicName>
