<taxonomicName id="4C7923F7FFB8FFBEFCAEF9060087E290" box="[882,999,1677,1702]" class="Insecta" family="Leuctridae" genus="Perlomyia" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="147" phylum="Arthropoda" rank="genus">
