<collectingCountry id="F30D08FFFF9F424AFE06FEDB07CDEA6D" box="[508,728,258,283]" name="Papua New Guinea" pageId="108" pageNumber="109">Papua New Guinea</collectingCountry>
. Alotau, 
<quantity id="4CE2E58AFF9F424AFCC7FEDB066FEA6C" box="[829,890,259,283]" metricMagnitude="1" metricUnit="m" metricValue="1.3" pageId="108" pageNumber="109" unit="m" value="13.0">13 m</quantity>
