<accessionNumber id="91CCA4C0FF822416763FFE55FCA1FE67" box="[803,886,436,460]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/R63433" pageId="11" pageNumber="72">
<collectionCode id="E88EA1E6FF822416763FFE55FCE3FE67" box="[803,820,436,460]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="11" pageNumber="72">R</collectionCode>
63433
