<taxonomicName id="4C6308DD9B4E607BFDCAFEAF556CF05B" authorityName="Grichanov" authorityYear="2022" box="[540,880,264,291]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="68" phylum="Arthropoda" rank="species" species="mufindiensis">
