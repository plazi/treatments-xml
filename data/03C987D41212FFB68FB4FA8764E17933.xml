<taxonomicName id="4C604D411215FFB68DD4FF46642078D3" box="[725,855,207,231]" class="Magnoliopsida" family="Boraginaceae" genus="Onosma" kingdom="Plantae" order="Boraginales" pageId="4" pageNumber="283" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B914EAD01215FFB68DD4FF46645878D2" box="[725,815,207,230]" italics="true" pageId="4" pageNumber="283">Onosma</emphasis>
