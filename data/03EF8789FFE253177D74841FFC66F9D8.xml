<emphasis id="B932EA8DFFE253177D74841FFD2DFCD7" bold="true" box="[264,659,788,814]" pageId="1" pageNumber="2">
Genus 
<taxonomicName id="4C464D1CFFE253177D23841FFDA3FCD7" authority="Young, 2004" authorityName="Young" authorityYear="2004" box="[351,541,788,814]" class="Maxillopoda" family="Verrucidae" genus="Globuloverruca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sessilia" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B932EA8DFFE253177D23841FFDA3FCD7" bold="true" box="[351,541,788,814]" italics="true" pageId="1" pageNumber="2">Globuloverruca</emphasis>
</taxonomicName>
