<typeStatus id="54BD884CFF945C4BFD85D711FDBCFC10" box="[571,619,875,899]" pageId="2" pageNumber="120">type</typeStatus>
specimen of 
<taxonomicName id="4C064D6DFF945C4BFCBAD716FB0BFC10" authority="Hancock, 1915" authorityName="Hancock" authorityYear="1915" box="[772,1244,874,899]" class="Insecta" family="Tetrigidae" genus="Euparatettix" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="120" phylum="Arthropoda" rank="species" species="sikkimensis">
