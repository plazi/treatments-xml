<taxonomicName id="BA13FCBAF2950D37085979DD375B63BC" authorityName="Nuttall" authorityYear="1818" class="Magnoliopsida" family="Juglandaceae" genus="Carya" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carya" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">
