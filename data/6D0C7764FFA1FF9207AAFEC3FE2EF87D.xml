<taxonomicName id="22A5BDF1FFA1FF9207FBFD57FEC8FD09" authorityName="Simon" authorityYear="1903" box="[198,357,691,717]" class="Arachnida" family="Theraphosidae" genus="Hapalotremus" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
