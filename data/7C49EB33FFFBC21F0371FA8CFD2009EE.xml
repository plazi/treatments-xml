<emphasis id="C6948637FFFBC2180371FAF7FE450E0E" box="[264,388,1383,1406]" italics="true" pageId="3" pageNumber="24">Villa albida</emphasis>
</taxonomicName>
<taxonomicName id="33E021A6FFFBC21803F5FAF6FD2D0E0D" authority="Engel, 1937: 572" authorityName="Engel" authorityPageNumber="572" authorityYear="1937" box="[396,748,1382,1406]" pageId="3" pageNumber="24" rank="variety" variety="aegyptiaca">
