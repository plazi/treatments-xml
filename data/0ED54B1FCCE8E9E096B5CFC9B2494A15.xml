<taxonomicName id="7B3B25D6E68F7D4FC2D1D0FC865C4938" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26360" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus concolor Forel" order="Hymenoptera" pageNumber="214" phylum="Arthropoda" rank="species" species="concolor" status="nov. spec.">CAMPONOTUS CONCOLOR</taxonomicName>
, 
