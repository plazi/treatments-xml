<location id="8E9B600262008A5ED822E1EC7E71A342" LSID="urn:lsid:plazi:treatment:03ED87CF62008A5EDA81E1CA7F32A2A4:8E9B600262008A5ED822E1EC7E71A342" box="[512,586,1015,1037]" country="Bolivia" municipality="Inquisivi" name="Plazuela" pageId="44" pageNumber="1189">Plazuela</location>
