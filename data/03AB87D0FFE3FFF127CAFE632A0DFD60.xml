, 
<emphasis id="B976EAD4FFE0FFF02418FD032A08FD42" box="[837,866,728,751]" italics="true" pageId="2" pageNumber="259">u”</emphasis>
, 
<emphasis id="B976EAD4FFE0FFF0242FFD032AF9FD42" box="[882,915,728,751]" italics="true" pageId="2" pageNumber="259">vs’</emphasis>
). Setae 
<emphasis id="B976EAD4FFE0FFF024B2FD032D66FD42" box="[1007,1036,728,751]" italics="true" pageId="2" pageNumber="259">tc’</emphasis>
, 
<emphasis id="B976EAD4FFE0FFF02341FD032D2AFD42" box="[1052,1088,728,751]" italics="true" pageId="2" pageNumber="259">tc”</emphasis>
, 
<emphasis id="B976EAD4FFE0FFF02312FD032D0CFD42" box="[1103,1126,728,751]" italics="true" pageId="2" pageNumber="259">a’</emphasis>
