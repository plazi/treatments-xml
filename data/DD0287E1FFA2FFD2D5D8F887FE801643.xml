<specimenCount id="43ADFD7EFFA1FFD3D1FAF865FADA12C6" box="[1214,1285,2003,2027]" pageId="14" pageNumber="153" type="generic">
<quantity id="92539B12FFA1FFD3D1FAF865FADE12C6" box="[1214,1281,2003,2027]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="14" pageNumber="153" unit="m" value="600.0">
<elevation id="DE86D1C4FFA1FFD3D1FAF865FADE12C6" box="[1214,1281,2003,2027]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="14" pageNumber="153" unit="m" value="600.0">600 m</elevation>
</quantity>
.
</specimenCount>
</materialsCitation>
</paragraph>
