<taxonomicName id="4C314D48E266FF95AD58AE8FCEDDFDE5" box="[551,678,475,499]" class="Malacostraca" family="Hyalidae" genus="Insula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="nigricula" status="sp. nov.">
<emphasis id="B945EAD9E266FF95AD58AE8FCEDDFDE5" box="[551,678,475,499]" italics="true" pageId="12" pageNumber="13">I. nigricula</emphasis>
</taxonomicName>
<emphasis id="B945EAD9E266FF95ADD2AE8ECF7CFDE5" bold="true" box="[685,775,474,499]" pageId="12" pageNumber="13">
<taxonomicNameLabel id="A27657A2E266FF95ADD2AE8ECF7CFDE5" box="[685,775,474,499]" pageId="12" pageNumber="13" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<figureCitation id="130A2A4EE266FF95AC6AAE8ECFD4FDE5" box="[789,943,474,499]" captionStart="FIGURES 180 – 183" captionStartId="65.[151,269,1967,1991]" captionTargetBox="[198,1391,197,1928]" captionTargetId="figure@65.[180,1410,194,1954]" captionTargetPageId="65" captionText="FIGURES 180 – 183. Insula nigricula sp. nov., male holotype and female paratype. 180, palp, ventral view; 181, palp, retrolateral view; 182, epigyne; 183, vulva. Scale bars: 0.1 mm." httpUri="https://zenodo.org/record/284785/files/figure.png" pageId="12" pageNumber="13">
