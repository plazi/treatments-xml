<location id="8EBD60791E14FFC5FE81FBEFA18B856B" LSID="urn:lsid:plazi:treatment:03CB87B41E15FFC5FF70FD29A2FE8753:8EBD60791E14FFC5FE81FBEFA18B856B" box="[323,397,1126,1147]" county="Das" latitude="36.15" longLatPrecision="713" longitude="27.56" municipality="Taxons" name="Taxons" pageId="59" pageNumber="612" stateProvince="Peloponnisos">Taxons</location>
