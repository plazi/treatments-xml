<quantity id="A33CBFAAF54A3F2B4FD0E7EAF9C5C9CE" box="[1456,1569,2178,2214]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="70" pageNumber="81" unit="mm" value="2.1">2.1 mm</quantity>
