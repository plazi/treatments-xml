201.90; South Passage Shark Bay, 
<quantity id="EE369B7BFF83FF1381FC46CFFCD4A4B9" box="[705,803,1712,1736]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="1.6" metricValueMin="1.2" pageId="24" pageNumber="754" unit="m" value="14.0" valueMax="16.0" valueMin="12.0">12â€“16 m</quantity>
