<taxonomicName id="478E4D7DFF8EFFE6CA029F7AFBEFFBF6" authorityName="Lobl" authorityYear="1977" box="[831,1112,1052,1075]" class="Insecta" family="Staphylinidae" genus="Scaphobaeocera" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="116" phylum="Arthropoda" rank="species" species="curvipes">
