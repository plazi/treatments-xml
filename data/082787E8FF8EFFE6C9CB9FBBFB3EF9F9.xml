<taxonomicName id="478E4D7DFF8EFFE6CDAE9F7AFAA4FBF6" authorityName="Lobl" authorityYear="1971" box="[1171,1299,1052,1075]" class="Insecta" family="Staphylinidae" genus="Scaphobaeocera" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="116" phylum="Arthropoda" rank="species" species="mussardi">
