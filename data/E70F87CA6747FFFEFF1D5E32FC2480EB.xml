<taxonomicName id="A8A64D5F6744FFFFFBBF5F03FA638059" authority="Goodnight" authorityName="Goodnight" box="[1077,1409,1858,1880]" class="Arachnida" family="Cranaidae" genus="Aguaytiella" kingdom="Animalia" order="Opiliones" pageId="6" pageNumber="573" phylum="Arthropoda" rank="species" species="maculata">
