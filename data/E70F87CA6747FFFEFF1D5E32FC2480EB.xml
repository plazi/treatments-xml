<taxonomicName id="A8A64D5F6744FFFFFBBF5F03FEA78076" authority="Goodnight &amp; Goodnight, 1943" authorityName="Goodnight &amp; Goodnight" authorityYear="1943" class="Arachnida" family="Cranaidae" genus="Aguaytiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="6" pageNumber="573" phylum="Arthropoda" rank="species" species="maculata">
