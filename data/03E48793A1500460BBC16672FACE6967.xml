<collectionCode id="ED5CAE40A1500463BA15678EFE1568F9" box="[323,387,1920,1946]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="17" pageNumber="18">ZMB</collectionCode>
<number id="3D8524A2A1500463BADC678EFE5168F9" box="[394,455,1920,1946]" pageId="17" pageNumber="18" value="5421.0">5421</number>
