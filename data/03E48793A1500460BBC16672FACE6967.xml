<taxonomicName id="4C4D4D06A1500463B91E67A2FD5468A6" box="[584,706,1964,1989]" class="Mammalia" family="Soricidae" genus="Crocidura" kingdom="Animalia" order="Soricomorpha" pageId="17" pageNumber="18" phylum="Chordata" rank="species" species="poensis">
