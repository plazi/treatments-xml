</collectingCountry>
, Java, Tjibodas Gardens, 
<specimenCount id="BBCCFD7AFFE02B5A597AFAEB73D66A73" box="[828,925,1302,1327]" pageId="10" pageNumber="191" type="female">
<number id="1B0224D4FFE02B5A597AFAEB73016A73" box="[828,842,1303,1327]" pageId="10" pageNumber="191" value="1.0">1</number>
female
</specimenCount>
from grass, 
<date id="D9741033FFE02B5A5E6FFAEB74D06A73" box="[1065,1179,1303,1327]" pageId="10" pageNumber="191" value="1973-10-22">
<number id="1B0224D4FFE02B5A5E6FFAEB74036A73" box="[1065,1096,1303,1327]" pageId="10" pageNumber="191" value="22.0">22</number>
.x. 
<number id="1B0224D4FFE02B5A5E19FAEB74D06A73" box="[1119,1179,1303,1327]" pageId="10" pageNumber="191" value="1973.0">1973</number>
