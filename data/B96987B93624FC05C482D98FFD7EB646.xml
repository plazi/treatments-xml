<quantity id="F6389B4A3627FC05C582D908FE55B646" box="[434,506,1079,1099]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="30" pageNumber="395" unit="mm" value="0.2">0.2 mm</quantity>
