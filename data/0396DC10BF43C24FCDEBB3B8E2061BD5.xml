<quantity id="4CC7C0E3BF43C24FC981B6F2E2551C8F" box="[1241,1325,199,223]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="76" pageNumber="229" unit="mm" value="4.2">4.2 mm</quantity>
