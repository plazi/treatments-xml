<taxonomicName id="05773A130079548992E1561C6C6F262B" class="Insecta" family="Chironomidae" genus="Pseudorthocladius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudorthocladius (Pseudorthocladius) ovatus" order="Diptera" pageId="10" pageNumber="61" phylum="Arthropoda" rank="species" species="ovatus" subGenus="Pseudorthocladius">Pseudorthocladius (Pseudorthocladius) ovatus</taxonomicName>
sp. n. and 
<taxonomicName id="7DD25B9F643654078AFCE4E90BED6518" class="Insecta" family="Chironomidae" genus="Pseudorthocladius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudorthocladius (Pseudorthocladius) matusecundus" order="Diptera" pageId="10" pageNumber="61" phylum="Arthropoda" rank="species" species="matusecundus" subGenus="Pseudorthocladius">Pseudorthocladius (Pseudorthocladius) matusecundus</taxonomicName>
<bibRefCitation id="AC444AD5792681611F00FE9C0069745F" author="Sasa, M" journalOrPublisher="Lake Biwa Research Institute, Otsu" pageId="15" pageNumber="66" pagination="1 - 119" title="Studies on chironomid midges of Lake Biwa (Diptera, Chironomidae)." year="1987">Sasa and Kawai (1987)</bibRefCitation>
.
</paragraph>
</caption>
<paragraph id="99D43B49EAA89A2E6197402854936453" pageId="10" pageNumber="61">
