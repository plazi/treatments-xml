<number id="186624D98E57FF961D74BAC8FD8BA2E1" box="[606,620,1416,1442]" pageId="7" pageNumber="44" value="1.0">1</number>
<emphasis id="9CDAEAEC8E57FF961D5EBAC9FD69A2E1" box="[628,654,1417,1442]" italics="true" pageId="7" pageNumber="44">av</emphasis>
