<location id="2874603EFFCFFFC1FD14FEA1FC6CFE99" LSID="urn:lsid:plazi:treatment:A50287F3FFCFFFC1FF0EFF58FC6EFDB1:2874603EFFCFFFC1FD14FEA1FC6CFE99" box="[653,905,294,319]" country="India" name="eastern Mediterranean" pageId="34" pageNumber="35" stateProvince="East">eastern Mediterranean</location>
