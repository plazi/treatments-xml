<taxonomicName id="33CE36FA2D0CFF97FECAF86CEEB9761B" authorityName="Fibiger" authorityYear="2010" box="[373,481,272,297]" class="Insecta" family="Erebidae" genus="Asyprocessa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="spinus">
<emphasis id="C6BA916B2D0CFF97FECAF86CEEB9761B" box="[373,481,272,297]" italics="true" pageId="63" pageNumber="64">A. spinus</emphasis>
