<taxonomicName id="4C319EBF194BFFA76E31941A1C62FBC1" baseAuthorityName="Linnaeus" baseAuthorityYear="1766" class="Aves" family="Corvidae" genus="Perisoreus" kingdom="Animalia" order="Passeriformes" pageId="116" pageNumber="116" phylum="Chordata" rank="species" species="canadensis">
<emphasis id="B945392E194BFFA76E31941A1C62FBC1" italics="true" pageId="116" pageNumber="116">Perisoreus canadensis</emphasis>
