(
<taxonomicName id="7537FA88C4F42B29C116DDC3466B39D5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:135418" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus quadrimaculatus var. gibber Forel" order="Hymenoptera" pageNumber="215" phylum="Arthropoda" rank="variety" species="quadrimaculatus" variety="gibber">Camponotus quadrimaculatus, var. gibber</taxonomicName>
