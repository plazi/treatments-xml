<taxonomicName id="4C1B38C39FEFA4E5E6A7FEE4FE73AE55" authority="(Skou 1986)" baseAuthorityName="Skou" baseAuthorityYear="1986" box="[140,437,307,334]" class="Insecta" family="Geometridae" genus="Rheumaptera" kingdom="Animalia" order="Lepidoptera" pageId="195" pageNumber="196" phylum="Arthropoda" rank="genus">
