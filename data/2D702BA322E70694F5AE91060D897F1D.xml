<taxonomicName id="8A31BD67605AB0E6002D1D30CBBAB4E5" authority="Panzer, 1803" authorityName="Panzer" authorityYear="1803" class="Insecta" family="Pamphiliidae" genus="Cephalcia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cephalcia arvensis" order="Hymenoptera" pageId="0" pageNumber="1168" phylum="Arthropoda" rank="species" species="arvensis">Cephalcia arvensis</taxonomicName>
