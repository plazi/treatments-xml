<taxonomicName id="ADCD4D04FFA2FFCCEED7FAE0FDB0FAEA" authority="Smith, 1879" authorityName="Smith" authorityYear="1879" box="[242,589,1368,1395]" class="Insecta" family="Mutillidae" genus="Mutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="581" phylum="Arthropoda" rank="species" species="simulatrix">
