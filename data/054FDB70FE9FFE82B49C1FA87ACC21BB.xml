<emphasis id="BF92B674FE9FFE82B49C1FA87A532284" bold="true" box="[498,865,1461,1491]" pageId="344" pageNumber="345">
<taxonomicName id="4AE611E5FE9FFE82B49C1FA87B5D2284" baseAuthorityName="Mackay" baseAuthorityYear="2000" box="[498,623,1461,1491]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="344" pageNumber="345" phylum="Arthropoda" rank="species" species="rugosus">
