<taxonomicName id="4C394D52FFA2FF8BE315485634996C41" box="[270,432,803,827]" class="Insecta" family="Carabidae" genus="Blennidus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="rectangulus">
