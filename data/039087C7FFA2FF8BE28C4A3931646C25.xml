<taxonomicName id="4C394D52FFA2FF8BE043485637FA6C40" box="[600,723,803,826]" class="Insecta" family="Carabidae" genus="Blennidus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="curtatus">
