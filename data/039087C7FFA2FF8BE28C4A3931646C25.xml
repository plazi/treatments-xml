<taxonomicName id="4C394D52FFA2FF8BE3A5485637636C41" box="[446,586,803,827]" class="Insecta" family="Carabidae" genus="Blennidus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="nigritulus">
