<quantity id="090E9B2DFFAD7440FC84F816FC00F82F" box="[788,946,1977,2003]" metricMagnitude="3" metricUnit="m" metricValue="2.55" metricValueMax="2.7" metricValueMin="2.4" pageId="19" pageNumber="20" unit="m" value="2550.0" valueMax="2700.0" valueMin="2400.0">2400–2700 m</quantity>
