<taxonomicName id="9C7D0F1EDEAE7F629F370054B6D0824F" authorityName="C.Agardh" authorityYear="1820" class="Phaeophyceae" family="Cystoseiraceae" genus="Cystoseira" higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="Cystoseira" order="Fucales" pageId="0" pageNumber="1" phylum="Ochrophyta" rank="genus">
