and 
<taxonomicName id="D0DAC8C0E2DE9C7BD78A3FAA41ACF2A5" authorityName="C.Agardh" authorityYear="1820" class="Phaeophyceae" family="Cystoseiraceae" genus="Cystoseira" higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="Cystoseira" order="Fucales" pageId="0" pageNumber="1" phylum="Ochrophyta" rank="genus">
