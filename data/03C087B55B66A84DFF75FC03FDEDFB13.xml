<taxonomicName id="4C694D205B66A84AFF75FC03FEDBFC7B" authorityName="Stresemann" authorityYear="1914" box="[151,334,942,968]" class="Aves" family="Melanocharitidae" genus="Toxorhamphus" kingdom="Animalia" order="Passeriformes" pageId="3" pageNumber="504" phylum="Chordata" rank="genus">
<emphasis id="B91DEAB15B66A84AFF75FC03FEDBFC7B" bold="true" box="[151,334,942,968]" italics="true" pageId="3" pageNumber="504">Toxorhamphus</emphasis>
</taxonomicName>
and 
<emphasis id="B91DEAB15B66A84AFE6EFC03FD98FC7B" bold="true" box="[396,525,942,968]" italics="true" pageId="3" pageNumber="504">Oedistoma</emphasis>
