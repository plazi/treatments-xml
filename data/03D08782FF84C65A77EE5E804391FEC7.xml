<collectingCountry id="F36E7604FF86C65B75C05831432EFF40" box="[712,820,1322,1344]" name="Australia" pageId="42" pageNumber="42">Australia</collectingCountry>
and 
<taxonomicName id="4C794D17FF86C65B747A583143D3FF3F" authorityName="Distant (Oriental, Palearctic" authorityYear="1912" box="[882,969,1322,1343]" class="Insecta" family="Miridae" genus="Sejanus" kingdom="Animalia" order="Hemiptera" pageId="42" pageNumber="42" phylum="Arthropoda" rank="genus">
<emphasis id="B90DEA86FF86C65B747A583143D3FF3F" box="[882,969,1322,1343]" italics="true" pageId="42" pageNumber="42">Sejanus</emphasis>
