<caption id="72E766752F22FFDD2EF6E54A95A8FDA7" ID-DOI="http://doi.org/10.5281/zenodo.5429450" ID-Zenodo-Dep="5429450" box="[143,1170,612,632]" httpUri="https://zenodo.org/record/5429450/files/figure.png" pageId="10" pageNumber="91" startId="10.[143,221,612,631]" targetBox="[302,1262,200,580]" targetPageId="10">
