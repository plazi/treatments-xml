<pageBreakToken id="7CDFEABF591717A9EDB1FFF5352A03A2" pageId="null" pageNumber="501" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="80F4244F31FFACCFDDD7C1F2DDAD1973" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="501" phylum="Tracheophyta" rank="species" species="vulgaris">
Senecio 
<normalizedToken id="52DA500BE3AF6EB224D820391CE4B0EE" originalValue="vulgáris" pageId="null" pageNumber="501">vulgaris</normalizedToken>
<authorityName id="65D11C3DFE427AA9DE4D53E85D363CD6" pageId="null" pageNumber="501">L.</authorityName>
</taxonomicName>
