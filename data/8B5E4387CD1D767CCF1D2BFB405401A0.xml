<taxonomicName id="39A9EDA380EBBB7475B8AB566D150BAD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:251737" class="Insecta" family="Formicidae" genus="Gesomyrmex" genus.bestMatchDistance="239" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Gesomyrmex flavescens" order="Hymenoptera" pageNumber="10" phylum="Arthropoda" rank="species" species="flavescens">G. flavescens</taxonomicName>
