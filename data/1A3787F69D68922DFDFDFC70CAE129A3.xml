wide. Pronotum 
<quantity id="55669B059D69922EFC10FEE5CBBB2914" box="[899,1005,347,374]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="117" pageNumber="118" unit="mm" value="1.5">1.50 mm</quantity>
long, maximum width 
<quantity id="55669B059D69922EFA96FEE5CD252914" box="[1285,1395,347,374]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="117" pageNumber="118" unit="mm" value="2.6">2.60 mm</quantity>
. Elytral length 
<quantity id="55669B059D69922EFEE3FE3EC9B729F8" box="[368,481,384,410]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="117" pageNumber="118" unit="mm" value="3.9">3.90 mm</quantity>
, maximum combined width 
<quantity id="55669B059D69922EFCD5FEC1CBE129F8" box="[838,951,383,410]" metricMagnitude="-3" metricUnit="m" metricValue="3.15" pageId="117" pageNumber="118" unit="mm" value="3.15">3.15 mm</quantity>
. Female 
<typeStatus id="4D2588429D69922EFBB8FE3ECCC529F8" box="[1067,1171,384,410]" pageId="117" pageNumber="118">paratype</typeStatus>
, total body length 
<quantity id="55669B059D69922EFF2EFE1DC97C29DC" box="[189,298,419,446]" metricMagnitude="-3" metricUnit="m" metricValue="6.7" pageId="117" pageNumber="118" unit="mm" value="6.7">6.70 mm</quantity>
