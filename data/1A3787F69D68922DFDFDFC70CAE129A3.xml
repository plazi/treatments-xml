<quantity id="55669B059D69922EFEEDFE1DC9BE29DC" box="[382,488,419,446]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="117" pageNumber="118" unit="mm" value="1.2">1.20 mm</quantity>
