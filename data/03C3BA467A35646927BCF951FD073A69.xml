<paragraph id="8BD50B507A35646427BCF8CCFC9D390D" blockId="4.[151,1437,1828,1999]" box="[151,874,1828,1855]" pageId="4" pageNumber="405">
<emphasis id="B91ED7427A35646427BCF8CCFED6390D" bold="true" box="[151,289,1828,1855]" pageId="4" pageNumber="405">Description</emphasis>
. MALE. Body length 
<quantity id="4C92A6B57A3564642535F8CDFD81390D" box="[542,630,1829,1855]" metricMagnitude="-3" metricUnit="m" metricValue="6.9" pageId="4" pageNumber="405" unit="mm" value="6.9">6.9 mm</quantity>
