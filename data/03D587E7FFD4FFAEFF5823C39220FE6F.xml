<taxonomicName id="4C7C4D72FFDAFFAEFCB621D09237FF33" authority="Curletti &amp; Sakalian, 2009" authorityName="Curletti &amp; Sakalian" authorityYear="2009" box="[866,1025,202,228]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="ljubomirovi" status="sp. n.">
<emphasis id="B908EAE3FFDAFFAEFCB621D09237FF33" box="[866,1025,202,228]" italics="true" pageId="8" pageNumber="9">A. ljubomirovi</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A23B5798FFDAFFAEFBDB21D09278FF33" box="[1039,1102,202,228]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
; anterior margin of prosternal lobe truncate in 
<taxonomicName id="4C7C4D72FFDAFFAEFD4121F79507FED0" authority="Curletti &amp; Sakalian, 2009" authorityName="Curletti &amp; Sakalian" authorityYear="2009" box="[661,817,237,263]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="gueorguievi" status="sp. n.">
<emphasis id="B908EAE3FFDAFFAEFD4121F79507FED0" box="[661,817,237,263]" italics="true" pageId="8" pageNumber="9">A. gueorguievi</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A23B5798FFDAFFAEFCEB21F4954AFEDF" box="[831,892,238,264]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
