(
<typeStatus id="54C78853FFD5FFA1FDD12330945CFD93" box="[517,618,554,580]" pageId="7" pageNumber="8">holotype</typeStatus>
<quantity id="4C849B14FFD5FFA1FDA4233094EDFD93" box="[624,731,554,581]" metricMagnitude="-3" metricUnit="m" metricValue="3.35" pageId="7" pageNumber="8" unit="mm" value="3.35">3.35 mm</quantity>
