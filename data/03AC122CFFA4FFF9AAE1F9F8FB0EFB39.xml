<quantity id="4CFD0EDFFFA4FFF9AE43FDCDFB3AFB39" box="[1077,1172,1186,1212]" metricMagnitude="-1" metricUnit="m" metricValue="2.1" pageId="3" pageNumber="45" unit="mm" value="210.0">210 µm</quantity>
