<taxonomicName id="1381E691FFDAFFF8FC2650B21A81FB39" authority="Weber, 1801" authorityName="Weber" authorityYear="1801" box="[924,1179,1193,1218]" class="Insecta" family="Scarabaeidae" genus="Ateuchus" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="478" phylum="Arthropoda" rank="genus">
