<quantity id="58079B75FFE6735E22EA0426FDF2C445" box="[469,537,1328,1355]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="5" pageNumber="556" unit="mm" value="6.0">6 mm</quantity>
