<taxonomicName id="7EE386B39C642A2EEE785989FD1A5AEC" authority="(Navas, 1915)" baseAuthorityName="Navas" baseAuthorityYear="1915" box="[302,668,1974,1996]" class="Insecta" family="Myrmeleontidae" genus="Neuroleon" kingdom="Animalia" order="Neuroptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="assimilis">
