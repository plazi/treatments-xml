<collectingCountry id="C1F4BDA09C652A2FEDE55F50FC835CA8" box="[691,773,367,392]" name="Greece" pageId="29" pageNumber="30">Greece</collectingCountry>
, 
<collectingCountry id="C1F4BDA09C652A2FEC465F50FCF55CA8" box="[784,883,367,392]" name="Turkey" pageId="29" pageNumber="30">Anatolia</collectingCountry>
