<subSubSection id="C354A3DEFFE00A0AFF14329F741DFAE0" pageId="8" pageNumber="319" type="etymology">
<paragraph id="8BF1F055FFE00A0AFF14329F741DFAE0" blockId="8.[151,1437,261,1331]" pageId="8" pageNumber="319">
<emphasis id="B93A2C47FFE00A0AFF14329F7177FAC3" bold="true" box="[199,334,1270,1296]" pageId="8" pageNumber="319">Etymology.</emphasis>
