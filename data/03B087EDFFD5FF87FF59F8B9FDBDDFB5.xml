.Mc2015.624 254(273) 
<quantity id="4CE19B1EFFD4FF84FCF3F955FC29D9FE" box="[829,908,1665,1692]" metricMagnitude="-1" metricUnit="m" metricValue="3.01" pageId="29" pageNumber="490" unit="mm" value="301.0">301µm</quantity>
