<collectingCountry id="F31695CE9639FFDBFD38F806CB18A085" box="[667,777,1992,2018]" name="Australia" pageId="15" pageNumber="16">Australia</collectingCountry>
, in 
<quantity id="4CF978BB9639FFDBFC90F806CB8AA085" box="[819,923,1992,2018]" metricMagnitude="1" metricUnit="m" metricValue="6.35" metricValueMax="6.7" metricValueMin="6.0" pageId="15" pageNumber="16" unit="m" value="63.5" valueMax="67.0" valueMin="60.0">60–67 m</quantity>
