<taxonomicName id="4C714D1DFFE86017A988FB54FE396E8A" authorityName="Giesbert" authorityYear="1993" box="[192,465,1225,1249]" class="Insecta" family="Cerambycidae" genus="Plesioclytus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="relictus">
<emphasis id="B905EA8CFFE86017A988FB54FE396E8A" bold="true" box="[192,465,1225,1249]" italics="true" pageId="3" pageNumber="2">Plesioclytus relictus</emphasis>
