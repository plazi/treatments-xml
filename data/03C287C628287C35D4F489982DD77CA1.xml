<quantity id="4C939B3528287C35D1718E41297F7C3E" box="[1298,1389,1104,1128]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="35" pageNumber="436" unit="m" value="1000.0">1000 m</quantity>
