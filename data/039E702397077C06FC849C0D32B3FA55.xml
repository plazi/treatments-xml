<taxonomicName id="4C37BAB697077C06FADF9B9C353AFB7A" class="Bivalvia" family="Unionidae" genus="Quadrula" kingdom="Animalia" order="Unionoida" pageId="14" pageNumber="620" phylum="Mollusca" rank="species" species="symmetrica">
