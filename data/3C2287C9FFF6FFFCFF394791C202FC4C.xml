<subSubSection id="FC916554FFF6FFFCFF3946E4C202FC4C" pageId="2" pageNumber="575" type="etymology">
<paragraph id="B43436DFFFF6FFFCFF3946E4C444FC60" blockId="2.[151,1436,904,1002]" pageId="2" pageNumber="575">
<emphasis id="86FFEACDFFF6FFFCFF3946E4C040FC04" bold="true" box="[151,286,904,930]" pageId="2" pageNumber="575">Etymology.</emphasis>
