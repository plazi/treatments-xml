<taxonomicName id="26EF679F91310755FEA0FE68FE3C01EF" box="[264,507,463,488]" class="Insecta" family="Aphididae" genus="Aspidaphis" kingdom="Animalia" order="Aphidomorpha" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="adjuvans">
