<taxonomicName id="26EF679F91310755FE45FE03FD5201BA" authority="Zhang, Chen, Zhong &amp; Li, 1999" authorityName="Zhang, Chen, Zhong &amp; Li" authorityYear="1999" box="[493,661,420,445]" class="Insecta" family="Aphididae" genus="Polygonaphis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="avicularisucta">
