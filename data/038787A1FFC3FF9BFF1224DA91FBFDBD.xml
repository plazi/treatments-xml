<quantity id="4CD69B52FFC3FF9AFDA5273797E1F887" box="[544,661,1799,1825]" metricMagnitude="-2" metricUnit="m" metricValue="1.3039999999999998" pageId="12" pageNumber="413" unit="mm" value="13.04">13.04 mm</quantity>
; TL = 
<quantity id="4CD69B52FFC3FF9AFD5A27369620F887" box="[735,852,1798,1825]" metricMagnitude="-2" metricUnit="m" metricValue="2.864" pageId="12" pageNumber="413" unit="mm" value="28.64">28.64 mm</quantity>
