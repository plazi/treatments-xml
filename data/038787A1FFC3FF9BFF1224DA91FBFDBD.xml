<quantity id="4CD69B52FFC3FF9AFA8026D29018F95B" box="[1285,1388,1762,1789]" metricMagnitude="-3" metricUnit="m" metricValue="3.45" pageId="12" pageNumber="413" unit="mm" value="3.45">3.45 mm</quantity>
