<taxonomicName id="13914D5BFFDBBB4D2FF3281C0B42FBF2" authority="Boeck, 1861" authorityName="Boeck" authorityYear="1861" box="[193,584,1123,1150]" class="Malacostraca" family="Leucothoidae" genus="Leucothoe" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="384" phylum="Arthropoda" rank="species" species="lilljeborgi">
