(3); 
<taxonomicName id="4C224D59FFD4021CFF37B3AB6C31518E" authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[151,307,1949,1973]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="276" phylum="Arthropoda" rank="species" species="ecuadoranus" status="sp. n.">
<emphasis id="B956EAC8FFD4021CFF37B3AB6C31518E" box="[151,307,1949,1973]" italics="true" pageId="2" pageNumber="276">V. ecuadoranus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26557B3FFD4021CFE99B3A86C72518F" box="[313,368,1949,1972]" pageId="2" pageNumber="276" rank="species">sp. n.</taxonomicNameLabel>
(4); 
<taxonomicName id="4C224D59FFD4021CFE01B3AB6F2A518E" authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[417,552,1949,1973]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="276" phylum="Arthropoda" rank="species" species="bejumanus" status="sp. n.">
<emphasis id="B956EAC8FFD4021CFE01B3AB6F2A518E" box="[417,552,1949,1973]" italics="true" pageId="2" pageNumber="276">V. bejumanus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26557B3FFD4021CFD8EB3A86F66518F" box="[558,612,1949,1972]" pageId="2" pageNumber="276" rank="species">sp. n.</taxonomicNameLabel>
