); HL 
<quantity id="4CDA9B3FFFD5021DFD0DB57E6E16575E" box="[685,788,331,357]" metricMagnitude="-4" metricUnit="m" metricValue="2.3" pageId="3" pageNumber="277" unit="mm" value="0.23">0.23 mm</quantity>
, HW 
<quantity id="4CDA9B3FFFD5021DFCF7B57F6EBC575E" box="[855,958,330,357]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" pageId="3" pageNumber="277" unit="mm" value="0.25">0.25 mm</quantity>
, AnL 
<quantity id="4CDA9B3FFFD5021DFBA4B57F69AA575E" box="[1028,1192,330,357]" metricMagnitude="-4" metricUnit="m" metricValue="4.9" metricValueMax="5.0" metricValueMin="4.8" pageId="3" pageNumber="277" unit="mm" value="0.49" valueMax="0.5" valueMin="0.48">0.48–0.50 mm</quantity>
(mean 
<quantity id="4CDA9B3FFFD5021DFB5EB57E686A575E" box="[1278,1384,331,357]" metricMagnitude="-4" metricUnit="m" metricValue="4.8" pageId="3" pageNumber="277" unit="mm" value="0.48">0.48 mm</quantity>
); PL 
<quantity id="4CDA9B3FFFD5021DFF37B55A6C3857B2" box="[151,314,367,393]" metricMagnitude="-4" metricUnit="m" metricValue="3.15" metricValueMax="3.3" metricValueMin="3.0" pageId="3" pageNumber="277" unit="mm" value="0.315" valueMax="0.33" valueMin="0.3">0.30–0.33 mm</quantity>
(mean 
<quantity id="4CDA9B3FFFD5021DFE2DB55A6CF757B2" box="[397,501,367,393]" metricMagnitude="-4" metricUnit="m" metricValue="3.2" pageId="3" pageNumber="277" unit="mm" value="0.32">0.32 mm</quantity>
), PW 
<quantity id="4CDA9B3FFFD5021DFD98B55B6FD957B2" box="[568,731,366,393]" metricMagnitude="-4" metricUnit="m" metricValue="3.65" metricValueMax="3.8" metricValueMin="3.5" pageId="3" pageNumber="277" unit="mm" value="0.365" valueMax="0.38" valueMin="0.35">0.35–0.38 mm</quantity>
(mean 
<quantity id="4CDA9B3FFFD5021DFC8EB55A6E9457B2" box="[814,918,367,393]" metricMagnitude="-4" metricUnit="m" metricValue="3.5999999999999996" pageId="3" pageNumber="277" unit="mm" value="0.36">0.36 mm</quantity>
); EL 
<quantity id="4CDA9B3FFFD5021DFC72B55A693A57B2" box="[978,1080,367,393]" metricMagnitude="-4" metricUnit="m" metricValue="7.3" pageId="3" pageNumber="277" unit="mm" value="0.73">0.73 mm</quantity>
, EW 
<quantity id="4CDA9B3FFFD5021DFBD5B55A681A57B2" box="[1141,1304,367,393]" metricMagnitude="-4" metricUnit="m" metricValue="6.15" metricValueMax="6.3" metricValueMin="6.0" pageId="3" pageNumber="277" unit="mm" value="0.615" valueMax="0.63" valueMin="0.6">0.60–0.63 mm</quantity>
(mean 
