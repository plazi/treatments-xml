Dorsal habitus of male. 
<taxonomicName id="4C224D59FFD4021CFD8FB34C6E5851AA" authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[559,858,1913,1937]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="276" phylum="Arthropoda" rank="species" species="meridanus" status="sp. n.">
<emphasis id="B956EAC8FFD4021CFD8FB34C6E5851AA" box="[559,858,1913,1937]" italics="true" pageId="2" pageNumber="276">Venezolanoconnus meridanus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26557B3FFD4021CFCC0B34C6E9451AB" box="[864,918,1913,1936]" pageId="2" pageNumber="276" rank="species">sp. n.</taxonomicNameLabel>
(1); 
<taxonomicName id="4C224D59FFD4021CFC65B34F693151AA" authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[965,1075,1914,1937]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="276" phylum="Arthropoda" rank="species" species="minimus" status="sp. n.">
<emphasis id="B956EAC8FFD4021CFC65B34F693151AA" box="[965,1075,1914,1937]" italics="true" pageId="2" pageNumber="276">V. minimus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26557B3FFD4021CFB99B34C696C51AB" box="[1081,1134,1913,1936]" pageId="2" pageNumber="276" rank="species">sp. n.</taxonomicNameLabel>
(2); 
<taxonomicName id="4C224D59FFD4021CFB3EB34F683551AA" authority="Jałoszyński, 2020" authorityName="Jałoszyński" authorityYear="2020" box="[1182,1335,1913,1937]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="276" phylum="Arthropoda" rank="species" species="colombianus" status="sp. n.">
<emphasis id="B956EAC8FFD4021CFB3EB34F683551AA" box="[1182,1335,1913,1937]" italics="true" pageId="2" pageNumber="276">V. colombianus</emphasis>
