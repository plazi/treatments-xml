<taxonomicName id="92FF678917503B5FA177BAE106348E16" authority="" authorityName="" class="Gastropoda" family="Hydrobiidae" genus="Paludina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paludina exigua" order="Littorinimorpha" pageId="0" pageNumber="43" phylum="Mollusca" rank="species" species="exigua">
