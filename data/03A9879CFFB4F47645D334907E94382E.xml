<quantity id="4CF89B6FFFB4F47647F530707E4C3A72" box="[689,853,1086,1110]" metricMagnitude="-4" metricUnit="m" metricValue="8.7" metricValueMax="9.3" metricValueMin="8.1" pageId="74" pageNumber="75" unit="mm" value="0.87" valueMax="0.93" valueMin="0.81">
