<typeStatus id="54A508A4FFBC697EF374722627F88A69" box="[113,184,531,550]" pageId="7" pageNumber="302">SYNTYPE</typeStatus>
(
<specimenCount id="9D187D8FFFBC697EF3C07226279D8A64" box="[197,221,530,553]" pageId="7" pageNumber="302" type="female">1 ♀</specimenCount>
): ‘ 
<taxonomicName id="4C1ECD85FFBC697EF3F6722626058A6B" authorityName="Latreille" authorityYear="1804" box="[243,325,531,550]" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="302" phylum="Arthropoda" rank="genus">Myrmica</taxonomicName>
