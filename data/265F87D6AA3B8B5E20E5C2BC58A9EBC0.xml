<taxonomicName id="69F64D43AA3B8B5C227BC2FA58F4EE82" box="[521,649,1742,1767]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="266" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
