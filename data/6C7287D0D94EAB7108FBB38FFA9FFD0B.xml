<taxonomicName id="23DB4D45D94EAB7E0948B50EFD8AF9A2" box="[292,537,1558,1580]" class="Insecta" family="Mogoplistidae" genus="Micrornebius" higherTaxonomySource="CoL" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="124" phylum="Arthropoda" rank="species" species="distinctus" status="sp. n.">
<emphasis id="D6AFEAD4D94EAB7E0948B50EFD8AF9A2" box="[292,537,1558,1580]" italics="true" pageId="7" pageNumber="124">Micrornebius distinctus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CD9C57AFD94EAB7E0A49B50EFDF0F9A5" box="[549,611,1558,1579]" pageId="7" pageNumber="124" rank="species">
