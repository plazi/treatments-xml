<number id="3DD324DFFFEDFFB1FD435FD73192FF2C" box="[668,676,151,172]" pageId="3" pageNumber="46" value="1.0">1</number>
/ 
<number id="3DD324DFFFEDFFB1FD725FD7318CFF2C" box="[685,698,151,172]" pageId="3" pageNumber="46" value="2.0">2</number>
as long as thoracic horn................. 
<taxonomicName id="4C1B4D7BFFEDFFB1FBAD5FD836ADFF2C" authority="Walley" authorityName="Walley" box="[1138,1435,151,173]" class="Insecta" family="Chironomidae" genus="Ablabesmyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="46" phylum="Arthropoda" rank="species" species="peleensis" subGenus="Karelia">
