Loriol 
<number id="E54D7F3DFFEFFFCAFB46FD3557B8FD2C" box="[1204,1261,727,751]" pageId="14" pageNumber="287" value="1893.0">1893</number>
a
</bibRefCitation>
) and 
<collectingCountry id="2B922D8AFFEFFFCAFACFFD3456C9FD2C" box="[1341,1436,726,751]" name="Réunion" pageId="14" pageNumber="287">Réunion</collectingCountry>
(
<bibRefCitation id="371410EBFFEFFFCAFF6CFD1852D8FCD0" author="Guille" box="[158,397,762,787]" pageId="14" pageNumber="287" refString="Guille, A. &amp; Ribes, S. (1981) Echinodermes associes aux Scleractiniaires d'un recif frangeant de l'ile de La Reunion (ocean Indien). Bulletin du Museum national d'Histoire Naturelle, 3, 1, 73 - 92." type="journal article" year="1981">
