<subSubSection id="C3634ED78117CE0EFF5AF90BFB27E2B9" box="[151,1279,1760,1788]" pageId="9" pageNumber="338" type="materials_examined">
<paragraph id="8BC61D5C8117CE0EFF5AF90BFB27E2B9" blockId="9.[151,1437,1760,2040]" box="[151,1279,1760,1788]" pageId="9" pageNumber="338">
