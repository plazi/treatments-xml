<taxonomicName id="26EF679F9128074CFD48FCF4FB1E036B" authority="Basu 1964" authorityName="Basu" authorityYear="1964" box="[736,1241,850,877]" class="Insecta" family="Aphididae" genus="Indomasonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="anaphalidis">
<emphasis id="D39BC00E9128074CFD48FCF4FBF8036A" bold="true" box="[736,1087,851,877]" italics="true" pageId="11" pageNumber="12">Indomasonaphis anaphalidis</emphasis>
(Basu 
<number id="57270E3B9128074CFB3DFCF5FB16036B" box="[1173,1233,850,876]" pageId="11" pageNumber="12" value="1964.0">1964</number>
)
</taxonomicName>
<taxonomicName id="26EF679F9128074CFEA0FCD9FBFE039F" authority="Chakrabarti, Mandal &amp; Raha 1983" authorityName="Chakrabarti, Mandal &amp; Raha" authorityYear="1983" box="[264,1081,894,920]" class="Insecta" family="Aphididae" genus="Indumasonaphis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="tuberculata">
<emphasis id="D39BC00E9128074CFEA0FCD9FDAF039F" bold="true" box="[264,616,894,920]" italics="true" pageId="11" pageNumber="12">Indumasonaphis tuberculata</emphasis>
<bibRefCitation id="857E61ED9128074CFDDBFCD9FBFE039F" author="Chakrabarti" box="[627,1081,894,920]" pageId="11" pageNumber="12" refString="Chakrabarti, S, Mandal, A. K. &amp; Raha, S. (1983) Rhododendron-infesting aphids (Homoptera: Aphididae) of the Himalayas. Zoological Journal of the Linnean Society, 78, 349 - 362." type="journal article" year="1983">
Chakrabarti, Mandal &amp; Raha 
<number id="57270E3B9128074CFC57FCD9FBFE039F" box="[1023,1081,894,920]" pageId="11" pageNumber="12" value="1983.0">1983</number>
</bibRefCitation>
</taxonomicName>
= 
<taxonomicName id="26EF679F9128074CFBC9FCD9FDF703C5" authority="Basu 1964" authorityName="Basu" authorityYear="1964" class="Insecta" family="Aphididae" genus="Indomasonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="anaphalidis">
