<taxonomicName id="5B7B0C370510FEE3AB4C4A899858E68A" class="Insecta" family="Carabidae" genus="Forcipator" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Forcipator" order="Coleoptera" pageId="70" pageNumber="71" phylum="Arthropoda" rank="genus">Forcipator</taxonomicName>
