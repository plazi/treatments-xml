<taxonomicName id="4C562CA70A3FF54AFB3ECE42AF16FD14" box="[1217,1314,620,645]" class="Gastropoda" family="Helicinidae" genus="Helicina" higherTaxonomySource="CoL" kingdom="Animalia" order="Cycloneritida" pageId="58" pageNumber="59" phylum="Mollusca" rank="genus">
