(
<emphasis id="B9396909E25DFFACC8614DFC3980FF16" bold="true" box="[314,461,151,177]" italics="true" pageId="12" pageNumber="13">Homoneura</emphasis>
) 
<emphasis id="B9396909E25DFFACC8854DFC3A64FF16" bold="true" box="[478,553,151,177]" italics="true" pageId="12" pageNumber="13">cornis</emphasis>
