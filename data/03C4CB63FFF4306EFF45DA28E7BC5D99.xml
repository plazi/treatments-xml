<taxonomicName id="4C6D01F6FFF7306EFEC5D959E7335E04" authorityName="Hull" authorityYear="1941" box="[279,591,310,335]" class="Magnoliopsida" family="Asteraceae" genus="Mesogramma" kingdom="Plantae" order="Asterales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="species" species="steatogaster">
