and male 
<typeStatus id="54D6C4D7FFF7306EFD1AD95AE6525E04" box="[712,814,309,335]" pageId="54" pageNumber="55">holotype</typeStatus>
and female 
<typeStatus id="54D6C4D7FFF7306EFC6DD95AE1615E04" box="[959,1053,309,335]" pageId="54" pageNumber="55">allotype</typeStatus>
of 
<taxonomicName id="4C6D01F6FFF7306EFB96D959E01C5E04" authorityName="Hull" authorityYear="1943" box="[1092,1376,310,335]" class="Magnoliopsida" family="Asteraceae" genus="Mesogramma" kingdom="Plantae" order="Asterales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="species" species="steatornis">
