<taxonomicName id="A6934F80CB313126FD26F65F68E00410" class="Magnoliopsida" family="Araliaceae" genus="Hedera" kingdom="Plantae" order="Apiales" pageId="15" pageNumber="124" phylum="Tracheophyta" rank="species" species="maroccana">
