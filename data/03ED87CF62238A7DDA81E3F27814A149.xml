<quantity id="4CBC9B3C62238A7DD8F1E7557F67A22C" box="[723,860,1358,1379]" metricMagnitude="2" metricUnit="m" metricValue="3.35" metricValueMax="3.7" metricValueMin="3.0" pageId="15" pageNumber="1160" unit="m" value="335.0" valueMax="370.0" valueMin="300.0">300 to 370 m</quantity>
