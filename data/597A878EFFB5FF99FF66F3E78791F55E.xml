<heading id="8A2481F4FFB5FF99FF66F3E786F2F7E6" bold="true" box="[151,509,1230,1257]" fontSize="11" level="1" pageId="9" pageNumber="390" reason="1">
<emphasis id="E3A7EA8AFFB5FF99FF66F3E786F2F7E6" bold="true" box="[151,509,1230,1257]" pageId="9" pageNumber="390">
<taxonomicName id="16D34D1BFFB5FF99FF66F3E78641F7E6" box="[151,334,1230,1257]" class="Insecta" family="Cicadellidae" genus="Hecaloidella" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="390" phylum="Arthropoda" rank="species" species="borneoensis">
<emphasis id="E3A7EA8AFFB5FF99FF66F3E787A1F7E6" bold="true" box="[151,174,1231,1257]" italics="true" pageId="9" pageNumber="390">H</emphasis>
. 
<emphasis id="E3A7EA8AFFB5FF99FF4CF3E68641F7E6" bold="true" box="[189,334,1230,1257]" italics="true" pageId="9" pageNumber="390">borneoensis</emphasis>
