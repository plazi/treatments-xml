<taxonomicName id="145BE3D0F98BFFA09B55289ECC4F3B03" class="Arachnida" family="Telemidae" genus="Telema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Telema yashanensis" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="yashanensis">Telema yashanensis</taxonomicName>
