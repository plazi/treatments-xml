<subSubSection id="C37A651C6D66961E5FAFFBF3822BFBBD" pageId="12" pageNumber="313" type="etymology">
<paragraph id="8BDF36976D66961E5FAFFBF3822BFBBD" blockId="12.[151,1437,151,1149]" pageId="12" pageNumber="313">
<emphasis id="B914EA856D66961E5FAFFBF3806AFB9E" bold="true" box="[199,334,1087,1112]" inLineHeading="true" pageId="12" pageNumber="313" reason="1">Etymology.</emphasis>
