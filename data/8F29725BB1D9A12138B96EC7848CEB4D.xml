<taxonomicName id="64C86DE81B5670D43865AAF12943B748" class="Insecta" family="Mycteridae" genus="Hemipeplus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemipeplus" order="Coleoptera" pageId="192" pageNumber="193" phylum="Arthropoda" rank="genus">Hemipeplus</taxonomicName>
