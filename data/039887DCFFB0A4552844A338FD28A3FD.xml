<taxonomicName id="4C314D49FFB0A45429D5A118FE78A304" authorityName="Santschi" authorityYear="1932" box="[248,388,1796,1822]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="dichroum">
