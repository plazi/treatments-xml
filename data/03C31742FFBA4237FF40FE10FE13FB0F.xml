<taxonomicName id="4C6ADDD7FFBA4237FCBDFCFAFB96FC43" authorityName="Thomas" authorityYear="1908" box="[874,1113,894,919]" class="Mammalia" family="Muridae" genus="Phodopus" kingdom="Animalia" order="Rodentia" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="bedfordiae">
