<taxonomicName id="4C6ADDD7FFBA4237FB47FCFBFAF0FC43" authorityName="Thomas" authorityYear="1908" box="[1168,1343,894,919]" class="Mammalia" family="Dipodidae" genus="Dipus" kingdom="Animalia" order="Rodentia" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="sowerbyi">
<emphasis id="B91E7A46FFBA4237FB47FCFBFAF0FC43" box="[1168,1343,894,919]" italics="true" pageId="9" pageNumber="10">Dipus sowerbyi</emphasis>
