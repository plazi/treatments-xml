<taxonomicName id="4C571393FFF23870FF0EC3B2FE79FE2F" authority="Johannsen" authorityName="Johannsen" box="[151,445,438,464]" class="Insecta" family="Chironomidae" genus="Brillia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="parva">
<emphasis id="B923B402FFF23870FF0EC3B2FE79FE2F" bold="true" box="[151,445,438,464]" pageId="16" pageNumber="17">
<emphasis id="B923B402FFF23870FF0EC3B2FEF4FE2F" bold="true" box="[151,304,438,464]" italics="true" pageId="16" pageNumber="17">Brillia parva</emphasis>
