<taxonomicName id="E5DB4D5F891F065AFD81FECCFD18CA51" box="[535,643,260,285]" class="Secernentea" family="Rhabdiasidae" genus="Rhabdias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="11" pageNumber="462" phylum="Nematoda" rank="genus">
