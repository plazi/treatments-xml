<taxonomicName id="7FEDC9E0FFF0FFDDB0C26CDDE719A2E6" authority="Chen, Lin, Huang &amp; Yang, 2015" authorityName="Chen, Lin, Huang &amp; Yang" authorityYear="2015" box="[151,1034,393,419]" class="Insecta" family="Nitidulidae" genus="Meligethes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="115" phylum="Arthropoda" rank="species" species="scrobescens" subGenus="Odontogethes">
<emphasis id="8A996E71FFF0FFDDB0C26CDDE719A2E6" bold="true" box="[151,1034,393,419]" pageId="14" pageNumber="115">
<emphasis id="8A996E71FFF0FFDDB0C26CDDE208A2E6" bold="true" box="[151,283,393,419]" italics="true" pageId="14" pageNumber="115">Meligethes</emphasis>
(
<emphasis id="8A996E71FFF0FFDDB1796CDDE2C1A2E6" bold="true" box="[300,466,393,419]" italics="true" pageId="14" pageNumber="115">Odontogethes</emphasis>
) 
<emphasis id="8A996E71FFF0FFDDB1B66CDDE161A2E6" bold="true" box="[483,626,393,419]" italics="true" pageId="14" pageNumber="115">scrobescens</emphasis>
<bibRefCitation id="DC7CCF92FFF0FFDDB22C6CDDE719A2E6" author="Chen" box="[633,1034,393,419]" pageId="14" pageNumber="115" refString="Chen, Y., Lin, X. L., Huang, M. &amp; Yang, X. K. (2015) A new species of Lamiogethes and a new species of Meligethes from China (Coleoptera: Nitidulidae: Meligethinae). Zootaxa, 3999 (3), 413 - 420. http: // dx. doi. org / 10.11646 / zootaxa. 3999.3.6" type="journal article" year="2015">
Chen, Lin, Huang &amp; Yang, 
<number id="0E25A044FFF0FFDDB39A6CDDE719A2E6" box="[975,1034,393,419]" pageId="14" pageNumber="115" value="2015.0">2015</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
