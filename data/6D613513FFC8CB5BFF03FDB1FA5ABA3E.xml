<taxonomicName id="22C8FF86FFC8CB5BFE65FDB0FD4ABF5C" authorityName="C. G. Thomson" authorityYear="1863" box="[497,652,582,608]" family="Bostrichidae" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="508" rank="subFamily" subFamily="Dinoderinae">Dinoderinae</taxonomicName>
