<materialsCitation id="BDAB3CA5FF9FFFAB813AB226FD0DFD11" collectingDate="2001-11-12" collectingDateMax="2001-11-13" collectingDateMin="2001-11-12" collectionCode="QM" collectorName="C. Burwell &amp; G. Monteith" country="New Caledonia" elevation="1100" location="New Caledonia" pageId="14" pageNumber="1879" specimenCount="2" specimenCount-female="1" specimenCount-male="1" typeStatus="paratype">
