<taxonomicName id="4C4A26A9FFBF8F75FD51F801ACCBF800" authority="Pic" authorityName="Pic" box="[725,928,1969,1994]" class="Insecta" family="Cleridae" genus="Lemidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="282" phylum="Arthropoda" rank="species" species="albonotata">
