<quantity id="4CFCCF9DFF90DD288624FE62FD1D5FFA" box="[602,758,440,464]" metricMagnitude="-5" metricUnit="kg" metricValue="2.9899999999999998" metricValueMax="3.84" metricValueMin="2.1399999999999997" pageId="8" pageNumber="315" unit="mg" value="29.9" valueMax="38.4" valueMin="21.4">21.4–38.4 mg</quantity>
/L. Sessile on 
<taxonomicName id="4C0419FBFF90DD2887EDFE6DFBD95FFA" box="[915,1074,439,464]" class="Magnoliopsida" family="Haloragaceae" genus="Myriophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Saxifragales" pageId="8" pageNumber="315" phylum="Tracheophyta" rank="genus">
