), 
<collectingRegion id="49C0AC9AFF90DD28869BFE4EFCD25F87" box="[741,825,404,429]" country="United States of America" name="Florida" pageId="8" pageNumber="315">Florida</collectingRegion>
, and 
<collectingRegion id="49C0AC9AFF90DD28870DFE4EFC345F87" box="[883,991,404,429]" country="United States of America" name="Michigan" pageId="8" pageNumber="315">Michigan</collectingRegion>
(
<collectingCountry id="F31322E8FF90DD288791FE4EFBD55F86" box="[1007,1086,404,428]" name="United States of America" pageId="8" pageNumber="315">U.S.A.</collectingCountry>
