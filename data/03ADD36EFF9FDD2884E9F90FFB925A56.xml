</subSubSection>
<subSubSection id="C31E31F3FF90DD2884B9FE6DFB185FE5" box="[199,1267,439,464]" pageId="8" pageNumber="315" type="biology_ecology">
<paragraph id="8BBB6278FF90DD2884B9FE6DFB185FE5" blockId="8.[151,1437,151,1149]" box="[199,1267,439,464]" pageId="8" pageNumber="315">
<emphasis id="B970BE6AFF90DD2884B9FE6DFEC45FFA" bold="true" box="[199,303,439,464]" pageId="8" pageNumber="315">Ecology:</emphasis>
