<location id="1F38602E9648425DFD8C3B201895F70F" LSID="urn:lsid:plazi:treatment:924E87E3964C425DFF4C3FCD19D6F7E3:1F38602E9648425DFD8C3B201895F70F" box="[599,957,295,321]" country="Iran" county="Shiraz" municipality="Shiraz University" name="Department of Plant Protection" pageId="6" pageNumber="377">Department of Plant Protection</location>
, 
<location id="1F38602E9648425DFC113B201FEBF70F" LSID="urn:lsid:plazi:treatment:924E87E3964C425DFF4C3FCD19D6F7E3:1F38602E9648425DFC113B201FEBF70F" box="[970,1219,295,321]" country="Iran" county="Shiraz" municipality="Shiraz University" name="School of Agriculture" pageId="6" pageNumber="377">School of Agriculture</location>
, 
<collectingMunicipality id="FA3CAC8F9648425DFB0B3B201EB0F70F" box="[1232,1432,295,321]" pageId="6" pageNumber="377">Shiraz University</collectingMunicipality>
