<emphasis id="B957EAA1FF8E2A3EFB2FFD6DFB6FDED5" bold="true" box="[1162,1192,715,736]" pageId="7" pageNumber="6">12.</emphasis>
<taxonomicName id="4C234D30FF8E2A3EFB15FD6DFB33DED5" authorityName="Pearse" authorityYear="1952" box="[1200,1268,715,736]" class="Maxillopoda" family="Telsidae" genus="Telson" kingdom="Animalia" order="Poecilostomatoida" pageId="7" pageNumber="12" phylum="Arthropoda" rank="genus">Telson</taxonomicName>
