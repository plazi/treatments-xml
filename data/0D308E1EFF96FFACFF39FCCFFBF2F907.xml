'N
</geoCoordinate>
, 
<geoCoordinate id="E0AD59CFFF96FFACFE9FF9C8FE46F9B7" box="[305,404,1632,1658]" direction="east" orientation="longitude" pageId="30" pageNumber="31" precision="925" value="34.9">
<number id="33512D2FFF96FFACFE9FF9C8FE83F9B7" box="[305,337,1632,1658]" pageId="30" pageNumber="31" value="34.0">34</number>
