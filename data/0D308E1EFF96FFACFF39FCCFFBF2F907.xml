</geoCoordinate>
, 
<geoCoordinate id="E0AD59CFFF96FFACFF39F990FF24F99F" box="[151,246,1592,1618]" direction="east" orientation="longitude" pageId="30" pageNumber="31" precision="925" value="40.216667">
