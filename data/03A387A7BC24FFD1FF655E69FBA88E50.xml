<caption id="DF756639BC23FFD0FF655B77FD9A8B85" httpUri="https://zenodo.org/record/197927/files/figure.png" pageId="14" pageNumber="62" targetBox="[151,1436,194,1878]" targetPageId="14">
<paragraph id="8BB536B1BC23FFD0FF655B77FD9A8B85" blockId="14.[151,1436,1900,1956]" pageId="14" pageNumber="62">
