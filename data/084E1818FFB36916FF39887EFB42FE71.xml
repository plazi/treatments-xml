(n=5); WL= 
<quantity id="471F04EBFFB16916FCE38EE3FC55FF56" box="[845,977,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="4.125" metricValueMax="4.25" metricValueMin="4.0" pageId="46" pageNumber="47" unit="mm" value="4.125" valueMax="4.25" valueMin="4.0">4–4.25 mm</quantity>
, C2= 
<quantity id="471F04EBFFB16916FBBE8EECFB01FF56" box="[1040,1157,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" metricValueMax="1.2" metricValueMin="1.0" pageId="46" pageNumber="47" unit="mm" value="1.1" valueMax="1.2" valueMin="1.0">1–1.2 mm</quantity>
; AL= 
<quantity id="471F04EBFFB16916FB668EE3FAE6FF56" box="[1224,1378,151,176]" metricMagnitude="-4" metricUnit="m" metricValue="8.75" metricValueMax="9.0" metricValueMin="8.5" pageId="46" pageNumber="47" unit="mm" value="0.875" valueMax="0.9" valueMin="0.85">0.85–0.9 mm</quantity>
; AL/ C2=0.90–1.02. 
<emphasis id="B293751CFFB16916FEEB8EC8FE0DFF2C" bold="true" box="[325,393,188,213]" pageId="46" pageNumber="47">Male.</emphasis>
BL=4.0– 
<quantity id="471F04EBFFB16916FE5B8EC8FDCEFF2D" box="[501,586,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="46" pageNumber="47" unit="mm" value="4.5">4.5 mm</quantity>
(n=5); WL= 
<quantity id="471F04EBFFB16916FD748EC8FCE4FF2D" box="[730,864,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="4.2" metricValueMin="3.8" pageId="46" pageNumber="47" unit="mm" value="4.0" valueMax="4.2" valueMin="3.8">3.8–4.2 mm</quantity>
(n=5).
</paragraph>
