<taxonomicName id="B6854D67FFDACF13F054DB55BBDBF88D" authorityName="Wilke" authorityYear="2003" box="[806,981,1883,1904]" class="Gastropoda" family="Hydrobiidae" genus="Salenthydrobia" kingdom="Animalia" order="Littorinimorpha" pageId="14" pageNumber="333" phylum="Mollusca" rank="genus">
