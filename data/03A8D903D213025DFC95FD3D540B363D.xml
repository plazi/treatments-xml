<taxonomicName id="4C011396D213025DFC02FC52543830A0" authority="Back et al. 2000" authorityName="Back" authorityYear="2000" box="[939,1338,934,958]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="28" pageNumber="2810" phylum="Firmicutes" rank="species" species="perolens">
<emphasis id="B975B407D213025DFC02FC52558D30A0" box="[939,1167,934,958]" italics="true" pageId="28" pageNumber="2810">Lactobacillus perolens</emphasis>
Back 
<emphasis id="B975B407D213025DFB66FC5255F930A0" box="[1231,1275,934,958]" italics="true" pageId="28" pageNumber="2810">et al</emphasis>
. 2000
</taxonomicName>
, 3 
<superScript id="7C74C55DD213025DFAFDFC50546A30AC" attach="none" box="[1364,1384,932,946]" fontSize="6" pageId="28" pageNumber="2810">VP</superScript>
