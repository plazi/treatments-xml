<quantity id="4CC92BA8FFF71304B8B66096FD90FB00" box="[439,519,1237,1260]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="0" pageNumber="498" unit="m" value="1.0">1 meter</quantity>
