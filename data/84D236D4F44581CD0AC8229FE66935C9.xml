<paragraph id="05656E7C6A2698A2D33A7ABDDD8C9491" pageId="2" pageNumber="3">
<taxonomicName id="792C7B58CF38BCBD9E25BE183CDFB73B" class="Monogenea" family="Capsalidae" genus="Benedenia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Benedenia hawaiiensis" order="Capsalidea" pageId="2" pageNumber="3" phylum="Platyhelminthes" rank="species" species="hawaiiensis">Benedenia hawaiiensis</taxonomicName>
