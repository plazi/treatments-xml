<taxonomicName id="270DADEEED6AF5619920855EF2E3C644" class="Actinopterygii" family="Priacanthidae" genus="Priacanthus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Priacanthus cruentatus" order="Perciformes" pageId="2" pageNumber="3" phylum="Chordata" rank="species" species="cruentatus">Priacanthus cruentatus</taxonomicName>
