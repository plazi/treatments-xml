</taxonomicName>
(me.tri.o.pte’rae. N.L. fem. gen. n. 
<taxonomicName id="4C204D1FFF952169DAB1FC45FBA5618D" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[902,1034,979,1002]" class="Bacilli" family="Lactobacillaceae" genus="Companilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="24" pageNumber="2806" phylum="Firmicutes" rank="species" species="metriopterae">
