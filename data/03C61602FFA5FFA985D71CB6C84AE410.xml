<taxonomicName id="4C6FDC97FFA5FFA887091D09C897E046" authority="(BERLESE, 1883)" baseAuthorityName="BERLESE" baseAuthorityYear="1883" box="[620,933,1349,1371]" class="Arachnida" family="Protoribatidae" genus="Protoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="1785" phylum="Arthropoda" rank="species" species="dentatus">
