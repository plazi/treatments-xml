<taxonomicName id="4C6FDC97FFA5FFA884E41D2FC92FE06A" baseAuthorityName="BERLESE" baseAuthorityYear="1904" box="[385,541,1378,1400]" class="Arachnida" family="Protoribatidae" genus="Protoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="1785" phylum="Arthropoda" rank="species" species="lophotrichus">
