<quantity id="9D1B36C4FFB2FFF85AF9E630E6170EA6" box="[762,852,1906,1932]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="40" pageNumber="41" unit="mm" value="0.5">0.5 mm</quantity>
