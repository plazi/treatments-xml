<emphasis id="7085E735FFBFFFACFC9CCCDEFC23FF69" bold="true" box="[835,963,160,183]" pageId="8" pageNumber="639">Discussion.</emphasis>
<taxonomicName id="85F140A4FFBFFFACFC15CCDEFB46FF69" authorityName="Costa" authorityYear="2009" baseAuthorityName="Monte" baseAuthorityYear="1945" box="[970,1190,160,183]" class="Insecta" family="Tingidae" genus="Ceratotingis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="639" phylum="Arthropoda" rank="species" species="spatula">
