<number id="3DF7DE2D741D296FFBE11560FBAEF8F2" box="[1092,1136,1801,1827]" pageId="22" pageNumber="23" value="165.0">
<part id="FF9B6697741D296FFBE11560FBAEF8F2" box="[1092,1136,1801,1827]" pageId="22" pageNumber="23" type="volume">165</part>
