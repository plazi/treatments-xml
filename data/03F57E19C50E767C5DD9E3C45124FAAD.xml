<taxonomicName id="4C5CB48CC50E767C5F15E2A457C3FB9B" baseAuthorityName="Fabricius" baseAuthorityYear="1793" box="[623,761,1007,1029]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="1757" phylum="Arthropoda" rank="species" species="rotundata">
