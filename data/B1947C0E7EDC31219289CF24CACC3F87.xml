<taxonomicName id="CD68AE0CD392D0F63C95CF719E59B2D5" authority="Borkhausen" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="416" phylum="Tracheophyta" rank="species" species="tomentosus">
