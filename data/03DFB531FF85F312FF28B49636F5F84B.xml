<collectingCountry id="F36144B7FF85F312FC8CB37B37A5F833" box="[819,903,1909,1932]" name="Finland" pageId="1" pageNumber="66">Finland</collectingCountry>
, 
<collectingCountry id="F36144B7FF85F312FC2CB37B37D5F833" box="[915,1015,1909,1932]" name="Germany" pageId="1" pageNumber="66">Germany</collectingCountry>
, 
<collectingCountry id="F36144B7FF85F312FBBBB37B3040F833" box="[1028,1122,1909,1932]" name="Hungary" pageId="1" pageNumber="66">Hungary</collectingCountry>
, 
<collectingCountry id="F36144B7FF85F312FBD0B37B30FAF833" box="[1135,1240,1909,1932]" name="Lithuania" pageId="1" pageNumber="66">Lithuania</collectingCountry>
, 
<collectingCountry id="F36144B7FF85F312FB5AB37B3110F833" box="[1253,1330,1909,1932]" name="Poland" pageId="1" pageNumber="66">Poland</collectingCountry>
, 
<collectingCountry id="F36144B7FF85F312FA80B37B31BEF833" box="[1343,1436,1909,1932]" name="Slovakia" pageId="1" pageNumber="66">Slovakia</collectingCountry>
and 
<collectingCountry id="F36144B7FF85F312FF7BB3963567F810" box="[196,325,1944,1967]" name="Switzerland" pageId="1" pageNumber="66">Switzerland</collectingCountry>
