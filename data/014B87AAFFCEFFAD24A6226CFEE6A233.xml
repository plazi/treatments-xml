<subSubSection id="C1F86537FFCDFFAD24F620D1FAABA1FB" box="[199,1337,475,501]" pageId="10" pageNumber="49" type="etymology">
<paragraph id="895D36BCFFCDFFAD24F620D1FAABA1FB" blockId="10.[151,1437,151,573]" box="[199,1337,475,501]" pageId="10" pageNumber="49">
<emphasis id="BB96EAAEFFCDFFAD24F620D1FEC0A1FB" bold="true" box="[199,338,475,501]" pageId="10" pageNumber="49">Etymology:</emphasis>
