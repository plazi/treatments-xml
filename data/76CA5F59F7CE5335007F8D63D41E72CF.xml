<taxonomicName id="A2E69C3FF9C52E3547BE39BABDCB8F80" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia philippinensis" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="philippinensis">Loboscelidia philippinensis</taxonomicName>
