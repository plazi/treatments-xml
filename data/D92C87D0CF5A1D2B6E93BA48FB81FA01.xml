<taxonomicName id="96854D45CF5A1D2B6B70B9D5FA88FA81" baseAuthorityName="Hayward &amp; Rohde" baseAuthorityYear="1999" box="[1292,1406,1267,1294]" class="Actinopterygii" family="Scombridae" genus="Scomberomorus" kingdom="Animalia" order="Perciformes" pageId="61" pageNumber="562" phylum="Chordata" rank="species" species="concolor">
<emphasis id="63F1EAD4CF5A1D2B6B70B9D5FA88FA81" box="[1292,1406,1267,1294]" italics="true" pageId="61" pageNumber="562">S. concolor</emphasis>
</taxonomicName>
and 
<taxonomicName id="96854D45CF5A1D2B6D52B835FC74FAA1" baseAuthorityName="Hayward &amp; Rohde" baseAuthorityYear="1999" box="[814,898,1300,1326]" class="Actinopterygii" family="Scombridae" genus="Scomberomorus" kingdom="Animalia" order="Perciformes" pageId="61" pageNumber="562" phylum="Chordata" rank="species" species="sierra">
<emphasis id="63F1EAD4CF5A1D2B6D52B835FC74FAA1" box="[814,898,1300,1326]" italics="true" pageId="61" pageNumber="562">S. sierra</emphasis>
</taxonomicName>
from 
<collectingRegion id="9341F824CF5A1D2B6DBAB835FBD1FAA2" box="[966,1063,1300,1326]" country="Mexico" name="Veracruz de Ignacio de la Llave" pageId="61" pageNumber="562">Veracruz</collectingRegion>
; 
<taxonomicName id="96854D45CF5A1D2B6A4FB835FB4EFAA1" box="[1075,1208,1299,1326]" class="Actinopterygii" family="Scombridae" genus="Scomberomorus" kingdom="Animalia" order="Perciformes" pageId="61" pageNumber="562" phylum="Chordata" rank="species" species="maculatus">
<emphasis id="63F1EAD4CF5A1D2B6A4FB835FB4EFAA1" box="[1075,1208,1299,1326]" italics="true" pageId="61" pageNumber="562">S. maculatus</emphasis>
</taxonomicName>
from 
<collectingRegion id="9341F824CF5A1D2B6A82B835FAA6FAA2" box="[1278,1360,1300,1326]" country="Mexico" name="Oaxaca" pageId="61" pageNumber="562">Oaxaca</collectingRegion>
, 
<collectingRegion id="9341F824CF5A1D2B6B20B835FA5DFAA2" box="[1372,1451,1300,1326]" country="Mexico" name="Sonora" pageId="61" pageNumber="562">Sonora</collectingRegion>
, 
<collectingRegion id="9341F824CF5A1D2B6D51B812FC77FAC1" box="[813,897,1331,1357]" country="Mexico" name="Nayarit" pageId="61" pageNumber="562">Nayarit</collectingRegion>
, 
<collectingRegion id="9341F824CF5A1D2B6DF0B812FC25FAC1" box="[908,979,1331,1357]" country="Mexico" name="Jalisco" pageId="61" pageNumber="562">Jalisco</collectingRegion>
, and 
<collectingRegion id="9341F824CF5A1D2B6A73B812FB83FAC1" box="[1039,1141,1331,1357]" country="Mexico" name="Guerrero" pageId="61" pageNumber="562">Guerrero</collectingRegion>
. Finally, the record of Bravo- Hollis (1953), assigned to 
<collectingRegion id="9341F824CF5A1D2B6A24B872FB5DFAE1" box="[1112,1195,1363,1389]" country="Mexico" name="Nayarit" pageId="61" pageNumber="562">Nayarit</collectingRegion>
