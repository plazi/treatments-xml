<taxonomicName id="96854D45CF5A1D2B6A4EB9F5FB7DFB61" authorityName="MacCallum" authorityYear="1913" box="[1074,1163,1236,1261]" class="Monogenea" family="Thoracocotylidae" genus="Thoracocotyle" kingdom="Animalia" order="Mazocraeidea" pageId="61" pageNumber="562" phylum="Platyhelminthes" rank="species" species="crocea">
<emphasis id="63F1EAD4CF5A1D2B6A4EB9F5FB7DFB61" box="[1074,1163,1236,1261]" italics="true" pageId="61" pageNumber="562">T. crocea</emphasis>
</taxonomicName>
listed by Kohn 
<emphasis id="63F1EAD4CF5A1D2B6B53B9F5FA97FB62" box="[1327,1377,1236,1262]" italics="true" pageId="61" pageNumber="562">et al.</emphasis>
