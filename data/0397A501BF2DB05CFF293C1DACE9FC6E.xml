); Marina di Arbus, 2006 (
<collectionCode id="ED2F8CD2BF2EB05CFF213AE3AC5AFE3E" box="[159,281,432,458]" pageId="26" pageNumber="365">CNBFVR</collectionCode>
, 
<collectionCode id="ED2F8CD2BF2EB05CFE9B3AE3ACD0FE3E" box="[293,403,432,458]" pageId="26" pageNumber="365">MCSNG</collectionCode>
); Montevecchio, 1990 (
<collectionCode id="ED2F8CD2BF2EB05CFD193AE3AFAEFE3E" box="[679,749,432,458]" pageId="26" pageNumber="365">CME</collectionCode>
); 
<collectingRegion id="49FADAF5BF2EB05CFCBF3AE3AE03FE3E" box="[769,832,432,458]" country="Portugal" name="Porto" pageId="26" pageNumber="365">Porto</collectingRegion>
Palma, 2003 (
<collectionCode id="ED2F8CD2BF2EB05CFC553AE3A96BFE3E" box="[1003,1064,432,458]" pageId="26" pageNumber="365">CPA</collectionCode>
); Sant'Antonio di Santadi, 2003 (
