(CASENT0901156) C 
<taxonomicName id="E31CABA4914371A57F6EF70F2C490214" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium tabarum" order="Hymenoptera" pageId="8" pageNumber="85" phylum="Arthropoda" rank="species" species="tabarum">Tetramorium tabarum</taxonomicName>
