<quantity id="60406CAFFF975F05E823F9F0E33EF9D6" box="[282,381,1630,1653]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="6" pageNumber="537" unit="mm" value="0.35">0.35 mm</quantity>
