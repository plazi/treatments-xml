<quantity id="60406CAFFF975F05E8BBF9B0E3A2F996" box="[386,481,1566,1589]" metricMagnitude="-3" metricUnit="m" metricValue="3.35" pageId="6" pageNumber="537" unit="mm" value="3.35">3.35 mm</quantity>
, WHum 
<quantity id="60406CAFFF975F05EB04F9B0E0DFF996" box="[573,668,1566,1589]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="6" pageNumber="537" unit="mm" value="1.1">1.10 mm</quantity>
, EL 
<quantity id="60406CAFFF975F05EBF0F9B0E280F9F6" metricMagnitude="-3" metricUnit="m" metricValue="2.45" pageId="6" pageNumber="537" unit="mm" value="2.45">2.45 mm</quantity>
, WHe 
<quantity id="60406CAFFF975F05E831F990E324F9F6" box="[264,359,1598,1621]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="6" pageNumber="537" unit="mm" value="0.8">0.80 mm</quantity>
, PL 
<quantity id="60406CAFFF975F05E8AAF990E3B1F9F6" box="[403,498,1598,1621]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="6" pageNumber="537" unit="mm" value="0.55">0.55 mm</quantity>
, PW 1.00 mm, Edist 
<quantity id="60406CAFFF975F05EBF0F990E280F9D6" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="6" pageNumber="537" unit="mm" value="0.4">0.40 mm</quantity>
