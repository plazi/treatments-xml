<subSubSection id="4B904DCBFFD0157210D11E42FE4986B7" pageId="5" pageNumber="599" type="materials_examined">
<paragraph id="03351E40FFD0157210D11E42FE4986B7" blockId="5.[151,1436,1913,2011]" pageId="5" pageNumber="599">
<emphasis id="31FEC252FFD0157210D11E42FEDA8693" bold="true" box="[151,353,1914,1939]" pageId="5" pageNumber="599">Material studied</emphasis>
