<taxonomicName id="4C074D71E668F600FF66FDC05A5203DE" authority="Trybom, 1910: 164" authorityName="Trybom" authorityPageNumber="164" authorityYear="1910" box="[151,590,634,657]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="quadridentata">
