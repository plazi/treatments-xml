<typeStatus box="[363,465,565,587]" pageId="38" pageNumber="39">Holotype</typeStatus>
<specimenCount box="[494,515,565,587]" pageId="38" pageNumber="39" type="female">♀</specimenCount>
, 
<collectingCountry box="[552,758,565,587]" name="El Salvador" pageId="38" pageNumber="39">EL SALVADOR</collectingCountry>
: 
<emphasis box="[145,290,596,617]" italics="true" pageId="38" pageNumber="39">
<collectingRegion box="[145,290,596,617]" country="El Salvador" name="Ahuachapan" pageId="38" pageNumber="39">Ahuachapán</collectingRegion>
</emphasis>
, El Corozo, El Imposible, 
<date box="[589,703,596,618]" pageId="38" pageNumber="39" value="1987-07-09">9.vii.1987</date>
, ‘#10 
<emphasis box="[145,367,626,648]" italics="true" pageId="38" pageNumber="39">
ex 
<taxonomicName baseAuthorityName="Doubleday" baseAuthorityYear="1844" box="[174,367,626,647]" class="Insecta" family="Papilionidae" genus="Parides" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="photinus">Parides photinus</taxonomicName>
</emphasis>
at 500 m’ (
<taxonomicName box="[489,580,627,648]" class="Actinopterygii" family="Serranidae" genus="Serrano" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="38" pageNumber="39" phylum="Chordata" rank="genus">
