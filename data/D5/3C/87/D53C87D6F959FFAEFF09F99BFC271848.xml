, 
<date box="[1009,1091,657,679]" pageId="26" pageNumber="27" value="1970-11">xi.1970</date>
(
<emphasis box="[1111,1231,657,678]" italics="true" pageId="26" pageNumber="27">Plaumann</emphasis>
