Deuve, Lassalle &amp; Queinnec, 
<number box="[285,349,1582,1608]" pageId="9" pageNumber="10" value="1985.0">1985</number>
</bibRefCitation>
</taxonomicName>
, from Uttar Pradesh, 
<taxonomicName authority="Morvan, 1994" authorityName="Morvan" authorityYear="1994" box="[616,909,1582,1608]" class="Insecta" family="Carabidae" genus="Pristosia" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="dahud">
<emphasis box="[616,723,1583,1608]" italics="true" pageId="9" pageNumber="10">P. dahud</emphasis>
<bibRefCitation author="Morvan" box="[732,909,1582,1608]" pageId="9" pageNumber="26" refString="Morvan, D. m. (1994) Carabidae nouveaux du Nepal et de Malaisie (Coleoptera, Carabidae). Bulletin de la Societe entomologique de France 99 (3), 323 - 334." type="journal article" year="1994">
Morvan, 
<number box="[845,909,1582,1608]" pageId="9" pageNumber="10" value="1994.0">1994</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName box="[923,1055,1583,1608]" class="Insecta" family="Carabidae" genus="Pristosia" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="glabella" status="sp. n.">
