and 
<taxonomicName box="[330,443,940,966]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="60" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
; 
<taxonomicName authorityName="Schoenherr" authorityYear="1838" box="[454,617,940,965]" class="Insecta" family="Dryophthoridae" genus="Orthognathus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="genus">
<emphasis box="[454,617,940,965]" italics="true" pageId="3" pageNumber="60">Orthognathus</emphasis>
