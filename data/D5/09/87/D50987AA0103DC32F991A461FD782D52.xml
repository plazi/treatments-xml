<taxonomicName box="[837,973,728,752]" class="Insecta" family="Formicidae" genus="Orthocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[837,973,728,752]" italics="true" pageId="7" pageNumber="54">Orthocrema</emphasis>
</taxonomicName>
species (
<taxonomicName box="[1077,1217,728,752]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="species" species="irritabilis">
<emphasis box="[1077,1217,728,752]" italics="true" pageId="7" pageNumber="54">C. irritabilis</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1271,1369,728,752]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="species" species="polita">
<emphasis box="[1271,1369,728,752]" italics="true" pageId="7" pageNumber="54">C. polita</emphasis>
</taxonomicName>
).
</paragraph>
<paragraph blockId="7.[151,1436,799,1400]" pageId="7" pageNumber="54">
<number box="[151,166,799,823]" pageId="7" pageNumber="54" value="1.0">1</number>
