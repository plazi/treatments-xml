<paragraph blockId="7.[151,1436,655,752]" pageId="7" pageNumber="54">
<emphasis bold="true" box="[151,297,655,680]" pageId="7" pageNumber="54">Distribution</emphasis>
: GLOBAL (same as genus). Former members of subgenera 
<taxonomicName box="[974,1141,656,679]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus" sensu="stricto">
<emphasis box="[974,1141,656,679]" italics="true" pageId="7" pageNumber="54">Crematogaster</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1148,1290,655,679]" pageId="7" pageNumber="54" sensu="stricto">sensu stricto</taxonomicNameLabel>
, 
<taxonomicName authority="Oxygyne" authorityName="Oxygyne" class="Insecta" family="Formicidae" genus="Decacrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[1301,1429,657,680]" italics="true" pageId="7" pageNumber="54">Decacrema</emphasis>
, 
<emphasis box="[151,249,693,716]" italics="true" pageId="7" pageNumber="54">Oxygyne</emphasis>
</taxonomicName>
(incl. 
<taxonomicName box="[340,496,693,716]" class="Insecta" family="Formicidae" genus="Nematocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[340,496,693,716]" italics="true" pageId="7" pageNumber="54">Nematocrema</emphasis>
</taxonomicName>
), 
<taxonomicName box="[530,660,693,716]" class="Insecta" family="Formicidae" genus="Mesocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[530,660,693,716]" italics="true" pageId="7" pageNumber="54">Mesocrema</emphasis>
</taxonomicName>
(in part), 
<taxonomicName box="[800,920,692,716]" class="Insecta" family="Hymenoptera" genus="Atopogyne" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[800,920,692,716]" italics="true" pageId="7" pageNumber="54">Atopogyne</emphasis>
</taxonomicName>
, 
<taxonomicName box="[944,1105,692,716]" class="Insecta" family="Formicidae" genus="Sphaerocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[944,1105,692,716]" italics="true" pageId="7" pageNumber="54">Sphaerocrema</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1129,1280,692,716]" class="Insecta" family="Formicidae" genus="Colobocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[1129,1280,692,716]" italics="true" pageId="7" pageNumber="54">Colobocrema</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1304,1429,693,716]" class="Insecta" family="Formicidae" genus="Paracrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[1304,1429,693,716]" italics="true" pageId="7" pageNumber="54">Paracrema</emphasis>
</taxonomicName>
, 
<taxonomicName box="[151,288,728,752]" class="Insecta" family="Formicidae" genus="Physocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[151,288,728,752]" italics="true" pageId="7" pageNumber="54">Physocrema</emphasis>
</taxonomicName>
, 
<taxonomicName box="[302,438,728,752]" class="Insecta" family="Formicidae" genus="Xiphocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[302,438,728,752]" italics="true" pageId="7" pageNumber="54">Xiphocrema</emphasis>
</taxonomicName>
