<paragraph blockId="9.[151,600,1873,1898]" box="[151,600,1873,1898]" pageId="9" pageNumber="56">
<heading bold="true" box="[151,600,1873,1898]" fontSize="10" level="2" pageId="9" pageNumber="56" reason="2">
<emphasis bold="true" box="[151,600,1873,1898]" pageId="9" pageNumber="56">
i. 
<emphasis bold="true" box="[199,529,1873,1898]" italics="true" pageId="9" pageNumber="56">
<taxonomicName box="[199,369,1873,1898]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="56" phylum="Arthropoda" rank="genus">Crematogaster</taxonomicName>
ranavalonae-
</emphasis>
group
</emphasis>
</heading>
</paragraph>
