<tr box="[151,1414,1509,1531]" gridrow="8" pageId="2" pageNumber="3">
<th box="[151,468,1509,1531]" gridcol="0" gridrow="8" pageId="2" pageNumber="3">Atlantis II cruise 60/259A&amp;D</th>
<td box="[505,736,1509,1531]" gridcol="1" gridrow="8" pageId="2" pageNumber="3">37°13’S–52°45’W</td>
<td box="[791,905,1509,1531]" gridcol="2" gridrow="8" pageId="2" pageNumber="3">3305–3317</td>
