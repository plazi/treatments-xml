<taxonomicNameLabel box="[583,645,1887,1911]" pageId="6" pageNumber="35" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName box="[660,808,1886,1911]" class="Insecta" family="Figitidae" genus="Dilyta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="longinqua" status="n. sp.">
