<taxonomicNameLabel box="[1326,1389,1923,1947]" pageId="6" pageNumber="35" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName box="[151,306,1958,1983]" class="Insecta" family="Figitidae" genus="Dilyta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="rathmanae">
<emphasis box="[151,306,1958,1983]" italics="true" pageId="6" pageNumber="35">D. rathmanae</emphasis>
</taxonomicName>
