<accessionNumber box="[389,534,1766,1790]" httpUri="http://www.uniprot.org/uniprot/MMF41648" pageId="8" pageNumber="37">
<collectionCode box="[389,454,1767,1790]" httpUri="http://grbio.org/cool/mme0-gx6r" name="Museu Municipal do Funchal" pageId="8" pageNumber="37">MMF</collectionCode>
<number box="[461,534,1766,1790]" pageId="8" pageNumber="37" value="41648.0">41648</number>
