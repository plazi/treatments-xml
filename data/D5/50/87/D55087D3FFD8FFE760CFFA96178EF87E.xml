); 
<accessionNumber box="[531,677,1838,1862]" httpUri="http://www.uniprot.org/uniprot/MMF41650" pageId="8" pageNumber="37">
<collectionCode box="[531,596,1839,1862]" httpUri="http://grbio.org/cool/mme0-gx6r" name="Museu Municipal do Funchal" pageId="8" pageNumber="37">MMF</collectionCode>
<number box="[604,677,1838,1862]" pageId="8" pageNumber="37" value="41650.0">41650</number>
