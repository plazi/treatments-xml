<number box="[1171,1291,376,402]" pageId="5" pageNumber="6" value="4.62" valueMax="5.33" valueMin="3.91">3.91–5.33</number>
, 
<number box="[1303,1354,376,402]" pageId="5" pageNumber="6" value="4.86">4.86</number>
(
<number box="[1371,1387,376,402]" pageId="5" pageNumber="6" value="6.0">6</number>
)
</paragraph>
