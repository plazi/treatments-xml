<taxonomicName authorityName="Komai and Asakura" authorityYear="1995" box="[627,839,469,492]" class="Malacostraca" family="Paguridae" genus="Pagurixus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="1110" phylum="Arthropoda" rank="species" species="nomurai">
