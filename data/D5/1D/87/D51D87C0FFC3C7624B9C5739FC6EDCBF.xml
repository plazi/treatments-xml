<taxonomicName box="[293,589,1928,1951]" class="Malacostraca" family="Pinnotheridae" genus="Calyptraeotheres" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="49" phylum="Arthropoda" rank="species" species="pepeluisi" status="new species">
<emphasis box="[293,589,1928,1951]" italics="true" pageId="8" pageNumber="49">
<taxonomicName box="[293,483,1928,1951]" class="Malacostraca" family="Pinnotheridae" genus="Calyptraeotheres" higherTaxonomySource="CoL" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="49" phylum="Arthropoda" rank="genus">Calyptraeotheres</taxonomicName>
pepeluisi
</emphasis>
</taxonomicName>
<emphasis bold="true" box="[598,734,1927,1951]" pageId="8" pageNumber="49">
<taxonomicNameLabel box="[598,734,1927,1951]" pageId="8" pageNumber="49" rank="species">new species</taxonomicNameLabel>
