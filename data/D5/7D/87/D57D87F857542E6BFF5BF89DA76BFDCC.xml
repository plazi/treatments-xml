</taxonomicName>
; 
<taxonomicName baseAuthorityName="Keyserling" baseAuthorityYear="1880" box="[151,265,532,558]" class="Arachnida" family="Sparassidae" genus="Sparianthina" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="422" phylum="Arthropoda" rank="species" species="pumilla">
<emphasis box="[151,265,532,558]" italics="true" pageId="21" pageNumber="422">S. pumilla</emphasis>
</taxonomicName>
