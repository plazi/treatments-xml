<emphasis box="[511,718,497,523]" italics="true" pageId="21" pageNumber="422">Sparianthina adisi</emphasis>
</taxonomicName>
; 
<taxonomicName authority="Rheims, 2021" authorityName="Rheims" authorityYear="2021" box="[734,844,497,524]" class="Arachnida" family="Sparassidae" genus="Sparianthina" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="422" phylum="Arthropoda" rank="species" species="boyaca" status="sp. nov.">
<emphasis box="[734,844,497,524]" italics="true" pageId="21" pageNumber="422">S. boyaca</emphasis>
</taxonomicName>
<emphasis bold="true" box="[851,941,497,523]" pageId="21" pageNumber="422">
<taxonomicNameLabel box="[851,941,497,523]" pageId="21" pageNumber="422" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; 
<taxonomicName authorityName="Jager, Rheims &amp; Labarque" authorityYear="2009" box="[956,1085,497,524]" class="Arachnida" family="Sparassidae" genus="Sparianthina" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="422" phylum="Arthropoda" rank="species" species="deltshevi">
<emphasis box="[956,1085,497,524]" italics="true" pageId="21" pageNumber="422">S. deltshevi</emphasis>
</taxonomicName>
; 
<taxonomicName authorityName="Rheims" authorityYear="2011" box="[1099,1186,498,524]" class="Arachnida" family="Sparassidae" genus="Sparianthina" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="422" phylum="Arthropoda" rank="species" species="gaita">
<emphasis box="[1099,1186,498,524]" italics="true" pageId="21" pageNumber="422">S. gaita</emphasis>
</taxonomicName>
; 
