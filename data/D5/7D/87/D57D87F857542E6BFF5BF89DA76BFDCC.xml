<emphasis box="[1201,1304,497,524]" italics="true" pageId="21" pageNumber="422">S. milleri</emphasis>
</taxonomicName>
; 
<taxonomicName authorityName="Rheims" authorityYear="2011" box="[1319,1429,498,524]" class="Arachnida" family="Sparassidae" genus="Sparianthina" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="422" phylum="Arthropoda" rank="species" species="parang">
