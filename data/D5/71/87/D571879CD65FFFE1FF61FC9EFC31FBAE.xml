species 
<taxonomicName authority="De Leon, 1959 a: 117" authorityName="De Leon" authorityPageNumber="117" authorityYear="1959" box="[287,1004,897,921]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="427" phylum="Arthropoda" rank="species" species="simplicissimus" subGenus="Typhlodromopsis">
