<subSubSection pageId="28" pageNumber="429" type="etymology">
<paragraph blockId="28.[151,1437,150,789]" pageId="28" pageNumber="429">
<emphasis bold="true" box="[199,334,475,501]" pageId="28" pageNumber="429">Etymology.</emphasis>
The specific name 
<taxonomicName box="[556,693,475,501]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromips" kingdom="Animalia" order="Mesostigmata" pageId="28" pageNumber="429" phylum="Arthropoda" rank="species" species="jhilmiliensis">
<emphasis box="[556,693,475,501]" italics="true" pageId="28" pageNumber="429">jhilmiliensis</emphasis>
</taxonomicName>
