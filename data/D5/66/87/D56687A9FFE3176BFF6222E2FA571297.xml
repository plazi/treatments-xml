<taxonomicName box="[438,595,979,1003]" class="Entognatha" family="Entomobryidae" genus="Homidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="528" phylum="Arthropoda" rank="species" species="obliquistria" status="sp. nov.">
<emphasis box="[438,595,979,1003]" italics="true" pageId="9" pageNumber="528">H. obliquistria</emphasis>
</taxonomicName>
<emphasis bold="true" box="[602,694,980,1003]" pageId="9" pageNumber="528">
<taxonomicNameLabel box="[602,689,980,1003]" pageId="9" pageNumber="528" rank="species">sp. nov.</taxonomicNameLabel>
,
</emphasis>
<emphasis box="[700,825,979,1003]" italics="true" pageId="9" pageNumber="528">
<taxonomicName box="[700,821,979,1003]" class="Entognatha" family="Entomobryidae" genus="Homidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="528" phylum="Arthropoda" rank="species" species="latifolia">
H. 
<taxonomicName box="[733,821,979,1003]" pageId="9" pageNumber="528" rank="species" species="latifolia">latifolia</taxonomicName>
</taxonomicName>
.
</emphasis>
and 
<taxonomicName box="[876,1000,979,1003]" class="Entognatha" family="Entomobryidae" genus="Homidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="528" phylum="Arthropoda" rank="species" species="polyseta">
H. 
<taxonomicName box="[908,1000,979,1003]" pageId="9" pageNumber="528" rank="species" species="polyseta">polyseta</taxonomicName>
