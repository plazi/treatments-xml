<collectingCountry id="F30F7651FFE14C18FAE9DBACFAD7FA56" box="[1283,1350,1425,1448]" name="Japan" pageId="5" pageNumber="492">Japan</collectingCountry>
, 
<collectingCountry id="F30F7651FFE14C18FABBDBACFA09FA56" box="[1361,1432,1425,1448]" name="South Korea" pageId="5" pageNumber="492">Korea</collectingCountry>
