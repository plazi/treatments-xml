<taxonomicName id="7CF44D425E051F1FFC7158C723C4ACCF" box="[1015,1170,1759,1784]" class="Arachnida" family="Salticidae" genus="Tabuina" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="rufa">
