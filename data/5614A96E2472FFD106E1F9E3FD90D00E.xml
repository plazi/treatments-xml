<number id="68750A5F2473FFDC07BBFF4CFE18D3E6" box="[461,505,152,178]" pageId="5" pageNumber="74" value="373.0">373</number>
± 
<number id="68750A5F2473FFDC046DFF4CFDBCD3E6" box="[539,605,152,178]" pageId="5" pageNumber="74" value="70.29">70.29</number>
