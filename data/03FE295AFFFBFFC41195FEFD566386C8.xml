<paragraph id="8BE8984CFFFBFFC410BCFE66502485D0" blockId="43.[213,580,422,446]" box="[213,580,422,446]" pageId="43" pageNumber="727">
<collectingRegion id="499356AEFFFBFFC410BCFE66535A85D0" box="[213,314,422,446]" country="Turkey" name="Erzincan" pageId="43" pageNumber="727">Erzincan</collectingRegion>
