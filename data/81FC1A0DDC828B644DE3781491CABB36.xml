<taxonomicName id="9E9767B1C87FE56DB7BF65C23AF392C6" authority="Scherer, 1962" authorityName="Scherer" authorityYear="1962" class="Insecta" family="Chrysomelidae" genus="Ulrica" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ulrica" order="Coleoptera" pageId="15" pageNumber="76" phylum="Arthropoda" rank="genus">Ulrica</taxonomicName>
</td>
</tr>
