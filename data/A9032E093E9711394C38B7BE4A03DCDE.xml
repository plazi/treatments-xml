<subSubSection id="DA7F27C004CE584FF9416C006DEC9CEE" pageNumber="478" type="discussion">
<paragraph id="312F48EC905E7E1E2C85E1C3D3E8C51D" pageNumber="478">
<emphasis id="8779E83C00BCA1AC406CF7B80E687609" italics="true" pageNumber="478">
Note: 
<taxonomicName id="CE31F38346E86F587C36B2C0137548C0" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Ranunculaceae" genus="Delphinium" kingdom="Plantae" order="Ranunculales" phylum="Tracheophyta" rank="species" species="peregrinum">Delphinium peregrinum</taxonomicName>
