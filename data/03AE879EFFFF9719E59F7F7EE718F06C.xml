<taxonomicName id="4C074D0BFFFF9719E4D17A58E777F264" box="[473,589,1089,1114]" class="Malacostraca" family="Palaemonidae" genus="Cuapetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="40" phylum="Arthropoda" rank="genus">
