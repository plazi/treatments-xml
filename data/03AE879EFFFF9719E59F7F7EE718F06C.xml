and 
<taxonomicName id="4C074D0BFFFF9719E71B7B6EE6FAF3AE" authority="Borradaile, 1915" authorityName="Borradaile" authorityYear="1915" box="[531,960,1398,1424]" class="Malacostraca" family="Palaemonidae" genus="Cuapetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="suvadiensis">
<emphasis id="B973EA9AFFFF9719E71B7B6EE7FFF3AE" box="[531,709,1399,1424]" italics="true" pageId="1" pageNumber="40">C. suvadiensis</emphasis>
(Borradaile, 
<number id="3DCF24AFFFFF9719E6627B6FE691F3AE" box="[874,939,1398,1424]" pageId="1" pageNumber="40" value="1915.0">1915</number>
))
</taxonomicName>
. Coral-associated 
<taxonomicName id="4C074D0BFFFF9719E1AB7B6EE401F389" authority="Bruce, 1977" authorityName="Bruce" authorityYear="1977" class="Malacostraca" family="Palaemonidae" genus="Cuapetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="kororensis">
