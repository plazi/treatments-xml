<taxonomicName id="4C074D0BFFFF9719E1657B49E0ADF354" authority="Kemp, 1915" authorityName="Kemp" authorityYear="1915" box="[1133,1431,1360,1386]" class="Malacostraca" family="Palaemonidae" genus="Cuapetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="demani">
<emphasis id="B973EA9AFFFF9719E1657B49E1DDF357" box="[1133,1255,1360,1385]" italics="true" pageId="1" pageNumber="40">C. demani</emphasis>
(Kemp, 
<number id="3DCF24AFFFFF9719E0587B49E0B4F354" box="[1360,1422,1360,1386]" pageId="1" pageNumber="40" value="1915.0">1915</number>
)
</taxonomicName>
, 
<taxonomicName id="4C074D0BFFFF9719E59F7B6EE4ECF3AE" authority="Kemp, 1922" authorityName="Kemp" authorityYear="1922" box="[151,470,1398,1424]" class="Malacostraca" family="Palaemonidae" genus="Cuapetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="digitalis">
<emphasis id="B973EA9AFFFF9719E59F7B6EE41BF3AE" box="[151,289,1399,1424]" italics="true" pageId="1" pageNumber="40">C. digitalis</emphasis>
(Kemp, 
<number id="3DCF24AFFFFF9719E4867B6FE4F7F3AE" box="[398,461,1398,1424]" pageId="1" pageNumber="40" value="1922.0">1922</number>
)
