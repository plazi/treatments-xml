<number id="3D8524E0FF9FFFCF2843FE45FC52FE5E" box="[836,939,479,503]" pageId="4" pageNumber="275" value="1013172.0">1013172</number>
], and 
<specimenCount id="9D4BFD4EFF9FFFCF28F4FE45FBD8FE5E" box="[1011,1057,479,503]" pageId="4" pageNumber="275" type="generic">
