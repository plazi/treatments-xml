<taxonomicName id="4BD808B0402EFFCBFEB2ABF7FDF796B9" box="[264,526,1414,1439]" class="Magnoliopsida" family="Fabaceae" genus="Brachystegia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="65" pageNumber="66" phylum="Magnoliophyta" rank="species" species="boehmii">
