<taxonomicName id="4BD808B0402FFFCAFEB2AF6DFC5E9213" authority="Moraes, Ueckermann &amp; Oliveira" authorityName="Moraes, Ueckermann &amp; Oliveira" box="[264,935,283,309]" class="Arachnida" family="Phytoseiidae" genus="Euseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="zambiaensis" status="n. sp.">
