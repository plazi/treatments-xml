<taxonomicName id="627C3813F108095212C4A2EF43648B50" authority="Sharp, 1903" authorityName="Sharp" authorityYear="1903" class="Insecta" family="Carabidae" genus="Metrothorax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Metrothorax" order="Coleoptera" pageId="185" pageNumber="186" phylum="Arthropoda" rank="genus">Metrothorax</taxonomicName>
