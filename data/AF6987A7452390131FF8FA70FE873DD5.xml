, 
<collectingCountry id="5FD77621452290111EAAFF29FDF23FD5" box="[453,578,223,247]" name="Costa Rica" pageId="9" pageNumber="10">Costa Rica</collectingCountry>
, 
<collectingCountry id="5FD77621452290111D22FF29FD1C3FD5" box="[589,684,223,247]" name="Ecuador" pageId="9" pageNumber="10">Ecuador</collectingCountry>
