<number id="91082496452090131FF8FF15FF053FD9" box="[151,181,227,251]" pageId="11" pageNumber="12" value="30.0">30</number>
.iv 
<emphasis id="15B4EAA3452090131FA0FF12FF6D3FDE" bold="true" box="[207,221,228,252]" pageId="11" pageNumber="12">–</emphasis>
<number id="91082496452090131FB2FF15FF5A3FD9" box="[221,234,227,251]" pageId="11" pageNumber="12" value="8.0">8</number>
.v. 
<number id="91082496452090131E6FFF15FEF03FD9" box="[256,320,227,251]" pageId="11" pageNumber="12" value="2002.0">2002</number>
