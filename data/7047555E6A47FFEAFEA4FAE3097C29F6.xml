<paragraph id="F851E4486A47FFEAFFC0F9E4097C29F6" blockId="9.[111,647,1547,1699]" pageId="9" pageNumber="918">
<taxonomicName id="3FEE9FCB6A47FFEAFFC0F9E408F72911" authorityName="Alexander" authorityYear="1929" box="[113,254,1578,1604]" class="Ostracoda" family="Cytherellidae" genus="Cytherelloidea" kingdom="Animalia" order="Platycopida" pageId="9" pageNumber="918" phylum="Arthropoda" rank="genus">
