<taxonomicName id="1CF05EC397CBFF1DFEC4FDA4FD60BD10" authority="Serville, 1838" authorityName="Serville" authorityYear="1838" box="[294,670,553,575]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="156" pageNumber="157" phylum="Arthropoda" rank="species" species="translucens">
