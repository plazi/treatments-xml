<specimenCode id="DBA89ECFF21CF17AEB14149CFD6C5E50" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="40" pageNumber="40">AMNH 659274</specimenCode>
