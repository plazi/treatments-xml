<taxonomicName id="4C7DC6A0FFE5FFFE9BD7D2BB0656A8D2" box="[922,1079,480,505]" class="Insecta" family="Scelionidae" genus="Cyphacolus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="veniprivus">
