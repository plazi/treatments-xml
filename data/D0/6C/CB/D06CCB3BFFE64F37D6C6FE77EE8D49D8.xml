<taxonomicName authorityName="Stal" authorityYear="1866" box="[641,719,1661,1684]" class="Insecta" family="Issidae" genus="Tetrica" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="360" phylum="Arthropoda" rank="genus">
