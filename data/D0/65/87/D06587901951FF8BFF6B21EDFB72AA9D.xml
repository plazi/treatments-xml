species:
</emphasis>
<taxonomicName authority="Koch, 1836" authorityName="Koch" authorityYear="1836" box="[299,624,807,829]" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" kingdom="Animalia" order="Prostigmata" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="cruentus">
