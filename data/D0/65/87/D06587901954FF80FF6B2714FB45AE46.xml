<number box="[1344,1403,151,175]" pageId="41" pageNumber="42" value="1987.0">1987</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Liang &amp; Hu, 1987" authorityName="Liang &amp; Hu" authorityYear="1987" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" kingdom="Animalia" order="Prostigmata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="macroposbus">
