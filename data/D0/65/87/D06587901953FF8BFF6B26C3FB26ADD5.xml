<emphasis box="[986,1022,1625,1648]" italics="true" pageId="32" pageNumber="33">sce</emphasis>
, 
<emphasis box="[1037,1049,1625,1648]" italics="true" pageId="32" pageNumber="33">c</emphasis>
<number box="[1049,1057,1639,1653]" pageId="32" pageNumber="33" value="2.0">2</number>
