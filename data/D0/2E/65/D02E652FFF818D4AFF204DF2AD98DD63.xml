<location LSID="urn:lsid:plazi:treatment:D02E652FFF818D4AFF204DF2AD98DD63:5D5882E2FF878D4AFF594C3FA932DD60" box="[163,264,1864,1886]" country="Ecuador" county="Sebundoi'" municipality="Sibundoy Valley" name="Santiago" pageId="20" stateProvince="Putumayo">Santiago</location>
