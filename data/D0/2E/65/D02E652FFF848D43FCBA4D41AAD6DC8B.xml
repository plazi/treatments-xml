<location LSID="urn:lsid:plazi:treatment:D02E652FFF848D43FCBA4D41AAD6DC8B:5D5882E2FF8B8D46FDDD4DB5AA56DCE6" box="[551,620,1730,1752]" county="Although" latitude="19.353739" longLatPrecision="1" longitude="-98.67071" municipality="Rio Frio" name="Given" pageId="24" stateProvince="Puebla">Given</location>
