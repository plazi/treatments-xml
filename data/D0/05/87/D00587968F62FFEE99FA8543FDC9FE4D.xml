<subSubSection pageId="2" pageNumber="577" type="etymology">
<paragraph blockId="2.[151,1437,152,503]" pageId="2" pageNumber="577">
<emphasis bold="true" box="[199,334,441,467]" pageId="2" pageNumber="577">Etymology.</emphasis>
