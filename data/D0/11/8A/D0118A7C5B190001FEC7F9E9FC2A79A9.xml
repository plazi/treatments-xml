<paragraph blockId="21.[151,756,2014,2070]" lastBlockId="21.[832,1436,257,313]" pageId="21" pageNumber="257">
<emphasis bold="true" box="[151,270,2014,2038]" pageId="21" pageNumber="257">Diagnosis:</emphasis>
<taxonomicName authorityName="Maas" authorityYear="1905" box="[279,424,2014,2038]" class="Hydrozoa" family="Bythotiaridae" kingdom="Animalia" order="Anthoathecata" pageId="21" pageNumber="257" phylum="Cnidaria" rank="family">Bythotiaridae</taxonomicName>
