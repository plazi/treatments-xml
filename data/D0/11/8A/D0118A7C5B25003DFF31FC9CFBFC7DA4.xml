<emphasis box="[832,1063,1388,1412]" italics="true" pageId="41" pageNumber="277">
<taxonomicName baseAuthorityName="Brooks" baseAuthorityYear="1883" box="[832,1059,1388,1412]" class="Hydrozoa" family="Corymorphidae" genus="Corymorpha" kingdom="Animalia" order="Anthoathecata" pageId="41" pageNumber="277" phylum="Cnidaria" rank="species" species="gracilis">Corymorpha gracilis</taxonomicName>
