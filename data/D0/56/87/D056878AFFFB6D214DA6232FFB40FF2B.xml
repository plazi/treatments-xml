. 
<emphasis box="[643,746,189,212]" italics="true" pageId="26" pageNumber="127">projectus</emphasis>
</taxonomicName>
and 
<taxonomicName authority="Distant" authorityName="Distant" box="[801,1038,188,213]" class="Insecta" family="Cicadellidae" genus="Viridomarus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="capitatus">
<emphasis box="[801,815,189,212]" italics="true" pageId="26" pageNumber="127">S</emphasis>
