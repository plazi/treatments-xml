<paragraph blockId="234.[151,1436,1425,1522]" pageId="234" pageNumber="235">
<emphasis bold="true" box="[151,289,1425,1450]" pageId="234" pageNumber="235">
<taxonomicName box="[151,284,1425,1450]" class="Insecta" family="Corduliidae" kingdom="Animalia" order="Odonata" pageId="234" pageNumber="235" phylum="Arthropoda" rank="family">Corduliidae</taxonomicName>
