<collectingCountry box="[693,796,474,500]" name="Trinidad and Tobago" pageId="18" pageNumber="19">Trinidad</collectingCountry>
, 
<date box="[807,965,474,500]" pageId="18" pageNumber="19" value="1918-06-16">
<number box="[807,836,474,500]" pageId="18" pageNumber="19" value="16.0">16</number>
