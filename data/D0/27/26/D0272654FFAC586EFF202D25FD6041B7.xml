[
<collectingRegion box="[481,580,1741,1765]" country="Indonesia" name="Sumatera Utara" pageId="45" pageNumber="46">Sumatra</collectingRegion>
] (
<figureCitation box="[602,687,1741,1765]" captionStart="FIGURE 99" captionStartId="161.[151,250,1132,1155]" captionTargetBox="[154,1433,193,1111]" captionTargetId="figure-40@161.[154,1434,193,1111]" captionTargetPageId="161" captionText="FIGURE 99. Distribution of Psechrus in western Malaysia and Sumatra. Symbols: species belonging to the singaporensis- group. Arrow: Distribution of P. libelti (argentatus-group)." figureDoi="http://doi.org/10.5281/zenodo.6040041" httpUri="https://zenodo.org/record/6040041/files/figure.png" pageId="45" pageNumber="46">Fig. 99</figureCitation>
