<number box="[1009,1081,1756,1781]" pageId="8" pageNumber="243" value="26.5" valueMax="27.0" valueMin="26.0">26–27</number>
)&gt; 
<emphasis box="[1112,1151,1756,1780]" italics="true" pageId="8" pageNumber="243">
