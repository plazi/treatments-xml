<geoCoordinate degrees="46" direction="north" minutes="22" orientation="latitude" precision="925" value="46.366665">46°22'N</geoCoordinate>
, 
<geoCoordinate degrees="102" direction="east" minutes="49" orientation="longitude" precision="925" value="102.816666">102°49'E</geoCoordinate>
