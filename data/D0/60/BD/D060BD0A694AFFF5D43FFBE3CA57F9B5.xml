<taxonomicName authority="Brg." authorityName="Brg." box="[895,1223,1233,1259]" class="Insecta" family="Lycidae" genus="Pyropterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="himalejicus">
