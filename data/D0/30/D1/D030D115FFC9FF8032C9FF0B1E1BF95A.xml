<number box="[176,222,885,911]" pageId="10" pageNumber="30" value="181.0">181</number>
, dL and %L (in parentheses): P- 
<number box="[603,613,885,911]" pageId="10" pageNumber="30" value="1.0">1</number>
<number box="[621,650,885,911]" pageId="10" pageNumber="30" value="28.0">28</number>
(
<number box="[666,721,885,911]" pageId="10" pageNumber="30" value="15.5">15.5</number>
), P- 
<number box="[769,783,885,911]" pageId="10" pageNumber="30" value="2.0">2</number>
