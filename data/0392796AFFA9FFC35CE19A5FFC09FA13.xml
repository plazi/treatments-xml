species: 
<taxonomicName id="4C3BB3FFFFA9FFC25D509AF4FD72FA40" authority="Viette, 1968 a: 88" authorityName="Viette" authorityPageNumber="88" authorityYear="1968" box="[294,722,1438,1462]" class="Insecta" family="Lecithoceridae" genus="Lecithocera" kingdom="Animalia" order="Lepidoptera" pageId="30" pageNumber="181" phylum="Arthropoda" rank="species" species="andrianella">
