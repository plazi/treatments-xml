<collectingRegion id="564B40A2AA55FFDFA1023599FB48FD10" box="[1087,1173,736,761]" country="China" name="Beijing" pageId="29" pageNumber="30">Beijing</collectingRegion>
, 
<collectingRegion id="564B40A2AA55FFDFA19F3599FB24FD10" box="[1186,1273,736,761]" country="China" name="Jiangsu" pageId="29" pageNumber="30">Jiangsu</collectingRegion>
