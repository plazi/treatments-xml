<taxonomicName id="EDAC463E916516FE5CF9FB339BF6D209" authority="Tang, Grootaert &amp; Yang, 2018" authorityName="Tang, Grootaert &amp; Yang" authorityYear="2018" class="Insecta" family="Dolichopodidae" genus="Protomedetera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Protomedetera singaporensis" order="Diptera" pageId="0" pageNumber="137" phylum="Arthropoda" rank="species" species="singaporensis">Protomedetera singaporensis</taxonomicName>
