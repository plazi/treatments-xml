<taxonomicName id="4C6D4D5EEFF33A97FBA0FB02FAF1FB27" authority="Illg, 1951" authorityName="Illg" authorityYear="1951" box="[1125,1332,1227,1254]" class="Maxillopoda" family="Notodelphyidae" genus="Ustina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="162" pageNumber="163" phylum="Arthropoda" rank="genus">
