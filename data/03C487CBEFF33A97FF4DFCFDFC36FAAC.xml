material of 
<taxonomicName id="4C6D4D5EEFF33A97FBBCFB20FAB8FAC9" authority="Illg, 1951" authorityName="Illg" authorityYear="1951" box="[1145,1405,1261,1288]" class="Maxillopoda" family="Notodelphyidae" genus="Ustina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="162" pageNumber="163" phylum="Arthropoda" rank="species" species="clarki">
