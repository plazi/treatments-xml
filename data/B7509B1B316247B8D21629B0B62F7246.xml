<taxonomicName id="A72C5D46A5F974489AC01317608C4C0E" genus="T." lsidName="T. rufoalba" pageId="0" pageNumber="1" rank="species" species="rufoalba">T. rufoalba</taxonomicName>
, 440-490 in 
<taxonomicName id="FA4F9DDD1CF536BF2F2EA641EE046AB5" genus="T." lsidName="T. dunni" pageId="0" pageNumber="1" rank="species" species="dunni">T. dunni</taxonomicName>
; ♂ = 320 in 
<taxonomicName id="3F81EB88D7621A5809C10BFD7C6A6885" genus="T." lsidName="T. rufoalba" pageId="0" pageNumber="1" rank="species" species="rufoalba">T. rufoalba</taxonomicName>
