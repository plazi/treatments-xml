<taxonomicName id="3DC3D2E6FFE4FFE6FEE06686FD8BFB18" authority="Flechtmann" authorityName="Flechtmann" box="[264,575,1171,1198]" class="Arachnida" family="Eriophyidae" genus="Diptilostatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis id="C8B77577FFE4FFE6FEE06686FE15FB1B" bold="true" box="[264,417,1172,1198]" italics="true" pageId="6" pageNumber="7">Diptilostatus</emphasis>
