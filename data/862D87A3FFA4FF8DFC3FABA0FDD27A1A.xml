<taxonomicName id="C9844D36FFA6FF8DFE77AB5AFD7B7A7A" box="[499,738,1548,1572]" class="Insecta" family="Hydropsychidae" genus="Macrostemum" kingdom="Animalia" order="Trichoptera" pageId="19" pageNumber="683" phylum="Arthropoda" rank="species" species="undetermined">
