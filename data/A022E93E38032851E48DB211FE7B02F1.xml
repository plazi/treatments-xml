<taxonomicName id="EF8B23AB38062851E7DBB5E2FB9D01B1" box="[961,1104,398,423]" class="Agaricomycetes" family="Mycenaceae" genus="Dictyoploca" higherTaxonomySource="CoL" kingdom="Animalia" order="Agaricales" pageId="22" pageNumber="53" phylum="Basidiomycota" rank="genus">
