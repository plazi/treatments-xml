<quantity id="4C9AFC884F0EFFDFFF43FC9EFF4AFCAD" box="[160,245,798,822]" metricMagnitude="2" metricUnit="m" metricValue="1.43891" pageId="8" pageNumber="904" unit="in" value="5665.0">5665 in</quantity>
