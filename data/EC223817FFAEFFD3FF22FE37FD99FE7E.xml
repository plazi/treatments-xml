<taxonomicName id="A38BF282FFAEFFD2FC23F92DFB78F904" box="[918,1249,1668,1693]" class="Magnoliopsida" family="Apocynaceae" genus="Tabernaemontana" kingdom="Plantae" order="Gentianales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="stapfiana">
