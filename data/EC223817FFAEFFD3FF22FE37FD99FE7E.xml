(
<taxonomicName id="A38BF282FFAEFFD2FB47F92DFA0DF907" box="[1266,1428,1668,1694]" class="Magnoliopsida" family="Apocynaceae" kingdom="Plantae" order="Gentianales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="family">Apocynaceae</taxonomicName>
