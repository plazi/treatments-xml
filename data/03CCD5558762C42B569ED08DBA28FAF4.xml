<taxonomicName id="4C651FC08761C42A569ED71DBFF5F8B3" box="[151,216,1799,1821]" class="Insecta" family="Thripidae" genus="Mycterothrips" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="197" phylum="Arthropoda" rank="species" species="doostii">
