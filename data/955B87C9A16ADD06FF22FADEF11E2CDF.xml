<quantity id="DA0A9B3AA154DD06FD50FCE6F11A2CDF" box="[741,857,871,896]" metricMagnitude="1" metricUnit="m" metricValue="9.75" metricValueMax="14.0" metricValueMin="5.5" pageId="33" pageNumber="34" unit="m" value="97.5" valueMax="140.0" valueMin="55.0">
