<taxonomicName id="47D9B1D2FFC23975FE4AE830FD3EDEBB" authority="WASMANN, 1896" authorityName="WASMANN" authorityYear="1896" box="[394,753,1641,1665]" class="Insecta" family="Staphylinidae" genus="Rhynchodonia" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="104" phylum="Arthropoda" rank="genus">
