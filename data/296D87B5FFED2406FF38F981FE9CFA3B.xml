<taxonomicName id="66C44D20FFEB2406FEE0FA62FE7BFA00" authorityName="Bernhauer" authorityYear="1934" box="[292,406,1454,1475]" class="Insecta" family="Staphylinidae" genus="Dysanellus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="282" phylum="Arthropoda" rank="species" species="excellens">
