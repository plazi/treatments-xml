<taxonomicName id="66C44D20FFEB2406FEC6FA04FE30FA1E" authority="Bernhauer" authorityName="Bernhauer" authorityYear="1906" box="[258,477,1480,1501]" class="Insecta" family="Staphylinidae" genus="Xenopygus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="282" phylum="Arthropoda" rank="genus">
