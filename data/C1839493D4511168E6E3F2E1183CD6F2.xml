<bibRefCitation id="5CC79FC45408B1A42389F8B5A6457820" author="Vacelet, J" journalOrPublisher="Travaux Scientifique Parc National de Port-Cros" pageId="48" pageNumber="49" pagination="167 - 186" title="Inventaire des Spongiaires du Parc National de Port-Cros (Var)." volume="2" year="1976">1976</bibRefCitation>
; 
<bibRefCitation id="237D9A8B7F7463A15848C858FD00778D" author="Sara, M" journalOrPublisher="Annuario dell'Istituto e Museo di Zoologia dell'Universita di Napoli" pageId="48" pageNumber="49" pagination="1 - 32" title="Zonazione dei Poriferi nella Grotta &quot; della Gaiola &quot;." volume="13" year="1961 a">
<normalizedToken id="FF32C031E32F4567F9D10125886C20A8" originalValue="Sarà">Sara</normalizedToken>
1961a
</bibRefCitation>
b, 
<bibRefCitation id="8C5927160A251E53C45390840F12139F" author="Sara, M" journalOrPublisher="Archivio Zoologico Italiano" pageId="48" pageNumber="49" pagination="181 - 248" title="Distribuzione ed ecologia dei Poriferi in acque superficiali della Riviera ligure di Levante." volume="49" year="1964 a">1964a</bibRefCitation>
; 
<bibRefCitation id="C16D5D7C344386050C1F831A3604E727" author="Boury-Esnault, N" journalOrPublisher="Vie Milieu" pageId="48" pageNumber="49" pagination="287 - 349" title="Spongiaires de la zone rocheuse littorale de Banyuls-sur-Mer. II. Systematique." volume="22" year="1971">Boury-Esnault 1971</bibRefCitation>
