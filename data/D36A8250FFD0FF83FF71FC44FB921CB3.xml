<subSubSection id="13D960CDFFD0FF82FF44FB24FC7E1F30" box="[185,959,1258,1284]" pageId="2" pageNumber="79" type="etymology">
<paragraph id="5B7C3346FFD0FF82FF44FB24FC7E1F30" blockId="2.[140,1108,1046,1672]" box="[185,959,1258,1284]" pageId="2" pageNumber="79">
