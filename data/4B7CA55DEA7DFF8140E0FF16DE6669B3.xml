<emphasis id="F1A1C859EA7AFF8140E0FB63DD2D6BCF" bold="true" box="[151,275,1250,1274]" pageId="4" pageNumber="121">FIGURE 4.</emphasis>
<taxonomicName id="04D56FC8EA7AFF81416DFB62DD806BCF" box="[282,446,1251,1274]" class="Insecta" family="Tettigoniidae" genus="Paraxantia" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="121" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="F1A1C859EA7AFF81416DFB62DDB26BCF" box="[282,396,1251,1274]" italics="true" pageId="4" pageNumber="121">Paraxantia</emphasis>
spp.
</taxonomicName>
, 
<emphasis id="F1A1C859EA7AFF8141BFFB63DE3D6BCF" bold="true" box="[456,515,1250,1274]" pageId="4" pageNumber="121">A, C:</emphasis>
<taxonomicName id="04D56FC8EA7AFF81427EFB62DE466BCF" authority="Wu &amp; Liu, 2021" authorityName="Wu &amp; Liu" authorityYear="2021" box="[521,632,1250,1274]" class="Insecta" family="Tettigoniidae" genus="Paraxantia" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="121" phylum="Arthropoda" rank="species" species="rubripes" status="sp. nov.">
<emphasis id="F1A1C859EA7AFF81427EFB62DE466BCF" box="[521,632,1250,1274]" italics="true" pageId="4" pageNumber="121">P. rubripes</emphasis>
</taxonomicName>
