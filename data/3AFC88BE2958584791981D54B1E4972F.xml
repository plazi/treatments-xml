<paragraph id="67F6B22C53C6235E75E4C25764E4D14C" pageId="0" pageNumber="89">
<taxonomicName id="7150B9FA079125F9BF20A321D56A1CB1" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" kingdom="Plantae" lsidName="Conostegia xalapensis" order="Myrtales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="species" species="xalapensis">
<emphasis id="AA4F91219A238C611CD9CDF0B32E0108" italics="true" pageId="0" pageNumber="89">Conostegia xalapensis</emphasis>
