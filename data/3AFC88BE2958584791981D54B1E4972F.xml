<taxonomicName id="8FFA7638FEE642AED375C652FA5DAE4F" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" kingdom="Plantae" lsidName="" order="Myrtales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="family">Melastomataceae</taxonomicName>
