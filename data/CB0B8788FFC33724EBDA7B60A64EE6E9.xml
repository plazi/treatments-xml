</subSubSection>
<subSubSection id="0BB86515FFC43724EB8A7EE1A666E692" box="[199,1088,151,176]" pageId="12" pageNumber="71" type="materials_examined">
<paragraph id="431D369EFFC43724EB8A7EE1A666E692" blockId="12.[199,1128,151,212]" box="[199,1088,151,176]" pageId="12" pageNumber="71">
<emphasis id="71D6EA8CFFC43724EB8A7EE1A310E68D" bold="true" box="[199,310,151,176]" pageId="12" pageNumber="71">Material.</emphasis>
<typeStatus id="9C19883CFFC43724EA707EE1A383E692" box="[317,421,151,175]" pageId="12" pageNumber="71">Holotype</typeStatus>
male, 
<collectingCountry id="3BB5760EFFC43724EABD7EE1A06EE692" box="[496,584,151,175]" name="Finland" pageId="12" pageNumber="71">Finland</collectingCountry>
, 
<collectingCountry id="3BB5760EFFC43724E9197EE1A0BEE692" box="[596,664,151,175]" name="Aland Islands" pageId="12" pageNumber="71">Åland</collectingCountry>
archipelago, Kökar, Hellén, (
<collectionCode id="25B3AE5BFFC43724E8A97EE1A61FE692" LSID="urn:lsid:biocol.org:col:34795" box="[996,1081,151,175]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="12" pageNumber="71">FMNH</collectionCode>
