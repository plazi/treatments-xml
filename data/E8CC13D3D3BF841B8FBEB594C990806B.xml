<taxonomicName id="B50762125328F051F25BEE6C16487016" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141200" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica graminicola Latreille" order="Hymenoptera" pageNumber="67" phylum="Arthropoda" rank="species" species="graminicola">Formica graminicola Latreille</taxonomicName>
