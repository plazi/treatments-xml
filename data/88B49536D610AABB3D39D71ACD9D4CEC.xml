<taxonomicName id="2FE390F072D2EEE06327C27B57A46B7C" class="Insecta" family="Aphididae" genus="Ericaphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ericaphis voegtlini" order="Hemiptera" pageId="1" pageNumber="134" phylum="Arthropoda" rank="species" species="voegtlini">Ericaphis voegtlini</taxonomicName>
