Male (n = 
<number id="3DCDCE49C81B215948D8EA063760FC3D" box="[509,526,872,896]" pageId="26" pageNumber="477" value="4.0">4</number>
): TL 
<number id="3DCDCE49C81B21594B6BEA0637D3FC3D" box="[590,701,872,896]" pageId="26" pageNumber="477" value="1.975" valueMax="2.02" valueMin="1.93">1.93–2.02</number>
(
<number id="3DCDCE49C81B21594BEBEA09366FFC3D" box="[718,769,871,896]" pageId="26" pageNumber="477" value="1.96">1.96</number>
) mm; PW 
<number id="3DCDCE49C81B21594A5BEA063680FC3D" box="[894,1006,872,896]" pageId="26" pageNumber="477" value="0.8049999999999999" valueMax="0.83" valueMin="0.78">0.78–0.83</number>
(
<number id="3DCDCE49C81B21594ADBEA06315FFC3D" box="[1022,1073,872,896]" pageId="26" pageNumber="477" value="0.81">0.81</number>
) mm; PL 
<number id="3DCDCE49C81B21594D80EA09307BFC3D" box="[1189,1301,871,896]" pageId="26" pageNumber="477" value="0.33499999999999996" valueMax="0.35" valueMin="0.32">0.32–0.35</number>
(
<number id="3DCDCE49C81B21594C00EA063037FC3D" box="[1317,1369,872,896]" pageId="26" pageNumber="477" value="0.34">0.34</number>
) mm; EW 
<number id="3DCDCE49C81B215949ECEAE23457FC19" box="[201,313,908,932]" pageId="26" pageNumber="477" value="1.2349999999999999" valueMax="1.27" valueMin="1.2">1.20–1.27</number>
(
<number id="3DCDCE49C81B2159486DEAE23415FC19" box="[328,379,908,932]" pageId="26" pageNumber="477" value="1.24">1.24</number>
) mm; EL 
<number id="3DCDCE49C81B215948CBEAE23731FC18" box="[494,607,908,933]" pageId="26" pageNumber="477" value="1.6400000000000001" valueMax="1.7" valueMin="1.58">1.58–1.70</number>
(
<number id="3DCDCE49C81B21594B4BEAE237CFFC18" box="[622,673,908,933]" pageId="26" pageNumber="477" value="1.62">1.62</number>
