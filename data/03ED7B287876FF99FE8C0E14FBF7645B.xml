<taxonomicName id="4C44B1BD7875FF98FDFD0DF2FCD86436" box="[633,772,1770,1793]" class="Insecta" family="Bombyliidae" genus="Mythicomyia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
