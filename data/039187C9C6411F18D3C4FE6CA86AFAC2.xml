<taxonomicName id="4C384D5CC6471F18D24AFC7DADA8FC14" box="[281,443,953,974]" class="Branchiopoda" family="Triopsidae" genus="Triops" kingdom="Animalia" order="Notostraca" pageId="12" pageNumber="340" phylum="Arthropoda" rank="species" species="maximus" status="sp. nov.">
<emphasis id="B94CEACDC6471F18D24AFC7DADA8FC14" box="[281,443,953,974]" italics="true" pageId="12" pageNumber="340">Triops maximus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27F57B6C6471F18D292FC7CAE01FC17" box="[449,530,952,973]" pageId="12" pageNumber="340" rank="species">
