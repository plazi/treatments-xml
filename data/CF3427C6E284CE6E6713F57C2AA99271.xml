<taxonomicName id="BC9942F3EAB30B163CDB249A4CBFDB91" class="Magnoliopsida" family="Campanulaceae" genus="Campanula" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Campanula macrostachya" order="Asterales" pageId="42" pageNumber="65" phylum="Tracheophyta" rank="species" species="macrostachya">Campanula macrostachya</taxonomicName>
