<taxonomicName id="227A52CC9587D1CEC7A73618DC1800EC" authorityName="Handmann" authorityYear="1882" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis haueri" order="Pteropoda" pageId="127" pageNumber="128" phylum="Mollusca" rank="species" species="haueri">
