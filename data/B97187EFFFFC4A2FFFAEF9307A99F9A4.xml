<taxonomicName id="F6D84D7AFFFC4A2FFFAEF9307F7AF8DE" baseAuthorityName="Russell" baseAuthorityYear="1970" box="[102,281,1779,1804]" class="Reptilia" family="Tyrannosauridae" genus="Daspletosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="30" pageNumber="221" phylum="Chordata" rank="genus">
<emphasis id="03ACEAEBFFFC4A2FFFAEF9307F7AF8DE" bold="true" box="[102,281,1779,1804]" italics="true" pageId="30" pageNumber="221">Daspletosaurus</emphasis>
