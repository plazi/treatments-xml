<taxonomicName id="6DFD27A9BF5BFFE75F1EF9FA31884D5B" box="[1121,1254,1565,1591]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
), and sweet­fern (
<taxonomicName id="6DFD27A9BF5BFFE75ADFF9A237FC4D32" box="[416,658,1605,1630]" class="Magnoliopsida" family="Myricaceae" genus="Myrica" kingdom="Plantae" order="Fagales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="species" species="asplenifolia">
