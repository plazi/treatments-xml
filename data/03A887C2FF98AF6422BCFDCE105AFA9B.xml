<taxonomicName id="4C014D57FF98AF662345F899106EF88D" authority="Kondo and Gonzalez" authorityName="Kondo and Gonzalez" authorityYear="2014" box="[313,796,1846,1868]" class="Insecta" family="Coccidae" genus="Toumeyella" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="martinezi">
<emphasis id="B975EAC6FF98AF662345F899115FF88D" box="[313,557,1846,1868]" italics="true" pageId="4" pageNumber="3">Toumeyella martinezi</emphasis>
Kondo and Gonzalez
</taxonomicName>
on 
<taxonomicName id="4C014D57FF98AF662134F89917F2F88D" box="[840,1152,1846,1868]" class="Magnoliopsida" family="Cactaceae" genus="Myrtillocactus" kingdom="Plantae" order="Caryophyllales" pageId="4" pageNumber="3" phylum="Tracheophyta" rank="species" species="geometrizans">
<emphasis id="B975EAC6FF98AF662134F89917F2F88D" box="[840,1152,1846,1868]" italics="true" pageId="4" pageNumber="3">Myrtillocactus geometrizans</emphasis>
</taxonomicName>
