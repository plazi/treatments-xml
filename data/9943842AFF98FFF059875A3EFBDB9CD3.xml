<taxonomicName id="D6EA4EBFFF98FFF359875A3EFDA29D43" authority="Gumovsky" authorityName="Gumovsky" box="[151,522,1374,1400]" class="Insecta" family="Eulophidae" genus="Goetheana" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="560" phylum="Arthropoda" rank="species" species="kobzari" status="sp. n.">
<emphasis id="239EE92EFF98FFF359875A3EFED49D43" bold="true" box="[151,380,1374,1400]" italics="true" pageId="9" pageNumber="560">Goetheana kobzari</emphasis>
Gumovsky
</taxonomicName>
, 
<taxonomicNameLabel id="38AD5455FF98FFF35B075A3EFDF19D43" box="[535,601,1374,1400]" pageId="9" pageNumber="560" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
