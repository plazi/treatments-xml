<taxonomicName id="4C46EABC4A029F54FEF9F9EA3126F9D0" authority="Jedlicka, 1951" authorityName="Jedlicka" authorityYear="1951" box="[278,639,1588,1607]" class="Insecta" family="Carabidae" genus="Sinechostictus" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="588" phylum="Arthropoda" rank="species" species="chuji" subGenus="Pseudolimnaeum">
