<emphasis id="BA920D10FFC4FFDCFED58619D1ABFEFE" bold="true" box="[383,483,322,346]" pageId="8" pageNumber="769">
<collectingCountry id="F0F19192FFC4FFDCFED58619D197FEFE" box="[383,479,322,346]" name="Malaysia" pageId="8" pageNumber="769">Malaysia</collectingCountry>
.
</emphasis>
<specimenCount id="9EE01A8BFFC4FFDCFE46861AD25CFEFC" box="[492,532,321,345]" pageId="8" pageNumber="769" type="female">1 ♀</specimenCount>
, 
<typeStatus id="575D6FA0FFC4FFDCFD8E861BD2CAFEFC" box="[548,642,320,344]" pageId="8" pageNumber="769">holotype</typeStatus>
, “Sar., Wallace, 
<taxonomicName id="4FE6AA81FFC4FFDCFEBA8600D18EFED7" authorityName="Walker" authorityYear="1857" box="[272,454,347,371]" class="Insecta" family="Issidae" genus="Eupilis" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="769" phylum="Arthropoda" rank="species" species="albilineola">
