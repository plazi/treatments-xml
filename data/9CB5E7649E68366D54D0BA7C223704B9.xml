<taxonomicName id="103E847C9E9E42D33545055AB61E510D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33860" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole megacephala (Fabricius)" order="Hymenoptera" pageNumber="226" phylum="Arthropoda" rank="species" species="megacephala">Ph. megacephala</taxonomicName>
