<normalizedToken id="EC6C0B7BFD6C705071A5B9B280913C39" originalValue="espèces">especes</normalizedToken>
<normalizedToken id="3FC5D5CAA9991D53371235155150F860" originalValue="mentionnées">mentionnees</normalizedToken>
