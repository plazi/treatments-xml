<taxonomicName id="4C6FDC97FFA7FFAA81401D7ECA9DE078" authority="MIHELCIC, 1956" authorityName="MIHELCIC" authorityYear="1956" class="Arachnida" family="Oppiidae" genus="Oppia" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="1783" phylum="Arthropoda" rank="species" species="simplex">
