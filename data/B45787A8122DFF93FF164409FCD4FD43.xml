<typeStatus id="E345881C122DFF95FF1644D2FE97F907" box="[151,201,1716,1740]" pageId="3" pageNumber="378">Type</typeStatus>
species: 
<taxonomicName id="FBFE4D3D122DFF95FEA744D2FC43F907" authority="Wang &amp; Zhang &amp; Bourgoin, 2019" authorityName="Wang &amp; Zhang &amp; Bourgoin" authorityYear="2019" box="[294,541,1716,1740]" class="Insecta" family="Issidae" genus="Duplexissus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="378" phylum="Arthropoda" rank="species" species="punctatulus" status="sp. n.">
<emphasis id="0E8AEAAC122DFF95FEA744D2FC43F907" box="[294,541,1716,1740]" italics="true" pageId="3" pageNumber="378">Duplexissus punctatulus</emphasis>
