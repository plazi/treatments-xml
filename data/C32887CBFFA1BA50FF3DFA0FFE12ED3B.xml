<heading id="107681B1FFA1BA52FF3DFA0FFD2BEC31" bold="true" box="[151,710,1528,1555]" fontSize="11" level="1" pageId="49" pageNumber="50" reason="1">
<taxonomicName id="8C814D5EFFA1BA52FF3DFA0FFD2BEC31" authority="Thomas, 1919" authorityName="Thomas" authorityYear="1919" box="[151,710,1528,1555]" class="Mammalia" family="Muridae" genus="Meriones" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="49" pageNumber="50" phylum="Chordata" rank="species" species="libycus" subGenus="Pallasiomys">
