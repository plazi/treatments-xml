<taxonomicName id="AF348916A817E2A23ABEB9F7155B3A43" LSID="http://zoobank.org/0492923A-E5F2-4B2C-A775-6A269A84168F" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus poqomchi" order="Hymenoptera" pageId="395" pageNumber="396" phylum="Arthropoda" rank="species" species="poqomchi">Heterospilus poqomchi Marsh</taxonomicName>
