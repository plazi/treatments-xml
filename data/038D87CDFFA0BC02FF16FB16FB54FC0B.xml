<quantity id="4CDC9B3EFFA1BC02FC3CFDF9FC3DFDCA" box="[957,1011,572,594]" metricMagnitude="1" metricUnit="m" metricValue="1.1" pageId="23" pageNumber="80" unit="m" value="11.0">11 m</quantity>
