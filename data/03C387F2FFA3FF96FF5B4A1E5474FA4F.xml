<taxonomicName id="4C6A4D67FFA3FF96FF5B4A7D57D6FAD4" authority="Ruppell, 1835" authorityName="Ruppell" authorityYear="1835" box="[151,430,1375,1400]" class="Reptilia" family="Sphaerodactylidae" genus="Pristurus" kingdom="Animalia" order="Squamata" pageId="47" pageNumber="48" phylum="Chordata" rank="genus">
<emphasis id="B91EEAF6FFA3FF96FF5B4A7D57D6FAD4" bold="true" box="[151,430,1375,1400]" pageId="47" pageNumber="48">
<emphasis id="B91EEAF6FFA3FF96FF5B4A7D577AFAD4" bold="true" box="[151,258,1375,1400]" italics="true" pageId="47" pageNumber="48">Pristurus</emphasis>
<bibRefCitation id="EFFB4B15FFA3FF96FEC54A7D57D6FAD4" author="Ruppell, E." box="[265,430,1375,1400]" pageId="47" pageNumber="48" refId="ref64387" refString="Ruppell, E. (1835) Neue Wirbelthiere zu der Fauna von Abyssinien gehorig, entdeckt und beschrieben. Amphibien. S. Schmerber, Frankfurt am Main. http: // dx. doi. org / 10.5962 / bhl. title. 53778" type="book" year="1835">Rüppell, 1835</bibRefCitation>
