<taxonomicName id="3BD4529851105CF793B386ECD58A5601" authorityName="Will &amp; Guéorguiev" authorityYear="2021" class="Insecta" family="Carabidae" genus="Coptocarpus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coptocarpus amieuensis" order="Coleoptera" pageId="0" pageNumber="375" phylum="Arthropoda" rank="species" species="amieuensis">
