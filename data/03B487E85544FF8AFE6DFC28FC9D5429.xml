<taxonomicName id="4C1D4D7D5546FF8AFD4FFDF8FF5B55A8" authority="(Michaelsen, 1922)" baseAuthorityName="Michaelsen" baseAuthorityYear="1922" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="26" pageNumber="1254" phylum="Annelida" rank="species" species="formosae">
