<taxonomicName id="4C1D4D7D5546FF8AFDF8FDB8FD235508" baseAuthorityName="Gates" baseAuthorityYear="1931" box="[609,694,541,565]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="26" pageNumber="1254" phylum="Annelida" rank="species" species="rimosus">
