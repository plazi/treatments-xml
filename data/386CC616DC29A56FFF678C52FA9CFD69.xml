<taxonomicName id="77C50C83DC2FA56CFEAA8ECEFE22F943" box="[346,492,1732,1754]" pageId="93" pageNumber="94" rank="species" species="insulagemmae" status="sp.nov.">insulagemmae</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="82B1AB12DC2FA56CFE068ECEFD8FF940" bold="true" box="[502,577,1732,1753]" pageId="93" pageNumber="94">
<taxonomicNameLabel id="99821669DC2FA56CFE068ECEFD8FF940" box="[502,577,1732,1753]" pageId="93" pageNumber="94" rank="species">sp.nov.</taxonomicNameLabel>
