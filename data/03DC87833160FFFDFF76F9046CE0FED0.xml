(
<bibRefCitation id="EFE44B643163FFFCFE70F9416E4DF8FE" author="Song" box="[401,781,1785,1809]" pageId="2" pageNumber="523" refString="Song, D. X. &amp; Zheng, S. X. (1982) Two new species of Hahnia (Araneae: Hahniidae) from Zhejiang Province, China. Sinozoologica, 2, 81 - 84." type="journal article" year="1982">
