<taxonomicName id="4C7B4D5FFF875252EFCFFBD93235FBC3" authority="Heydon, 2005" authorityName="Heydon" authorityYear="2005" box="[962,1344,1028,1050]" class="Insecta" family="Pteromalidae" genus="Ambogaster" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="422" phylum="Arthropoda" rank="species" species="wibawae">
