<paragraph id="8B8B36E3557EFF84FF4AF943FE6FB8DA" blockId="69.[151,400,1765,1791]" box="[151,400,1765,1791]" pageId="69" pageNumber="70">
<heading id="D0C3818F557EFF84FF4AF943FE6FB8DA" bold="true" box="[151,400,1765,1791]" fontSize="11" level="1" pageId="69" pageNumber="70" reason="1">
