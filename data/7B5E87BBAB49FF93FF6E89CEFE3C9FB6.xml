<taxonomicName id="34F74D2EAB49FF92FDA78A7BFCF99964" authorityName="Logvinenko et Starobogatov" authorityYear="1968" box="[606,812,1804,1830]" class="Gastropoda" family="Hydrobiidae" genus="Pyrgula" kingdom="Animalia" order="Littorinimorpha" pageId="4" pageNumber="412" phylum="Mollusca" rank="species" species="sowinskyi">
<emphasis id="C183EABFAB49FF92FDA78A7BFCF99964" box="[606,812,1804,1830]" italics="true" pageId="4" pageNumber="412">Pyrgula sowinskyi</emphasis>
</taxonomicName>
