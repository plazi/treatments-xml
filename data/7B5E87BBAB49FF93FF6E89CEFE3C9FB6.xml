<taxonomicName id="34F74D2EAB49FF92FC6C8B9FFBB19940" authorityName="Logvinenko et Starobogatov" authorityYear="1968" box="[917,1124,1768,1794]" class="Gastropoda" family="Hydrobiidae" genus="Pyrgula" kingdom="Animalia" order="Littorinimorpha" pageId="4" pageNumber="412" phylum="Mollusca" rank="species" species="sowinskyi">
