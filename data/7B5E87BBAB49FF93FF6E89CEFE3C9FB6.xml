<collectionCode id="95E6AE68AB49FF92FBFD8A7AFBE69965" box="[1028,1075,1805,1831]" country="Russia" httpUri="http://grbio.org/cool/z2y8-2jfr" name="Russian Academy of Sciences, Zoological Institute, Zoological Museum" pageId="4" pageNumber="412" type="Museum">ZIN</collectionCode>
collection (
<collectionCode id="95E6AE68AB49FF92FB438A7AFB3F9965" box="[1210,1258,1805,1831]" country="Russia" httpUri="http://grbio.org/cool/z2y8-2jfr" name="Russian Academy of Sciences, Zoological Institute, Zoological Museum" pageId="4" pageNumber="412" type="Museum">ZIN</collectionCode>
