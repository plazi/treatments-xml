<collectingCountry id="9AF27662FF81652B178444D6FDF0728A" box="[474,590,403,429]" name="Argentina" pageId="1" pageNumber="388">Argentina</collectingCountry>
: 
<collectingRegion id="2021F810FF81652B140944D6FD7C728A" box="[599,706,403,429]" country="Argentina" name="Misiones" pageId="1" pageNumber="388">Misiones</collectingRegion>
