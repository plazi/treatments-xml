<taxonomicName id="26711646E3A8C3D712174E0517A197DD" class="Arachnida" family="Suctobelbidae" genus="Suctobelba" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="138" phylum="Arthropoda" rank="species" species="aliena">Suctobelba aliena</taxonomicName>
<taxonomicNameLabel id="69A2A66FB1BAD44466C8C52E423BF3FC" pageId="4" pageNumber="138" rank="species">nov. spec.</taxonomicNameLabel>
