<quantity id="4CAE9B05FFAFFFBCFEE18E138FD8FBF8" box="[268,400,1065,1089]" metricMagnitude="-3" metricUnit="m" metricValue="6.6" metricValueMax="6.8" metricValueMin="6.4" pageId="10" pageNumber="11" unit="mm" value="6.6" valueMax="6.8" valueMin="6.4">6.4–6.8 mm</quantity>
