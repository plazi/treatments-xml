<quantity id="4CAE9B05FFAFFFBCFE018E138C27FBF8" box="[492,623,1065,1089]" metricMagnitude="-3" metricUnit="m" metricValue="5.75" metricValueMax="6.2" metricValueMin="5.3" pageId="10" pageNumber="11" unit="mm" value="5.75" valueMax="6.2" valueMin="5.3">5.3–6.2 mm</quantity>
; length of forebody in  2.8–3.0 mm, in  
<quantity id="4CAE9B05FFAFFFBCFDD98E738CF0FBD8" box="[564,696,1097,1121]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" metricValueMax="2.7" metricValueMin="2.5" pageId="10" pageNumber="11" unit="mm" value="2.6" valueMax="2.7" valueMin="2.5">2.5–2.7 mm</quantity>
