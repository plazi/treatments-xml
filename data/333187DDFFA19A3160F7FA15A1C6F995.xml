<quantity id="7C609B2EFFA39A3164E1FCCBA01AFC1A" box="[1153,1366,877,903]" metricMagnitude="1" metricUnit="m" metricValue="6.4118" metricValueMax="12.8121" metricValueMin="0.0115" pageId="11" pageNumber="64" unit="mm" value="64118.0" valueMax="128121.0" valueMin="115.0">115–128, 121 µm</quantity>
