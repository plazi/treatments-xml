<quantity id="4CA89B79FF96E81A0931899826C4FA6B" box="[1298,1441,1419,1441]" metricMagnitude="6" metricUnit="m" metricValue="4.24435" pageId="25" pageNumber="540" unit="m" value="4244350.0">4244, 350 m</quantity>
