<subSubSection id="C3689BF9FFE5FFB1FF3DF92FFBDFF962" box="[197,1128,1709,1735]" pageId="14" pageNumber="15" type="etymology">
<paragraph id="8BCDC872FFE5FFB1FF3DF92FFBDFF962" blockId="14.[151,1437,1596,2034]" box="[197,1128,1709,1735]" pageId="14" pageNumber="15">
<emphasis id="B9061460FFE5FFB1FF3DF92FFEE6F962" bold="true" box="[197,337,1709,1735]" pageId="14" pageNumber="15">Etymology.</emphasis>
