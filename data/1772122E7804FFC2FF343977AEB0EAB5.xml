<taxonomicName id="58DBD8BB7804FFC5FE9F3977AEEBEC4C" baseAuthorityName="Hoffman" baseAuthorityYear="1980" box="[274,434,1539,1565]" class="Diplopoda" family="Pachybolidae" kingdom="Animalia" order="Spirobolida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="family">Pachybolidae</taxonomicName>
