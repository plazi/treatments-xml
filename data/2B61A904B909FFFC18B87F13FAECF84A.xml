<taxonomicName id="64C86391B90AFFFC196E7ED6FD4FFB0E" authority="Billard, 1940" authorityName="Billard" authorityYear="1940" box="[321,680,1182,1206]" class="Hydrozoa" family="Campanulinidae" genus="Campanulina" kingdom="Animalia" order="Leptothecata" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="paucilaminosa">
<emphasis id="91BCC400B90AFFFC196E7ED6FDEFFB0E" box="[321,520,1182,1206]" italics="true" pageId="2" pageNumber="3">C. paucilaminosa</emphasis>
<bibRefCitation id="C75965E3B90AFFFC1A3F7ED7FD4FFB0E" author="Billard" box="[528,680,1182,1206]" pageId="2" pageNumber="3" refString="Billard, A. (1940) Note sur deux especes nouvelles de Campanulinidae (Hydroides) Bulletin de la Societe Zoologique de France, 62, 292 - 293." type="journal article" year="1940">
Billard, 
<number id="15000A35B90AFFFC1A437ED7FD4FFB0E" box="[620,680,1182,1206]" pageId="2" pageNumber="3" value="1940.0">1940</number>
</bibRefCitation>
</taxonomicName>
. 
<emphasis id="91BCC400B90AFFFC1A9C7ED7FC0EFB0E" box="[691,1001,1182,1206]" italics="true" pageId="2" pageNumber="3">
<taxonomicName id="64C86391B90AFFFC1A9C7ED7FC02FB0E" box="[691,997,1182,1206]" class="Hydrozoa" family="Campanulinidae" genus="Campanulina" kingdom="Animalia" order="Leptothecata" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="maduraensis">Campanulina maduraensis</taxonomicName>
,
</emphasis>
C. 
