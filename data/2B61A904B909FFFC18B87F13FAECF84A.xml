, 
<taxonomicName id="64C86391B90AFFFC1C407ED6FB14FB0E" box="[1135,1267,1182,1206]" class="Hydrozoa" family="Campanulinidae" genus="Campanulina" kingdom="Animalia" order="Leptothecata" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="panicula">C. panicula</taxonomicName>
</emphasis>
and 
<taxonomicName id="64C86391B90AFFFC1D047ED6FF11FB63" class="Hydrozoa" family="Campanulinidae" genus="Campanulina" kingdom="Animalia" order="Leptothecata" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="paucilaminosa">
