<taxonomicName id="64C86391B90AFFFC1B8A7E1FFA94FBD6" authority="G.O. Sars, 1874" authorityName="G.O. Sars" authorityYear="1874" box="[933,1395,1110,1134]" class="Hydrozoa" family="Campanulinidae" genus="Campanulina" kingdom="Animalia" order="Leptothecata" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="panicula">
<emphasis id="91BCC400B90AFFFC1B8A7E1FFB4CFBD6" box="[933,1195,1110,1134]" italics="true" pageId="2" pageNumber="3">Campanulina panicula</emphasis>
<bibRefCitation id="C75965E3B90AFFFC1C997E1FFA94FBD6" author="Sars" box="[1206,1395,1110,1134]" pageId="2" pageNumber="3" refString="Sars, G. O. (1874) Bidrag til Kundskaben om Norges Hydroider. Forhandlinger i Videnskabsselscabet i Kristiania, 1873, 91 - 150." type="journal article" year="1874">
