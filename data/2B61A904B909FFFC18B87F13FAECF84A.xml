</taxonomicName>
, 
<taxonomicName id="64C86391B90AFFFC1A9F7E32FC42FB2A" authority="Bale, 1924" authorityName="Bale" authorityYear="1924" box="[688,933,1146,1171]" class="Hydrozoa" family="Campanulinidae" genus="Campanulina" kingdom="Animalia" order="Leptothecata" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="humilis">
