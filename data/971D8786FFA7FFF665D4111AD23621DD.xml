<taxonomicName id="D8B44D13FFA7FFF665D4111AD08C209A" box="[151,440,1647,1673]" class="Insecta" family="Formicidae" genus="Pogonomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="laticeps">
