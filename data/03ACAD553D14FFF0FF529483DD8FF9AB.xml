<typeStatus id="54BEA2E13D14FFF0FE57955CDC2EFEF2" box="[439,530,262,284]" pageId="23" pageNumber="1694">Holotypus</typeStatus>
() und 1 - 
<typeStatus id="54BEA2E13D14FFF0FD7B955DDCC9FEF2" box="[667,757,263,284]" pageId="23" pageNumber="1694">Paratypus</typeStatus>
: 
<collectingCountry id="F3125CD33D14FFF0FCE4955DDDACFEF2" box="[772,912,263,284]" name="Costa Rica" pageId="23" pageNumber="1694">COSTA RICA</collectingCountry>
: 
<collectingRegion id="49C1D2A13D14FFF0FC7E955DDA07FEF2" box="[926,1083,263,284]" country="Costa Rica" name="Puntarenas" pageId="23" pageNumber="1694">Puntarenas Prov.</collectingRegion>
, Monte Verde, 
<quantity id="4CFDB1A63D14FFF0FEBD9544DFA0FEDA" box="[349,412,286,308]" metricMagnitude="3" metricUnit="m" metricValue="1.5699999999999998" pageId="23" pageNumber="1694" unit="m" value="1570.0">1570m</quantity>
, berlese, 
<date id="FFBB3A833D14FFF0FE0C9544DC76FEDA" box="[492,586,286,308]" pageId="23" pageNumber="1694" value="1989-05-16">16.V.1989</date>
