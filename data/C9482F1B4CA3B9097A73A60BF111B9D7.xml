<taxonomicName id="27ACCEF5902683E1DC3E744FE44382C5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138485" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica nitidula Nylander" order="Hymenoptera" pageNumber="78" phylum="Arthropoda" rank="species" species="nitidula">Myrmica nitidula Nylander</taxonomicName>
