, HW 
<quantity id="C06190D8DB004E27A2557D9DFD7132B9" box="[567,673,384,410]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="7" pageNumber="58" unit="mm" value="1.5">1.50 mm</quantity>
, PL 
<quantity id="C06190D8DB004E27A2B47D9DFC9032B9" box="[726,832,384,410]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="7" pageNumber="58" unit="mm" value="0.8">0.80 mm</quantity>
, PW 
<quantity id="C06190D8DB004E27A31D7D9DFC3932B9" box="[895,1001,384,410]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="7" pageNumber="58" unit="mm" value="1.1">1.10 mm</quantity>
, EL 5.00 mm, Ediam 
<quantity id="C06190D8DB004E27A4887D9DFA8432B9" box="[1258,1364,384,410]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="7" pageNumber="58" unit="mm" value="0.35">0.35 mm</quantity>
, Edist 
<quantity id="C06190D8DB004E27A0F57DBBFED232E3" box="[151,258,422,448]" metricMagnitude="-4" metricUnit="m" metricValue="4.4" pageId="7" pageNumber="58" unit="mm" value="0.44">0.44 mm</quantity>
