<subSubSection id="4F836EB6DB004E27A0A47DEEFBAF312D" box="[198,1151,499,526]" pageId="7" pageNumber="58" type="etymology">
<paragraph id="07263D3DDB004E27A0A47DEEFBAF312D" blockId="7.[151,1436,152,526]" box="[198,1151,499,526]" pageId="7" pageNumber="58">
<emphasis id="35EDE12FDB004E27A0A47DEEFE83312E" bold="true" box="[198,339,499,525]" pageId="7" pageNumber="58">Etymology.</emphasis>
