<taxonomicName id="6ABEF1ED0F16A60E1419AF1A3E7F50D5" class="Anthozoa" family="Fungiidae" genus="Cycloseris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cycloseris boschmai" order="Scleractinia" pageId="12" pageNumber="69" phylum="Cnidaria" rank="species" species="boschmai">Cycloseris boschmai</taxonomicName>
