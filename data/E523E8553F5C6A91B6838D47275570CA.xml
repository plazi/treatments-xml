<taxonomicName id="73FB5FCCF2882935972917930C761A9D" authorityName="Bertoloni" authorityYear="1819" class="Anthozoa" family="Gorgoniidae" genus="Gorgonia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Gorgonia savaglia" order="Alcyonacea" pageId="16" pageNumber="17" phylum="Cnidaria" rank="species" species="savaglia">
