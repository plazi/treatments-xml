<taxonomicName id="4C2CB389F57C6700FE62DFB5FD9E68A4" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" box="[432,560,1742,1768]" class="Insecta" family="Chrysididae" genus="Cleptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="60" phylum="Arthropoda" rank="subGenus" subGenus="Leiocleptes">
