<quantity id="4CEF9B25FFBBFF9F9BD7FAADF9A2357F" box="[938,995,1375,1402]" metricMagnitude="-1" metricUnit="m" metricValue="3.302" pageId="42" pageNumber="43" unit="in" value="13.0">13 in</quantity>
