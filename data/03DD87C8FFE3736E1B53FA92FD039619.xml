<taxonomicName id="4C744D5DFFE3736F1C3EF9EBFA619124" authority="(Waite, 1898 a)" baseAuthorityName="Waite" baseAuthorityYear="1898" box="[1036,1460,1535,1560]" class="Mammalia" family="Muridae" genus="Rattus" kingdom="Animalia" order="Rodentia" pageId="90" pageNumber="367" phylum="Chordata" rank="species" species="villosissimus">
