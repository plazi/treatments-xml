<specimenCode id="DBA39E8FFF9BFFEDFD19FBEC3AFBFB25" box="[742,924,1131,1153]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="32" pageNumber="32">AMNH 216116</specimenCode>
