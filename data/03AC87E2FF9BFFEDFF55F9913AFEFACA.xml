<specimenCode id="DBA39E8FFF9BFFEDFBB3FC723B8AFB8C" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="32" pageNumber="32">AMNH 216119</specimenCode>
