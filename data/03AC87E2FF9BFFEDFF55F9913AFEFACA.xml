<specimenCode id="DBA39E8FFF9BFFEDFCD9FB943ABAFB8C" box="[806,989,1042,1064]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="32" pageNumber="32">AMNH 216123</specimenCode>
