, 
<taxonomicName id="4C544D7671670920FC1FF937FBBFB897" authorityName="Jaderholm" authorityYear="1904" box="[989,1137,1679,1703]" class="Hydrozoa" family="Sertulariidae" genus="Selaginopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="dichotoma">
<emphasis id="B920EAE771670920FC1FF937FBBFB897" box="[989,1137,1679,1703]" italics="true" pageId="26" pageNumber="2678">S. dichotoma</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C544D7671670920FB40F937FB23B897" authorityName="Pena Cantero, Svoboda and Vervoort" authorityYear="1997" box="[1154,1261,1679,1703]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="frigida">
<emphasis id="B920EAE771670920FB40F937FB23B897" box="[1154,1261,1679,1703]" italics="true" pageId="26" pageNumber="2678">S. frigida</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C544D7671670920FEFBF917FE05B8F8" authorityName="Pena Cantero, Svoboda and Vervoort" authorityYear="1997" box="[313,459,1712,1736]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="glomulosa">
<emphasis id="B920EAE771670920FEFBF917FE05B8F8" box="[313,459,1712,1736]" italics="true" pageId="26" pageNumber="2678">S. glomulosa</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C544D7671670920FE1CF917FDBDB8F7" authorityName="Pena Cantero, Svoboda and Vervoort" authorityYear="1997" box="[478,627,1712,1735]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="nonscripta">
<emphasis id="B920EAE771670920FE1CF917FDBDB8F7" box="[478,627,1712,1735]" italics="true" pageId="26" pageNumber="2678">S. nonscripta</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C544D7671670920FD77F917FC9BB8F7" authorityName="Pena Cantero and Garcia Carrascosa" authorityYear="1994" baseAuthorityName="Pena Cantero and Garcia Carrascosa" baseAuthorityYear="1994" box="[693,853,1712,1736]" class="Hydrozoa" family="Sertulariidae" genus="Selaginopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="vanhoeffeni">
<emphasis id="B920EAE771670920FD77F917FC9BB8F7" box="[693,853,1712,1736]" italics="true" pageId="26" pageNumber="2678">S. vanhoeffeni</emphasis>
</taxonomicName>
(cf. 
<tableCitation id="C6D6034E71670920FC52F917FC2FB8F8" box="[912,993,1712,1736]" captionStart="Table 6" captionStartId="62.[911,967,397,420]" captionText="Table 6. Major features of the known species of" pageId="26" pageNumber="2678">table 6</tableCitation>
). Nevertheless, they all
</paragraph>
