<taxonomicName id="4C544D7671670920FDD3F937FD6FB897" box="[529,673,1680,1703]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="antarctica">
<emphasis id="B920EAE771670920FDD3F937FD6FB897" box="[529,673,1680,1703]" italics="true" pageId="26" pageNumber="2678">S. antarctica</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C544D7671670920FD71F937FC88B897" authorityName="Briggs" authorityYear="1938" box="[691,838,1680,1703]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="compressa">
