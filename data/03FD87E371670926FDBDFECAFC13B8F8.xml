<taxonomicName id="4C544D7671610926FD33FF16FC66BEF6" authorityName="Pena Cantero &amp; Vervoort" authorityYear="2003" box="[753,936,177,198]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="28" pageNumber="2680" phylum="Cnidaria" rank="species" species="densa" status="sp. nov.">
<emphasis id="B920EAE771610926FD33FF16FC66BEF6" box="[753,936,177,198]" italics="true" pageId="28" pageNumber="2680">Staurotheca densa</emphasis>
</taxonomicName>
