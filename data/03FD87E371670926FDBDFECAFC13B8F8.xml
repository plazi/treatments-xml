</taxonomicName>
, 
<taxonomicName id="4C544D7671670920FC95F937FC02B897" authorityName="Pena Cantero, Garcia Carrascosa and Vervoort" authorityYear="1999" box="[855,972,1680,1703]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="2678" phylum="Cnidaria" rank="species" species="cornuta">
<emphasis id="B920EAE771670920FC95F937FC02B897" box="[855,972,1680,1703]" italics="true" pageId="26" pageNumber="2678">S. cornuta</emphasis>
