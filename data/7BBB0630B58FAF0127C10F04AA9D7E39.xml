<taxonomicName id="25740BFF92B3E52C57A1D7C1D4FD95E4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:141826" authority="Mayr" authorityName="Mayr" class="Hexapoda" family="Formicidae" genus="Bothroponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Bothroponera silvestrii r. nimba Bernard" order="Hymenoptera" pageNumber="188" phylum="Arthropoda" rank="subSpecies" species="silvestrii" subSpecies="nimba">B. silvestrii Sant., race nimba</taxonomicName>
