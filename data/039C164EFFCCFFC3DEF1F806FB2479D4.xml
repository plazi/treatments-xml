<taxonomicName id="4C35DCDBFFCFFFC3D869FE6FFB217991" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[1115,1211,888,909]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="coruscus">
