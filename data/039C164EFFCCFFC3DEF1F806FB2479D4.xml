<paragraph id="8B8AA758FFCCFFC0DEF1F806FBF57F34" blockId="77.[707,1135,1297,1320]" box="[707,1135,1297,1320]" pageId="77" pageNumber="78">
<heading id="D0C21034FFCCFFC0DEF1F806FBF57F34" box="[707,1135,1297,1320]" centered="true" fontSize="9" level="2" pageId="77" pageNumber="78" reason="2">
<taxonomicName id="4C35DCDBFFCCFFC0DEF1F806FC427F34" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[707,984,1297,1320]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="coruscus" status="new species">
<emphasis id="B9417B4AFFCCFFC0DEF1F806FC427F34" bold="true" box="[707,984,1297,1320]" italics="true" pageId="77" pageNumber="78">Paramblynotus coruscus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFCCFFC0DFDBF804FBF57F34" box="[1001,1135,1298,1320]" pageId="77" pageNumber="78" rank="species">new species</taxonomicNameLabel>
