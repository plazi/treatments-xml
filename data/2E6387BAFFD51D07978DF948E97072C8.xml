<taxonomicName id="61CA4D2FFFD51D07958EF893EEE07200" box="[660,765,1882,1907]" class="Malacostraca" family="Niphargidae" genus="Exniphargus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="518" phylum="Arthropoda" rank="species" species="tzanisi">
