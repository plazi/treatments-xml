<taxonomicName id="65F08AF1D468E3302AB7A905BE33A718" LSID-ZBK="urn:lsid:zoobank.org:act:9330FF4A-A250-45FD-81A4-07CC54FEE346" class="Actinopterygii" family="Loricariidae" genus="Loricaria" kingdom="Animalia" lsidName-ZBK="Loricaria lima Kner 1853:89 [25]" order="Siluriformes" pageNumber="35" phylum="Chordata" rank="species" species="lima">Loricaria lima Kner, 1853</taxonomicName>
