<td id="76931D77831FCE8D7949FB0CFB0AA8E4" box="[1044,1172,1228,1251]" gridcol="3" gridrow="33" pageId="5" pageNumber="2969">9, 11</td>
</tr>
<tr id="3542740B831FCE8D7D8EFB2AFB0AA907" box="[211,1172,1258,1280]" gridrow="34" pageId="5" pageNumber="2969" rowspan-2="1">
<td id="76931D77831FCE8D7D8EFB2AFD8CA907" box="[211,530,1258,1280]" gridcol="0" gridrow="34" pageId="5" pageNumber="2969">Ventromedian supplements</td>
