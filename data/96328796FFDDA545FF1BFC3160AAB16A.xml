<collectingCountry id="668C7610FFDDA545FE27F9856049B051" box="[404,512,1636,1660]" name="Suriname" pageId="7" pageNumber="884">Suriname</collectingCountry>
, French Guiana, 
<collectingCountry id="668C7610FFDDA545FD79F9856287B0B3" name="Ecuador" pageId="7" pageNumber="884">Ecuador</collectingCountry>
, 
