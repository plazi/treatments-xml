<taxonomicName id="0FC24D141353FFF8FE8BFC300828FC33" box="[371,620,952,977]" class="Insecta" family="Siphlonuridae" genus="Parameletus" kingdom="Animalia" order="Ephemeroptera" pageId="6" pageNumber="23" phylum="Arthropoda" rank="species" species="chelifer">
