<location id="3D7112E5FFC5FFC816D7FCC6FE965AFA" LSID="urn:lsid:plazi:treatment:B007F528FFC5FFC816D7FD12FDF55DDA:3D7112E5FFC5FFC816D7FCC6FE965AFA" box="[151,324,871,897]" name="N'Dalla Gando" pageId="29" pageNumber="30" stateProvince="Kwanza Norte">N’Dalla Gando</location>
[=N’dalatando] [-9.30000º, 14.91667º, 
<elevation id="B383A30DFFC5FFC81546FCC6FC585AFA" box="[774,906,870,898]" metricMagnitude="2" metricUnit="m" metricValue="7.82" pageId="29" pageNumber="30" unit="m" value="782.0">
<quantity id="FF56E9DBFFC5FFC81546FCC6FC9C5AF9" box="[774,846,871,898]" metricMagnitude="2" metricUnit="m" metricValue="7.82" pageId="29" pageNumber="30" unit="m" value="782.0">782 m</quantity>
