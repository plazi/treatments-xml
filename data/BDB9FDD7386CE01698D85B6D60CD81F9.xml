<taxonomicName id="0B0BD9021C066B090F37EDF9E87D9999" authority="L." class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="130" phylum="Tracheophyta" rank="species" species="palustris">
