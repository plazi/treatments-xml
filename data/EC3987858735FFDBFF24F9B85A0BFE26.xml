Tropical 
<collectingRegion id="A654F8718734FFDBFE63FE915965FEB5" box="[464,548,296,319]" country="Sri Lanka" name="Eastern" pageId="27" pageNumber="428">Eastern</collectingRegion>
Pacific. Gulf of 
<collectingRegion id="A654F8718734FFDBFD54FE9E581DFECA" box="[743,860,295,320]" country="United States of America" name="California" pageId="27" pageNumber="428">California</collectingRegion>
, 
<collectingRegion id="A654F8718734FFDBFCDAFE9158FCFECA" box="[873,957,296,320]" country="Mexico" name="Oaxaca" pageId="27" pageNumber="428">Oaxaca</collectingRegion>
(
<collectingRegion id="A654F8718734FFDBFC7DFE9E5F66FECA" box="[974,1063,295,320]" country="Mexico" name="Mexico" pageId="27" pageNumber="428">
<collectingCountry id="1C8776038734FFDBFC7DFE9E5F66FECA" box="[974,1063,295,320]" name="Mexico" pageId="27" pageNumber="428">México</collectingCountry>
</collectingRegion>
) and 
<collectingRegion id="A654F8718734FFDBFBDAFE915F83FECA" box="[1129,1218,296,320]" country="Panama" name="Panama" pageId="27" pageNumber="428">
<collectingCountry id="1C8776038734FFDBFBDAFE915F83FECA" box="[1129,1218,296,320]" name="Panama" pageId="27" pageNumber="428">Panamá</collectingCountry>
</collectingRegion>
(Bastida-Zavala &amp; ten 
<bibRefCitation id="00014B628734FFDBFF73FEF45A00FEEE" author="Hove" box="[192,321,332,356]" pageId="27" pageNumber="428" refString="Bastida-Zavala, J. R. &amp; Hove, H. A. ten (2003) Revision of Hydroides Gunnerus, 1768 (Polychaeta: Serpulidae) from the Eastern Pacific Region and Hawaii. Beaufortia, Bulletin of Zoological Museum, 53 (4), 67 - 110." type="journal article" year="2003">Hove 2003</bibRefCitation>
).
</paragraph>
