<taxonomicName id="8DE7E3EFF54CF906737CE1FE499D974E" authority="L." authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Fabaceae" genus="Phaca" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="species" species="alpina">
