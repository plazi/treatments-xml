<taxonomicName id="4C274D61FFB9FFE6FCBAFB40FF21E93E" authority="(RIBES et al. 2007)" baseAuthorityName="RIBES" baseAuthorityYear="2007" class="Insecta" family="Pentatomidae" genus="Carpocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="fuscispinus">
