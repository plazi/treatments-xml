<taxonomicName id="4AF767BC36E0A5F11AF79BFB50873FE5" authority="Berlese &amp; Trouessart, 1889" authorityName="Berlese &amp; Trouessart" authorityYear="1889" class="Arachnida" family="Rhinonyssidae" genus="Sternostoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sternostoma cryptorhynchum" order="Mesostigmata" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="cryptorhynchum">Sternostoma cryptorhynchum</taxonomicName>
