(
<bibRefCitation id="A65CD9F31804FFCDFB08F839FA1DF828" author="Milne-Edwards" box="[1217,1428,1989,2010]" pageId="78" pageNumber="79" refString="Milne-Edwards, H. (1840) Histoire naturelle des Crustaces, comprenant l'anatomie, la physiologie, et la classification de ces animaux. Tome 3. Paris, 638 pp., plates 1 - 42. http: // dx. doi. org / 10.5962 / bhl. title. 16170" type="book chapter" year="1840">Milne-Edwards, 1840</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="C272A4021805FFCCFF5EFF6BFA15FF38" blockId="79.[151,1436,151,319]" pageId="79" pageNumber="80">
<keyLead id="793C07371805FFCCFF5EFF6BFA15FF38" pageId="79" pageNumber="80">
