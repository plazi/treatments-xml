<taxonomicName id="05CDDF811804FFCDFBF7F839FA15F828" authority="Milne-Edwards, 1840" authorityName="Milne-Edwards" authorityYear="1840" box="[1086,1436,1989,2010]" class="Malacostraca" family="Oxycephalidae" genus="Oxycephalus" kingdom="Animalia" order="Amphipoda" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="armatum">
