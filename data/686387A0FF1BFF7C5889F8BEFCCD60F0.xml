<taxonomicName id="27CA4D35FF1AFF7C5A6FFEAAFCDC6038" box="[625,879,295,321]" class="Insecta" family="Sciomyzidae" genus="Coremacera" kingdom="Animalia" order="Diptera" pageId="215" pageNumber="216" phylum="Arthropoda" rank="species" species="confluens">
