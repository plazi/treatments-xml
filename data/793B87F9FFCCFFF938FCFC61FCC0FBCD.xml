<taxonomicName id="36924D6CFFCCFFF93AD5FBB7FC85FBA0" box="[667,778,1060,1082]" class="Florideophyceae" family="Liagoraceae" genus="Ganonema" kingdom="Plantae" order="Nemaliales" pageId="19" pageNumber="852" phylum="Rhodophyta" rank="genus">
