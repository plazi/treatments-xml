<taxonomicName id="4C6DA3CD99043A39FBE9FA8CFB58FA91" authority="Tiga Puluh" authorityName="Tiga Puluh" box="[1032,1234,1361,1387]" genus="Bukit" pageId="18" pageNumber="24" rank="genus">Bukit Tiga Puluh</taxonomicName>
