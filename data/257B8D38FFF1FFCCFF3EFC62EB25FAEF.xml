<collectingCountry id="D5C57CBEFFF1FFCCFCCBFC1FEAD0FC0E" box="[866,971,979,1005]" name="Paraguay" pageId="32" pageNumber="33">Paraguay</collectingCountry>
, and 
<collectingCountry id="D5C57CBEFFF1FFCCFBADFC1EED6CFC0F" box="[1028,1143,978,1004]" name="Argentina" pageId="32" pageNumber="33">Argentina</collectingCountry>
