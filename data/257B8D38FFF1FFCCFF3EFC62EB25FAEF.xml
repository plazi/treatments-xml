<collectingCountry id="D5C57CBEFFF1FFCCFD87FC1EEB84FC0F" box="[558,671,978,1004]" name="Colombia" pageId="32" pageNumber="33">Colombia</collectingCountry>
, 
<collectingCountry id="D5C57CBEFFF1FFCCFD03FC1EEA1FFC0F" box="[682,772,978,1004]" name="Guyana" pageId="32" pageNumber="33">Guyana</collectingCountry>
, 
<taxonomicName id="6AD247ADFFF1FFCCFCA6FC1EEA4DFC0F" box="[783,854,978,1004]" class="Insecta" family="Pentatomidae" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="73" phylum="Arthropoda" rank="subFamily" subFamily="Brazil">Brazil</taxonomicName>
