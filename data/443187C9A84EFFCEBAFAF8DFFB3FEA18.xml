<emphasis id="FEECEACDA84FFFCEBAAAFEECFEFDE940" bold="true" box="[199,352,295,320]" pageId="9" pageNumber="10">Distribution.</emphasis>
<collectingCountry id="B48F764FA84FFFCEBBF5FEECFD9EE940" box="[408,515,295,320]" name="Denmark" pageId="9" pageNumber="10">Denmark</collectingCountry>
, 
<collectingCountry id="B48F764FA84FFFCEB853FEECFD08E940" box="[574,661,295,320]" name="Estonia" pageId="9" pageNumber="10">Estonia</collectingCountry>
, 
