, 
<specimenCount id="9D0BFD48FFD6B141FC83DD15E1CCF87E" box="[825,915,1907,1933]" pageId="5" pageNumber="254" type="male">2 males</specimenCount>
. 
<collectingRegion id="49C9F823FFD6B141FC26DD15E663F87E" box="[924,1084,1907,1933]" country="Colombia" name="Antioquia" pageId="5" pageNumber="254">ANTIOQUIA</collectingRegion>
, 
<collectingRegion id="49C9F823FFD6B141FBFDDD15E6C9F87E" box="[1095,1174,1907,1933]" country="Colombia" name="Caldas" pageId="5" pageNumber="254">Caldas</collectingRegion>
