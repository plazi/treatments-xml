<taxonomicName id="E7F34D4BDC3BFFA927BBFB5A196DFB72" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[745,1078,1196,1221]" class="Insecta" family="Papilionidae" genus="Papilio" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="75" phylum="Arthropoda" rank="species" species="zetes">
