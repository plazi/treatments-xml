<quantity id="097E6CB336621059FF1F0B8AD283DECF" box="[138,226,656,680]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="19" pageNumber="1509" unit="mm" value="2.1">2.1 mm</quantity>
