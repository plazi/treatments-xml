<quantity id="097E6CB336621059FEAF0B8AD3F0DECF" box="[314,401,656,680]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="19" pageNumber="1509" unit="mm" value="3.4">3.4 mm</quantity>
(
<specimenCount id="D8800ADF36621059FE300B95D3D2DECF" box="[421,435,655,680]" pageId="19" pageNumber="1509" type="female">♀</specimenCount>
