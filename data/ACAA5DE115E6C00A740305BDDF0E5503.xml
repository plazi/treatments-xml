<taxonomicName id="B7ACAE0FB1EF759629A4A6B669872B9D" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="433" phylum="Tracheophyta" rank="species" species="leersii">
<italic id="97C334B0798B86E432BEA4CFD34D7AE7" pageId="null" pageNumber="433">C. Leersii</italic>
</taxonomicName>
(Nr. 20c)
