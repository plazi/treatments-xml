<heading id="6C7E3B01315D385DDEE34CDB418EE585" bold="true" box="[151,728,1961,1987]" fontSize="11" level="1" pageId="2" pageNumber="193" reason="1">
<emphasis id="05FD507F315D385DDEE34CDB418EE585" bold="true" box="[151,728,1961,1987]" pageId="2" pageNumber="193">
Genus 
<taxonomicName id="F089F7EE315D385DDE9A4CDB4104E585" authority="Quatrefages, 1865" authorityName="Quatrefages" authorityYear="1865" box="[238,594,1961,1987]" class="Polychaeta" family="Eunicidae" genus="Marphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Eunicida" pageId="2" pageNumber="193" phylum="Annelida" rank="genus">
