<taxonomicName id="4C4475C6FFF5FFF1FE80F9588843F959" authority="Zannou, Moraes &amp; Oliveira" authorityName="Zannou, Moraes &amp; Oliveira" authorityYear="2008" box="[288,1041,1732,1756]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="asperosetosus" status="n. sp." subGenus="Anthoseius">
