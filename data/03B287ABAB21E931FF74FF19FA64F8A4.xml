<subSubSection id="C3016536AB21E931FF24FA98FA64F8A4" pageId="2" pageNumber="110" type="discussion">
<paragraph id="8BA436BDAB21E931FF24FA98FB91F8C9" blockId="2.[151,1436,327,1901]" pageId="2" pageNumber="110">
<emphasis id="B96FEAAFAB21E931FF24FA98FEA6FABC" bold="true" box="[199,328,1372,1397]" pageId="2" pageNumber="110">Comments</emphasis>
. 
<taxonomicName id="4C1B4D3EAB21E931FEBDFA98FE28FABC" box="[350,454,1372,1397]" class="Insecta" family="Membracidae" genus="Cyphotes" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="110" phylum="Arthropoda" rank="genus">
<emphasis id="B96FEAAFAB21E931FEBDFA98FE28FABC" box="[350,454,1372,1397]" italics="true" pageId="2" pageNumber="110">Cyphotes</emphasis>
</taxonomicName>
