<quantity id="883DCBD1B904FFBFFBADF9D2FB101321" box="[1124,1231,1568,1594]" metricMagnitude="-4" metricUnit="m" metricValue="5.8" pageId="8" pageNumber="36" unit="mm" value="0.58">0.58 mm</quantity>
