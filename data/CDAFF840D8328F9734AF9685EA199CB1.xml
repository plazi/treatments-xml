<taxonomicName id="A8CE5D4F29201458ED6B5EA815373528" authority="Gaudin" class="Liliopsida" family="Iridaceae" genus="Gladiolus" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="593" phylum="Tracheophyta" rank="species" species="paluster">
