<taxonomicName id="4C785EC7FFD586106EA0411AFECD9C06" box="[210,299,768,793]" class="Insecta" family="Sciaridae" genus="Scatopsciara" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="434" phylum="Arthropoda" rank="species" species="alawae" status="sp. n.">Alawae</taxonomicName>
