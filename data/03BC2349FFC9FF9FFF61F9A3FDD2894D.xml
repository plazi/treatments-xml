<heading id="D0E22533FFC9FF9AFF61F9A3FD288D70" bold="true" box="[151,742,1547,1573]" fontSize="11" level="1" pageId="1" pageNumber="576" reason="1">
<emphasis id="B9614E4DFFC9FF9AFF61F9A3FD288D70" bold="true" box="[151,742,1547,1573]" pageId="1" pageNumber="576">
<taxonomicName id="4C15E9DCFFC9FF9AFF61F9A3FDCA8D70" authorityName="Harbach &amp; Culverwell" authorityYear="2014" box="[151,516,1547,1573]" class="Insecta" family="Culicidae" genus="Topomyia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="576" phylum="Arthropoda" rank="species" species="discors" subGenus="Miyagiella">
<emphasis id="B9614E4DFFC9FF9AFF61F9A3FEC38D70" bold="true" box="[151,269,1547,1573]" italics="true" pageId="1" pageNumber="576">Topomyia</emphasis>
(
<emphasis id="B9614E4DFFC9FF9AFEEBF9A3FE508D70" bold="true" box="[285,414,1547,1573]" italics="true" pageId="1" pageNumber="576">Miyagiella</emphasis>
) 
<emphasis id="B9614E4DFFC9FF9AFE59F9A3FDCA8D70" bold="true" box="[431,516,1547,1573]" italics="true" pageId="1" pageNumber="576">discors</emphasis>
</taxonomicName>
Harbach, 
<taxonomicNameLabel id="A252F336FFC9FF9AFD7FF9A3FD288D70" box="[649,742,1547,1573]" pageId="1" pageNumber="576" rank="species">sp. nov.</taxonomicNameLabel>
