<paragraph id="8BCB204315637348FE0EB4B35424FB2A" blockId="2.[463,821,1167,1191]" box="[463,821,1167,1191]" pageId="2" pageNumber="1956">
<heading id="D083972F15637348FE0EB4B35424FB2A" box="[463,821,1167,1191]" centered="true" fontSize="10" level="2" pageId="2" pageNumber="1956" reason="2">
<emphasis id="B900FC5115637348FE0EB4B355AEFB2A" bold="true" box="[463,703,1167,1191]" pageId="2" pageNumber="1956">
Genus 
<taxonomicName id="4C745BC015637348FDE8B4B355AEFB2A" authority="Panampunnayil &amp; Biju, 2007" authorityName="Panampunnayil &amp; Biju" authorityYear="2007" box="[553,703,1167,1191]" class="Malacostraca" family="Mysidae" genus="Kochimysis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mysida" pageId="2" pageNumber="1956" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B900FC5115637348FDE8B4B355AEFB2A" bold="true" box="[553,703,1167,1191]" italics="true" pageId="2" pageNumber="1956">Kochimysis</emphasis>
</taxonomicName>
