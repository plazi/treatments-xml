<collectingCountry id="3DF7F6DED82EFFD5FD3842D04717F85D" box="[686,812,2009,2034]" name="Philippines" pageId="62" pageNumber="63">Philippines</collectingCountry>
where the 
<typeStatus id="9A5B08ECD82EFFD5FC3142D347ECF85D" box="[935,983,2010,2034]" pageId="62" pageNumber="63">type</typeStatus>
