<taxonomicName id="4C274185D833FFA7BAD5E5E12917FE71" authorityName="Gordon and Hanley" authorityYear="2017" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="38" phylum="Arthropoda" rank="species" species="leona">
