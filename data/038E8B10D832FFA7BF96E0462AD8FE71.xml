<taxonomicName id="4C274185D833FFA7BD01E5C12AD1FE71" authorityName="Gorham" authorityYear="1897" box="[599,711,420,443]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="38" phylum="Arthropoda" rank="genus">
