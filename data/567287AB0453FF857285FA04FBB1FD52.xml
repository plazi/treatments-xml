<collectingCountry id="A6CC762D0450FF8573AEFD20FE54FD53" box="[444,511,649,672]" name="Japan" pageId="2" pageNumber="251">Japan</collectingCountry>
, New South 
<collectingCountry id="A6CC762D0450FF85709FFD21FD7AFD52" box="[653,721,648,673]" name="United Kingdom" pageId="2" pageNumber="251">Wales</collectingCountry>
<collectingCountry id="A6CC762D0450FF8570CAFD21FCEAFD52" box="[728,833,648,673]" name="Australia" pageId="2" pageNumber="251">Australia</collectingCountry>
, and 
<collectingCountry id="A6CC762D0450FF85716EFD20FBBDFD52" box="[892,1046,648,673]" name="New Zealand" pageId="2" pageNumber="251">New Zealand</collectingCountry>
