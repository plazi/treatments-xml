<paragraph id="8BF0368FFF9EFFE8F3C2FD4E2C4176F3" blockId="125.[151,585,691,716]" box="[151,585,691,716]" pageId="125" pageNumber="126">
<heading id="D0B881E3FF9EFFE8F3C2FD4E2C4176F3" bold="true" box="[151,585,691,716]" fontSize="10" level="2" pageId="125" pageNumber="126" reason="2">
<emphasis id="B93BEA9DFF9EFFE8F3C2FD4E2C4176F3" bold="true" box="[151,585,691,716]" pageId="125" pageNumber="126">
<taxonomicName id="4C4F4D0CFF9EFFE8F3C2FD4E2FAA76F4" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Fennah" baseAuthorityYear="1956" box="[151,418,691,716]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="guamana">
<emphasis id="B93BEA9DFF9EFFE8F3C2FD4E2FAA76F4" bold="true" box="[151,418,691,716]" italics="true" pageId="125" pageNumber="126">Saccharodite guamana</emphasis>
