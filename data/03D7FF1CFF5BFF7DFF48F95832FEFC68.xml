<taxonomicName id="4C7E3589FF5BFF7DFD72F9ED3288FD6B" authorityName="Burmeister" authorityYear="1844" box="[762,1012,643,667]" class="Insecta" family="Rutelidae" genus="Macraspis" kingdom="Animalia" order="Coleoptera" pageId="144" pageNumber="143" phylum="Arthropoda" rank="species" species="maculata">
