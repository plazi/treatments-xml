<taxonomicName id="4C7E3589FF5BFF7DFD6DF9CD35FEFD4B" authority="Soula, 1998" authorityName="Soula" authorityYear="1998" box="[741,1154,675,699]" class="Insecta" family="Rutelidae" genus="Macraspis" kingdom="Animalia" order="Coleoptera" pageId="144" pageNumber="143" phylum="Arthropoda" rank="subSpecies" species="maculata" subSpecies="crosarai">
