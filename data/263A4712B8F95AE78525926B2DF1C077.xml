<taxonomicName id="127A019D5A1D2DC7F2B4609DC4FE35D4" authorityName="Herbert &amp; Kilburn" authorityYear="1986" class="Gastropoda" family="Fissurellidae" genus="Fissurisepta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Fissurisepta onychoides" order="Lepetellida" pageId="0" pageNumber="49" phylum="Mollusca" rank="species" species="onychoides">
