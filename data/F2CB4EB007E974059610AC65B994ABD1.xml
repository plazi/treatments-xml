<taxonomicName id="747C4314131BB144BF412539B4FFA7D3" authorityName="da Costa" authorityYear="1901" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="55" pageNumber="56" phylum="Mollusca" rank="species" species="exoticus" subGenus="Drymaeus">
