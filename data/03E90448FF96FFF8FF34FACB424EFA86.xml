(
<emphasis id="B934694CFF97FFFEFE4DF858446CF817" box="[494,631,1966,1990]" italics="true" pageId="11" pageNumber="46">Trypaneoides</emphasis>
) 
<emphasis id="B934694CFF97FFFEFD24F858450AF817" box="[647,785,1966,1990]" italics="true" pageId="11" pageNumber="46">menglunensis</emphasis>
</taxonomicName>
<emphasis id="B934694CFF97FFFEFCB9F8584576F817" bold="true" box="[794,877,1966,1990]" pageId="11" pageNumber="46">
