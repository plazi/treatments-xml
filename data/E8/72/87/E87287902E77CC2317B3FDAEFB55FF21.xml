<paragraph blockId="7.[246,1646,1442,2140]" box="[753,1135,1447,1472]" pageId="7" pageNumber="146">
<emphasis bold="true" box="[753,1135,1447,1472]" pageId="7" pageNumber="146">
<taxonomicName authority="Leidy, 1856" authorityName="Leidy" authorityYear="1856" box="[753,1131,1447,1472]" class="Reptilia" family="Nodosauridae" genus="Palaeoscincus" higherTaxonomySource="CoL" kingdom="Animalia" order="Dinosauria" pageId="7" pageNumber="146" phylum="Chordata" rank="species" species="costatus">Palaeoscincus costatus</taxonomicName>
