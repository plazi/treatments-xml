<subSubSection pageId="4" pageNumber="265" type="materials_examined">
<paragraph blockId="4.[151,1437,151,680]" pageId="4" pageNumber="265">
<emphasis bold="true" box="[151,386,151,176]" pageId="4" pageNumber="265">Material examined.</emphasis>
