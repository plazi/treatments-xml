<taxonomicName authorityName="Scheffrahn" authorityYear="2020" class="Insecta" family="Kalotermitidae" genus="Rugitermes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="rufus" order="Blattodea" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="rufus">rufus</taxonomicName>
