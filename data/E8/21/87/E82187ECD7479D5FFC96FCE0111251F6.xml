<quantity box="[1036,1112,1186,1210]" metricMagnitude="-2" metricUnit="m" metricValue="5.4" pageId="3" pageNumber="794" unit="mm" value="54.0">54 mm</quantity>
), 
<specimenCount box="[1127,1225,1186,1210]" pageId="3" pageNumber="794" type="adult">two adult</specimenCount>
females (
<collectionCode box="[1323,1419,1186,1210]" pageId="3" pageNumber="794">USMHC</collectionCode>
2443, 
<collectionCode box="[873,924,1220,1244]" pageId="3" pageNumber="794">SVL</collectionCode>
<quantity box="[931,1009,1220,1244]" metricMagnitude="-2" metricUnit="m" metricValue="5.3" pageId="3" pageNumber="794" unit="mm" value="53.0">53 mm</quantity>
; 
<collectionCode box="[1020,1114,1220,1244]" pageId="3" pageNumber="794">USMHC</collectionCode>
2444, 
<collectionCode box="[1188,1239,1220,1244]" pageId="3" pageNumber="794">SVL</collectionCode>
<quantity box="[1246,1323,1220,1244]" metricMagnitude="-2" metricUnit="m" metricValue="4.4" pageId="3" pageNumber="794" unit="mm" value="44.0">44 mm</quantity>
