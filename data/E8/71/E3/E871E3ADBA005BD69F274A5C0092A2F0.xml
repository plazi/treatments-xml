<taxonomicName class="Agaricomycetes" family="Boletaceae" genus="Boletus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Boletus miniatoaurantiacus" order="Boletales" pageId="0" pageNumber="111" phylum="Basidiomycota" rank="species" species="miniatoaurantiacus">
