<taxonomicName class="Dicotyledoneae" family="Fagaceae" genus="Quercus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Fagales" pageId="19" pageNumber="32" phylum="Angiospermae" rank="species" species="coccinea">
