<taxonomicName authorityName="ab. astrapta Dannehl" authorityYear="1933" class="Insecta" family="Hepialidae" genus="Theresimima" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Theresimima ampellophaga" order="Lepidoptera" pageId="0" pageNumber="39" phylum="Arthropoda" rank="species" species="ampellophaga">
