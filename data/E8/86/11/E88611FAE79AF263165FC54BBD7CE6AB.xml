<paragraph pageId="1" pageNumber="38">
<taxonomicName class="Insecta" family="Tettigoniidae" genus="Griffinipteryx" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Griffinipteryx mukonja" order="Orthoptera" pageId="1" pageNumber="38" phylum="Arthropoda" rank="species" species="mukonja">Griffinipteryx mukonja</taxonomicName>
