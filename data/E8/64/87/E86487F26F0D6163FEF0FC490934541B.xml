<emphasis bold="true" box="[264,646,953,979]" pageId="1" pageNumber="16">
<taxonomicName authority="Malm &amp; Johanson, 2005" authorityName="Malm &amp; Johanson" authorityYear="2005" box="[264,488,953,979]" class="Insecta" family="Petrothrincidae" genus="Gyrocarisa" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="16" phylum="Arthropoda" rank="species" species="scottae" status="new species">
<emphasis bold="true" box="[264,488,953,979]" italics="true" pageId="1" pageNumber="16">Gyrocarisa scottae</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[502,646,953,979]" pageId="1" pageNumber="16" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="1.[264,646,953,1014]" box="[264,362,988,1014]" pageId="1" pageNumber="16">
