Is; Tahiti; 
<collectingCountry box="[782,912,303,327]" name="Philippines" pageId="14" pageNumber="1116">Philippines</collectingCountry>
; 
<collectingCountry box="[925,968,303,327]" name="Fiji" pageId="14" pageNumber="1116">Fiji</collectingCountry>
; 
<collectingCountry box="[980,1098,303,327]" name="Sri Lanka" pageId="14" pageNumber="1116">Sri Lanka</collectingCountry>
