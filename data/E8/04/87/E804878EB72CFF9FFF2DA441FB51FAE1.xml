<taxonomicName authorityName="K. J. Knight &amp; Lado" authorityYear="2020" box="[215,411,1490,1509]" class="Myxomycetes" family="Clastodermataceae" genus="Clastoderma" kingdom="Protozoa" order="Echinosteliales" pageId="3" pageNumber="38" phylum="Mycetozoa" rank="species" species="confusum">
