<taxonomicName authority="Wheeler" authorityName="Wheeler" box="[151,406,311,337]" class="Insecta" family="Chloropidae" genus="Medeventor" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="48" phylum="Arthropoda" rank="genus">
