<taxonomicName authority="Bernhauer" authorityName="Bernhauer" box="[639,931,689,714]" class="Insecta" family="Staphylinidae" genus="Xanthopygus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="133" phylum="Arthropoda" rank="species" species="cordovensis">
<emphasis box="[639,804,689,714]" italics="true" pageId="2" pageNumber="133">X. cordovensis</emphasis>
Bernhauer
</taxonomicName>
, 
<taxonomicName authority="Chatzimanolis" authorityName="Chatzimanolis" box="[942,1214,689,714]" class="Insecta" family="Staphylinidae" genus="Xenopygus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="133" phylum="Arthropoda" rank="species" species="davidi" status="sp. n.">
<emphasis box="[942,1044,689,714]" italics="true" pageId="2" pageNumber="133">X. davidi</emphasis>
Chatzimanolis
</taxonomicName>
<emphasis bold="true" box="[1222,1287,689,714]" pageId="2" pageNumber="133">
<taxonomicNameLabel box="[1222,1287,689,714]" pageId="2" pageNumber="133" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="Bernhauer" authorityName="Bernhauer" class="Insecta" family="Staphylinidae" genus="Dysanellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="133" phylum="Arthropoda" rank="species" species="excellens" status="comb. n.">
<emphasis box="[1301,1436,689,714]" italics="true" pageId="2" pageNumber="133">X. excellens</emphasis>
(Bernhauer)
</taxonomicName>
<emphasis bold="true" box="[296,401,725,750]" pageId="2" pageNumber="133">
<taxonomicNameLabel box="[296,401,725,750]" pageId="2" pageNumber="133">comb. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="Sharp" authorityName="Sharp" box="[418,656,725,750]" class="Insecta" family="Staphylinidae" genus="Gastrisus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="133" phylum="Arthropoda" rank="species" species="punctatus" status="comb. n.">
<emphasis box="[418,561,726,749]" italics="true" pageId="2" pageNumber="133">X. punctatus</emphasis>
(Sharp)
</taxonomicName>
<emphasis bold="true" box="[666,771,725,750]" pageId="2" pageNumber="133">
<taxonomicNameLabel box="[666,771,725,750]" pageId="2" pageNumber="133">comb. n.</taxonomicNameLabel>
</emphasis>
