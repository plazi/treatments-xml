<quantity box="[682,800,1549,1571]" metricMagnitude="-3" metricUnit="m" metricValue="5.45" metricValueMax="6.2" metricValueMin="4.7" pageId="9" pageNumber="260" unit="mm" value="5.45" valueMax="6.2" valueMin="4.7">4.7–6.2 mm</quantity>
)................................................... 
<number box="[1423,1435,1549,1571]" pageId="9" pageNumber="260" value="2.0">2</number>
</keyLead>
