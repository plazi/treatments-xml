, as 
<taxonomicName box="[933,1056,1643,1669]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="89" pageNumber="88" phylum="Arthropoda" rank="species" species="spinosus">
<emphasis box="[933,951,1643,1669]" italics="true" pageId="89" pageNumber="88">C</emphasis>
. 
<emphasis box="[964,1056,1643,1668]" italics="true" pageId="89" pageNumber="88">spinosus</emphasis>
</taxonomicName>
). 
<bibRefCitation author="Jones RH" box="[1078,1220,1642,1669]" pageId="89" pageNumber="88" pagination="702 - 710" refId="ref132807" refString="Jones RH. 1961 b. Observations on the larval habitats of some North American species of Culicoides (Diptera: Ceratopogonidae). Annals of the Entomological Society of America 54: 702 - 710." type="journal article" year="1961">Jones (1961b</bibRefCitation>
, as 
<taxonomicName box="[1258,1381,1643,1669]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="89" pageNumber="88" phylum="Arthropoda" rank="species" species="spinosus">
<emphasis box="[1258,1276,1643,1669]" italics="true" pageId="89" pageNumber="88">C</emphasis>
. 
<emphasis box="[1289,1381,1643,1668]" italics="true" pageId="89" pageNumber="88">spinosus</emphasis>
