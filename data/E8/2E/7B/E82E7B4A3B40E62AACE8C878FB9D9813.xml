<taxonomicName authorityName="Ashmead" authorityYear="1897" box="[211,320,707,731]" class="Insecta" family="Cynipidae" genus="Aulacidea" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="2955" phylum="Arthropoda" rank="genus">
<emphasis box="[211,320,707,731]" italics="true" pageId="20" pageNumber="2955">Aulacidea</emphasis>
</taxonomicName>
, 
<taxonomicName box="[335,424,707,731]" class="Trilobita" family="Isocolidae" genus="Isocolus" kingdom="Animalia" order="Ptychopariida" pageId="20" pageNumber="2955" phylum="Arthropoda" rank="genus">
<emphasis box="[335,424,707,731]" italics="true" pageId="20" pageNumber="2955">Isocolus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Forster" authorityYear="1869" box="[439,570,707,731]" class="Insecta" family="Cynipidae" genus="Liposthenes" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="2955" phylum="Arthropoda" rank="genus">
<emphasis box="[439,570,707,731]" italics="true" pageId="20" pageNumber="2955">Liposthenes</emphasis>
