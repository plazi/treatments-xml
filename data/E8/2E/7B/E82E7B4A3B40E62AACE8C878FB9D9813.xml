, 
<taxonomicName authorityName="Foerster" authorityYear="1860" box="[585,685,707,731]" class="Insecta" family="Cynipidae" genus="Phanacis" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="2955" phylum="Arthropoda" rank="genus">
