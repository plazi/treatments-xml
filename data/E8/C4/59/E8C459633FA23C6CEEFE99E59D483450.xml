3. 
<normalizedToken originalValue="Äußere" pageId="null" pageNumber="718">Aeussere</normalizedToken>
<normalizedToken originalValue="Perigonblätter" pageId="null" pageNumber="718">Perigonblaetter</normalizedToken>
zur Zeit der Fruchtreife den innern 
<normalizedToken originalValue="Perigonblättern" pageId="null" pageNumber="718">Perigonblaettern</normalizedToken>
anliegend
</td>
<td align="left" pageId="null" pageNumber="718" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="718" phylum="Tracheophyta" rank="species" species="scutatus">
