<geoCoordinate id="EE075048D064FFAA5CBA3F740848775E" box="[478,550,536,559]" degrees="1" direction="east" minutes="35" orientation="longitude" pageId="11" pageNumber="136" precision="925" value="1.5833333">
<emphasis id="B947EA9DD064FFAA5CBA3F740B93775E" box="[478,509,536,559]" italics="true" pageId="11" pageNumber="136">e1</emphasis>
35
</geoCoordinate>
–38, 
<emphasis id="B947EA9DD064FFAA5F0C3F7408EF775E" box="[616,641,536,559]" italics="true" pageId="11" pageNumber="136">f1</emphasis>
37–40, 
<emphasis id="B947EA9DD064FFAA5F883F740968775E" box="[748,774,536,559]" italics="true" pageId="11" pageNumber="136">f2</emphasis>
41–43, 
<emphasis id="B947EA9DD064FFAA5E153F7409FF775E" box="[881,913,536,559]" italics="true" pageId="11" pageNumber="136">h1</emphasis>
33–36, 
