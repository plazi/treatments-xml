<taxonomicName id="4C1A4D03E84D5843FF52ADBCFDE8FA4A" authority=": Tomasovic 2006: 148" authorityName=": Tomasovic" authorityPageNumber="148" authorityYear="2006" box="[144,624,1494,1516]" class="Insecta" family="Asilidae" genus="Congomochtherus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="343" phylum="Arthropoda" rank="species" species="russnota">
