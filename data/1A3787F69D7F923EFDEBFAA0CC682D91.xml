<subSubSection id="DA84656B9D7F9238FF2EF9ABCD742EE2" pageId="99" pageNumber="100" type="etymology">
<paragraph id="922136E09D7F9238FF2EF9ABC9122E4D" blockId="99.[189,324,1557,1583]" box="[189,324,1557,1583]" pageId="99" pageNumber="100">
<heading id="C969818C9D7F9238FF2EF9ABC9122E4D" bold="true" box="[189,324,1557,1583]" fontSize="11" level="3" pageId="99" pageNumber="100" reason="3">
