. 
<taxonomicName id="4C454D2CFF8DFFB0FC2DFC26F6C81A89" authorityName="Yamaguti" authorityYear="1942" box="[964,1176,996,1020]" class="Dothideomycetes" family="Massarinaceae" genus="Bertiella" kingdom="Fungi" order="Pleosporales" pageId="2" pageNumber="453" phylum="Ascomycota" rank="species" species="musasabi">
