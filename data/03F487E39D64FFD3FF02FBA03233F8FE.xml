<quantity id="4CA59B109D64FFD3FD34FB4E33D8FB66" box="[670,739,1184,1208]" metricMagnitude="2" metricUnit="m" metricValue="1.03" pageId="24" pageNumber="675" unit="m" value="103.0">103 m</quantity>
; 
<date id="FFE310359D64FFD3FD47FB4E3028FB08" pageId="24" pageNumber="675" value="2016-04-13">13 Apr. 2016</date>
; 
<emphasis id="B929EAE79D64FFD3FEB4FB2C30F3FB08" box="[286,456,1217,1241]" italics="true" pageId="24" pageNumber="675">
<collectionCode id="ED4CAE309D64FFD3FEB4FB2C300FFB08" box="[286,308,1218,1241]" country="Norway" lsid="urn:lsid:biocol.org:col:13093" name="Botanical Museum - University of Oslo" pageId="24" pageNumber="675" type="Herbarium">O</collectionCode>
