<taxonomicName id="2BF04D73AF63FF985F83FAB6DE1A3B3A" authority="Fairmaire (1886)" authorityName="Fairmaire" authorityYear="1886" box="[478,776,1286,1311]" class="Insecta" family="Pentatomidae" genus="Leprosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="olcesii">
