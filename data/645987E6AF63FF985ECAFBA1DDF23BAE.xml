<collectingCountry id="94E77660AF63FF985F17FA9BDC853B66" box="[330,407,1322,1347]" name="Russia" pageId="66" pageNumber="67">Russia</collectingCountry>
, and 
<taxonomicName id="2BF04D73AF63FF985F8BFA9DDE083B66" authority="Horvath (1911)" authorityName="Horvath" authorityYear="1911" box="[470,794,1322,1347]" class="Insecta" family="Pentatomidae" genus="Leprosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="inaequale">
