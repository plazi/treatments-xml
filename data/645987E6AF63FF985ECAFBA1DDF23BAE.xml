<collectingCountry id="94E77660AF63FF985DE0FAB6D9373B3B" box="[957,1061,1287,1310]" name="Morocco" pageId="66" pageNumber="67">Morocco</collectingCountry>
, 
<taxonomicName id="2BF04D73AF63FF985A6DFAB6D88E3B3A" authority="Jakovlev (1874)" authorityName="Jakovlev" authorityYear="1874" box="[1072,1436,1286,1311]" class="Insecta" family="Pentatomidae" genus="Ancyrosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="tuberculatum">
