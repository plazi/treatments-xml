<taxonomicName id="4C1AF0E45619FFAA364BDF5316EDFCDB" authority=", NMW. Z." authorityName="NMW. Z." box="[330,648,856,878]" class="Magnoliopsida" family="Asteraceae" genus="Glossocardia" kingdom="Plantae" order="Asterales" pageId="45" pageNumber="3219" phylum="Tracheophyta" rank="species" species="obesa">
