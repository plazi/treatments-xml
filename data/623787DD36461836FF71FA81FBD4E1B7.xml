<taxonomicName id="2D9E4D4836461836FF71FA0CFEA7E1D1" baseAuthorityName="PAYKULL" baseAuthorityYear="1796" box="[177,382,1438,1460]" class="Insecta" family="Staphylinidae" genus="Rugilus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="1246" phylum="Arthropoda" rank="species" species="orbiculatus">
