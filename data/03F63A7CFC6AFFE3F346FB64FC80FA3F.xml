<geoCoordinate id="EE6BEDADFC6AFFE3F60CFB37FB89FB30" box="[1054,1132,1217,1237]" degrees="18" direction="north" minutes="32" orientation="latitude" pageId="43" pageNumber="1052" precision="925" value="18.533333">18°32‘N</geoCoordinate>
