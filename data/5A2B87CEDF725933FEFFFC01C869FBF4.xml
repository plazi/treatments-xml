</taxonomicName>
sp. (
<taxonomicName id="15824D5BDF705933FDB0FCB0CB82FC01" authorityName="de Candolle" authorityYear="1806" box="[553,710,879,903]" class="Magnoliopsida" family="Passifloraceae" kingdom="Plantae" order="Malpighiales" pageId="10" pageNumber="7" phylum="Tracheophyta" rank="family">Passifloraceae</taxonomicName>
