<taxonomicName id="15824D5BDF705933FE1EFCB0C8B1FC01" authorityName="Linnaeus" authorityYear="1753" box="[391,501,879,903]" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" kingdom="Plantae" order="Malpighiales" pageId="10" pageNumber="7" phylum="Tracheophyta" rank="genus">
