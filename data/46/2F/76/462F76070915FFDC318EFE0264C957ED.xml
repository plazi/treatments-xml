</bibRefCitation>
. 
<taxonomicName authority="Sasa, Kawai et Ueno, 1988: 59" authorityName="Sasa, Kawai et Ueno" authorityPageNumber="59" authorityYear="1988" box="[151,809,565,588]" class="Insecta" family="Chironomidae" genus="Trissopelopia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="oyabetrispinosa">
