<taxonomicName authorityName="Brolemann" authorityYear="1931" box="[823,928,1584,1603]" class="Diplopoda" family="Julidae" genus="Cylindroiulus" kingdom="Animalia" order="Julida" pageId="36" pageNumber="1526" phylum="Arthropoda" rank="species" species="gauthieri">
