</taxonomicName>
and 
<taxonomicName box="[337,498,1947,1970]" class="Insecta" family="Termitidae" genus="Cyrilliotermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isoptera" pageId="11" pageNumber="36" phylum="Arthropoda" rank="species" species="strictinasus">
