</taxonomicName>
and 
<taxonomicName box="[340,502,1911,1934]" class="Insecta" family="Termitidae" genus="Cyrilliotermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isoptera" pageId="11" pageNumber="36" phylum="Arthropoda" rank="species" species="strictinasus">
<emphasis box="[340,502,1911,1934]" italics="true" pageId="11" pageNumber="36">C. strictinasus</emphasis>
</taxonomicName>
