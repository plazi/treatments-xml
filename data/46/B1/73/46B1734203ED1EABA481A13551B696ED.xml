<subSubSection pageId="null" pageNumber="627" type="vernacular_name">
<paragraph pageId="null" pageNumber="627">
Kleine 
<taxonomicName class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="627" phylum="Tracheophyta" rank="genus">Orchis</taxonomicName>
</paragraph>
