<location LSID="urn:lsid:plazi:treatment:465687ECAA6AFFDAB0D3B52BFD13F7BC:CB206021AA6AFFDAB178B6D1FE4DF739" box="[363,429,1772,1795]" country="Cuba" county="Specimens" municipality="Eastern" name="From" pageId="13" pageNumber="12" stateProvince="Holguin">From</location>
