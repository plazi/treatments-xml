<paragraph blockId="20.[192,1440,573,677]" pageId="20" pageNumber="19">
<materialsCitation collectingDate="1939-03" collectorName="J. G. Needham. The" location="Pinar del Rio, Santa Cruz" pageId="20" pageNumber="19" specimenCount="2" stateProvince="Pinar del Rio, Santa Cruz" typeStatus="holotype">
