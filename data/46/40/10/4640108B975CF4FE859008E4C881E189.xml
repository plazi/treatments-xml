<paragraph pageId="15" pageNumber="34">
<taxonomicName class="Lecanoromycetes" family="Curculionidae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xyleborus validus" order="Coleoptera" pageId="15" pageNumber="34" phylum="Ascomycota" rank="species" species="validus">Xyleborus validus</taxonomicName>
