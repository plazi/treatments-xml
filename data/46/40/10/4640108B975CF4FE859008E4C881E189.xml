<paragraph pageId="15" pageNumber="34">
<taxonomicName authority="Eichhoff, 1875" authorityName="Eichhoff" authorityYear="1875" class="Insecta" family="Curculionidae" genus="Euwallacea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euwallacea validus" order="Coleoptera" pageId="15" pageNumber="34" phylum="Arthropoda" rank="species" species="validus">Euwallacea validus (Eichhoff, 1875)</taxonomicName>
