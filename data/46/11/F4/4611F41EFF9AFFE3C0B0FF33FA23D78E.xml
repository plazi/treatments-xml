<number box="[1422,1435,992,1015]" pageId="16" pageNumber="61" value="8.0">8</number>
</keyLead>
</paragraph>
<paragraph blockId="16.[151,1437,224,2007]" pageId="16" pageNumber="61">
<keyLead pageId="16" pageNumber="61">
