<quantity box="[639,732,600,626]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="5" pageNumber="50" unit="mm" value="4.3">4.3 mm</quantity>
; abdominal ventrites: 
<number box="[991,1026,600,626]" pageId="5" pageNumber="50" value="58.0">58</number>
