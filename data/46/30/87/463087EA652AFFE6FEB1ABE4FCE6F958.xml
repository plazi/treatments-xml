<materialsCitation ID-GBIF-Occurrence="2987373304" collectionCode="QM" location="Dahms" pageId="6" pageNumber="686" specimenCode="Hy/2965" specimenCount="1" specimenCount-male="1" stateProvince="North Queensland" typeStatus="holotype">
