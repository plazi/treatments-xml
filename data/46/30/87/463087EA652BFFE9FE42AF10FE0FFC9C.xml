, 
<location LSID="urn:lsid:plazi:treatment:463087EA652BFFE9FE42AF10FE0FFC9C:CB466027652BFFE7FC70AC1EFBC9F9D5" box="[920,1024,1565,1590]" name="Chindera" pageId="7" pageNumber="687" stateProvince="QMB">Chindera</location>
