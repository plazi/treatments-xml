<materialsCitation ID-GBIF-Occurrence="2985536308" collectingDate="1919-03-06" collectionCode="QM" lastPageId="18" lastPageNumber="698" pageId="17" pageNumber="697" specimenCode="QM T4406" specimenCount="1" specimenCount-female="1" specimenCount-male="1" typeStatus="holotype">
