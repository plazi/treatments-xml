<materialsCitation ID-GBIF-Occurrence="2987373303" pageId="3" pageNumber="683" specimenCount="1" specimenCount-female="1" typeStatus="paralectotype">
<emphasis bold="true" box="[951,1155,900,925]" pageId="3" pageNumber="683">
