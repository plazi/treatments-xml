<emphasis bold="true" pageId="12" pageNumber="57">
<taxonomicName authority="WITH LONGITUDINAL PRONOTAL SULCI AND FROM" box="[452,580,1468,1492]" class="Insecta" family="Melolonthidae" genus="Dichelonyx" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="57" phylum="Arthropoda" rank="genus">
