<taxonomicName authority="Paramonov, 1953" authorityName="Paramonov" authorityYear="1953" box="[860,1202,271,296]" class="Insecta" family="Apioceridae" genus="Apiocera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="197" phylum="Arthropoda" rank="species" species="australis">
<emphasis box="[860,993,272,296]" italics="true" pageId="2" pageNumber="197">A. australis</emphasis>
<bibRefCitation author="Paramonov" box="[1004,1202,271,295]" pageId="2" pageNumber="197" refString="Paramonov, S. J. (1953) A review of Australian Apioceridae (Diptera). Australian Journal of Zoology, 1 (3), 449 - 536." type="journal article" year="1953">
Paramonov, 
<number box="[1146,1202,271,295]" pageId="2" pageNumber="197" value="1953.0">1953</number>
</bibRefCitation>
</taxonomicName>
(SA, VIC, WA), 
<taxonomicName authority="Paramonov, 1953" authorityName="Paramonov" authorityYear="1953" class="Insecta" family="Apioceridae" genus="Apiocera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="197" phylum="Arthropoda" rank="species" species="vespera">
