<geoCoordinate box="[151,276,692,716]" direction="north" orientation="latitude" pageId="5" pageNumber="28" precision="1" value="8.87157">
<number box="[151,247,692,716]" pageId="5" pageNumber="28" value="8.87157">8.87157</number>
°N
</geoCoordinate>
, 
<geoCoordinate box="[292,435,692,716]" direction="west" orientation="longitude" pageId="5" pageNumber="28" precision="1" value="-82.77099">
<number box="[292,399,692,716]" pageId="5" pageNumber="28" value="82.77099">82.77099</number>
