<paragraph pageId="3" pageNumber="295">
<table box="[163,1416,732,977]" gridcols="12" gridrows="4" pageId="3" pageNumber="295">
<tr box="[163,1416,732,763]" gridrow="0" pageId="3" pageNumber="295" rowspan-0="1">
