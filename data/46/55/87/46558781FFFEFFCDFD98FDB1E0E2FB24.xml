<taxonomicName box="[1172,1229,971,992]" class="Arachnida" family="Zerconidae" genus="Aspar" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="280" phylum="Arthropoda" rank="genus">
