<typeStatus box="[312,430,1404,1430]" pageId="23" pageNumber="24">Paratypes</typeStatus>
: 
<number box="[441,456,1404,1430]" pageId="23" pageNumber="24" value="2.0">2</number>
Ψ, 
<number box="[497,511,1404,1430]" pageId="23" pageNumber="24" value="2.0">2</number>
ɗ (
<collectionCode LSID="urn:lsid:biocol.org:col:34697" box="[554,642,1404,1430]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="23" pageNumber="24">BPBM</collectionCode>
). 
<number box="[661,676,1404,1430]" pageId="23" pageNumber="24" value="1.0">1</number>
Ψ, 
<number box="[718,733,1404,1430]" pageId="23" pageNumber="24" value="1.0">1</number>
ɗ (
