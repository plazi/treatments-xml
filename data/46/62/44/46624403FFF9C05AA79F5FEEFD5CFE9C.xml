<emphasis bold="true" box="[151,299,1830,1855]" pageId="1" pageNumber="298">
<typeStatus box="[151,210,1831,1855]" pageId="1" pageNumber="298">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName box="[313,501,1830,1854]" class="Insecta" family="Grammolingiidae" genus="Protolingia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="298" phylum="Arthropoda" rank="species" species="mira">
