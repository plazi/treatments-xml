<taxonomicName box="[749,845,1433,1458]" class="Malacostraca" family="Paramunnidae" genus="Xigonus" kingdom="Animalia" order="Isopoda" pageId="26" pageNumber="47" phylum="Arthropoda" rank="genus">
<emphasis box="[749,845,1433,1458]" italics="true" pageId="26" pageNumber="47">Xigonus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[865,1012,1433,1458]" class="Malacostraca" family="Paramunnidae" genus="Austronanus" kingdom="Animalia" order="Isopoda" pageId="26" pageNumber="47" phylum="Arthropoda" rank="genus">
<emphasis box="[865,1012,1433,1458]" italics="true" pageId="26" pageNumber="47">Austronanus</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1078,1272,1433,1458]" class="Malacostraca" family="Paramunnidae" genus="Stephenseniellus" kingdom="Animalia" order="Isopoda" pageId="26" pageNumber="47" phylum="Arthropoda" rank="genus">
<emphasis box="[1078,1272,1433,1458]" italics="true" pageId="26" pageNumber="47">Stephenseniellus</emphasis>
</taxonomicName>
see remarks under 
<taxonomicName box="[440,587,1473,1498]" class="Malacostraca" family="Paramunnidae" genus="Austronanus" kingdom="Animalia" order="Isopoda" pageId="26" pageNumber="47" phylum="Arthropoda" rank="genus">
