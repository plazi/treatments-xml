<subSubSection box="[312,1026,1113,1139]" pageId="26" pageNumber="47" type="etymology">
<paragraph blockId="26.[264,1323,1033,1499]" box="[312,1026,1113,1139]" pageId="26" pageNumber="47">
<emphasis bold="true" box="[312,445,1113,1139]" pageId="26" pageNumber="47">Etymology</emphasis>
