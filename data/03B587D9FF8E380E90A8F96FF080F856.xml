<quantity id="4CE49B2AFF89380F92ACFD95F191FDE7" box="[659,756,552,578]" metricMagnitude="-1" metricUnit="m" metricValue="5.5" pageId="28" pageNumber="59" unit="mm" value="550.0">550 mm</quantity>
