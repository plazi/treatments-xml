<taxonomicName id="068B4D6CDB79FF91FCBFAB66FBD9F8B3" authorityName="Weise" authorityYear="1889" box="[874,1090,1833,1856]" class="Insecta" family="Chrysomelidae" genus="Liroetis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="535" phylum="Arthropoda" rank="species" species="aeneipennis">
<emphasis id="F3FFEAFDDB79FF91FCBFAB66FBD9F8B3" box="[874,1090,1833,1856]" italics="true" pageId="6" pageNumber="535">Liroetis aeneipennis</emphasis>
