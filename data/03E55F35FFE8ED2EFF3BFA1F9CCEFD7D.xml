locality in 
<collectingCountry id="F35BAEB3FFE9ED2EFF5AFD1F9C1DFD51" box="[236,300,689,713]" name="Brazil" pageId="85">Brazil</collectingCountry>
(
<collectingRegion id="498820C1FFE9ED2EFE8DFD1F9C5EFD51" box="[315,367,689,713]" country="Brazil" name="Para" pageId="85">Pará</collectingRegion>
