<subSubSection id="E3D8A6C946421703FF45F9BC5AF4451C" pageId="8" pageNumber="9" type="materials_examined">
<paragraph id="AB7DF54246421703FF45F9BC5FAB4479" blockId="8.[151,1437,224,1879]" box="[197,1152,1621,1647]" pageId="8" pageNumber="9">
<emphasis id="99B6295046421703FF45F9BC5A6B4479" bold="true" box="[197,320,1621,1647]" pageId="8" pageNumber="9">
<typeStatus id="74794BE046421703FF45F9BC5A174479" box="[197,316,1621,1647]" pageId="8" pageNumber="9">Holotype</typeStatus>
:
