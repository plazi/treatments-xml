</taxonomicName>
on 
<taxonomicName id="D7D44D10FFB40616FCADA8B54100D0BA" authority="Decne." box="[820,1090,1158,1183]" class="Magnoliopsida" family="Plantaginaceae" genus="Plantago" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="15" pageNumber="316" phylum="Magnoliopsida" rank="species" species="rugelii">
