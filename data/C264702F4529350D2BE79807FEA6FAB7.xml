of 
<taxonomicName id="8DCDBABA4529350D2A169A38FDD6FCC7" authorityName="Moser" authorityYear="1913" box="[358,585,849,876]" class="Insecta" family="Melolonthidae" genus="Lepidiota" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="470" phylum="Arthropoda" rank="species" species="rugicollis">
