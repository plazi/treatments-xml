<taxonomicName id="A1C1D392A041FFE3FD70C7ABFBE3BABF" authority="Lynch, 1998" authorityName="Lynch" authorityYear="1998" box="[655,1142,222,247]" class="Amphibia" family="Strabomantidae" genus="Pristimantis" kingdom="Animalia" order="Anura" pageId="7" phylum="Chordata" rank="species" species="angustilineatus">
