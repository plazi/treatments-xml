<paragraph id="E7726A0D5E5AFFB5FC9DF92EFBE2F8A5" blockId="26.[806,1421,1788,1902]" box="[806,1080,1788,1810]" pageId="26" pageNumber="55">
<heading id="BC3ADD615E5AFFB5FC9DF92EFBE2F8A5" bold="true" box="[806,1080,1788,1810]" fontSize="9" level="5" pageId="26" pageNumber="55" reason="6">
<emphasis id="D5B9B61F5E5AFFB5FC9DF92EFBE2F8A5" bold="true" box="[806,1080,1788,1810]" italics="true" pageId="26" pageNumber="55">Type material examined</emphasis>
