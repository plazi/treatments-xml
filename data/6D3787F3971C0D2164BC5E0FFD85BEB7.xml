<paragraph id="E52136E5971C0D2364BC5E0FFC0FBB77" blockId="44.[192,984,1826,1883]" box="[192,984,1826,1851]" pageId="44" pageNumber="43">
<heading id="BE698189971C0D2364BC5E0FFC0FBB77" bold="true" box="[192,984,1826,1851]" fontSize="10" level="2" pageId="44" pageNumber="43" reason="3">
