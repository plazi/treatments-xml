<quantity id="4CEF9B25FF34FF109CEAFEF1FEB93118" box="[1175,1272,259,285]" metricMagnitude="-1" metricUnit="m" metricValue="3.91" pageId="165" pageNumber="166" unit="cm" value="39.1">39.1 cm</quantity>
