<collectionCode id="ED08AE21F663FF9E1559FF3176C0FF38" box="[324,413,188,212]" pageId="5" pageNumber="587">MHBU</collectionCode>
); Guizhou province, Suiyang, Kuankuoshui Reserve, Xiangshuwan Village, 
<quantity id="4CE19B01F663FF9E10EBFF317224FF38" box="[1270,1401,188,212]" metricMagnitude="3" metricUnit="m" metricValue="1.02" metricValueMax="1.2" metricValueMin="0.84" pageId="5" pageNumber="587" unit="m" value="1020.0" valueMax="1200.0" valueMin="840.0">840-1200m</quantity>
, 
<date id="FFA71024F663FF9E1198FF3177ACFF14" pageId="5" pageNumber="587" value="2010-06-09">
