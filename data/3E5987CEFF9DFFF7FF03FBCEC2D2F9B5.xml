<taxonomicName id="71F04D5BFF9DFFF7FF03FBE2C60FFB1B" box="[151,321,1145,1170]" class="Insecta" family="Haglotettigoniidae" genus="Kevanemobius" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="30" phylum="Arthropoda" rank="genus" status="gen. n.">
<emphasis id="8484EACAFF9DFFF7FF03FBE2C60FFB1B" bold="true" box="[151,321,1145,1170]" italics="true" pageId="11" pageNumber="30">Kevanemobius</emphasis>
</taxonomicName>
