<taxonomicName id="71F04D5BFF9DFFF7FD9DFBCEC58CFBE7" box="[521,706,1109,1134]" class="Insecta" family="Trigonidiidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="30" phylum="Arthropoda" rank="tribe" tribe="Pteronemobiini">Pteronemobiini</taxonomicName>
