<collectingCountry id="CEE77648FF9DFFF7FBBDFBCEC33CFBE7" box="[1065,1138,1109,1134]" name="Brazil" pageId="11" pageNumber="30">Brazil</collectingCountry>
comprising 
<taxonomicName id="71F04D5BFF9DFFF7FA93FBCEC2D8FBE7" box="[1287,1430,1109,1134]" class="Insecta" family="Haglotettigoniidae" genus="Amanayara" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="30" phylum="Arthropoda" rank="genus">Amanayara</taxonomicName>
,
</emphasis>
</heading>
</paragraph>
