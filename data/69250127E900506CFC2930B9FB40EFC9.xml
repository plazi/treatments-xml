(
<figureCitation id="79B7ACB4E900506BFC4D335FFB8CE86E" box="[979,1088,1566,1588]" captionStart="Figure 15" captionStartId="32.[144,228,1610,1629]" captionTargetBox="[144,1424,192,1579]" captionTargetId="figure-66@32.[144,1424,192,1580]" captionTargetPageId="32" captionText="Figure 15. Habitus dorsal: A, Heraeus annulatus sp. nov.; B, Heraeus antennalis sp. nov.; C, Heraeus chamamecinus sp. nov.; D, Heraeus illitus Distant, 1882; E, Heraeus inca sp. nov.; F, Heraeus panamaensis sp. nov.; G, Heraeus similis sp. nov.; H, Heraeus spinosus sp. nov. I, Heraeus splendens sp. nov.; J, Heraeus tiputini sp. nov." pageId="35" pageNumber="64">FIGS 15B</figureCitation>
, 
<figureCitation id="79B7ACB4E900506BFBD3335FFBB2E86E" box="[1101,1150,1566,1588]" captionStart="Figure 16" captionStartId="33.[164,241,1062,1081]" captionTargetBox="[325,1281,195,1030]" captionTargetId="figure-363@33.[323,1283,193,1032]" captionTargetPageId="33" captionText="Figure 16. Legs: A, Heraeus annulatus sp. nov.; B, Heraeus antennalis sp. nov.; C, Heraeus chamamecinus sp. nov.; D, Heraeus illitus Distant, 1882; E, Heraeus inca sp. nov.; F, Heraeus panamaensis sp. nov." pageId="35" pageNumber="64">16B</figureCitation>
, 
<figureCitation id="79B7ACB4E900506BFB15335FFB13E869" box="[1163,1247,1566,1587]" captionStart="Figure 17" captionStartId="34.[144,223,1714,1733]" captionTargetBox="[241,1332,196,1675]" captionTargetId="figure-0@34.[229,1338,192,1684]" captionTargetPageId="34" captionText="Figure 17. Male genitalia. Heraeus annulatus sp. nov.: A, pygophore, dorsal view; B, pygophore, lateral view; C, right paramere, inner view; D, right paramere, external view. Heraeus antennalis sp. nov.: E, pygophore, dorsal view; F, pygophore, lateral view; G, right paramere, inner view; H, right paramere, external view. Heraeus chamamecinus sp. nov.: I, pygophore, dorsal view; J, pygophore, lateral view; K, right paramere, inner view; L, right paramere, external view. Heraeus illitus Distant, 1882: M, pygophore, dorsal view; N, pygophore, lateral view; O, right paramere, inner view; P, right paramere, external view." pageId="35" pageNumber="64">17E–H</figureCitation>
