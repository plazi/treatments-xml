<quantity id="3D129B44D26F505EBDE828B88117FA65" box="[762,851,1410,1434]" metricMagnitude="-3" metricUnit="m" metricValue="9.8" pageId="24" pageNumber="225" unit="mm" value="9.8">9.8 mm</quantity>
