<number id="DE1C24E8FFF5651B6B44FA82BA94852F" box="[873,904,1344,1368]" pageId="23" pageNumber="424" value="32.0">32</number>
, n = 
<number id="DE1C24E8FFF5651B6BE7FA82BAFB852F" box="[970,999,1344,1368]" pageId="23" pageNumber="424" value="11.0">11</number>
