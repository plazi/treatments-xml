<emphasis id="B978EAE5DD64FFB3DC96FB6EFE98FB12" bold="true" box="[177,279,1241,1263]" pageId="8" pageNumber="493">Figs 15-23</emphasis>
: 
<taxonomicName id="4C0C4D74DD64FFB3DD02FB6DFE67FB12" authority="Assing, 2009" authorityName="Assing" authorityYear="2009" box="[293,488,1242,1262]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="493" phylum="Arthropoda" rank="species" species="bicoloricornis" status="nov.sp.">
<emphasis id="B978EAE5DD64FFB3DD02FB6DFE67FB12" box="[293,488,1242,1262]" italics="true" pageId="8" pageNumber="493">Atheta bicoloricornis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24B579EDD64FFB3DDD6FB6DFDB8FB12" box="[497,567,1242,1262]" pageId="8" pageNumber="493" rank="species">nov.sp.</taxonomicNameLabel>
: (
<emphasis id="B978EAE5DD64FFB3DE6EFB6EFDEFFB12" bold="true" box="[585,608,1241,1262]" pageId="8" pageNumber="493">15</emphasis>
) habitus; (
<emphasis id="B978EAE5DD64FFB3DEE0FB6EFD51FB12" bold="true" box="[711,734,1241,1262]" pageId="8" pageNumber="493">16</emphasis>
) forebody; (
<emphasis id="B978EAE5DD64FFB3DF72FB6EFCE3FB12" bold="true" box="[853,876,1241,1262]" pageId="8" pageNumber="493">17</emphasis>
) head in lateral view; (
<emphasis id="B978EAE5DD64FFB3DC9EFB43FF5FFAF5" bold="true" box="[185,208,1268,1289]" pageId="8" pageNumber="493">18</emphasis>
) antenna; (
<emphasis id="B978EAE5DD64FFB3DD1CFB43FEDDFAF5" bold="true" box="[315,338,1268,1289]" pageId="8" pageNumber="493">19</emphasis>
