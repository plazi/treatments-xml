<number id="3DDF24C3FFA1C6247C34F9FEFB3F59A3" box="[1146,1178,1651,1677]" pageId="18" pageNumber="19" value="13.0">13</number>
; 
<number id="3DDF24C3FFA1C6247CE9F9FEFB6259A3" box="[1191,1223,1651,1677]" pageId="18" pageNumber="19" value="15.0">15</number>
; 
<number id="3DDF24C3FFA1C6247C9AF9FEFB5059A3" box="[1236,1269,1651,1677]" pageId="18" pageNumber="19" value="35.0">35</number>
. Palpomere 2, 3 and 
<number id="3DDF24C3FFA1C6247890F917FF48599A" box="[222,237,1690,1716]" pageId="18" pageNumber="19" value="4.0">4</number>
