<taxonomicName id="E58D4D2AFFC3BB5D34C9FA0DFD91DA9F" box="[403,618,1425,1447]" class="Magnoliopsida" family="Casuarinaceae" genus="Casuarina" kingdom="Plantae" order="Fagales" pageId="2" pageNumber="89" phylum="Tracheophyta" rank="species" species="equisetifolia">
