<number id="3D8F24C8FF81DE0F71A07FADFE5BFE06" box="[409,481,444,470]" pageId="6" pageNumber="53" value="34.5" valueMax="37.0" valueMin="32.0">32–37</number>
ventral and dorsal fringe setae. Dimensions: TW, 
<number id="3D8F24C8FF81DE0F74577FADFB5DFE06" box="[1134,1255,444,470]" pageId="6" pageNumber="53" value="0.46499999999999997" valueMax="0.47" valueMin="0.46">0.46–0.47</number>
; HL, 
<number id="3D8F24C8FF81DE0F71317FF5FE3BFE2E" box="[264,385,484,510]" pageId="6" pageNumber="53" value="0.315" valueMax="0.32" valueMin="0.31">0.31–0.32</number>
; PW, 
<number id="3D8F24C8FF81DE0F71FA7FF5FD87FE2E" box="[451,573,484,510]" pageId="6" pageNumber="53" value="0.3" valueMax="0.31" valueMin="0.29">0.29–0.31</number>
; MW, 
<number id="3D8F24C8FF81DE0F72B07FF5FD7AFE2E" box="[649,704,484,510]" pageId="6" pageNumber="53" value="0.48">0.48</number>
; 
<collectionCode id="ED56AE2AFF81DE0F72F27FF5FCA2FE2E" box="[715,792,484,510]" pageId="6" pageNumber="53">AWIV</collectionCode>
, 
<number id="3D8F24C8FF81DE0F731A7FF5FC21FE2E" box="[803,923,484,510]" pageId="6" pageNumber="53" value="0.645" valueMax="0.65" valueMin="0.64">0.64–0.65</number>
; ANW, 
<number id="3D8F24C8FF81DE0F73C17FF5FBCBFE2E" box="[1016,1137,484,510]" pageId="6" pageNumber="53" value="0.225" valueMax="0.23" valueMin="0.22">0.22–0.23</number>
