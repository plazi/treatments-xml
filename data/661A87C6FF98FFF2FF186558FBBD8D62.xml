), 
<collectingCountry id="96A47640FF99FFF2FDCE63A3FD2B8FBE" box="[577,644,434,460]" name="China" pageId="11" pageNumber="82">China</collectingCountry>
(
<bibRefCitation id="8A224B21FF99FFF2FD1C63A3FCE18FBE" author="Ma, L. - M. &amp; Yin, X. - Q." box="[659,846,434,460]" pageId="11" pageNumber="82" pagination="119 - 122" refId="ref11431" refString="Ma, L. - M. &amp; Yin, X. - Q. (2011) Mesostigmata (Acari) in grassland soil of Changling in Jilin Province. Acta Arachnologica Sinica, 20, 119 - 122. [In Chinese]" type="journal article" year="2011">Ma &amp; Yin, 2011</bibRefCitation>
), 
<collectingCountry id="96A47640FF99FFF2FCEE63A3FC6A8FBE" box="[865,965,434,460]" name="Tanzania" pageId="11" pageNumber="82">Zanzibar</collectingCountry>
(
<bibRefCitation id="8A224B21FF99FFF2FC5C63A3FB158FBE" author="Joharchi, O. &amp; Halliday, B. &amp; Khaustov, A. A. &amp; Ermilov, S. G." box="[979,1210,434,460]" pageId="11" pageNumber="82" pagination="23 - 40" refId="ref10713" refString="Joharchi, O., Halliday, B., Khaustov, A. A. &amp; Ermilov, S. G. (2018) Some soil-inhabiting mites from Zanzibar (Acari: Laelapidae). Zootaxa, 4514 (1), 23 - 40. http: // doi. org / 10.11646 / zootaxa. 4514.1.2" type="journal article" year="2018">
Joharchi 
<emphasis id="DCC7EAC2FF99FFF2FBB563A2FBC48FBE" box="[1082,1131,434,460]" italics="true" pageId="11" pageNumber="82">et al</emphasis>
., 2018
</bibRefCitation>
) and 
<collectingCountry id="96A47640FF99FFF2FB7763A3FA8A8FBE" box="[1272,1317,434,460]" name="Iran" pageId="11" pageNumber="82">Iran</collectingCountry>
(
<bibRefCitation id="8A224B21FF99FFF2FABC63A3FF508F83" author="Khezri, A. &amp; Ostovan, H. &amp; Kamali, K. &amp; Al Mansur, H." pageId="11" pageNumber="82" pagination="192" refId="ref11191" refString="Khezri, A., Ostovan, H., Kamali, K. &amp; Al Mansur, H. (2006) Some edaphic mites from Iran. In: Manzari, S. (Eds). Abstracts and Proceedings of the 17 th Iranian Plant Protection Congress. Karaj, Iran, p. 192." type="book chapter" year="2006">
Khezri 
<emphasis id="DCC7EAC2FF99FFF2FA0763A2FF028F82" italics="true" pageId="11" pageNumber="82">et al</emphasis>
., 2006
</bibRefCitation>
