<paragraph id="EE0C36D0FF99FFF2FF486357FB9C8C4A" blockId="11.[151,1437,253,784]" pageId="11" pageNumber="82">
<emphasis id="DCC7EAC2FF99FFF2FF486357FEB68F12" bold="true" box="[199,281,326,352]" pageId="11" pageNumber="82">Notes.</emphasis>
<taxonomicName id="29B34D53FF99FFF2FEAE6357FDE58F12" baseAuthorityName="Willmann" baseAuthorityYear="1949" box="[289,586,326,352]" class="Arachnida" family="Laelapidae" genus="Gaeolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="11" pageNumber="82" phylum="Arthropoda" rank="species" species="praesternalis">
