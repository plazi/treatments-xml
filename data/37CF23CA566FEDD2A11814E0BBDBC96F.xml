<taxonomicName id="4635C4797812B0ECC4A1AEE17C9D686A" class="Insecta" genus="P." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="P. discicollis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="discicollis">
