<taxonomicName id="C8E54D13FFEAFFFF52DB1DD92B2AFA67" authorityName="Santschi" authorityYear="1926" box="[651,771,1444,1465]" class="Insecta" family="Formicidae" genus="Lepisiota" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="543" phylum="Arthropoda" rank="genus">
