<quantity id="5C719B6EFFABFF83FD65FBF569DBFB06" box="[664,759,1135,1159]" metricMagnitude="-2" metricUnit="m" metricValue="3.79" pageId="20" pageNumber="443" unit="mm" value="37.9">37.9 mm</quantity>
