<quantity id="5C719B6EFFABFF83FE0FFB78697DFB79" box="[498,593,1250,1274]" metricMagnitude="-2" metricUnit="m" metricValue="1.83" pageId="20" pageNumber="443" unit="mm" value="18.3">18.3 mm</quantity>
