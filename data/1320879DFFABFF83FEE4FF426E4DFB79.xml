<quantity id="5C719B6EFFABFF83FC27FB5F6F15FB5C" box="[986,1081,1221,1245]" metricMagnitude="-2" metricUnit="m" metricValue="4.1899999999999995" pageId="20" pageNumber="443" unit="mm" value="41.9">41.9 mm</quantity>
