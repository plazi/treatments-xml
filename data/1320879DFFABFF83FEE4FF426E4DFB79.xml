<quantity id="5C719B6EFFABFF83FCA3FB116892FB23" box="[862,958,1163,1187]" metricMagnitude="-2" metricUnit="m" metricValue="1.67" pageId="20" pageNumber="443" unit="mm" value="16.7">16.7 mm</quantity>
