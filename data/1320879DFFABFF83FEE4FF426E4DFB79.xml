<quantity id="5C719B6EFFABFF83FD7AFB5F69C9FB5C" box="[647,741,1221,1245]" metricMagnitude="-2" metricUnit="m" metricValue="4.15" pageId="20" pageNumber="443" unit="mm" value="41.5">41.5 mm</quantity>
