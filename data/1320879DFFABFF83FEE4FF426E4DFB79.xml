<taxonomicName id="5C894D08FFABFF83FB97FB166E4DFB23" authority="Allen, 1904" authorityName="Allen" authorityYear="1904" box="[1130,1377,1163,1187]" class="Mammalia" family="Molossidae" genus="Molossus" kingdom="Animalia" order="Chiroptera" pageId="20" pageNumber="443" phylum="Chordata" rank="species" species="coibensis">
