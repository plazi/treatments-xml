<taxonomicName id="7EC64D66FFBFFFDAFE57FEEDCF9EFEC0" authority="Humbert &amp; Saussure, 1869" authorityName="Humbert &amp; Saussure" authorityYear="1869" box="[415,963,357,383]" class="Diplopoda" family="Parajulidae" genus="Parajulus" kingdom="Animalia" order="Julida" pageId="6" pageNumber="67" phylum="Arthropoda" rank="species" species="olmecus">
