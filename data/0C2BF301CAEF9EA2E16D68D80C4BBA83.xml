<taxonomicName id="07AF3EAB432482AED73F4FDADEB0F5AE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:35407" class="Insecta" family="Formicidae" genus="Ponera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ponera affinis Jerdon" lsidName-HNS="Ponera affinis Jerdon" order="Hymenoptera" pageNumber="118" phylum="Arthropoda" rank="species" species="affinis">Ponera affinis</taxonomicName>
