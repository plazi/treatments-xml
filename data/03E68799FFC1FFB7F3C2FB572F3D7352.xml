<taxonomicName id="4C4F4D0CFFC1FFB7F21CF8F02C3A731A" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[329,562,1805,1829]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="sublimis">
