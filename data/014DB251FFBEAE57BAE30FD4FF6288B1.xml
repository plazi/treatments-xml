<tr id="37D40105FFBE51AABAD20C55FAFB8F23" box="[166,1382,864,889]" gridrow="5" pageId="2" pageNumber="550">
<th id="74056879FFBE51AABAD20C55FD3C8F23" box="[166,673,864,889]" gridcol="0" gridrow="5" pageId="2" pageNumber="550">hind tibiae (mm)</th>
<td id="74056879FFBE51AAB9EC0C55FBBD8F23" box="[920,1056,864,889]" gridcol="1" gridrow="5" pageId="2" pageNumber="550">1.30–1.85</td>
<td id="74056879FFBE51AABEDC0C55FAFB8F23" box="[1192,1382,864,889]" gridcol="2" gridrow="5" pageId="2" pageNumber="550">1.80–2.03</td>
