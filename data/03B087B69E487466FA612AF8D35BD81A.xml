<taxonomicName id="4C194D239E487467FA612AB5D1FADFB6" box="[264,452,1753,1778]" class="Insecta" family="Reduviidae" genus="Rasahus" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="surinamensis">
<emphasis id="B96DEAB29E487467FA612AB5D1FADFB6" box="[264,452,1753,1778]" italics="true" pageId="30" pageNumber="31">R. surinamensis</emphasis>
</taxonomicName>
