<subSubSection id="C37689C76616B46EC1FB4D373A7B279D" pageId="67" pageNumber="68" type="biology_ecology">
<paragraph id="8BD3DA4C6616B46EC1FB4D373A7B279D" blockId="67.[151,1437,1103,1741]" pageId="67" pageNumber="68">
<emphasis id="B918065E6616B46EC1FB4D37393D2731" bold="true" box="[199,295,1607,1632]" pageId="67" pageNumber="68">Biology.</emphasis>
