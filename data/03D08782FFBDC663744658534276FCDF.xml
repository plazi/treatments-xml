<taxonomicName id="4C794D17FFBEC66376B95E654276F992" box="[433,620,894,914]" class="Insecta" family="Miridae" genus="Abuyogocoris" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="18" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="B90DEA86FFBEC66376B95E654226F992" box="[433,572,894,914]" italics="true" pageId="18" pageNumber="18">Abuyogocoris</emphasis>
