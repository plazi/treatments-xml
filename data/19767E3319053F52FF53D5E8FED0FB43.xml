<taxonomicName id="56DFB4A619053F50FE35D2B6FDBBF8B0" box="[494,636,1790,1813]" class="Rhabditophora" family="Dalyelliidae" genus="Gieysztoria" kingdom="Animalia" order="Rhabdocoela" pageId="4" pageNumber="489" phylum="Platyhelminthes" rank="species" species="ramayana">
<emphasis id="A3AB133719053F50FE35D2B6FDC6F8B0" box="[494,513,1790,1813]" italics="true" pageId="4" pageNumber="489">G</emphasis>
