</taxonomicName>
, 
<taxonomicName id="56DFB4A619053F50FAC0D293FA62F956" baseAuthorityName="Ruebush &amp; Hayes" baseAuthorityYear="1939" box="[1307,1445,1755,1779]" class="Rhabditophora" family="Dalyelliidae" genus="Gieysztoria" kingdom="Animalia" order="Rhabdocoela" pageId="4" pageNumber="489" phylum="Platyhelminthes" rank="species" species="okugawai">
<emphasis id="A3AB133719053F50FAC0D293FAE9F957" box="[1307,1326,1755,1778]" italics="true" pageId="4" pageNumber="489">G</emphasis>
