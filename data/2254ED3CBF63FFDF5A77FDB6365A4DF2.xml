(
<quantity id="6D05F1CFBF63FFDF5906FA0337A84E92" box="[633,710,1508,1534]" metricMagnitude="2" metricUnit="m" metricValue="6.22" pageId="14" pageNumber="15" unit="m" value="622.0">622 m</quantity>
