<collectingRegion id="CF6BE1E4FFEE7F57E74F684EA9A5FB1E" box="[319,416,1132,1154]" country="United States of America" name="California" pageId="1" pageNumber="569">California</collectingRegion>
to 
<collectingRegion id="CF6BE1E4FFEE7F57E7CF684EAA60FB1E" box="[447,613,1132,1154]" country="Canada" name="British Columbia" pageId="1" pageNumber="569">British Columbia</collectingRegion>
