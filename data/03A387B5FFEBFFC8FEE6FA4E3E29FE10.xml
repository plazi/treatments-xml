<taxonomicName id="4C0A4D20FFEAFFB7FCB2F9A039BFF936" authorityName="Moulton" authorityYear="1930" box="[860,974,1649,1672]" class="Insecta" family="Thripidae" genus="Mycterothrips" kingdom="Animalia" order="Thysanoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="flavens">
