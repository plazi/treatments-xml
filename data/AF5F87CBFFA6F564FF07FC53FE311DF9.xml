<quantity id="E00E9B38FFA6F567FEB5F961FE6B1D8B" box="[293,410,1687,1712]" metricMagnitude="-3" metricUnit="m" metricValue="3.25" metricValueMax="3.5" metricValueMin="3.0" pageId="13" pageNumber="14" unit="mm" value="3.25" valueMax="3.5" valueMin="3.0">3â€“3.5 mm</quantity>
