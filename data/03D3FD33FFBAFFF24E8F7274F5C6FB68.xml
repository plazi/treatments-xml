<number id="3DB25E02FFBBFFF24E8F7305F6BAFB90" box="[151,181,1087,1112]" pageId="25" pageNumber="226" value="47.0">47</number>
º 
<number id="3DB25E02FFBBFFF24EA67305F6D6FB90" box="[190,217,1087,1112]" pageId="25" pageNumber="226" value="16.0">16</number>
