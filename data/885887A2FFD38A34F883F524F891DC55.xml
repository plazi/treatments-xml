. 
<collectingCountry id="78E67624FFD38A35F440F2E6F3E7D40D" box="[2956,3170,3399,3438]" name="Philippines" pageId="28" pageNumber="307">Philippines</collectingCountry>
on Batan, Sabtang, Calayan, Luzon, Lubang, Mindoro, Tablas, Sibuyan, 
<collectingRegion id="C235F856FFD38A35F759F20FF60AD4B6" box="[2197,2447,3502,3541]" country="Philippines" name="Catanduanes" pageId="28" pageNumber="307">Catanduanes</collectingRegion>
, Gigantes, Maripipi, 
<collectingRegion id="C235F856FFD38A35F4E1F20FF412D4B6" box="[2861,2967,3502,3541]" country="Philippines" name="Southern Leyte" pageId="28" pageNumber="307">Leyte</collectingRegion>
, Panay, Negros, 
<collectingRegion id="C235F856FFD38A35F32EF20FF2CFD4B6" box="[3298,3402,3502,3541]" country="Philippines" name="Cebu" pageId="28" pageNumber="307">Cebu</collectingRegion>
, Camotes, 
<collectingRegion id="C235F856FFD38A35F818F245F7CCD768" box="[2004,2121,3556,3595]" country="Philippines" name="Bohol" pageId="28" pageNumber="307">Bohol</collectingRegion>
, 
<collectingRegion id="C235F856FFD38A35F791F245F69FD768" box="[2141,2330,3556,3595]" country="Philippines" name="Camiguin" pageId="28" pageNumber="307">Camiguin</collectingRegion>
, Mindanao, Busuanga, 
<collectingRegion id="C235F856FFD38A35F518F245F4F7D768" box="[2772,2930,3556,3595]" country="Philippines" name="Palawan" pageId="28" pageNumber="307">Palawan</collectingRegion>
