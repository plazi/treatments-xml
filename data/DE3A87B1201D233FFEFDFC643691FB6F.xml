<taxonomicName id="91934D24201D233FFED7FC6436B3FBB5" authority="(MacLeay, 1825)" baseAuthorityName="MacLeay" baseAuthorityYear="1825" box="[372,921,1008,1032]" class="Insecta" family="Curculionidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="fabricii" subGenus="Guignotites">
