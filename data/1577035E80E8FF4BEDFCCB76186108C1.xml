<collectingRegion id="5F1A7CAA80E8FF4BE987C83D1CC10851" box="[1260,1436,1556,1581]" country="South Africa" name="KwaZulu-Natal" pageId="236" pageNumber="237">KwaZulu-Natal</collectingRegion>
(
<collectingCountry id="E5C9F2D880E8FF4BEDF5C81E186A082C" box="[158,311,1591,1616]" name="South Africa" pageId="236" pageNumber="237">South Africa</collectingCountry>
