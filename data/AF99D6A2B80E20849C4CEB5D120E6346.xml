<taxonomicName id="18FB7D8E62CC6DE50166E5DDBF568DE2" class="Insecta" family="Hesperiidae" genus="Anisochoria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anisochoria polysticta" order="Lepidoptera" pageId="154" pageNumber="155" phylum="Arthropoda" rank="species" species="polysticta">Anisochoria polysticta</taxonomicName>
, 
<taxonomicName id="6F6792627D4E467665FFD4E9776A5C58" class="Insecta" family="Hesperiidae" genus="Timochreon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Timochreon satyrus" order="Lepidoptera" pageId="154" pageNumber="155" phylum="Arthropoda" rank="species" species="satyrus">Timochreon satyrus</taxonomicName>
