<taxonomicName id="E1C81386FFE7FF80FBA94CFEFBC0F81A" box="[1028,1101,1964,1989]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="70" pageNumber="71" phylum="Tracheophyta" rank="genus">
