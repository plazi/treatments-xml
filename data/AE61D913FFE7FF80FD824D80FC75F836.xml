<quantity id="E130C5E0FFE7FF80FA914CF9FF59F836" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="70" pageNumber="71" unit="m" value="1000.0">1000 m</quantity>
in 
<taxonomicName id="E1C81386FFE7FF80FF544C9CFEE5F837" box="[249,360,1998,2024]" class="Magnoliopsida" family="Fagaceae" genus="Castanea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="70" pageNumber="71" phylum="Tracheophyta" rank="genus">
