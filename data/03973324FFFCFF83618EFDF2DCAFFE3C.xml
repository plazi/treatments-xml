<paragraph id="8B818232FFFCFF8760F2FCD6DF74FC3C" blockId="3.[138,1185,709,961]" pageId="3" pageNumber="405">
<typeStatus id="54853C90FFFCFF8760F2FCD6DF0BFCE2" box="[165,277,775,799]" pageId="3" pageNumber="405">Holotype</typeStatus>
