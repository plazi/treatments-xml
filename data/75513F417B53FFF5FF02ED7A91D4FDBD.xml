<paragraph id="FD478E577B53FFF4FF02ED9493D8FA2F" blockId="34.[140,757,1469,1496]" box="[140,757,1469,1496]" pageId="34" pageNumber="35">
<treatmentCitationGroup id="DDE8A9797B53FFF4FF02ED9493D8FA2F" box="[140,757,1469,1496]" pageId="34" pageNumber="35">
<treatmentCitation id="7C59A8467B53FFF4FF02ED9493DCFA2F" author="McDunnough J" box="[140,753,1469,1496]" page="307" pageId="34" pageNumber="35" year="1925">
<taxonomicName id="3AF8F5D47B53FFF4FF02ED9493DCFA2F" authority="McDunnough, 1925: 307" authorityName="McDunnough" authorityPageNumber="307" authorityYear="1925" box="[140,753,1469,1496]" class="Insecta" family="Noctuidae" genus="Anarta" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="subSpecies" species="leucocycla" subSpecies="albertensis">
<emphasis id="CF8C52457B53FFF4FF02ED949093FA2F" box="[140,446,1469,1495]" italics="true" pageId="34" pageNumber="35">Anarta leucocycla albertensis</emphasis>
<bibRefCitation id="9969F3A67B53FFF4FE48ED9493DCFA2F" author="McDunnough J" box="[454,753,1469,1496]" pageId="34" pageNumber="35" pagination="307 - 309" refId="ref65164" refString="McDunnough J (1925) Notes on some species of the genus Anarta (Lepidoptera). The Canadian Entomologist 57: 307 - 309." type="journal article" year="1925">McDunnough, 1925: 307</bibRefCitation>
