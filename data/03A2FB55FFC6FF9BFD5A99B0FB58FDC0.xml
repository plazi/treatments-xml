<taxonomicName id="4C0B31C0FFD8FF9AFE669E57FDB5FDDC" authorityName="Myers &amp; Donnelly" authorityYear="2008" baseAuthorityName="Myers and Donnelly" baseAuthorityYear="2008" box="[457,612,519,540]" class="Amphibia" family="Craugastoridae" genus="Eleutherodactylus" kingdom="Animalia" order="Anura" pageId="80" pageNumber="81" phylum="Chordata" rank="species" species="auricarens">
