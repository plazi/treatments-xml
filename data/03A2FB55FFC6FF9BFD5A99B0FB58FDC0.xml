<paragraph id="8BB44A43FFD8FF9AFF329DB8FD5FFB91" blockId="80.[128,654,308,1759]" pageId="80" pageNumber="81">
<taxonomicName id="4C0B31C0FFD8FF9AFF329DB8FE35FE3F" box="[157,484,488,511]" class="Amphibia" family="Craugastoridae" genus="Eleutherodactylus" kingdom="Animalia" order="Anura" pageId="80" pageNumber="81" phylum="Chordata" rank="species" species="pulvinatus">
