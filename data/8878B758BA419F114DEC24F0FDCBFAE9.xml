<taxonomicName id="C7D17DCDBA4E9F1048EB27A0FE7EF829" authority="Magdefrau, 1937" authorityName="Magdefrau" authorityYear="1937" class="Incertae sedis" family="Incertae sedis" genus="Dendrina" higherTaxonomySource="GBIF" kingdom="Fungi" order="Incertae sedis" pageId="56" pageNumber="57" phylum="Ascomycota" rank="species" species="minor">
