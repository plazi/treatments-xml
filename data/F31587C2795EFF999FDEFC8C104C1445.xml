<taxonomicName id="BCBC4D57795EFF999B64FBAE15AD15E1" box="[1069,1171,1133,1158]" class="Holothuroidea" family="Cucumariidae" genus="Pseudocolochirus" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="mollis">
