<taxonomicName id="BCBC4D57795EFF999A58FB84105C15E0" authority="Mortensen, 1925" authorityName="Mortensen" authorityYear="1925" class="Holothuroidea" family="Cucumariidae" genus="Psolidiella" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="genus">
