<taxonomicName id="4C3881C0207EFFEA4C07E38DFDF0F849" box="[326,576,1936,1958]" class="Insecta" family="Phlaeothripidae" genus="Liophloeothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="24" phylum="Arthropoda" rank="species" species="acaciae">
