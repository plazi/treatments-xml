<number id="3DD624CBF152CC17FD51482B9999AC75" box="[747,761,1317,1338]" pageId="2" pageNumber="269" value="3.0">3</number>
</figureCitation>
; 
<taxonomicName id="4C1E4D6FF152CC17FCBE482B98F1AC75" box="[772,913,1317,1338]" class="Insecta" family="Reduviidae" genus="Reduvius" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="269" phylum="Arthropoda" rank="species" species="sonoraensis">
