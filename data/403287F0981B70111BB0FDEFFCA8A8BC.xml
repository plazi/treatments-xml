Family 
<taxonomicName id="0F9B4D65981B70111BD0FDEFFE85A900" authorityName="Gill" authorityYear="1893" box="[247,382,547,573]" class="Elasmobranchii" family="Mobulidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="12" pageNumber="223" phylum="Chordata" rank="family">Mobulidae</taxonomicName>
