<taxonomicName id="24F550E94756FFD0C59AE650FCD4FEB6" box="[666,769,332,357]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="408" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
