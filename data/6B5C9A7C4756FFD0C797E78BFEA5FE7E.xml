<taxonomicName id="24F550E94756FFD0C7D6E66DFD99FE5B" authority="Samin et al. 2011" authorityName="Samin et al." authorityYear="2011" box="[214,588,367,392]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="408" phylum="Tracheophyta" rank="species" species="brutia">
