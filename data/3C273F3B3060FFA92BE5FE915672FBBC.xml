<taxonomicName id="738EF5AE3060FFA928DCFC3C539DFC09" authority="Waldren" authorityName="Waldren &amp; Williams &amp; Cambra &amp; Pitts" authorityYear="2020" box="[942,1157,906,932]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Invreiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="31" pageNumber="182" phylum="Arthropoda" rank="species" species="manleyi" status="sp. nov.">
