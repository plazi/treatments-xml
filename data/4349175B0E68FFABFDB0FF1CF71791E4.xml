<location id="CE3FF0960E68FFABFDC6FED7F162966C" LSID="urn:lsid:plazi:treatment:4349175B0E68FFABFDB0FF1CF71791E4:CE3FF0960E68FFABFDC6FED7F162966C" box="[553,619,306,332]" country="China" latitude="27.783333" longLatPrecision="1235" longitude="98.5" name="Dong" pageId="4" stateProvince="Yunnan">Dong</location>
et al.: 65, female, male – “ 
<collectingCountry id="B3F7E6DD0E68FFABFC74FED4F0ED966B" box="[923,996,305,331]" name="China" pageId="4">China</collectingCountry>
... Gongshan (
<geoCoordinate id="AED4C08A0E68FFABFB62FED4F7FA966C" box="[1165,1267,305,332]" degrees="27" direction="north" minutes="47" orientation="latitude" pageId="4" precision="925" value="27.783333">27°47′N</geoCoordinate>
, 
<geoCoordinate id="AED4C08A0E68FFABFF47FEB2F2039651" box="[168,266,342,369]" degrees="98" direction="east" minutes="30" orientation="longitude" pageId="4" precision="925" value="98.5">98°30′ E</geoCoordinate>
), 
<collectingRegion id="092468AF0E68FFABFECEFEB2F2FA9651" box="[289,499,343,369]" country="China" name="Yunnan" pageId="4">Yunnan Province</collectingRegion>
, alt. 2 
<quantity id="0C180BA80E68FFABFDACFEB3F1869651" box="[579,655,342,369]" metricMagnitude="2" metricUnit="m" metricValue="9.5" pageId="4" unit="m" value="950.0">
<elevation id="40CD417E0E68FFABFDACFEB3F1869651" box="[579,655,342,369]" metricMagnitude="2" metricUnit="m" metricValue="9.5" pageId="4" unit="m" value="950.0">950 m</elevation>
</quantity>
” (
<typeStatus id="145B18EF0E68FFABFD46FEB2F01D9651" box="[681,788,343,369]" pageId="4">holotype</typeStatus>
