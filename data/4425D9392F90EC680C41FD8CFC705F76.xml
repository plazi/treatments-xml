<taxonomicName id="89304EF7127D18747F090287C857B45E" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus furnius" order="Hymenoptera" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="furnius">Euplectrus furnius</taxonomicName>
