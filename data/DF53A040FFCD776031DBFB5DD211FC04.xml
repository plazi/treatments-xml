<quantity id="9002BCB3FFCF77673228F875D588F812" box="[868,984,1925,1952]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" metricValueMax="1.3" metricValueMin="1.0" pageId="3" pageNumber="298" unit="mm" value="11.5" valueMax="13.0" valueMin="10.0">10–13 µm</quantity>
and 
<quantity id="9002BCB3FFCF7767355CF875D2D4F812" box="[1040,1156,1925,1952]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" metricValueMax="2.3" metricValueMin="1.3" pageId="3" pageNumber="298" unit="mm" value="18.0" valueMax="23.0" valueMin="13.0">13–23 µm</quantity>
