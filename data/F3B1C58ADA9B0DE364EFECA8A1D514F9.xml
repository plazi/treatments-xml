<taxonomicName id="A361E647057C830DE6622042475B1376" authorityName="Swnmerh" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="3" pageNumber="219" phylum="Tracheophyta" rank="species" species="katangensis">
