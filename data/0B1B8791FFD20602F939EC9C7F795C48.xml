<paragraph id="830D3687FFD20600F8B1EDF17C8458C2" blockId="40.[145,761,1514,1874]" pageId="40" pageNumber="595">
<emphasis id="B1C6EA95FFD20600F8B1EDF17F045BA7" box="[145,518,1514,1536]" italics="true" pageId="40" pageNumber="595">
