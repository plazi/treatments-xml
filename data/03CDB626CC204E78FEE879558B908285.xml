<taxonomicName id="4C647CB3CC204E77FD1479C88CB08578" authority="Hartman, 1967" authorityName="Hartman" authorityYear="1967" box="[756,1198,1273,1299]" class="Polychaeta" family="Syllidae" genus="Levidorum" kingdom="Animalia" order="Phyllodocida" pageId="87" pageNumber="88" phylum="Annelida" rank="species" species="scotarium">
