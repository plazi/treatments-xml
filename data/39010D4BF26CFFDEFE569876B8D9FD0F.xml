<paragraph id="B117BC5DF26CFFDEFE569876BA16FEEF" blockId="4.[445,816,311,336]" box="[445,816,311,336]" pageId="4" pageNumber="91">
<heading id="EA5F0B31F26CFFDEFE569876BA16FEEF" box="[445,816,311,336]" centered="true" fontSize="10" level="2" pageId="4" pageNumber="91" reason="3">
<taxonomicName id="76A8C7DEF26CFFDEFE569876BA16FEEF" authority="(Sars, 1863)" baseAuthorityName="Sars" baseAuthorityYear="1863" box="[445,816,311,336]" class="Hexanauplia" family="Canthocamptidae" genus="Elaphoidella" kingdom="Animalia" order="Harpacticoida" pageId="4" pageNumber="91" phylum="Arthropoda" rank="species" species="gracilis">
