<subSubSection id="ABDD6557FFF6FFD30FE9FEF2C2FDAA27" box="[160,1156,262,323]" pageId="36" pageNumber="1733" type="etymology">
<paragraph id="E37836DCFFF6FFD30FE9FEF2C764AA44" blockId="36.[160,1156,262,323]" box="[160,285,262,288]" pageId="36" pageNumber="1733">
<emphasis id="D1B3EACEFFF6FFD30FE9FEF2C764AA44" bold="true" box="[160,285,262,288]" italics="true" pageId="36" pageNumber="1733">Etymology</emphasis>
</paragraph>
<paragraph id="E37836DCFFF6FFD30FE9FEDEC2FDAA27" blockId="36.[160,1156,262,323]" box="[160,1156,298,323]" pageId="36" pageNumber="1733">
<materialsCitation id="53AF3C81FFF6FFD30FE9FEDEC2F9AA27" box="[160,1152,298,323]" country="Saudi Arabia" location="The" pageId="36" pageNumber="1733" specimenCount="1">
<location id="E6186007FFF6FFD30FE9FEDEC6B1AA27" LSID="urn:lsid:plazi:treatment:6B6E87CAFFCCFFD30E2FFA2BC468A947:E6186007FFF6FFD30FE9FEDEC6B1AA27" box="[160,200,298,323]" country="Saudi Arabia" name="The" pageId="36" pageNumber="1733">The</location>
