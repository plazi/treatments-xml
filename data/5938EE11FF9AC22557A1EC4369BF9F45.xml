<taxonomicName id="16912484FF9AC2255447EDC56EFD9E52" box="[881,1014,1515,1540]" class="Insecta" family="Buprestidae" genus="Meliboeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="aurarius">
