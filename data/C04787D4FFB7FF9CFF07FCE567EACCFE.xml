<taxonomicName id="8FEE4D41FFB7FF9CFF07FC74647AC8AA" box="[151,313,979,1005]" class="Mammalia" family="Dasyproctidae" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Dasyproctidae</taxonomicName>
and 
<taxonomicName id="8FEE4D41FFB7FF9CFEE6FC7467E6C8AA" authority=", Crocodilia" authorityName="Crocodilia" box="[374,677,979,1005]" class="Mammalia" family="Erethizontidae" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Erethizontidae, Crocodilia</taxonomicName>
