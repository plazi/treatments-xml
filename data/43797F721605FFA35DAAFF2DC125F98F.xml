<paragraph id="CB6FCE641605FFA35DAAFF06C1ADFF3B" blockId="4.[151,681,151,212]" box="[151,400,188,212]" pageId="4" pageNumber="24">
(
<figureCitation id="53EBD2E11605FFA35DA2FF06C136FF3B" box="[159,267,188,212]" captionStart="FIGURES 5 – 9" captionStartId="3.[151,264,1972,1995]" captionTargetBox="[152,1430,543,1949]" captionTargetId="figure@3.[151,1436,533,1951]" captionTargetPageId="3" captionText="FIGURES 5 – 9. Dolichopeza (Sinoropeza) hamulifera sp. nov. 5. Habitus of male, lateral view; 6. head, lateral view; 7. head, dorsal view; 8. thorax, lateral view; 9. female ovipositor, lateral view." httpUri="https://zenodo.org/record/203477/files/figure.png" pageId="4" pageNumber="24">
Figs. 
<number id="7D18DC431605FFA35DE2FF06C136FF3B" box="[223,267,188,212]" pageId="4" pageNumber="24" value="7.1" valueMax="9.2" valueMin="5.0">5–9</number>
</figureCitation>
, 
<figureCitation id="53EBD2E11605FFA35C2AFF06C108FF3B" box="[279,309,188,212]" captionStart="FIGURE 20" captionStartId="7.[151,250,846,869]" captionText="FIGURE 20. Dolichopeza (Sinoropeza) hamulifera sp. nov. Wing." pageId="4" pageNumber="24">20</figureCitation>
, 
<figureCitation id="53EBD2E11605FFA35C7DFF06C1B5FF3B" box="[320,392,188,212]" captionStart="FIGURES 27 – 30" captionStartId="9.[151,264,1543,1566]" captionTargetBox="[171,1428,202,1513]" captionTargetId="figure@9.[151,1436,193,1522]" captionTargetPageId="9" captionText="FIGURES 27 – 30. Dolichopeza (Sinoropeza) hamulifera sp. nov. Male. 27. Hypopygium, lateral view; 28. ninth tergite, dorsal view; 29. lobe of gonostylus, lateral view; 30. clasper of gonostylus, lateral view. Scale bar of 27 – 28 = 0.33 mm, 29 – 30 = 0.2 mm." httpUri="https://zenodo.org/record/203481/files/figure.png" pageId="4" pageNumber="24">
<number id="7D18DC431605FFA35C7DFF06C1B5FF3B" box="[320,392,188,212]" pageId="4" pageNumber="24" value="28.5" valueMax="30.0" valueMin="27.0">27–30</number>
</figureCitation>
)
</paragraph>
