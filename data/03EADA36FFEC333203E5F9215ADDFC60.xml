<taxonomicName id="4C4310A3FFEC333307E5F8095ADDF81B" authority="Gyllenhal, 1813" authorityName="Gyllenhal" authorityYear="1813" box="[1175,1436,1990,2012]" class="Insecta" family="Curculionidae" genus="Hylurgops" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="315" phylum="Arthropoda" rank="species" species="palliatus">
