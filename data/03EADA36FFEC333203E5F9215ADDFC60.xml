(
<bibRefCitation id="EFD216D1FFEC3333078AF8085AD4F81B" author="Gyllenhal" box="[1272,1429,1991,2012]" pageId="14" pageNumber="315" refString="Gyllenhal, L. (1813) Insecta Svecica Descripta. Clasis I, Coleoptera sive, Scolytidae. F. J. Leverentz, Scaris, 730 pp." type="book" year="1813">
Gyllenhal, 
<number id="3D8B7907FFEC33330616F8085AD4F81B" box="[1380,1429,1991,2012]" pageId="14" pageNumber="315" value="1813.0">1813</number>
</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8BFC6B20FFED333203E5FF585ADAFF0D" blockId="15.[151,1436,151,935]" pageId="15" pageNumber="316">
<keyLead id="30B2C815FFED333203E5FF585ADAFF0D" pageId="15" pageNumber="316">
