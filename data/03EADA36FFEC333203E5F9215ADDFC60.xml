<number id="3D8B7907FFEC333303E5F8655FE4F878" box="[151,165,1962,1983]" pageId="14" pageNumber="315" value="2.0">2</number>
(
<number id="3D8B7907FFEC333303DFF8655FF7F878" box="[173,182,1962,1983]" pageId="14" pageNumber="315" value="1.0">1</number>
) Smaller species (
<quantity id="4CBBC6C5FFEC333302FAF8655D43F878" box="[392,514,1962,1983]" metricMagnitude="-3" metricUnit="m" metricValue="2.95" metricValueMax="3.4" metricValueMin="2.5" pageId="14" pageNumber="315" unit="mm" value="2.95" valueMax="3.4" valueMin="2.5">2.5–3.4 mm</quantity>
); declivital apex upturned (
<figureCitation id="137877A5FFEC33330062F8655C19F878" box="[784,856,1962,1983]" captionStart="FIGURE 15" captionStartId="17.[151,250,1122,1144]" captionTargetBox="[167,1419,202,1095]" captionTargetId="figure@17.[160,1426,193,1101]" captionTargetPageId="17" captionText="FIGURE 15. Elytral declivities of New World Hylurgops: a) H. palliatus, b) H. planirostris, c) H. rugipennis, d) H. pinifex, e) H. incomptus, f) H. longipennis, g) H. knausi, h) H. reticulatus, and i) H. porosus." httpUri="https://zenodo.org/record/252629/files/figure.png" pageId="14" pageNumber="315">
