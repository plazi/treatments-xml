<taxonomicName id="4C0569BEFFABFF8A2B05138BFBA3FABB" authorityName="LeConte" authorityYear="1863" box="[1031,1109,1277,1299]" class="Insecta" family="Carabidae" genus="Harpalus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="30" phylum="Arthropoda" rank="species" species="opalinus">
