<geoCoordinate id="5C71660F70FC4E01D91191EE79DC3146" degrees="71" direction="west" minutes="42" orientation="longitude" precision="1" seconds="03.9" value="-71.70108">71°42'03.9&quot;W</geoCoordinate>
) 
<emphasis id="A5FE13B55632D6CFB30C7293F5F2406A" bold="true" pageId="0" pageNumber="49">D</emphasis>
rock face seep, type locality of 
<taxonomicName id="8659A12166877929940E663AD0B0742B" genus="N." lsidName="N. bispinosa" pageId="0" pageNumber="49" rank="species" species="bispinosa">
