<taxonomicName id="4C7840B6C85D1841593C2A8C9569F972" authority="Moreno, 1889: 29" authorityName="Moreno" authorityPageNumber="29" authorityYear="1889" box="[94,547,1758,1783]" class="Aves" family="Phorusrhacidae" genus="Mesembriornis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gruiformes" pageId="31" pageNumber="86" phylum="Chordata" rank="species" species="milneedwardsi">
