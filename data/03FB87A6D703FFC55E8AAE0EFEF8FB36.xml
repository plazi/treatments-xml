<taxonomicName id="4C524D33D700FFC45A20ACDFFAA6FB65" authority="(Broun)" authorityName="Broun" baseAuthorityName="Broun" baseAuthorityYear="1914" box="[1085,1339,1245,1269]" class="Insecta" family="Curculionidae" genus="Chalepistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="434" phylum="Arthropoda" rank="species" species="asperatus">
<emphasis id="B926EAA2D700FFC45A20ACDFFB4EFB64" box="[1085,1235,1245,1268]" italics="true" pageId="2" pageNumber="434">C. asperatus</emphasis>
