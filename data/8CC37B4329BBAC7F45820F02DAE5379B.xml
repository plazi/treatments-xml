<paragraph id="0B3D5ED008A77B55222164329FA485DA" pageId="null" pageNumber="142">
<taxonomicName id="35CB0DFFC6BEB7EA5E5D294C454E30AD" authority="L." class="Polypodiopsida" family="Ophioglossaceae" genus="Ophioglossum" kingdom="Plantae" order="Ophioglossales" pageId="null" pageNumber="142" phylum="Tracheophyta" rank="species" species="vulgatum">
<pageBreakToken id="3216E74593CF439EB0533ABD6BBFEE62" pageId="null" pageNumber="142">Ophioglossum</pageBreakToken>
<normalizedToken id="A671AE09F500AD29DFB046CB9C959711" originalValue="vulgátum" pageId="null" pageNumber="142">vulgatum</normalizedToken>
