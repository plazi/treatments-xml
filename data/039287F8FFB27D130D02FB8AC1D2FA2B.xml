</taxonomicName>
(
<taxonomicName id="4C3B4D6DFFB27D130887FF0CC1B6FED1" authorityName="Illiger" authorityYear="1815" box="[1155,1243,282,305]" class="Mammalia" family="Muridae" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="29" phylum="Chordata" rank="family">Muridae</taxonomicName>
