<taxonomicName id="4C32D3EBFFCFF554E469FF5BFD3014EE" authority="Calder, 2013" authorityName="Calder" authorityYear="2013" box="[519,681,152,176]" class="Hydrozoa" family="Lafoeidae" genus="Lafoea" kingdom="Animalia" order="Leptothecata" pageId="16" pageNumber="17" phylum="Cnidaria" rank="species" species="intorta" status="sp. nov.">
