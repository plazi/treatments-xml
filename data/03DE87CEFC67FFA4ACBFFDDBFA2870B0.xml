<quantity id="4C8F9B3DFC67FFA4AD66FBBAFE10710A" box="[334,419,1084,1105]" metricMagnitude="-4" metricUnit="m" metricValue="5.3" pageId="21" pageNumber="55" unit="mm" value="0.53">0.53 mm</quantity>
long; about 
<number id="3DBF24FFFC67FFA4AE34FBBAFD87710A" box="[540,564,1084,1105]" pageId="21" pageNumber="55" value="40.0">40</number>
