<keyLead id="308695EDFC67FFA4ACBFFC40FA2876A2" pageId="21" pageNumber="55">
<number id="3DBF24FFFC67FFA4ACBFFC40FF167680" box="[151,165,966,987]" pageId="21" pageNumber="55" value="2.0">2</number>
