<taxonomicName id="4C2D4D64FF8AFF9CFC04FA30FDDD092A" authorityName="Luk &amp; Marshall" authorityYear="2014" box="[340,538,1051,1075]" class="Insecta" family="Sphaeroceridae" genus="Aptilotella" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="macta">
