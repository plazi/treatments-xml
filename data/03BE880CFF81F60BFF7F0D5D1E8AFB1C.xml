<taxonomicName id="4C174299FF81F60BFF7F0D5D1F50FEE5" authority="Young, 1977" authorityName="Young" authorityYear="1977" box="[151,563,332,358]" class="Insecta" family="Cicadellidae" genus="Apogonalia" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="109" phylum="Arthropoda" rank="species" species="imitatrix">
