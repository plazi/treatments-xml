<taxonomicName id="4C319EBF190DFFE1684D961A1E60F9C5" class="Aves" family="Oriolidae" genus="Sphecotheres" kingdom="Animalia" order="Passeriformes" pageId="50" pageNumber="50" phylum="Chordata" rank="variety" species="maxilliaris" variety="vieilloti">
