</taxonomicName>
and 
<taxonomicName id="4C319EBF190DFFE16BF8963E1D15F9C5" box="[388,564,1583,1603]" class="Aves" family="Oriolidae" genus="Sphecotheres" kingdom="Animalia" order="Passeriformes" pageId="50" pageNumber="50" phylum="Chordata" rank="variety" variety="flaviventris">
