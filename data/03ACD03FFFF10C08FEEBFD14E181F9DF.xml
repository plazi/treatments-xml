<paragraph id="8BBA6129FFF00C08FF88FA4EE181F9DF" blockId="17.[106,1096,1479,1564]" pageId="17" pageNumber="318">
<materialsCitation id="3B6D6B74FFF00C08FF88FA4EE185F9DF" country="Taiwan" elevation="2900" location="The" municipality="Nenkaoshan" pageId="17" pageNumber="318" specimenCount="1" stateProvince="Nantou">
