species: 
<taxonomicName id="4C3E4D142E49FFC596B54CEFED08E9C0" authority="Morawitz, 1862" authorityName="Morawitz" authorityYear="1862" box="[735,1071,1732,1754]" class="Insecta" family="Carabidae" genus="Harpalus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="capito">
