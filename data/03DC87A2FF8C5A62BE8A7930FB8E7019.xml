<taxonomicName id="4C754D37FF8C5A63BD127A98FBC377D4" authority="Iwan, 1999" authorityName="Iwan" authorityYear="1999" box="[783,1050,1719,1744]" class="Insecta" family="Tenebrionidae" genus="Atrocrates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="185" phylum="Arthropoda" rank="species" species="bellamyi">
