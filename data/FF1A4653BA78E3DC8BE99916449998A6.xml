Genre 
<taxonomicName id="26986B9937DE7E81A9059DE3C5C27222" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:146789" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Aeromyrma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Aeromyrma Forel" order="Hymenoptera" pageNumber="198" phylum="Arthropoda" rank="genus" status="nov. gen.">AEROMYRMA</taxonomicName>
