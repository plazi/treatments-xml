<subSubSection pageId="6" pageNumber="5" type="etymology">
<paragraph blockId="6.[192,1440,1506,1562]" pageId="6" pageNumber="5">
<emphasis bold="true" box="[192,348,1506,1530]" pageId="6" pageNumber="5">Etymology.</emphasis>
