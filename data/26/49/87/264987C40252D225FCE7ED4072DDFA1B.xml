<quantity box="[935,1045,1321,1343]" metricMagnitude="2" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="10" pageNumber="54" unit="m" value="400.0" valueMax="500.0" valueMin="300.0">300–500 m</quantity>
