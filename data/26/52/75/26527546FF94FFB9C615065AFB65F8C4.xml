(
<number box="[499,540,1158,1184]" pageId="2" pageNumber="3" value="7.0" valueMax="8.0" valueMin="6.0">6­8</number>
), 
<emphasis box="[562,583,1159,1184]" italics="true" pageId="2" pageNumber="3">l&quot;</emphasis>
