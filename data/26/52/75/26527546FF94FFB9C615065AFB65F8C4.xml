<number box="[636,708,1158,1184]" pageId="2" pageNumber="3" value="18.0" valueMax="19.0" valueMin="17.0">17­19</number>
); tibia 
<number box="[789,818,1158,1184]" pageId="2" pageNumber="3" value="10.0">10</number>
(
<number box="[835,890,1158,1184]" pageId="2" pageNumber="3" value="9.5" valueMax="11.0" valueMin="8.0">8­11</number>
); tarsus 
<number box="[988,1016,1158,1184]" pageId="2" pageNumber="3" value="11.0">11</number>
(
<number box="[1033,1088,1158,1184]" pageId="2" pageNumber="3" value="9.5" valueMax="11.0" valueMin="8.0">8­11</number>
), solenidion 
<number box="[1240,1270,1158,1184]" pageId="2" pageNumber="3" value="12.0">12</number>
(
<number pageId="2" pageNumber="3" value="12.0" valueMax="13.0" valueMin="11.0">11­ 13</number>
), empodium 
<number box="[452,481,1198,1224]" pageId="2" pageNumber="3" value="11.0">11</number>
(
<number box="[498,568,1198,1224]" pageId="2" pageNumber="3" value="11.5" valueMax="12.0" valueMin="11.0">11­12</number>
