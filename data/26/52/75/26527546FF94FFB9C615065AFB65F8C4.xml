), 
<emphasis box="[367,388,1239,1264]" italics="true" pageId="2" pageNumber="3">u'</emphasis>
<number box="[395,410,1238,1264]" pageId="2" pageNumber="3" value="9.0">9</number>
(
<number box="[427,484,1238,1264]" pageId="2" pageNumber="3" value="9.0" valueMax="10.0" valueMin="8.0">8­10</number>
