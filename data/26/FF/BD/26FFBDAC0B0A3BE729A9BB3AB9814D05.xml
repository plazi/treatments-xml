<normalizedToken originalValue="Fruchtschläuche" pageId="null" pageNumber="448">Fruchtschlaeuche</normalizedToken>
kahl; 
<normalizedToken originalValue="Blätter" pageId="null" pageNumber="448">Blaetter</normalizedToken>
nicht 
<normalizedToken originalValue="fadenförmig" pageId="null" pageNumber="448">fadenfoermig</normalizedToken>
