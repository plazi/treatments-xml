<taxonomicName authorityName="Leach" authorityYear="1815" box="[895,988,1091,1113]" class="Insecta" family="Erebidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="724" phylum="Arthropoda" rank="family">Erebidae</taxonomicName>
