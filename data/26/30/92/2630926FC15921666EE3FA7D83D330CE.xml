<materialsCitation box="[826,1058,338,360]" country="Brazil" location="Brazil" pageId="37" pageNumber="452" specimenCount="1" typeStatus="holotype">
<emphasis box="[826,970,338,360]" italics="true" pageId="37" pageNumber="452">
