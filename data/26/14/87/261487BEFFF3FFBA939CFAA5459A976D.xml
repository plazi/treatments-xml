<emphasis box="[319,543,1979,2000]" italics="true" pageId="1" pageNumber="56">
<taxonomicName box="[319,539,1979,2000]" class="Insecta" family="Reduviidae" genus="Isocondylus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="species" species="elongatus">Isocondylus elongatus</taxonomicName>
