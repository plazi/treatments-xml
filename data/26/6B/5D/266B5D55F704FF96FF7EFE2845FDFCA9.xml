as 
<taxonomicName box="[380,575,608,630]" class="Entognatha" family="Isotomidae" genus="Isotoma" kingdom="Animalia" order="Collembola" pageId="68" pageNumber="69" phylum="Arthropoda" rank="variety" species="olivacea" variety="stachi">
