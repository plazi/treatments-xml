<taxonomicName authorityName="sensu Fjellberg" authorityYear="1985" baseAuthorityName="Macnamara" baseAuthorityYear="1922" box="[583,678,525,548]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="pannosa">
