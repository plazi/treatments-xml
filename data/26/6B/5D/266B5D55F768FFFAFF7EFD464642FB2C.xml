<taxonomicName authorityName="Axelson" authorityYear="1902" box="[893,997,943,968]" class="Collembola" family="Neanuridae" genus="Micranurida" higherTaxonomySource="CoL" kingdom="Animalia" order="Poduromorpha" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="papillosa">
