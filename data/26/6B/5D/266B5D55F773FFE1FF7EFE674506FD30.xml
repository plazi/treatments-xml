<taxonomicName authority="Martynova, 1973" authorityName="Martynova" authorityYear="1973" box="[925,1316,589,614]" class="Entognatha" family="Isotomidae" genus="Ballistura" kingdom="Animalia" order="Collembola" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="wrangeliensis">
