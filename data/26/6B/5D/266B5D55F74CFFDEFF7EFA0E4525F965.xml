<taxonomicName authorityName="Fitch" authorityYear="1847" box="[192,282,1612,1637]" class="Entognatha" family="Poduridae" genus="Podura" kingdom="Animalia" order="Collembola" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="nivicola">
