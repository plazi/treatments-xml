<paragraph blockId="10.[151,1436,1839,1953]" pageId="10" pageNumber="411">
<emphasis bold="true" box="[151,276,1839,1861]" pageId="10" pageNumber="411">FIGURE 8.</emphasis>
<taxonomicName authority="Ruiz, 2017" authorityName="Ruiz" authorityYear="2017" box="[285,503,1839,1861]" class="Arachnida" family="Salticidae" genus="Amphidraus" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="411" phylum="Arthropoda" rank="species" species="pulvinus" status="sp. nov.">
<emphasis box="[285,503,1839,1861]" italics="true" pageId="10" pageNumber="411">Amphidraus pulvinus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[511,593,1839,1860]" pageId="10" pageNumber="411" rank="species">
