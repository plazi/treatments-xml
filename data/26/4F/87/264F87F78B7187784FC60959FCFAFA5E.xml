<subSubSection pageId="35" pageNumber="36" type="discussion">
<paragraph blockId="35.[151,1437,151,1654]" pageId="35" pageNumber="36">
<emphasis bold="true" box="[199,335,1412,1437]" pageId="35" pageNumber="36">Comments.</emphasis>
<taxonomicName box="[346,591,1412,1437]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="maculata">
