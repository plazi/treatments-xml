and 
<taxonomicName box="[1260,1376,583,608]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="solanicola">
<emphasis box="[1260,1376,583,608]" italics="true" pageId="48" pageNumber="49">solanicola</emphasis>
