, 
<taxonomicName box="[1032,1093,585,608]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="nigra">
