of Indo-West Pacific (
<taxonomicName authority=", Brachyura" authorityName="Brachyura" box="[942,1098,79,96]" class="Insecta" family="Apidae" genus="Crustacea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="523" phylum="Arthropoda" rank="genus">Crustacea, Brachyura</taxonomicName>
, 
<taxonomicName authorityName="MacLeay" authorityYear="1838" box="[1105,1189,79,96]" class="Malacostraca" family="Dorippidae" kingdom="Animalia" order="Decapoda" pageId="24" pageNumber="523" phylum="Arthropoda" rank="family">Dorippidae</taxonomicName>
)
</paragraph>
