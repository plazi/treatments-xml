<collectingCountry box="[1017,1105,799,822]" name="Vanuatu" pageId="54" pageNumber="553">Vanuatu</collectingCountry>
and 
<collectingCountry name="New Caledonia" pageId="54" pageNumber="553">New Caledonia</collectingCountry>
. Depth: 
<quantity box="[872,1012,825,848]" metricMagnitude="3" metricUnit="m" metricValue="1.23" metricValueMax="1.25" metricValueMin="1.21" pageId="54" pageNumber="553" unit="m" value="1230.0" valueMax="1250.0" valueMin="1210.0">1210-1250 m</quantity>
(
