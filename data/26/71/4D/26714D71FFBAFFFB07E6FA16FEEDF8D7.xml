. 
<collectingRegion box="[281,480,1859,1883]" country="Australia" name="Western Australia" pageId="13" pageNumber="230">Western Australia</collectingRegion>
and 
<collectingRegion box="[540,744,1859,1883]" country="Australia" name="New South Wales" pageId="13" pageNumber="230">New South Wales</collectingRegion>
, 
<collectingCountry box="[121,219,1888,1912]" name="Australia" pageId="13" pageNumber="230">Australia</collectingCountry>
(
<figureCitation box="[233,312,1888,1912]" captionStart="Figure 13" captionStartId="20.[166,234,1083,1105]" captionTargetBox="[167,1467,151,1070]" captionTargetId="figure-10@20.[167,1467,151,1071]" captionTargetPageId="20" captionText="Figure 13. Distribution records of Stephanopis altifrons, S. angulata, S. arenata sp. nov., S. armata and S. barbipes." figureDoi="http://doi.org/10.5281/zenodo.4653864" httpUri="https://zenodo.org/record/4653864/files/figure.png" pageId="13" pageNumber="230">Fig. 13</figureCitation>
).
</paragraph>
