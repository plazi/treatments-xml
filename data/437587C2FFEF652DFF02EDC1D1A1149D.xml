<collectionCode id="ADCDAE11FFEF652AFF22EE11D50B120E" box="[183,289,1614,1640]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="43" pageNumber="44" type="Museum">NHMUK</collectionCode>
