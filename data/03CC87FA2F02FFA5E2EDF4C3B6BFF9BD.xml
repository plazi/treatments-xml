<taxonomicName id="4C654D6F2F01FFA5E32DF268B6DDFBAC" authority="Matsumura, 1906" authorityName="Matsumura" authorityYear="1906" box="[343,735,1071,1094]" class="Insecta" family="Cicadellidae" genus="Tautoneura" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="196" phylum="Arthropoda" rank="species" species="mori">
