<materialsCitation id="39A1235DAC7B3B1CFE18D9AB5FEBFDA3" box="[457,936,494,514]" collectingDate="1957-03-28" collectingDateMax="1957-05-04" collectingDateMin="1957-03-28" collectionCode="V" collectorName="Ziela, U." location="Nimba" pageId="55" pageNumber="448" specimenCount="1" stateProvince="Nimba">
