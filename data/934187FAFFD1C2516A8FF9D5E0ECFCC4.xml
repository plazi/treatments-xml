<quantity id="DC109B09FFD1C2556980F948E681F907" box="[920,1009,1703,1729]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="3" pageNumber="432" unit="mm" value="4.1">4.1 mm</quantity>
