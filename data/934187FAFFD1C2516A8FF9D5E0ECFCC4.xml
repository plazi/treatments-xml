<paragraph id="1B5736ECFFD1C2556A8FF949E685F907" blockId="3.[151,1437,1702,2017]" box="[151,1013,1702,1729]" pageId="3" pageNumber="432">
<emphasis id="299CEAFEFFD1C2556A8FF949E459F906" bold="true" box="[151,297,1702,1728]" pageId="3" pageNumber="432">Description.</emphasis>
<materialsCitation id="AB803CB1FFD1C2556B28F949E681F907" ID-GBIF-Occurrence="3712143301" box="[304,1009,1702,1729]" pageId="3" pageNumber="432" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
<typeStatus id="C453884EFFD1C2556B28F949E4EBF906" box="[304,411,1702,1728]" pageId="3" pageNumber="432">Holotype</typeStatus>
, 
<specimenCount id="0DEEFD65FFD1C2556BBEF948E4CBF907" box="[422,443,1703,1729]" pageId="3" pageNumber="432" type="female">â™€</specimenCount>
, length of body 
<quantity id="DC109B09FFD1C255686BF949E7BBF907" box="[627,715,1702,1729]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="3" pageNumber="432" unit="mm" value="4.5">4.5 mm</quantity>
