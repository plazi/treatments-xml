<subSubSection id="508F62AC87495966FEB3BDC7FDB2FC30" box="[322,548,919,943]" pageId="39" pageNumber="298" type="nomenclature">
<paragraph id="182A312787495966FEB3BDC7FDB2FC30" blockId="39.[322,548,919,943]" box="[322,548,919,943]" pageId="39" pageNumber="298">
<heading id="4362864B87495966FEB3BDC7FDB2FC30" box="[322,548,919,943]" centered="true" fontSize="11" level="2" pageId="39" pageNumber="298" reason="2">
<emphasis id="2AE1ED3587495966FEB3BDC7FDB2FC30" bold="true" box="[322,548,919,943]" pageId="39" pageNumber="298">
<taxonomicName id="DF954AA487495966FEB3BDC7FE40FC30" authority="Cairns, 2004" authorityName="Cairns" authorityYear="2004" box="[322,470,919,943]" class="Anthozoa" family="Turbinoliidae" genus="Lissotrochus" kingdom="Animalia" order="Scleractinia" pageId="39" pageNumber="298" phylum="Cnidaria" rank="genus" status="n.gen.">
<emphasis id="2AE1ED3587495966FEB3BDC7FE40FC30" bold="true" box="[322,470,919,943]" italics="true" pageId="39" pageNumber="298">Lissotrochus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="31D2504E87495966FE2DBDC7FDB2FC30" box="[476,548,919,943]" pageId="39" pageNumber="298" rank="genus">n.gen.</taxonomicNameLabel>
