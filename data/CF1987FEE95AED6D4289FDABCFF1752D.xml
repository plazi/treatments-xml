<collectionCode id="21A1AE2DE95CED6D41C2FB24CAA470AA" box="[421,505,1218,1242]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="52" pageNumber="52">AMNH</collectionCode>
