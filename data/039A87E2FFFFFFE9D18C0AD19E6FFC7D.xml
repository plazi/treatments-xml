<taxonomicName id="4C334D77FFFFFFE9D18C0AD19EDDFDA0" authority="Stal, 1867" authorityName="Stal" authorityYear="1867" box="[151,447,489,517]" class="Insecta" family="Reduviidae" genus="Graptocleptes" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="389" phylum="Arthropoda" rank="genus">
