<quantity box="[578,737,1808,1833]" metricMagnitude="-2" metricUnit="m" metricValue="6.5405" metricValueMax="7.747" metricValueMin="5.334" pageId="4" pageNumber="432" unit="in" value="2.575" valueMax="3.05" valueMin="2.1">2.1 to 3.05 in</quantity>
