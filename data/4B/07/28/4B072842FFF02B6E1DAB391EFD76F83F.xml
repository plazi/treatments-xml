<quantity box="[460,595,1844,1869]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" metricValueMax="8.636" metricValueMin="6.604" pageId="4" pageNumber="432" unit="in" value="3.0" valueMax="3.4" valueMin="2.6">2.6 to 3.4 in</quantity>
