<paragraph pageId="155" pageNumber="156">
<taxonomicName genus="Cardiophorites" lsidName="Cardiophorites" pageId="155" pageNumber="156" rank="genus">Cardiophorites</taxonomicName>
<normalizedToken originalValue="Candèze">Candeze</normalizedToken>
