<taxonomicName class="Insecta" family="Elateridae" genus="Cardiophorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cardiophorus" order="Coleoptera" pageId="155" pageNumber="156" phylum="Arthropoda" rank="genus">Cardiophorus</taxonomicName>
