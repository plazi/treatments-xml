<taxonomicName authorityName=": Josifov &amp; Kerzhner" authorityYear="1978" box="[1000,1158,1257,1278]" class="Insecta" family="Acanthosomatidae" genus="Elasmostethus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="362" phylum="Arthropoda" rank="species" species="interstinctus">
<emphasis box="[1000,1158,1257,1278]" italics="true" pageId="11" pageNumber="362">E. interstinctus</emphasis>
</taxonomicName>
; 50. 
<taxonomicName authorityName=": Lee" authorityYear="1971" box="[1219,1353,1256,1278]" class="Insecta" family="Acanthosomatidae" genus="Elasmostethus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="362" phylum="Arthropoda" rank="species" species="humeralis">
<emphasis box="[1219,1353,1256,1278]" italics="true" pageId="11" pageNumber="362">E. humeralis</emphasis>
</taxonomicName>
; 51. 
<taxonomicName authorityName=": Gollner-Scheiding" authorityYear="2006" class="Insecta" family="Acanthosomatidae" genus="Elasmostethus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="362" phylum="Arthropoda" rank="species" species="yunnanus">
<emphasis italics="true" pageId="11" pageNumber="362">E. yunnanus</emphasis>
</taxonomicName>
