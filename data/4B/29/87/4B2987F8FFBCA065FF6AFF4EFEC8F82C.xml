<paragraph blockId="11.[151,1436,1353,1953]" pageId="11" pageNumber="362">
<materialsCitation ID-GBIF-Occurrence="1638471498" country="China" location="Songbai Township" pageId="11" pageNumber="362" specimenCount="1" stateProvince="Yunnan">
