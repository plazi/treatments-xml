<subSubSection pageId="13" pageNumber="14" type="type locality">
<paragraph pageId="13" pageNumber="14">Type locality.</paragraph>
<paragraph pageId="13" pageNumber="14">
PHG 74 Bukit Sagu, Pahang (
<geoCoordinate direction="north" orientation="latitude" precision="15" value="3.9816668">3°58'54&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="15" value="103.144165">103°08'39&quot;E</geoCoordinate>
