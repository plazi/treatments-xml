<caption httpUri="https://zenodo.org/record/236526/files/figure.png" pageId="3" pageNumber="134" targetBox="[199,1387,210,1859]" targetPageId="3">
<paragraph blockId="3.[151,1436,1897,2011]" pageId="3" pageNumber="134">
<emphasis bold="true" box="[151,327,1897,1920]" pageId="3" pageNumber="134">FIGURES 9–16.</emphasis>
<taxonomicName box="[334,563,1898,1920]" class="Arachnida" family="Caponiidae" genus="Tarsonops" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="134" phylum="Arthropoda" rank="species" species="ariguanabo" status="n. comb.">
<emphasis box="[334,563,1898,1920]" italics="true" pageId="3" pageNumber="134">Tarsonops ariguanabo</emphasis>
