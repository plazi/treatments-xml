<collectingCountry box="[843,914,1626,1651]" name="China" pageId="2" pageNumber="453">China</collectingCountry>
: 
<taxonomicName box="[926,1093,1627,1651]" class="Insecta" family="Dilaridae" genus="Dilar" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="formosanus">
