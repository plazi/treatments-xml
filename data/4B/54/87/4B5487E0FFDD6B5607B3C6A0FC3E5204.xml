</taxonomicName>
, 
<taxonomicName box="[1108,1244,1627,1651]" class="Insecta" family="Dilaridae" genus="Dilar" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="subdolus">
<emphasis box="[1108,1244,1627,1651]" italics="true" pageId="2" pageNumber="453">D. subdolus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1259,1393,1627,1650]" class="Insecta" family="Dilaridae" genus="Dilar" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="cornutus">
<emphasis box="[1259,1393,1627,1650]" italics="true" pageId="2" pageNumber="453">D. cornutus</emphasis>
</taxonomicName>
