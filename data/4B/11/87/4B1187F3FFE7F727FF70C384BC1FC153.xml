<number box="[662,721,1838,1862]" pageId="3" pageNumber="454" value="1983.0">1983</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Alvarez &amp; Hooper, 2009" authorityName="Alvarez &amp; Hooper" authorityYear="2009" box="[732,1146,1837,1862]" class="Demospongiae" family="Axinellidae" genus="Axinella" kingdom="Animalia" order="Halichondrida" pageId="3" pageNumber="454" phylum="Porifera" rank="species" species="loribella">
