); 
<taxonomicName authority="Burton, 1959" authorityName="Burton" authorityYear="1959" class="Demospongiae" family="Axinellidae" genus="Axinella" kingdom="Animalia" order="Halichondrida" pageId="4" pageNumber="455" phylum="Porifera" rank="species" species="flabelloreticulata">
