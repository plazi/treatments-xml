Thomas 
<number box="[779,837,332,357]" pageId="4" pageNumber="455" value="1981.0">1981</number>
</bibRefCitation>
</taxonomicName>
,is finger-shaped; 
<taxonomicName authority="Ridley, 1984" authorityName="Ridley" authorityYear="1984" box="[1055,1391,332,357]" class="Demospongiae" family="Axinellidae" genus="Axinella" kingdom="Animalia" order="Halichondrida" pageId="4" pageNumber="455" phylum="Porifera" rank="species" species="proliferans">
<emphasis box="[1055,1219,332,357]" italics="true" pageId="4" pageNumber="455">A. proliferans</emphasis>
Ridley, 
<number box="[1332,1391,332,356]" pageId="4" pageNumber="455" value="1984.0">1984</number>
</taxonomicName>
, 
