and 
<taxonomicName box="[626,753,260,284]" class="Demospongiae" family="Axinellidae" genus="Axinella" kingdom="Animalia" order="Halichondrida" pageId="4" pageNumber="455" phylum="Porifera" rank="species" species="aruensis">
