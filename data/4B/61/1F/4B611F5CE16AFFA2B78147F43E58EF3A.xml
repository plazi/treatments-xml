<number box="[653,726,1053,1077]" pageId="4" pageNumber="405" value="14646.0">14646</number>
; light reddish and small-sized piece of almost rectangular shape amber (
<date box="[242,323,1089,1114]" pageId="4" pageNumber="405" value="1915-10-09">
<number box="[242,271,1089,1114]" pageId="4" pageNumber="405" value="15.0">15</number>
x 
<quantity box="[309,323,1090,1114]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="405" unit="mm" value="9.0">9</quantity>
