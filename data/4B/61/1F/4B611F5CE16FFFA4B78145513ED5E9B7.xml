</taxonomicNameLabel>
, 
<taxonomicName box="[1312,1435,1897,1921]" class="Insecta" family="Miridae" genus="Metoisops" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="402" phylum="Arthropoda" rank="species" species="groehni" status="sp. nov.">
<emphasis box="[1312,1435,1897,1921]" italics="true" pageId="1" pageNumber="402">M. groehni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[151,241,1932,1957]" pageId="1" pageNumber="402" rank="species">
