are more elongate than 
<taxonomicName box="[988,1175,1880,1904]" class="Insecta" family="Cynipidae" genus="Amphibolips" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="48" phylum="Arthropoda" rank="species" species="zacatecaensis">
<emphasis box="[988,1175,1880,1904]" italics="true" pageId="1" pageNumber="48">A. zacatecaensis</emphasis>
</taxonomicName>
(
<figureCitation box="[1192,1323,1880,1904]" captionStart="FIGURES 14 – 20. 14 – 17" captionStartId="5.[151,264,1977,2000]" captionTargetBox="[181,1405,219,1955]" captionTargetId="figure@5.[181,1405,219,1956]" captionTargetPageId="5" captionText="FIGURES 14 – 20. 14 – 17, Amphibolips zacatecaensis, new species: 14, forewing, female; 15 – 17, gall (photos by J. Pujade-Villar); 18 – 20, Amphibolips hidalgoensis, new species, gall (photos by J. Pujade-Villar)." httpUri="https://zenodo.org/record/279221/files/figure.png" pageId="1" pageNumber="48">
Figs 
<number box="[1251,1323,1880,1904]" pageId="1" pageNumber="48" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
</figureCitation>
), spindleshaped (
<figureCitation box="[243,375,1916,1940]" captionStart="FIGURES 42 – 48" captionStartId="11.[151,264,1756,1779]" captionTargetBox="[151,1436,196,1735]" captionTargetId="figure@11.[151,1436,193,1735]" captionTargetPageId="11" captionText="FIGURES 42 – 48. Galls: 42 – 46, Amphibolips nassa; 44 – 45, A. fusus; 46, A. jubatus (photos by G. Melika, Kinsey collection, AMNH, NYC). 47 – 48, A. fusus, female: 48, head, anterior view; 49, mesoscutellum, dorsal view." httpUri="https://zenodo.org/record/279225/files/figure.png" pageId="1" pageNumber="48">
Figs 
<number box="[300,375,1916,1940]" pageId="1" pageNumber="48" value="42.5" valueMax="43.0" valueMin="42.0">42–43</number>
