<paragraph blockId="25.[151,1438,152,719]" pageId="25" pageNumber="26">
<emphasis box="[197,355,152,177]" italics="true" pageId="25" pageNumber="26">
<taxonomicName box="[197,351,152,177]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="25" pageNumber="26" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
