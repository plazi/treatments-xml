<emphasis bold="true" pageId="0" pageNumber="95">A-H</emphasis>
<taxonomicName authority="" authorityName="Glushchenko, Kulikovskiy &amp; Kociolek" authorityYear="2021" class="Bacillariophyceae" family="Cymbellaceae" genus="Cymbopleura" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Cymbopleura natellia" order="Cymbellales" pageId="0" pageNumber="95" phylum="Bacillariophyta" rank="species" species="natellia">
