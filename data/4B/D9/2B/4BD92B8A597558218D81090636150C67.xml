<taxonomicName authority="" authorityName="K.Krammer" authorityYear="2003" baseAuthorityName="Ehrenb." class="Bacillariophyceae" family="Cymbellaceae" genus="Cymbopleura" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Cymbopleura inaequalis" order="Cymbellales" pageId="0" pageNumber="95" phylum="Bacillariophyta" rank="species" species="inaequalis">
