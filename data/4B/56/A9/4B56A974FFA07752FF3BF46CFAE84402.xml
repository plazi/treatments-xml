</paragraph>
<paragraph blockId="10.[809,1419,160,352]" pageId="10" pageNumber="299">
<quantity box="[810,882,160,184]" metricMagnitude="-1" metricUnit="m" metricValue="2.8" pageId="10" pageNumber="299" unit="cm" value="28.0">28 cm</quantity>
