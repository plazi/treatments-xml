<specimenCode box="[230,386,1309,1333]" collectionCode="INVE" pageId="26" pageNumber="632">INVE 34237</specimenCode>
, 
<location LSID="urn:lsid:plazi:treatment:4B6087F18B50FFD7CB2AFBBFFE9BFC55:C616603C8B50FFD1CBD5FA98FDD6FA92" box="[400,575,1309,1333]" country="Italy" municipality="Island of Capri" name="Mediterranean" pageId="26" pageNumber="632">Mediterranean</location>
, 
<collectingCountry box="[590,645,1309,1333]" name="Italy" pageId="26" pageNumber="632">Italy</collectingCountry>
, 
<collectingMunicipality box="[659,842,1309,1333]" pageId="26" pageNumber="632">Island of Capri</collectingMunicipality>
, coll. 
<date box="[915,1115,1309,1333]" pageId="26" pageNumber="632" value="1892-01-22">
<collectingDate box="[915,1115,1309,1333]" pageId="26" pageNumber="632" value="1892-01-22">22 January 1892</collectingDate>
