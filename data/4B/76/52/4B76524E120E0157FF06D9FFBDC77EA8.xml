, PL 
<quantity box="[763,871,1541,1567]" metricMagnitude="-3" metricUnit="m" metricValue="1.26" pageId="9" pageNumber="10" unit="mm" value="1.26">1.26 mm</quantity>
, PW 
<quantity box="[934,1041,1541,1567]" metricMagnitude="-3" metricUnit="m" metricValue="1.48" pageId="9" pageNumber="10" unit="mm" value="1.48">1.48 mm</quantity>
, Ediam 
<quantity box="[1137,1245,1541,1567]" metricMagnitude="-4" metricUnit="m" metricValue="4.6" pageId="9" pageNumber="10" unit="mm" value="0.46">0.46 mm</quantity>
, Edist 
<quantity box="[1324,1432,1541,1567]" metricMagnitude="-4" metricUnit="m" metricValue="6.4" pageId="9" pageNumber="10" unit="mm" value="0.64">0.64 mm</quantity>
