<quantity box="[748,841,352,378]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="16" pageNumber="17" unit="mm" value="1.2">1.2 mm</quantity>
, PW 
<quantity box="[904,998,352,378]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="16" pageNumber="17" unit="mm" value="1.5">1.5 mm</quantity>
