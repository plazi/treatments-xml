<quantity box="[743,835,1848,1874]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="20" pageNumber="21" unit="mm" value="1.2">1.2 mm</quantity>
, PW 
<quantity box="[897,1004,1848,1874]" metricMagnitude="-3" metricUnit="m" metricValue="1.36" pageId="20" pageNumber="21" unit="mm" value="1.36">1.36 mm</quantity>
, Ediam 
<quantity box="[1098,1205,1848,1874]" metricMagnitude="-4" metricUnit="m" metricValue="5.4" pageId="20" pageNumber="21" unit="mm" value="0.54">0.54 mm</quantity>
, Edist 
<quantity box="[1282,1390,1848,1874]" metricMagnitude="-4" metricUnit="m" metricValue="6.2" pageId="20" pageNumber="21" unit="mm" value="0.62">0.62 mm</quantity>
, EL 
<quantity box="[151,244,1888,1914]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" pageId="20" pageNumber="21" unit="mm" value="6.5">6.5 mm</quantity>
