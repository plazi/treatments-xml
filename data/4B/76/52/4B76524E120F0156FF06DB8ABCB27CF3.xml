<quantity box="[393,500,1066,1092]" metricMagnitude="-3" metricUnit="m" metricValue="6.65" pageId="8" pageNumber="9" unit="mm" value="6.65">6.65 mm</quantity>
, HW 
<quantity box="[567,673,1066,1092]" metricMagnitude="-3" metricUnit="m" metricValue="1.41" pageId="8" pageNumber="9" unit="mm" value="1.41">1.41 mm</quantity>
