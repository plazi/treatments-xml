<quantity box="[726,833,1066,1092]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="8" pageNumber="9" unit="mm" value="1.05">1.05 mm</quantity>
, PW 
<quantity box="[895,1002,1066,1092]" metricMagnitude="-3" metricUnit="m" metricValue="1.13" pageId="8" pageNumber="9" unit="mm" value="1.13">1.13 mm</quantity>
, Ediam 
<quantity box="[1097,1204,1066,1092]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="8" pageNumber="9" unit="mm" value="0.5">0.50 mm</quantity>
, Edist 
<quantity box="[1282,1388,1066,1092]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="8" pageNumber="9" unit="mm" value="0.45">0.45 mm</quantity>
, EL 
<quantity box="[151,244,1106,1132]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" pageId="8" pageNumber="9" unit="mm" value="5.6">5.6 mm</quantity>
