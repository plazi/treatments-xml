<subSubSection pageId="20" pageNumber="21" type="etymology">
<paragraph blockId="20.[151,1436,152,378]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[198,339,311,337]" pageId="20" pageNumber="21">Etymology.</emphasis>
