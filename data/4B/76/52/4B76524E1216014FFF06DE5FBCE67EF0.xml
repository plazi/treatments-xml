<quantity box="[617,710,1621,1647]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="17" pageNumber="18" unit="mm" value="1.9">1.9 mm</quantity>
, PL 
<quantity box="[763,871,1621,1647]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="17" pageNumber="18" unit="mm" value="1.05">1.05 mm</quantity>
, PW 
<quantity box="[934,1042,1621,1647]" metricMagnitude="-3" metricUnit="m" metricValue="1.23" pageId="17" pageNumber="18" unit="mm" value="1.23">1.23 mm</quantity>
, Ediam 
<quantity box="[1137,1245,1621,1647]" metricMagnitude="-4" metricUnit="m" metricValue="5.6" pageId="17" pageNumber="18" unit="mm" value="0.56">0.56 mm</quantity>
, Edist 
<quantity box="[1324,1432,1621,1647]" metricMagnitude="-4" metricUnit="m" metricValue="5.699999999999999" pageId="17" pageNumber="18" unit="mm" value="0.57">0.57 mm</quantity>
