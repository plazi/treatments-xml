<subSubSection pageId="0" pageNumber="73" type="materials_examined">
<paragraph pageId="0" pageNumber="73">Material examined.</paragraph>
<paragraph pageId="0" pageNumber="73">
Two specimens 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="1.2" metricValueMax="1.6" metricValueMin="0.8" unit="mm" value="12.0" valueMax="16.0" valueMin="8.0">8-16 mm</quantity>
