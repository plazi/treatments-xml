<collectingCountry box="[762,843,705,729]" name="Turkey" pageId="2" pageNumber="83">Turkey</collectingCountry>
, 
<collectingCountry box="[855,1013,705,729]" name="Turkmenistan" pageId="2" pageNumber="83">Turkmenistan</collectingCountry>
, and 
<collectingCountry box="[1070,1200,705,729]" name="Uzbekistan" pageId="2" pageNumber="83">Uzbekistan</collectingCountry>
