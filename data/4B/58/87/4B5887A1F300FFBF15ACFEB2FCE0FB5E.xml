<collectingCountry box="[411,537,612,636]" name="Azerbaijan" pageId="3" pageNumber="84">Azerbaijan</collectingCountry>
, 
<collectingCountry box="[548,596,613,636]" name="Iran" pageId="3" pageNumber="84">Iran</collectingCountry>
, and 
<collectingCountry box="[655,813,612,636]" name="Turkmenistan" pageId="3" pageNumber="84">Turkmenistan</collectingCountry>
