<paragraph blockId="11.[151,717,1082,1108]" box="[151,717,1082,1108]" pageId="11" pageNumber="30">
<heading bold="true" box="[151,717,1082,1108]" fontSize="11" level="1" pageId="11" pageNumber="30" reason="1">
<taxonomicName authority="Westwood, 1842" authorityName="Westwood" authorityYear="1842" box="[151,717,1082,1108]" class="Insecta" family="Cicadidae" genus="Megapomponia" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="30" phylum="Arthropoda" rank="species" species="imperatoria">
<emphasis bold="true" box="[151,717,1082,1108]" pageId="11" pageNumber="30">
<emphasis bold="true" box="[151,489,1082,1108]" italics="true" pageId="11" pageNumber="30">Megapomponia imperatoria</emphasis>
(
<bibRefCitation author="Westwood" box="[504,709,1082,1108]" pageId="11" pageNumber="32" refString="Westwood, J. O. (1842) Insectorum novorum centuria, auctore. Annals and Magazine of Natural History, (1), 9, 118 - 119." type="journal article" year="1842">
