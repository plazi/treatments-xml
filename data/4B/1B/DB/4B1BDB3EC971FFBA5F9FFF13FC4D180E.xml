<taxonomicName authorityName="Alcock" authorityYear="1898" box="[742,875,431,455]" class="Elasmobranchii" family="Narcinidae" genus="Benthobatis" kingdom="Animalia" order="Torpediniformes" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="moresbyi">
