<paragraph blockId="1.[151,701,1738,1797]" box="[151,701,1738,1764]" pageId="1" pageNumber="485">
<heading bold="true" box="[151,701,1738,1764]" fontSize="11" level="1" pageId="1" pageNumber="485" reason="1">
<emphasis bold="true" box="[151,701,1738,1764]" pageId="1" pageNumber="485">
<taxonomicName box="[151,600,1738,1764]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="485" phylum="Arthropoda" rank="species" species="iannuzzae" status="sp. nov." subGenus="Dactylotrichia">
<emphasis bold="true" box="[151,272,1738,1764]" italics="true" pageId="1" pageNumber="485">Oxyethira</emphasis>
(
<emphasis bold="true" box="[288,460,1738,1764]" italics="true" pageId="1" pageNumber="485">Dactylotrichia</emphasis>
) 
<emphasis bold="true" box="[478,600,1738,1764]" italics="true" pageId="1" pageNumber="485">iannuzzae</emphasis>
