<taxonomicName box="[151,564,152,178]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="16" pageNumber="500" phylum="Arthropoda" rank="species" species="singularis" status="sp. nov." subGenus="Tanytrichia">
<emphasis bold="true" box="[151,272,152,178]" italics="true" pageId="16" pageNumber="500">Oxyethira</emphasis>
(
<emphasis bold="true" box="[288,427,152,178]" italics="true" pageId="16" pageNumber="500">Tanytrichia</emphasis>
) 
<emphasis bold="true" box="[444,564,152,178]" italics="true" pageId="16" pageNumber="500">singularis</emphasis>
