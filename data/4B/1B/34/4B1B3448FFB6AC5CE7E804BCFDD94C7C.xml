<paragraph blockId="16.[151,491,1198,1258]" box="[151,491,1198,1224]" pageId="16" pageNumber="500">
<heading bold="true" box="[151,491,1198,1224]" fontSize="11" level="1" pageId="16" pageNumber="500" reason="1">
<emphasis bold="true" box="[151,491,1198,1224]" pageId="16" pageNumber="500">
<taxonomicName authority="Tanytrichia" authorityName="Tanytrichia" box="[151,437,1198,1224]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="16" pageNumber="500" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,272,1198,1224]" italics="true" pageId="16" pageNumber="500">Oxyethira</emphasis>
(
<emphasis bold="true" box="[288,427,1198,1224]" italics="true" pageId="16" pageNumber="500">Tanytrichia</emphasis>
)
</taxonomicName>
<emphasis bold="true" box="[444,491,1198,1224]" italics="true" pageId="16" pageNumber="500">una</emphasis>
</emphasis>
