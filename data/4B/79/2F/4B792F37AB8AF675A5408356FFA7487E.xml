<taxonomicName authority="(Lam.) Desv." class="Polypodiopsida" family="Cystopteridaceae" genus="Cystopteris" kingdom="Plantae" order="Polypodiales" pageId="null" pageNumber="127" phylum="Tracheophyta" rank="species" species="montana">
