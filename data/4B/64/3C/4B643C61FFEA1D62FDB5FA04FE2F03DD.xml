<paragraph blockId="10.[569,814,1527,1552]" box="[569,814,1527,1552]" pageId="10" pageNumber="567">
<heading box="[569,814,1527,1552]" centered="true" fontSize="36" level="2" pageId="10" pageNumber="567" reason="7">
<taxonomicName authority="Souza-Filho &amp; Serejo, 2010" authorityName="Souza-Filho &amp; Serejo" authorityYear="2010" box="[569,703,1527,1551]" class="Malacostraca" family="Photidae" genus="Rocasphotis" kingdom="Animalia" order="Amphipoda" pageId="10" pageNumber="567" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[569,703,1527,1551]" italics="true" pageId="10" pageNumber="567">Rocasphotis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[710,814,1528,1552]" pageId="10" pageNumber="567" rank="genus">gen. nov.</taxonomicNameLabel>
</heading>
</paragraph>
