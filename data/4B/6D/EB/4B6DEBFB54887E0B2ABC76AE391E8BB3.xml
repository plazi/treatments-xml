<taxonomicName class="Insecta" family="Braconidae" genus="Opius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opius cheni" order="Hymenoptera" pageId="122" pageNumber="123" phylum="Arthropoda" rank="species" species="cheni">Opius cheni</taxonomicName>
