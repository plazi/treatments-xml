<taxonomicName id="DFAAE0B5FFD5B56C674D63D5AAAE21C6" authority="Attems, 1937" authorityName="Attems" authorityYear="1937" box="[298,704,1506,1528]" class="Diplopoda" family="Paradoxosomatidae" genus="Desmoxytes" kingdom="Animalia" order="Polydesmida" pageId="15" pageNumber="498" phylum="Arthropoda" rank="species" species="spectabilis">
<emphasis id="2ADE4724FFD5B56C674D63D5AA7721C6" box="[298,537,1506,1528]" italics="true" pageId="15" pageNumber="498">Desmoxytes spectabilis</emphasis>
