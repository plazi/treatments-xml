<collectingCountry id="F3777671FFBB4E13FBA5F971FBBB38AE" box="[1048,1113,1757,1781]" name="China" pageId="14" pageNumber="696">China</collectingCountry>
(
<collectingRegion id="49A4F803FFBB4E13FBD1F971FB2D38AE" box="[1132,1231,1757,1781]" country="China" name="Guangxi" pageId="14" pageNumber="696">Guangxi</collectingRegion>
, 
<collectingRegion id="49A4F803FFBB4E13FB60F971FABD38AE" box="[1245,1375,1757,1781]" country="China" name="Guangdong" pageId="14" pageNumber="696">Guangdong</collectingRegion>
, 
<collectingCountry id="F3777671FFBB4E13FAD2F972FC89394D" name="Hong Kong" pageId="14" pageNumber="696">Hong Kong</collectingCountry>
, Jiangxi) and northern 
<collectingCountry id="F3777671FFBB4E13FBD5F951FB26394E" box="[1128,1220,1789,1813]" name="Vietnam" pageId="14" pageNumber="696">Vietnam</collectingCountry>
(
<figureCitation id="135B2A64FFBB4E13FB69F951FAC4394E" box="[1236,1318,1789,1813]" captionStart="Fig" captionStartId="15.[835,871,653,675]" captionTargetBox="[836,1475,189,638]" captionTargetId="figure-699@15.[824,1487,179,648]" captionTargetPageId="15" captionText="Fig. 10. Distribution of Parasinoennea, new genus, Sinoennea Kobelt, 1904, Pupennea, new genus, Platylennea, new genus, and Rowsonia, new genus." figureDoi="http://doi.org/10.5281/zenodo.5349795" httpUri="https://zenodo.org/record/5349795/files/figure.png" pageId="14" pageNumber="696">Fig. 10</figureCitation>
