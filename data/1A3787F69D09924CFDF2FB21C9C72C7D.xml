based his description of 
<taxonomicName id="559E4D639D09924EFD56FA02CB662DB7" authorityName="Heller" authorityYear="1914" box="[709,816,1468,1493]" class="Insecta" family="Scarabaeidae" genus="Maechidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="aroae">
