<quantity id="4C9E41C13B1D3A23C8F5AB216B461B29" box="[206,282,1576,1597]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="8.0" metricValueMin="7.0" pageId="14" pageNumber="999" unit="mm" value="7.5" valueMax="8.0" valueMin="7.0">7-8mm</quantity>
