<collectingRegion id="9341F824CF5D1D2C6DE2BC39FBD9FEBF" box="[926,1071,280,307]" country="Mexico" name="Morelos" pageId="58" pageNumber="559">Morelos state</collectingRegion>
(
<taxonomicName id="96854D45CF5D1D2C6A3CBC38FB5BFEBE" baseAuthorityName="Caballero" baseAuthorityYear="1938" box="[1088,1197,280,306]" class="Reptilia" family="Kinosternidae" genus="Kinosternon" kingdom="Animalia" order="Testudines" pageId="58" pageNumber="559" phylum="Chordata" rank="species" species="hirtipes">
