<taxonomicName id="96854D45CF5D1D2C6EDCBAF0FE52F867" authorityName="Price" authorityYear="1939" box="[160,420,2001,2027]" class="Monogenea" family="Polystomatidae" genus="Polystomoidella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polystomatidea" pageId="58" pageNumber="559" phylum="Platyhelminthes" rank="species" species="whartoni">
