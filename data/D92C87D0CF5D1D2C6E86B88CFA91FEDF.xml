<taxonomicName id="96854D45CF5D1D2C6D51BC1BFC5CFEDF" baseAuthorityName="Caballero &amp; Herrera-Rosales" baseAuthorityYear="1947" box="[813,938,314,339]" class="Reptilia" family="Kinosternidae" genus="Kinosternon" kingdom="Animalia" order="Testudines" pageId="58" pageNumber="559" phylum="Chordata" rank="species" species="integrum">
