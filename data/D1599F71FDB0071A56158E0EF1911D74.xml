<taxonomicName id="BCAEF1F781B6A847838D3250F39EABD4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32702" class="Insecta" family="Formicidae" genus="Myrmicocrypta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Myrmicocrypta collaris Emery" order="Hymenoptera" pageNumber="252" phylum="Arthropoda" rank="species" species="collaris">Myrmicocrypta collaris</taxonomicName>
