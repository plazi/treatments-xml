<collectingCountry id="7F8A7611FFEFE30A82640ED570A29BFC" box="[359,421,477,503]" name="India" pageId="3" pageNumber="346">India</collectingCountry>
: 
<collectingRegion id="C559F863FFEFE30A82B30ED5733A9BFC" box="[432,573,477,503]" country="India" name="Uttarakhand" pageId="3" pageNumber="346">Uttarakhand</collectingRegion>
, 
<collectingRegion id="C559F863FFEFE30A814B0ED573CF9BFC" box="[584,712,477,503]" country="India" name="Meghalaya" pageId="3" pageNumber="346">Meghalaya</collectingRegion>
