<taxonomicName id="491F063BFD176255336B2F87322F3610" class="Insecta" family="Cerambycidae" genus="Deilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Deilus" order="Coleoptera" pageId="204" pageNumber="205" phylum="Arthropoda" rank="genus">Deilus</taxonomicName>
