<treatmentCitationGroup id="AB22119EB453FFF8A9ECD71AFD84A21F" box="[151,624,1546,1568]" pageId="13" pageNumber="14">
<taxonomicName id="4C324D33B453FFF8A9ECD71AFD84A21F" authority="Zhang and Li, 1999: 107" authorityName="Zhang and Li" authorityPageNumber="107" authorityYear="1999" box="[151,624,1546,1568]" class="Insecta" family="Cicadellidae" genus="Japanagallia" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="hamata">
<emphasis id="B946EAA2B453FFF8A9ECD71AFE9BA21F" box="[151,367,1546,1568]" italics="true" pageId="13" pageNumber="14">Japanagallia hamata</emphasis>
<treatmentCitation id="0A9310A1B453FFF8A80ED71AFD84A21F" author="Zhang" box="[373,624,1546,1568]" page="107" pageId="13" pageNumber="14" year="1999">
<bibRefCitation id="EFA34B41B453FFF8A80ED71AFDB4A21F" author="Zhang" box="[373,576,1546,1568]" pageId="13" pageNumber="14" refString="Zhang, Y. Z. &amp; Li, Z. Z. (1999) Two new species of Agalliinae (Homoptera: Cicadellidae) from Guizhou, China. Entomotaxonomia, 21, 107 - 110. (In Chinese)." type="journal article" year="1999">
Zhang and Li, 
<number id="3DFA2497B453FFF8AB71D71BFDB4A21F" box="[522,576,1547,1568]" pageId="13" pageNumber="14" value="1999.0">1999</number>
