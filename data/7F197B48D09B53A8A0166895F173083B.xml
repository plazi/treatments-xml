<emphasis id="7085C12BFDFE3E628F03E74D93860016" italics="true" pageId="0" pageNumber="41">Terminalia myriocarpa</emphasis>
, 
<taxonomicName id="376720BA9C6A139ACAC06F4D31F390E7" class="Monocotyledoneae" family="Arecaceae" genus="Caryota" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Caryota urens" order="Principes" pageId="0" pageNumber="41" phylum="Angiospermae" rank="species" species="urens">
