<emphasis id="C9BDEA95FFE4FFC3B9C8F88D86B1F893" bold="true" box="[667,692,1852,1873]" pageId="4" pageNumber="5">D.</emphasis>
<taxonomicName id="3CC94D04FFE4FFC3B9E9F88C8735F890" box="[698,816,1853,1874]" class="Insecta" family="Miridae" genus="Pilophorus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="meteorus">
