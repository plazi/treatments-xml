<taxonomicName id="3CC94D04FFE4FFC3BA62F88C85D1F890" box="[305,468,1852,1874]" class="Insecta" family="Miridae" genus="Pilophorus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="subparallelus">
