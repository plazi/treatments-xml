<taxonomicName id="4C0BEAD8BE63FF99A56269A6FDE4FF05" authorityName="Lepeletier &amp; Audinet-Serville in Latreille" authorityYear="1828" box="[1125,1244,152,177]" class="Insecta" family="Cerambycidae" genus="Euryptera" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="43" phylum="Arthropoda" rank="genus">
