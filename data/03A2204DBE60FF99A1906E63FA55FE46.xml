<taxonomicName id="4C0BEAD8BE63FF99A41F69A6F81EFF6E" authority="Bates, 1885" authorityName="Bates" authorityYear="1885" class="Insecta" family="Cerambycidae" genus="Choriolaus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="43" phylum="Arthropoda" rank="genus">
