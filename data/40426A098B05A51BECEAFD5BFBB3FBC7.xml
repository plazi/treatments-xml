(
<location id="CD348DC48B00A51AECECFAF6FF73FA20" LSID="urn:lsid:plazi:treatment:40426A098B05A51BECEAFD5BFBB3FBC7:CD348DC48B00A51AECECFAF6FF73FA20" box="[145,200,1402,1426]" country="Peru" county="The" municipality="Jose Gabriel Condorcanqui Noguera" name="Tinta" pageId="4" pageNumber="421" stateProvince="Cusco">Tinta</location>
