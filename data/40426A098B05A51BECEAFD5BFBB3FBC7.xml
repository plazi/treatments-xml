<location id="CD348DC48B00A51AECA1FAF6FEF3FA20" LSID="urn:lsid:plazi:treatment:40426A098B05A51BECEAFD5BFBB3FBC7:CD348DC48B00A51AECA1FAF6FEF3FA20" box="[220,328,1402,1426]" country="Peru" county="The" municipality="Jose Gabriel Condorcanqui Noguera" name="Virreinato" pageId="4" pageNumber="421" stateProvince="Cusco">Virreinato</location>
