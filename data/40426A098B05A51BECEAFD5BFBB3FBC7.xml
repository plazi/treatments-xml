</taxonomicName>
and 
<taxonomicName id="0FEBA09C8B00A51AEFB4F9C9FB98F9EE" baseAuthorityName="Yue &amp; Fu" baseAuthorityYear="2000" box="[969,1059,1605,1628]" class="Collembola" family="Hypogastruridae" genus="Ceratophysella" kingdom="Animalia" order="Poduromorpha" pageId="4" pageNumber="421" phylum="Arthropoda" rank="species" species="yinae">
