<emphasis box="[144,278,1279,1303]" italics="true" pageId="39" pageNumber="736">Distribution</emphasis>
</paragraph>
<paragraph blockId="39.[144,421,1319,1343]" box="[144,421,1319,1343]" pageId="39" pageNumber="736">
<collectionCode box="[144,195,1319,1343]" country="Canada" lsid="urn:lsid:biocol.org:col:12562" name="Agriculture and Agri-Food Canada" pageId="39" pageNumber="736" type="Herbarium">SCS</collectionCode>
