<geoCoordinate direction="north" orientation="latitude" precision="5" value="45.8406">45.8406°N</geoCoordinate>
, 
<geoCoordinate direction="west" orientation="longitude" precision="5" value="-66.7321">66.7321°W</geoCoordinate>
