<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181863" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole delicata Wilson" lsidName-HNS="Pheidole delicata Wilson" order="Hymenoptera" pageNumber="442" phylum="Arthropoda" rank="species" species="delicata">delicata</taxonomicName>
