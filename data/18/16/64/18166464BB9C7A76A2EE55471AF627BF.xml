, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181848" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole browni Wilson" lsidName-HNS="Pheidole browni Wilson" order="Hymenoptera" pageNumber="442" phylum="Arthropoda" rank="species" species="browni">browni</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181861" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole crinita Wilson" lsidName-HNS="Pheidole crinita Wilson" order="Hymenoptera" pageNumber="442" phylum="Arthropoda" rank="species" species="crinita">crinita</taxonomicName>
