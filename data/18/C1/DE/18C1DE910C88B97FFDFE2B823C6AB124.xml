<taxonomicName authorityName="Valentine" authorityYear="1952" class="Insecta" family="Carabidae" genus="Ameroduvalius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="330" pageNumber="331" phylum="Arthropoda" rank="species" species="jeanneli">
