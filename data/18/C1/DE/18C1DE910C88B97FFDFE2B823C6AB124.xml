<taxonomicName authorityName="Delarouzee" authorityYear="1859" class="Hexapoda" family="Carabidae" genus="Duvalius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="330" pageNumber="331" phylum="Arthropoda" rank="genus">
