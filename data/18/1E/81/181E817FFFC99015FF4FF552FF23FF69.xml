<emphasis box="[862,890,1775,1798]" italics="true" pageId="12" pageNumber="557">D.</emphasis>
</taxonomicName>
</taxonomicName>
subtauricum Tobias, 
<taxonomicName authority="Tobias" authorityName="Tobias" box="[1133,1353,1773,1798]" class="Insecta" family="Braconidae" genus="Dinotrema" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="species" species="sauricum">
<emphasis box="[1133,1271,1775,1798]" italics="true" pageId="12" pageNumber="557">D. sauricum</emphasis>
Tobias
</taxonomicName>
and 
<taxonomicName authority="Fischer" authorityName="Fischer" class="Insecta" family="Braconidae" genus="Dinotrema" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="species" species="incongruens">
<emphasis italics="true" pageId="12" pageNumber="557">D. incongruens</emphasis>
(Fischer)
</taxonomicName>
. 
<taxonomicName box="[412,686,1810,1834]" class="Insecta" family="Braconidae" genus="Dinotrema" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="species" species="fischerianum" status="sp. nov.">
<emphasis box="[412,686,1810,1834]" italics="true" pageId="12" pageNumber="557">Dinotrema fischerianum</emphasis>
