<taxonomicName authorityName="Warren" authorityYear="1906" box="[974,1092,1448,1475]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="528" phylum="Arthropoda" rank="species" species="simplex">
