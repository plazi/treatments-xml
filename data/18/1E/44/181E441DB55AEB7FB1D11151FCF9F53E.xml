<emphasis box="[335,443,1865,1889]" italics="true" pageId="6" pageNumber="525">Crasilogia</emphasis>
</taxonomicName>
spp., wings above: 
<emphasis bold="true" box="[650,669,1865,1890]" pageId="6" pageNumber="525">6.</emphasis>
<taxonomicName box="[677,773,1865,1889]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="dispar">
<emphasis box="[677,773,1865,1889]" italics="true" pageId="6" pageNumber="525">C. dispar</emphasis>
</taxonomicName>
, male, syntype, Papua New Guinea, Upp. Aroa R.; 
<emphasis bold="true" box="[1307,1326,1866,1890]" pageId="6" pageNumber="525">7.</emphasis>
<taxonomicName box="[1334,1430,1865,1889]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="dispar">
<emphasis box="[1334,1430,1865,1889]" italics="true" pageId="6" pageNumber="525">C. dispar</emphasis>
</taxonomicName>
, female, syntype, Papua New Guinea, Upp. Aroa R.; 
<emphasis bold="true" box="[687,706,1901,1926]" pageId="6" pageNumber="525">8.</emphasis>
<taxonomicName authorityName="Warren" authorityYear="1907" box="[713,854,1901,1925]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="flavipennis">
<emphasis box="[713,854,1901,1925]" italics="true" pageId="6" pageNumber="525">C. flavipennis</emphasis>
</taxonomicName>
, male, syntype, Papua New Guinea, Biagi, Mambare R.; 
<emphasis bold="true" box="[151,170,1937,1962]" pageId="6" pageNumber="525">9.</emphasis>
<taxonomicName authorityName="Warren" authorityYear="1907" box="[176,316,1937,1961]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="flavipennis">
<emphasis box="[176,316,1937,1961]" italics="true" pageId="6" pageNumber="525">C. flavipennis</emphasis>
</taxonomicName>
, female, syntype, Papua New Guinea, Biagi, Mambare R.; 
<emphasis bold="true" box="[913,945,1937,1962]" pageId="6" pageNumber="525">10.</emphasis>
<taxonomicName authorityName="Joicey &amp; Talbot" authorityYear="1917" box="[950,1082,1937,1961]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="fulvitincta">
<emphasis box="[950,1082,1937,1961]" italics="true" pageId="6" pageNumber="525">C. fulvitincta</emphasis>
</taxonomicName>
, female, holotype, Indonesia, West Papua, Angi Lakes, Arfak Mts; 
<emphasis bold="true" box="[471,502,1973,1998]" pageId="6" pageNumber="525">11.</emphasis>
<taxonomicName authorityName="Schmidt" authorityYear="2020" box="[508,683,1973,1997]" class="Insecta" family="Geometridae" genus="Crasilogia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="525" phylum="Arthropoda" rank="species" species="diversisimplex">
<emphasis box="[508,683,1973,1997]" italics="true" pageId="6" pageNumber="525">C. diversisimplex</emphasis>
