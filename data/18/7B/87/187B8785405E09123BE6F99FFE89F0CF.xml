<quantity box="[580,678,1698,1722]" metricMagnitude="-4" metricUnit="m" metricValue="2.2" pageId="11" pageNumber="250" unit="mm" value="0.22">0.22 mm</quantity>
long, 
<quantity box="[747,845,1698,1722]" metricMagnitude="-4" metricUnit="m" metricValue="3.2" pageId="11" pageNumber="250" unit="mm" value="0.32">0.32 mm</quantity>
