<quantity box="[413,581,1661,1686]" metricMagnitude="-4" metricUnit="m" metricValue="8.05" metricValueMax="8.1" metricValueMin="8.0" pageId="11" pageNumber="250" unit="mm" value="0.805" valueMax="0.81" valueMin="0.8">0.80–0.81 mm</quantity>
