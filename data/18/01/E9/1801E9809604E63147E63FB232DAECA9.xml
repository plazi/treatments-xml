<taxonomicName authority="All." class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="430" phylum="Tracheophyta" rank="species" species="foetida">
<pageBreakToken pageId="null" pageNumber="430">Carex</pageBreakToken>
