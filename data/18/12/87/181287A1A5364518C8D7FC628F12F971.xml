. 
<taxonomicName box="[285,485,1616,1639]" class="Amphibia" family="Strabomantidae" genus="Holoaden" kingdom="Animalia" order="Anura" pageId="2" pageNumber="63" phylum="Chordata" rank="species" species="pholeter" status="sp. nov.">
<emphasis box="[285,485,1616,1639]" italics="true" pageId="2" pageNumber="63">Holoaden pholeter</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[491,575,1615,1639]" pageId="2" pageNumber="63" rank="species">
