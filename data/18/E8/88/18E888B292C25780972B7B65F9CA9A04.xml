<taxonomicName baseAuthorityName="Bloch" baseAuthorityYear="1782" class="Actinopterygii" family="Acheilognathidae" genus="Rhodeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhodeus amarus" order="Cypriniformes" pageId="0" pageNumber="325" phylum="Chordata" rank="species" species="amarus">
<emphasis italics="true" pageId="0" pageNumber="325">Rhodeus amarus</emphasis>
</taxonomicName>
