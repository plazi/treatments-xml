<location LSID="urn:lsid:plazi:treatment:18196817FFFBFFC9FF64FA0DFD72F8A3:956F8FDAFFFBFFC9FE32F900FD82F937" box="[449,525,1766,1790]" country="Philippines" county="Luzon" latitude="14.35" longLatPrecision="773" longitude="121.32" municipality="Jalajala" name="Luzon" pageId="5" pageNumber="6" stateProvince="Manila">Luzon</location>
