<location LSID="urn:lsid:plazi:treatment:18196817FFFBFFC9FF64FA0DFD72F8A3:956F8FDAFFFBFFC9FDD1F900FDB6F934" box="[546,569,1766,1789]" country="Philippines" county="Luzon" latitude="14.35" longLatPrecision="773" longitude="121.32" municipality="Jalajala" name="In" pageId="5" pageNumber="6" stateProvince="Manila">In</location>
