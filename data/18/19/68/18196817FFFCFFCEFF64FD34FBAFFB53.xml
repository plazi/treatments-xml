<paragraph blockId="2.[151,1437,793,1178]" pageId="2" pageNumber="3">
<materialsCitation ID-GBIF-Occurrence="1455395161" collectingDate="1831-10-25" collectingDateMax="1831-10-27" collectingDateMin="1831-10-25" collectionCode="ZMB" collectorName="Lichtenstein &amp; Nr. &amp; Halieus &amp; Meyen &amp; October" country="China" location="Truro Shoal" pageId="2" pageNumber="3" specimenCount="1" stateProvince="Manila" typeStatus="holotype">
