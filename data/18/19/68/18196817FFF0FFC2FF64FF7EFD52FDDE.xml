<location LSID="urn:lsid:plazi:treatment:18196817FFF0FFC2FF64FF7EFD52FDDE:956F8FDAFFF0FFC2FC16FE75FC73FE63" box="[997,1020,403,426]" country="Peru" county="Apo" latitude="-16.2" longLatPrecision="769" longitude="-71.22" municipality="Meyen" name="In" pageId="14" pageNumber="15" stateProvince="Arequipa Region">In</location>
