<location LSID="urn:lsid:plazi:treatment:18196817FFF4FFC6FF64FF3BFEF2FD94:956F8FDAFFF4FFC6FC2AFE1AFBF0FDDC" box="[985,1151,508,533]" country="Chile" county="Metropolitan Region" latitude="-33.82" longLatPrecision="721" longitude="-70.75" municipality="Paine" name="Buin-Linderos" pageId="10" pageNumber="11">Buin-Linderos</location>
] and 
<location LSID="urn:lsid:plazi:treatment:18196817FFF4FFC6FF64FF3BFEF2FD94:956F8FDAFFF4FFC6FB35FE1AFAB5FDDD" box="[1222,1338,508,532]" country="Chile" county="Metropolitan Region" latitude="-33.82" longLatPrecision="721" longitude="-70.75" municipality="Paine" name="Angostura" pageId="10" pageNumber="11">Angostura</location>
pass [= 
<location LSID="urn:lsid:plazi:treatment:18196817FFF4FFC6FF64FF3BFEF2FD94:956F8FDAFFF4FFC6FF64FDC6FE0CFDF1" box="[151,387,543,568]" country="Chile" county="Metropolitan Region" latitude="-33.82" longLatPrecision="721" longitude="-70.75" municipality="Paine" name="Angostura de Paine" pageId="10" pageNumber="11">
