1832, 
<collectorName box="[860,896,619,642]" pageId="3" pageNumber="4">Nr.</collectorName>
69 sub “ 
<taxonomicName box="[999,1134,618,643]" class="Aves" family="Ardeidae" genus="Ardea" kingdom="Animalia" order="Pelecaniformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="leuce">
