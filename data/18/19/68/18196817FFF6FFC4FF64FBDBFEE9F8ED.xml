<collectingRegion box="[1181,1302,1551,1576]" country="Chile" name="Valparaiso" pageId="8" pageNumber="9">Valparaíso</collectingRegion>
, 
<collectingRegion country="Chile" name="Valparaiso" pageId="8" pageNumber="9">Valparaíso Region</collectingRegion>
, 
<geoCoordinate box="[246,338,1588,1613]" direction="south" orientation="latitude" pageId="8" pageNumber="9" precision="555" value="-33.05">33.05°S</geoCoordinate>
, 
<geoCoordinate box="[349,455,1588,1613]" direction="west" orientation="longitude" pageId="8" pageNumber="9" precision="555" value="-71.6">71.60°W</geoCoordinate>
).
</materialsCitation>
</paragraph>
