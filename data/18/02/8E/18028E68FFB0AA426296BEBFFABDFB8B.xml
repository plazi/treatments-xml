<emphasis box="[686,719,1897,1920]" italics="true" pageId="3" pageNumber="4">dm</emphasis>
, disc margin; 
<emphasis box="[873,903,1897,1920]" italics="true" pageId="3" pageNumber="4">AF</emphasis>
, anal fold; 
<emphasis box="[1027,1055,1897,1920]" italics="true" pageId="3" pageNumber="4">SL</emphasis>
