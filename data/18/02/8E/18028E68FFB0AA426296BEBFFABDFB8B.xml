<emphasis box="[1405,1429,1862,1885]" italics="true" pageId="3" pageNumber="4">ds</emphasis>
, discal spot; 
<emphasis box="[280,307,1897,1920]" italics="true" pageId="3" pageNumber="4">pb</emphasis>
, postbasal pattern; 
<emphasis box="[516,551,1897,1920]" italics="true" pageId="3" pageNumber="4">BD</emphasis>
