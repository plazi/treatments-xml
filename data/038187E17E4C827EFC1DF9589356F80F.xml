</taxonomicName>
, &amp; 
<taxonomicName id="4C284D747E4C827FFBACF93F95F6F918" box="[1082,1232,1739,1763]" class="Actinopterygii" family="Zenarchopteridae" genus="Nomorhamphus" kingdom="Animalia" order="Beloniformes" pageId="4" pageNumber="251" phylum="Chordata" rank="species" species="sagittarius">
<emphasis id="B95CEAE57E4C827FFBACF93F95F6F918" bold="true" box="[1082,1232,1739,1763]" italics="true" pageId="4" pageNumber="251">N. sagittarius</emphasis>
</taxonomicName>
