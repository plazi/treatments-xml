, 
<taxonomicName id="4C284D747E4C827FFB23F95F9473F938" authorityName="Huylebrouck, Hadiaty &amp; Herder" authorityYear="2014" box="[1205,1365,1707,1731]" class="Actinopterygii" family="Zenarchopteridae" genus="Nomorhamphus" kingdom="Animalia" order="Beloniformes" pageId="4" pageNumber="251" phylum="Chordata" rank="species" species="lanceolatus">
