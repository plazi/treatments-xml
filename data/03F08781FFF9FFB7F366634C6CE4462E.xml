<taxonomicName id="4C594D14FFF7FFB7F0C2627C69BF462E" authorityName="Hughes" authorityYear="1948" box="[386,467,1026,1050]" class="Arachnida" family="Phytoseiidae" genus="Neoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="8" pageNumber="1721" phylum="Arthropoda" rank="species" species="barkeri">
