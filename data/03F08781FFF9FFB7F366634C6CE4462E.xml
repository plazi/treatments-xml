<taxonomicName id="4C594D14FFF8FFB8F1B8624069CE4660" authorityName="Palevsky &amp; Gal &amp; Ueckermann" authorityYear="2009" box="[248,418,1086,1108]" class="Arachnida" family="Phytoseiidae" genus="Neoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="1720" phylum="Arthropoda" rank="species" species="cozae">
