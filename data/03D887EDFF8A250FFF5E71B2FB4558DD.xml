<taxonomicName id="4C714D78FF88250FFEAF741FFDF05B6F" authorityName="Belon" authorityYear="1903" box="[358,586,1332,1358]" class="Insecta" family="Cerambycidae" genus="Pteroplatus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="76" phylum="Arthropoda" rank="species" species="anchora">
