<taxonomicName id="54BB4D73C43EFF80127EFA9CCB73F245" authorityName="Schuh &amp; C. Weirauch" authorityYear="2010" box="[334,625,1353,1375]" class="Insecta" family="Miridae" genus="Melaleucoides" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="39" phylum="Arthropoda" rank="species" species="brevifoliae">
