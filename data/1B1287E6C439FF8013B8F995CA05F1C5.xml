<taxonomicName id="54BB4D73C43EFF8012DCFAB3CB8CF266" authorityName="Schuh &amp; C. Weirauch" authorityYear="2010" box="[492,654,1382,1404]" class="Insecta" family="Miridae" genus="Melaleucoides" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="39" phylum="Arthropoda" rank="genus">
