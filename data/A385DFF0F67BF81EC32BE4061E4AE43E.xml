(
<taxonomicName id="A55EB4A19295EEBE3DBDE175EB042517" class="Magnoliopsida" family="Lamiaceae" genus="Calamintha" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="139" phylum="Tracheophyta" rank="species" species="nepetoides">
