<collectingRegion id="4F210E8F710C237464DFF8EBFDC8F84F" box="[405,535,1896,1922]" country="United States of America" name="New Jersey" pageId="23" pageNumber="24">New Jersey</collectingRegion>
and 
<collectingRegion id="4F210E8F710C23746707F8EBFD64F84F" box="[589,699,1896,1922]" country="United States of America" name="Delaware" pageId="23" pageNumber="24">Delaware</collectingRegion>
, continental slope, 
<quantity id="4A1D6D88710C237466D9F8EBFC34F84F" box="[915,1003,1896,1922]" metricMagnitude="3" metricUnit="m" metricValue="2.013" pageId="23" pageNumber="24" unit="m" value="2013.0">2013 m</quantity>
.
</paragraph>
</subSubSection>
