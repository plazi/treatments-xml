<taxonomicName id="4C5350C6FFBB172DFEA4FA68FE7742D2" box="[288,476,1459,1482]" class="Hexapoda" family="Pelecinidae" genus="Shoushida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="regilla">
