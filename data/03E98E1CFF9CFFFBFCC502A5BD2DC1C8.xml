<geoCoordinate id="EE7459CDFF9DFFF8FE47058EBE70C0FC" box="[412,526,672,696]" degrees="44" direction="east" minutes="26" orientation="longitude" pageId="21" pageNumber="626" precision="15" seconds="37" value="44.44361">44°26’37”E</geoCoordinate>
