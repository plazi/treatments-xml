<subSubSection id="6E5611E9994BA33C89F02D5C9A195922" pageId="null" pageNumber="484" type="nomenclature">
<paragraph id="3EA086FAD5ED4502A25FA6B9D3695A3E" pageId="null" pageNumber="484">
<taxonomicName id="2771FA6EF55B214BA556DFADCA7E6D58" authority="Wahlenb." class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="484" phylum="Tracheophyta" rank="species" species="supina">
