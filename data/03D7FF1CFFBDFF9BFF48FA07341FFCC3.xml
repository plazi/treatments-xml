<taxonomicName id="4C7E3589FFBDFF9BFC44FAB934DCFE1E" authority="(Arrow, 1900)" baseAuthorityName="Arrow" baseAuthorityYear="1900" box="[972,1440,470,494]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="118" pageNumber="117" phylum="Arthropoda" rank="subSpecies" species="rorulenta" subGenus="Phyllophaga" subSpecies="patens">
<emphasis id="B90A9218FFBDFF9BFC44FAB93299FE1E" box="[972,997,471,494]" italics="true" pageId="118" pageNumber="117">P.</emphasis>
(
<emphasis id="B90A9218FFBDFF9BFC7FFAB9356CFE1E" box="[1015,1040,471,494]" italics="true" pageId="118" pageNumber="117">P.</emphasis>
) 
<emphasis id="B90A9218FFBDFF9BFBAAFAB83591FE1E" box="[1058,1261,470,494]" italics="true" pageId="118" pageNumber="117">rorulenta patens</emphasis>
