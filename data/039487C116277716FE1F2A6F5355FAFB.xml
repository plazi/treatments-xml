<taxonomicName id="4C3D4D5416257711FBEA2C2755A3FEE4" authorityName="Lessert" authorityYear="1927" box="[1077,1202,267,294]" class="Arachnida" family="Salticidae" genus="Stenaelurillus" kingdom="Animalia" order="Araneae" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="hirsutus">
