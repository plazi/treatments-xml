<taxonomicName id="4C3D4D5416257711FEEB2C7950A1FEAD" authorityName="Lessert" authorityYear="1927" box="[308,432,338,364]" class="Arachnida" family="Salticidae" genus="Stenaelurillus" kingdom="Animalia" order="Araneae" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="hirsutus">
