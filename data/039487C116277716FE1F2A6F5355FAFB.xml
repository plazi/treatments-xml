<taxonomicName id="4C3D4D5416257711FBC62C04559AFE89" authorityName="Wesolowska &amp; Russell-Smith" authorityYear="2011" box="[1049,1163,302,329]" class="Arachnida" family="Salticidae" genus="Stenaelurillus" kingdom="Animalia" order="Araneae" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="pilosus">
