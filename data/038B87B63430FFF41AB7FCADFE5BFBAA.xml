<taxonomicName id="4C224D233430FFFA1AB7FC18FDACFC7A" authority="Rathke, 1843: 215" authorityName=", Rathke" authorityPageNumber="215" authorityYear="1843" box="[151,580,984,1006]" class="Polychaeta" family="Flabelligeridae" genus="Siphonostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Terebellida" pageId="86" pageNumber="87" phylum="Annelida" rank="species" species="villosum">
