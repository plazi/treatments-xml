<taxonomicName id="4C6660C5FFFD01785AA7FD13F90BD8D8" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[675,1071,654,681]" class="Aves" family="Mimidae" genus="Toxostoma" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="rufum">
