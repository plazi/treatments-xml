<quantity id="F618519ABE69AB2D09B4726BFB92FE17" box="[1000,1074,368,392]" metricMagnitude="2" metricUnit="m" metricValue="2.3" pageId="92" pageNumber="93" unit="m" value="230.0">230 m</quantity>
