<paragraph id="8BC30F53FFFDFFA47CF8F8C6FD3C8E55" blockId="8.[151,710,1806,1869]" box="[151,710,1806,1833]" pageId="8" pageNumber="209">
<heading id="D08BB83FFFFDFFA47CF8F8C6FD3C8E55" bold="true" box="[151,710,1806,1833]" fontSize="11" level="1" pageId="8" pageNumber="209" reason="1">
<emphasis id="B908D341FFFDFFA47CF8F8C6FD3C8E55" bold="true" box="[151,710,1806,1833]" pageId="8" pageNumber="209">
5. 
<taxonomicName id="4C7C74D0FFFDFFA47CDAF8C7FD3C8E55" authority="(Kirschbaum, 1868)" baseAuthorityName="Kirschbaum" baseAuthorityYear="1868" box="[181,710,1806,1833]" class="Insecta" family="Cicadellidae" genus="Neoaliturus" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="guttulatus">
<emphasis id="B908D341FFFDFFA47CDAF8C7FE3F8E54" bold="true" box="[181,453,1806,1833]" italics="true" pageId="8" pageNumber="209">Neoaliturus guttulatus</emphasis>
(Kirschbaum, 1868)
</taxonomicName>
</emphasis>
</heading>
</paragraph>
