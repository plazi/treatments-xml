<taxonomicName id="4C0F25D902224A6E51047FFAFB15FB3B" baseAuthorityName="De Carlo" baseAuthorityYear="1954" box="[1090,1236,1160,1184]" class="Insecta" family="Gelastocoridae" genus="Gelastocoris" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="bridarollii">
