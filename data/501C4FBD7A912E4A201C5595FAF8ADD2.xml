<taxonomicName id="0259AB2AB2224F513F563E107764BD9A" class="Arachnida" family="Nesticidae" genus="Hamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hamus bowoensis" order="Araneae" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="bowoensis">Hamus bowoensis</taxonomicName>
