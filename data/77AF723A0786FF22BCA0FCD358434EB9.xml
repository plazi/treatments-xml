<taxonomicName id="82186185AE060D92E9007C2A0DFE8A13" class="Insecta" family="Melolonthidae" genus="Neoserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neoserica tonkinensis" order="Coleoptera" pageId="18" pageNumber="101" phylum="Arthropoda" rank="species" species="tonkinensis">Neoserica tonkinensis</taxonomicName>
