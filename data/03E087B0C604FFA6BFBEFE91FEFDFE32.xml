<quantity id="4CB19B43C604FFA6BCE3FEF6FBABFEAA" box="[1007,1081,350,372]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="4" pageNumber="239" unit="m" value="1600.0">1600 m</quantity>
