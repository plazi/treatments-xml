<geoCoordinate id="EE7D5061C604FFA6BC4DFEF7FC19FEAA" box="[833,907,351,372]" degrees="38" direction="north" minutes="01" orientation="latitude" pageId="4" pageNumber="239" precision="925" value="38.016666">38°01'N</geoCoordinate>
