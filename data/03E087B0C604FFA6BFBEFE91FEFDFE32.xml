<geoCoordinate id="EE7D5061C604FFA6BC96FEF7FC73FEAA" box="[922,993,351,372]" degrees="22" direction="east" minutes="11" orientation="longitude" pageId="4" pageNumber="239" precision="925" value="22.183332">22°11'E</geoCoordinate>
