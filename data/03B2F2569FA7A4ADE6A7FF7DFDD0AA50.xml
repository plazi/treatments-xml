<taxonomicName id="4C1B38C39FA7A4ADE708FF1AFDA4AFF8" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[291,610,205,227]" class="Insecta" family="Pyralidae" genus="Galleria" kingdom="Animalia" order="Lepidoptera" pageId="139" pageNumber="140" phylum="Arthropoda" rank="species" species="mellonella">
