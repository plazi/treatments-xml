<tr id="BFBA34845F16C0B1C40387408D74F8B4" box="[267,1307,1871,1894]" gridrow="3" pageId="7" pageNumber="64" rowspan-0="1" rowspan-1="1" rowspan-2="1" rowspan-3="1" rowspan-4="1">
<td id="FC6B5DF85F16C0B1C61487408D74F8B4" box="[796,1307,1871,1894]" gridcol="5" gridrow="3" pageId="7" pageNumber="64">the hind tibia.</td>
</tr>
