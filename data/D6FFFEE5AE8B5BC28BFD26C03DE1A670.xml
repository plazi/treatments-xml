<taxonomicName id="3B3470A3CECAA00933F5C6B71670883E" authorityName="M. Tang &amp; H. L. Zheng" authorityYear="2020" class="Magnoliopsida" family="Asteraceae" genus="Synotis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Synotis panzhouensis" order="Asterales" pageId="0" pageNumber="79" phylum="Tracheophyta" rank="species" species="panzhouensis">
