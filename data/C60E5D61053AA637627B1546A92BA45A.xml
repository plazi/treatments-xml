<taxonomicName id="C753A630ABD1411D1F97CA39B110BE7F" class="Insecta" family="Ephydridae" genus="Ditrichophora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ditrichophora cana" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="cana">Ditrichophora cana</taxonomicName>
