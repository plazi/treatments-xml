<typeStatus id="54F32FDDFFD5FFA8FD4B561BD61F702C" box="[763,861,1117,1143]" pageId="3" pageNumber="264">holotype</typeStatus>
and 
<typeStatus id="54F32FDDFFD5FFA8FC24561BD6AC702C" box="[916,1006,1117,1143]" pageId="3" pageNumber="264">allotype</typeStatus>
will be returned to Father Kuschel for deposit in the collection of the Universidade de 
<collectingCountry id="F35FD1EFFFD5FFA8FD0956C7D7BA70C0" box="[697,760,1153,1179]" name="Chile" pageId="3" pageNumber="264">Chile</collectingCountry>
, 
<collectingRegion id="498C5F9DFFD5FFA8FCB456C7D62E70C0" box="[772,876,1153,1179]" country="Chile" name="Region Metropolitana" pageId="3" pageNumber="264">Santiago</collectingRegion>
