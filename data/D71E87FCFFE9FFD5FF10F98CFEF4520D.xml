<taxonomicName id="98B74D69FFF6FFD5FE2DFCACFD2A522D" authorityName="Peck and Cook" authorityYear="2014" box="[509,684,835,859]" class="Insecta" family="Leiodidae" genus="Isoplastus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="15" phylum="Arthropoda" rank="species" species="hispaniolensis">
<emphasis id="6DC3EAF8FFF6FFD5FE2DFCACFD2A522D" box="[509,684,835,859]" italics="true" pageId="16" pageNumber="15">hispaniolensis</emphasis>
</taxonomicName>
