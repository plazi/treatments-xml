<subSubSection id="17AD6561FFF6FFD5FF10FCACFEF4520D" pageId="16" pageNumber="15" type="etymology">
<paragraph id="5F0836EAFFF6FFD5FF10FCACFEF4520D" blockId="16.[192,1440,835,891]" pageId="16" pageNumber="15">
<emphasis id="6DC3EAF8FFF6FFD5FF10FCACFED5522D" bold="true" box="[192,339,835,859]" pageId="16" pageNumber="15">Etymology</emphasis>
