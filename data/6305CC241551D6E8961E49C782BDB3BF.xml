<paragraph id="D0090C86E9AB3C63B2A54A413B0D9580" pageId="null" pageNumber="407">
<pageBreakToken id="A10393A49694B30F33BD88E827BBAEFE" pageId="null" pageNumber="407" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="43570C1436123BF8A5221F939AD1A0BE" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Carlina" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="407" phylum="Tracheophyta" rank="species" species="vulgaris">
Carlina 
<normalizedToken id="BA0122909B8C2BBC22664F5D7DB6EF08" originalValue="vulgáris" pageId="null" pageNumber="407">vulgaris</normalizedToken>
<authorityName id="760C7E458C2053F1035511E65A897D80" pageId="null" pageNumber="407">L.</authorityName>
</taxonomicName>
</paragraph>
