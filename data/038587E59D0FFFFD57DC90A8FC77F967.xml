<taxonomicName id="4C2C4D709D0FFFFD54DA9388FC20F93B" authorityName="Humala" authorityYear="2019" box="[913,1014,1701,1727]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="lopezi">
