<collectingCountry id="F33B76639D0FFFFD558190DCFC9FF98F" box="[714,841,1521,1547]" name="Costa Rica" pageId="20" pageNumber="21">Costa Rica</collectingCountry>
, 
<taxonomicName id="4C2C4D709D0FFFFD541390DCFB7AF98F" authority="Veijalainen, 2014" authorityName="Veijalainen" authorityYear="2014" box="[856,1196,1521,1547]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="maculae">
<emphasis id="B958EAE19D0FFFFD541390DCFC0AF98F" box="[856,988,1521,1547]" italics="true" pageId="20" pageNumber="21">O. maculae</emphasis>
<bibRefCitation id="EFBD4B029D0FFFFD54AE90DCFB7AF98F" author="Veijalainen, A. &amp; Broad, G. R. &amp; Saaksjarvi, I. E." box="[997,1196,1521,1547]" pageId="20" pageNumber="21" pagination="201 - 252" refId="ref63341" refString="Veijalainen, A., Broad, G. R. &amp; Saaksjarvi, I. E. (2014) Twenty seven new species of Orthocentrus (Hymenoptera: Ichneumonidae; Orthocentrinae) with a key to the Neotropical species of the genus. Zootaxa, 3768 (3), 201 - 252. https: // doi. org / 10.11646 / zootaxa. 3768.3.1" type="journal article" year="2014">Veijalainen, 2014</bibRefCitation>
</taxonomicName>
from 
<collectingCountry id="F33B76639D0FFFFD53BC90DCFABCF98F" box="[1271,1386,1521,1547]" name="Nicaragua" pageId="20" pageNumber="21">Nicaragua</collectingCountry>
and 
<collectingCountry id="F33B76639D0FFFFD57DC9338FEC5F9AB" box="[151,275,1557,1583]" name="Costa Rica" pageId="20" pageNumber="21">Costa Rica</collectingCountry>
