<taxonomicName id="79D44D43FFCA8D7CFDF0FD6FFD5EE3B1" box="[543,652,642,667]" class="Insecta" family="Pipunculidae" genus="Chalarus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
