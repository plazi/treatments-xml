<collectingCountry id="85E37624A11AFFDAFC90991D9781F807" box="[801,920,604,630]" name="United States of America" pageId="56" pageNumber="57">Columbia</collectingCountry>
, 
<collectingCountry id="85E37624A11AFFDAFC15991D9019F807" box="[932,1024,604,630]" name="Canada" pageId="56" pageNumber="57">Canada</collectingCountry>
