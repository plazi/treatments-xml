<paragraph id="D4BFCF743E9C2477374367EB9C80671A" pageId="null" pageNumber="386">
<taxonomicName id="A5531FF9A4FB8C807DBAE176E667786C" authority="L." class="Magnoliopsida" family="Rosaceae" genus="Potentilla" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="386" phylum="Tracheophyta" rank="species" species="caulescens">
<pageBreakToken id="36A62865E2D522291B546CDA3EFF952F" pageId="null" pageNumber="386">Potentilla</pageBreakToken>
