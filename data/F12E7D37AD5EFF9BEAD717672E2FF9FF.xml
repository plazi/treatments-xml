(
<bibRefCitation id="1D16B1D0AD5EFF9BE9BD125B2983F99B" author="Wunderlich, J. &amp; Li, S. Q." box="[1021,1278,1567,1592]" pageId="2" pageNumber="247" pagination="335 - 342" refId="ref3258" refString="Wunderlich, J. &amp; Li, S. Q. (1995) Three new spider species and one new genus (family Linyphiidae) from China (Arachnida: Araneae). Beitrage zur Araneologie, 4, 335 - 342." type="journal article" year="1995">Wunderlich &amp; Li 1995</bibRefCitation>
) and from the 
<collectingRegion id="BB4302C3AD5EFF9BEAD712072D82F9FF" box="[151,255,1603,1628]" country="Russia" name="Primorskiy" pageId="2" pageNumber="247">Maritime</collectingRegion>
