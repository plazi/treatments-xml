<taxonomicName id="D0974D485607FFFFE97FFB6EFDB3FAD9" box="[370,568,1258,1280]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="kimminsi" status="new species">
