</taxonomicName>
, 
<emphasis id="25E3EAD95607FFFFEA4EFB6EFD35FAD9" bold="true" box="[579,702,1257,1280]" pageId="41" pageNumber="42">
<taxonomicNameLabel id="3ED057A25607FFFFEA4EFB6EFD35FAD9" box="[579,702,1257,1280]" pageId="41" pageNumber="42" rank="species">new species</taxonomicNameLabel>
