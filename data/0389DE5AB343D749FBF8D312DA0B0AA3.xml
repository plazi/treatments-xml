<quantity id="4CD8C2A9B342D746FB2CD4FDDBAD0942" box="[601,694,496,523]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="23" pageNumber="24" unit="mm" value="5.9">5.9 mm</quantity>
