<taxonomicName id="F780EBFDC57AFC69FF5F26C78C17FD7D" baseAuthorityName="Milne Edwards &amp; Haime" baseAuthorityYear="1848" box="[136,361,729,750]" class="Anthozoa" family="Fungiidae" genus="Cycloseris" kingdom="Animalia" order="Scleractinia" pageId="11" pageNumber="577" phylum="Cnidaria" rank="species" species="hexagonalis">
