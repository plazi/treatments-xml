<treatmentCitation id="0AF2EE2A4D0EFFE97EA532395FA4B5A3" author="Champion G" box="[140,474,1646,1671]" page="75" pageId="15" pageNumber="276" year="1889">
<taxonomicName id="4C53B3B84D0EFFE97EA532395FA4B5A3" authority="Champion, 1889: 75" authorityName="Champion" authorityPageNumber="75" authorityYear="1889" box="[140,474,1646,1671]" class="Insecta" family="Tetratomidae" genus="Eustrophus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="276" phylum="Arthropoda" rank="genus">
<emphasis id="B92714294D0EFFE97EA532395E89B5A2" box="[140,247,1647,1671]" italics="true" pageId="15" pageNumber="276">Eustrophus</emphasis>
<bibRefCitation id="EFC2B5CA4D0EFFE97ED432395FA4B5A3" author="Champion" box="[253,474,1646,1671]" firstAuthor="Champion" pageId="15" pageNumber="276" pagination="1 - 494" refId="ref17773" refString="Champion G (1889) Coleoptera (Lagriidae, Melandryidae, Pythidae, Oedemeridae). In: Godman, FD, Salvin, O (Eds). Biologia Centrali-Americana, Insecta, Coleoptera, Volume 4, Part 1. Dulau, London, 1 - 494." type="book chapter" year="1889">Champion, 1889: 75</bibRefCitation>
</taxonomicName>
</treatmentCitation>
, nec. 
<bibRefCitation id="EFC2B5CA4D0EFFE97C3C32395CE8B5A2" author="Illiger" box="[533,662,1647,1671]" firstAuthor="Illiger" pageId="15" pageNumber="276" pagination="285 - 305" refId="ref18258" refString="Illiger J (1802) Aufzahlung der Kafergattungen nach der Zahl der Fussglieder. Magazin fur Insektenkunde 1: 285 - 305." type="journal article" year="1802">Illiger, 1802</bibRefCitation>
.
