<taxonomicName id="4C534D4EFF9C46312D9CADF1D2882793" authority="V. &amp; Tanasevitch, 2011" authorityName="V. &amp; Tanasevitch" authorityYear="2011" box="[152,395,1448,1469]" class="Arachnida" family="Linyphiidae" genus="Stemonyphantes" kingdom="Animalia" order="Araneae" pageId="31" pageNumber="80" phylum="Arthropoda" rank="species" species="serratus" status="sp. n.">
<emphasis id="B927EADFFF9C46312D9CADF1D2882793" box="[152,395,1448,1469]" italics="true" pageId="31" pageNumber="80">Stemonyphantes serratus</emphasis>
</taxonomicName>
<materialsCitation id="3B3B3C90FF9C46312C96ADF1D1C027DE" collectionCode="Figs" country="Turkey" county="Embolic" location="Uludag" municipality="Tegulum" pageId="31" pageNumber="80" specimenCount="1" typeStatus="paratype">
<taxonomicNameLabel id="A21457A4FF9C46312C96ADF1D2CA2793" box="[402,457,1448,1469]" pageId="31" pageNumber="80" rank="species">sp. n.</taxonomicNameLabel>
, Ƌ 
<typeStatus id="54E8886FFF9C46312CF5ADF1D1452793" box="[497,582,1448,1469]" pageId="31" pageNumber="80">paratype</typeStatus>
, from 
<location id="8E8C6016FF9C46312F8DADF1D1D32793" LSID="urn:lsid:plazi:treatment:03FA87DBFF9E460E2D9CAD4CD08523A8:8E8C6016FF9C46312F8DADF1D1D32793" box="[649,720,1448,1469]" country="Turkey" county="Embolic" municipality="Tegulum" name="Uludag" pageId="31" pageNumber="80">Uludağ</location>
