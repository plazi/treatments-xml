<paragraph id="CDE9193255AAA0FC72CB086824970F9A" pageId="5" pageNumber="18">
<taxonomicName id="FE0AC70D891A71A0044DFF5A99629C44" LSID="D0437B99BE4186E513B886F58EDE1F91" authority="Froyen &amp; De Crop" class="Agaricomycetes" family="Russulacea" genus="Lactifluus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Lactifluus caliendrifer" order="Russulales" pageId="5" pageNumber="18" phylum="Basidiomycota" rank="species" species="caliendrifer">Lactifluus caliendrifer Froyen &amp; De Crop</taxonomicName>
