<typeStatus id="54FFB0E7FFE4FFE0FDBFFC7D8EDEFC78" box="[543,652,992,1018]" pageId="28" pageNumber="29">Holotype</typeStatus>
and 
<specimenCount id="9D42C5CCFFE4FFE0FD65FC7D8FF3FC78" box="[709,929,992,1018]" pageId="28" pageNumber="29" type="female" typeStatus="paratype">2 paratype females</specimenCount>
