<quantity id="4C95D5E9F101FFB4C01CFF37FC5AFFD7" box="[849,951,151,177]" metricMagnitude="-3" metricUnit="m" metricValue="1.24" pageId="6" pageNumber="565" unit="mm" value="1.24">1.24 mm</quantity>
