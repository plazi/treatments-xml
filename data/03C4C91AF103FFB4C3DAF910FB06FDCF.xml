<taxonomicName id="4C6D038FF101FFB4C28BFF37FDD5FFD7" box="[454,568,151,177]" class="Incertae sedis" family="Incertae sedis" genus="Scutellum" higherTaxonomySource="GBIF" kingdom="Fungi" order="Incertae sedis" pageId="6" pageNumber="565" phylum="Incertae sedis" rank="genus">Scutellum</taxonomicName>
