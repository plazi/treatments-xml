<quantity id="4C95D5E9F102FFB7C251F841FE68F89A" box="[284,389,2017,2044]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="5" pageNumber="564" unit="mm" value="1.8">1.80 mm</quantity>
