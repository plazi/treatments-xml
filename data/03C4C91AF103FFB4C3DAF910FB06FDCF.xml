<quantity id="4C95D5E9F102FFB7C1BCF81DFCB4F8BE" box="[753,857,1981,2008]" metricMagnitude="-3" metricUnit="m" metricValue="7.14" pageId="5" pageNumber="564" unit="mm" value="7.14">7.14 mm</quantity>
