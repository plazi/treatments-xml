<quantity id="4C95D5E9F101FFB4C3DAFF37FF10FFD7" box="[151,253,151,177]" metricMagnitude="-3" metricUnit="m" metricValue="4.17" pageId="6" pageNumber="565" unit="mm" value="4.17">4.17 mm</quantity>
