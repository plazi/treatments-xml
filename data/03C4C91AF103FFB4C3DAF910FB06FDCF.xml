<quantity id="4C95D5E9F102FFB7C067F841FC79F89A" box="[810,916,2017,2044]" metricMagnitude="-3" metricUnit="m" metricValue="1.59" pageId="5" pageNumber="564" unit="mm" value="1.59">1.59 mm</quantity>
