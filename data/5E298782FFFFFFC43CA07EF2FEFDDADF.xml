<quantity id="11789B71FFF0FFC43ED37E8FFCF7DABB" box="[740,825,1161,1187]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="9" pageNumber="64" unit="mm" value="3.3">3.3 mm</quantity>
for 
<taxonomicName id="11804D17FFF0FFC43F5F7E8CFC50DABB" authorityName="Casale &amp; Sciaky" authorityYear="1986" box="[872,926,1162,1187]" class="Insecta" family="Carabidae" genus="Oxytrechus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="64" phylum="Arthropoda" rank="species" species="zoiai">
