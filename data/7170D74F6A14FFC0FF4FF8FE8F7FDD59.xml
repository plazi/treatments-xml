<taxonomicName id="3ED91DDA6A14FFC1FF4FF8FE8C9DDB68" authority="Knutson &amp; Orth, 1990" authorityName="Knutson &amp; Orth. Russia-Cherny Mountains" authorityYear="1990" box="[151,696,1878,1905]" class="Insecta" family="Sciomyzidae" genus="Colobaea" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="canadensis">
