<th id="768C5DE3EFC1C5CAFC4FFD65FC00FD04" box="[906,965,683,709]" gridcol="1" gridrow="0" pageId="144" pageNumber="145">Coxa</th>
<th id="768C5DE3EFC1C5CAFC1CFD65FBD2FD04" box="[985,1047,683,709]" gridcol="2" gridrow="0" pageId="144" pageNumber="145">Basis</th>
<th id="768C5DE3EFC1C5CAFBEDFD65FB12FD04" box="[1064,1239,683,709]" gridcol="3" gridrow="0" pageId="144" pageNumber="145">Exopod</th>
<th id="768C5DE3EFC1C5CAFB2CFD65FA5FFD04" box="[1257,1434,683,709]" gridcol="4" gridrow="0" pageId="144" pageNumber="145">Endopod</th>
</tr>
<tr id="355D349FEFC1C5CAFCEFFD03FA5FFD29" box="[810,1434,717,744]" gridrow="1" pageId="144" pageNumber="145">
<th id="768C5DE3EFC1C5CAFCEFFD03FCAFFD29" box="[810,874,717,744]" gridcol="0" gridrow="1" pageId="144" pageNumber="145">Leg 1</th>
<td id="768C5DE3EFC1C5CAFC4FFD03FC00FD29" box="[906,965,717,744]" gridcol="1" gridrow="1" pageId="144" pageNumber="145">0-0</td>
