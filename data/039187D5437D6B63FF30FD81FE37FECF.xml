, 
<taxonomicName id="4C384D40437D6B62FB5FFC28FA53F89C" authority="K.B.Presl, 1851" authorityName="K.B.Presl" authorityYear="1851" box="[1272,1431,1884,1909]" class="Polypodiopsida" family="Tectariaceae" genus="Psomiocarpa" higherTaxonomySource="GBIF" kingdom="Plantae" order="Polypodiales" pageId="10" pageNumber="17" phylum="Tracheophyta" rank="genus">Psomiocarpa</taxonomicName>
