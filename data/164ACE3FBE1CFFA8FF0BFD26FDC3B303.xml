</emphasis>
<taxonomicName id="59E304AABE1CFFA8FEB7FC99FD63B617" authority="Tzvetkova, 1965" authorityName="Tzvetkova" authorityYear="1965" box="[299,676,834,859]" class="Malacostraca" family="Mesogammaridae" genus="Mesogammarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="59" pageNumber="60" phylum="Arthropoda" rank="genus">
<emphasis id="AC97A33BBE1CFFA8FEB7FC99FE19B616" box="[299,478,836,859]" italics="true" pageId="59" pageNumber="60">Mesogammarus</emphasis>
Tzvetkova, 
<number id="282B6D0EBE1CFFA8FDF5FC9FFD63B617" box="[617,676,834,858]" pageId="59" pageNumber="60" value="1965.0">1965</number>
</taxonomicName>
