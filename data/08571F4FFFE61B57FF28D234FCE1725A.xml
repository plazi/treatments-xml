<caption id="D481FED1FFE71B55FF28D6D5FEBC734B" ID-DOI="http://doi.org/10.5281/zenodo.6127753" ID-Zenodo-Dep="6127753" httpUri="https://zenodo.org/record/6127753/files/figure.png" pageId="5" pageNumber="59" startId="5.[151,250,997,1021]" targetBox="[151,1435,204,955]" targetPageId="5">
