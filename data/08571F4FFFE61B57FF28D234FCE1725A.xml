<taxonomicName id="47FED5DAFFE71B55FEA6D6D6FE727497" baseAuthorityName="Regan" baseAuthorityYear="1907" box="[281,440,997,1021]" class="Actinopterygii" family="Cyprinidae" genus="Devario" kingdom="Animalia" order="Cypriniformes" pageId="5" pageNumber="59" phylum="Chordata" rank="species" species="browni">
