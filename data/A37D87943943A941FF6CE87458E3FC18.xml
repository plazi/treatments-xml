<subSubSection id="63CE65093943A943FF6CE8C159EBFE5F" box="[151,1157,402,427]" pageId="8" pageNumber="515" type="etymology">
<paragraph id="2B6B36823943A943FF6CE8C159EBFE5F" blockId="8.[151,1436,402,643]" box="[151,1157,402,427]" pageId="8" pageNumber="515">
<emphasis id="19A0EA903943A943FF6CE8C15C70FE5F" bold="true" box="[151,286,402,427]" pageId="8" pageNumber="515">Etymology.</emphasis>
