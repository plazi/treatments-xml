<taxonomicName id="70CA4D3CFF68E2CC8438F884DF616259" box="[375,623,1879,1904]" class="Hydrozoa" family="Sertulariidae" genus="Papilionella" kingdom="Animalia" order="Leptothecata" pageId="133" pageNumber="134" phylum="Cnidaria" rank="species" species="vervoorti">
