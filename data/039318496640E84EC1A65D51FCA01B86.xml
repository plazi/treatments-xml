<taxonomicName id="4C3AD2DC6640E84EC50A5D96FE961B1D" authority="Remane, 1968" authorityName="Remane" authorityYear="1968" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="argentea">
