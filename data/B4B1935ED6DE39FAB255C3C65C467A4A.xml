<taxonomicName id="D875A55BFBA466956568B31ECBB7E979" authority="Pocock, 1897" authorityName="Pocock" authorityYear="1897" class="Arachnida" family="Idiopidae" genus="Anidiops" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Anidiops manstridgei" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="manstridgei">Anidiops manstridgei</taxonomicName>
