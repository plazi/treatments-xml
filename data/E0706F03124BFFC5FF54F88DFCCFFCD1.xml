<taxonomicName id="AFD9A5961248FFC5FD2DFF2EFC42FF32" authority="Monts" authorityName="Monts" box="[750,999,223,248]" class="Gastropoda" family="Eulimidae" genus="Eulima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="50" pageNumber="51" phylum="Mollusca" rank="species" species="lubrica">Eulima lubrica Monts</taxonomicName>
