<taxonomicName id="4C3F4D04BD3DFFD693D79722FD93F8AA" authority="Verrill, 1881" authorityName="Verrill" authorityYear="1881" box="[189,580,1851,1878]" class="Cephalopoda" family="Sepiolidae" genus="Inioteuthis" higherTaxonomySource="CoL" kingdom="Animalia" order="Sepiida" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="japonica">
<emphasis id="B94BEA95BD3DFFD693D79722FE7EF8A9" box="[189,425,1851,1877]" italics="true" pageId="39" pageNumber="40">Inioteuthis japonica</emphasis>
