<taxonomicName id="F62BE4E418D092A81D9981C3A0B00FE7" authorityName="Fischer" authorityYear="1963" class="Insecta" family="Braconidae" genus="Nipponopius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nipponopius incisus" order="Hymenoptera" pageId="3" pageNumber="126" phylum="Arthropoda" rank="species" species="incisus">
