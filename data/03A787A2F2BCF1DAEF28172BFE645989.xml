<collectionCode id="ED1FAE71F2BCF1DAED541527FDEA5E13" box="[526,619,843,864]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="136" pageNumber="136">AMNH</collectionCode>
