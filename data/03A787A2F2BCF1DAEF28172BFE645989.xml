<specimenCode id="DBA89ECFF2BCF1DAED4B15E9FE8F5ECB" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="136" pageNumber="136">AMNH 690133–690138</specimenCode>
