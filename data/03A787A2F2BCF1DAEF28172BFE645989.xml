<typeStatus id="54B58816F2BCF1DAEFFD15EAFE9D5EE8" box="[167,284,902,923]" pageId="136" pageNumber="136">paratypes</typeStatus>
: southern 
<collectingCountry id="F3197624F2BCF1DAEEF915EAFE7C5EEF" box="[419,509,902,924]" name="Nigeria" pageId="136" pageNumber="136">Nigeria</collectingCountry>
