<taxonomicName id="01CBECCE5FD567401CC5EE7A3CC84A5D" class="Insecta" family="Staphylinidae" genus="Octomicrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Octomicrus" order="Coleoptera" pageId="99" pageNumber="100" phylum="Arthropoda" rank="genus">Octomicrus</taxonomicName>
