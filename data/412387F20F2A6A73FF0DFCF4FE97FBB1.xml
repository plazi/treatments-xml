<collectionCode id="AF9BAE210F2A6A73FEC5FC79FEFAFC5C" box="[338,363,954,977]" country="Japan" name="Herbarium of the Department of Botany, University of Tokyo" pageId="27" pageNumber="380">TI</collectionCode>
