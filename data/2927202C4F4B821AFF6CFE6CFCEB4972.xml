, and 
<collectingRegion id="634A5FD84F4B821BFB0EF86AF8154FA9" box="[1269,1387,1967,1991]" country="Argentina" name="Rio Negro" pageId="2" pageNumber="438">Río Negro</collectingRegion>
and 
<collectingRegion id="634A5FD84F4B821BFF6CF816FD824F85" box="[151,252,2003,2027]" country="Argentina" name="Neuquen" pageId="2" pageNumber="438">Neuquén</collectingRegion>
