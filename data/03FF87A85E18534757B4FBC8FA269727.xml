<taxonomicName id="4C564D3D5E18534756A0FAF3FE4F9727" box="[387,506,1325,1346]" class="Insecta" family="Carabidae" genus="Perigona" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="241" phylum="Arthropoda" rank="species" species="nigriceps">
