</emphasis>
<taxonomicName id="D4A708F91E115579FE12F3E819C1FA79" authority="Erwin" authorityName="Erwin" authorityYear="2000" box="[435,756,1451,1472]" class="Insecta" family="Carabidae" genus="Quammenis" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="279" phylum="Arthropoda" rank="species" species="spectabilis" status="new species">
