<collectingRegion id="49C24FD87139FFBCD69817854961F82A" box="[741,802,1930,1956]" country="Portugal" name="Porto" pageId="3" pageNumber="436">Porto</collectingRegion>
Santo Island, 
<geoCoordinate id="EE32E7FD7139FFBCD7C217854E0BF82A" box="[959,1096,1930,1956]" degrees="33" direction="north" minutes="00" orientation="latitude" pageId="3" pageNumber="436" precision="15" seconds="19" value="33.00528">33°00’19”N</geoCoordinate>
<geoCoordinate id="EE32E7FD7139FFBCD03217864E9CF82D" box="[1103,1247,1929,1955]" degrees="16" direction="west" minutes="22" orientation="longitude" pageId="3" pageNumber="436" precision="15" seconds="13" value="-16.370277">16°22’13”W</geoCoordinate>
