<taxonomicName id="27842CB2FFC5FFB9FE696E92FF36FD65" authority="(Allman, 1888)" authorityName="Allman" authorityYear="1888" baseAuthorityName="Allman" baseAuthorityYear="1888" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="27" pageNumber="282" phylum="Cnidaria" rank="species" species="implexa">
