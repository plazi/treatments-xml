<quantity id="4CF79B33FFB7FF9EC09CFCCBFC8DFCCA" box="[741,795,786,806]" metricMagnitude="2" metricUnit="m" metricValue="5.05" pageId="88" pageNumber="243" unit="m" value="505.0">505m</quantity>
