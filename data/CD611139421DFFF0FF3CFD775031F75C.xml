<geoCoordinate id="20FCC6E8421DFFF1FCF8FC4F569AF275" box="[851,994,1018,1043]" direction="south" orientation="latitude" pageId="2" pageNumber="571" precision="1" value="-32.156765">32°09.406’S</geoCoordinate>
, 
<geoCoordinate id="20FCC6E8421DFFF1FC45FC4F51F1F275" box="[1006,1161,1018,1043]" direction="west" orientation="longitude" pageId="2" pageNumber="571" precision="1" value="-51.471966">
<number id="F300B208421DFFF1FC45FC4F5171F275" box="[1006,1033,1018,1043]" pageId="2" pageNumber="571" value="51.0">51</number>
° 
<number id="F300B208421DFFF1FBB8FC4F511FF275" box="[1043,1127,1018,1043]" pageId="2" pageNumber="571" value="28.318">28.318</number>
’W
</geoCoordinate>
, 
