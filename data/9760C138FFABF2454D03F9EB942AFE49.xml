<quantity id="D831DDCBFFABF2424AD3F9309201F908" box="[1265,1337,1687,1711]" metricMagnitude="2" metricUnit="m" metricValue="1.7" pageId="11" pageNumber="86" unit="m" value="170.0">170 m</quantity>
