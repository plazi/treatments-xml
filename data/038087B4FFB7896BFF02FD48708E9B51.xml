<taxonomicName id="4C294D21FFB78968FF45FCD272EE9A10" authority="Linnaeus" authorityName="Linnaeus" authorityYear="1758" box="[208,506,870,892]" class="Echinoidea" family="Echinidae" genus="Echinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Echinoida" pageId="29" pageNumber="30" phylum="Echinodermata" rank="species" species="reticulatus">
