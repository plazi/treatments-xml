<typeStatus id="54CB609DFF86FFA3FCD0FEC7FCBC0F8D" box="[826,879,379,400]" pageId="35" pageNumber="480">Type</typeStatus>
locality
</emphasis>
: 
<location id="8EAF88E4FF86FFA3FC33FEC6FB130F92" LSID="urn:lsid:plazi:treatment:03D96F29FF86FFA4FCB5FEF6FD5C0F49:8EAF88E4FF86FFA3FC33FEC6FB130F92" box="[985,1216,378,400]" country="Japan" latitude="34.616665" longLatPrecision="1198" longitude="140.53334" name="North Pacific Ocean" pageId="35" pageNumber="480">North Pacific Ocean</location>
, south of 
<collectingCountry id="F3679EAFFF86FFA3FADBFEC6FAAA0F92" box="[1329,1401,378,399]" name="Japan" pageId="35" pageNumber="480">Japan</collectingCountry>
, off Yokohama; 
<collectorName id="2685BBE9FF86FFA3FC2CFE25FB6F0FB3" box="[966,1212,409,430]" pageId="35" pageNumber="480">
H. M. S. 
<emphasis id="B904022DFF86FFA3FBD5FE25FB6F0FB3" box="[1087,1212,409,430]" italics="true" pageId="35" pageNumber="480">Challenger</emphasis>
</collectorName>
sta. 237; 
<geoCoordinate id="EE44B8F8FF86FFA3FAABFE25FA4E0FB3" box="[1345,1437,407,430]" degrees="34" direction="north" minutes="37" orientation="latitude" pageId="35" pageNumber="480" precision="925" value="34.616665">34°37′N</geoCoordinate>
