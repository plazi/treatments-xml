<paragraph id="215236E1E145FFB7FF382C90FDBF5C29" blockId="8.[151,1437,921,1993]" lastBlockId="10.[151,1437,151,357]" lastPageId="10" lastPageNumber="11" pageId="8" pageNumber="9">
<materialsCitation id="91853CBCE145FFB7FF382C90FBF15DB4" ID-GBIF-Occurrence="1291045561" collectingDate="1752-06-01" collectingDateMax="1840-12" collectingDateMin="1752-06-01" collectorName="He &amp; British Museum. In &amp; This &amp; Possibly J." country="Georgia" county="Bulloch County" lastPageId="10" lastPageNumber="11" location="With" municipality="South America" pageId="8" pageNumber="9" specimenCount="2" typeStatus="holotype">
