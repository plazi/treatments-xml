<taxonomicName id="E6ED4D62E145FFB5FEBB2C93FE115BE9" box="[324,484,1677,1701]" class="Insecta" family="Osmylidae" genus="Osmylus" kingdom="Animalia" order="Neuroptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="longicornis">
