<paragraph id="82DD8CC541AF21A4E74A43B46BC92F52" pageId="0" pageNumber="57">
<taxonomicName id="C791310384B6CF0208B67C1BA36CB789" authorityName="Robinson" authorityYear="1975" class="Insecta" family="Dolichopodidae" genus="Chrysotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysotus xiphostoma" order="Diptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="species" species="xiphostoma">
