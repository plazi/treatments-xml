<taxonomicName id="9E1CE9C5E366ADD8199471C1FCE999CD" authorityName="Chopard" authorityYear="1954" class="Insecta" family="Tettigoniidae" genus="Leiodontocercus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leiodontocercus angustipennis" order="Orthoptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="species" species="angustipennis">Leiodontocercus angustipennis</taxonomicName>
