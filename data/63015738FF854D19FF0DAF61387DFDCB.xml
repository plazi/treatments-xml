; consequently, 
<taxonomicName id="2CA89DADFF854D19FA12AF613ADEFE1B" authority="Obst, 1908" authorityName="Obst" authorityYear="1908" class="Insecta" family="Curculionidae" genus="Prionorhinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="449" phylum="Arthropoda" rank="species" species="pleurocleonoides">
