<collectionCode id="ED62DC814E01FFE9FC07F911FB8DF95B" box="[964,1031,1687,1711]" httpUri="http://grbio.org/cool/8qxf-pkip" name="Geologisch-Palaeontologiches Institut der Universitt Haemburg" pageId="1" pageNumber="61">GPIH</collectionCode>
<number id="3DBB56634E01FFE9FBCDF911FBC1F95B" box="[1038,1099,1687,1711]" pageId="1" pageNumber="61" value="4666.0">4666</number>
