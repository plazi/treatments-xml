<taxonomicName id="4C074D63B05BA521AD96FDC6FEB0CB68" authority="Stal" authorityName="Stal" box="[206,359,568,591]" class="Insecta" family="Haglotettigoniidae" genus="Lebinthus" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="67" phylum="Arthropoda" rank="genus">
