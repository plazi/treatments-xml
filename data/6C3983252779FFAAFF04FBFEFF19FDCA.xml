<number id="525820142779FFABFC38FA75FC2DFA58" box="[939,978,1431,1455]" pageId="6" pageNumber="244" value="171.0">171</number>
used for 
<collectionCode id="8281AAF62779FFABFBD5FA75FB7CFA58" LSID="urn:lsid:biocol.org:col:15536" box="[1094,1155,1431,1455]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15536" name="Department of Natural Resources, Environment, The Arts and Sport" pageId="6" pageNumber="244">DNA</collectionCode>
