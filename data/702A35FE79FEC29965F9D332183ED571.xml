<taxonomicName id="D6CF89FECD29072F9886A0A6FC8896C8" class="Insecta" family="Curculionidae" genus="Phloeophagus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phloeophagus apionides" order="Coleoptera" pageId="12" pageNumber="361" phylum="Arthropoda" rank="species" species="apionides">Phloeophagus apionides</taxonomicName>
