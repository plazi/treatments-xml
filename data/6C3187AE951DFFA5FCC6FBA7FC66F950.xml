<specimenCode id="B43E9EC3951DFFA5FC17FB3BFB8DFB4A" box="[946,1071,1154,1176]" collectionCode="MHNB" country="Switzerland" httpUri="http://grbio.org/cool/kt43-hjs1" name="Museum d'Histoire Naturelle de Bale" pageId="47" pageNumber="848">MHNB 58</specimenCode>
