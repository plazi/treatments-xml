<collectingRegion id="265CF85A951DFFA5FB1CFB66FA3FFB26" box="[1209,1437,1246,1268]" country="France" name="Nord-Pas-de-Calais" pageId="47" pageNumber="848">Nord-Pas de Calais</collectingRegion>
