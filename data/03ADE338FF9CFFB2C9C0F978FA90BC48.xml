<materialsCitation id="3B6C5873FF9CFFB2C9F5F844FA2EB3C5" box="[189,1424,1927,1954]" collectingDate="2013-10-28" collectorName="O. L. M. Silva &amp; R. Hoinaski" country="Brazil" location="Atibaia" pageId="9" pageNumber="202" specimenCount="1" stateProvince="Sao Paulo">
