<quantity id="4C949B56FFD4F965FBAFFBCC62F6FB8D" box="[1027,1155,1068,1092]" metricMagnitude="3" metricUnit="m" metricValue="1.61" metricValueMax="3.2" metricValueMin="0.02" pageId="16" pageNumber="17" unit="m" value="1610.0" valueMax="3200.0" valueMin="20.0">20–3200 m</quantity>
