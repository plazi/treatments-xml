<paragraph id="8BD936F46235EE2B7580FB90B2A5FB29" blockId="14.[189,1399,1027,1159]" pageId="14" pageNumber="15">
<taxonomicName id="4C664D776235EE2B7580FB90B53EFBB3" authorityName="Ram. Kumar, Bandyop. &amp; S. Sharma" authorityYear="2014" box="[189,795,1027,1054]" class="Magnoliopsida" family="Fabaceae" genus="Phanera" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="variety" species="glauca" subSpecies="tenuiflora" variety="murlenensis">
