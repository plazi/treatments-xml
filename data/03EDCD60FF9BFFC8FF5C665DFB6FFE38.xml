<number id="3D8C6E51FF9BFFCBFEB7679AFE15F863" box="[380,475,2013,2038]" pageId="5" pageNumber="280" value="145809.0">145.809</number>
°E
</geoCoordinate>
, 
<date id="FFFA5AB6FF9BFFCBFDC96799FD5CF863" box="[514,658,2013,2038]" pageId="5" pageNumber="280" value="1997-07-28">
<number id="3D8C6E51FF9BFFCBFDC96799FDD0F863" box="[514,542,2014,2038]" pageId="5" pageNumber="280" value="28.0">28</number>
July 
<number id="3D8C6E51FF9BFFCBFD916799FD5CF863" box="[602,658,2014,2038]" pageId="5" pageNumber="280" value="1997.0">1997</number>
