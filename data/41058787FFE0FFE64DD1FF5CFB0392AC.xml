<quantity id="0E549B74FFE0FFE6497AFE45FBA792C0" box="[1084,1140,398,422]" metricMagnitude="1" metricUnit="m" metricValue="5.0546" pageId="7" pageNumber="587" unit="in" value="1990.0">1990</quantity>
</taxonomicName>
