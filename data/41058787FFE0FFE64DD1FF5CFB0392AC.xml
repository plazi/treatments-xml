<taxonomicName id="0EAC4D12FFE0FFE64FE4FE45FBA792C0" authority="Rao &amp; Ramakrishnan, 1990" authorityName="Rao &amp; Ramakrishnan" authorityYear="1990" box="[674,1140,397,422]" class="Insecta" family="Cicadellidae" genus="Hecalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="587" phylum="Arthropoda" rank="species" species="morrisoni">
