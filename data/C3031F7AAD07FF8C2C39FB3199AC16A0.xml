<tr id="F59AAC2EAD0700702C39FA3E986610D4" box="[264,1321,1449,1475]" gridrow="1" pageId="1" pageNumber="62">
<th id="B64BC552AD0700702C39FA3E9FC310D4" box="[264,652,1449,1475]" gridcol="0" gridrow="1" pageId="1" pageNumber="62">This species is distinguished</th>
<td id="B64BC552AD0700702FA2FA3E9F8210D4" box="[659,717,1449,1475]" gridcol="1" gridrow="1" pageId="1" pageNumber="62">from</td>
<td id="B64BC552AD0700702FE4FA3E99F810D4" box="[725,1207,1449,1475]" gridcol="2" gridrow="1" pageId="1" pageNumber="62">others lauxaniids by a shiny light brown</td>
<td id="B64BC552AD07007029F1FA3E984C10D4" box="[1216,1283,1449,1475]" gridcol="3" gridrow="1" pageId="1" pageNumber="62">hump</td>
<td id="B64BC552AD070070283DFA3E986610D4" box="[1292,1321,1449,1475]" gridcol="4" gridrow="1" pageId="1" pageNumber="62">on</td>
</tr>
<tr id="F59AAC2EAD0700702C39FA46986610FC" box="[264,1321,1489,1515]" gridrow="2" pageId="1" pageNumber="62" rowspan-3="1" rowspan-4="1">
<th id="B64BC552AD0700702C39FA469FC310FC" box="[264,652,1489,1515]" gridcol="0" gridrow="2" pageId="1" pageNumber="62">the frons; the wing is brown with</th>
