<geoCoordinate id="EE44B8F8FF88FFADFC39FF78FBDF0EC4" box="[979,1036,196,217]" degrees="13" direction="north" orientation="latitude" pageId="45" pageNumber="490" precision="55555" value="13.0">13°N</geoCoordinate>
