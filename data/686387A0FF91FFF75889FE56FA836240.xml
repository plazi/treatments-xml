<taxonomicName id="27CA4D35FF91FFF75A6AFD5AFC356389" authorityName="Rondani" authorityYear="1850" box="[628,902,727,753]" class="Insecta" family="Conopidae" genus="Conopaejus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="aegyptiacus">
