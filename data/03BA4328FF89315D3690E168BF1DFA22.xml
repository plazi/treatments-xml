<geoCoordinate id="EE2794F9FF89315D3422E1B6BFD9FE13" box="[512,647,398,420]" degrees="32" direction="north" minutes="04" orientation="latitude" pageId="15" pageNumber="376" precision="1" seconds="36.5" value="32.076805">N 32°04́36.5́́</geoCoordinate>
<geoCoordinate id="EE2794F9FF89315D34ADE1B6BE4BFE13" box="[655,789,398,420]" degrees="48" direction="east" minutes="14" orientation="longitude" pageId="15" pageNumber="376" precision="1" seconds="15.6" value="48.237667">E 48°14́15.6́́</geoCoordinate>
, 
<date id="FFADD4FEFF89315D3502E1B7BEA0FE13" box="[800,1022,398,420]" pageId="15" pageNumber="376" value="2015-03-11" valueMax="2015-05-10" valueMin="2015-03-11">
