<taxonomicName id="DABA081965F6E73DBF2BD3282B43AEF9" class="Insecta" family="Encyrtidae" genus="Oobius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oobius agrili" order="Hymenoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="species" species="agrili">Oobius agrili</taxonomicName>
