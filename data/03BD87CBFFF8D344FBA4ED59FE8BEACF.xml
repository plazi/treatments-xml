<taxonomicName id="4C144D5EFFF8D347FBDDE64EFAF4E775" authorityName=": Boulenger" authorityYear="1893" box="[1144,1292,2091,2114]" class="Reptilia" family="Homalopsidae" genus="Brachyorrhos" kingdom="Animalia" order="Squamata" pageId="1" pageNumber="516" phylum="Chordata" rank="genus">
