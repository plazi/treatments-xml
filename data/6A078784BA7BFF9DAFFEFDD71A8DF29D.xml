<materialsCitation id="52C63CCFBA7BFFA2AEA4FB101D7BF0BA" box="[461,1145,1213,1239]" collectingDate="1989-12-10" collectorName="Puketitiri" location="Little Bush" pageId="31" pageNumber="32" specimenCount="1" specimenCount-male="1">
