<emphasis id="B962EAD93C01A800FF7F29BE1018FC50" bold="true" box="[151,557,900,927]" pageId="8" pageNumber="139">
Genus 
<taxonomicName id="4C164D483C01A800FF0629BE1018FC50" authority="Luther (1997)" authorityName="Luther" authorityYear="1997" box="[238,557,900,927]" class="Actinopterygii" family="Mugilidae" genus="Osteomugil" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mugiliformes" pageId="8" pageNumber="139" phylum="Chordata" rank="genus">
