<materialsCitation id="3B4CE440FFD31569FF19FEDE9D56FE9E" box="[199,802,294,319]" pageId="35" pageNumber="36" specimenCount="4" specimenCount-female="3" specimenCount-male="1" typeStatus="paratype">
