<taxonomicName id="4C2678DDFF84FF838B03C114230E996F" box="[557,836,1798,1823]" class="Actinopterygii" family="Salmonidae" genus="Coregonus" kingdom="Animalia" order="Salmoniformes" pageId="53" pageNumber="54" phylum="Chordata" rank="species" species="clupeaformis">
