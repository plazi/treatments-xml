<taxonomicName id="52E04D478F15FFB73362AD8EB036E299" box="[335,582,1195,1218]" class="Rhabditophora" family="Gnesiocerotidae" genus="Echinoplana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="4" pageNumber="5" phylum="Platyhelminthes" rank="species" species="celerrima">
<emphasis id="A794EAD68F15FFB73362AD8EB036E299" box="[335,582,1195,1218]" italics="true" pageId="4" pageNumber="5">Echinoplana celerrima</emphasis>
