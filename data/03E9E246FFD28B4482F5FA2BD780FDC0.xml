<taxonomicName id="4C4028D3FFD58B478231F843D640F80D" authority="Samoh &amp; Grootaert" authorityName="Samoh &amp; Grootaert" box="[292,819,1921,1948]" class="Insecta" family="Dolichopodidae" genus="Thinophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="subapicalis" status="sp. nov.">
<emphasis id="B9348F42FFD58B478231F843D4C3F80D" box="[292,432,1922,1948]" italics="true" pageId="13" pageNumber="14">Thinophilus</emphasis>
<taxonomicName id="4C4028D3FFD58B4782ACF843D7EAF80A" authority="Samoh" authorityName="Samoh" box="[441,665,1921,1948]" pageId="13" pageNumber="14" rank="species" species="subapicalis">
