<quantity id="4CE03D80FFE46D3686BEFDDEFBB952EF" box="[979,1071,633,659]" metricMagnitude="3" metricUnit="m" metricValue="4.5" pageId="6" pageNumber="7" unit="m" value="4500.0">4500 m</quantity>
