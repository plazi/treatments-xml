, 
<collectingRegion id="984933D1FF9D767CBA1BFA7CFCBEFA13" box="[783,882,1422,1448]" country="China" name="Guizhou" pageId="15" pageNumber="512">Guizhou</collectingRegion>
, 
<collectingRegion id="984933D1FF9D767CBA6AFA7CFC1EFA13" box="[894,978,1422,1448]" country="China" name="Hainan" pageId="15" pageNumber="512">Hainan</collectingRegion>
