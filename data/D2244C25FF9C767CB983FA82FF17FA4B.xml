<collectingCountry id="229ABDA3FF9D767CB873FA7CFE66FA13" box="[359,426,1422,1448]" name="China" pageId="15" pageNumber="512">China</collectingCountry>
(
<collectingRegion id="984933D1FF9D767CB8ADFA7CFDC9FA13" box="[441,517,1422,1448]" country="China" name="Fujian" pageId="15" pageNumber="512">Fujian</collectingRegion>
, 
