<taxonomicName id="3FF64D7F4978FF996448F911FDB3F954" authority="Russell-Smith, 2017" authorityName="Russell-Smith" authorityYear="2017" box="[346,535,1689,1711]" class="Arachnida" family="Gnaphosidae" genus="Drassyllus" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="246" phylum="Arthropoda" rank="species" species="cyprius" status="sp. n.">
