<taxonomicName id="C9DF108B460EFFF5FAECF3ACFA16FC09" box="[1320,1437,982,1007]" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" kingdom="Plantae" order="Ericales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="genus">
