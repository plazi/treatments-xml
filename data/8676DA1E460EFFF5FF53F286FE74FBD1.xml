<emphasis id="3CABB71A460EFFF5FDF9F3ACFD32FC09" box="[573,697,982,1007]" italics="true" pageId="117" pageNumber="118">
<taxonomicName id="C9DF108B460EFFF5FDF9F3ACFD3FFC09" box="[573,692,982,1007]" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" kingdom="Plantae" order="Ericales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="genus">Vaccinium</taxonomicName>
