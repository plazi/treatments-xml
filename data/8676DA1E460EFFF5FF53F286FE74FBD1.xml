<taxonomicName id="C9DF108B460EFFF5FCEAF380FC28FBF5" box="[814,931,1018,1043]" class="Magnoliopsida" family="Ericaceae" genus="Empetrum" kingdom="Plantae" order="Ericales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="genus">
