(pl. 39C–E) (
<taxonomicName id="4C774D03BE0C042B787C3862FDD48D08" box="[309,512,328,350]" class="Magnoliopsida" family="Scrophulariaceae" kingdom="Plantae" order="Lamiales" pageId="53" pageNumber="53" phylum="Tracheophyta" rank="family">Scrophulariaceae</taxonomicName>
