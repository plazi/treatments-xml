<taxonomicName id="4BDC6B7AB0A63A6E5319F3572701A20B" authorityName="Brisson" authorityYear="1760" class="Aves" genus="Stercorarius" kingdom="Animalia" lsidName="" order="Charadriiformes" pageId="0" pageNumber="109" phylum="Chordata" rank="order">Charadriiformes</taxonomicName>
, 
<taxonomicName id="2731102BE9396D0EB758F305D01C5205" class="Aves" family="Stercorariidae" kingdom="Animalia" lsidName="" order="Charadriiformes" pageId="0" pageNumber="109" phylum="Chordata" rank="family">Stercorariidae</taxonomicName>
