<taxonomicName id="4C0A4D69C159035FFEBBFCFFFD61FCE1" authority="C. Y. Wu" authorityName="C. Y. Wu" box="[367,670,806,833]" class="Magnoliopsida" family="Lamiaceae" genus="Premna" kingdom="Plantae" order="Lamiales" pageId="1" phylum="Tracheophyta" rank="species" species="laevigata">
