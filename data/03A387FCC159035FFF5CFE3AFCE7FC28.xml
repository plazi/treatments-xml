<collectionCode id="ED1BAE2FC159035FFF77FCB7FF1CFC29" box="[163,227,879,905]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15842" lsid="urn:lsid:biocol.org:col:15842" name="Kunming Institute of Botany, Chinese Academy of Sciences" pageId="1" type="Herbarium">KUN</collectionCode>
