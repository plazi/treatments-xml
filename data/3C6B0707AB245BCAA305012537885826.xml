<taxonomicName id="1E5C798D05504E2EE0D239D123ADB8B7" LSID="3C6B0707-AB24-5BCA-A305-012537885826" authority="Goeze, 1777" authorityName="Goeze" authorityYear="1777" class="Insecta" family="Carabidae" genus="Microlestes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Microlestes minutulus" order="Coleoptera" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="minutulus">Microlestes minutulus Goeze, 1777</taxonomicName>
