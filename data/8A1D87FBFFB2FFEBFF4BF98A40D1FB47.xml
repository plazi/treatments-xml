<taxonomicName id="C5B44D6EFFB4FFEBFE8AFBEE464DFB9A" authorityName="Marshall" authorityYear="2022" box="[342,545,1051,1077]" class="Insecta" family="Micropezidae" genus="Amapeza" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="264" phylum="Arthropoda" rank="species" species="tricincta">
