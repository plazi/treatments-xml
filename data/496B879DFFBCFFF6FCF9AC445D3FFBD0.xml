<taxonomicName id="06C24D08FFBBFFF6FE30A33B5A78F79F" box="[484,623,2067,2091]" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" kingdom="Plantae" order="Zingiberales" pageId="12" pageNumber="617" phylum="Tracheophyta" rank="species" species="latisphata">
<emphasis id="F3B6EA99FFBBFFF6FE30A33B5A78F79F" box="[484,623,2067,2091]" italics="true" pageId="12" pageNumber="617">H. latisphata</emphasis>
