<taxonomicName id="06C24D08FFBBFFF6FE2EACDE5ACAF7BD" box="[506,733,2033,2057]" class="Liliopsida" family="Marantaceae" genus="Calathea" kingdom="Plantae" order="Zingiberales" pageId="12" pageNumber="617" phylum="Tracheophyta" rank="species" species="inocephala">
