</taxonomicName>
, 
<taxonomicName id="06C24D08FFBBFFF6FD3AACDE58CAF79F" class="Liliopsida" family="Marantaceae" genus="Calathea" kingdom="Plantae" order="Zingiberales" pageId="12" pageNumber="617" phylum="Tracheophyta" rank="species" species="lutea">
<emphasis id="F3B6EA99FFBBFFF6FD3AACDE58CAF79F" italics="true" pageId="12" pageNumber="617">C. lutea</emphasis>
</taxonomicName>
, 
<taxonomicName id="06C24D08FFBBFFF6FF39A33C59B4F79F" box="[237,419,2067,2091]" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" kingdom="Plantae" order="Zingiberales" pageId="12" pageNumber="617" phylum="Tracheophyta" rank="species" species="stricta">
