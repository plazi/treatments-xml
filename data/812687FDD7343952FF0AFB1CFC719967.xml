<taxonomicName id="CE8F4D68D7333952FD4EFF11FCBD98FB" box="[723,894,224,248]" class="Insecta" family="Pseudococcidae" genus="Spilococcus" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="mirzayansi">
