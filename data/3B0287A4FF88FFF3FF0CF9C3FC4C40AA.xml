<paragraph id="B31436B2FF88FFF5FF0CF932FDBB40CA" blockId="19.[151,540,1734,1757]" box="[151,540,1734,1757]" pageId="19" pageNumber="20">
<treatmentCitationGroup id="93BB119CFF88FFF5FF0CF932FDBB40CA" box="[151,540,1734,1757]" pageId="19" pageNumber="20">
“ 
<taxonomicName id="74AB4D31FF88FFF5FF39F933FF4A40CA" box="[162,237,1735,1756]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
