<collectingCountry id="64D102B8297F0703FDBCACA376EAC72B" box="[583,689,1279,1302]" name="Colombia" pageId="12" pageNumber="287">Colombia</collectingCountry>
(
<bibRefCitation id="78573FD9297F0703FD3FACA37494C70B" author="STAL" firstAuthor="STAL" pageId="12" pageNumber="287" pagination="1 - 232" refId="ref23478" refString="STAL C. 1870: Enumeratio Hemipterorum. Bidrag till en forteckning ofver alla hittels kanda Hemiptera, jemte systematiska meddelanden. 1. [Enumeration of Hemiptera. Contribution to a list of all currently known Hemiptera, along with systematic notes. 1.] Kongliga Svenska Vetenskaps-Akademiens Handlingar (N. F.) 9 (1): 1 - 232." type="journal article" year="1870">STÅL 1870</bibRefCitation>
, type locality; 
<bibRefCitation id="78573FD9297F0703FE95AD437661C70B" author="SCHOUTEDEN" box="[366,570,1311,1334]" firstAuthor="SCHOUTEDEN" pageId="12" pageNumber="287" refId="ref22877" refString="SCHOUTEDEN H. 1913 a: Heteroptera, Fam. Pentatomidae, Subfam. Dinidorinae. In: WYTSMAN P. (ed.): Genera Insectorum. 153 me Fascicule. V. Verteneuil &amp; L. Desmet, Bruxelles, 19 pp + 2 pls." type="book" year="1913" yearSuffix="a">SCHOUTEDEN 1913a</bibRefCitation>
), 
<collectingCountry id="64D102B8297F0703FDB5AD4376FCC70B" box="[590,679,1311,1334]" name="Ecuador" pageId="12" pageNumber="287">Ecuador</collectingCountry>
(
<bibRefCitation id="78573FD9297F0703FD4DAD43748BC76B" author="DURAI" firstAuthor="DURAI" pageId="12" pageNumber="287" pagination="163 - 360" refId="ref18671" refString="DURAI P. S. S. 1987: A revision of the Dinidoridae of the World (Heteroptera: Pentatomoidea). Oriental Insects 21: 163 - 360." type="journal article" year="1987">DURAI 1987</bibRefCitation>
),? 
<collectingCountry id="64D102B8297F0703FF0EAD637500C76B" box="[245,347,1343,1366]" name="Suriname" pageId="12" pageNumber="287">Suriname</collectingCountry>
