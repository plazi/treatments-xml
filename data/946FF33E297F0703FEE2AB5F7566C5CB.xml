<taxonomicName id="DBC639AB297F0703FEB5ACA2766DC72B" authorityName="Stal" authorityYear="1870" box="[334,566,1278,1302]" class="Insecta" family="Pentatomidae" genus="Dinidor" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="287" phylum="Arthropoda" rank="species" species="impicticollis">
<emphasis id="2EB29E3A297F0703FEB5ACA2766DC72B" bold="true" box="[334,566,1278,1302]" italics="true" pageId="12" pageNumber="287">Dinidor impicticollis</emphasis>
