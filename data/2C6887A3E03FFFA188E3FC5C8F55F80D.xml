<taxonomicName id="63C14D36E03FFFA188E3FC138CD3FC0D" authority="Gardner &amp; Shelley 1989:231" authorityName="Gardner &amp; Shelley" authorityPageNumber="231" authorityYear="1989" box="[151,641,984,1007]" class="Diplopoda" family="Caseyidae" genus="Caseya" kingdom="Animalia" order="Chordeumatida" pageId="8" pageNumber="31" phylum="Arthropoda" rank="species" species="borealis">
