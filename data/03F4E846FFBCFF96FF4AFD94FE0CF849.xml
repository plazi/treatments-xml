<materialsCitation id="3B35530DFFB3FF96FE59FF46FC54FB09" box="[388,1012,232,258]" collectingDate="1983-01-11" collectingDateMax="1983-01-14" collectingDateMin="1983-01-11" collectionCode="CNCI" collectorName="A. Newton &amp; M. Thayer" country="Peru" elevation="1700" location="Azul" longitude="-75.58305" pageId="24" pageNumber="25" specimenCount="7" specimenCount-male="7" stateProvince="Huanuco" typeStatus="paratype">
