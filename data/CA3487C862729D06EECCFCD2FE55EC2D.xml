<subSubSection id="0A87655562759D06EE9CFB67FC90EF57" pageId="20" pageNumber="21" type="etymology">
<paragraph id="422236DE62759D06EE9CFB67FC90EF57" blockId="20.[151,1437,151,1580]" pageId="20" pageNumber="21">
<emphasis id="70E9EACC62759D06EE9CFB67FE58EF0E" bold="true" box="[199,385,1270,1295]" pageId="20" pageNumber="21">ETYMOLOGY</emphasis>
