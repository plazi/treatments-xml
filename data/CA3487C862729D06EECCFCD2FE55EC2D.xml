), 
<quantity id="85659B3B62759D06ECFFFB42FC9DEEEA" box="[676,836,1235,1259]" metricMagnitude="2" metricUnit="m" metricValue="4.3" metricValueMax="5.6" metricValueMin="3.0" pageId="20" pageNumber="21" unit="m" value="430.0" valueMax="560.0" valueMin="300.0">300–560 masl</quantity>
