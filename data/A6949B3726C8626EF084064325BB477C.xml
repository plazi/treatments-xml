, 
<taxonomicName id="68A586C2CB9268B8E1F7CEE4AF2CE02F" class="Actinopterygii" family="Loricariidae" genus="Hypostomus" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="3" phylum="Chordata" rank="species" species="plecostomoides">H. plecostomoides</taxonomicName>
, 
<taxonomicName id="0DD28CD8BEE8C99F8FAD3B481E8545D2" LSID-ZBK="urn:lsid:zoobank.org:act:3F8E55C1-0EAD-49CA-B887-0D953CC48DA0" class="Actinopterygii" family="Loricariidae" genus="Hypostomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Hypostomus sculpodon Armbruster 2003:49" order="Siluriformes" pageNumber="3" phylum="Chordata" rank="species" species="sculpodon">H. sculpodon</taxonomicName>
