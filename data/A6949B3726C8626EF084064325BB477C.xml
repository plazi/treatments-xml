<taxonomicName id="9AA438D0F55C4355FB865009E742697A" LSID-ZBK="urn:lsid:zoobank.org:act:0E38AFF1-3B7F-4F10-9529-A7606A360AC9" class="Actinopterygii" family="Loricariidae" genus="Hypostomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Hypostomus soniae Carvalho &amp; Weber 2005:965" order="Siluriformes" pageNumber="3" phylum="Chordata" rank="species" species="soniae">H. soniae</taxonomicName>
