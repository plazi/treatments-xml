<taxonomicName id="7A0375EE7B2908A7C2BC6F7C47F6FF4D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:189638" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" lsidName-HNS="Aphaenogaster (Messor) barbarus (Linnaeus)" order="Hymenoptera" pageNumber="68" phylum="Arthropoda" rank="species" species="barbarus" subGenus="Messor">Aphaenogaster (Messor) barbarus</taxonomicName>
