<taxonomicName id="41804D5C79140C093F9AFA5FFC2BF9EF" box="[763,908,1517,1543]" class="Arachnida" family="Gnaphosidae" genus="Megamyrmaekion" kingdom="Animalia" order="Araneae" pageId="36" pageNumber="45" phylum="Arthropoda" rank="species" species="magshimim">Magshimim</taxonomicName>
