<quantity id="715A9B43C171FFAEF90D7218DEAFFC20" box="[586,711,882,906]" metricMagnitude="-1" metricUnit="m" metricValue="2.65" metricValueMax="2.9" metricValueMin="2.4" pageId="5" pageNumber="6" unit="mm" value="265.0" valueMax="290.0" valueMin="240.0">240–290 µm</quantity>
