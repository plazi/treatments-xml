<quantity id="715A9B43C171FFAEFBB9721BDD22FC20" box="[254,330,881,906]" metricMagnitude="-1" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="6" unit="mm" value="160.0">160 µm</quantity>
