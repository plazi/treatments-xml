<paragraph id="DF47083167638DD8393C324548E9DF9E" pageId="5" pageNumber="134">
<taxonomicName id="EC94B0554280DA3117A599C9439838C4" LSID="http://zoobank.org/C6C79A9A-0A51-4252-9603-AF408E46DF26" authority="Van Dam &amp; Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus silaliensis" order="Coleoptera" pageId="5" pageNumber="134" phylum="Arthropoda" rank="species" species="silaliensis">Trigonopterus silaliensis Van Dam &amp; Riedel</taxonomicName>
<taxonomicNameLabel id="3D37B1E67EC617E2EE9C70D0B03A64B3" pageId="5" pageNumber="134">sp. n.</taxonomicNameLabel>
