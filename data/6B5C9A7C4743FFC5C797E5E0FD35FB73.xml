</taxonomicName>
(both wild and cultivated), 
<taxonomicName id="24F550E94743FFC5C5AEE301FCAEFBE6" box="[686,891,1052,1077]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="18" pageNumber="419" phylum="Tracheophyta" rank="species" species="domestica">
<emphasis id="D181F7784743FFC5C5AEE301FCAEFBE6" box="[686,891,1052,1077]" italics="true" pageId="18" pageNumber="419">Prunus domestica</emphasis>
</taxonomicName>
, and sometimes on 
<taxonomicName id="24F550E94743FFC5C365E301FAC3FBE7" box="[1125,1302,1053,1076]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="18" pageNumber="419" phylum="Tracheophyta" rank="species" species="spinosa">
<emphasis id="D181F7784743FFC5C365E301FAC3FBE7" box="[1125,1302,1053,1076]" italics="true" pageId="18" pageNumber="419">Prunus spinosa</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E94743FFC5C227E300FA41FBE7" box="[1319,1428,1052,1076]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="18" pageNumber="419" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
) and 
<taxonomicName id="24F550E94743FFC5C7C6E323FEDAFB8B" box="[198,271,1087,1112]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="419" phylum="Arthropoda" rank="genus">
<emphasis id="D181F7784743FFC5C7C6E323FEDAFB8B" box="[198,271,1087,1112]" italics="true" pageId="18" pageNumber="419">Ulmus</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="24F550E94743FFC5C645E323FE6CFB8B" box="[325,441,1087,1112]" class="Magnoliopsida" family="Ulmaceae" kingdom="Plantae" order="Rosales" pageId="18" pageNumber="419" phylum="Tracheophyta" rank="family">Ulmaceae</taxonomicName>
) (
<bibRefCitation id="8764569B4743FFC5C6CEE323FD15FB8B" author="Modarres" box="[462,704,1087,1112]" pageId="18" pageNumber="419" refString="Modarres Awal, M. (1997) Platypodidae, Scolytidae. In: Modarres Awal, M. (Ed.), List of agricultural pests and their natural enemies in Iran. Ferdowsi University Press, Mashhad, pp. 185, 192 - 194." type="book chapter" year="1997">
Modarres Awal 
<number id="553D394D4743FFC5C584E35CFD15FB8B" box="[644,704,1088,1112]" pageId="18" pageNumber="419" value="1997.0">1997</number>
</bibRefCitation>
).
</paragraph>
</subSubSection>
<subSubSection id="ABEF78E14743FFC5C7C7E378FD35FB73" pageId="18" pageNumber="419" type="discussion">
