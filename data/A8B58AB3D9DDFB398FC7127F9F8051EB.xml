(Cameron, 1879, 
<taxonomicName id="C73844B39A9E40EC71869E90DC427D72" class="Insecta" family="Tenthredinidae" genus="Nematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nematus" order="Hymenoptera" pageId="0" pageNumber="1168" phylum="Arthropoda" rank="genus">Nematus</taxonomicName>
