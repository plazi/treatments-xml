, 
<figureCitation id="1333F0963732F4642C74FB9DFE9AFB8A" box="[306,338,1122,1148]" captionStart="FIGURES 44 – 47" captionStartId="14.[151,271,1711,1735]" captionTargetBox="[169,1412,182,1686]" captionTargetId="figure@14.[169,1413,182,1686]" captionTargetPageId="14" captionText="FIGURES 44 – 47. Thoracic dorsum. 44. Klyngon petalon sp. nov., female. 45. K. guimaraesi sp. nov., female. 46. K. serjaniae sp. nov., female. 47. K. bouceki Hansson, female." httpUri="https://zenodo.org/record/195768/files/figure.png" pageId="24" pageNumber="25">44</figureCitation>
, 
<figureCitation id="1333F0963732F4642C18FB9DFEB6FB8A" box="[350,382,1122,1148]" captionStart="FIGURES 59 – 68" captionStartId="19.[151,269,1883,1907]" captionTargetBox="[151,1436,204,1860]" captionTargetId="figure@19.[151,1436,194,1862]" captionTargetPageId="19" captionText="FIGURES 59 – 68. Forewing. 59. Klyngon albicornis sp. nov., female. 60. K. brasilense sp. nov., female. 61. K. aulacis sp. nov., female. 62. K. copaiferae sp. nov., female. 63. K. costalimai sp. nov., female. 64. K. hortense sp. nov., male. 65. K. pegosoma sp. nov., female. 66. K. petalon sp. nov., female. 67. K. guimaraesi sp. nov., female. 68. K. serjaniae sp. nov., female." httpUri="https://zenodo.org/record/195771/files/figure.png" pageId="24" pageNumber="25">66</figureCitation>
, 
<figureCitation id="1333F0963732F4642CCCFB9DFE62FB8A" box="[394,426,1122,1148]" captionStart="FIGURES 69 – 86" captionStartId="20.[151,269,1843,1867]" captionTargetBox="[176,1409,200,1814]" captionTargetId="figure@20.[176,1411,200,1814]" captionTargetPageId="20" captionText="FIGURES 69 – 86. Flagellum. 69. Klyngon albicornis sp. nov., female. 70. K. brasilense sp. nov., female. 71. K. aulacis sp. nov., female. 72. K. brasilense, male. 73. K. copaiferae sp. nov., female. 74. K. costalimai sp. nov., female. 75. K. costalimai, male. 76. K. gibberum sp. nov., female. 77. K. hortense sp. nov., male. 78. K. pegosoma sp. nov., female. 79. K. petalon sp. nov., female. 80. K. guimaraesi sp. nov., female. 81. K. guimaraesi, male. 82. K. serjaniae sp. nov., female. 83. K. serjaniae, male. 84. K. jimenezi Hansson, female. 85. K. bouceki Hansson, female. 86. K. jimenezi, male." httpUri="https://zenodo.org/record/195772/files/figure.png" pageId="24" pageNumber="25">79</figureCitation>
, 
<figureCitation id="1333F0963732F4642CF0FB9DFE1DFB8A" box="[438,469,1122,1148]" captionStart="FIGURES 87 – 97" captionStartId="22.[151,269,1917,1941]" captionTargetBox="[187,1397,494,1916]" captionTargetId="figure@22.[180,1414,488,1920]" captionTargetPageId="22" captionText="FIGURES 87 – 97. Head and antenna, lateral. 87. Klyngon albicornis sp. nov., female. 88. K. brasilense sp. nov., female. 89. K. brasilense, male. 90. K. aulacis sp. nov., female. 91. K. copaiferae sp. nov., female. 92. K. costalimai sp. nov., female. 93. K. costalimai, male. 94. K. gibberum sp. nov., female. 95. K. hortense sp. nov., male. 96. K. pegosoma sp. nov., female. 97. K. petalon sp. nov., female." httpUri="https://zenodo.org/record/195773/files/figure.png" pageId="24" pageNumber="25">97</figureCitation>
)
