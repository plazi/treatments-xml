<taxonomicName id="4C474D1C3B72A15D6E47FDF7AABAFDD3" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[1084,1209,585,611]" class="Arachnida" family="Prodidomidae" genus="Purcelliana" kingdom="Animalia" order="Araneae" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="kamaseb" status="sp. nov.">
<emphasis id="B933EA8D3B72A15D6E47FDF7AABAFDD3" box="[1084,1209,585,611]" italics="true" pageId="45" pageNumber="46">P. kamaseb</emphasis>
</taxonomicName>
<emphasis id="B933EA8D3B72A15D6EBBFDF7AB1AFDD4" bold="true" box="[1216,1305,586,612]" pageId="45" pageNumber="46">
