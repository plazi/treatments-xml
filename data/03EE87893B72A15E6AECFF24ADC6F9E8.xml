<taxonomicName id="4C474D1C3B72A15D6F27FD9BAEDCFDD3" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" class="Arachnida" family="Prodidomidae" genus="Purcelliana" kingdom="Animalia" order="Araneae" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="kamaseb">
