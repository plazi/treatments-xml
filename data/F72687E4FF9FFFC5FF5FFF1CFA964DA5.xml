<taxonomicName id="B88F4D71FF9FFFC5FF26FF1CFD944CDB" authority="Haag-Rutenberg, 1879" authorityName="Haag-Rutenberg" authorityYear="1879" box="[238,610,222,248]" class="Insecta" family="Meloidae" genus="Iselma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
