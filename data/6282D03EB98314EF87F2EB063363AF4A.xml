<taxonomicName id="C7715E146A6B1FE4D5CD2D9C17F08FAF" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Fabaceae" genus="Melilotus" higherTaxonomySource="CoL" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="species" species="cretica">M. cretica</taxonomicName>
