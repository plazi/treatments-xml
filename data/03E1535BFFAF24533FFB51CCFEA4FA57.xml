<location id="8E97B496FFAF24533F74505CFE67FE2B" LSID="urn:lsid:plazi:treatment:03E1535BFFAF24533FFB51CCFEA4FA57:8E97B496FFAF24533F74505CFE67FE2B" box="[296,478,378,402]" name="Lower Carolina" pageId="8" pageNumber="1837">Lower Carolina</location>
