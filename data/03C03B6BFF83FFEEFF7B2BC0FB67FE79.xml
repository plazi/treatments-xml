<paragraph id="8BD68A7DFF83FFEEFF2B2BACFC7EFE81" blockId="24.[199,920,220,274]" box="[199,920,250,274]" pageId="24" pageNumber="25">
<taxonomicName id="4C69F1FEFF83FFEEFF2B2BACFDA1FE81" authority="Hoshina, 2020" authorityName="Hoshina" authorityYear="2020" box="[199,583,250,274]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="akane" subGenus="Euconnus">
<emphasis id="B91D566FFF83FFEEFF2B2BACFECDFE81" box="[199,299,251,274]" italics="true" pageId="24" pageNumber="25">Euconnus</emphasis>
