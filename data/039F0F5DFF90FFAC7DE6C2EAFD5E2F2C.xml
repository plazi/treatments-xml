(n = 
<number id="3DFEAC6CFF90FFAC7CC3C3F5FE4F29E3" box="[434,465,941,965]" pageId="10" pageNumber="11" value="10.0">10</number>
): HB = 
<number id="3DFEAC6CFF90FFAC7F5AC3F5FD1129E3" box="[555,655,941,965]" pageId="10" pageNumber="11" value="314.0" valueMax="363.0" valueMin="265.0">265–363</number>
, T = 
<number id="3DFEAC6CFF90FFAC7FB8C3F5FCB029E3" box="[713,814,941,965]" pageId="10" pageNumber="11" value="293.5" valueMax="331.0" valueMin="256.0">256–331</number>
, HF = 
<number id="3DFEAC6CFF90FFAC7E0AC3F5FC5A29E3" box="[891,964,941,965]" pageId="10" pageNumber="11" value="45.5" valueMax="48.0" valueMin="43.0">43–48</number>
, E = 
<number id="3DFEAC6CFF90FFAC7E8FC3F5FBD929E3" box="[1022,1095,941,965]" pageId="10" pageNumber="11" value="55.5" valueMax="63.0" valueMin="48.0">48–63</number>
