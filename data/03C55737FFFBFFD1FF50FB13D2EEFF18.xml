<quantity id="4C944BC4FFF8FFD1FDC2FF1AD35CFF18" box="[517,627,190,216]" metricMagnitude="2" metricUnit="m" metricValue="6.096" pageId="46" pageNumber="47" unit="ft" value="2000.0">2000 feet</quantity>
