<quantity id="1267B4C1FF9DFFC6FB12BB1F76F8F8D3" box="[1185,1277,1881,1900]" metricMagnitude="-2" metricUnit="m" metricValue="3.95" pageId="7" pageNumber="1234" unit="mm" value="39.5">39.5 mm</quantity>
