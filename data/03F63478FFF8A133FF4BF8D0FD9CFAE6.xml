<subSubSection id="C345D6E5FFFEA133FF1FFCF0FDF0FC83" pageId="53" pageNumber="54" type="etymology">
<paragraph id="8BE0856EFFFEA133FF1FFCF0FDF0FC83" blockId="53.[151,1437,151,1335]" pageId="53" pageNumber="54">
<emphasis id="B92B597CFFFEA133FF1FFCF0FEA0FCFD" bold="true" box="[195,339,786,812]" pageId="53" pageNumber="54">Etymology.</emphasis>
