<taxonomicName id="559BFCED620E223C0B5EF86BFD546E8F" authorityName="Gunter &amp; Weir" authorityYear="2021" box="[528,662,2006,2031]" class="Insecta" family="Scarabaeidae" genus="Lepanus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="cameroni">
