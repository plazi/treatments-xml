<taxonomicName id="559BFCED620E223C0815F86BFE1B6E8F" authorityName="DET" authorityYear="1971" baseAuthorityName="Macleay" baseAuthorityYear="1887" box="[347,473,2005,2031]" class="Insecta" family="Scarabaeidae" genus="Lepanus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="globulus">
