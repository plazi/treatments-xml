<taxonomicName id="4C084D6293110749D3C1EE6E5F46FE0F" authorityName="Santschi" authorityYear="1914" box="[151,312,367,393]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="nigeriensis">
<emphasis id="B97CEAF393110749D3C1EE6E5F46FE0F" bold="true" box="[151,312,367,393]" italics="true" pageId="6" pageNumber="7">P. nigeriensis</emphasis>
