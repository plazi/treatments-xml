<taxonomicName id="3EFF26851A1CF44438534B181CFCFBA8" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="CB8B81141A1CF44438534B1819DDFBD1" box="[1350,1435,1042,1067]" italics="true" pageId="12" pageNumber="13">Prunus</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A1CF4443DDD4B321D7FFBA8" box="[200,313,1080,1106]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
) (
<bibRefCitation id="9D6E20F71A1CF4443C444B321D9DFBA8" author="Patch, E. M." box="[337,475,1080,1106]" pageId="12" pageNumber="13" pagination="321 - 322" refId="ref30879" refString="Patch, E. M. (1908) Crocigrapha normani Grote. Entomological News, 19, 321 - 322." type="journal article" year="1908">Patch 1908</bibRefCitation>
, 
<bibRefCitation id="9D6E20F71A1CF4443CFD4B321EC7FBA8" author="Forbes, W. T. M." box="[488,641,1080,1106]" pageId="12" pageNumber="13" pagination="5 - 433" refId="ref30614" refString="Forbes, W. T. M. (1954) Lepidoptera of New York and neighboring states. Noctuidae. Part III. Cornell University Agricultural Experiment Station Memoir, 329, 5 - 433." type="journal article" year="1954">Forbes 1954</bibRefCitation>
