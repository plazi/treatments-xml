<taxonomicName id="3EFF26851A1CF4443CE74CE7183CFBFD" authority="(Faw.) Laconte &amp; Blackwell" authorityName="Laconte &amp; Blackwell" baseAuthorityName="Faw." box="[498,1146,1005,1031]" class="Magnoliopsida" family="Berberidaceae" genus="Caulophyllum" kingdom="Plantae" order="Ranunculales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="giganteum">
<emphasis id="CB8B81141A1CF4443CE74CE71F5BFBFC" box="[498,797,1005,1030]" italics="true" pageId="12" pageNumber="13">Caulophyllum giganteum</emphasis>
(Faw.) Laconte &amp; Blackwell
</taxonomicName>
, 
<taxonomicName id="3EFF26851A1CF44439934CE71975FBFD" box="[1158,1331,1005,1031]" class="Magnoliopsida" family="Berberidaceae" kingdom="Plantae" order="Ranunculales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Berberidaceae</taxonomicName>
