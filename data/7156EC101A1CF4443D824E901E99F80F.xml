<taxonomicName id="3EFF26851A1CF4443ECF4B181816FBD6" box="[986,1104,1042,1068]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="CB8B81141A1CF4443ECF4B181862FBD1" box="[986,1060,1042,1067]" italics="true" pageId="12" pageNumber="13">Malus</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A1CF444394B4B181895FBD6" box="[1118,1235,1042,1068]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
