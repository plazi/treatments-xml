<taxonomicName id="3EFF26851A1CF44439124B5718B8FB8D" authority="Roth" authorityName="Roth" box="[1031,1278,1117,1143]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="pendula">
<emphasis id="CB8B81141A1CF44439124B5718FCFB8C" box="[1031,1210,1117,1142]" italics="true" pageId="12" pageNumber="13">Betula pendula</emphasis>
Roth
</taxonomicName>
, 
<taxonomicName id="3EFF26851A1CF444381F4B5719C9FB8D" authorityName="S.F.Gray" authorityYear="1822" box="[1290,1423,1117,1143]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
), sugar maple (
<taxonomicName id="3EFF26851A1CF4443C294B891E19FB66" authority="Marsh." authorityName="Marsh." box="[316,607,1154,1180]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="saccharum">
<emphasis id="CB8B81141A1CF4443C294B891DB9FB66" box="[316,511,1155,1180]" italics="true" pageId="12" pageNumber="13">Acer saccharum</emphasis>
Marsh.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A1CF4443F794B881EA8FB66" box="[620,750,1154,1180]" family="Aceraceae" pageId="12" pageNumber="13" rank="family">Aceraceae</taxonomicName>
