<taxonomicName id="3EFF26851A1CF4443C1B4B181E6FFBD6" authority="Marsh." authorityName="Marsh." box="[270,553,1042,1068]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="cornuti">
<emphasis id="CB8B81141A1CF4443C1B4B181D8CFBD1" box="[270,458,1042,1067]" italics="true" pageId="12" pageNumber="13">Corylus cornuti</emphasis>
Marsh.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A1CF4443F204B181EFFFBD6" authorityName="S.F.Gray" authorityYear="1822" box="[565,697,1042,1068]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
) (
<bibRefCitation id="9D6E20F71A1CF4443FC74B181F34FBD6" author="Gibson, A." box="[722,882,1042,1068]" pageId="12" pageNumber="13" pagination="17 - 20" refId="ref30651" refString="Gibson, A. (1903) The life history of Crocigrapha normani, Grt. The Canadian Entomologist, 35, 17 - 20." type="journal article" year="1903">Gibson 1903</bibRefCitation>
