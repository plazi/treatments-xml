<taxonomicName id="3EFF26851A1CF4443E984B891861FB66" box="[909,1063,1155,1180]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="rubrum">
