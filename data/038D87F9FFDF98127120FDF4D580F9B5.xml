<paragraph id="8B9B36EFFFDF98147120FD97D6CAFDBB" blockId="2.[264,603,563,625]" box="[264,465,599,625]" pageId="2" pageNumber="57">
(
<figureCitation id="131F2A6AFFDF98147139FD97D679FDBB" box="[273,354,599,625]" captionStart="FIGURES 1 – 4" captionStartId="3.[264,382,1795,1819]" captionTargetBox="[314,1277,326,1768]" captionTargetId="figure@3.[314,1277,326,1769]" captionTargetPageId="3" captionText="FIGURES 1 – 4. Habiti of new Priacma species, photographs of rock impressions. 1 — Priacma latidentata sp. nov., 2 — Priacma tuberculosa sp. nov., 3 — Priacma clavata sp. nov., 4 — Priacma renaria sp. nov." httpUri="https://zenodo.org/record/174107/files/figure.png" pageId="2" pageNumber="57">Figs. 1</figureCitation>
, 
<figureCitation id="131F2A6AFFDF98147158FD97D685FDBB" box="[368,414,599,625]" captionStart-0="FIGURES 5 – 7" captionStart-1="FIGURES 8 – 9" captionStartId-0="4.[264,382,972,996]" captionStartId-1="5.[264,383,1869,1893]" captionTargetBox-0="[267,1315,332,946]" captionTargetBox-1="[284,1309,316,1852]" captionTargetId-0="figure@4.[264,1323,326,948]" captionTargetId-1="figure@5.[284,1310,316,1852]" captionTargetPageId-0="4" captionTargetPageId-1="5" captionText-0="FIGURES 5 – 7. Priacma latidentata sp. nov., holotype. 5 — dorsal view, 6 — ventral view, 7 — outline of elytral cells." captionText-1="FIGURES 8 – 9. Priacma latidentata sp. nov., holotype. 8 — head, 9 — metaventrite." httpUri-0="https://zenodo.org/record/174108/files/figure.png" httpUri-1="https://zenodo.org/record/174109/files/figure.png" pageId="2" pageNumber="57">5–9</figureCitation>
, 
<figureCitation id="131F2A6AFFDF98147183FD97D6D1FDBB" box="[427,458,599,625]" captionStart="FIGURES 22 – 25. 22" captionStartId="11.[264,379,1823,1847]" captionTargetBox="[286,1311,1040,1807]" captionTargetId="figure@11.[286,1311,1040,1807]" captionTargetPageId="11" captionText="FIGURES 22 – 25. 22 — head of Priacma renaria sp. nov., 23 — metaventrite of Priacma renaria sp. nov., 24 — elytral cells of Priacma latidentata sp. nov., 25 — elytral cells of Priacma renaria sp. nov." httpUri="https://zenodo.org/record/174115/files/figure.png" pageId="2" pageNumber="57">24</figureCitation>
)
