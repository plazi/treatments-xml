, 
<collectingCountry id="F32A7783FFDBFF8EFDFDF8568D76F822" box="[632,785,2012,2038]" name="South Africa" pageId="33" pageNumber="34">South Africa</collectingCountry>
, 
<collectingCountry id="F32A7783FFDBFF8EFC98F8568DEDF822" box="[797,906,2012,2038]" name="Tanzania" pageId="33" pageNumber="34">Tanzania</collectingCountry>
, 
