<collectingCountry id="F35B761EFFFAFFC132C2FCC6CE11FC91" box="[454,563,836,860]" name="Barbados" pageId="5" pageNumber="6">Barbados</collectingCountry>
, 
<taxonomicName id="4C4C4D0DFFFAFFC1313AFCC7CE91FC91" box="[574,691,836,860]" class="Malacostraca" family="Alpheidae" genus="Synalpheus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="herricki">
<emphasis id="B938EA9CFFFAFFC1313AFCC7CE91FC91" box="[574,691,836,860]" italics="true" pageId="5" pageNumber="6">S. herricki</emphasis>
</taxonomicName>
