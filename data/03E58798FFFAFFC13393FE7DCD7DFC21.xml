<emphasis id="B938EA9CFFFAFFC13765FCC6C91FFC91" box="[1121,1341,836,860]" italics="true" pageId="5" pageNumber="6">
<taxonomicName id="4C4C4D0DFFFAFFC13765FCC6C91BFC91" box="[1121,1337,836,860]" class="Demospongiae" family="Aplysinidae" genus="Aiolochroia" kingdom="Animalia" order="Verongida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="crassa">Aiolochroia crassa</taxonomicName>
