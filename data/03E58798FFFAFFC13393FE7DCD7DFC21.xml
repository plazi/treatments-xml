<paragraph id="8BF3368EFFFAFFC133C1FD79CEE3FCFA" blockId="5.[151,1436,583,1004]" pageId="5" pageNumber="6">
<emphasis id="B938EA9CFFFAFFC133C1FD79CD2CFCD9" bold="true" box="[197,270,763,788]" pageId="5" pageNumber="6">Color.</emphasis>
In 
<collectingCountry id="F35B761EFFFAFFC1323FFD7ECD87FCD9" box="[315,421,764,788]" name="Barbados" pageId="5" pageNumber="6">Barbados</collectingCountry>
<taxonomicName id="4C4C4D0DFFFAFFC132B4FD7FCE0BFCD9" box="[432,553,764,788]" class="Malacostraca" family="Alpheidae" genus="Synalpheus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="herricki">
<emphasis id="B938EA9CFFFAFFC132B4FD7FCE0BFCD9" box="[432,553,764,788]" italics="true" pageId="5" pageNumber="6">S. herricki</emphasis>
</taxonomicName>
