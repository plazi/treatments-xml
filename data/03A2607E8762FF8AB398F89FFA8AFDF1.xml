<bibRefCitation id="EF9AAC998762FF89B1F5F86AFC40F82C" author="Verhoeff" box="[762,940,1970,1994]" pageId="13" pageNumber="14" refString="Verhoeff, K. W. (1938 a) Chilopoden-Studien zur Kenntnis der Epimorphen. Zoologische Jahrbucher, Abteilung fur Systematik, Okologie und Geographie der Tiere, 71, 339 - 388." type="journal article" year="1938" yearSuffix="a">
Verhoeff 
<number id="3DC3C34F8762FF89B06AF86AFC73F82C" box="[869,927,1970,1994]" pageId="13" pageNumber="14" value="1938.0">1938</number>
a
</bibRefCitation>
, 
<bibRefCitation id="EF9AAC998762FF89B0B6F86AFC12F82C" author="Verhoeff" box="[953,1022,1970,1994]" pageId="13" pageNumber="14" refString="Verhoeff, K. W. (1938 c) Uber einige amerikanische Myriapoden. Zoologischer Anzeiger, 122, 273 - 284." type="journal article" year="1938" yearSuffix="c">
<number id="3DC3C34F8762FF89B0B6F86AFC1EF82C" box="[953,1010,1970,1994]" pageId="13" pageNumber="14" value="1938.0">1938</number>
c
</bibRefCitation>
(both sub 
<taxonomicName id="4C0BAAEB8762FF89B774F86BFA66F82D" box="[1147,1418,1971,1995]" class="Chilopoda" family="Linotaeniidae" genus="Paraplanes" kingdom="Animalia" order="Geophilomorpha" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="californicus">
