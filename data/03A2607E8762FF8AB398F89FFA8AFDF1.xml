<bibRefCitation id="EF9AAC998762FF89B11FF86BFD49F82C" author="Attems" box="[528,677,1970,1995]" pageId="13" pageNumber="14" refString="Attems, C. (1903) Synopsis der Geophiliden. Zoologische Jahrbucher, Abteilung fur Systematik, 18, 155 - 302." type="journal article" year="1903">
Attems 
<number id="3DC3C34F8762FF89B165F86AFD49F82C" box="[618,677,1970,1994]" pageId="13" pageNumber="14" value="1903.1929">1903</number>
</bibRefCitation>
, 
<bibRefCitation id="EF9AAC998762FF89B1BDF86AFD01F82C" author="Attems" box="[690,749,1970,1994]" pageId="13" pageNumber="14" refString="Attems, C. (1929) Myriapoda. 1. Geophilomorpha. Das Tierreich 52. De Gruyter, Berlin, 388 pp." type="book" year="1929">1929</bibRefCitation>
