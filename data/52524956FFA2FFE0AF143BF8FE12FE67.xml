<taxonomicName id="1DFB83C3FFA2FFE3AF243AE9FDB1FA29" box="[312,561,1479,1504]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="33" pageNumber="40" phylum="Chordata" rank="subSpecies" species="cinctus" subSpecies="lapponicus">
