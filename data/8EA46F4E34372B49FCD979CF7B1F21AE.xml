<subSubSection id="42861DCC69F012BDC67ECBBBF08D84A9" pageId="1" pageNumber="2" type="species included">
<paragraph id="6409EA0196F6216259C48B0ED041672E" pageId="1" pageNumber="2">Species included.</paragraph>
<paragraph id="D98C2A10B3A4A833A5B4B88B6FEC487D" pageId="1" pageNumber="2">
<taxonomicName id="AA03A5036FB7661B25C9A0967F77E5EB" class="Malacostraca" family="Stenetriidae" genus="Tenupedunculus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tenupedunculus elongatus" order="Isopoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="elongatus">Tenupedunculus elongatus</taxonomicName>
