<taxonomicName id="CCC626C506C357F43EE748A55D6A70A6" class="Malacostraca" family="Stenetriidae" genus="Tenupedunculus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tenupedunculus" order="Isopoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">Tenupedunculus</taxonomicName>
