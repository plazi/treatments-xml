<taxonomicName id="9804CBC05A2E25FFAF9BD03204079365" class="Malacostraca" family="Stenetriidae" genus="Tenupedunculus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tenupedunculus acutum" order="Isopoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="acutum">Tenupedunculus acutum</taxonomicName>
(
<normalizedToken id="C3B024E97F82538F6791BB0B32814558" originalValue="Vanhöffen">Vanhoeffen</normalizedToken>
, 1914); 
<taxonomicName id="6028416983B5A72D151885023E8C6C31" genus="T." lsidName="T. beddardi" pageId="1" pageNumber="2" rank="species" species="beddardi">T. beddardi</taxonomicName>
(Kussakin, 1967); 
<taxonomicName id="A551280E4ABC47DA518D46362BB5D8D6" genus="T." lsidName="T. dentimanum" pageId="1" pageNumber="2" rank="species" species="dentimanum">T. dentimanum</taxonomicName>
(Kussakin, 1967); 
<taxonomicName id="805FE4A330C7080F30E435D8F58B6415" genus="T." lsidName="T. drakensis" pageId="1" pageNumber="2" rank="species" species="drakensis">T. drakensis</taxonomicName>
(Schultz, 1982); 
<taxonomicName id="D67A54231E69FDDF30F6DDC2482D3346" genus="T." lsidName="T. inflectofrons" pageId="1" pageNumber="2" rank="species" species="inflectofrons">T. inflectofrons</taxonomicName>
(Schultz, 1982); 
<taxonomicName id="EFC6B263F9D737F3CF81AADB1586962B" genus="T." lsidName="T. serraticaudum" pageId="1" pageNumber="2" rank="species" species="serraticaudum">T. serraticaudum</taxonomicName>
(Kussakin &amp; Vasina, 1984); 
<taxonomicName id="EBC777ADC27CB0283853E5A10881155E" genus="T." lsidName="T. smirnovi" pageId="1" pageNumber="2" rank="species" species="smirnovi">T. smirnovi</taxonomicName>
(Vasina, 1982); and 
<taxonomicName id="528804316080953E729FDF0AB6A42653" genus="T." lsidName="T. haswelli" pageId="1" pageNumber="2" rank="species" species="haswelli">T. haswelli</taxonomicName>
(Beddard, 1886).
</paragraph>
