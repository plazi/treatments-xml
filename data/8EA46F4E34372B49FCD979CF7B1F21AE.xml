<taxonomicName id="97937DA320DEFE836B8E3EF4A65DD468" genus="T." lsidName="T. virginale" pageId="1" pageNumber="2" rank="species" species="virginale">T. virginale</taxonomicName>
(Schultz, 1982), Scotia Sea, Antarctica, 567 m; 
<taxonomicName id="B69A097F6FB70682D8CD97C971754C46" genus="T." lsidName="T. pulchrum" pageId="1" pageNumber="2" rank="species" species="pulchrum">T. pulchrum</taxonomicName>
(Schultz, 1982), southern Argentina, 1911 m; 
<taxonomicName id="73533892C7B1C4A66BC09B6B4B540B30" genus="T." lsidName="T. serrulatus" pageId="1" pageNumber="2" rank="species" species="serrulatus">T. serrulatus</taxonomicName>
