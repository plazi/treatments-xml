<materialsCitation id="47FE3CE0FFEB327E1500FD0B3190FCA0" ID-GBIF-Occurrence="3461295313" box="[331,1411,762,787]" collectingDate="1922-01-29" collectingDateMax="1922-01-31" collectingDateMin="1922-01-29" collectionCode="BMNH" collectorName="L. J. Toxopeus" country="Indonesia" location="Station 4" municipality="Island Buru" pageId="3" pageNumber="65" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
