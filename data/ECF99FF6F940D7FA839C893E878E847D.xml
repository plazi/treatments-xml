<taxonomicName id="5C00B5D6533B7BD774C9A8CAAC1EB7A2" LSID="http://zoobank.org/0F412ACA-A932-4669-9B03-517C2F45B55B" authority="Khalaim &amp; Balueva" class="Insecta" family="Ichneumonidae" genus="Probles" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Probles (Euporizon) fulgida" order="Hymenoptera" pageId="3" pageNumber="80" phylum="Arthropoda" rank="species" species="fulgida" subGenus="Euporizon">Probles (Euporizon) fulgida Khalaim &amp; Balueva</taxonomicName>
<taxonomicNameLabel id="BE7AD7D9C7422C24D86E017197B852A4" pageId="3" pageNumber="80">sp. n.</taxonomicNameLabel>
