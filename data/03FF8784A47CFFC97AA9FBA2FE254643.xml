<taxonomicName id="4C564D11A47CFFC97BE8FA5FFDEB45F6" authorityName="E. Geoffroy Saint-Hilaire" authorityYear="1803" box="[476,581,1425,1448]" class="Mammalia" family="Talpidae" genus="Scalopus" kingdom="Animalia" order="Soricomorpha" pageId="29" pageNumber="30" phylum="Chordata" rank="genus">
