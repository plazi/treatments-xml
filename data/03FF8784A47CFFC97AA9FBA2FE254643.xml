<taxonomicName id="4C564D11A47CFFC97B2DFABAFE3B45D5" authorityName="Illiger" authorityYear="1811" box="[281,405,1396,1419]" class="Mammalia" family="Talpidae" genus="Condylura" kingdom="Animalia" order="Soricomorpha" pageId="29" pageNumber="30" phylum="Chordata" rank="genus">
