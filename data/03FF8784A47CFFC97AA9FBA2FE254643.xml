<taxonomicName id="4C564D11A47CFFC97AB4FA02FF5D45BD" authorityName="Milne-Edwards" authorityYear="1871" box="[128,243,1484,1507]" class="Mammalia" family="Talpidae" genus="Uropsilus" kingdom="Animalia" order="Soricomorpha" pageId="29" pageNumber="30" phylum="Chordata" rank="genus">
