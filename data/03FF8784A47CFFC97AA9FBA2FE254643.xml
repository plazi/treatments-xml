<taxonomicName id="4C564D11A47CFFC97AE3FA61FEE04598" authorityName="Milne-Edwards" authorityYear="1872" box="[215,334,1455,1478]" class="Mammalia" family="Talpidae" genus="Scaptonyx" kingdom="Animalia" order="Soricomorpha" pageId="29" pageNumber="30" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A47CFFC97AE3FA61FEE04598" box="[215,334,1455,1478]" italics="true" pageId="29" pageNumber="30">Scaptonyx</emphasis>
