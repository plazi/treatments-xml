<quantity id="4CFB9B59FF85FFE526F6FAAC2A40FADA" box="[443,553,1343,1369]" metricMagnitude="-3" metricUnit="m" metricValue="1.55" pageId="11" pageNumber="50" unit="mm" value="1.55">1.55 mm</quantity>
