<quantity id="4C969B25FFF0FF9919A0FD4FC82455C3" box="[947,1029,674,695]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="25" pageNumber="630" unit="mm" value="1.8">1,8 mm</quantity>
