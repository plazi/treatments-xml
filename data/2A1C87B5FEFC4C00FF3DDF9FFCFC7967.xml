<quantity id="654D9B46FEFC4C07FDF0DD55FDBD7938" box="[513,626,550,574]" metricMagnitude="2" metricUnit="m" metricValue="5.455" metricValueMax="6.21" metricValueMin="4.7" pageId="362" pageNumber="363" unit="m" value="545.5" valueMax="621.0" valueMin="470.0">
470- 
<specimenCount id="B4B3FD2AFEFC4C07FDC1DD55FDB97938" box="[560,630,550,574]" pageId="362" pageNumber="363" type="generic">621 m.</specimenCount>
