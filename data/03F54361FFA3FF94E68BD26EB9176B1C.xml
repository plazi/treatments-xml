<taxonomicName id="4C5C89F4FFA3FF94E21ED31DB83C6B38" box="[1026,1349,1329,1353]" class="Clitellata" family="Acanthodrilidae" genus="Parachilota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="27" pageNumber="31" phylum="Annelida" rank="subSpecies" species="homoeoticus" subSpecies="homoeoticus">
