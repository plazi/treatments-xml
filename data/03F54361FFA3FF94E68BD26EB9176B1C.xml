<taxonomicName id="4C5C89F4FFA3FF94E39BD31DBCD56B1C" class="Clitellata" family="Acanthodrilidae" genus="Parachilota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="27" pageNumber="31" phylum="Annelida" rank="subSpecies" species="homoeoticus" subSpecies="hibernaculi">
<emphasis id="B9282E65FFA3FF94E39BD31DBCD56B1C" italics="true" pageId="27" pageNumber="31">P. homoeoticus hibernaculi</emphasis>
</taxonomicName>
