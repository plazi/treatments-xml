<taxonomicName id="E0B641811B414E4FFD08EAD0A82DFE68" authority="Wiedemann, 1828" authorityName="Wiedemann" authorityYear="1828" box="[663,1067,461,484]" class="Insecta" family="Asilidae" genus="Laphria" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="scapularis">
