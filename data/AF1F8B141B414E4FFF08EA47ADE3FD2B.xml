<number id="917E28251B414E4FFC65E920A83FFDDB" box="[1018,1081,573,599]" pageId="19" pageNumber="20" value="1962.0">1962</number>
</bibRefCitation>
).
</treatmentCitationGroup>
</paragraph>
<paragraph id="27093A021B414E4FFF59E978ADE3FD2B" blockId="19.[151,1434,461,679]" pageId="19" pageNumber="20">
<taxonomicName id="E0B641811B414E4FFF59E978AD4DFDF2" box="[198,331,613,638]" class="Insecta" family="Asilidae" genus="Orthogonis" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
