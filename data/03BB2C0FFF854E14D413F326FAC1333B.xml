<taxonomicName id="4C12E69AFF854E13D774F38AFE6E3685" authorityName=", Sohn" authorityYear="1960" box="[317,472,480,506]" class="Ostracoda" family="Bairdiidae" genus="Orthobairdia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
