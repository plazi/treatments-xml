<taxonomicName id="D647AF72867E4424BFED1D7214F9AFFB" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="38" pageNumber="69" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
