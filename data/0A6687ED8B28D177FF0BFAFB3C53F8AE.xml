<taxonomicName id="45CF4D788B2ED177FE5BF8CF3CDAF895" authorityName="Nielsen, Pogonovski &amp; Appleyard" authorityYear="2019" box="[455,720,1841,1867]" class="Actinopterygii" family="Aphyonidae" genus="Barathronus" kingdom="Animalia" order="Ophidiiformes" pageId="5" pageNumber="236" phylum="Chordata" rank="species" species="algrahami">
