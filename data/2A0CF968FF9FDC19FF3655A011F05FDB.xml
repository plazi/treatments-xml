<taxonomicName id="65A533FDFF9FDC19FD0756F311545EDF" box="[678,776,654,679]" class="Rhabditophora" family="Cercyridae" genus="Sabussowia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Seriata" pageId="10" pageNumber="130" phylum="Platyhelminthes" rank="species" species="dioica">
