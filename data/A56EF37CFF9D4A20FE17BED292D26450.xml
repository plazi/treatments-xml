<quantity id="EA3FEF8FFF924A20FD24BA4093446433" box="[728,797,540,564]" metricMagnitude="4" metricUnit="m" metricValue="4.0" pageId="8" pageNumber="101" unit="km" value="40.0">40 km</quantity>
