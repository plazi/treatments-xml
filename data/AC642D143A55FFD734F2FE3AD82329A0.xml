<quantity id="E33531E73A55FFD73647FDADDF33291B" box="[546,616,545,570]" metricMagnitude="-1" metricUnit="m" metricValue="3.5" pageId="28" pageNumber="321" unit="cm" value="35.0">35 cm</quantity>
