<quantity id="E33531E73A55FFD73048FDADD9F4291B" box="[1069,1199,545,570]" metricMagnitude="2" metricUnit="m" metricValue="8.16" metricValueMax="8.66" metricValueMin="7.659999999999999" pageId="28" pageNumber="321" unit="m" value="816.0" valueMax="866.0" valueMin="766.0">766–866 m</quantity>
