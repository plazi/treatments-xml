Type host: 
<taxonomicName id="8DD84D78FFCDBC57FEEF2D7CFCE6F998" authority="(Reichenow)" authorityName="Reichenow" baseAuthorityName="Reichenow" baseAuthorityYear="1891" box="[263,783,1593,1615]" class="Aves" family="Nectariniidae" genus="Cyanomitra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="424" phylum="Chordata" rank="subSpecies" species="verticalis" subSpecies="viridisplendens">
<emphasis id="78ACEAE9FFCDBC57FEEF2D7CFD6CF998" box="[263,645,1593,1615]" italics="true" pageId="23" pageNumber="424">Cyanomitra verticalis viridisplendens</emphasis>
(Reichenow)
</taxonomicName>
(
<taxonomicName id="8DD84D78FFCDBC57FCF42D7FFC59F998" box="[796,944,1593,1615]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="424" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
: 
<taxonomicName id="8DD84D78FFCDBC57FC512D7FFBAAF998" authorityName="Vigors" authorityYear="1825" box="[953,1091,1593,1615]" class="Aves" family="Nectariniidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="424" phylum="Chordata" rank="family">Nectariniidae</taxonomicName>
