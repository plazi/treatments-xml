<emphasis id="B929EAA2435D4B00DADDFC811692AC02" box="[739,767,791,814]" italics="true" pageId="4" pageNumber="774">an</emphasis>
<subScript id="17D934F5435D4B00DAC1FCB71767AC1D" attach="left" box="[767,778,801,817]" fontSize="7" pageId="4" pageNumber="774">1</subScript>
considerably longer than 
<emphasis id="B929EAA2435D4B00DC07FC811038AC02" box="[1081,1109,791,814]" italics="true" pageId="4" pageNumber="774">an</emphasis>
<subScript id="17D934F5435D4B00DC68FCB7100CAC1D" attach="left" box="[1110,1121,801,817]" fontSize="7" pageId="4" pageNumber="774">2</subScript>
and 
<emphasis id="B929EAA2435D4B00D843FCA114F4AC62" box="[125,153,823,846]" italics="true" pageId="4" pageNumber="774">an</emphasis>
<subScript id="17D934F5435D4B00D8A7FCD614C9AC7C" attach="left" box="[153,164,832,848]" fontSize="7" pageId="4" pageNumber="774">3</subScript>
