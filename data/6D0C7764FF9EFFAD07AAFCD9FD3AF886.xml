<taxonomicName id="22A5BDF1FF9EFFAD03A9F992FB52F94B" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[1172,1279,1654,1679]" class="Arachnida" family="Sparassidae" genus="Heteropoda" kingdom="Animalia" order="Araneae" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="venatoria">
