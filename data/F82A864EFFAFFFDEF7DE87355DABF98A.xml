<quantity id="B77B9ABDFFAFFFDEF64780305E83FBEE" box="[365,447,1102,1124]" metricMagnitude="3" metricUnit="m" metricValue="4.828032" pageId="1" pageNumber="276" unit="mi" value="3.0">3 miles</quantity>
