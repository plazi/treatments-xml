<emphasis id="42F7EB4AFFAFFFDEF788806F5E01FBAC" box="[162,317,1041,1062]" italics="true" pageId="1" pageNumber="276">
<typeStatus id="AF3889FAFFAFFFDEF788806F5FEBFBAC" box="[162,215,1041,1062]" pageId="1" pageNumber="276">Type</typeStatus>
locality:
</emphasis>
<location id="755C6183FFAFFFDEF663806F5D44FBAC" LSID="urn:lsid:plazi:treatment:F82A864EFFAFFFDEF7DE87355DABF98A:755C6183FFAFFFDEF663806F5D44FBAC" box="[329,632,1041,1063]" country="Democratic Republic of the Congo" county="Dredged" municipality="Lukuga River" name="Off Cape Bwana n'Denge" pageId="1" pageNumber="276">Off Cape Bwana n’Denge</location>
, 
<collectingCountry id="089477C8FFAFFFDEF5A2806F5E47FBCE" name="Democratic Republic of the Congo" pageId="1" pageNumber="276">Democratic Republic of Congo</collectingCountry>
. 
<quantity id="B77B9ABDFFAFFFDEF6A380515EE1FBCF" box="[393,477,1071,1093]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="276" unit="m" value="1000.0">
<elevation id="FBAED06BFFAFFFDEF6A380515EE1FBCF" box="[393,477,1071,1093]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="276" unit="m" value="1000.0">1000 m</elevation>
</quantity>
