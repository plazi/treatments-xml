<taxonomicName id="B5AB76A9C97EFFA396EAF8FB4DF30795" authority="(Macquart, 1850)" baseAuthorityName="Macquart" baseAuthorityYear="1850" box="[416,934,1894,1921]" class="Insecta" family="Syrphidae" genus="Melanostoma" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="fasciatum">
