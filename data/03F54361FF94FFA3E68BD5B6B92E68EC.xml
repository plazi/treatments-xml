<taxonomicName id="4C5C89F4FF94FFA3E416D381BFD36BB4" box="[522,682,1453,1477]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="44" pageNumber="48" phylum="Annelida" rank="species" species="pafuriensis">
