<collectionCode id="ED4D6AB2FF94FFA3E6FCD036BC4D6840" LSID="urn:lsid:biocol.org:col:34985" box="[224,308,1561,1585]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34985" name="KwaZulu-Natal Museum" pageId="44" pageNumber="48">NMSA</collectionCode>
