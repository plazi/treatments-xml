<taxonomicName id="4C5C89F4FF94FFA3E4FED381B9AF6BB4" authority="Reinecke and Ackermann 1977" authorityName="Reinecke and Ackermann" authorityYear="1977" box="[738,1238,1453,1477]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="44" pageNumber="48" phylum="Annelida" rank="species" species="krugeri">
