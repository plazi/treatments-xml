<typeStatus id="54E74CD5FF94FFA3E68BD04DBC7B680B" box="[151,258,1634,1658]" pageId="44" pageNumber="48">paratypes</typeStatus>
of 
<taxonomicName id="4C5C89F4FF94FFA3E737D04EBCD16808" box="[299,424,1633,1657]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="44" pageNumber="48" phylum="Annelida" rank="species" species="pafuriensis">
<emphasis id="B9282E65FF94FFA3E737D04EBCD16808" box="[299,424,1633,1657]" italics="true" pageId="44" pageNumber="48">pafuriensis</emphasis>
</taxonomicName>
labelled: ‘ 
<taxonomicName id="4C5C89F4FF94FFA3E43FD04DBEFF6808" authority="Hululukulu Pan" authorityName="Hululukulu Pan" box="[547,902,1633,1657]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="44" pageNumber="48" phylum="Annelida" rank="species" species="pafuriensis">
<emphasis id="B9282E65FF94FFA3E43FD04DBF4E6808" box="[547,567,1634,1657]" italics="true" pageId="44" pageNumber="48">D</emphasis>
. 
<emphasis id="B9282E65FF94FFA3E45BD04EBFBA6808" box="[583,707,1633,1657]" italics="true" pageId="44" pageNumber="48">pafuriensis</emphasis>
, Hululukulu Pan
</taxonomicName>
<number id="3D94E050FF94FFA3E593D04EBED26808" box="[911,939,1633,1657]" pageId="44" pageNumber="48" value="21.0">21</number>
PU 
<number id="3D94E050FF94FFA3E5FCD04EBE946808" box="[992,1005,1633,1657]" pageId="44" pageNumber="48" value="2.0">2</number>
PT, 
<date id="FFE2D4B7FF94FFA3E201D04EB9FF6808" box="[1053,1158,1633,1657]" pageId="44" pageNumber="48" value="1976-02-14">
<number id="3D94E050FF94FFA3E201D04EB9456808" box="[1053,1084,1633,1657]" pageId="44" pageNumber="48" value="76.0">76</number>
-02- 
<number id="3D94E050FF94FFA3E276D04EB9FF6808" box="[1130,1158,1633,1657]" pageId="44" pageNumber="48" value="14.0">14</number>
