. 
<taxonomicName id="80A372C7FFC9E91FFEF3FDF4949DE629" authority="Cáceres-Chamizo &amp; Sanner &amp; Tilbrook &amp; Ostrovsky, 2017" authorityName="Cáceres-Chamizo &amp; Sanner &amp; Tilbrook &amp; Ostrovsky" authorityYear="2017" box="[359,462,513,536]" class="Gymnolaemata" family="Exechonellidae" genus="Exechonella" kingdom="Animalia" order="Cheilostomatida" pageId="55" pageNumber="56" phylum="Bryozoa" rank="species" species="vavrai" status="n. sp.">
<emphasis id="75D7D556FFC9E91FFEF3FDF4949DE629" box="[359,462,513,536]" italics="true" pageId="55" pageNumber="56">E. vavrai</emphasis>
</taxonomicName>
<emphasis id="75D7D556FFC9E91FFE40FE0A9740E629" bold="true" box="[468,531,511,536]" pageId="55" pageNumber="56">
<taxonomicNameLabel id="6EE4682DFFC9E91FFE40FE0A9740E629" box="[468,531,511,536]" pageId="55" pageNumber="56" rank="species">n. sp.</taxonomicNameLabel>
