<taxonomicName id="F5800E6853BCE0B2717BC0DBFF9E8B4D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:135999" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus clarus Mayr" lsidName-HNS="Camponotus clarus Mayr" order="Hymenoptera" pageNumber="660" phylum="Arthropoda" rank="species" species="clarus">C. clarus</taxonomicName>
<taxonomicNameLabel id="6BD03104786A96432CF405CFE9F6299E" rank="species">n. sp.</taxonomicNameLabel>
