<quantity id="4D759B4CFFC5FF87FE36F822FE7DF83F" box="[389,459,1987,2013]" metricMagnitude="0" metricUnit="m" metricValue="6.1213999999999995" pageId="24" pageNumber="225" unit="in" value="241.0">241 in</quantity>
