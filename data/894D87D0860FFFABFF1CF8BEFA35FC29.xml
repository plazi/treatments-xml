<emphasis id="3390EAD4860DFFABFEF2F8FEFDB5F8C3" box="[377,539,1877,1899]" italics="true" pageId="51" pageNumber="52">
<taxonomicName id="C6E44D45860DFFABFEF2F8FEFDB9F8C3" authorityName="Carpintero" authorityYear="2014" box="[377,535,1877,1899]" class="Insecta" family="Hemiptera (awaiting allocation)" genus="Eusolenophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="testaceoides">E. testaceoides</taxonomicName>
