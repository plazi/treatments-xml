<emphasis id="D6D8EAFDFFD0735D320AFF2D5509FF1B" bold="true" box="[265,578,176,200]" pageId="10" pageNumber="269">
<taxonomicName id="23AC4D6CFFD0735D320AFF2D56B4FF11" authority="Pace, 2010" authorityName="Pace" authorityYear="2010" box="[265,511,176,194]" class="Insecta" family="Staphylinidae" genus="Pseudatheta" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="269" phylum="Arthropoda" rank="species" species="chinensis" status="n. sp.">
<emphasis id="D6D8EAFDFFD0735D320AFF2D56B4FF11" bold="true" box="[265,511,176,194]" italics="true" pageId="10" pageNumber="269">Pseudatheta chinensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CDEB5786FFD0735D3104FF2B5509FF1B" box="[519,578,182,200]" pageId="10" pageNumber="269" rank="species">n. sp.</taxonomicNameLabel>
