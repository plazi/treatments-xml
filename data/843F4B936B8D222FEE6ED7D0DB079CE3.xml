<taxonomicName id="84C82A7E92CF113FF051655F5EB85B5A" LSID="MB829584" authority="Meng Zhou &amp; Y. C. Dai" class="Agaricomycetes" family="Haploporidae" genus="Haploporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Haploporus crassus" order="Plagiorchiida" pageId="5" pageNumber="82" phylum="Basidiomycota" rank="species" species="crassus">Haploporus crassus Meng Zhou&amp;Y.C.Dai</taxonomicName>
<taxonomicNameLabel id="1B9287BFAB6C12C31FCD897278B732D0" pageId="5" pageNumber="82">sp. nov.</taxonomicNameLabel>
