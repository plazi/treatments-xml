<taxonomicName id="4C164D483C04A805FEC328FF13C8FD13" baseAuthorityName="Smith" baseAuthorityYear="1846" box="[299,509,709,733]" class="Actinopterygii" family="Mugilidae" genus="Planiliza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mugiliformes" pageId="13" pageNumber="144" phylum="Chordata" rank="species" species="macrolepis">
