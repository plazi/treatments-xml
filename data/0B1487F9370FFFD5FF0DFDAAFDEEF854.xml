</geoCoordinate>
, altitude, 
<quantity id="44459B0A370FFFD5FE9BFCCEFEA4FCAC" box="[257,328,805,831]" metricMagnitude="2" metricUnit="m" metricValue="5.029999999999999" pageId="10" pageNumber="31" unit="m" value="503.0">503m</quantity>
