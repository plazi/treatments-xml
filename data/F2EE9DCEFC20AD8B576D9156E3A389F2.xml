<taxonomicName id="C4043176ED4FE9A1FD3A9754BB78F58B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:152259" class="Insecta" family="Formicidae" genus="Tapinoma" kingdom="Animalia" lsidName="Tapinoma melanocephalum coronatum Forel" lsidName-HNS="Tapinoma melanocephalum coronatum Forel" order="Hymenoptera" pageNumber="62" phylum="Arthropoda" rank="variety" species="melanocephalum" variety="coronatum">Tapinoma melanocephalum F., var. coronatum</taxonomicName>
