<taxonomicName id="B7F64D2FFF8BFF8A15F3394CA0E4FD5F" authority="Smirnov 1976" authorityName="Smirnov" authorityYear="1976" class="Branchiopoda" family="Moinidae" genus="Moina" kingdom="Animalia" order="Diplostraca" pageId="6" pageNumber="372" phylum="Arthropoda" rank="species" species="micrura">
