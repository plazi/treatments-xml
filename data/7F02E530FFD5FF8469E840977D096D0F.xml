<taxonomicName id="30AB2FA5FFDAFF846B46475B7F2E6D2B" authorityName="Eichler" authorityYear="1946" box="[569,779,295,321]" class="Insecta" family="Philopteridae" genus="Anatoecus" kingdom="Animalia" order="Psocodea" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="brantae">
