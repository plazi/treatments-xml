: 
<taxonomicName id="4C004D7DFF88FFC6FD97DDD3FD70F972" box="[533,728,1718,1742]" class="Magnoliopsida" family="Convolvulaceae" genus="Ipomoea" kingdom="Plantae" order="Solanales" pageId="56" pageNumber="57" phylum="Tracheophyta" rank="species" species="obscura">
