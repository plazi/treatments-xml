<emphasis id="75BEEACEFFB1FF89F169F95AFF77F8C3" italics="true" pageId="7" pageNumber="48">Meyer 950</emphasis>
); 
<emphasis id="75BEEACEFFB1FF89F4A6F8B1FF15F8C3" bold="true" box="[179,193,1793,1811]" pageId="7" pageNumber="48">C</emphasis>
, two green submature fruits; 
<emphasis id="75BEEACEFFB1FF89F5A0F8B1FE17F8C3" bold="true" box="[437,451,1793,1811]" pageId="7" pageNumber="48">D</emphasis>
, two ripe fruits. 
<emphasis id="75BEEACEFFB1FF89F65FF8B1FD83F8C3" bold="true" box="[586,599,1793,1811]" pageId="7" pageNumber="48">A</emphasis>
, 
<emphasis id="75BEEACEFFB1FF89F674F8B1FDBAF8C3" bold="true" box="[609,622,1793,1811]" pageId="7" pageNumber="48">B</emphasis>
, 
<emphasis id="75BEEACEFFB1FF89F66DF8B1FD04F8C3" box="[632,720,1793,1811]" italics="true" pageId="7" pageNumber="48">Meyer 950</emphasis>
; 
<emphasis id="75BEEACEFFB1FF89F6CFF8B1FD3CF8C3" bold="true" box="[730,744,1793,1811]" pageId="7" pageNumber="48">C</emphasis>
