<typeStatus id="913A8832FF82FB42FF39DA4BFEE3F8A3" box="[226,284,1842,1866]" pageId="34" pageNumber="135">Type</typeStatus>
species 
<taxonomicName id="89814D13FF82FB42FEA0DA48FD09F8A3" authority="Sellnick, 1973: 160" authorityName="Sellnick" authorityPageNumber="160" authorityYear="1973" box="[379,758,1841,1866]" class="Arachnida" family="Uropodidae" genus="Uropoda" kingdom="Animalia" order="Mesostigmata" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="alta">
