<typeStatus id="54F68871441D233AFBAE92A5FBF1FA48" box="[1039,1089,1461,1486]" pageId="8" pageNumber="2846">type</typeStatus>
of 
<taxonomicName id="4C4D4D50441D233AFBC892A5FEBAFA76" authorityName="Simon" authorityYear="1885" class="Arachnida" family="Gnaphosidae" genus="Melicymnis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="2846" phylum="Arthropoda" rank="species" species="bicolor">
