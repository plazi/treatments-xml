<taxonomicName id="4C5238D6FFB3C55E04F1F676FD60F9D5" authority="Cai &amp; Naiyanetr &amp; Ng, 2004" authorityName="Cai &amp; Naiyanetr &amp; Ng" authorityYear="2004" box="[404,656,1543,1564]" class="Malacostraca" family="Palaemonidae" genus="Macrobrachium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="602" phylum="Arthropoda" rank="species" species="dolatum" status="sp. nov.">
<emphasis id="B9269F47FFB3C55E04F1F676FD60F9D5" box="[404,656,1543,1564]" italics="true" pageId="22" pageNumber="602">Macrobrachium dolatum</emphasis>
