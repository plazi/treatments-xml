<subSubSection id="02F46558BF59FFFDFF24F2D06A6C60B6" box="[199,786,690,718]" pageId="10" pageNumber="161" type="biology_ecology">
<paragraph id="4A5136D3BF59FFFDFF24F2D06A6C60B6" blockId="10.[151,1437,151,861]" box="[199,786,690,718]" pageId="10" pageNumber="161">
<emphasis id="789AEAC1BF59FFFDFF24F2D0685F60B7" bold="true" box="[199,289,690,716]" pageId="10" pageNumber="161">Biology</emphasis>
. Collected on 
<taxonomicName id="8DEE4D50BF59FFFDFE26F2D16B6660B6" box="[453,536,691,717]" class="Magnoliopsida" family="Malvaceae" genus="Grewia" kingdom="Plantae" order="Malvales" pageId="10" pageNumber="161" phylum="Tracheophyta" rank="genus">
