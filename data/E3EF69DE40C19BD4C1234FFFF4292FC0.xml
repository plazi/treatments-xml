<geoCoordinate id="D42409D251B988ADC7905A0A82579201" direction="north" orientation="latitude" precision="5" value="39.5681">39.5681°N</geoCoordinate>
, 
<geoCoordinate id="D09A3FB83DDE929379FC57B3EA7BBACC" direction="east" orientation="longitude" precision="5" value="21.1313">21.1313°E</geoCoordinate>
