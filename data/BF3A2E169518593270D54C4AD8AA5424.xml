<taxonomicName id="4EAB92A5BFC8E9A7C03CAC2E0FA3F976" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:239697" class="Insecta" family="Formicidae" genus="Eciton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Eciton burchelli (Westwood)" order="Hymenoptera" pageNumber="404" phylum="Arthropoda" rank="species" species="burchelli">Eciton burchelli Westw</taxonomicName>
