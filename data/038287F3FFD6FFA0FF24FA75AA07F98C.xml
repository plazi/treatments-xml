<geoCoordinate id="EE1F5022FFD6FFA1FEBFF935AFD3F948" box="[347,491,1670,1694]" degrees="76" direction="west" minutes="19" orientation="longitude" pageId="4" pageNumber="3" precision="15" seconds="02" value="-76.31722">76°19’02”W</geoCoordinate>
