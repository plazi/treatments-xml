idae) and 
<taxonomicName id="4C2B4D66FFD7FFA0FC5BFD11AB3DFD6C" authority="(Curtis)" baseAuthorityName="Curtis" baseAuthorityYear="1846" box="[959,1285,674,698]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="cocois">
