<paragraph id="8A6AD45DA6CD001C82620507FFCE1583" pageId="31" pageNumber="32">
<taxonomicName id="2906E32B37B0A383FF953CC5512FF005" class="Insecta" family="Curculionidae" genus="Scolytus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scolytus dentatus" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="dentatus">
<pageBreakToken id="06EC93930B0762C468F14E4EA79980FC" pageId="31" pageNumber="32" start="start">Scolytus</pageBreakToken>
