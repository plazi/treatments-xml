<taxonomicName id="282F0B7C0736C0187F24BB126A2B1CEB" class="Insecta" family="Curculionidae" genus="Scolytus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scolytus obelus" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="obelus">Scolytus obelus</taxonomicName>
