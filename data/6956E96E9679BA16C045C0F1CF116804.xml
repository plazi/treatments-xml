and 
<taxonomicName id="B1D3FDB74A5DC30242ECBA98661CDCBE" class="Insecta" family="Curculionidae" genus="Scolytus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scolytus piceae" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="piceae">Scolytus piceae</taxonomicName>
