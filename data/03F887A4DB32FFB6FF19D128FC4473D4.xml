<taxonomicName id="4C514D31DB32FFB6FC63D279FEE37717" authority="Yamaguti, 1940" authorityName="Yamaguti" authorityYear="1940" class="Trematoda" family="Opecoelidae" genus="Pseudopecoeloides" kingdom="Animalia" order="Plagiorchiida" pageId="2" pageNumber="3" phylum="Platyhelminthes" rank="species" species="tenuis">
