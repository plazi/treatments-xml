(
<bibRefCitation id="EFC74B4F5E26537956C3F91EFD3594B3" author="Fabricius" box="[480,640,1728,1750]" pageId="22" pageNumber="223" refString="Fabricius, J. C. (1792) Entomologia systematica emendata et aucta, secundum classes, ordines, genera, species adjectis synonimis, locis, observationibus, descriptionibus. Tomus I. Pars I. C. G. Proft, Hafniae, 330 pp. http: // dx. doi. org / 10.5962 / bhl. title. 36532" type="book" year="1792">
