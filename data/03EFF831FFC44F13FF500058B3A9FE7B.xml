<taxonomicName id="4C4632A4FFC44F10FF500058B5D5F9E8" authority="Suriano &amp; Incorvaia, 1982" authorityName="Suriano &amp; Incorvaia" authorityYear="1982" box="[151,701,1614,1639]" class="Monogenea" family="Hexabothriidae" genus="Callorhynchocotyle" kingdom="Animalia" order="Diclybothriidea" pageId="13" pageNumber="14" phylum="Platyhelminthes" rank="genus">
