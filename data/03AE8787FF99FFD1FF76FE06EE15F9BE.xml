<number id="3DCF24B6FF99FFD1FB2BFE7BE8ABFDE6" box="[1226,1258,506,532]" pageId="27" pageNumber="28" value="60.0">60</number>
, PW 
<number id="3DCF24B6FF99FFD1FACAFE7BE920FDE6" box="[1323,1377,506,532]" pageId="27" pageNumber="28" value="0.43">0.43</number>
, WL 
<number id="3DCF24B6FF99FFD1FF76FDA3EC8FFDCE" box="[151,206,546,572]" pageId="27" pageNumber="28" value="1.09">1.09</number>
