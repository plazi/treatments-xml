<typeStatus id="E5678832E470A51E5FCCF95923F5F933" box="[386,500,1694,1720]" pageId="7" pageNumber="57">Holotype</typeStatus>
</emphasis>
. 
<emphasis id="08A8EA82E470A51E5C4CF95920CAF933" bold="true" box="[514,715,1694,1720]" pageId="7" pageNumber="57">
<collectingCountry id="42CB7600E470A51E5C4CF9592063F933" box="[514,610,1694,1720]" name="Mexico" pageId="7" pageNumber="57">México</collectingCountry>
