<taxonomicName id="4C7B4D12FFF2B302FD9549A0FC9BFF7B" authorityName="C.A.Viraktamath" authorityYear="2011" baseAuthorityName="C.A.Viraktamath" baseAuthorityYear="2011" box="[569,842,152,176]" class="Insecta" family="Cicadellidae" genus="Anaceratagallia" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="robusta">
