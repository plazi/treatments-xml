<paragraph id="8BC43691FFF2B302FF3B49AFFC0BFF7B" blockId="16.[151,1437,151,611]" box="[151,986,151,176]" pageId="16" pageNumber="17">
<emphasis id="B90FEA83FFF2B302FF3B49AFFF31FF7B" bold="true" box="[151,224,151,176]" inLineHeading="true" pageId="16" pageNumber="17" reason="1">Color.</emphasis>
