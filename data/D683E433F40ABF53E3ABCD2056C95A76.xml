<taxonomicName id="D397220453FE7808253845B5F7FA359D" LSID="http://zoobank.org/8DE1B8F8-0C9F-4D82-A81A-1700C90549B4" class="Arachnida" family="Euctenizidae" genus="Eucteniza" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucteniza rosalia" order="Araneae" pageId="29" pageNumber="60" phylum="Arthropoda" rank="species" species="rosalia">Eucteniza rosalia</taxonomicName>
