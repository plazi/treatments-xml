<taxonomicName id="EFE3B4F89C06FFCA4549FF76FC581EC4" box="[791,897,151,176]" class="Gastropoda" family="Plakobranchidae" genus="Elysia" kingdom="Animalia" order="Sacoglossa" pageId="73" pageNumber="74" phylum="Mollusca" rank="species" species="nisbeti">
