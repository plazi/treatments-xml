<taxonomicName id="EFE3B4F89C06FFCA4598FF78FB6B1EC4" box="[966,1202,151,176]" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" higherTaxonomySource="GBIF" kingdom="Plantae" order="Bryopsidales" pageId="73" pageNumber="74" phylum="Chlorophyta" rank="species" species="verticillata">
