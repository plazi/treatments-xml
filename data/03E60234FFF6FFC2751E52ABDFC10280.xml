<collectingMunicipality id="6B942958FFF6FFC2755F55F8DE0F0305" box="[582,875,1531,1567]" pageId="5" pageNumber="12">Orote Peninsula</collectingMunicipality>
, 
<date id="FFF195E2FFF6FFC2748A55F8D97E0305" box="[915,1050,1531,1567]" pageId="5" pageNumber="12">
<collectingDate id="EFB56C0AFFF6FFC2748A55F8D97E0305" box="[915,1050,1531,1567]" pageId="5" pageNumber="12">Aug. 2</collectingDate>
</date>
, on 
<collectedFrom id="3853F0DFFFF6FFC2736255F8DBA90305" box="[1147,1741,1530,1567]" pageId="5" pageNumber="12">
leaves of 
<taxonomicName id="4C4FC8A1FFF6FFC2722D55F9D89B0305" box="[1332,1535,1530,1567]" class="Magnoliopsida" family="Phyllanthaceae" genus="Phyllanthus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="5" pageNumber="12" phylum="Magnoliopsida" rank="genus">
<emphasis id="B93B6F30FFF6FFC2722D55F9D89B0305" box="[1332,1535,1530,1567]" italics="true" pageId="5" pageNumber="12">Phyllanthus</emphasis>
</taxonomicName>
rnarianus
</collectedFrom>
, 
<typeStatus id="54F40D80FFF6FFC271FD55F8DA100305" box="[1764,1908,1531,1567]" pageId="5" pageNumber="12">holotype</typeStatus>
<specimenCount id="9D4978ABFFF6FFC27513562CDF1B0349" box="[522,639,1583,1619]" pageId="5" pageNumber="12" type="female">female</specimenCount>
