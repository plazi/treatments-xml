<taxonomicName id="4C114D1C38264B46FED1FAF25DC1BC7C" authority="Murphy, 2011" authorityName="Murphy" authorityYear="2011" box="[281,441,1344,1367]" class="Reptilia" family="Homalopsidae" genus="Myron" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="232" phylum="Chordata" rank="species" species="resetari" status="new species">
<emphasis id="B965EA8D38264B46FED1FAF25DC1BC7C" box="[281,441,1344,1367]" italics="true" pageId="3" pageNumber="232">Myron resetari</emphasis>
</taxonomicName>
, 
