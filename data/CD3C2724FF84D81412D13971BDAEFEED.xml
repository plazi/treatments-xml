<taxonomicName id="8295EDB1FF84D81512D139C6BD67FE3D" authority="Hwang 1957: 383" authorityName="Hwang" authorityPageNumber="383" authorityYear="1957" box="[151,617,473,495]" class="Insecta" family="Polycentropodidae" genus="Plectrocnemia" kingdom="Animalia" order="Trichoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="yunnanensis">
<emphasis id="77E14A20FF84D81512D139C6BEA3FE3D" box="[151,429,473,495]" italics="true" pageId="18" pageNumber="19">Plectrocnemia yunnanensis</emphasis>
Hwang 
<number id="F35D8415FF84D815104539C6BD34FE3D" box="[515,570,473,495]" pageId="18" pageNumber="19" value="1957.0">1957</number>
: 
<number id="F35D8415FF84D815100539C5BD67FE3D" box="[579,617,474,495]" pageId="18" pageNumber="19" value="383.0">383</number>
</taxonomicName>
