<subSubSection id="C3776556EE243B40FF78FC75FDCBFBDB" pageId="373" pageNumber="374" type="etymology">
<paragraph id="8BD236DDEE243B40FF78FC75FDCBFBDB" blockId="373.[136,778,819,2070]" pageId="373" pageNumber="374">
<emphasis id="B919EACFEE243B40FF78FC75FEFBFC14" bold="true" box="[189,318,955,981]" pageId="373" pageNumber="374">Etymology</emphasis>
