: 
<collectingRegion id="337EF862FFD8F554FE6A5F5B65CFF8E2" box="[396,509,1809,1835]" country="Brazil" name="Para" pageId="7" pageNumber="158">Pará state</collectingRegion>
: 
<location id="F465605BFFD8F554FDE15F5B6646F8E2" LSID="urn:lsid:plazi:treatment:79138796FFD8F554FF715D876553F872:F465605BFFD8F554FDE15F5B6646F8E2" box="[519,628,1809,1835]" country="Brazil" name="Santarem" pageId="7" pageNumber="158" stateProvince="Para">Santarém</location>
