<collectedFrom id="D7D851B2F5023F634EDBEE9DFE20C33C" pageId="14" pageNumber="25">
on 
<taxonomicName id="A3C469CCF5023F634E99EE9DFE20C33C" authority=", Usinger" authorityName="Usinger" class="Magnoliopsida" family="Fabaceae" genus="Crotalaria" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="25" phylum="Tracheophyta" rank="species" species="quinquefolia">
