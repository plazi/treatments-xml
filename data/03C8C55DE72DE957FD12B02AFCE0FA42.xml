<collectionCode id="ED70EC8EE72DE957FD71B7DEFD39FB31" box="[711,726,1156,1181]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="6" pageNumber="249" type="Herbarium">P</collectionCode>
