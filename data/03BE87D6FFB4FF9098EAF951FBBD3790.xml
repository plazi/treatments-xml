<geoCoordinate id="EE235007FFB4FF909B07F935F9B236E4" box="[890,1011,1735,1761]" degrees="24" direction="north" minutes="46.1" orientation="latitude" pageId="37" pageNumber="38" precision="92" value="24.768333">24°46.1’N</geoCoordinate>
