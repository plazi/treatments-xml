<geoCoordinate id="EE235007FFB4FF909B83F935FE3736E7" box="[1022,1142,1735,1762]" degrees="112" direction="west" minutes="16" orientation="longitude" pageId="37" pageNumber="38" precision="925" value="-112.26667">112°16’W</geoCoordinate>
