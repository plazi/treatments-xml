<quantity id="4CEF9B25FFB4FF909C8CF8C1FF06374B" box="[1265,1351,1843,1870]" metricMagnitude="1" metricUnit="m" metricValue="1.524" metricValueMax="3.01752" metricValueMin="0.03048" pageId="37" pageNumber="38" unit="ft" value="50.0" valueMax="99.0" valueMin="1.0">1–99 ft</quantity>
