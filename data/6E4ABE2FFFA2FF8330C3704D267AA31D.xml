<collectingCountry id="9EF44FA9FFA1FF8332D3741D2446A5C9" box="[647,788,880,905]" name="Guadeloupe" pageId="15" pageNumber="216">Guadeloupe</collectingCountry>
, 
<location id="E33C59E2FFA1FF83334A741C24F6A5C8" LSID="urn:lsid:plazi:treatment:6E4ABE2FFFA2FF8330C3704D267AA31D:E33C59E2FFA1FF83334A741C24F6A5C8" box="[798,932,881,904]" country="Guadeloupe" county="Lezard" municipality="Pointe" name="Basse Terre" pageId="15" pageNumber="216">Basse Terre</location>
, 
<collectingMunicipality id="06389543FFA1FF8333FA741D24A4A5C9" box="[942,1014,880,905]" pageId="15" pageNumber="216">Pointe</collectingMunicipality>
a 
<collectingCounty id="0F3D77B5FFA1FF83345B741D230DA5C9" box="[1039,1119,880,905]" pageId="15" pageNumber="216">Lezard</collectingCounty>
, 
<collectingDate id="8219D011FFA1FF83343E741C23B7A5C9" box="[1130,1253,881,905]" pageId="15" pageNumber="216" value="2012-05-17">
<date id="925D29F9FFA1FF83343E741C23B7A5C9" box="[1130,1253,881,905]" pageId="15" pageNumber="216" value="2012-05-17">17-V-2012</date>
</collectingDate>
, 
