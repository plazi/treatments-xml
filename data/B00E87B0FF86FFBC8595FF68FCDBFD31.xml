<quantity id="FF5F9B43FF86FFBC84D8FDB5FC65FDFA" box="[474,574,586,612]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="15" pageNumber="16" unit="mm" value="4.5" valueMax="5.0" valueMin="4.0">4–5 mm</quantity>
