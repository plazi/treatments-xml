<taxonomicName id="4C5E4D19FFF4074FFE2CFF0DFDD9DEF2" box="[506,635,153,176]" class="Insecta" family="Sphecidae" genus="Prionyx" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="niveatus">
