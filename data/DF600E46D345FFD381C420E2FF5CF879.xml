<heading id="0C3E083CD345FFD181C420E2FD4FFB2D" bold="true" box="[151,736,1158,1184]" fontSize="11" level="1" pageId="1" pageNumber="96" reason="1">
<emphasis id="65BD6342D345FFD181C420E2FD4FFB2D" bold="true" box="[151,736,1158,1184]" pageId="1" pageNumber="96">
<taxonomicName id="90C9C4D3D345FFD181C420E2FDD9FB2D" authority="Young" authorityName="Young" box="[151,630,1158,1184]" class="Insecta" family="Pyrochroidae" genus="Pseudopyrochroa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="96" phylum="Arthropoda" rank="species" species="inthanonensis" status="sp. nov.">
<emphasis id="65BD6342D345FFD181C420E2FDB2FB2D" bold="true" box="[151,541,1158,1184]" italics="true" pageId="1" pageNumber="96">Pseudopyrochroa inthanonensis</emphasis>
Young
</taxonomicName>
, 
<taxonomicNameLabel id="7E8EDE39D345FFD183D120E2FD4FFB2D" box="[642,736,1158,1184]" pageId="1" pageNumber="96" rank="species">sp. nov.</taxonomicNameLabel>
