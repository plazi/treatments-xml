<paragraph id="8BA7326D142CFFD6FF2BFF08FDAF81EC" blockId="1.[151,562,220,247]" box="[151,562,220,247]" pageId="1" pageNumber="397">
<heading id="D0EF8501142CFFD6FF2BFF08FDAF81EC" bold="true" box="[151,562,220,247]" fontSize="11" level="1" pageId="1" pageNumber="397" reason="1">
<taxonomicName id="4C1849EE142CFFD6FF2BFF08FDAF81EC" authority="DeLong &amp; Freytag, 1963" authorityName="DeLong &amp; Freytag" authorityYear="1963" box="[151,562,220,247]" class="Insecta" family="Cicadellidae" genus="Zonana" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="397" phylum="Arthropoda" rank="genus">
<emphasis id="B96CEE7F142CFFD6FF2BFF08FDAF81EC" bold="true" box="[151,562,220,247]" pageId="1" pageNumber="397">
<emphasis id="B96CEE7F142CFFD6FF2BFF08FF6B81ED" bold="true" box="[151,246,220,246]" italics="true" pageId="1" pageNumber="397">Zonana</emphasis>
<bibRefCitation id="EF894F9C142CFFD6FF40FF09FDAF81EC" author="DeLong" box="[252,562,221,247]" pageId="1" pageNumber="397" refString="DeLong, D. M. &amp; Freytag, P. H. (1963) Studies of the Gyponinae 2. A new Genus - Zonana (Homoptera: Cicadellidae). The Ohio Journal Science, 63 (6), 262 - 265." type="journal article" year="1963">
DeLong &amp; Freytag, 
<number id="3DD0204A142CFFD6FE4BFF09FDAF81EC" box="[503,562,221,247]" pageId="1" pageNumber="397" value="1963.0">1963</number>
</bibRefCitation>
</emphasis>
