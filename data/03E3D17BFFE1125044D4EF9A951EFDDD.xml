<collectingCountry id="F35D20FDFFDE12504780ED709039FE96" box="[963,1042,312,338]" name="Kenya" pageId="32" pageNumber="33">Kenya</collectingCountry>
, 
<quantity id="4CB2CD88FFDE1250405DED709043FE96" box="[1054,1128,312,338]" metricMagnitude="2" metricUnit="m" metricValue="2.95" pageId="32" pageNumber="33" unit="m" value="295.0">295 m</quantity>
(
<figureCitation id="13717CE8FFDE1250403BED7090FBFE96" box="[1144,1232,312,338]" captionStart="FIGURE 12" captionStartId="67.[151,255,1994,2018]" captionTargetBox="[161,1421,178,1982]" captionTargetId="figure-33@67.[151,1436,162,1990]" captionTargetPageId="67" captionText="FIGURE 12. Distributions by quarter degree cells of Typhlops and two species of Afrotyphlops in northeastern Africa." pageId="32" pageNumber="33">Fig. 12</figureCitation>
