<taxonomicName id="4C610AA2FFB8FFA84822FE43FBD4FD95" box="[879,1055,496,519]" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="1477" phylum="Tracheophyta" rank="species" species="undetermined">
