<taxonomicName id="4C610AA2FFB8FFA849E7FE43FCE4FD95" box="[682,815,496,519]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="1477" phylum="Tracheophyta" rank="species" species="undetermined">
