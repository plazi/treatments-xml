<taxonomicName id="4C610AA2FFB8FFA84904FDA2FCF6FDBA" authority="Sing." authorityName="Sing." box="[585,829,528,552]" class="Agaricomycetes" family="Polyporaceae" genus="Lentinus" kingdom="Fungi" order="Polyporales" pageId="15" pageNumber="1477" phylum="Basidiomycota" rank="species" species="edodes">
