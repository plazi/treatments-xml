<taxonomicName id="9AA04D3F010DDC3CF991A478FEA52A7B" box="[151,307,570,593]" class="Insecta" family="Formicidae" genus="Nematocrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="56" phylum="Arthropoda" rank="genus">
<emphasis id="6FD4EAAE010DDC3CF991A478FEA52A7B" box="[151,307,570,593]" italics="true" pageId="9" pageNumber="56">Nematocrema</emphasis>
