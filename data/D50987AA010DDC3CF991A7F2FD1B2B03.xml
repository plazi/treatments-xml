<taxonomicName id="9AA04D3F010DDC3CFBE7A47BFCCF2A7B" box="[737,857,569,593]" class="Insecta" family="Hymenoptera" genus="Atopogyne" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="56" phylum="Arthropoda" rank="genus">
<emphasis id="6FD4EAAE010DDC3CFBE7A47BFCCF2A7B" box="[737,857,569,593]" italics="true" pageId="9" pageNumber="56">Atopogyne</emphasis>
