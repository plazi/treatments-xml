<subSubSection id="D8A66503CD3AFFBFFF1376CFFD3FBF05" pageId="70" pageNumber="71" type="diagnosis">
<paragraph id="90033688CD3AFFBFFF1376CFFD3FBF05" blockId="70.[151,1436,1732,1973]" pageId="70" pageNumber="71">
<emphasis id="A2C8EA9ACD3AFFBFFF1376CFFE84BED8" bold="true" box="[151,273,1732,1757]" inLineHeading="true" pageId="70" pageNumber="71" reason="1">Diagnosis.</emphasis>
