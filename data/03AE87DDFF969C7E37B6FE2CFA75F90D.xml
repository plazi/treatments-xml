<taxonomicName id="4C074D48FF969C7E33A9FCACFAB3FCED" authorityName="Horn" authorityYear="1900" box="[1247,1383,771,795]" class="Insecta" family="Carabidae" genus="Therates" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="kraatzi">
