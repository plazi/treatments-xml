<subSubSection id="B8B6FBC3FFC4E05F309CF97EFA9EE091" lastPageId="5" lastPageNumber="577" pageId="4" pageNumber="576" type="description">
<paragraph id="F013A848FFC4E05E309CF97EFC19E7AA" blockId="4.[151,1436,1717,2030]" pageId="4" pageNumber="576">
<emphasis id="C2D8745AFFC4E05E309CF97EFEB8E68A" bold="true" box="[199,345,1717,1742]" pageId="4" pageNumber="576">Description.</emphasis>
Male (
<typeStatus id="2F1716EAFFC4E05E31F0F97EFDF2E68A" box="[427,531,1717,1742]" pageId="4" pageNumber="576">holotype</typeStatus>
). Total length 
<number id="4664BA6FFFC4E05E32E3F97DFD0DE68A" box="[696,748,1718,1742]" pageId="4" pageNumber="576" value="3.72">3.72</number>
, carapace length 
