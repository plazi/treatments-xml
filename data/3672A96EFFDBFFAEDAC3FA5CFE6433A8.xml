<taxonomicName id="79DB63FBFFDAFFABD8D8F991FCBD376D" authorityName="Strecker" authorityYear="1898" box="[663,777,1622,1644]" class="Insecta" family="Noctuidae" genus="Acontia" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="30" phylum="Arthropoda" rank="species" species="areli">
