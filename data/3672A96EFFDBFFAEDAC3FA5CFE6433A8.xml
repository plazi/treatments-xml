<taxonomicName id="79DB63FBFFDDFFACDB86FF0CFD1E31E2" authority="Streck." authorityName="Strecker" authorityYear="1898" box="[457,682,202,228]" class="Insecta" family="Noctuidae" genus="Acontia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="31" phylum="Arthropoda" rank="species" species="areli">
<emphasis id="8CAFC46AFFDDFFACDB86FF0CFDE731E2" box="[457,595,202,228]" italics="true" pageId="4" pageNumber="31">Acontia areli</emphasis>
Streck.
</taxonomicName>
, 
<collectingRegion id="7C1FD69AFFDDFFACD8FBFF0CFCBB31E2" box="[692,783,202,228]" country="United States of America" name="Arizona" pageId="4" pageNumber="31">Arizona</collectingRegion>
