<taxonomicName id="79DB63FBFFDDFFACD814FF2BFD503001" authorityName="Linnaeus" authorityYear="1758" box="[603,740,237,263]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="31" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
