<taxonomicName id="79DB63FBFFDDFFACDB04FEF2FE6D304B" authorityName="Strecker" authorityYear="1898" box="[331,473,307,334]" class="Insecta" family="Noctuidae" genus="Acontia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="31" phylum="Arthropoda" rank="species" species="areli">
