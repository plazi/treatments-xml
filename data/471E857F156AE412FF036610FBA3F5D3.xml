<materialsCitation id="7FDF3E34156BE412FF536013FB12F53E" ID-GBIF-Occurrence="1804708064" location="South Africa's Northern Cape" pageId="25" pageNumber="26" specimenCount="1" stateProvince="South Africa's Northern Cape" typeStatus="holotype">
