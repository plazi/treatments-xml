<subSubSection id="C35E65733B03442631EDFEB6FDE8C421" lastPageId="64" lastPageNumber="65" pageId="63" pageNumber="64" type="materials_examined">
<paragraph id="8BFB36F83B03442631EDFEB6FA44C75D" blockId="63.[151,872,250,305]" lastBlockId="64.[151,1437,151,573]" lastPageId="64" lastPageNumber="65" pageId="63" pageNumber="64">
<materialsCitation id="3B2C3CA53B03442631EDFEB6FA48C75D" collectionCode="NFIC-FRI" country="India" lastPageId="64" lastPageNumber="65" location="Pusa" municipality="Bahadurpur" pageId="63" pageNumber="64" specimenCount="1" specimenCount-female="1" stateProvince="Bihar" typeStatus="holotype">
Type locality: 
<location id="8E9B60233B0344593053FEB6FE96C708" LSID="urn:lsid:plazi:treatment:03ED87EE3B03442631EDFF38FE43C405:8E9B60233B0344593053FEB6FE96C708" box="[297,347,281,304]" country="India" municipality="Bahadurpur" name="Pusa" pageId="63" pageNumber="64" stateProvince="Bihar">Pusa</location>
