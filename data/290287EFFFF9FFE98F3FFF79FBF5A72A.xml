<taxonomicName id="66AB4D7AFFF9FFE98FB6FE04FB58A2FC" authorityName="Barbour &amp; Loveridge" authorityYear="1929" box="[925,1195,427,450]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="mabouia">
