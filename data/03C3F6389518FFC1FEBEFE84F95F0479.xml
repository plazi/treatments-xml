<number id="3DA255099518FFC2FEFDFD5CFB6A0481" box="[331,358,708,734]" pageId="9" pageNumber="10" value="14.0">14</number>
th reduced, 
<number id="3DA255099518FFC2FE43FD5CF8020481" box="[501,526,708,734]" pageId="9" pageNumber="10" value="11.0">11</number>
