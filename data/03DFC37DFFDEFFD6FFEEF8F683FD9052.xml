18. 
<collectionCode id="ED67EAAEFFDEFFD1FAD3FECD85269713" box="[1356,1378,279,303]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="11" pageNumber="170" type="Museum">V</collectionCode>
