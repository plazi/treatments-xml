<taxonomicName id="F89F27C1FFEDC2038A95B16FBA3C7019" box="[398,520,1544,1568]" class="Insecta" family="Chironomidae" genus="Procladius" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="31" phylum="Arthropoda" rank="genus">
