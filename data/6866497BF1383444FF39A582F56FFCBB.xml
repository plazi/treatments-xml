<taxonomicName id="27CF83EEF1273444FD2EA1D6F769FD16" box="[640,792,728,752]" class="Insecta" family="Clusiidae" genus="Allometopon" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="sensitivum">
