<taxonomicName authority="MacLeay, 1819" authorityName="MacLeay" authorityYear="1819" lsidName="" pageId="123" pageNumber="124" rank="subFamily" subFamily="Syndesinae">Syndesinae MacLeay, 1819</taxonomicName>
