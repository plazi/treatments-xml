<taxonomicName authorityName="Leach" authorityYear="1815" box="[1118,1279,1279,1298]" class="Insecta" family="Pentatomidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="227" phylum="Arthropoda" rank="family">Pentatomidae</taxonomicName>
