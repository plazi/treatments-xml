<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[1300,1428,1279,1298]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="227" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
