<quantity metricMagnitude="2" metricUnit="m" metricValue="3.95" metricValueMax="6.4" metricValueMin="1.5" pageId="1" pageNumber="419" unit="m" value="395.0" valueMax="640.0" valueMin="150.0">150 to 640 m</quantity>
