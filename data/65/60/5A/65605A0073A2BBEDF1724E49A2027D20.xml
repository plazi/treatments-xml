species. From top left, 
<taxonomicName class="Insecta" family="Curculionidae" genus="Anisandrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anisandrus dispar" order="Coleoptera" pageId="8" pageNumber="27" phylum="Arthropoda" rank="species" species="dispar">Anisandrus dispar</taxonomicName>
, 
<taxonomicName genus="A." lsidName="A. maiche" pageId="8" pageNumber="27" rank="species" species="maiche">A. maiche</taxonomicName>
, 
<taxonomicName genus="A." lsidName="A. obesus" pageId="8" pageNumber="27" rank="species" species="obesus">A. obesus</taxonomicName>
