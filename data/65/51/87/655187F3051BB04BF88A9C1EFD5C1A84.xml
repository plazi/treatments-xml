<paragraph blockId="11.[151,386,581,607]" box="[151,386,581,607]" pageId="11" pageNumber="162">
<heading bold="true" box="[151,386,581,607]" fontSize="11" level="1" pageId="11" pageNumber="162" reason="1">
<taxonomicName authority="Gray, 1842" authorityName="Gray" authorityYear="1842" box="[151,386,581,607]" class="Reptilia" family="Diplodactylidae" genus="Oedura" kingdom="Animalia" order="Squamata" pageId="11" pageNumber="162" phylum="Chordata" rank="genus">
