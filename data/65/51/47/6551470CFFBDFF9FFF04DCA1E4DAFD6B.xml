<quantity box="[822,924,500,524]" metricMagnitude="1" metricUnit="m" metricValue="5.2" metricValueMax="9.4" metricValueMin="1.0" pageId="12" pageNumber="85" unit="m" value="52.0" valueMax="94.0" valueMin="10.0">
