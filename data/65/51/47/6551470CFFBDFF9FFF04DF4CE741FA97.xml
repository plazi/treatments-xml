<quantity box="[1099,1157,1207,1232]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="12" pageNumber="85" unit="m" value="15.0">
<elevation box="[1099,1157,1207,1232]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="12" pageNumber="85" unit="m" value="15.0">15 m</elevation>
</quantity>
depth, 
<date box="[1240,1414,1207,1232]" pageId="12" pageNumber="85" value="2018-03-14">
<collectingDate box="[1240,1414,1207,1232]" pageId="12" pageNumber="85" value="2018-03-14">14 March 2018</collectingDate>
</date>
