<emphasis bold="true" box="[151,661,766,792]" pageId="9" pageNumber="52">
<typeStatus box="[151,212,766,792]" pageId="9" pageNumber="52">Type</typeStatus>
species: 
<taxonomicName box="[323,587,766,792]" class="Insecta" family="Miridae" genus="Goodeniaphila" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="52" phylum="Arthropoda" rank="species" species="cassisi" status="sp. n.">
<emphasis bold="true" box="[323,587,766,792]" italics="true" pageId="9" pageNumber="52">Goodeniaphila cassisi</emphasis>
