<emphasis box="[462,600,850,876]" italics="true" pageId="21" pageNumber="668">
<taxonomicName authorityName="Thomas" authorityYear="1915" box="[462,596,850,876]" class="Mammalia" family="Muridae" genus="Grammomys" kingdom="Animalia" order="Rodentia" pageId="21" pageNumber="668" phylum="Chordata" rank="genus">Grammomys</taxonomicName>
