<paragraph blockId="21.[112,646,818,971]" pageId="21" pageNumber="668">
<taxonomicName authorityName="Thomas" authorityYear="1915" box="[113,244,850,876]" class="Mammalia" family="Muridae" genus="Grammomys" kingdom="Animalia" order="Rodentia" pageId="21" pageNumber="668" phylum="Chordata" rank="genus">
