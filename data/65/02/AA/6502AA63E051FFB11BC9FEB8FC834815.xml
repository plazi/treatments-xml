<collectionCode box="[677,750,613,636]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" lsid="urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="21" pageNumber="668" type="Museum">ZFMK</collectionCode>
