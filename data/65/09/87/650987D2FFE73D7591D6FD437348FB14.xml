<collectingRegion box="[499,784,1240,1266]" country="Laos" name="Xiangkhouang" pageId="7" pageNumber="144">Xieng Khouang Province</collectingRegion>
of 
<collectingCountry box="[821,875,1241,1267]" name="Laos" pageId="7" pageNumber="144">Laos</collectingCountry>
