<collectingRegion box="[499,641,1827,1853]" country="Laos" name="Bolikhamxai" pageId="1" pageNumber="138">Bolikhamsai</collectingRegion>
and 
<collectingRegion box="[695,873,1827,1853]" country="Laos" name="Xiangkhouang" pageId="1" pageNumber="138">Xieng Khouang</collectingRegion>
