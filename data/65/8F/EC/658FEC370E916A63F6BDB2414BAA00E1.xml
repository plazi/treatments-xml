<subSubSection type="nomenclature">
<paragraph pageNumber="638">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182000" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole pedana Wilson" lsidName-HNS="Pheidole pedana Wilson" order="Hymenoptera" pageNumber="638" phylum="Arthropoda" rank="species" species="pedana">Pheidole pedana</taxonomicName>
