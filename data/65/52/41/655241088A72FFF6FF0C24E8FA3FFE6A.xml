<heading bold="true" box="[151,632,1267,1294]" fontSize="11" level="1" pageId="1" pageNumber="198" reason="1">
<emphasis bold="true" box="[151,632,1267,1294]" pageId="1" pageNumber="198">
<taxonomicName authority="Cumming &amp; Brooks 2022" box="[151,457,1267,1294]" class="Insecta" family="Dolichopodidae" genus="Microphorella" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="198" phylum="Arthropoda" rank="species" species="breviradia">
