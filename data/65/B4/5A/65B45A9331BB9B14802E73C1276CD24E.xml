<subSubSection type="synonymic_list">
<paragraph pageNumber="291">
- &quot; 
<taxonomicName class="Arachnida" family="Oppiidae" genus="Graptoppia" kingdom="Animalia" order="Sarcoptiformes" pageNumber="291" phylum="Arthropoda" rank="species" species="foveolata">G. foveolata</taxonomicName>
