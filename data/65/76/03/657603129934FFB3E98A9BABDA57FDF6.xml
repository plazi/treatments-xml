<taxonomicName box="[151,577,151,178]" class="Insecta" family="Lauxaniidae" genus="Homoneura" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="bistriata" subGenus="Homoneura">
<emphasis bold="true" box="[151,298,151,177]" italics="true" pageId="7" pageNumber="8">Homoneura</emphasis>
(
<emphasis bold="true" box="[314,461,151,177]" italics="true" pageId="7" pageNumber="8">Homoneura</emphasis>
) 
<emphasis bold="true" box="[478,577,151,177]" italics="true" pageId="7" pageNumber="8">bistriata</emphasis>
</taxonomicName>
group
</emphasis>
