<emphasis box="[1285,1313,1203,1227]" italics="true" pageId="47" pageNumber="48">ad</emphasis>
; hind tibia with 
<number box="[207,221,1239,1263]" pageId="47" pageNumber="48" value="1.0">1</number>
preapical 
<emphasis box="[337,365,1239,1263]" italics="true" pageId="47" pageNumber="48">ad</emphasis>
and 
<number box="[419,433,1239,1263]" pageId="47" pageNumber="48" value="1.0">1</number>
