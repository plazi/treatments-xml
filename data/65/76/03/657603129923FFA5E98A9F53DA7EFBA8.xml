(occasionally male with 
<number box="[640,654,296,320]" pageId="17" pageNumber="18" value="2.0">2</number>
preapical 
<emphasis box="[772,800,296,320]" italics="true" pageId="17" pageNumber="18">ad</emphasis>
) and 
<number box="[866,880,296,320]" pageId="17" pageNumber="18" value="1.0">1</number>
<emphasis box="[887,926,297,320]" italics="true" pageId="17" pageNumber="18">apv</emphasis>
. Wing (
