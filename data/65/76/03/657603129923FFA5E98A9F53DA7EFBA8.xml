: 
<number box="[363,403,370,395]" pageId="17" pageNumber="18" value="1.7">1.7</number>
: 
<number box="[408,419,370,395]" pageId="17" pageNumber="18" value="1.0">1</number>
; 
<emphasis box="[435,474,371,394]" italics="true" pageId="17" pageNumber="18">r-m</emphasis>
