<number box="[675,689,1777,1801]" pageId="19" pageNumber="20" value="1.0">1</number>
preapical 
<emphasis box="[806,834,1776,1800]" italics="true" pageId="19" pageNumber="20">ad</emphasis>
; hind tibia with 
<number box="[1021,1035,1777,1801]" pageId="19" pageNumber="20" value="1.0">1</number>
preapical 
<emphasis box="[1153,1181,1776,1800]" italics="true" pageId="19" pageNumber="20">ad</emphasis>
and 
<number box="[1237,1251,1777,1801]" pageId="19" pageNumber="20" value="1.0">1</number>
short 
<emphasis box="[1322,1361,1777,1800]" italics="true" pageId="19" pageNumber="20">apv</emphasis>
. Wing (
<figureCitation box="[159,243,1812,1837]" captionStart="FIGURES 60 – 65" captionStartId="64.[151,264,1953,1976]" captionTargetBox="[203,1384,195,1928]" captionTargetId="figure@64.[202,1385,193,1932]" captionTargetPageId="64" captionText="FIGURES 60 – 65. Homoneura (Homoneura) chinensis Malloch, 1926. Male. 60, 61. body, lateral view; 62. head, anterior view; 63. thorax, dorsal view; 64. abdomen, dorsal view; 65. wing." httpUri="https://zenodo.org/record/250646/files/figure.png" pageId="19" pageNumber="20">
Fig. 
<number box="[214,243,1812,1837]" pageId="19" pageNumber="20" value="65.0">65</number>
