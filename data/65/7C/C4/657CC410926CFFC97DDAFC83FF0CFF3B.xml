<bibRefCitation author="Fabricius, J. C." box="[491,672,909,935]" pageId="5" pageNumber="6" refId="ref26605" refString="Fabricius, J. C. (1781) Species insectorum, exhibentes eorum differentias specificas, synonyma auctorum, loca natalia, metamorphosin, adjectis observationibus, descriptionibus. Tom 1. Hamburg &amp; Kilonii, Bohnii, 552 pp. https: // doi. org / 10.5962 / bhl. title. 36509" type="book" year="1781">Fabricius, 1781</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Eversmann, 1852)" authorityName="Eversmann" authorityYear="1852" baseAuthorityName="Eversmann" baseAuthorityYear="1852" box="[694,1029,908,935]" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="integra">
<emphasis box="[694,804,910,935]" italics="true" pageId="5" pageNumber="6">T. integra</emphasis>
(
<bibRefCitation author="Eversmann, E." box="[820,1020,908,935]" pageId="5" pageNumber="6" pagination="2 - 137" refId="ref26460" refString="Eversmann, E. (1852) Fauna Hymenopterologica Volgo-Uralensis. Bulletin de la Societe Imperiale des Naturalistes, Moscou, 25, 2 - 137." type="journal article" year="1852">Eversmann, 1852</bibRefCitation>
)
</taxonomicName>
<emphasis bold="true" box="[1037,1177,909,935]" pageId="5" pageNumber="6">stat. resurr.</emphasis>
, 
<taxonomicName authority="Kasparek, 2020" authorityName="Kasparek" authorityYear="2020" box="[1192,1327,909,935]" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="anatolica" status="sp. n.">
<emphasis box="[1192,1327,909,935]" italics="true" pageId="5" pageNumber="6">T. anatolica</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1335,1401,909,935]" pageId="5" pageNumber="6">
<taxonomicNameLabel box="[1335,1401,909,935]" pageId="5" pageNumber="6" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="(Olivier, 1789)" authorityName="Olivier" authorityYear="1789" baseAuthorityName="Olivier" baseAuthorityYear="1789" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="varia">
<emphasis italics="true" pageId="5" pageNumber="6">T. varia</emphasis>
(
<bibRefCitation author="Olivier, G. A." box="[226,378,945,971]" pageId="5" pageNumber="6" pagination="57 - 84" refId="ref27496" refString="Olivier, G. A. (1789) Abeille. Apis. Lin. Geof. Fab. [Linnaeus, Geoffroy, Fabricius]. Encyclopedie Methodique ou par Ordre de Matieres, Par une Societe de Gens de Lettres, de Savans et d'Artistes, Livr. Histoire Naturelle, Insectes, 4, 57 - 84." type="journal article" year="1789">Olivier, 1789</bibRefCitation>
)
</taxonomicName>
<emphasis bold="true" box="[394,532,945,971]" pageId="5" pageNumber="6">stat. resurr.</emphasis>
, 
<taxonomicName authority="Kasparek, 2020" authorityName="Kasparek" authorityYear="2020" box="[547,730,945,971]" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="maghrebensis" status="sp. n.">
<emphasis box="[547,730,945,971]" italics="true" pageId="5" pageNumber="6">T. maghrebensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[738,802,945,971]" pageId="5" pageNumber="6">
<taxonomicNameLabel box="[738,802,945,971]" pageId="5" pageNumber="6" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="Dubitzky, 2007" authorityName="Dubitzky" authorityYear="2007" box="[817,1096,945,971]" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="heinzi">
<emphasis box="[817,912,945,971]" italics="true" pageId="5" pageNumber="6">T. heinzi</emphasis>
<bibRefCitation author="Dubitzky, A." box="[920,1096,945,971]" pageId="5" pageNumber="6" pagination="107 - 113" refId="ref26374" refString="Dubitzky, A. (2007) Taxonomic notes on the western palaearctic species of Trachusa, subgenus Paraanthidium, with description of a new species from Turkey (Hymenoptera, Apoidea, Megachilidae). Mitteilungen der Munchener Entomologischen Gesellschaft, 97, 107 - 113." type="journal article" year="2007">Dubitzky, 2007</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Kasparek, 2020" authorityName="Kasparek" authorityYear="2020" box="[1108,1281,945,971]" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="grandicornis" status="sp. n.">
<emphasis box="[1108,1281,945,971]" italics="true" pageId="5" pageNumber="6">T. grandicornis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1289,1353,945,971]" pageId="5" pageNumber="6">
<taxonomicNameLabel box="[1289,1353,945,971]" pageId="5" pageNumber="6" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, and 
<taxonomicName authority="Kasparek, 2020" authorityName="Kasparek" authorityYear="2020" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="taurica" status="sp. n.">
<emphasis italics="true" pageId="5" pageNumber="6">T. taurica</emphasis>
</taxonomicName>
<emphasis bold="true" box="[239,303,981,1007]" pageId="5" pageNumber="6">
<taxonomicNameLabel box="[239,303,981,1007]" pageId="5" pageNumber="6" rank="species">sp. n.</taxonomicNameLabel>
