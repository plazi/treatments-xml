<paragraph pageId="null" pageNumber="716">
<taxonomicName authority="L." class="Magnoliopsida" family="Aristolochiaceae" genus="Aristolochia" kingdom="Plantae" order="Piperales" pageId="null" pageNumber="716" phylum="Tracheophyta" rank="species" species="rotunda">
