<taxonomicName authorityName="Haworth" authorityYear="1828" box="[868,994,1798,1821]" class="Insecta" family="Gelechiidae" genus="Recurvaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="364" phylum="Arthropoda" rank="genus">
<emphasis box="[868,994,1798,1821]" italics="true" pageId="11" pageNumber="364">Recurvaria</emphasis>
</taxonomicName>
sp. (
<taxonomicName baseAuthorityName="Guerrieri &amp; Noyes" baseAuthorityYear="2005" box="[1051,1191,1797,1822]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="364" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
, 
<taxonomicName authorityName="Stainton" authorityYear="1854" box="[1204,1340,1797,1822]" class="Insecta" family="Gelechiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="364" phylum="Arthropoda" rank="family">Gelechiidae</taxonomicName>
