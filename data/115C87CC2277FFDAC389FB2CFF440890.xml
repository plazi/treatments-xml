<emphasis id="AB81EAC82274FFDBC389F850FEBE08F9" bold="true" box="[151,325,1975,1999]" pageId="2" pageNumber="575">FIGURE 1A–E.</emphasis>
<taxonomicName id="5EF54D592274FFDBC250F850FD8F08F9" authority="Deng, 2021" authorityName="Deng" authorityYear="2021" box="[334,628,1975,1999]" class="Insecta" family="Tetrigidae" genus="Deltonotusoides" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="575" phylum="Arthropoda" rank="species" species="strictivertex" status="sp. nov.">
<emphasis id="AB81EAC82274FFDBC250F850FD8F08F9" box="[334,628,1975,1999]" italics="true" pageId="2" pageNumber="575">Deltonotusoides strictivertex</emphasis>
</taxonomicName>
<emphasis id="AB81EAC82274FFDBC163F850FD2B08F9" bold="true" box="[637,720,1975,1999]" pageId="2" pageNumber="575">
