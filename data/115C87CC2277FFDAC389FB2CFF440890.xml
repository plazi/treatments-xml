<taxonomicName id="5EF54D592275FFDAC205F98CFDFA09B3" authority="Hancock" authorityName="Hancock" authorityYear="1904" box="[283,513,1643,1669]" class="Insecta" family="Tetrigidae" genus="Deltonotus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="576" phylum="Arthropoda" rank="genus">
