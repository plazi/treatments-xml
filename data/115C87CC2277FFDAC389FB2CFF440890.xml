<taxonomicName id="5EF54D592275FFDAC129F8C7FCE8080F" authority="Hancock" authorityName="Hancock. A" authorityYear="1913" box="[567,787,1823,1850]" class="Insecta" family="Tetrigidae" genus="Stegaceps" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="576" phylum="Arthropoda" rank="genus">
