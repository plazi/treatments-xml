<taxonomicName id="5EF54D592275FFDAC389F93FFE4309C7" authority="Hancock, 1913" authorityName="Hancock. A" authorityYear="1913" box="[151,440,1751,1778]" class="Insecta" family="Tetrigidae" genus="Stegaceps" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="576" phylum="Arthropoda" rank="genus">
