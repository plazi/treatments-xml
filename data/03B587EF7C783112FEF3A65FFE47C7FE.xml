. 
<taxonomicName id="4C1C4D7A7C7B3113FE6AA2E9FD40C09E" box="[401,654,1719,1742]" class="Polychaeta" family="Terebellidae" genus="Streblosoma" kingdom="Animalia" order="Terebellida" pageId="6" pageNumber="7" phylum="Annelida" rank="species" species="hartmanae">
