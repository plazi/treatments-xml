<taxonomicName id="7888D3C4FFE57525FC97F9CDFB68E339" authority="(Thunb.) Benth." authorityName="Benth." baseAuthorityName="Thunb." box="[877,1248,1558,1583]" class="Magnoliopsida" family="Scrophulariaceae" genus="Nemesia" kingdom="Plantae" order="Lamiales" pageId="21" pageNumber="108" phylum="Tracheophyta" rank="species" species="fruticans">
