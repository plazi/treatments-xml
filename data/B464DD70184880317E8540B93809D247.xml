, 
<normalizedToken id="EE89166F4DB6D5113760BCF6096088C7" originalValue="Кобдоский">Kobdoskii</normalizedToken>
<normalizedToken id="76DF972D0685B39160B9EDCB4E0563C3" originalValue="аймак">aimak</normalizedToken>
, 
