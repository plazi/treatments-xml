. 
<normalizedToken id="4816CEC898B7DE72E348926F090CB45E" originalValue="Елхон">Elkhon</normalizedToken>
, 20 
<normalizedToken id="6ED4B94F45C3F9DFE166A77F57CCE860" originalValue="км">km</normalizedToken>
<normalizedToken id="87897E3C43A5F4268C22A406B3D2C88A" originalValue="ЮВ">YUV</normalizedToken>
<normalizedToken id="47360B62AD268CF15E5FB15B55FC3242" originalValue="Алтая">Altaya</normalizedToken>
<normalizedToken id="F7BB240222B113B5DD44E07FEB0BE936" originalValue="на">na</normalizedToken>
