<materialsCitation id="5ABC3CAAFFDDF739FF2EF950FE6D7A3D" collectingDate="2005-04-30" collectionCode="NRM" collectorName="N. Jonsson &amp; T. Malm &amp; B. Viklund" country="Laos" county="Nam Ha" elevation="749" location="Nam Leung" municipality="Lakkhammai Village" pageId="35" pageNumber="36" specimenCount="1" stateProvince="Louangnamtha" typeStatus="holotype">
