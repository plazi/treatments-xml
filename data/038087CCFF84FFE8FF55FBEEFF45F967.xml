<taxonomicName id="4C294D59FF83FFE8FEDBFB33FD1BFB61" authority="Zhao &amp; Du" authorityName="Zhao &amp; Du" authorityYear="2021" box="[281,645,1160,1184]" class="Insecta" family="Nemouridae" genus="Amphinemura" kingdom="Animalia" order="Plecoptera" pageId="4" pageNumber="577" phylum="Arthropoda" rank="species" species="triquetra" status="sp. nov.">
