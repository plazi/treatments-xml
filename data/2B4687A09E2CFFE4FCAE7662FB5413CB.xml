[
<taxonomicName id="64EF4D359E2CFFE4FC97766DFBD714C3" authority="Wied" authorityName="Wied" box="[804,1057,631,654]" class="Aves" family="Sylviidae" genus="Sylvia" kingdom="Animalia" order="Passeriformes" pageId="40" pageNumber="40" phylum="Chordata" rank="species" species="poicilotis">
<emphasis id="919BEAA49E2CFFE4FC97766DFC2C14C3" box="[804,986,631,654]" italics="true" pageId="40" pageNumber="40">Sylvia poicilotis</emphasis>
Wied
