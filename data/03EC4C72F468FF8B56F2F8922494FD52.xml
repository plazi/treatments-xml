<taxonomicName id="4C4586E7F464FF8B552BFEFB26EEFECB" box="[846,985,260,284]" class="Insecta" family="Muscidae" genus="Limnophora" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="509" phylum="Arthropoda" rank="genus">
<emphasis id="B9312176F464FF8B552BFEFB26EEFECB" box="[846,985,260,284]" italics="true" pageId="8" pageNumber="509">Limnophora</emphasis>
