<taxonomicName id="4C006CC3847A4665FD685BC36A37FA3D" box="[673,829,1498,1513]" class="Arachnida" family="Anapidae" genus="Gaiziapis" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="165" phylum="Arthropoda" rank="species" species="encunensis">Gaiziapis encunensis</taxonomicName>
<taxonomicNameLabel id="A2477629847A4665FC815BC36AA8FA3D" box="[840,930,1498,1513]" pageId="6" pageNumber="165" rank="species">new species</taxonomicNameLabel>
; 2. 
<taxonomicName id="4C006CC3847A4665FC025BC36D8EFA3D" box="[971,1156,1497,1513]" class="Arachnida" family="Anapidae" genus="Minanapis" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="165" phylum="Arthropoda" rank="species" species="menglunensis">Minanapis menglunensis</taxonomicName>
<taxonomicNameLabel id="A2477629847A4665FB465BC36BD9FA2A" pageId="6" pageNumber="165" rank="species">new species</taxonomicNameLabel>
; 3. 
<taxonomicName id="4C006CC3847A4665FD385BF66A88FA2A" box="[753,898,1518,1534]" class="Arachnida" family="Anapidae" genus="Sinanapis" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="165" phylum="Arthropoda" rank="species" species="longituba">Sinanapis longituba</taxonomicName>
