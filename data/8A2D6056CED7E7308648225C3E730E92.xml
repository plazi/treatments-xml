<taxonomicName id="66CAFE0F9F2542EDD066963B72EAA389" LSID-HNS="urn:lsid:biosci.ohio-state.edu:138343" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica congerens Nylander" order="Hymenoptera" pageId="3" pageNumber="3" phylum="Arthropoda" rank="species" species="congerens">Formica congerens</taxonomicName>
