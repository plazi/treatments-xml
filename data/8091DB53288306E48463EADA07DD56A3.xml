<taxonomicName id="B18931078AAC0BD15F983DB649DF6958" class="Insecta" family="Cerambycidae" genus="Phacellus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phacellus" order="Coleoptera" pageId="215" pageNumber="216" phylum="Arthropoda" rank="genus">Phacellus</taxonomicName>
