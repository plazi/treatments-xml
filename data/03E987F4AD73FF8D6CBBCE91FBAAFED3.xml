<taxonomicName id="4C404D61AD73FF8C6D0FC902FCEDF889" authority="Newman &amp; Cannon, 1997" authorityName="Newman &amp; Cannon" authorityYear="1997" box="[291,825,1796,1818]" class="Rhabditophora" family="Pseudocerotidae" genus="Pseudobiceros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="10" pageNumber="364" phylum="Platyhelminthes" rank="species" species="flowersi">
