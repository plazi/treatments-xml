<collectingCountry id="F3577672AD72FF8D6E3FCF21FD45FED3" box="[531,657,295,320]" name="Philippines" pageId="11" pageNumber="365">Philippines</collectingCountry>
(Luzon, 
<collectingCountry id="F3577672AD72FF8D6EDBCF21FCA1FED3" box="[759,885,295,320]" name="Philippines" pageId="11" pageNumber="365">Philippines</collectingCountry>
(
<bibRefCitation id="EFD14B13AD72FF8D6FA8CF21FBB3FED3" author="Gosliner" box="[900,1127,295,320]" pageId="11" pageNumber="365" refString="Gosliner, T., Behrens, D. W. &amp; Williams, G. C. (1996) Coral reef animals of the Indo-Pacific. Sea Challengers, Monterey, 314 pp." type="book" year="1996">
Gosliner 
<emphasis id="B934EAF0AD72FF8D6FC0CF2FFBF1FED3" box="[1004,1061,295,320]" italics="true" pageId="11" pageNumber="365">et al.</emphasis>
<number id="3D8824C5AD72FF8D6800CF21FBB3FED3" box="[1068,1127,295,320]" pageId="11" pageNumber="365" value="1996.0">1996</number>
</bibRefCitation>
)).
</paragraph>
