<taxonomicName id="52A94D127642817EFE89F85755EC75A8" box="[292,453,1955,1977]" class="Malacostraca" family="Diastylidae" genus="Makrokylindrus" kingdom="Animalia" order="Cumacea" pageId="30" pageNumber="331" phylum="Arthropoda" rank="genus">
<emphasis id="A7DDEA837642817EFE89F85755EC75A8" box="[292,453,1955,1977]" italics="true" pageId="30" pageNumber="331">Makrokylindrus</emphasis>
