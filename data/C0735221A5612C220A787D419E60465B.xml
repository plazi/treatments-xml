<taxonomicName id="175BC3CDEDA3B837201AC85E42F84BA6" LSID="http://zoobank.org/7349B69A-ED88-45E5-BAA4-34FCDCA79C22" class="Arachnida" family="Nesticidae" genus="Nesticella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nesticella baobab" order="Araneae" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="baobab">Nesticella baobab</taxonomicName>
