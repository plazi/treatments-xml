<taxonomicName id="4C2F4D69FFB0A77639D30014FD9C702C" authority="Peña Cantero &amp; Vervoort, 2004" authorityName="Peña Cantero &amp; Vervoort" authorityYear="2004" box="[417,614,1556,1577]" class="Hydrozoa" family="Kirchenpaueriidae" genus="Oswaldella" kingdom="Animalia" order="Leptothecata" pageId="36" pageNumber="840" phylum="Cnidaria" rank="species" species="medeae" status="sp. nov.">
