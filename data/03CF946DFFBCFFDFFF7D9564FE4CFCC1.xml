<paragraph id="8BD9257BFFA3FFDFFF7D906FFD1AFDE4" blockId="16.[151,726,584,606]" box="[151,726,584,606]" pageId="16" pageNumber="17">
<taxonomicName id="4C665EF8FFA3FFDFFF7D906FFD1DFDE4" authority="Muraleedharan &amp; Sen, 1981: 227" authorityName="Muraleedharan &amp; Sen" authorityPageNumber="227" authorityYear="1981" box="[151,721,584,606]" class="Insecta" family="Phlaeothripidae" genus="Stigmothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="okajimai">
