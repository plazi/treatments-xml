<treatmentCitationGroup id="AB0C6837FF9A783CFF08FC41FD7EFBC9" pageId="10" pageNumber="11">
<taxonomicName id="4C1C349AFF9A783CFF08FC41FD99FBE9" authority="Davis 1922: 41" authorityName="Davis" authorityPageNumber="41" authorityYear="1922" box="[151,553,1005,1028]" class="Insecta" family="Cicadidae" genus="Tibicen" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="robinsoniana">
<emphasis id="B968930BFF9A783CFF08FC41FECAFBE9" box="[151,378,1005,1028]" italics="true" pageId="10" pageNumber="11">Tibicen robinsoniana</emphasis>
<treatmentCitation id="0ABD6908FF9A783CFE1DFC41FD99FBE9" author="Davis" box="[386,553,1005,1028]" page="41" pageId="10" pageNumber="11" year="1922">
<bibRefCitation id="EF8D32E8FF9A783CFE1DFC41FDB0FBE9" author="Davis" box="[386,512,1005,1028]" pageId="10" pageNumber="19" refString="Davis, W. T. (1922) An annotated list of the cicadas of Virginia with description of a new species. Journal of the New York Entomological Society, 30, 36 - 52." type="journal article" year="1922">
Davis 
<number id="3DD45D3EFF9A783CFE58FC41FDB0FBE9" box="[455,512,1005,1028]" pageId="10" pageNumber="11" value="1922.0">1922</number>
</bibRefCitation>
