<taxonomicName id="57B0A282FFF3FFC1FCCBFC34FB99FC22" authority="Meyen" authorityName="Meyen" box="[824,1046,978,1003]" class="Aves" family="Sylviidae" genus="Sylvia" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="flava">
