<subSubSection id="D8AA8A8AFFF3FFC1FF34FBD8FCEFFBB2" pageId="13" pageNumber="14" type="discussion">
<paragraph id="900FD901FFF3FFC1FF34FBD8FCEFFBB2" blockId="13.[151,1436,870,1147]" pageId="13" pageNumber="14">
<emphasis id="A2C40513FFF3FFC1FF34FBD8FEB4FB9E" bold="true" box="[199,315,1086,1111]" pageId="13" pageNumber="14">Remarks.</emphasis>
<taxonomicName id="57B0A282FFF3FFC1FEB2FBD9FD8BFB9F" box="[321,516,1087,1110]" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="meyeni">
