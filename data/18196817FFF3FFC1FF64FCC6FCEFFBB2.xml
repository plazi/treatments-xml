<subSubSection id="D8AA8A8AFFF3FFC1FF34FC10FCA9FBFA" pageId="13" pageNumber="14" type="materials_examined">
<paragraph id="900FD901FFF3FFC1FF34FC10FCA9FBFA" blockId="13.[151,1436,870,1147]" pageId="13" pageNumber="14">
