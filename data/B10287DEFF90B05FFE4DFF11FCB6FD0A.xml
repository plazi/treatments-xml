<taxonomicName id="FEAB4D4BFF90B05FFF52FDDFFEE4FD9A" box="[203,382,567,593]" class="Conodonta" family="Conodonta (awaiting allocation)" genus="Oepikodus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Conodontophorida" pageId="12" phylum="Chordata" rank="species" species="evae">
