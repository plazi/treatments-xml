<taxonomicName id="FEAB4D4BFF90B05FFEDBFD69FE66FD50" baseAuthorityName="Fortey" baseAuthorityYear="1974" box="[322,508,641,667]" class="Trilobita" family="Olenidae" genus="Cloacaspis" kingdom="Animalia" order="Ptychopariida" pageId="12" phylum="Arthropoda" rank="species" species="ceryx">
