<number id="9C3CEE31D652763B7B9FFA567F3EFA5A" box="[251,265,1443,1468]" pageId="30" pageNumber="231" value="6.0">6</number>
(
<number id="9C3CEE31D652763B7A76FA567F19FA5A" box="[274,302,1443,1468]" pageId="30" pageNumber="231" value="10.0">10</number>
) 
<number id="9C3CEE31D652763B7A52FA567F65FA5A" box="[310,338,1443,1468]" pageId="30" pageNumber="231" value="10.0">10</number>
(
<number id="9C3CEE31D652763B7A3FFA567F5EFA5A" box="[347,361,1443,1468]" pageId="30" pageNumber="231" value="9.0">9</number>
) 
