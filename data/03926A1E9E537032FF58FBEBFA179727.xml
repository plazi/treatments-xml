<taxonomicName id="4C3BA08B9E53703DFE12F898FBAD9216" authority="(Sasa et Okazawa, 1992)" baseAuthorityName="Sasa et Okazawa" baseAuthorityYear="1992" box="[477,1062,1886,1909]" class="Insecta" family="Chironomidae" genus="Boreoheptagyia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="kurobebrevis">
