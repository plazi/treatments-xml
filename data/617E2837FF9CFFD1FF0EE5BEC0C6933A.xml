<subSubSection id="A1CDCAAAFF9CFFD1FF0EE496C42F92AB" pageId="16" pageNumber="265" type="materials_examined">
<paragraph id="E9689921FF9CFFD1FF0EE496C42F92AB" blockId="16.[151,1436,1023,1301]" pageId="16" pageNumber="265">
<emphasis id="DBA34533FF9CFFD1FF0EE496C0169237" bold="true" box="[151,334,1023,1048]" pageId="16" pageNumber="265">Type material.</emphasis>
