<taxonomicName id="4C554D12E164FFCBFB87FD929BB4FDC2" box="[1116,1273,612,637]" class="Actinopterygii" family="Istiophoridae" genus="Istiophorus" kingdom="Animalia" order="Perciformes" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="platypterus">
