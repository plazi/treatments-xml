); 
<taxonomicName id="4C554D12E164FFCBFBBAFDCA9EE5FDC1" authority="Tanaka, 1915" authorityName="Tanaka" authorityYear="1915" class="Actinopterygii" family="Istiophoridae" genus="Tetrapterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="angustirostris">
