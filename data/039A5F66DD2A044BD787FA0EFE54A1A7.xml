<taxonomicName id="4C3395F3DD2A044BD21DF886FFD3A172" box="[1293,1435,1792,1817]" class="Insecta" family="Apidae" genus="Bremus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="religiosus">
<emphasis id="B9473262DD2A044BD21DF886FFD3A172" box="[1293,1435,1792,1817]" italics="true" pageId="30" pageNumber="31">B. religiosus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3395F3DD2A044BD7D6F8A2FB72A156" box="[198,314,1828,1853]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="czerskii">
<emphasis id="B9473262DD2A044BD7D6F8A2FB72A156" box="[198,314,1828,1853]" italics="true" pageId="30" pageNumber="31">B. czerskii</emphasis>
</taxonomicName>
(see the key; 
<figureCitation id="1308F2F5DD2A044BD6C6F8A3F863A156" box="[470,555,1828,1853]" captionStart="FIGURES 30 – 31. 30" captionStartId="29.[151,264,1763,1785]" captionTargetBox="[162,1435,1308,1738]" captionTargetId="figure@29.[151,1436,1308,1738]" captionTargetPageId="29" captionText="FIGURES 30 – 31. 30. Map showing the distribution of B. religiosus in North China with records as black spots, the province boundaries in dark grey, and all sites with records of all bumblebee species in light grey. 31. B. religiosus worker from Gansu." httpUri="https://zenodo.org/record/286577/files/figure.png" pageId="30" pageNumber="31">
