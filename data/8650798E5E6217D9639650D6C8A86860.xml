<taxonomicName id="67CA291F61543C2AE4C63657E100EE95" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:231283" class="Insecta" family="Formicidae" genus="Myrmecocystus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Myrmecocystus hortus subsp. deorum" order="Hymenoptera" phylum="Arthropoda" rank="subSpecies" species="hortus" subSpecies="deorum">M. hortus deorum Mac Cook</taxonomicName>
