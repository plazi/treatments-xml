<taxonomicName id="EE4DEFB58140C7150811553AB24944D0" class="Lecanoromycetes" family="Teloschistaceae" genus="Laundonia" kingdom="Fungi" lsidName="Laundonia flavovirescens" order="Teloschistales" pageId="0" pageNumber="35" phylum="Ascomycota" rank="species" species="flavovirescens">
