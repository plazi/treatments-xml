<emphasis id="BED9E86B04476B1BFF554FD6FEBFFC1D" bold="true" box="[197,345,898,924]" pageId="9" pageNumber="10">Comments.</emphasis>
<taxonomicName id="4BAD4FFA04476B1BFEF14FD6FD98FC1A" box="[353,638,898,923]" class="Insecta" family="Encyrtidae" genus="Adelencyrtus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="chinensis">
