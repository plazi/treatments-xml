<taxonomicName id="4C3D4D72EF034A60ABA38A21FD80F993" authority="Fagan-Jeffries &amp; Bird &amp; Giannotta &amp; Bradford &amp; Austin, 2022" authorityName="Fagan-Jeffries &amp; Bird &amp; Giannotta &amp; Bradford &amp; Austin" authorityYear="2022" box="[189,515,1602,1628]" class="Insecta" family="Braconidae" genus="Glyptapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="99" pageNumber="100" phylum="Arthropoda" rank="species" species="rodriguezae" status="sp. nov.">
