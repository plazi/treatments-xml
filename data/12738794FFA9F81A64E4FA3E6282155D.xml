<taxonomicName id="5DDA4D01FFAEF81865C8FA3F613411CC" authority="Zhang &amp; Yang, 2008" authorityName="Zhang &amp; Yang" authorityYear="2008" box="[253,570,1494,1515]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="2526" phylum="Arthropoda" rank="species" species="yangi" status="sp. nov." subGenus="Dolichopus">
