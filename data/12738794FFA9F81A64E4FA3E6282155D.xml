<caption id="CEA5660AFFAEF81865A7FA3F671A122E" ID-DOI="http://doi.org/10.5281/zenodo.5224179" ID-Zenodo-Dep="5224179" httpUri="https://zenodo.org/record/5224179/files/figure.png" pageId="12" pageNumber="2526" startId="12.[146,213,1494,1516]" targetBox="[148,1103,167,1455]" targetPageId="12">
