<number id="3DB724A4FFABCB100579F8B5FDFEF8EC" box="[532,635,1826,1852]" pageId="6" pageNumber="41" value="211.0" valueMax="240.0" valueMin="182.0">182–240</number>
(
<number id="3DB724A4FFABCB1005E6F8B5FD4AF8EC" box="[651,719,1826,1852]" pageId="6" pageNumber="41" value="205.3">205.3</number>
