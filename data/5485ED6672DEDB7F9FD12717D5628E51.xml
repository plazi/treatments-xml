<taxonomicName id="414DDB57126FC805A9DD58A70C1B277D" authority="Berlese, 1916" authorityName="Berlese" authorityYear="1916" class="Arachnida" family="Verbenaceae" genus="Epicriopsis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epicriopsis" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="family">Verbenaceae</taxonomicName>
