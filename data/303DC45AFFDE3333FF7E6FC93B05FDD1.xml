<taxonomicName id="7F940ECFFFDE3333FE396C0B3804FDAA" authorityName="Stephens" authorityYear="1829" box="[501,565,523,544]" class="Insecta" family="Goeridae" genus="Goera" kingdom="Animalia" order="Trichoptera" pageId="23" pageNumber="1630" phylum="Arthropoda" rank="genus">
