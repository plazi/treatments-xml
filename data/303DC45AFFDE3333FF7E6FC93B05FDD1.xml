<taxonomicName id="7F940ECFFFDE3333FC9B6C0B3AD4FDB7" authority="ULMER, 1927" authorityName="ULMER" authorityYear="1927" class="Insecta" family="Goeridae" genus="Goera" kingdom="Animalia" order="Trichoptera" pageId="23" pageNumber="1630" phylum="Arthropoda" rank="species" species="minuta">
