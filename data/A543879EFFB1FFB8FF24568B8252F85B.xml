<taxonomicName id="EAEA4D0BFFB1FFB8FE9657528043F8CE" authority="Monne &amp; Martins, 1976" authorityName="Monne &amp; Martins" authorityYear="1976" box="[293,831,1808,1830]" class="Insecta" family="Cerambycidae" genus="Paranisopodus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="257" phylum="Arthropoda" rank="species" species="heterotarsus">
