<taxonomicName authorityName="VANDEL" authorityYear="1963" box="[449,588,866,887]" class="Malacostraca" family="Dubioniscidae" genus="Dubioniscus" kingdom="Animalia" order="Isopoda" pageId="41" pageNumber="401" phylum="Arthropoda" rank="genus">
<emphasis box="[449,588,866,887]" italics="true" pageId="41" pageNumber="401">Dubioniscus</emphasis>
