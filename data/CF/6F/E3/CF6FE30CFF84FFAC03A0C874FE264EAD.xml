<collectingCountry box="[593,708,1149,1171]" name="Venezuela" pageId="37" pageNumber="397">Venezuela</collectingCountry>
and 
<collectingCountry box="[144,214,1179,1201]" name="Brazil" pageId="37" pageNumber="397">Brazil</collectingCountry>
(
<bibRefCitation author="Schmalfuss H" box="[231,438,1179,1202]" pageId="37" pageNumber="397" pagination="1 - 341" refId="ref26008" refString="Schmalfuss H. 2003. World catalog of terrestrial isopods (Isopoda: Oniscidea). Stuttgarter Beitrage zur Naturkunde 654: 1 - 341." type="journal article" year="2003">Schmalfuss, 2003</bibRefCitation>
).
