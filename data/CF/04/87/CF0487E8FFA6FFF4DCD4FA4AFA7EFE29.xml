<collectingRegion box="[1303,1384,354,378]" country="China" name="Gansu" pageId="25" pageNumber="24">Gansu</collectingRegion>
and 
<collectingRegion box="[192,294,386,410]" country="China" name="Shaanxi" pageId="25" pageNumber="24">Shaanxi</collectingRegion>
