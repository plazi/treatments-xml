<collectingRegion box="[1307,1440,1186,1210]" country="China" name="Chongqing" pageId="24" pageNumber="23">Chongqing</collectingRegion>
(
<bibRefCitation author="Chen, B. &amp; Y. T. Li &amp; Z. B. He" box="[200,411,1218,1242]" pageId="24" pageNumber="23" refId="ref17535" refString="Chen, B., Y. T. Li, and Z. B. He. 2010. Insects in Chongqing Municipality. Science Press; Beijing. 331 p. (in Chinese)." type="book" year="2010">Chen et al. 2010</bibRefCitation>
), 
<collectingRegion box="[433,535,1218,1242]" country="China" name="Shaanxi" pageId="24" pageNumber="23">Shaanxi</collectingRegion>
(
<bibRefCitation author="Krajcik, M." box="[553,716,1218,1242]" pageId="24" pageNumber="23" pagination="1 - 113" refId="ref19042" refString="Krajcik, M. 2011. Illustrated catalogue of Cetoniinae, Trichiinae and Valginae of China (Coleoptera: Cetoniidae). Animma. X suppl. 1: 1 - 113 + 20 pls." type="journal article" year="2011">Krajčík 2011</bibRefCitation>
), 
<collectingRegion box="[739,840,1218,1242]" country="China" name="Sichuan" pageId="24" pageNumber="23">Sichuan</collectingRegion>
and 
<collectingRegion box="[905,1004,1218,1241]" country="China" name="Yunnan" pageId="24" pageNumber="23">Yunnan</collectingRegion>
(
<bibRefCitation author="Miksic, R." box="[1021,1191,1218,1242]" pageId="24" pageNumber="23" pagination="135 - 152" refId="ref20519" refString="Miksic, R. 1987 b. Revision der orientalischen und palaarktischen Arten der Gattung Campsiura Hope (Coleoptera, Scarabaeidae, Cetoniinae). Entomologische Abhandlungen und Berichte aus dem Staatlichen Museum fur Tierkunde in Dresden 50: 135 - 152." type="journal article" year="1987">Mikšić 1987b</bibRefCitation>
