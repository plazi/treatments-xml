<collectingCountry box="[1074,1170,1634,1658]" name="Croatia" pageId="21" pageNumber="20">Croatia</collectingCountry>
, 
<collectingCountry box="[1183,1331,1634,1658]" name="Serbia" pageId="21" pageNumber="20">Montenegro</collectingCountry>
(
<bibRefCitation author="Baraud, J." pageId="21" pageNumber="20" pagination="111 - 120" refId="ref16512" refString="Baraud, J. 1990. Revision des Anoxia Castelnau d'Europe et d'Asie. 3 e note: le sous-genre Mesanoxia Medvedev. Bulletin de la Societe Entomologique de France 95: 111 - 120." type="journal article" year="1990">Baraud 1990</bibRefCitation>
), 
<collectingCountry box="[275,372,1666,1690]" name="Albania" pageId="21" pageNumber="20">Albania</collectingCountry>
(
<bibRefCitation author="Petrovitz, R." box="[387,585,1666,1690]" pageId="21" pageNumber="20" pagination="861 - 885" refId="ref21325" refString="Petrovitz, R. 1969 a. Ergebnisse der Albanien-Expedition 1961 des Deutschen Entomologischen Institutes. 77. Beitrag. Coleoptera: Scarabaeidae II (Glaphyridae, Sericinae, Melolonthinae, Rutelinae, Hopliinae, Cetoniinae, nebst Nachtragen zu Trichiinae und Valginae). Beitrage zur Entomologie 19 (7 / 8): 861 - 885." type="journal article" year="1969">Petrovitz 1969a</bibRefCitation>
, sub 
<taxonomicName authorityName="Laporte" authorityYear="1832" box="[647,884,1666,1690]" class="Insecta" family="Melolonthidae" genus="Anoxia" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="20" phylum="Arthropoda" rank="species" species="matutinalis">
<emphasis box="[647,884,1666,1690]" italics="true" pageId="21" pageNumber="20">Anoxia matutinalis</emphasis>
</taxonomicName>
) and 
<collectingCountry box="[954,1028,1666,1690]" name="Malta" pageId="21" pageNumber="20">Malta</collectingCountry>
(
<bibRefCitation author="Sabatinelli, G. &amp; S. Schembri" box="[1044,1427,1666,1690]" pageId="21" pageNumber="20" pagination="31 - 36" refId="ref21894" refString="Sabatinelli, G., and S. Schembri. 1990. Gli Scarabeidi floricoli delle isole Maltesi (Coleoptera Scarabaeidae). Bollettino della Societa Entomologica Italiana 122 (1): 31 - 36." type="journal article" year="1990">Sabatinelli and Schembri 1990</bibRefCitation>
). The 
<taxonomicName authorityName="Reitter" authorityYear="1890" box="[247,413,1698,1722]" class="Insecta" family="Melolonthidae" genus="Anoxia" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="20" phylum="Arthropoda" rank="subSpecies" species="matutinalis" subSpecies="suturalis">
