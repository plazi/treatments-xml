(
<bibRefCitation author="Ioakimov, D." box="[802,983,1538,1562]" pageId="6" pageNumber="5" pagination="758 - 778" refId="ref18496" refString="Ioakimov, D. 1899. Contribution to the fauna of the insects of the Rila Mountain. Periodichesko spisanie na bulgarskogo knizhovno druzhestvo 59: 758 - 778 (in Bulgarian)." type="journal article" year="1899">Ioakimov 1899</bibRefCitation>
), 
<collectingCountry box="[1001,1085,1538,1562]" name="Greece" pageId="6" pageNumber="5">Greece</collectingCountry>
(
<bibRefCitation author="Fairmaire, L." box="[1098,1290,1538,1562]" pageId="6" pageNumber="5" pagination="167 - 176" refId="ref17949" refString="Fairmaire, L. 1848. Description de quelques Coleopteres nouveaux. I. Especes nouvelles d'Europe. Annales de la Societe Entomologique de France 6 (2): 167 - 176." type="journal article" year="1848">Fairmaire 1848</bibRefCitation>
), 
<collectingCountry box="[1308,1440,1538,1562]" name="Macedonia" pageId="6" pageNumber="5">Macedonia</collectingCountry>
(
<bibRefCitation author="Miksic, R." box="[200,350,1569,1594]" pageId="6" pageNumber="5" pagination="49 - 281" refId="ref20167" refString="Miksic, R. 1953. Fauna insectorum Balcanica - Scarabaeidae. Godisnjak instituta u Sarajevu 6 (1 - 2): 49 - 281." type="journal article" year="1953">Mikšic 1953</bibRefCitation>
