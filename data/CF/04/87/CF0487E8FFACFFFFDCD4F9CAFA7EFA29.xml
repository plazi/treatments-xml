<collectingCountry box="[539,704,1346,1370]" name="Saudi Arabia" pageId="18" pageNumber="17">Saudi Arabia</collectingCountry>
and 
<collectingCountry box="[768,853,1346,1369]" name="Yemen" pageId="18" pageNumber="17">Yemen</collectingCountry>
(d‘Orbigny 1898), 
<collectingCountry box="[1087,1188,1346,1369]" name="Morocco" pageId="18" pageNumber="17">Morocco</collectingCountry>
(
<bibRefCitation author="Bedel, L." box="[1204,1346,1346,1370]" pageId="18" pageNumber="17" pagination="223 - 228" refId="ref16793" refString="Bedel, L. 1904. Liste des principales especes de Coleopteres recueillies par le M. is de Segonzac. In: M. is de Segonzac, Voyages au Maroc (1899 - 1901), Paris, A. Colin 1903. L'Abeille 30: 223 - 228." type="journal article" year="1904">Bedel 1904</bibRefCitation>
