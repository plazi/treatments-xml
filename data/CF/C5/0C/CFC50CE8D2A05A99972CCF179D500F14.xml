<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla chaetopterana" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="chaetopterana">
<emphasis italics="true" pageId="0" pageNumber="1">Phestilla chaetopterana</emphasis>
</taxonomicName>
(Ekimova, Deart &amp; Schepetov, 2017), comb. nov., 
<taxonomicName baseAuthorityName="Bergh" baseAuthorityYear="1870" class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla lugubris" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="lugubris">
<emphasis italics="true" pageId="0" pageNumber="1">Phestilla lugubris</emphasis>
</taxonomicName>
(Bergh, 1870), 
<taxonomicName class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla melanobrachia" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="melanobrachia">
<emphasis italics="true" pageId="0" pageNumber="1">Phestilla melanobrachia</emphasis>
</taxonomicName>
(Bergh, 1874), 
<taxonomicName authorityName="Rudman" authorityYear="1981" class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla minor" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="minor">
<emphasis italics="true" pageId="0" pageNumber="1">Phestilla minor</emphasis>
</taxonomicName>
(Rudman, 1981), 
<taxonomicName authorityName="Rudman" authorityYear="1982" class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla panamica" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="panamica">
<emphasis italics="true" pageId="0" pageNumber="1">Phestilla panamica</emphasis>
</taxonomicName>
(Rudman, 1982), 
<taxonomicName baseAuthorityName="Rudman" baseAuthorityYear="1979" class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla poritophages" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="poritophages">
<emphasis italics="true" pageId="0" pageNumber="1">Phestilla poritophages</emphasis>
</taxonomicName>
(Rudman, 1979), 
<taxonomicName authorityName="Wang &amp; Conti-Jerpe &amp; Richards &amp; Baker" authorityYear="2020" class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla subodiosus" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="subodiosus">
