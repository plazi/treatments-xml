<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[170,287,258,282]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Coleoptera" pageId="6" pageNumber="89" phylum="Tracheophyta" rank="order">Coleoptera</taxonomicName>
, 
<taxonomicName authorityName="Latreille" authorityYear="1802" box="[301,409,258,282]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="89" phylum="Arthropoda" rank="family">Carabidae</taxonomicName>
