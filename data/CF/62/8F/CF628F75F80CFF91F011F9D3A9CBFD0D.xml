<subSubSection pageId="5" pageNumber="58" type="materials_examined">
<paragraph blockId="5.[264,1326,644,1190]" box="[264,486,644,669]" pageId="5" pageNumber="58">
<emphasis box="[264,486,644,669]" italics="true" pageId="5" pageNumber="58">Material examined</emphasis>
</paragraph>
<paragraph blockId="5.[264,1326,644,1190]" pageId="5" pageNumber="58">
