<taxonomicName authority="L." class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="630" phylum="Tracheophyta" rank="species" species="militaris">
Orchis 
<normalizedToken originalValue="militáris" pageId="null" pageNumber="630">militaris</normalizedToken>
<authorityName pageId="null" pageNumber="630">L.</authorityName>
</taxonomicName>
</paragraph>
