<td align="left" pageId="null" pageNumber="232" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Orobanchaceae" genus="Rhinanthus" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="232" phylum="Tracheophyta" rank="species" species="angustifolius">
