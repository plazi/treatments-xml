<collectingCountry box="[422,493,557,583]" name="Brazil" pageId="13" pageNumber="51">Brazil</collectingCountry>
and 
<collectingCountry box="[551,670,557,583]" name="Colombia" pageId="13" pageNumber="51">Colombia</collectingCountry>
