<taxonomicName box="[151,386,672,697]" class="Magnoliopsida" family="Burseraceae" genus="Protium" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="55" phylum="Tracheophyta" rank="species" species="tenuifolium">
<emphasis box="[151,386,672,697]" italics="true" pageId="12" pageNumber="55">Protium tenuifolium</emphasis>
</taxonomicName>
(
<taxonomicName authority="Martin" authorityName="Martin" box="[402,641,672,698]" class="Magnoliopsida" family="Burseraceae" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="55" phylum="Tracheophyta" rank="family">Burseraceae, Martin</taxonomicName>
