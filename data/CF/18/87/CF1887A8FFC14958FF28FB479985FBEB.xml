<heading bold="true" box="[151,567,1257,1284]" fontSize="11" level="1" pageId="3" pageNumber="201" reason="1">
<emphasis bold="true" box="[151,567,1257,1284]" pageId="3" pageNumber="201">
<taxonomicName authorityName="Schneider" authorityYear="1851" box="[151,280,1257,1283]" class="Insecta" family="Chrysopidae" genus="Apochrysa" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="201" phylum="Arthropoda" rank="genus">
