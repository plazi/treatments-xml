<taxonomicName authority="(Sluiter, 1909)" baseAuthorityName="Sluiter" baseAuthorityYear="1909" box="[771,1122,939,957]" class="Ascidiacea" family="Didemnidae" genus="Trididemnum" kingdom="Animalia" order="Enterogona" pageId="24" pageNumber="823" phylum="Chordata" rank="species" species="tenebricosum">
