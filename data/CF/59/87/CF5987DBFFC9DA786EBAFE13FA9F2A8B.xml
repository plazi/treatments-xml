<taxonomicName authority="Nearns and Tavakilian, 2012" authorityName="Nearns and Tavakilian" authorityYear="2012" class="Insecta" family="Cerambycidae" genus="Lingafelteria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="giuglarisi">
