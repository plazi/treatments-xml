, 
<location LSID="urn:lsid:plazi:treatment:CF53878CFFB58F222AD387E4FDC6FCCF:42256041FFB58F222E47860EFED9FE9A" country="Spain" municipality="Moratalla" name="Rio Alharabe" pageId="32" pageNumber="763" stateProvince="Murcia">Rio Alharabe</location>
