<taxonomicName authorityName="Bonelli" authorityYear="1810" class="Hexapoda" family="Carabidae" genus="Agonum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="754" pageNumber="755" phylum="Arthropoda" rank="genus">
