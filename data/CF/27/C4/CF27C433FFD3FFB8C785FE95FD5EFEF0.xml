<subSubSection lastPageId="3" lastPageNumber="4" pageId="2" pageNumber="3" type="description">
<paragraph blockId="2.[151,1436,1749,2015]" pageId="2" pageNumber="3">
<emphasis bold="true" box="[198,231,1749,1775]" pageId="2" pageNumber="3">
P 
<number box="[217,231,1749,1775]" pageId="2" pageNumber="3" value="5.0">5</number>
</emphasis>
baseoendopod and exopod fused to form 
<number box="[742,757,1749,1775]" pageId="2" pageNumber="3" value="1.0">1</number>
plate, left and right P 
<number box="[1027,1041,1749,1775]" pageId="2" pageNumber="3" value="5.0">5</number>
separated; baseoendopod with 
<number box="[1421,1436,1749,1775]" pageId="2" pageNumber="3" value="1.0">1</number>
outer basal seta, endopodal lobe with 
<number box="[599,614,1789,1815]" pageId="2" pageNumber="3" value="2.0">2</number>
setae (
<number box="[697,713,1789,1815]" pageId="2" pageNumber="3" value="4.0">4</number>
+ 
<number box="[746,762,1789,1815]" pageId="2" pageNumber="3" value="5.0">5</number>
); exopodal lobe with 
<number box="[1022,1036,1789,1815]" pageId="2" pageNumber="3" value="3.0">3</number>
