<number box="[1274,1288,1789,1815]" pageId="2" pageNumber="3" value="1.0">1</number>
surface seta (
<number box="[160,224,1829,1855]" pageId="2" pageNumber="3" value="10.5" valueMax="12.0" valueMin="9.0">9–12</number>
). 
<emphasis bold="true" box="[244,276,1829,1855]" pageId="2" pageNumber="3">
P 
<number box="[262,276,1829,1855]" pageId="2" pageNumber="3" value="6.0">6</number>
</emphasis>
with 
<number box="[343,358,1829,1855]" pageId="2" pageNumber="3" value="1.0">1</number>
