<quantity box="[808,876,1077,1098]" metricMagnitude="-2" metricUnit="m" metricValue="4.0" pageId="8" pageNumber="249" unit="mm" value="40.0">40 mm</quantity>
)............................................. 
<number box="[1423,1435,1077,1098]" pageId="8" pageNumber="249" value="9.0">9</number>
</keyLead>
</paragraph>
<paragraph blockId="8.[151,1436,402,2008]" pageId="8" pageNumber="249">
<keyLead pageId="8" pageNumber="249">
