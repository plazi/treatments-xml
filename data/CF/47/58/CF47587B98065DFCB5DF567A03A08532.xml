<paragraph pageId="0" pageNumber="13">
<taxonomicName authorityName="Hentz" authorityYear="1847" class="Arachnida" family="Araneidae" genus="Epeira" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epeira displicata" order="Araneae" pageId="0" pageNumber="13" phylum="Arthropoda" rank="species" species="displicata">
