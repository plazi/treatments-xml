<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Eggers" authorityYear="1927" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus dimidiatus" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="dimidiatus">
