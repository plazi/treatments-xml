<materialsCitation ID-GBIF-Occurrence="1657176781" collectingDate="1935-12-28" country="Curaçao" county="Considering" lastPageId="28" lastPageNumber="29" location="Osburn" municipality="Instead" pageId="27" pageNumber="28" specimenCount="4" stateProvince="California" typeStatus="holotype">
