<taxonomicName authority="(Lawrence, 1939)" baseAuthorityName="Lawrence" baseAuthorityYear="1939" box="[1051,1398,1026,1053]" class="Arachnida" family="Phyxelididae" genus="Xevioso" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="zuluana">
