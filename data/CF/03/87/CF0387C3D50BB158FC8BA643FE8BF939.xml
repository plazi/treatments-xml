<quantity box="[820,955,1792,1817]" metricMagnitude="-2" metricUnit="m" metricValue="2.85" metricValueMax="3.08" metricValueMin="2.62" pageId="6" pageNumber="19" unit="mm" value="28.5" valueMax="30.8" valueMin="26.2">26.2â€“30.8 Âµm</quantity>
