<quantity box="[1252,1375,1793,1817]" metricMagnitude="-2" metricUnit="m" metricValue="1.06" metricValueMax="1.18" metricValueMin="0.94" pageId="6" pageNumber="19" unit="mm" value="10.6" valueMax="11.8" valueMin="9.4">9.4â€“11.8 Âµm</quantity>
