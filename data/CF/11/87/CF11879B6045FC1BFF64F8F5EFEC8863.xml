<taxonomicName authority="Makarkin, 1990" authorityName="Makarkin" authorityYear="1990" box="[291,617,1924,1950]" class="Insecta" family="Osmylidae" genus="Cretosmylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="4" pageNumber="98" phylum="Arthropoda" rank="genus">
