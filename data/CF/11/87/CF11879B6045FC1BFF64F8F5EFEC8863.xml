</taxonomicName>
and 
<taxonomicName authority="Makarkin, 2014" authorityName="Makarkin" authorityYear="2014" box="[673,1043,1924,1950]" class="Insecta" family="Osmylidae" genus="Kempynosmylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="4" pageNumber="98" phylum="Arthropoda" rank="genus">
