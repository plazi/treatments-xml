<taxonomicName id="4C6D4D5EEE693B0DFF1AFDA9FDD5FD40" authority="Thorell, 1859" authorityName="Thorell" authorityYear="1859" box="[223,528,615,641]" class="Maxillopoda" family="Notodelphyidae" genus="Doropygus" kingdom="Animalia" order="Cyclopoida" pageId="312" pageNumber="313" phylum="Arthropoda" rank="genus">
