<taxonomicName id="4C6D4D5EEE693B0DFEAEF8FEFD18F88B" authority="Thorell, 1859" authorityName=", Thorell" authorityYear="1859" box="[363,733,1839,1866]" class="Maxillopoda" family="Notodelphyidae" genus="Doropygus" kingdom="Animalia" order="Cyclopoida" pageId="312" pageNumber="313" phylum="Arthropoda" rank="species" species="pulex">
