<caption id="DF2DEBD6FFAFA67CFEABFE68FBF63AE7" ID-DOI="http://doi.org/10.5281/zenodo.5227132" ID-Zenodo-Dep="5227132" box="[260,1039,483,502]" httpUri="https://zenodo.org/record/5227132/files/figure.png" pageId="7" pageNumber="860" startId="7.[260,329,483,502]" targetBox="[126,1171,133,447]" targetPageId="7">
