<taxonomicName id="4C7C20AAFF99FF89B995FE2DFAFBF6B7" box="[1196,1365,428,449]" class="Insecta" family="Megachilidae" genus="Chalicodoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="76" phylum="Arthropoda" rank="subGenus" subGenus="Pseudomegachile">
