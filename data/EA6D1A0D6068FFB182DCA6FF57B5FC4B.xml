<taxonomicName id="A5C4D0986068FFB181B3A70756E9FEF2" authorityName="Linnaeus" authorityYear="1758" box="[685,730,291,310]" class="Aves" kingdom="Animalia" pageId="8" pageNumber="243" phylum="Chordata" rank="class">Aves</taxonomicName>
