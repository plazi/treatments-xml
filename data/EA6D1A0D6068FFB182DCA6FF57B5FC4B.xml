<taxonomicName id="A5C4D0986068FFB182CEA7005606FEF2" authorityName="Quate &amp; Quate" authorityYear="1967" box="[464,565,292,310]" class="Aves" family="Acanthizidae" genus="Oreoscopus" kingdom="Animalia" order="Passeriformes" pageId="8" pageNumber="243" phylum="Chordata" rank="genus">
<emphasis id="50B077096068FFB182CEA7005606FEF2" box="[464,565,292,310]" italics="true" pageId="8" pageNumber="243">Oreoscopus</emphasis>
