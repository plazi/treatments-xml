<taxonomicName id="65F56AAAFFCEFFA3FA43A7671534F8CB" class="Insecta" family="Tiphiidae" genus="Rhagigaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="372" phylum="Arthropoda" rank="species" species="neocorrugatus" status="sp. nov.">
