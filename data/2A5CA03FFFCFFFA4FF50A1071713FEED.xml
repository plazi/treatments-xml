<taxonomicName id="65F56AAAFFCFFFA2FB2EA7D0117CF840" box="[1257,1394,1968,1993]" class="Insecta" family="Tiphiidae" genus="Rhagigaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="371" phylum="Arthropoda" rank="genus">
