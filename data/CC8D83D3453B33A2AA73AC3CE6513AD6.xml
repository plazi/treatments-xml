<taxonomicName id="BBE49436E9AAC3E8A6AC550526742DF9" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Adoxaceae" genus="Viburnum" kingdom="Plantae" order="Dipsacales" phylum="Tracheophyta" rank="species" species="lantana">Viburnum lantana</taxonomicName>
