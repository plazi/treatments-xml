<taxonomicName id="22A5BDF1FFA6FF9507FBFAF3FEC1FAF5" authorityName="Pocock" authorityYear="1901" box="[198,364,1303,1329]" class="Arachnida" family="Theraphosidae" genus="Megaphobema" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
