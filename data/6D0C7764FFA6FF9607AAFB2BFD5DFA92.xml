<paragraph id="E51AC672FFA6FF9507AAF947FB73F8B5" blockId="21.[151,1437,1231,2014]" pageId="21" pageNumber="22">
<materialsCitation id="55CDCC2FFFA6FF9507AAF947FB77F8B5" ID-GBIF-Occurrence="2990869388" collectingDate="2020-01-21" collectorName="C. Neto. Host" country="Brazil" location="Ibitipoca" municipality="State" pageId="21" pageNumber="22" specimenCount="1" stateProvince="Minas Gerais">
<collectingCountry id="9DB286E2FFA6FF9507AAF947FEACF979" box="[151,257,1699,1725]" name="Brazil" pageId="21" pageNumber="22">BRAZIL</collectingCountry>
