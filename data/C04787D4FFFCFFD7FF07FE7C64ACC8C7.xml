<taxonomicName id="8FEE4D41FFFCFFD7FF70FCB864CFC87F" authorityName="Arthur" authorityYear="1960" box="[224,396,799,825]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="cornuae">
