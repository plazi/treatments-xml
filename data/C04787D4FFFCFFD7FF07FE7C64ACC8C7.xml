<taxonomicName id="8FEE4D41FFFCFFD7FBEEFCE46069C81B" authorityName="Arthur" authorityYear="1960" box="[1150,1322,835,861]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="cornuae">
