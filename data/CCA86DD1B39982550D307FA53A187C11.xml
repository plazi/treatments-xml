<taxonomicName id="D5A1F029BF5F9C3A1527F32C43E8C858" authority="Henderson, 1888" authorityName="Henderson" authorityYear="1888" class="Malacostraca" family="Diogenidae" genus="Paguropsis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paguropsis" order="Decapoda" pageId="3" pageNumber="20" phylum="Arthropoda" rank="genus">Paguropsis</taxonomicName>
