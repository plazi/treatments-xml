<taxonomicName id="85C93EFE4E5C060DF35C312DB8BF8531" class="Insecta" family="Miridae" genus="Tytthus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tytthus entrerianus" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="entrerianus">Tytthus entrerianus</taxonomicName>
