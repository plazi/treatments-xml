<taxonomicName id="4C59A3EFFFFBFFCB9E66C18F8BA9FAE9" authorityName="Published" authorityYear="2007" box="[561,706,1272,1297]" class="Insecta" family="Hybotidae" genus="Elaphropeza" kingdom="Animalia" order="Diptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="monacantha">
<emphasis id="B92D047EFFFBFFCB9E66C18F8BA9FAE9" box="[561,706,1272,1297]" italics="true" pageId="40" pageNumber="41">monacantha</emphasis>
</taxonomicName>
