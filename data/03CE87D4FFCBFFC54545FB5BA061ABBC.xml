<quantity id="4C9F9B27FFCBFFC64681F9BBA051AB1C" box="[714,776,1547,1573]" metricMagnitude="-1" metricUnit="m" metricValue="6.604" pageId="5" pageNumber="456" unit="in" value="26.0">26 in</quantity>
<taxonomicName id="4C674D41FFCBFFC64758F9BAA0FCAB1C" authorityName="Sowerby" authorityYear="1832" box="[787,933,1546,1573]" class="Gastropoda" family="Cypraeidae" genus="Cypraea" kingdom="Animalia" order="Littorinimorpha" pageId="5" pageNumber="456" phylum="Mollusca" rank="species" species="adamsonii">
