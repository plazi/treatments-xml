<taxonomicName id="483188B3BB9495104A5FC9AC92617F66" class="Magnoliopsida" family="Asteraceae" genus="Elephantopus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Elephantopus mollis" order="Asterales" pageId="0" pageNumber="117" phylum="Tracheophyta" rank="species" species="mollis">
