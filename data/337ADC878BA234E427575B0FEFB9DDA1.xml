<taxonomicName id="4CE02949DC3FBC743006C11B5672084D" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora edulis" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="edulis">Passiflora edulis</taxonomicName>
