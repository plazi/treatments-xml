<quantity id="DB109B65FFD2FFAF6492FB10FCB46F8F" box="[754,838,1192,1213]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="11" pageNumber="109" unit="m" value="1100.0">
<elevation id="97C5D1B3FFD2FFAF6492FB10FCB46F8F" box="[754,838,1192,1213]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="11" pageNumber="109" unit="m" value="1100.0">1,100 m</elevation>
</quantity>
: 
<collectingRegion id="DE2CF862FFD2FFAF6531FB10FC626F8F" box="[849,912,1192,1213]" country="Peru" name="Cusco" pageId="11" pageNumber="109">Cuzco</collectingRegion>
, [Paucartambo,] 
