<quantity id="DB109B65FFD2FFAF6207FBE0FCBA6FBA" metricMagnitude="3" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="11" pageNumber="109" unit="m" value="1650.0" valueMax="1800.0" valueMin="1500.0">
<elevation id="97C5D1B3FFD2FFAF6207FBE0FCBA6FBA" metricMagnitude="3" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="11" pageNumber="109" unit="m" value="1650.0" valueMax="1800.0" valueMin="1500.0">1,500â€“ 1,800 m</elevation>
</quantity>
in cloud forest (
<collectionCode id="7AF9AE45FFD2FFAF6264FBCBFBB56FBA" box="[1028,1095,1139,1160]" pageId="11" pageNumber="109">LRKC</collectionCode>
, 
<collectionCode id="7AF9AE45FFD2FFAF6239FBCBFB6D6FBA" box="[1113,1183,1139,1160]" pageId="11" pageNumber="109">INBIO</collectionCode>
). 
<location id="1937605BFFD2FFAF6492FB35FCE56F90" LSID="urn:lsid:plazi:treatment:94418796FFD2FFAF6578FF7AFBF06E15:1937605BFFD2FFAF6492FB35FCE56F90" box="[754,791,1165,1186]" country="Costa Rica" county="Schedl" municipality="Wood" name="The" pageId="11" pageNumber="109" stateProvince="Cusco">The</location>
