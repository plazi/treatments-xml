<paragraph id="1C573680FFD2FFAF6492FC56FBF06E15" blockId="11.[706,1194,236,1319]" pageId="11" pageNumber="109">
<emphasis id="2E9CEA92FFD2FFAF6492FC56FCC36F31" bold="true" box="[754,817,1006,1027]" pageId="11" pageNumber="109">Notes.</emphasis>
<materialsCitation id="AC803CDDFFD2FFAF655EFC56FC0D6E15" collectingDate="2007-11-04" collectingDateMax="2007-11-09" collectingDateMin="2007-11-04" collectionCode="NHMW, USNM, LRKC, INBIO, SEMC" country="Costa Rica" county="Schedl" elevation="1650" location="The" municipality="Wood" pageId="11" pageNumber="109" specimenCount="1" stateProvince="Cusco">
