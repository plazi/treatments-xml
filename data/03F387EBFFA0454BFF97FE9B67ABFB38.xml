<typeStatus id="54E1885FFFA0454BFF4AFE5C615BFE70" box="[813,861,400,423]" pageId="19" pageNumber="170">TYPE</typeStatus>
SPECIES
</collectionCode>
. — 
<taxonomicName id="4C5A4D7EFFA0454BFFB0FE5C666CFE7E" authority="Lemaitre &amp; Felder &amp; Poupin, 2017" authorityName="Lemaitre &amp; Felder &amp; Poupin" authorityYear="2017" box="[983,1130,400,424]" class="Malacostraca" family="Paguridae" genus="Pagurina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="19" pageNumber="170" phylum="Arthropoda" rank="species" species="bifida" status="n. gen., n. sp.">
<emphasis id="B92EEAEFFFA0454BFFB0FE5C666CFE7E" box="[983,1130,400,424]" italics="true" pageId="19" pageNumber="170">Pagurina bifida</emphasis>
