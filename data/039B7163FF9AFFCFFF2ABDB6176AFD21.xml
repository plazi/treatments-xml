<taxonomicName id="4C32BBF6FF95FFCFFAABBD7113D8FEE2" class="Insecta" family="Tenebrionidae" genus="Phobelius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="semigranosus">
