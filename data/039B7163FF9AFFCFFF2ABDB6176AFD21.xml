<taxonomicName id="4C32BBF6FF95FFCFFBD8BD7117BEFECA" box="[1125,1242,312,337]" class="Insecta" family="Tenebrionidae" genus="Phobelius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus">
