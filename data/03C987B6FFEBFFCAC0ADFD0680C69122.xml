<subSubSection id="C37A652BFFEBFFCAC0ADFD0686E696B9" box="[151,660,725,751]" pageId="4" pageNumber="5" type="nomenclature">
<paragraph id="8BDF36A0FFEBFFCAC0ADFD0686E696B9" blockId="4.[151,660,725,751]" box="[151,660,725,751]" pageId="4" pageNumber="5">
<heading id="D09781CCFFEBFFCAC0ADFD0686E696B9" bold="true" box="[151,660,725,751]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<emphasis id="B914EAB2FFEBFFCAC0ADFD0686E696B9" bold="true" box="[151,660,725,751]" pageId="4" pageNumber="5">
Subgenus 
<taxonomicName id="4C604D23FFEBFFCAC12DFD0686E696B9" authority="Audinet-Serville, 1835" authorityName="Audinet-Serville" authorityYear="1835" box="[279,660,725,751]" class="Insecta" family="Cerambycidae" genus="Leiopus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
