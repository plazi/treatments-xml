<quantity id="4CD99B367D04B95AA052A442FB55F93B" box="[1098,1191,1656,1679]" metricMagnitude="-2" metricUnit="m" metricValue="1.65" pageId="2" pageNumber="51" unit="mm" value="16.5">16.5mm</quantity>
