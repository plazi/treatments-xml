<taxonomicName id="4C1F2BA1153D0246242E8881FDE494DD" authority="Mikhaljova, 2020" authorityName="Mikhaljova" authorityYear="2020" box="[294,543,1993,2017]" class="Diplopoda" family="Julidae" genus="Nepalmatoiulus" kingdom="Animalia" order="Julida" pageId="15" pageNumber="62" phylum="Arthropoda" rank="species" species="pallidus" status="sp. nov.">
