<collectingCountry id="F355760772436879FED3D3EEFE9627B6" box="[283,347,1473,1496]" name="China" pageId="3" pageNumber="350">China</collectingCountry>
(
<collectingRegion id="4986F87572436879FEA2D3EEFE2327B6" box="[362,494,1473,1496]" country="China" name="Guangdong" pageId="3" pageNumber="350">Guangdong</collectingRegion>
