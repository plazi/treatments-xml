<bibRefCitation id="EFF34B56FFF11D62FF41FC49FE150A80" author="Chamberlin" box="[158,467,986,1011]" pageId="21" pageNumber="354" refString="Chamberlin, R. V. &amp; Wang, Y. H. M. (1952) Some records and descriptions of chilopods from Japan and other Oriental areas. Proceedings of the Biological Society of Washington, 65, 177 - 187." type="journal article" year="1952">
Chamberlin &amp; Wang, 
<number id="3DAA2480FFF11D62FE48FC49FE150A80" box="[407,467,986,1011]" pageId="21" pageNumber="354" value="1952.0">1952</number>
</bibRefCitation>
); 
<collectingCountry id="F3757637FFF11D62FE38FC49FDFF0A80" box="[487,569,986,1011]" name="Taiwan" pageId="21" pageNumber="354">Taiwan</collectingCountry>
(Hualien) (
<bibRefCitation id="EFF34B56FFF11D62FD65FC48FC800A80" author="Wang" box="[698,838,986,1011]" pageId="21" pageNumber="354" refString="Wang, Y. H. M. (1956) Serica 1 e: Records of myriapods on Formosa with description of new species (2). Quarterly Journal of the Taiwan Museum, 2, 155 - 159." type="journal article" year="1956">
Wang, 
<number id="3DAA2480FFF11D62FCD5FC49FC800A80" box="[778,838,986,1011]" pageId="21" pageNumber="354" value="1956.0">1956</number>
</bibRefCitation>
); 
<collectingCountry id="F3757637FFF11D62FC85FC49FC6A0A80" box="[858,940,986,1011]" name="Taiwan" pageId="21" pageNumber="354">Taiwan</collectingCountry>
(Ta Ping Shan) (
<bibRefCitation id="EFF34B56FFF11D62FBB6FC48FB300A80" author="Wang" box="[1129,1270,986,1011]" pageId="21" pageNumber="354" refString="Wang, Y. H. M. (1963) Serica 1 q: Millipedes and centipedes of Quemoy, Fukien Province and Taiwan Island, Botel Tobago (Lan Yu), Taiwan Province and of Singapore. Quarterly Journal of the Taiwan Museum, (1 - 2), 89 - 96." type="journal article" year="1963">
Wang, 
<number id="3DAA2480FFF11D62FB65FC49FB300A80" box="[1210,1270,986,1011]" pageId="21" pageNumber="354" value="1963.0">1963</number>
