<subSubSection id="267B980DB2AD310135971A0F8ACD10C9" pageId="null" pageNumber="590" type="nomenclature">
<paragraph id="A9983A78F326F5D1A846ECA7D4AD22D6" pageId="null" pageNumber="590">
<taxonomicName id="2334F42A6288D5FC13BD96F471BA5347" authority="Tausch" class="Gymnolaemata" family="Cymuloporidae" genus="Crepis" kingdom="Animalia" order="Cheilostomatida" pageId="null" pageNumber="590" phylum="Bryozoa" rank="species" species="jacquinii">
