<taxonomicName id="4C3E4D5F595EFFFDFED1F8C5300BCBDD" authority="Sanborn, 2019" authorityName="Sanborn" authorityYear="2019" box="[279,513,1902,1925]" class="Insecta" family="Cicadidae" genus="Parnisa" kingdom="Animalia" order="Hemiptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="santacruzensis" status="n. sp.">
<emphasis id="B94AEACE595EFFFDFED1F8C5300BCBDD" box="[279,513,1902,1925]" italics="true" pageId="50" pageNumber="51">Parnisa santacruzensis</emphasis>
</taxonomicName>
<emphasis id="B94AEACE595EFFFDFDC1F8C53034CBDE" bold="true" box="[519,574,1902,1926]" pageId="50" pageNumber="51">
