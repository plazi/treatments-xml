<collectingCountry id="F33C7652BB4E5D0F95780A6FFDB83A3D" box="[527,613,259,285]" name="Mexico" pageId="95" pageNumber="96">Mexico</collectingCountry>
(
<collectingRegion id="49EFF820BB4E5D0F950D0A6FFCE33A3D" box="[634,830,259,285]" country="Mexico" name="San Luis Potosi" pageId="95" pageNumber="96">San Luis Potosí</collectingRegion>
