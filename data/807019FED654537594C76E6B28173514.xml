<taxonomicName id="243EAD16E123ADA23FCCAD71E9B644C9" class="Insecta" family="Dytiscidae" genus="Copelatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Copelatus" order="Coleoptera" pageId="0" pageNumber="73" phylum="Arthropoda" rank="genus">Copelatus</taxonomicName>
