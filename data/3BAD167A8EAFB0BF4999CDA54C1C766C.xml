<geoCoordinate id="BAB37F69893CD70DA84CD004C79472D9" direction="north" orientation="latitude" precision="15" value="15.974166">15°58'27&quot;N</geoCoordinate>
; 
<geoCoordinate id="C09EDCAFBD57958F5C14D7D70C1E914C" direction="east" orientation="longitude" precision="15" value="101.41">101°24'36&quot;E</geoCoordinate>
, altitude: 394 m, collected on 19 September 2016; Khao Maka Cave (NPU 2018-016-017), Sala Lamduan Subdistrict, Muang District, Sa Kaeo Province, 
<geoCoordinate id="C34791FFD648081B4F86DA06D3ECA451" direction="north" orientation="latitude" precision="15" value="13.786112">13°47'10&quot;N</geoCoordinate>
