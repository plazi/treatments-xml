<collectionCode id="03FAAE5559419860D5BA8EC471D99BBC" box="[1184,1275,1508,1534]" country="Austria" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="20" pageNumber="21" type="Museum">NHMW</collectionCode>
