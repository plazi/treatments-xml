<collectionCode id="03FAAE5559419860D4448EC470BE9BBC" box="[1374,1436,1508,1534]" country="Germany" httpUri="http://grbio.org/cool/kdnf-0379" name="Zoologisches Museum Hamburg" pageId="20" pageNumber="21" type="Museum">ZMH</collectionCode>
