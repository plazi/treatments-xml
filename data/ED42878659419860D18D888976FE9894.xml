<taxonomicName id="A2EB4D1359419860D3048E2B760F9B64" authority="Att." authorityName="Attems" authorityYear="1897" box="[542,813,1291,1318]" class="Chilopoda" family="Schendylidae" genus="Schendyla" kingdom="Animalia" order="Geophilomorpha" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="psilopus">
