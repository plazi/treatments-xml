<taxonomicName id="FB0A82F082F0CBA14A30B7C08AE30C78" authority="Mayr, 1861" authorityName="Mayr" authorityYear="1861" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Temnothorax" order="Hymenoptera" pageId="24" pageNumber="51" phylum="Arthropoda" rank="genus">Temnothorax</taxonomicName>
