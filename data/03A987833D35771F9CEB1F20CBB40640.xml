<taxonomicName id="4C004D163D3B771C9D9C1AA7CB3707C9" authority="Ditlevsen, 1918" authorityName="Ditlevsen" authorityYear="1918" box="[480,852,152,175]" class="Adenophorea" family="Comesomatidae" genus="Sabatieria" kingdom="Animalia" order="Desmodorida" pageId="9" pageNumber="206" phylum="Nematoda" rank="species" species="ornata">
