<taxonomicName id="4C5C4D6D46A7C7C4DAC6FC00F79BC265" authority=": HEINRICH 1974" authorityName="HEINRICH" authorityYear="1974" box="[178,459,1020,1040]" class="Insecta" family="Ichneumonidae" genus="Poecilostictus" kingdom="Animalia" order="Hymenoptera" pageId="179" pageNumber="1496" phylum="Arthropoda" rank="genus">
