<paragraph id="3A2BAC1EFFEEFE54E887D5F8FEE1FF39" blockId="2.[151,1436,1976,2037]" lastBlockId="3.[151,1436,151,501]" lastPageId="3" lastPageNumber="583" pageId="2" pageNumber="582">
<emphasis id="08E0700CFFEEFE55E887D5F8FED3F83D" bold="true" box="[151,273,1976,2001]" pageId="2" pageNumber="582">Diagnosis.</emphasis>
<collectionCode id="5C8534DBFFEEFE55E908D5F8FE82F83C" box="[280,320,1976,2000]" country="Germany" httpUri="http://grbio.org/cool/ptpx-c500" name="Senckenbergisches Institut" pageId="2" pageNumber="582">SIF</collectionCode>
