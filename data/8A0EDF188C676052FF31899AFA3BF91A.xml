<paragraph id="02186E0E8C676052FFAD882CFE83F819" blockId="49.[121,749,1799,1929]" pageId="49" pageNumber="182">
<emphasis id="30D3B21C8C676052FFAD882CFE85F88F" bold="true" box="[121,304,1799,1823]" pageId="49" pageNumber="182">Type material</emphasis>
. 
<materialsCitation id="B2CF64538C676052FE918822FE83F819" collectionCode="ZMUC" country="Virgin Islands" latitude="18.333334" location="St Thomas" longLatPrecision="1275" longitude="-64.916664" pageId="49" pageNumber="182" specimenCode="CRU152" specimenCount="1" typeStatus="holotype">
<typeStatus id="DD1CD0AC8C676052FE918822FE1AF88E" box="[325,431,1801,1822]" pageId="49" pageNumber="182">Holotype</typeStatus>
