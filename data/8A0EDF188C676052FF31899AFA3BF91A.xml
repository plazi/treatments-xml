<collectionCode id="64B6F6CB8C676052FDED8822FD36F88E" box="[569,643,1801,1822]" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="49" pageNumber="182">ZMUC</collectionCode>
<specimenCode id="5201C6758C676052FD448822FD58F88E" box="[656,749,1801,1822]" collectionCode="ZMUC" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="49" pageNumber="182">CRU152</specimenCode>
