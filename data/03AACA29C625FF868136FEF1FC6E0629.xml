<taxonomicName id="4C0300BCC625FF868280FE1BFCF904B2" authorityName="Burr" authorityYear="1905" box="[745,854,480,499]" class="Insecta" family="Forficulidae" genus="Anechura" kingdom="Animalia" order="Dermaptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="sokotrana">
