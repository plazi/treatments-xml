Now 
<taxonomicName id="4C34EFF6BA2C9D45FF51FE4FFF75FE07" authority="Swinhoe, 1870" authorityName="Swinhoe" authorityYear="1870" class="Aves" family="Leiothrichidae" genus="Garrulax" kingdom="Animalia" order="Passeriformes" pageId="89" pageNumber="90" phylum="Chordata" rank="subSpecies" species="chinensis" subSpecies="monachus">
