<collectingCountry id="F368764C24643B43FCB9FB15E79A7F70" box="[843,935,1262,1288]" name="Mexico" pageId="5" pageNumber="62">Mexico</collectingCountry>
, 
<collectingCountry id="F368764C24643B43FC41FB15E0297F70" box="[947,1044,1262,1288]" name="Jamaica" pageId="5" pageNumber="62">Jamaica</collectingCountry>
, 
<collectingCountry id="F368764C24643B43FBD2FB15E0B97F70" box="[1056,1156,1262,1288]" name="Trinidad and Tobago" pageId="5" pageNumber="62">Trinidad</collectingCountry>
