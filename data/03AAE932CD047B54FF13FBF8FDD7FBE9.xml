<taxonomicName id="4C0323A7CD077B54FC13FEBDFBB3FEE4" authorityName="Simon" authorityYear="1910" box="[919,1033,261,284]" class="Arachnida" family="Theridiidae" genus="Steassa" kingdom="Animalia" order="Araneae" pageId="30" pageNumber="481" phylum="Arthropoda" rank="species" species="mormorata">
