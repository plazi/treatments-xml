<taxonomicName id="4C433A87FFA28C0F5DA77C4BFDE95616" authority="Sharp, 1874: 42" authorityName="Sharp" authorityPageNumber="42" authorityYear="1874" box="[151,577,1344,1367]" class="Insecta" family="Staphylinidae" genus="Philonthus" kingdom="Animalia" order="Coleoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="rectangulus">
