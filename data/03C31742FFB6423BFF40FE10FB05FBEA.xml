<taxonomicName id="4C6ADDD7FFB6423BFAF4FCCAFEBEFC5E" authority="Wroughton, 1849" authorityName="Wroughton" authorityYear="1849" class="Mammalia" family="Muridae" genus="Rattus" kingdom="Animalia" order="Rodentia" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="eha">
