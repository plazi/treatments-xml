<subSubSection id="0C1DC649AAB6783C8B9B9211E096A5F9" pageId="6" pageNumber="17" type="distribution">
<paragraph id="C0670564E16C274CE3965F437B7C5E8A" pageId="6" pageNumber="17">Distribution and phenology.</paragraph>
<paragraph id="BA1C50672B6BC0C4B008E8F61835D7AE" pageId="6" pageNumber="17">
<taxonomicName id="5E221FDE5F7313FB8891E89EA34D988B" class="Insecta" family="Mycetophilidae" genus="Acomopterella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acomopterella yoshiwae" order="Diptera" pageId="6" pageNumber="17" phylum="Arthropoda" rank="species" species="yoshiwae">Acomopterella yoshiwae</taxonomicName>
