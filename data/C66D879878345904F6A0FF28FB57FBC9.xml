and the 
<collectingRegion id="8C00F86C78345904F2ABFC15FAF5FC59" box="[1180,1325,937,962]" country="Belize" name="Cayo" pageId="12" pageNumber="267">Cayo district</collectingRegion>
in central 
<collectingCountry id="36D3761E78345904F6A0FC72FF07FC7D" box="[151,223,973,998]" name="Belize" pageId="12" pageNumber="267">
<collectingRegion id="8C00F86C78345904F6A0FC72FF07FC7D" box="[151,223,973,998]" country="Belize" name="Belize" pageId="12" pageNumber="267">Belize</collectingRegion>
