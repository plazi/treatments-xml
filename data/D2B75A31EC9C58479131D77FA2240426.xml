<taxonomicName id="6965CAA0D90A5314AC5F69F7059A969F" baseAuthorityName="Fabricius" baseAuthorityYear="1793" class="Arachnida" family="Araneidae" genus="Macracantha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Macracantha arcuata" order="Araneae" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="arcuata">
<emphasis id="F6939D967C6E911D2E033105F4EF190D" italics="true" pageId="0" pageNumber="17">Macracantha arcuata</emphasis>
