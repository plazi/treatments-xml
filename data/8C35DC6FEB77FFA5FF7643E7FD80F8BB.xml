<taxonomicName id="C39C16FAEB77FFA5FC8E4406FC58F9CB" authorityName="C.Linnaeus" authorityYear="1753" box="[879,961,1624,1650]" class="Magnoliopsida" family="Cornaceae" genus="Cornus" kingdom="Plantae" order="Caryophyllales" pageId="27" pageNumber="478" phylum="Tracheophyta" rank="genus">
