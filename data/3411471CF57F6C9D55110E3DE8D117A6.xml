<paragraph id="0BD8FB8B156A9D636A2A8DF0A877F68F" pageId="264" pageNumber="265">
<taxonomicName id="F3E33673D988D559CA9E95EBA9FFDAF5" authorityName="Casey" authorityYear="1924" class="Hexapoda" family="Carabidae" genus="Elaphrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="264" pageNumber="265" phylum="Arthropoda" rank="species" species="rhodeanus">Elaphrus rhodeanus</taxonomicName>
