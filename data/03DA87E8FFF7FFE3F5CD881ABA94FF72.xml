</taxonomicName>
sp. (Angel’s trumpets) (
<bibRefCitation id="EFE24B0FFFCAFFE2F13C899FBE5EF946" author="Giesbert" box="[1126,1295,1736,1760]" pageId="32" pageNumber="133" refString="Giesbert, E. F. (1993) New species of Cerambycidae (Coleoptera: Cerambycidae) from Guatemala and Chiapas, Mexico. Coleopterists Bulletin, 47 (2), 137 - 149." type="journal article" year="1993">
Giesbert 
<number id="3DBB24D9FFCAFFE2F18E899FBE5EF946" box="[1236,1295,1736,1760]" pageId="32" pageNumber="133" value="1993.0">1993</number>
</bibRefCitation>
), 
<taxonomicName id="4C734D7DFFCAFFE2F073899FBBA2F8A3" class="Magnoliopsida" family="Fabaceae" genus="Phaseolus" kingdom="Animalia" order="Fabales" pageId="32" pageNumber="133" phylum="Tracheophyta" rank="species" species="vulgaris">
