<taxonomicName id="4C524D76FF95FFB63946FD46FCAA7F6F" authority="Meigen, 1800" authorityName="Meigen" authorityYear="1800" box="[574,833,762,787]" class="Sordariomycetes" family="Melanconidaceae" genus="Titania" higherTaxonomySource="GBIF" kingdom="Fungi" order="Diaporthales" pageId="53" pageNumber="54" phylum="Ascomycota" rank="genus">
