<heading id="49720913FFE0FFCBFF7121F1A1D3F953" bold="true" box="[151,467,1762,1788]" fontSize="11" level="1" pageId="8" pageNumber="9" reason="1">
<emphasis id="20F1626DFFE0FFCBFF7121F1A1D3F953" bold="true" box="[151,467,1762,1788]" pageId="8" pageNumber="9">
<taxonomicName id="D585C5FCFFE0FFCBFF7121F1A18AF953" box="[151,394,1762,1788]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="hexacantha" status="sp. n.">
<emphasis id="20F1626DFFE0FFCBFF7121F1A18AF953" bold="true" box="[151,394,1762,1788]" italics="true" pageId="8" pageNumber="9">Manota hexacantha</emphasis>
</taxonomicName>
<taxonomicNameLabel id="3BC2DF16FFE0FFCBFE7721F1A1D3F953" box="[401,467,1762,1788]" pageId="8" pageNumber="9" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
