<taxonomicName id="2FF1AE8FE99FF804C7D823A7782A0B0B" class="Liliopsida" family="Liliaceae" genus="Astelia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Astelia nadeaudii" order="Asparagales" pageId="5" pageNumber="110" phylum="Tracheophyta" rank="species" species="nadeaudii">
