<taxonomicName id="73C6A5FA828EE7ABF4AB5733DE9316F4" LSID="http://zoobank.org/DEB6CC36-74FD-42A8-BFEE-8A45EA212429" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus dieciocho" order="Hymenoptera" pageId="339" pageNumber="340" phylum="Arthropoda" rank="species" species="dieciocho">Heterospilus dieciocho Marsh</taxonomicName>
<taxonomicNameLabel id="FA54AD1F571CD5FC186BBC8891D30763" pageId="339" pageNumber="340">sp. n.</taxonomicNameLabel>
Figure 230
