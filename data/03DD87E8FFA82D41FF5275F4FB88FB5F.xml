<taxonomicName id="4C744D7DFFA82D41FE8E75F4FE15FC58" authorityName="Kudon" authorityYear="1982" box="[331,476,957,983]" class="Arachnida" family="Proctophyllodidae" genus="Platyacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="oligolaccius">
<emphasis id="B900EAECFFA82D41FE8E75F4FE15FC58" bold="true" box="[331,476,957,983]" italics="true" pageId="8" pageNumber="9">oligolaccius</emphasis>
