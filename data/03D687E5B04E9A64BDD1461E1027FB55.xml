<taxonomicName id="4C7F4D70B04E9A65B9BF447A1657F98F" box="[1273,1348,1542,1566]" class="Insecta" family="Staphylinidae" genus="Scydmaenilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="69" phylum="Arthropoda" rank="species" species="pusilla">pusilla</taxonomicName>
