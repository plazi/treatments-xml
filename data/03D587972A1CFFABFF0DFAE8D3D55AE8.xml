<taxonomicName id="4C7C4D022A1CFFA8FA12F8C7D3415C20" authorityName="Lindsey" authorityYear="1925" class="Insecta" family="Lepidoptera (awaiting allocation)" genus="Tarmia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="216" phylum="Arthropoda" rank="species" species="monastica">
