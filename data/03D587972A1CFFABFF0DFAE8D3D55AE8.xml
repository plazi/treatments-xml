<quantity id="4C849B642A1CFFA8FE08F918D3A35DE8" box="[402,488,1757,1783]" metricMagnitude="3" metricUnit="m" metricValue="2.1" pageId="1" pageNumber="216" unit="m" value="2100.0">2100 m</quantity>
