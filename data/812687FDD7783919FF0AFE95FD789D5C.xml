<collectingCountry id="7198767BD7783919FCB4FBBDFC769C67" box="[809,949,1100,1124]" name="Afghanistan" pageId="30" pageNumber="31">Afghanistan</collectingCountry>
, 
<collectingCountry id="7198767BD7783919FC22FBBCFBC69C66" box="[959,1029,1101,1125]" name="Egypt" pageId="30" pageNumber="31">Egypt</collectingCountry>
, 
<collectingCountry id="7198767BD7783919FB8DFBBDFBAE9C67" box="[1040,1133,1100,1124]" name="Georgia" pageId="30" pageNumber="31">Georgia</collectingCountry>
, 
<collectingCountry id="7198767BD7783919FBE5FBBCFB6B9C67" box="[1144,1192,1101,1124]" name="Iran" pageId="30" pageNumber="31">Iran</collectingCountry>
, 
<collectingCountry id="7198767BD7783919FB2EFBBDFB379C67" box="[1203,1268,1100,1124]" name="Israel" pageId="30" pageNumber="31">Israel</collectingCountry>
