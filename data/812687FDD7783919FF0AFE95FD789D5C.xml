<collectingCountry id="7198767BD7783919FF0AFB9EFEF69C84" box="[151,309,1135,1159]" name="Turkmenistan" pageId="30" pageNumber="31">Turkmenistan</collectingCountry>
. In 
<collectingCountry id="7198767BD7783919FEFDFB81FE539C84" box="[352,400,1136,1159]" name="Iran" pageId="30" pageNumber="31">Iran</collectingCountry>
, 
<taxonomicName id="CE8F4D68D7783919FE01FB80FDDD9C8B" box="[412,542,1136,1160]" class="Insecta" family="Eriococcidae" genus="Ripersia" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="sacchari">
