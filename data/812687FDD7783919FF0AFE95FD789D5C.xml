<taxonomicName id="CE8F4D68D7783919FC6DFB9EFBB09C84" box="[1008,1139,1135,1159]" class="Liliopsida" family="Cyperaceae" kingdom="Plantae" order="Poales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="family">Cyperaceae</taxonomicName>
