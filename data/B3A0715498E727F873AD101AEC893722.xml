<taxonomicName id="C2D05AA1658B114C33A5DB1EEFEA3A90" class="Insecta" family="Pseudococcidae" genus="Pseudorhodania" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudorhodania marginata" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="marginata">Pseudorhodania marginata</taxonomicName>
