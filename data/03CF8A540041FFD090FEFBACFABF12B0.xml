Brevignon 
<number id="3DAE29650041FFD19111FA79FE721535" box="[376,436,1514,1538]" pageId="14" pageNumber="15" value="2002.0">2002</number>
</bibRefCitation>
), 
<collectingCountry id="F3717BD20041FFD191A1FA79FDFD1535" box="[456,571,1514,1538]" name="Venezuela" pageId="14" pageNumber="15">Venezuela</collectingCountry>
