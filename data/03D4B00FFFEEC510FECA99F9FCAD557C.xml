<paragraph id="8BC20119FFEEC50EFECA99F9FD9E50BC" blockId="47.[264,628,1724,1790]" box="[264,628,1724,1750]" pageId="47" pageNumber="48">
<heading id="D08AB675FFEEC50EFECA99F9FD9E50BC" bold="true" box="[264,628,1724,1750]" fontSize="11" level="1" pageId="47" pageNumber="48" reason="1">
