<taxonomicName id="A51F55D738902C1D0B1F55707CA1D691" authority="Spach, 1839" authorityName="Spach" authorityYear="1839" class="Magnoliopsida" family="Ranunculaceae" genus="Aconitella" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Aconitella teheranica" order="Ranunculales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="species" species="teheranica">≡ Aconitella teheranica</taxonomicName>
