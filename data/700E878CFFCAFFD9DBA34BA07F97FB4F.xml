<materialsCitation id="48CF3CC7FFCAFFD9D90A4E747B2DFBB7" box="[545,1296,1088,1115]" collectingDate="1974-05-02" country="China" county="Luding County" location="Dadu" pageId="4" pageNumber="222" specimenCount="1" stateProvince="Sichuan">
