, south to 
<collectingRegion id="0811B388FFC0FF9B02261FC7FC6786B3" box="[772,948,1981,2007]" country="United States of America" name="North Carolina" pageId="73" pageNumber="74">North Carolina</collectingRegion>
(
<figureCitation id="52EE61EFFFC0FF9B02E61FC7FBCF86B3" box="[964,1052,1981,2007]" captionStart="FIGURE 75" captionStartId="72.[151,255,1519,1543]" captionTargetBox="[172,1365,397,1493]" captionTargetId="figure-201@72.[172,1415,397,1493]" captionTargetPageId="72" captionText="FIGURE 75. Distribution of Lasioglossum asteris (dark grey) and L. atwoodi (light grey)." pageId="73" pageNumber="74">Fig. 75</figureCitation>
).
</paragraph>
<caption id="9EAA2DE2FFC3FF9801B51DA8FD0B8768" pageId="74" pageNumber="75" startId="74.[151,255,1490,1514]" targetBox="[169,1436,183,1463]" targetPageId="74">
