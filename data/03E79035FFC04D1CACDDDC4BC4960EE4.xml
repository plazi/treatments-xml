<taxonomicName id="4C4E5AA0FFC04D1EAD63DA01C717083C" authority="Bolivar, 1887" authorityName="Bolivar" authorityYear="1887" box="[297,589,1850,1875]" class="Insecta" family="Tetrigidae" genus="Scaria" kingdom="Animalia" order="Orthoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="lineata">
