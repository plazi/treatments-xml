<taxonomicName id="4C2AD9B17C5CFFD8FE79FE583F4CE0C2" authority="Puthz, 2012" authorityName="Puthz" authorityYear="2012" box="[458,588,443,464]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="1438" phylum="Arthropoda" rank="species" species="perpauper" status="nov.sp.">
