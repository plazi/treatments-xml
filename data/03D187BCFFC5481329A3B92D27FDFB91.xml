and 
<taxonomicName id="4C784D29FFC448132B90BFC2261EFC5B" authority="(Jedlicka)" authorityName="Liang &amp; Kavanaugh" authorityYear="2007" baseAuthorityName="Jedlicka" baseAuthorityYear="1960" class="Insecta" family="Carabidae" genus="Dendrocellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" phylum="Arthropoda" rank="species" species="kulti">
