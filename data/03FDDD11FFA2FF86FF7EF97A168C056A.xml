<quantity id="4CACC1E2FF9CFF86FC40FEBC13430617" box="[937,1053,268,294]" metricMagnitude="-2" metricUnit="m" metricValue="1.071" pageId="32" pageNumber="33" unit="mm" value="10.71">10.71mm</quantity>
