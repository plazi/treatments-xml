<quantity id="4CACC1E2FF9CFF86FB2EFEBC12650617" box="[1223,1339,268,294]" metricMagnitude="-2" metricUnit="m" metricValue="1.014" pageId="32" pageNumber="33" unit="mm" value="10.14">10.14mm</quantity>
