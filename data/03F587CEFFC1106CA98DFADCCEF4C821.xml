<taxonomicName id="4C5C4D5BFFC1106CAAA8F9E6C97FC805" authorityName="Thompson" authorityYear="1913" box="[946,1025,1578,1603]" class="Reptilia" family="Natricidae" genus="Hebius" higherTaxonomySource="CoL" kingdom="Animalia" order="Squamata" pageId="12" pageNumber="437" phylum="Chordata" rank="genus">
