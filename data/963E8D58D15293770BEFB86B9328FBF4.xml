<taxonomicName id="D99747CDD15193770ADEBDCB9271FD34" authorityName="Erichson" authorityYear="1843" box="[497,606,707,731]" class="Insecta" family="Nitidulidae" genus="Stelidota" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="37" phylum="Arthropoda" rank="genus">
