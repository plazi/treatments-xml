<taxonomicName id="A28B967A9A21E3BFB417F114432D8E25" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145352" authority="Roger, 1863" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sima grandidieri Forel" order="Hymenoptera" pageNumber="203" phylum="Arthropoda" rank="species" species="grandidieri">Sima Grandidieri</taxonomicName>
