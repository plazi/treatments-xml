<taxonomicName id="E21ED59FB68F196C08C465C6500D6DEB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:190980" class="Insecta" family="Scelionidae" genus="Archaeoteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Archaeoteleia chambersi Early" order="Hymenoptera" pageNumber="8" phylum="Arthropoda" rank="species" species="chambersi">Archaeoteleia chambersi Early</taxonomicName>
