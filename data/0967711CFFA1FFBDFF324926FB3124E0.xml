<taxonomicName id="46CEBB89FFA1FFBDFDDE48A6FD512484" box="[635,766,930,955]" class="Magnoliopsida" family="Campanulaceae" genus="Campanula" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="416" phylum="Tracheophyta" rank="genus">
