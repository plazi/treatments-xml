<taxonomicName id="46CEBB89FFA1FFBDFF4A48C3FDAB24E0" authority="Muller, 2012" authorityName="Muller" authorityYear="2012" box="[239,516,966,991]" class="Magnoliopsida" family="Campanulaceae" genus="Legousia" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="416" phylum="Tracheophyta" rank="genus">
