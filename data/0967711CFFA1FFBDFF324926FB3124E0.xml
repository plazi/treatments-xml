</taxonomicName>
(
<taxonomicName id="46CEBB89FFA1FFBDFCAA48A6FC6B2484" box="[783,964,930,955]" class="Magnoliopsida" family="Campanulaceae" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="416" phylum="Tracheophyta" rank="family">Campanulaceae</taxonomicName>
