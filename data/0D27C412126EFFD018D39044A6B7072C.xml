<taxonomicName id="428E0E87126DFFD01A60968BA75C06E4" box="[548,631,188,213]" class="Insecta" family="Curculionidae" genus="Acalles" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="genus" sensu="stricto">
