<taxonomicName id="428E0E87126DFFD01CAB96A0A0B00681" box="[1263,1435,151,176]" class="Insecta" family="Curculionidae" genus="Acalles" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="sierrae">
