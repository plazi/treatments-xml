<taxonomicName id="4C1F4D3C7365AB20FE03FECAD53C9763" authority="Udalzow, 1907" authorityName="Udalzow" authorityYear="1907" box="[410,545,345,371]" class="Arachnida" family="Hydryphantidae" genus="Hydryphantes" kingdom="Animalia" order="Trombidiformes" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="peroviensis">peroviensis</taxonomicName>
</emphasis>
Udalzow, 
<number id="3DD724987365AB20FD3BFECBD5C09762" box="[674,733,344,370]" pageId="13" pageNumber="14" value="1907.0">1907</number>
</emphasis>
