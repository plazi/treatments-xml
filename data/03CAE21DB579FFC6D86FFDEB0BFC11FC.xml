<taxonomicName id="4C632888B579FFC6D86FFDEB0BC7104D" authority="(Verrill, 1872) Siddall et al., 2005" authorityName="Siddall et al." authorityYear="2005" baseAuthorityName="Verrill" baseAuthorityYear="1872" box="[336,959,536,563]" class="Clitellata" family="Glossiphoniidae" genus="Helobdella" kingdom="Animalia" order="Rhynchobdellida" pageId="9" pageNumber="10" phylum="Annelida" rank="species" species="modesta">
