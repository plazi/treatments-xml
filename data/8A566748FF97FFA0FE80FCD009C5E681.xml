<paragraph id="0240D65EFF97FFA0FF3DF8EB0942E365" blockId="17.[144,759,1819,1903]" lastBlockId="17.[806,1421,198,311]" pageId="17" pageNumber="147">
<taxonomicName id="C5FFADDDFF97FFA0FF3DF8EB0D1FE502" authority="(NCN)" baseAuthorityName="NCN" box="[144,340,1850,1872]" class="Amphibia" family="Ceratobatrachidae" genus="Lupacolus" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="genus">
