<taxonomicName id="C5FFADDDFF97FFA0FAB9FF1709C6E289" authorityName="Dumeril" authorityYear="1853" box="[1300,1421,198,219]" class="Amphibia" family="Ceratobatrachidae" genus="Cornufer" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="species" species="dorsalis">
<emphasis id="308B0A4CFF97FFA0FAB9FF1709C6E289" box="[1300,1421,198,219]" italics="true" pageId="17" pageNumber="147">Pl. dorsalis</emphasis>
</taxonomicName>
and all extant species that share a more recent common ancestor with 
<taxonomicName id="C5FFADDDFF97FFA0FC7EFED2081DE34A" authorityName="Dumeril" authorityYear="1853" box="[979,1110,259,280]" class="Amphibia" family="Ceratobatrachidae" genus="Cornufer" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="species" species="dorsalis">
<emphasis id="308B0A4CFF97FFA0FC7EFED2081DE34A" box="[979,1110,259,280]" italics="true" pageId="17" pageNumber="147">Pl. dorsalis</emphasis>
</taxonomicName>
than with 
<taxonomicName id="C5FFADDDFF97FFA0FB4EFED209CDE34B" baseAuthorityName="Dumeril" baseAuthorityYear="1853" box="[1251,1414,259,281]" class="Amphibia" family="Ceratobatrachidae" genus="Platymantis" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="species" species="corrugatus">
<emphasis id="308B0A4CFF97FFA0FB4EFED209CDE34B" box="[1251,1414,259,281]" italics="true" pageId="17" pageNumber="147">Pl. corrugatus</emphasis>
</taxonomicName>
, 
<taxonomicName id="C5FFADDDFF97FFA0FC8BFEF0080FE364" box="[806,1092,289,311]" class="Amphibia" family="Ceratobatrachidae" genus="Platymantis" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="variety" species="hazelae" variety="guentheri">
<emphasis id="308B0A4CFF97FFA0FC8BFEF00FE9E364" box="[806,930,289,310]" italics="true" pageId="17" pageNumber="147">Pl. hazelae</emphasis>
, 
<emphasis id="308B0A4CFF97FFA0FC1FFEF0080FE364" box="[946,1092,289,310]" italics="true" pageId="17" pageNumber="147">Pl. guentheri</emphasis>
</taxonomicName>
, or 
<taxonomicName id="C5FFADDDFF97FFA0FBDBFEF00949E364" authorityName="Brown &amp; Alcala" authorityYear="1974" box="[1142,1282,289,310]" class="Amphibia" family="Ceratobatrachidae" genus="Platymantis" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="species" species="levigatus">
<emphasis id="308B0A4CFF97FFA0FBDBFEF00949E364" box="[1142,1282,289,310]" italics="true" pageId="17" pageNumber="147">Pl. levigatus</emphasis>
</taxonomicName>
.
</paragraph>
