<taxonomicName id="C5FFADDDFF97FFA0FC59FE6D0823E383" authorityName="Brown, Alcala &amp; Diemsos" authorityYear="1999" box="[1012,1128,444,465]" class="Amphibia" family="Ceratobatrachidae" genus="Platymantis" kingdom="Animalia" order="Anura" pageId="17" pageNumber="147" phylum="Chordata" rank="species" species="taylori">
