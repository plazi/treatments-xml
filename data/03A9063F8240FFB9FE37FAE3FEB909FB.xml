<subSubSection id="C31AE4A28240FFBAFF25FABCFE1A0EF7" pageId="1" pageNumber="535" type="etymology">
<paragraph id="8BBFB7298240FFBAFF25FABCFE1A0EF7" blockId="1.[175,1004,1360,1435]" pageId="1" pageNumber="535">
<emphasis id="B9746B3B8240FFBAFF25FABCFE860E12" bold="true" box="[199,315,1360,1381]" pageId="1" pageNumber="535">Etymology.</emphasis>
