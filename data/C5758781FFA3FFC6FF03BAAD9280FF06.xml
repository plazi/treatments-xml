<taxonomicName id="8ADC4D14FFA2FFC6FA10BF9B924BFF09" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="dispunctella">
<emphasis id="7FA8EA85FFA2FFC6FA10BF9B924BFF09" italics="true" pageId="17" pageNumber="318">E. dispunctella</emphasis>
</taxonomicName>
, not 
<taxonomicName id="8ADC4D14FFA2FFC6FECCBFC7928EFF09" box="[344,485,224,248]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="grandella">
