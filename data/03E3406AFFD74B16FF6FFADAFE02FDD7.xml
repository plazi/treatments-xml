<number id="3D82E35BFFD44B10FCA3F9BFFC4CF9CA" box="[859,960,1580,1603]" pageId="26" pageNumber="27" value="2043283.0">2043283</number>
). 
<number id="3D82E35BFFD44B10FC2AF9BFFC7CF9CA" box="[978,1008,1580,1603]" pageId="26" pageNumber="27" value="65.0">65</number>
