<taxonomicName id="4C10A3C5FFB7D95D5AAEFF17BF8841E0" authority="Ishihara, 1965" authorityName="Ishihara" authorityYear="1965" box="[151,464,212,236]" class="Insecta" family="Cicadellidae" genus="Formotettigella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="436" phylum="Arthropoda" rank="genus">
