<paragraph id="073F0D20FE54DE98B98FC05E0D49BE9A" pageId="17" pageNumber="383">
<taxonomicName id="1957BA1C9AB20CCAC11B3A8F860CE6D6" class="Arachnida" family="Carabodidae" genus="Carabodes" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="17" pageNumber="383" phylum="Arthropoda" rank="species" species="forsslundi">Carabodes forsslundi Sellnick</taxonomicName>
