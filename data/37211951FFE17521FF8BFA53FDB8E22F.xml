<subSubSection id="F792FBCCFFE17521FF6DF95BFDB8E22F" pageId="17" pageNumber="104" type="discussion">
<paragraph id="BF37A847FFE17521FF6DF95BFDB8E22F" blockId="17.[113,764,1664,1849]" pageId="17" pageNumber="104">
<emphasis id="8DFC7455FFE17521FF6DF95BFF58E38E" box="[151,208,1665,1688]" italics="true" pageId="17" pageNumber="104">Notes</emphasis>
