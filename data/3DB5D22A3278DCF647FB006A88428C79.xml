<paragraph id="64093D9A3419CD8DEC4B88E33B8B4D9E" pageId="null" pageNumber="392">
<pageBreakToken id="60F9FE7FA5628096ABF6701B32A3B94A" pageId="null" pageNumber="392" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="09CF96FAE8BC841837872B3CEACAE476" authority="L." class="Magnoliopsida" family="Campanulaceae" genus="Campanula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="392" phylum="Tracheophyta" rank="species" species="rotundifolia">
Campanula 
<normalizedToken id="E2E12A5BB8D566D6F395F93A5DF4DE15" originalValue="rotundifólia" pageId="null" pageNumber="392">rotundifolia</normalizedToken>
<authorityName id="C75CC54FD48376EDF2B08CA8711444F0" pageId="null" pageNumber="392">L.</authorityName>
</taxonomicName>
</paragraph>
