, 
<taxonomicName id="E28F8FA8FF96FF8AFED00FB2FE2CDD5F" authority="(Gagarin &amp; Nguyen Vu Thanh, 2007)" authorityName="Holovachov &amp; Boström" authorityYear="2013" baseAuthorityName="Gagarin &amp; Nguyen Vu Thanh" baseAuthorityYear="2007" box="[265,790,764,789]" class="Adenophorea" family="Leptolaimidae" genus="Leptolaimus" kingdom="Animalia" order="Araeolaimida" pageId="48" pageNumber="49" phylum="Nematoda" rank="species" species="rivalis" status="comb. n.">
<emphasis id="17FB2839FF96FF8AFED00FB2FC54DD5E" box="[265,366,764,788]" italics="true" pageId="48" pageNumber="49">L. rivalis</emphasis>
(Gagarin &amp; Nguyen Vu Thanh, 2007)
</taxonomicName>
<emphasis id="17FB2839FF96FF8AFCC50FB3FEB8DD5F" bold="true" box="[796,898,764,789]" pageId="48" pageNumber="49">
<taxonomicNameLabel id="0CC89542FF96FF8AFCC50FB3FEB8DD5F" box="[796,898,764,789]" pageId="48" pageNumber="49" rank="species">comb. n.</taxonomicNameLabel>
