<taxonomicName id="E28F8FA8FF96FF8AFDF80EFEFF95DD82" authorityName="Holovachov &amp; Boström" authorityYear="2013" baseAuthorityName="Urkmez &amp; Brennan" baseAuthorityYear="2013" box="[545,687,945,968]" class="Adenophorea" family="Leptolaimidae" genus="Halaphanolaimus" kingdom="Animalia" order="Araeolaimida" pageId="48" pageNumber="49" phylum="Nematoda" rank="species" species="sergeevae">
