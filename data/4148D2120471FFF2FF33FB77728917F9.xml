<geoCoordinate id="ACD505C30471FFF2FD3CF88B753A1756" box="[664,808,1862,1889]" degrees="28" direction="west" minutes="07" orientation="longitude" pageId="33" pageNumber="34" precision="15" seconds="45" value="-28.129168">28°07’45”W</geoCoordinate>
, 
<quantity id="0E19CEE10471FFF2FC96F88B75821755" box="[818,912,1862,1890]" metricMagnitude="0" metricUnit="m" metricValue="1.095" pageId="33" pageNumber="34" unit="m" value="1.095">1,095 m</quantity>
