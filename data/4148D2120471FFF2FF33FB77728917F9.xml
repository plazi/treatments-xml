<collectionCode id="AFF0FBC10471FFF2FD23F87E74BE17FA" box="[647,684,1971,1997]" country="Czech Republic" name="Západoceské muzeum v Plzni" pageId="33" pageNumber="34" type="Museum">PL</collectionCode>
181–3: 
<geoCoordinate id="ACD505C30471FFF2FD59F87E759A17FA" box="[765,904,1971,1997]" degrees="36" direction="north" minutes="33" orientation="latitude" pageId="33" pageNumber="34" precision="15" seconds="38" value="36.560555">36°33’38”N</geoCoordinate>
, 
<geoCoordinate id="ACD505C30471FFF2FC30F87E723117FA" box="[916,1059,1971,1997]" degrees="33" direction="west" minutes="24" orientation="longitude" pageId="33" pageNumber="34" precision="15" seconds="49" value="-33.413612">33°24’49”W</geoCoordinate>
, 
<quantity id="0E19CEE10471FFF2FB8BF87E729D17F9" box="[1071,1167,1971,1998]" metricMagnitude="0" metricUnit="m" metricValue="2.2" pageId="33" pageNumber="34" unit="m" value="2.2">2,200 m</quantity>
).
