<emphasis id="FB95BF160471FFF2FE94F8E977DA170A" box="[304,456,1827,1854]" italics="true" pageId="33" pageNumber="34">O. crassidens</emphasis>
</taxonomicName>
from the 
<collectingRegion id="0B25ADE60471FFF2FD91F8EE7495170A" box="[565,647,1827,1853]" country="Portugal" name="Acores" pageId="33" pageNumber="34">Azores</collectingRegion>
, based on material collected by 
<emphasis id="FB95BF160471FFF2FC4BF8E9728F170A" box="[1007,1181,1827,1853]" italics="true" pageId="33" pageNumber="34">Princesse Alice</emphasis>
(sta 703: 
<geoCoordinate id="ACD505C30471FFF2FAA8F8EE7385170A" box="[1292,1431,1827,1853]" degrees="39" direction="north" minutes="21" orientation="latitude" pageId="33" pageNumber="34" precision="15" seconds="20" value="39.355553">39°21’20”N</geoCoordinate>
, 
<geoCoordinate id="ACD505C30471FFF2FF33F88B77341756" box="[151,294,1862,1889]" degrees="31" direction="west" minutes="05" orientation="longitude" pageId="33" pageNumber="34" precision="15" seconds="45" value="-31.095835">31°05’45”W</geoCoordinate>
, 
<quantity id="0E19CEE10471FFF2FE95F88A779C1755" box="[305,398,1863,1890]" metricMagnitude="0" metricUnit="m" metricValue="1.36" pageId="33" pageNumber="34" unit="m" value="1.36">1,360 m</quantity>
; sta 1344: 
<geoCoordinate id="ACD505C30471FFF2FDA7F88B749C1756" box="[515,654,1862,1889]" degrees="38" direction="north" minutes="45" orientation="latitude" pageId="33" pageNumber="34" precision="15" seconds="30" value="38.758335">38°45’30”N</geoCoordinate>
