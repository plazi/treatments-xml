<taxonomicName id="0EE118870471FFF2FD70F8A1757F17B2" authorityName="Verrill" authorityYear="1885" box="[724,877,1899,1926]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="33" pageNumber="34" phylum="Echinodermata" rank="species" species="crassidens">
<emphasis id="FB95BF160471FFF2FD70F8A1757F17B2" box="[724,877,1899,1926]" italics="true" pageId="33" pageNumber="34">O. crassidens</emphasis>
</taxonomicName>
