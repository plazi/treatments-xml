<collectingCountry id="F31F76355764FFAD6A9F7BD5FEB4E300" box="[207,286,1008,1028]" name="Morocco" pageId="22" pageNumber="1205">Morocco</collectingCountry>
(Spanish) = (2) / 
<collectingCountry id="F31F76355764FFAD6BE17BD5FE45E300" box="[433,495,1008,1028]" name="Spain" pageId="22" pageNumber="1205">Melilla</collectingCountry>
<date id="FFB610655764FFAD6BA37BD5FDE6E300" box="[499,588,1008,1028]" bridgedPair="-" pageId="22" pageNumber="1205" value="1909-12">
<emphasis id="B97CEAB75764FFAD6BA37BD5FDA5E300" box="[499,527,1008,1028]" italics="true" pageId="22" pageNumber="1205">XII</emphasis>
