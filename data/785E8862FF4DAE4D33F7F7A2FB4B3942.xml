<quantity id="370F9491FF4CAE4D3021F7A7FD143F93" box="[611,716,727,751]" metricMagnitude="-4" metricUnit="m" metricValue="9.5" pageId="21" pageNumber="1099" unit="mm" value="0.95">0.95 mm</quantity>
