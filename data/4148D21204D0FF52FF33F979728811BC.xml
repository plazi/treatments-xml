<quantity id="0E19CEE104D1FF52FE96FEBC779D11BC" box="[306,399,369,395]" metricMagnitude="0" metricUnit="m" metricValue="1.022" pageId="129" pageNumber="130" unit="m" value="1.022">1,022 m</quantity>
