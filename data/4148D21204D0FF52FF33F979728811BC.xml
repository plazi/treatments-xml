<geoCoordinate id="ACD505C304D1FF52FAA8FE80738A1150" box="[1292,1432,333,359]" degrees="38" direction="north" minutes="47" orientation="latitude" pageId="129" pageNumber="130" precision="15" seconds="40" value="38.794445">38°47’40”N</geoCoordinate>
, 
<geoCoordinate id="ACD505C304D1FF52FF33FEBD773511BC" box="[151,295,368,395]" degrees="28" direction="west" minutes="17" orientation="longitude" pageId="129" pageNumber="130" precision="15" seconds="05" value="-28.284721">28°17’05”W</geoCoordinate>
