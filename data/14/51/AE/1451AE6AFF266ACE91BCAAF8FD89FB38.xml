<taxonomicName box="[1289,1436,729,755]" class="Insecta" family="Formicidae" genus="Lasius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="195" pageNumber="196" phylum="Arthropoda" rank="species" species="flavus">
