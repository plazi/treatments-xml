<taxonomicName baseAuthorityName="Halbert" baseAuthorityYear="1915" box="[407,553,954,980]" class="Insecta" family="Formicidae" genus="Lasius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="flavus">
