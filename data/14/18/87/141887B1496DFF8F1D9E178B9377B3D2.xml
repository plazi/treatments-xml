<typeStatus box="[689,735,658,682]" pageId="3" pageNumber="100">type</typeStatus>
locality “Luodian”, 
<collectingRegion box="[954,1049,658,682]" country="China" name="Guizhou" pageId="3" pageNumber="100">Guizhou</collectingRegion>
, 
<collectingCountry box="[1059,1126,658,682]" name="China" pageId="3" pageNumber="100">China</collectingCountry>
.
</paragraph>
