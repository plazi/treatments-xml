<emphasis bold="true" box="[151,281,1485,1510]" pageId="45" pageNumber="213">FIGURE 43</emphasis>
. 
<taxonomicName authority="Khaustov &amp; Frolov, 2019" authorityName="Khaustov &amp; Frolov" authorityYear="2019" box="[294,526,1486,1510]" class="Arachnida" family="Athyreacaridae" genus="Athyreacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="45" pageNumber="213" phylum="Arthropoda" rank="species" species="angustus" status="sp. nov.">
<emphasis box="[294,526,1486,1510]" italics="true" pageId="45" pageNumber="213">Athyreacarus angustus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[532,613,1486,1510]" pageId="45" pageNumber="213" rank="species">
<emphasis bold="true" box="[532,613,1486,1510]" pageId="45" pageNumber="213">sp. nov.</emphasis>
