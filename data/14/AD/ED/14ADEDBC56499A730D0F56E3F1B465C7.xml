<taxonomicName class="Magnoliopsida" family="Sapindaceae" genus="Acer" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acer" order="Sapindales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">
