<taxonomicName authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[151,595,431,457]" class="Insecta" family="Nymphalidae" genus="Brassolis" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="54" phylum="Arthropoda" rank="species" species="sophorae">
