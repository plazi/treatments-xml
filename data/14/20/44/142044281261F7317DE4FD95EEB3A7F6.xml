<typeStatus box="[685,733,819,843]" pageId="1" pageNumber="257">type</typeStatus>
locality, Moreton Bay, 
<collectingCountry box="[997,1103,818,843]" name="Australia" pageId="1" pageNumber="257">Australia</collectingCountry>
