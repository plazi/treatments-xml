<subSubSection box="[199,1108,818,843]" pageId="1" pageNumber="257" type="etymology">
<paragraph blockId="1.[151,1437,674,2031]" box="[199,1108,818,843]" pageId="1" pageNumber="257">
<emphasis bold="true" box="[199,334,818,843]" pageId="1" pageNumber="257">Etymology.</emphasis>
