<taxonomicName authorityName="Wang &amp; Zhang" authorityYear="2014" box="[678,823,223,248]" class="Insecta" family="Cicadellidae" genus="Concavocorona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="supercilia">
