</taxonomicName>
and 
<taxonomicName authority="Lee &amp; Kim &amp; Kim, 2020" authorityName="Lee &amp; Kim &amp; Kim" authorityYear="2020" box="[1208,1323,1271,1294]" class="Maxillopoda" family="Dirivultidae" genus="Stygiopontius" kingdom="Animalia" order="Siphonostomatoida" pageId="30" pageNumber="331" phylum="Arthropoda" rank="species" species="geminus" status="n. sp.">
