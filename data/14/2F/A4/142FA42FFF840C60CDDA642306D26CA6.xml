<emphasis bold="true" box="[151,516,610,636]" pageId="6" pageNumber="7">
<taxonomicName box="[151,357,610,636]" class="Insecta" family="Psychomyiidae" genus="Tinodes" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="gapbona" status="new species">
<emphasis bold="true" box="[151,357,610,636]" italics="true" pageId="6" pageNumber="7">Tinodes gapbona</emphasis>
