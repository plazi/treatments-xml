<paragraph pageId="27" pageNumber="64">
<taxonomicName class="Insecta" family="Hydroptilidae" genus="Oxyethira" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oxyethira scutica" order="Trichoptera" pageId="27" pageNumber="64" phylum="Arthropoda" rank="species" species="scutica">
