<subSubSection pageId="12" type="description">
<paragraph blockId="12.[677,1210,944,1048]" pageId="12">
<taxonomicName box="[677,900,944,968]" class="Cephalopoda" family="Aspidoceratidae" genus="Aspidoceras" kingdom="Animalia" order="Ammonoidea" pageId="12" phylum="Mollusca" rank="species" species="longispinum">
