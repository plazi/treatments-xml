<taxonomicName authority="King" authorityName="King" box="[690,1000,486,508]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Dipterocarpus" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="kunstleri">
<emphasis box="[690,931,487,508]" italics="true" pageId="17">Dipterocarpus kunstleri</emphasis>
King
</taxonomicName>
, 
<taxonomicName authority="Dyer" authorityName="Dyer" box="[175,502,513,535]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Dryobalanops" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="oblongifolia">
<emphasis box="[175,439,513,534]" italics="true" pageId="17">Dryobalanops oblongifolia</emphasis>
Dyer
</taxonomicName>
, 
<taxonomicName authority="Lanessan" authorityName="Lanessan" box="[513,752,513,535]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Hopea" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="ferrea">
<emphasis box="[513,646,513,534]" italics="true" pageId="17">Hopea ferrea</emphasis>
Lanessan
</taxonomicName>
, 
<taxonomicName authority="(de Vriese) P. S. Ashton" authorityName="P. S. Ashton" baseAuthorityName="de Vriese" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="macrophylla">
<emphasis box="[764,965,513,534]" italics="true" pageId="17">Shorea macrophylla</emphasis>
(de Vriese) P. S. Ashton
</taxonomicName>
(as 
<taxonomicName box="[432,585,540,561]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="gysbertsiana">
<emphasis box="[432,585,540,561]" italics="true" pageId="17">S. gysbertsiana</emphasis>
</taxonomicName>
), 
<taxonomicName authority="Dyer ex Brandis" authorityName="Brandis" box="[608,872,540,562]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="ovata">
<emphasis box="[608,691,540,561]" italics="true" pageId="17">S. ovata</emphasis>
Dyer 
<emphasis box="[758,782,540,561]" italics="true" pageId="17">ex</emphasis>
Brandis
</taxonomicName>
, 
<taxonomicName box="[884,1005,540,562]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[884,954,540,561]" italics="true" pageId="17">Shorea</emphasis>
spp.
</taxonomicName>
(all 
<taxonomicName box="[218,399,566,588]" class="Magnoliopsida" family="Dipterocarpaceae" kingdom="Plantae" order="Malvales" pageId="17" phylum="Tracheophyta" rank="family">Dipterocarpaceae</taxonomicName>
) (
<bibRefCitation author="Browne" box="[423,563,566,588]" pageId="17" pagination="1 - 255" refId="ref12791" refString="Browne, F. G. 1961. The biology of Malayan Scolytidae and Platypodidae. Malayan Forest Records 22: 1 - 255." type="journal article" year="1961">Browne 1961</bibRefCitation>
; 
<bibRefCitation author="Schedl" box="[577,705,566,588]" pageId="17" pagination="241 - 254" refId="ref13749" refString="Schedl, K. E. 1964. Scolytoidea from Borneo III. Reichenbachia 4: 241 - 254." type="journal article" year="1964">Schedl 1964</bibRefCitation>
; Beaver &amp; Browne 1979). It usually attacks small stems of 
