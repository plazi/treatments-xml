<subSubSection id="C33A6526FFA40C765DB2DD10FD57F8F1" pageId="2" pageNumber="299" type="materials_examined">
<paragraph id="8B9F36ADFFA40C765DB2DD10FD57F8F1" blockId="2.[151,1436,1851,2012]" pageId="2" pageNumber="299">
<emphasis id="B954EABFFFA40C765DB2DD10FEC8F8D7" bold="true" box="[199,370,1851,1874]" pageId="2" pageNumber="299">
<typeStatus id="549B880FFFA40C765DB2DD10FF45F8D7" box="[199,255,1851,1874]" pageId="2" pageNumber="299">Type</typeStatus>
material.
</emphasis>
<typeStatus id="549B880FFFA40C765C0BDD10FE5CF8D7" box="[382,486,1851,1874]" pageId="2" pageNumber="299">Holotype</typeStatus>
: male, 
<collectingCountry id="F337763DFFA40C765F49DD10FD3AF8D7" box="[572,640,1851,1874]" name="China" pageId="2" pageNumber="299">China</collectingCountry>
: Sichuan Province, Wolongshan, 
<quantity id="4CD89B48FFA40C765E8EDD10FBE0F8D7" box="[1019,1114,1851,1874]" metricMagnitude="3" metricUnit="m" metricValue="2.1" pageId="2" pageNumber="299" unit="m" value="2100.0">2,100 m</quantity>
, VIII. 
<number id="3DE8248AFFA40C7659EBDD10FB42F8D7" box="[1182,1272,1851,1874]" pageId="2" pageNumber="299" value="16.1994">16.1994</number>
