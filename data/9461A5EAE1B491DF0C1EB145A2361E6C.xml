<location id="AEDF35CA878932C7E23AF4F6FDB38F80" LSID="urn:lsid:plazi:treatment:9461A5EAE1B491DF0C1EB145A2361E6C:AEDF35CA878932C7E23AF4F6FDB38F80" country="Germany" county="Upper Palatinate" latitude="49.64907" longitude="12.40631" name="Pleistein in der Oberpfalz" stateProvince="Bavaria">Pleistein in der Oberpfalz</location>
