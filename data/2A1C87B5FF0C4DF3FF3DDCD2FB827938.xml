<taxonomicName id="65B54D20FF084DF3FCCDDE47FC747A4D" authority="Baba, 2018" authorityName="Baba" authorityYear="2018" box="[828,955,307,331]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="158" pageNumber="159" phylum="Arthropoda" rank="species" species="corbariae" status="n. sp.">
