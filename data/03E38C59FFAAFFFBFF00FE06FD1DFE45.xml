<emphasis id="B93EE15DFFABFFFBFF35FEB2FF66FEEE" box="[185,243,343,369]" inLineHeading="true" pageId="5" pageNumber="62" reason="1">
<typeStatus id="54F183EDFFABFFFBFF35FEB2FF66FEEE" box="[185,243,343,369]" pageId="5" pageNumber="62">Type</typeStatus>
</emphasis>
and only species.
</emphasis>
<taxonomicName id="4C4A46CCFFABFFFBFE47FEB3FD65FEEF" authority="Kihara &amp; Huys, 2009" authorityName="Kihara &amp; Huys" authorityYear="2009" box="[459,752,342,368]" class="Maxillopoda" family="Ectinosomatidae" genus="Chaulionyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="5" pageNumber="62" phylum="Arthropoda" rank="species" species="paivacarvalhoi" status="sp. n.">
<emphasis id="B93EE15DFFABFFFBFE47FEB3FD65FEEF" box="[459,752,342,368]" italics="true" pageId="5" pageNumber="62">Chaulionyx paivacarvalhoi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20D5C26FFABFFFBFD7BFEB2FCA1FEEE" box="[759,820,343,369]" pageId="5" pageNumber="62" rank="species">sp. n.</taxonomicNameLabel>
