<number id="3DCDCE49C829216B48CBEA713730FC85" box="[494,606,799,824]" pageId="40" pageNumber="491" value="1.5" valueMax="1.55" valueMin="1.45">1.45–1.55</number>
(
<number id="3DCDCE49C829216B4B4BEA4E37CFFC85" box="[622,673,800,824]" pageId="40" pageNumber="491" value="1.49">1.49</number>
