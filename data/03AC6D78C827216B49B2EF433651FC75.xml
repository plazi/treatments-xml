(
<number id="3DCDCE49C829216B4BEBEB92366FFCA9" box="[718,769,764,788]" pageId="40" pageNumber="491" value="1.87">1.87</number>
) mm; PW 
<number id="3DCDCE49C829216B4A5BEB923680FCA9" box="[894,1006,764,788]" pageId="40" pageNumber="491" value="0.79" valueMax="0.8" valueMin="0.78">0.78–0.80</number>
(
<number id="3DCDCE49C829216B4ADBEB92315CFCA9" box="[1022,1074,764,788]" pageId="40" pageNumber="491" value="0.8">0.80</number>
) mm; PL 
<number id="3DCDCE49C829216B4D80EB92307BFCA8" box="[1189,1301,764,789]" pageId="40" pageNumber="491" value="0.375" valueMax="0.4" valueMin="0.35">0.35–0.40</number>
(
<number id="3DCDCE49C829216B4C00EB923037FCA9" box="[1317,1369,764,788]" pageId="40" pageNumber="491" value="0.38">0.38</number>
) mm; EW 
<number id="3DCDCE49C829216B49ECEA4E3454FC85" box="[201,314,800,824]" pageId="40" pageNumber="491" value="1.15" valueMax="1.2" valueMin="1.1">1.10–1.20</number>
(
<number id="3DCDCE49C829216B486CEA713412FC85" box="[329,380,799,824]" pageId="40" pageNumber="491" value="1.16">1.16</number>
