<taxonomicName id="CA173D3F0CF20DA8380A5846B88A167B" authority="Ashmead, 1893" authorityName="Ashmead" authorityYear="1893" baseAuthorityName="Harrington" baseAuthorityYear="1900" class="Insecta" family="Scelionidae" genus="Trissolcus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trissolcus hullensis" order="Hymenoptera" pageId="0" pageNumber="49" phylum="Arthropoda" rank="species" species="hullensis">
