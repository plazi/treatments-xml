<paragraph id="8BCE19389E2AFFEFF58A1CFCFEB0F9EC" blockId="1.[151,511,1490,1556]" box="[151,309,1530,1556]" pageId="1" pageNumber="52">
(
<figureCitation id="134A05BD9E2AFFEFF5821CFCFF74F9EC" box="[159,241,1530,1556]" captionStart="FIGURES 1 – 2" captionStartId="2.[151,269,1960,1984]" captionTargetBox="[165,1378,968,1907]" captionTargetId="figure@2.[149,1438,945,1952]" captionTargetPageId="2" captionText="FIGURES 1 – 2. Wing (male). 1. Brithura jinpingensis sp. nov.; 2. Brithura guangxiensis sp. nov." httpUri="https://zenodo.org/record/185550/files/figure.png" pageId="1" pageNumber="52">
Figs. 
<number id="3DB90B1F9E2AFFEFF5FF1CFCFF74F9EC" box="[226,241,1530,1556]" pageId="1" pageNumber="52" value="3.65" valueMax="6.0" valueMin="1.3">1</number>
</figureCitation>
, 
<figureCitation id="134A05BD9E2AFFEFF5E21CFCFEA9F9EC" box="[255,300,1530,1556]" captionStart="FIGURES 3 – 6" captionStartId="3.[151,269,1003,1027]" captionTargetBox="[424,1160,209,972]" captionTargetId="figure@3.[393,1193,176,997]" captionTargetPageId="3" captionText="FIGURES 3 – 6. Brithura jinpingensis sp. nov. Male. 3. Hypopygium, lateral view; 4. inner gonostylus, lateral view; 5. outer gonostylus, lateral view; 6. ninth tergite, dorsal view. Scale bar of 3 = 0.5 mm, 4 – 6 = 0.25 mm." httpUri="https://zenodo.org/record/185551/files/figure.png" pageId="1" pageNumber="52">3–6</figureCitation>
)
