<taxonomicName id="918D4D1FF654D62C3602C08A59C8FC20" authorityName="White" authorityYear="1846" box="[1367,1441,903,926]" class="Malacostraca" family="Pinnotheridae" genus="Pinnixa" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="97" phylum="Arthropoda" rank="genus">
