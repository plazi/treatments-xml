<taxonomicName id="918D4D1FF654D62C309AC08A589BFC20" authority="(Say, 1818)" authorityName="Theil &amp; Felder" authorityYear="2020" baseAuthorityName="Say" baseAuthorityYear="1818" box="[975,1266,902,926]" class="Malacostraca" family="Pinnotheridae" genus="Sayixa" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="97" phylum="Arthropoda" rank="species" species="monodactyla" status="n. comb.">
