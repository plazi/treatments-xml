<taxonomicName id="0BF94D5D856E8F0498EED051FCD38D6F" authorityName="C. Linnaeus" authorityYear="1753" box="[682,792,298,321]" class="Magnoliopsida" family="Apiaceae" genus="Eryngium" kingdom="Plantae" order="Apiales" pageId="33" pageNumber="334" phylum="Tracheophyta" rank="genus">
