<taxonomicName id="0BF94D5D856E8F04990AD052FC718D6C" box="[846,954,297,322]" class="Magnoliopsida" family="Apiaceae" kingdom="Plantae" order="Apiales" pageId="33" pageNumber="334" phylum="Tracheophyta" rank="family">Apiaceae</taxonomicName>
