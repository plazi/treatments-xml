<taxonomicName id="AF37FB6EA0389831FB098DE8BF72428C" class="Clitellata" family="Bignoniaceae" genus="Fridericia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fridericia" order="Lamiales" pageId="16" pageNumber="47" phylum="Annelida" rank="genus">
