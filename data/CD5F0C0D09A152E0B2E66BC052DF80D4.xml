<taxonomicName id="359E28B75B6FE4EAA6461324429447B5" authorityName="O.Swartz" authorityYear="1788" class="Magnoliopsida" family="Bignoniaceae" genus="Tanaecium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Tanaecium" order="Lamiales" pageId="16" pageNumber="47" phylum="Tracheophyta" rank="genus">
<emphasis id="6234D97DF2F4F75C8E8C6FAB0005500D" italics="true" pageId="16" pageNumber="47">Tanaecium</emphasis>
</taxonomicName>
(
<normalizedToken id="80522FACDA2D9D84548B501EFFEB6339" originalValue="Frazão">Frazao</normalizedToken>
