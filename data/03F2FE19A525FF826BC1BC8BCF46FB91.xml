<taxonomicName id="4C5B348CA525FF82692AB949C8A4F898" baseAuthorityName="Curran" baseAuthorityYear="1928" box="[596,758,1820,1841]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="147" phylum="Arthropoda" rank="species" species="hervebazini">
