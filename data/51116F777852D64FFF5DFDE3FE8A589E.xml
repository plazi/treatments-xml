<number id="6F70CC467852D64FFD19FD03FD135DBB" box="[723,763,699,721]" pageId="5" pageNumber="6" value="191.0">191</number>
; Solervicens 
<number id="6F70CC467852D64FFC48FD03FC505DBB" box="[898,952,699,721]" pageId="5" pageNumber="6" value="2007.0">2007</number>
