<taxonomicName id="73C34D0B1D7BFFB7160236DE3767F842" authority="Bochkov and Wauthy, 2009" authorityName="Bochkov and Wauthy" authorityYear="2009" box="[293,793,1949,1971]" class="Arachnida" family="Psoroptidae" genus="Listropsoralges" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="faini">
<emphasis id="86B7EA9A1D7BFFB7160236DE3586F842" box="[293,504,1949,1971]" italics="true" pageId="33" pageNumber="34">Listropsoralges faini</emphasis>
