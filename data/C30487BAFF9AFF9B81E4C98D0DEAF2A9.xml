</taxonomicName>
<taxonomicNameLabel id="62EA57C5FF9DFF9A82E8C93B0DF6F7F4" box="[923,1015,1392,1418]" pageId="6" pageNumber="581" rank="species">
<emphasis id="79D9EABEFF9DFF9A82E8C93B0DF6F7F4" bold="true" box="[923,1015,1392,1418]" pageId="6" pageNumber="581">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="8CAD4D2FFF9DFF9A8574C93A0A75F7F4" baseAuthorityName="Distant" baseAuthorityYear="1887" box="[1031,1140,1392,1419]" class="Insecta" family="Cicadidae" genus="Orientopsaltria" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="581" phylum="Arthropoda" rank="species" species="padda">
<emphasis id="79D9EABEFF9DFF9A8574C93A0A1AF7F5" box="[1031,1051,1393,1419]" italics="true" pageId="6" pageNumber="581">O</emphasis>
. 
<emphasis id="79D9EABEFF9DFF9A8558C93B0A75F7F4" box="[1067,1140,1392,1418]" italics="true" pageId="6" pageNumber="581">padda</emphasis>
</taxonomicName>
<emphasis id="79D9EABEFF9DFF9A8507C93B0A79F7F4" box="[1140,1144,1392,1418]" italics="true" pageId="6" pageNumber="581">,</emphasis>
<taxonomicName id="8CAD4D2FFF9DFF9A85F2C93A0B46F7F4" authorityName="Duffels &amp; Zaidi" authorityYear="1998" box="[1153,1351,1392,1419]" class="Insecta" family="Cicadidae" genus="Orientopsaltria" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="581" phylum="Arthropoda" rank="species" species="saudarapadda">
