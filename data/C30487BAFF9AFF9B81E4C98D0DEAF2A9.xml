<taxonomicName id="8CAD4D2FFF9DFF9A833BC93B0D92F7F4" authority="Pham &amp; Nguyen &amp; Constant, 2019" authorityName="Pham &amp; Nguyen &amp; Constant" authorityYear="2019" box="[584,915,1392,1418]" class="Insecta" family="Cicadidae" genus="Orientopsaltria" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="581" phylum="Arthropoda" rank="species" species="dongnaiensis" status="sp. nov.">
