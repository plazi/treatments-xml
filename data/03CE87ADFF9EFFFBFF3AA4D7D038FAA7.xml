(
<number id="3DAF249CFF9EFFFBFDE0A634D1A4FD13" box="[589,648,628,652]" pageId="5" pageNumber="6" value="2009.0">2009</number>
)
</bibRefCitation>
mentioned 
<taxonomicName id="4C674D38FF9EFFFBFCBBA634D0ACFD13" box="[790,896,628,652]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
<emphasis id="B913EAA9FF9EFFFBFCBBA634D0ACFD13" box="[790,896,628,652]" italics="true" pageId="5" pageNumber="6">Myrsidea</emphasis>
</taxonomicName>
sp. from 
<taxonomicName id="4C674D38FF9EFFFBFC40A635D76AFD13" box="[1005,1094,628,652]" class="Aves" family="Thraupidae" genus="Tangara" kingdom="Animalia" order="Passeriformes" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="dowii">
<emphasis id="B913EAA9FF9EFFFBFC40A635D76AFD13" box="[1005,1094,628,652]" italics="true" pageId="5" pageNumber="6">T. dowii</emphasis>
</taxonomicName>
