<taxonomicName authorityName="Boieldieu" authorityYear="1865" box="[282,459,1142,1164]" class="Insecta" family="Tenebrionidae" genus="Euboeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" phylum="Arthropoda" rank="species" species="mimonti">
