<taxonomicName class="Insecta" family="Dolichopodidae" genus="Liancalus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liancalus" order="Diptera" pageId="6" pageNumber="103" phylum="Arthropoda" rank="genus">Liancalus</taxonomicName>
