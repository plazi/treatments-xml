</taxonomicName>
<taxonomicNameLabel box="[451,522,1278,1302]" pageId="5" pageNumber="110" rank="genus">n. gen.</taxonomicNameLabel>
, 
<taxonomicName authority="Bianucci, Lambert &amp; Post, 2007" authorityName="Bianucci, Lambert &amp; Post" authorityYear="2007" class="Mammalia" family="Hyperoodontidae" genus="Izikoziphius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="5" pageNumber="110" phylum="Chordata" rank="genus">
<emphasis box="[533,647,1277,1301]" italics="true" pageId="5" pageNumber="110">Izikoziphius</emphasis>
<bibRefCitation author="BIANUCCI G. &amp; LAMBERT O. &amp; POST K." box="[113,427,1304,1328]" pageId="5" pageNumber="110" pagination="561 - 618" refId="ref23004" refString="BIANUCCI G., LAMBERT O. &amp; POST K. 2007. - A high diversity in fossil beaked whales (Odontoceti, Ziphiidae) recovered by trawling from the sea floor off South Africa. Geodiversitas 29 (4): 561 - 618." type="journal article" year="2007">Bianucci, Lambert &amp; Post, 2007</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Bianucci, 1997" authorityName="Bianucci" authorityYear="1997" class="Mammalia" family="Hyperoodontidae" genus="Tusciziphius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="5" pageNumber="110" phylum="Chordata" rank="genus">
<emphasis box="[436,551,1304,1328]" italics="true" pageId="5" pageNumber="110">Tusciziphius</emphasis>
<bibRefCitation author="BIANUCCI G." pageId="5" pageNumber="110" pagination="163 - 192" refId="ref22734" refString="BIANUCCI G. 1997. - ffle Odontoceti (Mammalia Cetacea) from Italian Pliocene. ffle Ziphiidae. Palaeontographia Italica 84: 163 - 192." type="journal article" year="1997">Bianucci, 1997</bibRefCitation>
