</taxonomicName>
<taxonomicNameLabel box="[282,353,1278,1302]" pageId="5" pageNumber="110" rank="genus">n. gen.</taxonomicNameLabel>
, 
<taxonomicName authority="Bianucci &amp; Miján &amp; Lambert &amp; Post &amp; Mateus, 2013" authorityName="Bianucci &amp; Miján &amp; Lambert &amp; Post &amp; Mateus" authorityYear="2013" box="[363,444,1278,1301]" class="Mammalia" family="Hyperoodontidae" genus="Imocetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="5" pageNumber="110" phylum="Chordata" rank="genus" status="n. gen.">
