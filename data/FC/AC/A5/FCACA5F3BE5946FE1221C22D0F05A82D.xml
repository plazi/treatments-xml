<taxonomicName authority="Leach, 1817" authorityName="Leach" authorityYear="1817" class="Insecta" family="Cimbicidae" genus="Abia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Abia" order="Hymenoptera" pageId="0" pageNumber="1168" phylum="Arthropoda" rank="genus">Abia</taxonomicName>
