<quantity box="[495,661,1651,1678]" metricMagnitude="-2" metricUnit="m" metricValue="2.035" metricValueMax="2.1399999999999997" metricValueMin="1.93" pageId="4" pageNumber="33" unit="mm" value="20.35" valueMax="21.4" valueMin="19.3">19.3–21.4 mm</quantity>
