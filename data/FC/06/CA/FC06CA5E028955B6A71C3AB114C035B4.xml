<taxonomicName authorityName="Leach" authorityYear="1815" family="Erebidae" lsidName="" pageId="0" pageNumber="1" rank="family">Erebidae</taxonomicName>
(
<taxonomicName lsidName="" pageId="0" pageNumber="1" rank="subFamily" subFamily="Lymantriinae">Lymantriinae</taxonomicName>
) in Japan: 
<taxonomicName authorityName="Linnaeus" authorityYear="1758" class="Insecta" family="Lymantriidae" genus="Calliteara" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calliteara pudibunda" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pudibunda">
<emphasis italics="true" pageId="0" pageNumber="1">Calliteara pudibunda</emphasis>
</taxonomicName>
(L.) (
<bibRefCitation author="Uchida, T" journalOrPublisher="Journal of the Faculty of Agriculture, Hokkaido University" pageId="0" pageNumber="1" pagination="177 - 297" refId="B180" refString="Uchida, T, 1928. Zweiter Beitrag zur Ichneumoniden-Fauna Japans. Journal of the Faculty of Agriculture, Hokkaido University 21: 177 - 297" title="Zweiter Beitrag zur Ichneumoniden-Fauna Japans." volume="21" year="1928">Uchida 1928</bibRefCitation>
