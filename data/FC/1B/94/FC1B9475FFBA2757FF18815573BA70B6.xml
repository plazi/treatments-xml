<heading bold="true" box="[151,734,1123,1150]" fontSize="11" level="1" pageId="3" pageNumber="262" reason="1">
<emphasis bold="true" box="[151,734,1123,1150]" pageId="3" pageNumber="262">
<taxonomicName box="[151,506,1123,1149]" class="Insecta" family="Cicadellidae" genus="Hishimonus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="262" phylum="Arthropoda" rank="species" species="pantanagarensis" status="sp. nov.">
<emphasis bold="true" box="[151,506,1123,1149]" italics="true" pageId="3" pageNumber="262">Hishimonus pantanagarensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[514,607,1124,1150]" pageId="3" pageNumber="262" rank="species">sp. nov.</taxonomicNameLabel>
Meshram
