<td box="[537,634,301,323]" gridcol="1" gridrow="1" pageId="20" pageNumber="2828">0: 1: 023</td>
<td box="[742,839,301,323]" gridcol="2" gridrow="1" pageId="20" pageNumber="2828">1: 1: 021</td>
</tr>
