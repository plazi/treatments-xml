<taxonomicName id="4C114D3EFFD3FFFBFCF5FB47FB87FB41" authority="Agassiz, 1833" authorityName="Agassiz" authorityYear="1833" box="[861,1101,1189,1215]" class="Insecta" family="Curculionidae" genus="Gyrodus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="146" phylum="Arthropoda" rank="genus">
