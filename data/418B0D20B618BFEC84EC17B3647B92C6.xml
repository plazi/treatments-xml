<taxonomicName id="8C69313CFC33F93A9857E31A7021C484" authorityName="Gauld" authorityYear="1991" class="Insecta" family="Ichneumonidae" genus="Rodrigama" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="1" pageNumber="66" phylum="Arthropoda" rank="genus">Rodrigama</taxonomicName>
