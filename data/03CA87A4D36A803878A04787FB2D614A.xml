<taxonomicName id="4C634D31D36A80387A00423AFC1F6168" authority="(Grube, 1859)" baseAuthorityName="Grube" baseAuthorityYear="1859" box="[541,983,1573,1600]" class="Phascolosomatidea" family="Phascolosomatidae" genus="Antillesoma" kingdom="Animalia" order="Phascolosomatiformes" pageId="26" pageNumber="103" phylum="Sipuncula" rank="species" species="antillarum">
