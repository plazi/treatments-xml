<taxonomicName id="5AAC4D4CFFE3FF92FB19F8BEFAADC4C4" authority="Mound &amp; Masumoto, 2009" authorityName="Mound &amp; Masumoto" authorityYear="2009" box="[1276,1362,1901,1924]" class="Insecta" family="Thripidae" genus="Ozanaphothrips" kingdom="Animalia" order="Thysanoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="torridus" status="sp. n.">
