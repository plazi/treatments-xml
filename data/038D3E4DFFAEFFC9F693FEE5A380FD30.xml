species: 
<taxonomicName id="4C24F4D8FFAEFFC9F722FEA3A408FE72" authority="Franz, 1975" authorityName="Franz" authorityYear="1975" box="[294,804,366,388]" class="Insecta" family="Staphylinidae" genus="Horaeomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="578" phylum="Arthropoda" rank="genus" species="montistamborinensis">
<emphasis id="B9505349FFAEFFC9F722FEA3A589FE72" box="[294,677,366,388]" italics="true" pageId="11" pageNumber="578">Horaeomorphus montistamborinensis</emphasis>
<bibRefCitation id="EFB5F2AAFFAEFFC9F4AFFEA2A408FE72" author="Franz, H." box="[683,804,366,388]" pageId="11" pageNumber="578" pagination="1 - 312" refId="ref5991" refString="Franz, H. (1975) Revision der Scydmaeniden von Australien, Neuseeland und den benachbarten Inseln. Denkschriften Osterreichische Akademie der Wissenschaften, Mathematisch-Naturwissenschaftliche Klasse, 118, 1 - 312. https: // doi. org / 10.1007 / 978 - 3 - 7091 - 5642 - 1" type="journal article" year="1975">Franz, 1975</bibRefCitation>
