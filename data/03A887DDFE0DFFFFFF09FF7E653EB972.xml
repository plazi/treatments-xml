<taxonomicName id="4C014D48FE0DFFFFFBBFFE1E6208BA9F" authority="St Hil" authorityName="St Hil" box="[1057,1351,476,501]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="576" phylum="Tracheophyta" rank="species" species="paraguariensis">
