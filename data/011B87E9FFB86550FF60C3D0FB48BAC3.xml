<taxonomicName id="4EB24D7CFFB86550FEDCC653FE45BA7F" authorityName="Schott" authorityYear="1917" box="[299,389,1810,1835]" class="Entognatha" family="Entomobryidae" genus="Entomobrya" kingdom="Animalia" order="Collembola" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="varia">
<emphasis id="BBC6EAEDFFB86550FEDCC653FE45BA7F" box="[299,389,1810,1835]" italics="true" pageId="41" pageNumber="42">E. varia</emphasis>
</taxonomicName>
