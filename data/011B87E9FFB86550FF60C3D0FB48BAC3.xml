<taxonomicName id="4EB24D7CFFB86550FBAAC653FB77BA7F" authority="Jordana &amp; Greenslade, 2020" authorityName="Jordana &amp; Greenslade" authorityYear="2020" box="[1117,1207,1809,1835]" class="Entognatha" family="Entomobryidae" genus="Entomobrya" kingdom="Animalia" order="Collembola" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="karta" status="sp. nov.">
<emphasis id="BBC6EAEDFFB86550FBAAC653FB77BA7F" box="[1117,1207,1809,1835]" italics="true" pageId="41" pageNumber="42">E. karta</emphasis>
</taxonomicName>
