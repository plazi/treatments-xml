. 
<taxonomicName id="4C7A4D1FB711FFA464087A2CDFD1F8A3" authority="T. Majewski. A. Paired" box="[348,929,1822,1849]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Euphoriomyces" kingdom="Fungi" order="Laboulbeniales" pageId="64" pageNumber="65" phylum="Ascomycota" rank="species" species="unilateralis">
