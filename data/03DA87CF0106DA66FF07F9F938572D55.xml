<taxonomicName id="4C734D5A0106DA66FE52F96D3EDA2CAD" authorityName="Sánchez-Montes &amp; Colunga-Salas &amp; Álvarez-Castillo &amp; Guzmán-Cornejo &amp; Montiel-Parra" authorityYear="2018" box="[450,684,1729,1754]" class="Insecta" family="Menoponidae" genus="Conciella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="glaucidiae">
