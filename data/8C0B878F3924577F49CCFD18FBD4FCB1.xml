<taxonomicName id="C3A24D1A3924577F49FEFD18FDA7FD0D" authority="Blaszyk, 1987" authorityName="Blaszyk" authorityYear="1987" box="[165,583,657,683]" class="Ostracoda" family="Neocytherideidae" genus="Copytus" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="184" phylum="Arthropoda" rank="species" species="ezcurraensis">
