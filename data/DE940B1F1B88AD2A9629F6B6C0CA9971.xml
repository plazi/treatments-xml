<normalizedToken id="DB849AC93272843133A890FEFB1880AE" originalValue="bandförmigen" pageId="null" pageNumber="217">bandfoermigen</normalizedToken>
<normalizedToken id="6E1FC68F64BAE42FF1940FD0AC1AB76A" originalValue="Blättern" pageId="null" pageNumber="217">Blaettern</normalizedToken>
.
