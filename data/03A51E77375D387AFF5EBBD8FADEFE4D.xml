<taxonomicName id="4C0CD4E2375C3879FE7EB8F6FD6AF937" box="[439,640,1728,1750]" class="Insecta" family="Staphylinidae" genus="Achmonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="266" phylum="Arthropoda" rank="species" species="carinata" status="sp. nov.">
