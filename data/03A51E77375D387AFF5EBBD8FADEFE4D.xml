<taxonomicName id="4C0CD4E2375C3879FA4FB8F7FE64F914" authority="Bernh." authorityName="Bernh." class="Insecta" family="Staphylinidae" genus="Achmonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="266" phylum="Arthropoda" rank="species" species="flavomarginata">
