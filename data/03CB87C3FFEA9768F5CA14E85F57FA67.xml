<quantity id="4C9A9B30FFF49769F50617545ADCF9BE" box="[355,426,1631,1655]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="48" pageNumber="1555" unit="m" value="1400.0">
1400 
<emphasis id="B916EAC7FFF49769F5F917545ADCF9BE" bold="true" box="[412,426,1631,1655]" italics="true" pageId="48" pageNumber="1555">m</emphasis>
</quantity>
m long and 
<emphasis id="B916EAC7FFF49769F65D176B5927F9BE" box="[568,593,1632,1655]" italics="true" pageId="48" pageNumber="1555">ca</emphasis>
<quantity id="4C9A9B30FFF49769F63F175459E2F9BE" box="[602,660,1631,1655]" metricMagnitude="2" metricUnit="m" metricValue="6.4" pageId="48" pageNumber="1555" unit="m" value="640.0">
