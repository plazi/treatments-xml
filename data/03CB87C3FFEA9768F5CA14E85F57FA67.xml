<emphasis id="B916EAC7FFEB9776F49412F15A90FBD9" box="[241,486,1018,1040]" italics="true" pageId="47" pageNumber="1554">
<taxonomicName id="4C624D56FFEB9776F49412F15AD0FBD9" authorityName="Broch Cantero Cantero" authorityYear="1948" box="[241,422,1018,1040]" class="Hydrozoa" family="Symplectoscyphidae" genus="Symplectoscyphus" kingdom="Animalia" order="Leptothecata" pageId="47" pageNumber="1554" phylum="Cnidaria" rank="genus">Symplectoscyphus</taxonomicName>
