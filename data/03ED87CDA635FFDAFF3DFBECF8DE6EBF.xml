<taxonomicName id="4C444D58A634FFDAFEFAFAE2F8F36CAE" authority="Zhang &amp; Wang" authorityName="Zhang &amp; Wang" box="[336,905,1393,1415]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="conghuaense" status="sp. n." subGenus="Tripodura">
<emphasis id="B930EAC9A634FFDAFEFAFAE2FAAA6CAE" box="[336,464,1393,1415]" italics="true" pageId="13" pageNumber="14">Polypedilum</emphasis>
(
<emphasis id="B930EAC9A634FFDAFE48FAE2F9326CAE" box="[482,584,1393,1415]" italics="true" pageId="13" pageNumber="14">Tripodura</emphasis>
) 
<emphasis id="B930EAC9A634FFDAFDF1FAE2F9A56CAE" box="[603,735,1393,1415]" italics="true" pageId="13" pageNumber="14">conghuaense</emphasis>
Zhang &amp; Wang
</taxonomicName>
<taxonomicNameLabel id="A20357B2A634FFDAFC3EFAE2F8AA6CAF" box="[916,976,1393,1414]" pageId="13" pageNumber="14" rank="species">
