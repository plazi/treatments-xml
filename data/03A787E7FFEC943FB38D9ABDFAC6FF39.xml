<paragraph id="8BB136F1FFEC9431B38D9ABDFD5BF918" blockId="6.[151,725,1754,1780]" box="[151,725,1754,1780]" pageId="6" pageNumber="307">
<heading id="D0F9819DFFEC9431B38D9ABDFD5BF918" bold="true" box="[151,725,1754,1780]" fontSize="11" level="1" pageId="6" pageNumber="307" reason="1">
<taxonomicName id="4C0E4D72FFEC9431B38D9ABDFD5BF918" authority="de Saussure" authorityName="de Saussure" authorityYear="1853" box="[151,725,1754,1780]" class="Insecta" family="Vespidae" genus="Polistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="species" species="cavapyta" subGenus="Aphanilopterus">
<emphasis id="B97AEAE3FFEC9431B38D9ABDFD5BF918" bold="true" box="[151,725,1754,1780]" pageId="6" pageNumber="307">
<emphasis id="B97AEAE3FFEC9431B38D9ABDFF7AF918" bold="true" box="[151,244,1754,1780]" italics="true" pageId="6" pageNumber="307">Polistes</emphasis>
(
<emphasis id="B97AEAE3FFEC9431B21E9ABDFE31F918" bold="true" box="[260,447,1754,1780]" italics="true" pageId="6" pageNumber="307">Aphanilopterus</emphasis>
) 
<emphasis id="B97AEAE3FFEC9431B2CB9ABDFDB4F918" bold="true" box="[465,570,1754,1780]" italics="true" pageId="6" pageNumber="307">cavapyta</emphasis>
