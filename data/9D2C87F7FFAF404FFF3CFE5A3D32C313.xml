<taxonomicName id="D2854D62FFAF404FFCACFD973A50C703" authority="Nurse, 1902" authorityName="Nurse" authorityYear="1902" box="[775,1058,608,632]" class="Insecta" family="Hymenoptera" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="69" phylum="Arthropoda" rank="species" species="timidus">
