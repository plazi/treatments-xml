<taxonomicName id="4C715DD1D654FF9EFEC0E501FDA81116" authorityName="Turrisi &amp; Smith" authorityYear="2020" box="[342,558,1047,1066]" class="Insecta" family="Aulacidae" genus="Pristaulacus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="205" phylum="Arthropoda" rank="species" species="perfidus">
<emphasis id="B905FA40D654FF9EFEC0E501FDA81116" box="[342,558,1047,1066]" italics="true" pageId="8" pageNumber="205">Pristaulacus perfidus</emphasis>
</taxonomicName>
