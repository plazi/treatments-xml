<taxonomicName id="30944D004A3DFF82FF5AFF196C6794FB" box="[151,530,153,180]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="metallicus">
<emphasis id="C5E0EA914A3DFF82FF5AFF196C6794FB" bold="true" box="[151,530,153,180]" italics="true" pageId="36" pageNumber="37">Morphostenophanes metallicus</emphasis>
</taxonomicName>
species group (
<taxonomicName id="30944D004A3DFF82FD1EFF196D3A94FB" box="[723,847,153,180]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="metallicus">
