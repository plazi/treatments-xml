<taxonomicName id="19F34D1E9337621EFD14F960FCBDF891" authorityName="Coquillett" authorityYear="1899" box="[689,820,1772,1797]" class="Insecta" family="Tachinidae" genus="Ateloglossa" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="genus">
