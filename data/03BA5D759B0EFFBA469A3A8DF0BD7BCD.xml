<taxonomicName id="4C1397E09B0CFFA5473F3EC5F10A7C89" authority="Nakamura, 2010" authorityName="Nakamura" authorityYear="2010" box="[306,540,1948,1971]" class="Entognatha" family="Eosentomidae" genus="Eosentomon" kingdom="Animalia" order="Protura" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="hitakami" status="sp. nov.">
