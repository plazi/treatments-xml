<taxonomicName id="7A04979D389204A82B9E5CB562CEA205" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139719" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica interrupta Schenck" order="Hymenoptera" pageNumber="75" phylum="Arthropoda" rank="species" species="interrupta">Myrmica interrupta Schenck</taxonomicName>
