<taxonomicName id="4C044D40524D367DB9B3CB6EFCAFF766" authorityName="Latreille" authorityYear="1802" box="[723,885,1302,1327]" class="Insecta" family="Curculionidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="74" phylum="Arthropoda" rank="family">Curculionidae</taxonomicName>
