<emphasis id="B2F7EAC5FFCC6C21861D6B479718FAC2" box="[1076,1103,1299,1325]" italics="true" pageId="20" pageNumber="82">C.</emphasis>
<taxonomicName id="47834D54FFCC6C21867F6B469621FAC2" authority="(Wu, 1948)" baseAuthorityName="Wu" baseAuthorityYear="1948" box="[1110,1398,1298,1325]" class="Insecta" family="Perlidae" genus="Chinoperla" kingdom="Animalia" order="Plecoptera" pageId="20" pageNumber="82" phylum="Arthropoda" rank="species" species="nigroflavata">
