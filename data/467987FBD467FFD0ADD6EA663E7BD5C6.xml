<taxonomicName id="09D04D6ED467FFCFAF06EE9F3E29D392" box="[583,776,1683,1709]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="candicans">
