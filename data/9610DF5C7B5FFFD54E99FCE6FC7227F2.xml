<taxonomicName id="D9B915C97B5FFFD54E99FC7FFD9F22FE" authority="Gozmany, 1973: 440" authorityName="Gozmany" authorityPageNumber="440" authorityYear="1973" box="[151,591,903,927]" class="Insecta" family="Lecithoceridae" genus="Torodora" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="112" phylum="Arthropoda" rank="species" species="macrosigna">
