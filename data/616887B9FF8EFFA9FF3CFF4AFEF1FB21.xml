<taxonomicName id="2EC14D2CFF8EFFA9FF3CFF4AFD25FF55" authority="Vogt, 1909" authorityName="Vogt" authorityYear="1909" box="[151,683,151,176]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="60" phylum="Arthropoda" rank="subSpecies" species="humilis" subSpecies="quasimuscorum">
<emphasis id="DBB5EABDFF8EFFA9FF3CFF4AFD25FF55" bold="true" box="[151,683,151,176]" pageId="19" pageNumber="60">
