(
<bibRefCitation id="88B6C8D4775D5B85BF238B8B956BBEF0" author="Griffiths, G. C. D." journalOrPublisher="Beitraege zur Entomologie" pageId="0" pageNumber="8151" pagination="823 - 914" title="The Alysiinae (Hym. Braconidae) parasites of the Agromyzidae (Diptera). I. General questions of taxonomy, biology and evolution" volume="14" year="1964">Griffiths 1964</bibRefCitation>
, 
<bibRefCitation id="714D7659F074F6BA4C6E2BD34CC67D56" author="Griffiths, G. C. D." journalOrPublisher="Beitraege zur Entomologie" pageId="0" pageNumber="8151" pagination="653 - 696" title="The Alysiinae (Hym. Braconidae) parasites of the Agromyzidae (Diptera) IV. The parasites of Hexomyza Enderlein, Melanagromyza Hendel, Ophiomyia Braschnikov and Napomyza Westwood" volume="17" year="1967">Griffiths 1967a</bibRefCitation>
, 
<bibRefCitation id="22AA6B90292AD73FAA70A673640B44E7" author="Griffiths, G. C. D." journalOrPublisher="Beitraege zur Entomologie" pageId="0" pageNumber="8151" pagination="551 - 605" title="The Alysiinae (Hym. Braconidae) parasites of the Agromyzidae (Diptera) II. The parasites of Agromyza Fallen" volume="16 (1966)" year="1967">Griffiths 1967b</bibRefCitation>
, 
<bibRefCitation id="03953C5B7F24B8ED5B1158AA4E2CB481" author="Griffiths, G. C. D." journalOrPublisher="Beitraege zur Entomologie" pageId="0" pageNumber="8151" pagination="775 - 951" title="The Alysiinae (Hym. Braconidae) parasites of the Agromyzidae (Diptera) III. The parasites of Paraphytomyza Enderlein, Phytagromyza Hendel, and Phytomyza Fallen" volume="16 (1966)" year="1967">Griffiths 1967c</bibRefCitation>
