<taxonomicName id="395FB355CD0B6AD99C7638F948CFB709" class="Insecta" family="Braconidae" genus="Exotela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exotela" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Exotela</taxonomicName>
