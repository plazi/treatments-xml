<taxonomicName id="6F1F0D16B7E9380CB595DADDDFF1FE42" class="Insecta" family="Braconidae" genus="Antrusa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Antrusa" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Antrusa</taxonomicName>
