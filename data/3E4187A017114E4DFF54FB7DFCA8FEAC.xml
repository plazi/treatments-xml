<quantity id="71109B5317104E4AFBB5FE03FB40FE19" box="[1142,1232,476,501]" metricMagnitude="5" metricUnit="m" metricValue="1.5" pageId="67" pageNumber="68" unit="km" value="150.0">150 km</quantity>
