<taxonomicName id="1C674A6059F9E3531DAB87BD1D14062D" authorityName="Soula" authorityYear="2006" class="Insecta" family="Scarabaeidae" genus="Sorocha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sorocha" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
