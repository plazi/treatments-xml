<materialsCitation id="3B0C3CCDFF91FFEB3A4BFE627B77FDFA" ID-GBIF-Occurrence="3396403301" collectionCode="American Museum of Natural History" collectorName="Barnum Brown" country="United States" county="Dawson County" formation="Laramie" geologicalContext="Cretaceous" httpUri="http://research.amnh.org/paleontology/search.php?action=detail&amp;specimen_id=52853  " location="Hell Creek, Ft. Pierre (Judith River)" pageId="3" pageNumber="262" preparations="The larger portion of a skeleton" specimenCode="American Museum No. 973" stateProvince="Montana" typeStatus="holotype">
