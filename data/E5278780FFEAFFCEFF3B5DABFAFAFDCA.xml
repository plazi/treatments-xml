<taxonomicName id="AA8E4D15FFEAFFCEFF125D9CFE70FE19" box="[190,462,432,457]" class="Mammalia" family="Cervidae" genus="Odocoileus" kingdom="Animalia" order="Artiodactyla" pageId="7" pageNumber="38" phylum="Chordata" rank="species" species="virginianus">
