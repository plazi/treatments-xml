= 
<treatmentCitation id="A506A31AFF1A83EAFF7959E5FE84FE46" author="Boiss." journal="Ann. Sci. Nat., Bot." page="87" pageId="167" pageNumber="167" score="32.7652104432798" volume="17" year="1842">
<taxonomicName id="E3A7FE88FF1A83EAFF7959E5FE39FE66" authority="Boiss." authorityName="Boiss." box="[189,425,389,413]" class="Magnoliopsida" family="Brassicaceae" genus="Brassica" kingdom="Plantae" order="Brassicales" pageId="167" pageNumber="167" phylum="Tracheophyta" rank="species" species="deflexa">
<emphasis id="16D35919FF1A83EAFF7959E5FEF4FE66" bold="true" box="[189,356,389,413]" italics="true" pageId="167" pageNumber="167">Brassica deflexa</emphasis>
Boiss.
</taxonomicName>
in Ann. Sci. 
<taxonomicName id="E3A7FE88FF1A83EAFDF759E6FDF0FE66" authorityName="Slipinski" authorityYear="2007" box="[563,608,390,413]" class="Insecta" family="Coccinellidae" genus="Nat" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="167" pageNumber="180" phylum="Arthropoda" rank="genus">Nat</taxonomicName>
