<emphasis id="B92DEABA3A17FFD1FDD8F9F9D994342E" box="[525,675,1599,1637]" italics="true" pageId="12" pageNumber="273">
<taxonomicName id="4C594D2B3A17FFD1FDD8F9F9D9AB342E" authorityName="Deeleman-Reinhold" authorityYear="2001" box="[525,668,1599,1637]" class="Arachnida" family="Corinnidae" genus="Oedignatha" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="273" phylum="Arthropoda" rank="species" species="barbata">barbata</taxonomicName>
