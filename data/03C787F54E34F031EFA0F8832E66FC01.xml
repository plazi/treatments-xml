<number id="3DA624C44E37F031EE9EFF332E82FF39" box="[425,439,188,212]" pageId="2" pageNumber="48" value="8.0">8</number>
and IV 
<number id="3DA624C44E37F031ED26FF332D2AFF39" box="[529,543,188,212]" pageId="2" pageNumber="48" value="6.0">6</number>
(usually 
<date id="FFD010234E37F031EDB1FF322DE5FF39" box="[646,720,188,212]" pageId="2" pageNumber="48">
I–IV 
<quantity id="4C969B064E37F031EDF5FF332DE5FF39" box="[706,720,188,212]" metricMagnitude="-1" metricUnit="m" metricValue="1.524" pageId="2" pageNumber="48" unit="in" value="6.0">6</quantity>
