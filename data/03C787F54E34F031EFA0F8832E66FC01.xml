<taxonomicName id="4C6E4D604E37F031EEBBFF172D27FF5D" box="[396,530,152,176]" class="Arachnida" family="Sparassidae" genus="Spariolenus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
