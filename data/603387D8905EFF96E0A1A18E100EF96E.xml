<paragraph id="E82536CE905EFF96E0A1A18E1704FA0B" blockId="9.[151,525,1444,1470]" box="[151,525,1444,1470]" pageId="9" pageNumber="584">
<heading id="B36D81A2905EFF96E0A1A18E1704FA0B" bold="true" box="[151,525,1444,1470]" fontSize="11" level="1" pageId="9" pageNumber="584" reason="1">
<taxonomicName id="2F9A4D4D905EFF96E0A1A18E1704FA0B" authority="Rathbun, 1901" authorityName="Rathbun" authorityYear="1901" box="[151,525,1444,1470]" class="Malacostraca" family="Pandalidae" genus="Pandalus" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="584" phylum="Arthropoda" rank="species" species="longicauda">
