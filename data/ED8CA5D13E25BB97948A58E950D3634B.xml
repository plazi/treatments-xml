<taxonomicName id="50D2F5F36F1D172E377E0C8239790F64" genus="Proctotrupoidea" lsidName="Proctotrupoidea foveolatus" pageId="0" pageNumber="7936" rank="species" species="foveolatus">foveolatus</taxonomicName>
