<taxonomicName id="B744B66D2E6EC279EA042AB99236362E" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" class="Insecta" family="Proctotrupidae" genus="Proctotrupes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Proctotrupes" order="Hymenoptera" pageId="0" pageNumber="7936" phylum="Arthropoda" rank="genus">Proctotrupes</taxonomicName>
