<paragraph id="97294B60C85A7165899AFE5EFE718C48" blockId="2.[151,449,511,570]" box="[151,449,511,537]" pageId="2" pageNumber="41">
<heading id="CC61FC0CC85A7165899AFE5EFE718C48" bold="true" box="[151,449,511,537]" fontSize="11" level="1" pageId="2" pageNumber="41" reason="1">
<emphasis id="A5E29772C85A7165899AFE5EFE718C48" bold="true" box="[151,449,511,537]" pageId="2" pageNumber="41">
<taxonomicName id="509630E3C85A7165899AFE5EFEED8C48" box="[151,349,511,537]" class="Mammalia" family="Bathyergidae" genus="Fukomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="41" phylum="Chordata" rank="species" species="ilariae" status="sp. nov.">
<emphasis id="A5E29772C85A7165899AFE5EFEED8C48" bold="true" box="[151,349,511,537]" italics="true" pageId="2" pageNumber="41">Fukomys ilariae</emphasis>
</taxonomicName>
<taxonomicNameLabel id="BED12A09C85A71658869FE5EFE718C48" box="[356,449,511,537]" pageId="2" pageNumber="41" rank="species">sp. nov.</taxonomicNameLabel>
