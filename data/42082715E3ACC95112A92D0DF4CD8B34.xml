<taxonomicName id="1E8E917840AB3EB61E00CD6CF8D9056E" class="Magnoliopsida" family="Convolvulaceae" genus="Ipomoea" kingdom="Plantae" order="Solanales" phylum="Tracheophyta" rank="species" species="gloverae">
<emphasis id="D3D3CDE55B382816041CCA61F751DFFB" italics="true" pageId="0" pageNumber="1">I. gloverae</emphasis>
</taxonomicName>
