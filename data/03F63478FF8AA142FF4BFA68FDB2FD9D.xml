<collectingCountry id="F348C5FEFF8FA142FD98FE9AFD34FE43" box="[580,711,376,402]" name="Costa Rica" pageId="68" pageNumber="69">Costa Rica</collectingCountry>
; 
<quantity id="4CA7288BFF8FA142FD0FFE9AFCDFFE43" box="[723,812,376,402]" metricMagnitude="3" metricUnit="m" metricValue="1.85" pageId="68" pageNumber="69" unit="m" value="1850.0">1850 m</quantity>
