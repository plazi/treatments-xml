<quantity id="4C9A9B15FFB6F96BFB1AFEA1FAD4FE76" box="[1270,1344,369,396]" metricMagnitude="2" metricUnit="m" metricValue="9.8" pageId="42" pageNumber="43" unit="m" value="980.0">980 m</quantity>
(
