<taxonomicName id="4C39C6F9BF37FF97252CFC71FC4F2D99" authority="Simon, 1900" authorityName="Simon" authorityYear="1900" box="[587,910,903,927]" class="Arachnida" family="Salticidae" genus="Gypogyna" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="242" phylum="Arthropoda" rank="species" species="forceps">
