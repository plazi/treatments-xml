<taxonomicName id="4C7B4D7AFFAEE444AB1F53D6454DFE2E" authority="Nutt." authorityName="Nutt." box="[877,1102,439,464]" class="Magnoliopsida" family="Fabaceae" genus="Robinia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="parviflorus">
<emphasis id="B90FEAEBFFAEE444AB1F53D64509FE31" box="[877,1034,439,464]" italics="true" pageId="14" pageNumber="15">R. parviflorus</emphasis>
