(mean value 
<quantity id="4CDB9B37FFA8FF91F1079564FE57BC69" box="[301,386,1821,1845]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="13" pageNumber="322" unit="mm" value="4.5">4.5 mm</quantity>
, 
<typeStatus id="54988870FFA8FF91F1A49564FE3EBC69" box="[398,491,1821,1845]" pageId="13" pageNumber="322">holotype</typeStatus>
<quantity id="4CDB9B37FFA8FF91F1D89564FD9CBC69" box="[498,585,1821,1845]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="13" pageNumber="322" unit="mm" value="4.5">4.5 mm</quantity>
