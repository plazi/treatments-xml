, 
<typeStatus id="54988870FFA8FF91F1BE9544FE24BC09" box="[404,497,1853,1877]" pageId="13" pageNumber="322">holotype</typeStatus>
<quantity id="4CDB9B37FFA8FF91F1D09544FD87BC09" box="[506,594,1853,1877]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="13" pageNumber="322" unit="mm" value="2.5">2.5 mm</quantity>
