<quantity id="4CDB9B37FFA8FF91F28A9564FF4FBC09" metricMagnitude="-3" metricUnit="m" metricValue="2.55" metricValueMax="2.6" metricValueMin="2.5" pageId="13" pageNumber="322" unit="mm" value="2.55" valueMax="2.6" valueMin="2.5">2.5–2.6 mm</quantity>
