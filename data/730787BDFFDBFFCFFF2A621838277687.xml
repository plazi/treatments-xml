<taxonomicName id="3CAE4D28FFDBFFCCFCED62A83EB17680" box="[848,940,1195,1219]" class="Magnoliopsida" family="Primulaceae" genus="Primula" kingdom="Plantae" order="Ericales" pageId="5" pageNumber="116" phylum="Tracheophyta" rank="genus">
