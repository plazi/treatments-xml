<subSubSection id="B3B46520FFDBFFCCFF7A62A939EE7680" box="[199,1267,1194,1219]" pageId="5" pageNumber="116" type="etymology">
<paragraph id="FB1136ABFFDBFFCCFF7A62A939EE7680" blockId="5.[151,1437,1158,2011]" box="[199,1267,1194,1219]" pageId="5" pageNumber="116">
<emphasis id="C9DAEAB9FFDBFFCCFF7A62A93C537680" bold="true" box="[199,334,1194,1219]" pageId="5" pageNumber="116">Etymology.</emphasis>
