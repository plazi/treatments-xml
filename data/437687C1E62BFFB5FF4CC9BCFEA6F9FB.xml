<taxonomicName id="0CDF4D54E628FFB3FEF7CACFFD57F83A" authority="Fain, 1959" authorityName="Fain" authorityYear="1959" box="[300,675,1995,2018]" class="Arachnida" family="Gastronyssidae" genus="Rodhainyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="longipilis">
