, 
<emphasis id="B92DEABFB404FFA3FA2EFE2843A2FE29" box="[1116,1168,402,451]" italics="true" pageId="16" pageNumber="41">
<specimenCount id="9D5FFD24B404FFA3FA2EFE284344FE29" box="[1116,1142,402,451]" pageId="16" pageNumber="41" type="female">â™€</specimenCount>
,
