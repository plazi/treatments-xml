<taxonomicName id="9AFC4D1D6802FFF0FEF5FCD23A58FCC5" box="[151,306,835,859]" class="Insecta" family="Figitidae" genus="Dilyta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="rathmanae">
<emphasis id="6F88EA8C6802FFF0FEF5FCD23A58FCC5" box="[151,306,835,859]" italics="true" pageId="6" pageNumber="35">D. rathmanae</emphasis>
