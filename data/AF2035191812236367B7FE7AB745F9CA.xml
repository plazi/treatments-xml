<quantity id="E07129EA181223636686FB8EB646FBC2" box="[422,552,1105,1131]" metricMagnitude="-3" metricUnit="m" metricValue="3.25" metricValueMax="3.3" metricValueMin="3.2" pageId="2" pageNumber="566" unit="mm" value="3.25" valueMax="3.3" valueMin="3.2">3.2–3.3mm</quantity>
, female 
<quantity id="E07129EA1812236365A6FB8EB767FBC2" box="[646,777,1105,1131]" metricMagnitude="-3" metricUnit="m" metricValue="3.75" metricValueMax="3.8" metricValueMin="3.7" pageId="2" pageNumber="566" unit="mm" value="3.75" valueMax="3.8" valueMin="3.7">3.7–3.8mm</quantity>
