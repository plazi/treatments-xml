<taxonomicName id="23874D78FFFD9730FD2B902AFC08F981" box="[754,896,1619,1644]" class="Arachnida" family="Galumnellidae" genus="Galumnella" kingdom="Animalia" order="Sarcoptiformes" pageId="16" pageNumber="59" phylum="Arthropoda" rank="genus">
