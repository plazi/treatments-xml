<collectingCountry id="9C90766BFFE0972DFBDF9771FBE4FECF" box="[1030,1132,264,290]" name="Ethiopia" pageId="13" pageNumber="56">Ethiopia</collectingCountry>
, 
<geoCoordinate id="81B3503CFFE0972DFBA09771FB44FECF" box="[1145,1228,264,290]" direction="north" orientation="latitude" pageId="13" pageNumber="56" precision="925" value="6.633333">
<number id="524F24DCFFE0972DFBA09771FB00FECF" box="[1145,1160,264,290]" pageId="13" pageNumber="56" value="6.0">6</number>
º 
