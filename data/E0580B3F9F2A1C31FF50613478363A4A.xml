<taxonomicName id="AFF1C1AA9F2A1C31FF5061887CE83FD2" authority="Phalaenae" authorityName="Linnaeus" authorityYear="1758" box="[151,416,405,431]" class="Insecta" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="405" phylum="Arthropoda" rank="order">Lepidoptera Phalaenae</taxonomicName>
