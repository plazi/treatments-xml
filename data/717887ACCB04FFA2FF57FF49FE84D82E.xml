<paragraph id="F96E36BACB04FFA3FF57FEDCFC21D852" blockId="6.[151,993,258,280]" box="[151,993,258,280]" pageId="6" pageNumber="507">
<taxonomicName id="3ED14D39CB04FFA3FF57FEDCFDDAD852" authority="Navas, 1931: 74" authorityName="Navas" authorityPageNumber="74" authorityYear="1931" box="[151,538,258,280]" class="Insecta" family="Corydalidae" genus="Protohermes" kingdom="Animalia" order="Megaloptera" pageId="6" pageNumber="507" phylum="Arthropoda" rank="species" species="decolor">
