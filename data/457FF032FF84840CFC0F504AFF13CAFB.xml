<taxonomicName id="0AD63AA7FF84840DFB195164FAE5CC1F" authorityName="MacGillivray" authorityYear="1921" box="[1187,1305,1804,1828]" class="Insecta" family="Diaspididae" genus="Lindingaspis" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="141" phylum="Arthropoda" rank="genus">
