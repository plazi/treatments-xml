<caption id="DF2E660D6B49647038FDFA4EFE15F9AD" ID-DOI="http://doi.org/10.5281/zenodo.3999899" ID-Zenodo-Dep="3999899" httpUri="https://zenodo.org/record/3999899/files/figure.png" pageId="22" pageNumber="2089" startId="22.[212,279,1527,1549]" targetBox="[222,1154,171,1478]" targetPageId="22">
<paragraph id="8BEE36856B49647038FDFA4EFE15F9AD" blockId="22.[212,1172,1527,1608]" pageId="22" pageNumber="2089">
