<subSubSection id="D5C4E1C3804FFFECEDACCBC7184108EB" pageId="75" pageNumber="76" type="distribution">
<paragraph id="9D61B248804FFFECEDACCBC7184108EB" blockId="75.[151,1436,1410,1687]" pageId="75" pageNumber="76">
<emphasis id="AFAA6E5A804FFFECEDACCBC718A6087B" bold="true" box="[199,507,1518,1543]" pageId="75" pageNumber="76">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA804FFFECEF6CCBC61BC6087B" box="[519,667,1518,1543]" country="Papua New Guinea" name="New Ireland" pageId="75" pageNumber="76">New Ireland</collectingRegion>
