meters upstream, at the foot of Mount Ibuki, Ueno, Maibara, Shiga Prefecture 
<geoCoordinate id="EE7397B0BE407018AF7EE14BFC11F9DC" box="[836,1016,1571,1595]" direction="north" orientation="latitude" pageId="7" pageNumber="22" precision="1" value="35.391945">
N 
<number id="3D8FE350BE407018AF63E14BFC9DF9DC" box="[857,884,1571,1595]" pageId="7" pageNumber="22" value="35.0">35</number>
º 
<number id="3D8FE350BE407018AFBDE14BFC4EF9DC" box="[903,935,1571,1595]" pageId="7" pageNumber="22" value="23.0">23</number>
’ 31.0”
</geoCoordinate>
, 
<geoCoordinate id="EE7397B0BE407018A83CE14BFB2CF9DC" box="[1030,1221,1571,1595]" direction="east" orientation="longitude" pageId="7" pageNumber="22" precision="1" value="136.39268">
E 
<number id="3D8FE350BE407018A822E14BFBA8F9DC" box="[1048,1089,1571,1595]" pageId="7" pageNumber="22" value="136.0">136</number>
º 
<number id="3D8FE350BE407018A86FE14BFB9CF9DC" box="[1109,1141,1571,1595]" pageId="7" pageNumber="22" value="23.0">23</number>
’ 
<number id="3D8FE350BE407018A8BFE14BFB53F9DC" box="[1157,1210,1571,1595]" pageId="7" pageNumber="22" value="33.7">33.7</number>
”
</geoCoordinate>
, altitude 
<quantity id="4CBF5C92BE407018A908E14BFA96F9DC" box="[1330,1407,1571,1595]" metricMagnitude="2" metricUnit="m" metricValue="3.46" pageId="7" pageNumber="22" unit="m" value="346.0">346 m</quantity>
, 
<date id="FFF9D7B7BE407018A9B4E14BFEC2F9B8" pageId="7" pageNumber="22" value="2010-08-02">
<number id="3D8FE350BE407018A9B4E14BFA75F9DC" box="[1422,1436,1571,1595]" pageId="7" pageNumber="22" value="2.0">2</number>
