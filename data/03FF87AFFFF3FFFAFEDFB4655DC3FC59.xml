<taxonomicName id="4C564D3AFFF3FFFAFEDFB41E5CEBFF45" authority="Matile, 1973: 202" authorityName="Matile" authorityPageNumber="202" authorityYear="1973" box="[264,766,358,382]" class="Insecta" family="Mycetophilidae" genus="Mycomyiella" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="camerounensis">
