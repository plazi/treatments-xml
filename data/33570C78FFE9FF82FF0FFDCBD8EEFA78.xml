<number id="0D36AF49FFE9FF82FE03FA83DDD9FAE8" box="[411,474,1349,1375]" pageId="8" pageNumber="9" value="1889.0">1889</number>
)
</bibRefCitation>
recorded 
<taxonomicName id="7CFEC6EDFFE9FF82FDC6FA83DFB0FAE9" box="[606,947,1349,1374]" class="Malacostraca" family="Ocypodidae" genus="Gelasimus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="chlorophthalmus">
