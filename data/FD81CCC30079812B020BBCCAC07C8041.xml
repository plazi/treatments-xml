<taxonomicName id="A6E1B6AC0083FFB8D77985730F43E3CB" authorityName="Latreille" authorityYear="1802" class="Insecta" genus="Cerceris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
