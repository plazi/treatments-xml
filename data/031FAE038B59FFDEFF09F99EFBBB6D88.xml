<taxonomicName id="4CB664968B59FFDEFE37F827FDD66D88" authorityName="Becker" authorityYear="1909" box="[425,600,1958,1984]" class="Insecta" family="Chloropidae" genus="Elachiptereicus" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
