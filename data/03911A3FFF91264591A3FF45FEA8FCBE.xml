<quantity id="4CC006CCFF9126459186FD24FEADFCBE" box="[178,266,760,787]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="25" pageNumber="496" unit="m" value="1000.0">1000 m</quantity>
