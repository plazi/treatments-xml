<taxonomicName id="EF96ABB910E39C2249F1A13635AA2816" class="Liliopsida" family="Amaryllidaceae" genus="Allium" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="566" phylum="Tracheophyta" rank="species" species="ochroleucum">
<italic id="CEFFD53311E59B209A2FC2F0FF52914A" pageId="null" pageNumber="566">A. ochroleucum</italic>
</taxonomicName>
(Nr. 11 c)
</td>
</tr>
