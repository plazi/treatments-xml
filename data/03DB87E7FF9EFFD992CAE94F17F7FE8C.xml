57. 
<taxonomicName id="4C724D72FF9EFFDB92F0E94F14CEF890" authority="(Spegazzini 1923: 168) Seigler &amp; Ebinger (2006: 62)" authorityName="Seigler &amp; Ebinger" authorityPageNumber="62" authorityYear="2006" baseAuthorityName="Spegazzini" baseAuthorityPageNumber="168" baseAuthorityYear="1923" box="[178,1011,1850,1877]" class="Magnoliopsida" family="Fabaceae" genus="Senegalia" kingdom="Plantae" order="Fabales" pageId="24" pageNumber="269" phylum="Tracheophyta" rank="species" species="parviceps">
<emphasis id="B906EAE3FF9EFFDB92F0E94F16A5F891" bold="true" box="[178,408,1850,1877]" italics="true" pageId="24" pageNumber="269">Senegalia parviceps</emphasis>
(
<bibRefCitation id="EFE34B00FF9EFFDB93E4E94F15A6F890" author="Spegazzini, C. L." box="[422,667,1850,1877]" pageId="24" pageNumber="269" pagination="1 - 7" refId="ref24045" refString="Spegazzini, C. L. (1923) Algunos observaciones relativas al suborden de las Mimosoideas. Physis (Buenos Aires) 6: 1 - 7." type="journal article" year="1923">Spegazzini 1923: 168</bibRefCitation>
