<taxonomicName id="3DAC4D52FFF78F7B92E9E08F1233F83E" authority="(Torres 1960)" baseAuthorityName="Torres" baseAuthorityYear="1960" box="[473,806,1937,1962]" class="Insecta" family="Cicadidae" genus="Pachypsaltria" kingdom="Animalia" order="Hemiptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="haematodes">
