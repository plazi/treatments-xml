<caption id="E3AF6634FFE18054FF227DF6A18CFA2F" ID-DOI="http://doi.org/10.5281/zenodo.5335699" ID-Zenodo-Dep="5335699" box="[151,1270,1503,1527]" httpUri="https://zenodo.org/record/5335699/files/figure.png" pageId="16" pageNumber="17" startId="16.[151,255,1503,1527]" targetBox="[407,1177,202,1469]" targetPageId="16">
<paragraph id="B76F36BCFFE18054FF227DF6A18CFA2F" blockId="16.[151,1270,1503,1527]" box="[151,1270,1503,1527]" pageId="16" pageNumber="17">
