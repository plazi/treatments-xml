<subSubSection id="C368C4C9FE0BFFECFF59F8803567E2CB" pageId="2" pageNumber="61" type="etymology">
<paragraph id="8BCD9742FE0BFFECFF59F8803567E2CB" blockId="2.[151,1436,1719,1985]" pageId="2" pageNumber="61">
<emphasis id="B9064B50FE0BFFECFF59F8803714E293" bold="true" box="[198,331,1919,1945]" pageId="2" pageNumber="61">Etymology</emphasis>
