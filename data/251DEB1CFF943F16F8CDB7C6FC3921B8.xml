<taxonomicName id="6AB42189FF943F11F8CDB7C6FEB02537" authorityName="Neto &amp; Ribeiro &amp; Passos" authorityYear="2020" box="[151,357,1261,1288]" class="Insecta" family="Hydroptilidae" genus="Flintiella" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="286" phylum="Arthropoda" rank="species" species="triaena">
<emphasis id="9FC08618FF943F11F8CDB7C6FEB02537" bold="true" box="[151,357,1261,1288]" italics="true" pageId="3" pageNumber="286">Flintiella triaena</emphasis>
