<subSubSection id="20F265117132FFEF06E4FD881438FD70" pageId="10" pageNumber="103" type="biology_ecology">
<paragraph id="6857369A7132FFEF06E4FD881438FD70" blockId="10.[151,1436,223,682]" pageId="10" pageNumber="103">
<emphasis id="5A9CEA887132FFEF06E4FD88163BFDB8" bold="true" box="[199,300,583,609]" pageId="10" pageNumber="103">Ecology.</emphasis>
