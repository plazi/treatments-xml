<taxonomicName id="C482A1EF20F2E8045F12954D0CCB9446" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vaccinium myrtillus" order="Ericales" pageId="0" pageNumber="1068" phylum="Tracheophyta" rank="species" species="myrtillus">Vaccinium myrtillus</taxonomicName>
