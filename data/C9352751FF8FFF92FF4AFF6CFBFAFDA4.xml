<location id="4443C09CFF8FFF92FE35FEBEFD92FECC" LSID="urn:lsid:plazi:treatment:C9352751FF8FFF92FF4AFF6CFBFAFDA4:4443C09CFF8FFF92FE35FEBEFD92FECC" box="[488,617,325,350]" country="Mauritius" name="Depository" pageId="30" pageNumber="481">Depository</location>
