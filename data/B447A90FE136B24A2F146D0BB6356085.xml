<taxonomicName id="7402D83C7755016ECBB306E11DAC00FA" LSID="http://zoobank.org/01990EEF-7F33-4A35-8809-E6151F7708E1" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus leiponotaulus" order="Hymenoptera" pageId="363" pageNumber="364" phylum="Arthropoda" rank="species" species="leiponotaulus">Heterospilus leiponotaulus Marsh</taxonomicName>
