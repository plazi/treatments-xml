, and S to 
<collectingCountry id="F3037624C663A211F312F0C6F2824CC5" box="[3306,3437,3932,3971]" name="Gabon" pageId="21" pageNumber="248">Gabon</collectingCountry>
, 
<collectingCountry id="F3037624C663A211F27BF0C6F71A4CF1" name="Republic of the Congo" pageId="21" pageNumber="248">Republic of the Congo</collectingCountry>
, 
<collectingCountry id="F3037624C663A211F6F3F00AF6374CF1" box="[2315,2520,3984,4023]" name="Democratic Republic of the Congo" pageId="21" pageNumber="248">DR Congo</collectingCountry>
, and 
<collectingCountry id="F3037624C663A211F5BBF00AF51E4CF1" box="[2627,2801,3984,4023]" name="Tanzania" pageId="21" pageNumber="248">Tanzania</collectingCountry>
