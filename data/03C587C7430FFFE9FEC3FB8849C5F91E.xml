<taxonomicName id="4C6C4D52430FFFEFFCF6F80F4BE6F877" authorityName="Merrem" authorityYear="1820" box="[842,938,1939,1957]" class="Reptilia" family="Anguidae" genus="Pseudopus" kingdom="Animalia" order="Squamata" pageId="11" pageNumber="352" phylum="Chordata" rank="genus">
