<taxonomicName id="4C0974AE6482FF37FF36FBA8FDE95332" authority="Gloger, 1841" authorityName="Gloger" authorityYear="1841" box="[238,552,1086,1112]" class="Mammalia" family="Delphinidae" genus="Lissodelphis" kingdom="Animalia" order="Cetacea" pageId="136" pageNumber="137" phylum="Chordata" rank="genus">
