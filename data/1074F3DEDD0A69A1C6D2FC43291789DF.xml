<taxonomicName id="083BC4A723A2366B74547D6B9BD96B32" LSID="http://zoobank.org/23E32F56-CAF3-4B3D-938A-0BF6A590C070" authority="Conway, Stewart &amp; Summers, 2018" authorityName="Conway, Stewart &amp; Summers" authorityYear="2018" class="Actinopterygii" family="Gobiesocidae" genus="Flexor" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Flexor" order="Gobiesociformes" pageId="0" pageNumber="75" phylum="Chordata" rank="genus">Flexor</taxonomicName>
