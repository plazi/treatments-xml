, 
<quantity id="192F88E8AC1CD3449D84FF76FBB0FF53" box="[978,1045,189,215]" metricMagnitude="4" metricUnit="m" metricValue="2.1" pageId="3" pageNumber="203" unit="km" value="21.0">21km</quantity>
