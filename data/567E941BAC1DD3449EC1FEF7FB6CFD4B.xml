<collectingCountry id="A6C0659DAC1CD3449F31FECEFE04FE9B" box="[359,417,261,287]" name="India" pageId="3" pageNumber="203">India</collectingCountry>
(
<collectingRegion id="1C13EBEFAC1CD3449FF9FECEFD9EFEA4" box="[431,571,261,288]" country="India" name="Tamil Nadu" pageId="3" pageNumber="203">Tamil Nadu</collectingRegion>
).
</paragraph>
