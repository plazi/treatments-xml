<taxonomicName id="58766E9603A08DB5B9CAC84F94EE5385" class="Insecta" family="Braconidae" genus="Dinotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dinotrema" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Dinotrema</taxonomicName>
