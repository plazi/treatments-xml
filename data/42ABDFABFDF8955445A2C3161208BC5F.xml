<taxonomicName id="316F7048C36B030BEDC22139BDCC4CE9" LSID="urn:lsid:zoobank.org:act:2549F9C7-9FEF-4BF6-B044-783CA798F6F5" authority="Li &amp; Wang" class="Insecta" family="Perlidae" genus="Neoperla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neoperla similidella" order="Plecoptera" pageId="3" pageNumber="24" phylum="Arthropoda" rank="species" species="similidella">Neoperla similidella Li &amp; Wang</taxonomicName>
<taxonomicNameLabel id="66381CBCF5FE5E8661E46FDDBD50E193" pageId="3" pageNumber="24">sp. n.</taxonomicNameLabel>
