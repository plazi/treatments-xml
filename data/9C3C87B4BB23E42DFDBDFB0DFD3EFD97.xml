<paragraph id="142A36A2BB23E42CFF33FA3CFE37FA59" blockId="24.[189,1398,1454,1515]" pageId="24" pageNumber="25">
<collectingCountry id="6C827632BB23E42CFF33FA3CFEA4FA7A" box="[189,346,1454,1480]" name="Malaysia" pageId="24" pageNumber="25">MALAYSIA</collectingCountry>
