<collectionCode id="1F80AE44FFDA8416FC70500CFB958B81" box="[943,1029,367,393]" country="New Zealand" httpUri="http://grbio.org/cool/15kb-kde6" name="Museum of New Zealand Te Papa Tongarewa - Entomology" pageId="14" pageNumber="15" type="Museum">MONZ</collectionCode>
