<subSubSection id="C318DBDAFFF5FFE743F2FA0598E89592" box="[199,1256,1412,1437]" pageId="7" pageNumber="160" type="etymology">
<paragraph id="8BBD8851FFF5FFE743F2FA0598E89592" blockId="7.[151,1437,151,1473]" box="[199,1256,1412,1437]" pageId="7" pageNumber="160">
<emphasis id="B9765443FFF5FFE743F2FA059D4E9592" bold="true" box="[199,334,1412,1437]" pageId="7" pageNumber="160">Etymology.</emphasis>
