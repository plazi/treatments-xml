<taxonomicName id="29C34A08F71DB3D429C88D52CF2FD991" class="Dicotyledoneae" family="Apocynaceae" genus="Calotropis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Calotropis procera" order="Gentianales" pageId="41" pageNumber="42" phylum="Angiospermae" rank="species" species="procera">
<emphasis id="0D30EAB57AD039B2F55B9B4B25D601D8" italics="true" pageId="41" pageNumber="42">Calotropis procera</emphasis>
</taxonomicName>
, 
<taxonomicName id="6296E08B2555655B06147F82AFAF0EDD" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="41" pageNumber="42" phylum="Angiospermae" rank="species" species="shawii">
<emphasis id="0DC79F40382B71D5690EF5759FB5A793" italics="true" pageId="41" pageNumber="42">Lycium shawii</emphasis>
</taxonomicName>
