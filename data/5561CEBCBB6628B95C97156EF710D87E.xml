<taxonomicName id="F8176C407AB136BD9286F07FDC051422" authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="41" pageNumber="42" phylum="Angiospermae" rank="species" species="stricta">
