and 
<taxonomicName id="F3D4158038A76D1012A8E23BFA09550F" class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="nummularia">
