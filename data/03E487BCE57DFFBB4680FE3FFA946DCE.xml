<number id="3D85248DE57DFFB942B0F927FB656916" box="[1191,1236,1772,1798]" pageId="8" pageNumber="51" value="2.2">2, 2</number>
, tibia 
<number id="3D85248DE57DFFB9430AF927FAFD6916" box="[1309,1356,1772,1798]" pageId="8" pageNumber="51" value="2.2">2, 2</number>
