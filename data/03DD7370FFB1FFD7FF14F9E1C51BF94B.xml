<caption id="DF0B92EEFFBAFFD7FF14FBEAC1C7FB4C" pageId="44" pageNumber="45" startId="44.[151,255,1145,1169]" targetBox="[172,1416,194,1121]" targetPageId="44">
<paragraph id="8BCBC266FFBAFFD7FF14FBEAC1C7FB4C" blockId="44.[151,1435,1145,1203]" pageId="44" pageNumber="45">
<emphasis id="B9001E74FFBAFFD7FF14FBEAC03FFB6E" bold="true" box="[151,295,1145,1169]" pageId="44" pageNumber="45">FIGURE 34.</emphasis>
<taxonomicName id="4C74B9E5FFBAFFD7FEADFBE9C0F2FB6E" authority="David &amp; Vogel &amp; van Rooijen, 2008" authorityName="David &amp; Vogel &amp; van Rooijen" authorityYear="2008" box="[302,490,1146,1169]" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="44" pageNumber="45" phylum="Chordata" rank="species" species="moricei" status="spec. nov.">
<emphasis id="B9001E74FFBAFFD7FEADFBE9C0F2FB6E" box="[302,490,1146,1169]" italics="true" pageId="44" pageNumber="45">Oligodon moricei</emphasis>
</taxonomicName>
<emphasis id="B9001E74FFBAFFD7FE71FBEAC378FB6E" bold="true" box="[498,608,1145,1169]" pageId="44" pageNumber="45">
<taxonomicNameLabel id="A233A30FFFBAFFD7FE71FBEAC378FB6E" box="[498,608,1145,1169]" pageId="44" pageNumber="45" rank="species">spec. nov.</taxonomicNameLabel>
