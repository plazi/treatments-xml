<taxonomicName id="46CC4D7DEC0B120CFC5C67A0FBA7778F" authorityName="DeCandolle" authorityYear="1830" box="[954,1080,1965,1990]" class="Magnoliopsida" family="Apiaceae" genus="Ferula" kingdom="Plantae" order="Apiales" pageId="2" pageNumber="37" phylum="Tracheophyta" rank="species" species="rigidula">
