<taxonomicName id="4C6B4D1EFFA7BB38708EFD31FF18FD4E" class="Branchiopoda" genus="Alona" kingdom="Animalia" order="Diplostraca" pageId="8" pageNumber="459" phylum="Arthropoda" rank="species" species="cheni">
