, 
<quantity id="C55FC3EB8C1A602FFD858C7AFD2EFCF6" box="[593,667,849,870]" metricMagnitude="2" metricUnit="m" metricValue="3.85" pageId="76" pageNumber="209" unit="m" value="385.0">
<elevation id="898A893D8C1A602FFD858C7AFD2EFCF6" box="[593,667,849,870]" metricMagnitude="2" metricUnit="m" metricValue="3.85" pageId="76" pageNumber="209" unit="m" value="385.0">385 m</elevation>
