<taxonomicName id="D8280606D5DBAEC3A061456993D9F655" LSID="http://zoobank.org/90BC4041-BD4F-4B19-A729-D66AFD4FE46B" class="Insecta" family="Tettigoniidae" genus="Melidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Melidia claudiae" order="Orthoptera" pageId="6" pageNumber="83" phylum="Arthropoda" rank="species" species="claudiae">Melidia claudiae</taxonomicName>
