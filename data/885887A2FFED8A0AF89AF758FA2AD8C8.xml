<collectingCountry id="78E67624FFED8A0BF3CDF424F3F3D2CF" box="[3073,3190,2949,2988]" name="Nepal" pageId="34" pageNumber="313">Nepal</collectingCountry>
, 
<collectingCountry id="78E67624FFED8A0BF345F424F29CD2CF" box="[3209,3353,2949,2988]" name="Bhutan" pageId="34" pageNumber="313">Bhutan</collectingCountry>
, NE India (Arunachal Pradesh and Meghalaya), N 
<collectingCountry id="78E67624FFED8A0BF84FF44EF7E5D575" box="[1923,2144,3055,3094]" name="Bangladesh" pageId="34" pageNumber="313">Bangladesh</collectingCountry>
