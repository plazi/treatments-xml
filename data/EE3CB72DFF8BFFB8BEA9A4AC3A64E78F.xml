</taxonomicName>
<emphasis id="54E1DA29FF89FFB8BA7DA7E93AD6E4F7" bold="true" box="[1091,1210,151,177]" pageId="23" pageNumber="474">
<taxonomicNameLabel id="4FD26752FF89FFB8BA7DA7E93AD6E4F7" box="[1091,1210,151,177]" pageId="23" pageNumber="474" rank="species">spec. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="A1957DB8FF89FFB8BAC5A7E93B11E4F7" authorityName="Chamberlin" authorityYear="1924" box="[1275,1405,151,177]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="474" phylum="Arthropoda" rank="species" species="tardatus">
