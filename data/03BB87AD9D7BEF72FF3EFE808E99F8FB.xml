<taxonomicName id="4C124D389D7BEF72FBFDF9E08E99F9D6" authority="Yang, Hu and Zhu" authorityName="Yang, Hu and Zhu" authorityYear="2003" box="[1108,1436,1550,1571]" class="Insecta" family="Lauxaniidae" genus="Homoneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="didyma" subGenus="Homoneura">
