<taxonomicName id="3FE707A3FFEE216CFE26EDEDCF83D885" authority="Erseus and Milligan, 1988" authorityName="Erseus and Milligan" authorityYear="1988" box="[417,944,975,997]" class="Clitellata" family="Naididae" genus="Doliodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="25" pageNumber="293" phylum="Annelida" rank="species" species="puertoricensis">
