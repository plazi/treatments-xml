<caption id="AC982CA8FFEE216CFEBEEDEDCF50D89E" ID-DOI="http://doi.org/10.5281/zenodo.5258642" ID-Zenodo-Dep="5258642" httpUri="https://zenodo.org/record/5258642/files/figure.png" pageId="25" pageNumber="293" startId="25.[313,329,975,996]" targetBox="[314,1272,181,947]" targetPageId="25">
<paragraph id="F8587C20FFEE216CFEBEEDEDCF50D89E" blockId="25.[313,1268,975,1023]" pageId="25" pageNumber="293">
