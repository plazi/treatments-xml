<taxonomicName id="4C434D30FF8A120AF0AC8CF2373EF29B" authority="Boulenger" authorityName="Boulenger" box="[931,1024,428,453]" class="Amphibia" family="Hylidae" genus="Agalychnis" kingdom="Animalia" order="Anura" pageId="35" pageNumber="36" phylum="Chordata" rank="species" species="buckleyi">
