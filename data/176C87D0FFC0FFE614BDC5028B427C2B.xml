<taxonomicName id="58C54D45FFC0FFE614BDC5BF8D1C7AA6" authority="Menke, 1964:18" authorityName="Menke" authorityPageNumber="18" authorityYear="1964" box="[151,581,666,689]" class="Insecta" family="Sphecidae" genus="Ammophila" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="37" phylum="Arthropoda" rank="species" species="californica">
<emphasis id="ADB1EAD4FFC0FFE614BDC5BF8ED47AA6" box="[151,397,666,689]" italics="true" pageId="6" pageNumber="37">Ammophila californica</emphasis>
<treatmentCitation id="1E6410D7FFC0FFE615BEC5BF8D1C7AA6" author="Menke" box="[404,581,666,689]" page="18" pageId="6" pageNumber="37" year="1964">
<bibRefCitation id="FB544B37FFC0FFE615BEC5BF8D7E7AA6" author="Menke" box="[404,551,666,689]" pageId="6" pageNumber="38" refString="Menke, A. S. (1964) New species of North American Ammophila (Hymenoptera, Sphecidae). Acta Hymenopterologica, 2 (1), 5 - 27 (dated Dec. 1, 1963 in error)." type="journal article" year="1964">
Menke, 
<number id="290D24E1FFC0FFE615C1C5BF8D7E7AA6" box="[491,551,666,689]" pageId="6" pageNumber="37" value="1964.0">1964</number>
