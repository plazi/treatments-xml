<taxonomicName id="0FF4F51D51CA4BA589BE654871EC8D7B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:132739" authority="Smith, 1857" authorityName="Smith" authorityYear="1857" class="Insecta" family="Formicidae" genus="Typhlatta" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Typhlatta laeviceps Smith" lsidName-HNS="Typhlatta laeviceps Smith" order="Hymenoptera" pageNumber="49" phylum="Arthropoda" rank="species" species="laeviceps">Typhlatta laeviceps</taxonomicName>
