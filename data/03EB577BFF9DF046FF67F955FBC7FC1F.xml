<emphasis id="B9363A7FFF9AF046FF67FC69FEA5FC38" bold="true" box="[192,324,960,986]" pageId="44" pageNumber="43">Etymology.</emphasis>
<taxonomicName id="4C429DEEFF9AF046FEECFC69FD9EFC38" authorityName="Phillips and Brown" authorityYear="2021" box="[331,639,960,986]" class="Insecta" family="Oecophoridae" genus="Chlamydastis" kingdom="Animalia" order="Lepidoptera" pageId="44" pageNumber="43" phylum="Arthropoda" rank="species" species="noramartinae">
<emphasis id="B9363A7FFF9AF046FEECFC69FD9EFC38" box="[331,639,960,986]" italics="true" pageId="44" pageNumber="43">Chlamydastis noramartinae</emphasis>
</taxonomicName>
