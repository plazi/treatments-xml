<quantity id="4CC59B14EF5B4A38A8A58B50FBA6F881" box="[955,1061,1843,1870]" metricMagnitude="-4" metricUnit="m" metricValue="1.7" pageId="59" pageNumber="60" unit="mm" value="0.17">0.17 mm</quantity>
