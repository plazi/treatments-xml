<quantity id="4CC59B14EF5B4A38AF768B50FB51F881" box="[1128,1234,1843,1870]" metricMagnitude="-4" metricUnit="m" metricValue="1.6" pageId="59" pageNumber="60" unit="mm" value="0.16">0.16 mm</quantity>
; 
<quantity id="4CC59B14EF5B4A38AFC28B50FA81F882" box="[1244,1282,1843,1869]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="59" pageNumber="60" unit="m" value="2.0">2m</quantity>
