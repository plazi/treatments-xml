<quantity id="4CC59B14EF5B4A38AA2D8B34FE1CF8BE" box="[307,415,1879,1905]" metricMagnitude="-4" metricUnit="m" metricValue="1.2" pageId="59" pageNumber="60" unit="mm" value="0.12">0.12 mm</quantity>
