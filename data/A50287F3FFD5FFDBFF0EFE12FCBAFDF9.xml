<collectingCountry id="55BC7675FFD5FFDBFDF7FE5AFD26FE53" box="[622,707,476,501]" name="Zambia" pageId="56" pageNumber="57">Zambia</collectingCountry>
(Larsen, 2005). 
<location id="2874603EFFD5FFDBFCE7FE5AFC4FFE53" LSID="urn:lsid:plazi:treatment:A50287F3FFD5FFDBFF0EFE12FCBAFDF9:2874603EFFD5FFDBFCE7FE5AFC4FFE53" box="[894,938,476,501]" country="Democratic Republic of the Congo" county="Senegal" municipality="Uganda" name="The" pageId="56" pageNumber="57">The</location>
caterpillars also feed 
<collectedFrom id="9EB77518FFD5FFDBFB06FE5BFACBFE53" box="[1183,1326,476,501]" pageId="56" pageNumber="57">
on 
<taxonomicName id="EAAB4D66FFD5FFDBFB5BFE5AFACBFE53" box="[1218,1326,476,501]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="56" pageNumber="57" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
</collectedFrom>
. 
<emphasis id="1FDFEAF7FFD5FFDBFF5EFE79FE6AFDBE" bold="true" box="[199,399,511,536]" pageId="56" pageNumber="57">
<location id="2874603EFFD5FFDBFF5EFE79FEC7FDBE" LSID="urn:lsid:plazi:treatment:A50287F3FFD5FFDBFF0EFE12FCBAFDF9:2874603EFFD5FFDBFF5EFE79FEC7FDBE" box="[199,290,511,536]" country="Democratic Republic of the Congo" county="Senegal" municipality="Uganda" name="Studied" pageId="56" pageNumber="57">Studied</location>
