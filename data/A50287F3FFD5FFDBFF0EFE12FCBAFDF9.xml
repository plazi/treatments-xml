WL: 
<quantity id="EA539B00FFD5FFDBFE9BFE3EFEA8FE76" box="[258,333,440,464]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="56" pageNumber="57" unit="mm" value="19.0">19mm</quantity>
. 
<taxonomicName id="EAAB4D66FFD5FFDBFEC1FE3FFE44FE76" box="[344,417,441,464]" class="Insecta" family="Hesperiidae" genus="Fresna" kingdom="Animalia" order="Lepidoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="cojo">
