</taxonomicName>
is known in all 
<typeStatus id="F2108847FFD5FFDBFDC2FE3EFD73FE76" box="[603,662,440,464]" pageId="56" pageNumber="57">types</typeStatus>
of forest and ranges from 
<collectingCountry id="55BC7675FFD5FFDBFC5BFE31FBFEFE76" box="[962,1051,439,464]" name="Senegal" pageId="56" pageNumber="57">
<collectingCounty id="C4754E69FFD5FFDBFC5BFE31FBFEFE76" box="[962,1051,439,464]" pageId="56" pageNumber="57">Senegal</collectingCounty>
</collectingCountry>
to 
<collectionCode id="4BBAAE20FFD5FFDBFBA6FE3EFB9EFE76" box="[1087,1147,440,464]" pageId="56" pageNumber="57">
<collectingCountry id="55BC7675FFD5FFDBFBA6FE3EFB9EFE76" box="[1087,1147,440,464]" name="Democratic Republic of the Congo" pageId="56" pageNumber="57">DRC</collectingCountry>
</collectionCode>
, 
<collectingCountry id="55BC7675FFD5FFDBFB11FE31FB07FE76" box="[1160,1250,439,464]" name="Uganda" pageId="56" pageNumber="57">
<collectingMunicipality id="CD70AC9FFFD5FFDBFB11FE31FB07FE76" box="[1160,1250,439,464]" pageId="56" pageNumber="57">Uganda</collectingMunicipality>
</collectingCountry>
, 
<location id="2874603EFFD5FFDBFB74FE3EFA7DFE76" LSID="urn:lsid:plazi:treatment:A50287F3FFD5FFDBFF0EFE12FCBAFDF9:2874603EFFD5FFDBFB74FE3EFA7DFE76" box="[1261,1432,440,464]" country="Democratic Republic of the Congo" county="Senegal" municipality="Uganda" name="western Kenya" pageId="56" pageNumber="57">
western 
<collectingCountry id="55BC7675FFD5FFDBFAD5FE3EFA7DFE76" box="[1356,1432,440,464]" name="Kenya" pageId="56" pageNumber="57">Kenya</collectingCountry>
</location>
, 
<location id="2874603EFFD5FFDBFF0EFE5AFE71FE53" LSID="urn:lsid:plazi:treatment:A50287F3FFD5FFDBFF0EFE12FCBAFDF9:2874603EFFD5FFDBFF0EFE5AFE71FE53" box="[151,404,476,501]" country="Democratic Republic of the Congo" county="Senegal" municipality="Uganda" name="northwestern Tanzania" pageId="56" pageNumber="57">
northwestern 
<collectingCountry id="55BC7675FFD5FFDBFEA9FE5AFE71FE53" box="[304,404,476,501]" name="Tanzania" pageId="56" pageNumber="57">Tanzania</collectingCountry>
</location>
