<quantity id="AE787F8DFF95A44CFE29F8E6FDD5F8AF" box="[505,591,1848,1875]" metricMagnitude="-2" metricUnit="m" metricValue="2.9" pageId="15" pageNumber="16" unit="mm" value="29.0">29 mm</quantity>
