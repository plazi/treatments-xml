<taxonomicName id="AE80A9EBFF8AA453FBC5FF47FADCFF4F" baseAuthorityName="Valenciennes" baseAuthorityYear="1847" box="[1045,1350,153,179]" class="Actinopterygii" family="Hemiramphidae" genus="Hyporhamphus" kingdom="Animalia" order="Beloniformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="dussumieri">
<emphasis id="5BF40E7AFF8AA453FBC5FF47FADCFF4F" box="[1045,1350,153,179]" italics="true" pageId="16" pageNumber="17">Hyporhamphus dussumieri</emphasis>
</taxonomicName>
(previously 
<taxonomicName id="AE80A9EBFF8AA453FF0BFF60FECEFF2B" baseAuthorityName="Günther" baseAuthorityYear="1866" box="[219,340,189,215]" class="Actinopterygii" family="Hemiramphidae" genus="Hyporhamphus" kingdom="Animalia" order="Beloniformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="laticeps">
<emphasis id="5BF40E7AFF8AA453FF0BFF60FF75FF2B" box="[219,239,190,215]" italics="true" pageId="16" pageNumber="17">H</emphasis>
