<taxonomicName id="AE80A9EBFF8AA453FD97FF47FC9AFF4F" authorityName="T.N.Gill" authorityYear="1859" box="[583,768,153,179]" class="Actinopterygii" family="Hemiramphidae" kingdom="Animalia" order="Beloniformes" pageId="16" pageNumber="17" phylum="Chordata" rank="family">Hemiramphidae</taxonomicName>
