<collectingRegion id="49F4F1A3FFD7FFE68848FB90FEDDBF4A" box="[859,1002,1087,1113]" country="Thailand" name="Chaiyaphum" pageId="8" pageNumber="339">Chaiyaphum</collectingRegion>
