<taxonomicName id="D5B54D03C734EF793A84FCFDFEC7F4D9" authority="Sg. Bebar" authorityName="Sg. Bebar" class="Insecta" family="Coenagrionidae" genus="Amphicnemis" kingdom="Animalia" order="Odonata" pageId="7" pageNumber="52" phylum="Arthropoda" rank="species" species="gracilis">
