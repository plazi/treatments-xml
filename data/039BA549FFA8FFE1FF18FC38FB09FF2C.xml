and 
<taxonomicName id="4C326FDCFFA9FFE1FCD8FAF2FC08FF5D" box="[855,982,152,174]" class="Insecta" family="Coenagrionidae" genus="Protallagma" kingdom="Animalia" order="Odonata" pageId="19" pageNumber="47" phylum="Arthropoda" rank="species" species="hoffmanni">
