<quantity id="1F1E319FFFDFFF8F95E1F407FCED193E" box="[712,810,1054,1080]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="6" pageNumber="33" unit="m" value="1600.0">1,600 m</quantity>
