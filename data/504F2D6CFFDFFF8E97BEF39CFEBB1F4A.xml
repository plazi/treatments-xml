<collectionCode id="BEF704BFFFDFFF8F95FBF45AFCDB195B" LSID="urn:lsid:biocol.org:col:34797" box="[722,796,1091,1117]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="6" pageNumber="33">INHS</collectionCode>
