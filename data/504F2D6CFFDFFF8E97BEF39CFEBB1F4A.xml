o 
<number id="6E2E8E5DFFDFFF8F9273F3ECFABB1909" box="[1370,1404,1013,1039]" pageId="6" pageNumber="33" value="24.0">24</number>
'N
</geoCoordinate>
, 
<geoCoordinate id="BDD2FABDFFDFFF8F97BEF407FECC193E" box="[151,267,1051,1080]" direction="west" orientation="longitude" pageId="6" pageNumber="33" precision="925" value="-145.51666">
