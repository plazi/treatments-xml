<taxonomicName id="3B844D48FFC8FF94F2ACFD2E0067FD47" authority="Rondani, 1871" authorityName="Rondani" authorityYear="1871" box="[644,792,724,750]" class="Insecta" family="Muscidae" genus="Pogonomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">Pogonomyia</taxonomicName>
