<typeStatus id="233F8869FFC8FF94F3A2FC660086FC1F" box="[906,1017,924,950]" pageId="2" pageNumber="3">lectotype</typeStatus>
. One 
<typeStatus id="233F8869FFC8FF94F46FFC66079AFC1F" box="[1095,1253,924,950]" pageId="2" pageNumber="3">paralectotype</typeStatus>
