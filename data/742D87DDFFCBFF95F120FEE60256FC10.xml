</specimenCount>
and 
<specimenCount id="EA82FD42FFC8FF94F44CFC8E07A2FC27" box="[1124,1245,884,910]" pageId="2" pageNumber="3" type="female">
<number id="4A4C24ECFFC8FF94F44CFC8E070CFC27" box="[1124,1139,884,910]" pageId="2" pageNumber="3" value="2.0">2</number>
