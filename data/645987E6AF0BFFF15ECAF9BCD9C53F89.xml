<emphasis id="DE84EAE2AF0AFFF15ECAFEDEDC053FAD" box="[151,279,367,392]" italics="true" pageId="43" pageNumber="44">Rhynocoris</emphasis>
</taxonomicName>
spp. (
<taxonomicName id="2BF04D73AF0AFFF15F38FEC1DCFD3FAD" box="[357,495,368,392]" class="Insecta" family="Sphaeroceridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="order">Heteroptera</taxonomicName>
