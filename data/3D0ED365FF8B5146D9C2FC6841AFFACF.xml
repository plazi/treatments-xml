<taxonomicName id="72A719F0FF8B5146D9C2FC6842B7FC19" authority="Banks. This" authorityName="Banks. This" box="[189,511,935,959]" class="Insecta" family="Capniidae" genus="Nemocapnia" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="38" phylum="Arthropoda" rank="species" species="carolina">
<emphasis id="87D3BE61FF8B5146D9C2FC6842EDFC19" box="[189,421,935,959]" italics="true" pageId="7" pageNumber="38">Nemocapnia carolina</emphasis>
