<taxonomicName id="ABFB4D7AFFE6430BFEB4F8C980145256" authority="Wu &amp; Hwang &amp; Lin, 2008" authorityName="Wu &amp; Hwang &amp; Lin" authorityYear="2008" box="[294,513,1853,1872]" class="Gastropoda" family="Camaenidae" genus="Satsuma" kingdom="Animalia" order="Stylommatophora" pageId="19" pageNumber="456" phylum="Mollusca" rank="species" species="phoenicis" status="sp. nov.">
<emphasis id="5E8FEAEBFFE6430BFEB4F8C980145256" bold="true" box="[294,513,1853,1872]" italics="true" pageId="19" pageNumber="456">Satsuma phoenicis</emphasis>
