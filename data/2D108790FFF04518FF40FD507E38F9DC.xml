<number id="137124A1FFF04518FC24FBCE7B35FB8C" box="[1011,1081,1122,1146]" pageId="4" pageNumber="5" value="15.5" valueMax="17.0" valueMin="14.0">14–17</number>
(X= 
<number id="137124A1FFF04518FBAFFBCE7BA0FB8C" box="[1144,1196,1122,1146]" pageId="4" pageNumber="5" value="15.2">15.2</number>
