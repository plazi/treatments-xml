<number id="137124A1FFF04518FD80FBCE7D91FB8C" box="[599,669,1122,1146]" pageId="4" pageNumber="5" value="80.5" valueMax="84.0" valueMin="77.0">77–84</number>
(X= 
<number id="137124A1FFF04518FD0AFBCE7DF0FB8C" box="[733,764,1122,1146]" pageId="4" pageNumber="5" value="81.0">81</number>
; SD= 
<number id="137124A1FFF04518FC91FBCE7C75FB8C" box="[838,889,1122,1146]" pageId="4" pageNumber="5" value="2.83">2.83</number>
