sp 
<specimenCount id="B3BFFD0FFFF04518FE8CFB927EE8FBA0" box="[347,484,1086,1110]" pageId="4" pageNumber="5" type="generic">
<number id="137124A1FFF04518FE8CFB927E65FBA0" box="[347,361,1086,1110]" pageId="4" pageNumber="5" value="4.0">4</number>
specimens
</specimenCount>
have 
<number id="137124A1FFF04518FDF1FB927D60FBA0" box="[550,620,1086,1110]" pageId="4" pageNumber="5" value="57.0" valueMax="61.0" valueMin="53.0">53–61</number>
(X= 
