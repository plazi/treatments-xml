<quantity id="F0319B178155B509999DFA0096912BEE" box="[151,252,1455,1479]" metricMagnitude="1" metricUnit="m" metricValue="1.9" metricValueMax="2.0" metricValueMin="1.8" pageId="15" pageNumber="296" unit="m" value="19.0" valueMax="20.0" valueMin="18.0">
