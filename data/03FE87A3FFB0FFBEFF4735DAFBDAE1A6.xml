<typeStatus id="54EC8817FFB0FFBFFF473677FF73E417" box="[151,204,1566,1589]" pageId="34" pageNumber="35">Type</typeStatus>
species: 
<taxonomicName id="4C574D36FFB0FFBFFEFD3677FD18E417" authority="Meyrick, 1913" authorityName="Meyrick" authorityYear="1913" box="[301,679,1566,1589]" class="Insecta" family="Gelechiidae" genus="Telphusa" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="castrigera">
