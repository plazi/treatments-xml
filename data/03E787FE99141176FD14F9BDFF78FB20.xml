<taxonomicName id="4C4E4D6B99141175FD14F9BDFC87F994" box="[699,820,1587,1610]" class="Insecta" family="Cleridae" genus="Aphelocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="scutellaris">
