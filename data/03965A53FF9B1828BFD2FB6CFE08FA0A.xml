<taxonomicName id="4C3F90C6FF851828BC1DFB54FC5EFB16" authorityName="Sabella" authorityYear="2011" box="[839,935,1249,1270]" class="Insecta" family="Staphylinidae" genus="Tychus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="211" phylum="Arthropoda" rank="species" species="spurius">
