<quantity id="4CC746A0FF851828BEAEFA27FDA6FA4A" box="[500,607,1426,1450]" metricMagnitude="-4" metricUnit="m" metricValue="1.75" pageId="16" pageNumber="211" unit="mm" value="0.175">0.175 mm</quantity>
