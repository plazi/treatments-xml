<taxonomicName id="4C3F90C6FF851828BEEBFB54FDD2FB16" authorityName="Kurbatov" authorityYear="2011" box="[433,555,1248,1270]" class="Insecta" family="Staphylinidae" genus="Tychus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="211" phylum="Arthropoda" rank="species" species="sodalicius">
