<paragraph id="8BEA36B8992D447D07D2F9037854F95C" blockId="20.[151,1437,1730,2045]" box="[151,838,1730,1757]" pageId="20" pageNumber="21">
<emphasis id="B921EAAA992D447D07D2F9037A3BF95C" bold="true" box="[151,297,1730,1757]" pageId="20" pageNumber="21">Description.</emphasis>
<typeStatus id="54EE881A992D447D0675F9037A8EF95D" box="[304,412,1730,1756]" pageId="20" pageNumber="21">Holotype</typeStatus>
. Male. Length 
<quantity id="4CAD9B5D992D447D0501F902798FF95C" box="[580,669,1731,1757]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="20" pageNumber="21" unit="mm" value="3.7">3.7 mm</quantity>
. LFW 
<quantity id="4CAD9B5D992D447D05ACF9037850F95C" box="[745,834,1730,1757]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="20" pageNumber="21" unit="mm" value="3.5">3.5 mm</quantity>
