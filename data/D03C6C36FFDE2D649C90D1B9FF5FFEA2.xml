<taxonomicName id="9F95A6A3FFDD2D649E11D653FFA1FEC4" box="[534,757,264,289]" class="Insecta" family="Geometridae" genus="Percnia" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="grisearia">
