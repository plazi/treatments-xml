<taxonomicName id="E2904D73FA18FFFB5118FD85FDA1BCBA" authorityName="Hutton" authorityYear="1883" box="[313,524,614,640]" class="Gastropoda" family="Endodontidae" genus="Endodonta" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="nerissa">
