<taxonomicName id="4C6D4D5EEF8B3AEFFEF1F904FDF8F922" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[308,573,1737,1763]" class="Maxillopoda" family="Notodelphyidae" genus="Periproctia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="218" pageNumber="219" phylum="Arthropoda" rank="species" species="biunguifera" status="sp. nov.">
