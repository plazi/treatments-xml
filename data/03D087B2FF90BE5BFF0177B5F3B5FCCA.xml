, live in 
<quantity id="4C819B41FF90BE5AFE31758DF579FC1A" box="[450,592,994,1018]" metricMagnitude="3" metricUnit="m" metricValue="1.008" metricValueMax="1.012" metricValueMin="1.004" pageId="116" pageNumber="703" unit="m" value="1008.0" valueMax="1012.0" valueMin="1004.0">
<elevation id="0054D197FF90BE5AFE31758DF579FC1A" box="[450,592,994,1018]" metricMagnitude="3" metricUnit="m" metricValue="1.008" metricValueMax="1.012" metricValueMin="1.004" pageId="116" pageNumber="703" unit="m" value="1008.0" valueMax="1012.0" valueMin="1004.0">1004-1012 m</elevation>
