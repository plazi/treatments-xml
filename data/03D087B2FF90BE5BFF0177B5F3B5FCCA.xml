<collectingCountry id="F36E7634FF90BE5AFE5D75D0F6F9FC34" box="[430,464,959,983]" name="Fiji" pageId="116" pageNumber="703">Fiji</collectingCountry>
and 
<collectingCountry id="F36E7634FF90BE5AFDF075D0F547FC34" box="[515,622,959,983]" name="Indonesia" pageId="116" pageNumber="703">Indonesia</collectingCountry>
(Fig. 58), in 
<quantity id="4C819B41FF90BE5AFF1C758DF659FC1A" box="[239,368,994,1018]" metricMagnitude="3" metricUnit="m" metricValue="1.1184999999999998" metricValueMax="1.71" metricValueMin="0.527" pageId="116" pageNumber="703" unit="m" value="1118.5" valueMax="1710.0" valueMin="527.0">
<elevation id="0054D197FF90BE5AFF1C758DF659FC1A" box="[239,368,994,1018]" metricMagnitude="3" metricUnit="m" metricValue="1.1184999999999998" metricValueMax="1.71" metricValueMin="0.527" pageId="116" pageNumber="703" unit="m" value="1118.5" valueMax="1710.0" valueMin="527.0">527-1710 m</elevation>
