<taxonomicName id="AFD9A5961255FFD8FCA4FF66FB87FF75" box="[871,1058,151,176]" class="Gastropoda" family="Tonnidae" genus="Eudolium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="47" pageNumber="48" phylum="Mollusca" rank="species" species="bairdi">
