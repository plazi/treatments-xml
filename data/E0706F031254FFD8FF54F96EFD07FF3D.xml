<collectionCode id="0EC846D01254FFD9FCAAF84EFC04F812" box="[873,929,1983,2007]" country="Israel" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15399" name="Hebrew University" pageId="46" pageNumber="47">HUJ</collectionCode>
