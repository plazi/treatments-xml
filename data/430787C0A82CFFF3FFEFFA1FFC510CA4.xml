<treatmentCitationGroup id="EBBE11F8A82CFFF3FFC8F992FF0D0FC4" pageId="52" pageNumber="52">
Now 
<taxonomicName id="0CAE4D55A82CFFF3FF36F993FF0E0F6E" authority="(Chapman, 1923)" baseAuthorityName="Chapman" baseAuthorityYear="1923" class="Aves" family="Emberizidae" genus="Arremon" kingdom="Animalia" order="Passeriformes" pageId="52" pageNumber="52" phylum="Chordata" rank="subSpecies" species="torquatus" subSpecies="nigrifrons">
