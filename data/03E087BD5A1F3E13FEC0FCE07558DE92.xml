<taxonomicName id="4C494D285A1F3E13FE75FA48755CDE92" authority="Tuomikoski, 1966" authorityName="Tuomikoski" authorityYear="1966" box="[445,794,1428,1454]" class="Insecta" family="Mycetophilidae" genus="Notolopha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
