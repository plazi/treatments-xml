<taxonomicName id="30E65FE69342FFDBFF56444A8B26F794" authority="(Foerster, 1863)" baseAuthorityName="Foerster" baseAuthorityYear="1863" box="[248,610,2057,2079]" class="Insecta" family="Braconidae" genus="Neoneurus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="283" phylum="Arthropoda" rank="species" species="clypeatus">
