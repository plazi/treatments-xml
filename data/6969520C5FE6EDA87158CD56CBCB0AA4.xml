61. 
<taxonomicName id="1DB3CE0FA1EB855222CD28F9871D71B1" LSID="http://zoobank.org/91BDBA22-A340-44C5-9A72-29D631278F41" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus paracollaris" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="paracollaris">Trigonopterus paracollaris Riedel</taxonomicName>
<taxonomicNameLabel id="BA6C26BD894977EF21EDC1EC8571EFC9" pageId="0" pageNumber="1">sp. n.</taxonomicNameLabel>
