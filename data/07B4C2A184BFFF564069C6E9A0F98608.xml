<normalizedToken id="66FFCD918D60675B03BBF4807472F889" originalValue="verschmälert" pageId="null" pageNumber="127">verschmaelert</normalizedToken>
</td>
<td id="AAFA4A5229D344C3DE8391B50CC46CFD" align="left" pageId="null" pageNumber="127" valign="bottom">
<taxonomicName id="1DC906A3E2F898F1C186A10C3315C488" class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="127" phylum="Tracheophyta" rank="species" species="olympica">
<italic id="37458EACA931C1FFFADB0F56ACC4514E" pageId="null" pageNumber="127">S. olympica</italic>
