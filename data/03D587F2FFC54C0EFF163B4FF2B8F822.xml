<taxonomicName id="4C7C4D67FFC54C0EFE813A99FC5EEFFC" authority="Eydoux &amp; P. Gervais, 1836" authorityName="Eydoux &amp; P. Gervais" authorityYear="1836" box="[292,934,4078,4117]" class="Mammalia" family="Emballonuridae" genus="Emballonura" kingdom="Animalia" order="Chiroptera" pageId="10" pageNumber="359" phylum="Chordata" rank="species" species="alecto">
