<taxonomicName id="4C1C4D58FFADFFA21E56F8E2386AF8D5" authority="Willemoes-Suhm, 1875" authorityName="Willemoes-Suhm" authorityYear="1875" box="[286,782,1855,1877]" class="Malacostraca" family="Lophogastridae" genus="Gnathophausia" kingdom="Animalia" order="Lophogastrida" pageId="6" pageNumber="205" phylum="Arthropoda" rank="species" species="gracilis">
