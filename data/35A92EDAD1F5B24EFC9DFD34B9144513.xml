<taxonomicName id="38EB65C857B3853BE8CEA0C9CF5FE8DD" authority="(Jacq.) Cass." class="Magnoliopsida" family="Asteraceae" genus="Willemetia" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="stipitata">
