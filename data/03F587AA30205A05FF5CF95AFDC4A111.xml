<taxonomicName id="4C5C4D3F30235A04FE87F8F0FD4CA54A" authority="Yu &amp; Yang" authorityName="Yu &amp; Yang" authorityYear="2019" box="[332,657,1863,1888]" class="Insecta" family="Cicadellidae" genus="Okubasca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="397" phylum="Arthropoda" rank="species" species="convoluta" status="sp. nov.">
