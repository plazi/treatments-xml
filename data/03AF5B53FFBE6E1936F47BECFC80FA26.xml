<taxonomicName id="4C0691C6FFBE6E1934897E12FCB1FA27" box="[746,856,1385,1410]" class="Arachnida" family="Diptilomiopidae" genus="Rhyncaphytoptus" kingdom="Animalia" order="Prostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="acilius">
