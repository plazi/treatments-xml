<taxonomicName id="D8F14D22FFEDFFD367C4FC9910343176" box="[468,587,835,859]" class="Insecta" family="Cerambycidae" genus="Parandra" kingdom="Animalia" order="Coleoptera" pageId="56" pageNumber="53" phylum="Arthropoda" rank="genus">
