<taxonomicName id="FCEB534F738742D327F35BD16407FB36" genus="P." lsidName="P. doro" pageId="0" pageNumber="77" rank="species" species="doro">
<emphasis id="F53DC6E235B889D2E9930B63CFAC2832" italics="true" pageId="0" pageNumber="77">P. doro</emphasis>
