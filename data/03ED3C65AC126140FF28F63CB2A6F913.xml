<taxonomicName id="4C44F6F0AC126140FC59F57AB2D0F9D1" box="[998,1268,1539,1564]" class="Magnoliopsida" family="Solanaceae" genus="Physalis" kingdom="Plantae" order="Solanales" pageId="11" pageNumber="44" phylum="Tracheophyta" rank="species" species="heterophylla">
<emphasis id="B9305161AC126140FC59F57AB2D0F9D1" box="[998,1268,1539,1564]" italics="true" pageId="11" pageNumber="44">Physalis heterophylla</emphasis>
</taxonomicName>
(
<taxonomicName id="4C44F6F0AC126140FABAF57BB3B0F9D1" box="[1285,1428,1538,1564]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="11" pageNumber="44" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
) growing in a flower bed in Kangemi, Nairobi (
<geoCoordinate id="EE70EBB4AC126140FD04F550B56BF98E" box="[699,847,1577,1603]" direction="south" orientation="latitude" pageId="11" pageNumber="44" precision="1" value="-1.2664">
S01° 
<number id="3D8C9F54AC126140FD4DF550B568F98E" box="[754,844,1577,1603]" pageId="11" pageNumber="44" value="15.984">15.984</number>
'
</geoCoordinate>
; 
<geoCoordinate id="EE70EBB4AC126140FCE5F550B5D9F98E" box="[858,1021,1577,1603]" direction="east" orientation="longitude" pageId="11" pageNumber="44" precision="1" value="36.7635">
E036° 
<number id="3D8C9F54AC126140FC1CF550B5DDF98E" box="[931,1017,1577,1603]" pageId="11" pageNumber="44" value="45.81">45.810</number>
'
</geoCoordinate>
) and on 
<taxonomicName id="4C44F6F0AC126140FBDDF550B325F98F" box="[1122,1281,1577,1602]" class="Magnoliopsida" family="Asteraceae" genus="Bidens" kingdom="Plantae" order="Asterales" pageId="11" pageNumber="44" phylum="Tracheophyta" rank="species" species="pilosa">
<emphasis id="B9305161AC126140FBDDF550B325F98F" box="[1122,1281,1577,1602]" italics="true" pageId="11" pageNumber="44">Bidens pilosa</emphasis>
</taxonomicName>
(
<taxonomicName id="4C44F6F0AC126140FAAFF550B3B0F98E" box="[1296,1428,1577,1603]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="11" pageNumber="44" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
) in Runda (
<geoCoordinate id="EE70EBB4AC126140FEAEF529B782F9A7" box="[273,422,1616,1642]" direction="south" orientation="latitude" pageId="11" pageNumber="44" precision="1" value="-1.2245">
S01° 
<number id="3D8C9F54AC126140FEF7F529B786F9A7" box="[328,418,1616,1642]" pageId="11" pageNumber="44" value="13.47">13.470</number>
