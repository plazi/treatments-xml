<taxonomicName id="47DF7FAFC914930D555F5C65477E28A1" class="Leotiomycetes" family="Vibrisseaceae" genus="Vibrissea" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Vibrissea flavovirens" order="Helotiales" pageId="2" pageNumber="3" phylum="Ascomycota" rank="species" species="flavovirens">Vibrissea flavovirens</taxonomicName>
