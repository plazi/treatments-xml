<taxonomicName id="BABA4D4D16209345FC1DE033B655FB9B" authority="BORRADAILE, 1910" authorityName="Borradaile" authorityYear="1910" box="[932,1295,1107,1128]" class="Malacostraca" family="Sergestidae" genus="Sicyonella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="357" phylum="Arthropoda" rank="genus">
