<taxonomicName id="3A89B1ABB006FFFDA05CF931FE4C1FAF" authority="Zamani &amp; Marusik, 2020" authorityName="Zamani &amp; Marusik" authorityYear="2020" box="[349,460,1679,1702]" class="Arachnida" family="Agelenidae" genus="Persiscape" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="373" phylum="Arthropoda" rank="species" species="caspica" status="sp. n.">
