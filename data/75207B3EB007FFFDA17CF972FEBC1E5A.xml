<taxonomicName id="3A89B1ABB006FFFDA357F90CFD781FC1" authority="Zamani &amp; Marusik, 2020" authorityName="Zamani &amp; Marusik" authorityYear="2020" box="[598,760,1712,1737]" class="Arachnida" family="Agelenidae" genus="Persiscape" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="373" phylum="Arthropoda" rank="species" species="nassirkhanii" status="sp. n.">
