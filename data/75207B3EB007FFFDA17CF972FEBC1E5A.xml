<emphasis id="CFFD163AB006FFFDA397F9D4FD481F8B" box="[662,712,1642,1666]" italics="true" pageId="5" pageNumber="373">levyi</emphasis>
</taxonomicName>
, 
<taxonomicName id="3A89B1ABB006FFFDA3E0F9D2FF5F1FAC" authority="Zamani &amp; Marusik, 2020" authorityName="Zamani &amp; Marusik" authorityYear="2020" class="Arachnida" family="Agelenidae" genus="Persiscape" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="373" phylum="Arthropoda" rank="species" species="ecbatana" status="sp. n.">
