<taxonomicName id="3A89B1ABB006FFFDA1D3F9F9FEA21F56" authorityName="Zamani &amp; Marusik" authorityYear="2020" baseAuthorityName="Levy" baseAuthorityYear="1996" box="[210,290,1607,1631]" class="Arachnida" family="Agelenidae" genus="Persiscape" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="373" phylum="Arthropoda" rank="species" species="gideoni">
