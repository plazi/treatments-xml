<taxonomicName id="4C4A7BDDA842FFFEE4D8792838A6F0EF" authority="Tshernyshev, 2021" authorityName="Tshernyshev" authorityYear="2021" box="[996,1179,1126,1153]" genus="Pectapalochrus" pageId="7" pageNumber="13" rank="genus" status="gen. nov.">
