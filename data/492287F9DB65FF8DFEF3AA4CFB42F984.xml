<subSubSection id="89916564DB65FF8DFCE9AA4FFB42F984" pageId="26" pageNumber="555" type="discussion">
<paragraph id="C13436EFDB65FF8DFCE9AA4FFB42F984" blockId="26.[828,1437,189,1655]" pageId="26" pageNumber="555">
<emphasis id="F3FFEAFDDB65FF8DFCE9AA4FFC27F9E4" bold="true" box="[828,956,1536,1559]" pageId="26" pageNumber="555">Comments.</emphasis>
<taxonomicName id="068B4D6CDB65FF8DFC15AA4FFAA8F9E4" authority="Medvedev, 2009" authorityName="Medvedev" authorityYear="2009" box="[960,1331,1536,1559]" class="Insecta" family="Chrysomelidae" genus="Liroetis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="555" phylum="Arthropoda" rank="species" species="nigricollis">
