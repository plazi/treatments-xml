<taxonomicName id="94D9333ED49F141636264291AC321EE8" authority="Keyserling, 1886" authorityName="Keyserling" authorityYear="1886" class="Arachnida" family="Araneidae" genus="Heurodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heurodes turritus" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="turritus">Heurodes turritus</taxonomicName>
