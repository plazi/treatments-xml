<materialsCitation id="8E93EF48FF9B60613821FD58F9DBFCCF" country="Argentina" location="Laguna del Hunco" longitude="-70.0" municipality="Patagonia" pageId="3" pageNumber="2911" specimenCount="1" stateProvince="Chubut">
