<taxonomicName id="4C404D2BFF99BA70BFA0F938AA9B1EAB" box="[749,898,1687,1710]" class="Actinopterygii" family="Muraenidae" genus="Uropterygius" kingdom="Animalia" order="Anguilliformes" pageId="23" pageNumber="163" phylum="Chordata" rank="genus">
