. 
<taxonomicName id="4C144D37C678A20AFB1BF71EFA2E4BED" authorityName="Gray" authorityYear="1834" box="[1251,1473,2180,2219]" class="Mammalia" family="Hipposideridae" genus="Hipposiderus" kingdom="Animalia" order="Chiroptera" pageId="14" pageNumber="241" phylum="Chordata" rank="genus">
