<taxonomicName id="4C7840B6C858184459C12E0F9638FDF5" authorityName="Alvarenga" authorityYear="1985" box="[163,370,605,624]" class="Aves" family="Psilopteridae" genus="Paleopsilopterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gruiformes" pageId="26" pageNumber="81" phylum="Chordata" rank="species" species="itaboraiensis">
