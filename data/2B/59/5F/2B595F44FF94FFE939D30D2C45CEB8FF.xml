<superScript attach="left" box="[823,831,770,784]" fontSize="6" pageId="27" pageNumber="254">2</superScript>
, EOO= 
<quantity box="[918,1102,770,797]" metricMagnitude="8" metricUnit="m" metricValue="6.817991509999999" pageId="27" pageNumber="254" unit="km" value="681799.151">681,799.151 km</quantity>
<superScript attach="left" box="[1102,1110,770,784]" fontSize="6" pageId="27" pageNumber="254">2</superScript>
