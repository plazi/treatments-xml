<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:83B65CFA-1F1E-4A82-A9E6-0E2F4BEDF726" class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pareuchiloglanis rhabdurus Ng 2004:7" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="rhabdurus">P. rhabdurus</taxonomicName>
