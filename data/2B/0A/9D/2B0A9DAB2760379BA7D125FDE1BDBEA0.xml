<taxonomicName class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="longicauda">P. longicauda</taxonomicName>
and 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:AF241874-BEBF-4718-B795-BF5807DD0826" class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pareuchiloglanis nebulifer Ng &amp; Kottelat 2000:11" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="nebulifer">P. nebulifer</taxonomicName>
