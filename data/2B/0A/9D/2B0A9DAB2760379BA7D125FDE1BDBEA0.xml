, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:DF3EB4EA-6044-44FF-AE48-B8FAD8978F40" class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pareuchiloglanis gongshanensis Chu 1981:28" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="gongshanensis">P. gongshanensis</taxonomicName>
, 
<taxonomicName class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="longicauda">P. longicauda</taxonomicName>
, 
<taxonomicName class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="macrotrema">P. macrotrema</taxonomicName>
, 
<taxonomicName class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="myzostoma">P. myzostoma</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:A61BED49-950B-4A1A-8BC9-9046E1F724E0" class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pareuchiloglanis robusta Ding, Fu &amp; Ye 1991:369 (English p. 374)" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="robusta">P. robusta</taxonomicName>
and 
<taxonomicName class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="12" phylum="Chordata" rank="species" species="sinensis">P. sinensis</taxonomicName>
