<taxonomicName authority="(Benedict, 1902)" baseAuthorityName="Benedict" baseAuthorityYear="1902" box="[306,616,1055,1081]" class="Malacostraca" family="Galatheidae" genus="Phylladiorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="integrus">
