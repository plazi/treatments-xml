<number box="[1250,1264,1689,1715]" pageId="7" pageNumber="60" value="2.0">2</number>
<number box="[1272,1301,1689,1715]" pageId="7" pageNumber="60" value="41.0">41</number>
(
<number box="[1318,1374,1689,1715]" pageId="7" pageNumber="60" value="33.1">33.1</number>
), P- 
