. 
<taxonomicName class="Agaricomycetes" family="Polyporaceae" genus="Hapalopilus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hapalopilus percoctus" order="Polyporales" pageId="10" pageNumber="11" phylum="Basidiomycota" rank="species" species="percoctus">Hapalopilus percoctus</taxonomicName>
