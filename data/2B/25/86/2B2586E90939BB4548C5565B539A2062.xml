, lectotype f 
<taxonomicName class="Agaricomycetes" family="Polyporaceae" genus="Hapalopilus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hapalopilus percoctus" order="Polyporales" pageId="10" pageNumber="11" phylum="Basidiomycota" rank="species" species="percoctus">Hapalopilus percoctus</taxonomicName>
, holotype g 
<taxonomicName class="Agaricomycetes" family="Polyporaceae" genus="Hapalopilus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hapalopilus ribicola" order="Polyporales" pageId="10" pageNumber="11" phylum="Basidiomycota" rank="species" species="ribicola">Hapalopilus ribicola</taxonomicName>
, lectotype h 
<taxonomicName class="Agaricomycetes" family="Polyporaceae" genus="Hapalopilus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hapalopilus rutilans" order="Polyporales" pageId="10" pageNumber="11" phylum="Basidiomycota" rank="species" species="rutilans">Hapalopilus rutilans</taxonomicName>
, 
<normalizedToken originalValue="Niemelä">Niemelae</normalizedToken>
7134.
</paragraph>
