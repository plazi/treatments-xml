<taxonomicName authorityName="Ormerod" authorityYear="2002" baseAuthorityName="K.Y.Lang" box="[1257,1436,1025,1048]" class="Liliopsida" family="Orchidaceae" genus="Zeuxine" kingdom="Plantae" order="Asparagales" pageId="2" pageNumber="67" phylum="Tracheophyta" rank="species" species="gengmanensis">
