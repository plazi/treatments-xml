<quantity box="[352,458,1121,1144]" metricMagnitude="-1" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="2" pageNumber="67" unit="cm" value="40.0" valueMax="50.0" valueMin="30.0">30–50 cm</quantity>
