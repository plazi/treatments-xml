<quantity box="[376,469,1441,1464]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" metricValueMax="9.0" metricValueMin="7.0" pageId="2" pageNumber="67" unit="mm" value="8.0" valueMax="9.0" valueMin="7.0">7–9 mm</quantity>
