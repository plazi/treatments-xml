<emphasis box="[508,616,686,709]" italics="true" pageId="83" pageNumber="83">
<taxonomicName authorityName="Gould" authorityYear="1851" box="[508,612,686,709]" class="Aves" family="Fringillidae" genus="Eophona" kingdom="Animalia" order="Passeriformes" pageId="83" pageNumber="83" phylum="Chordata" rank="genus">Eophona</taxonomicName>
