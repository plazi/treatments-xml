are: 
<collectorName box="[716,834,507,529]" pageId="120" pageNumber="120">Donggala</collectorName>
, 
<emphasis bold="true" box="[843,1021,507,528]" pageId="120" pageNumber="120">
<specimenCode box="[843,1021,507,528]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="120" pageNumber="120">AMNH 721261</specimenCode>
