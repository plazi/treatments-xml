<taxonomicName authorityName="LeCroy" authorityYear="2013" box="[667,890,477,500]" class="Aves" family="Estrildidae" genus="Munia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="120" pageNumber="120" phylum="Chordata" rank="species" species="subcastanea">
<emphasis box="[667,890,477,500]" italics="true" pageId="120" pageNumber="120">Munia subcastanea</emphasis>
