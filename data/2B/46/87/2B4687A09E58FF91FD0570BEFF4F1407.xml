<typeStatus box="[187,290,242,263]" pageId="93" pageNumber="93">paratype</typeStatus>
is in 
<collectionCode box="[365,462,242,263]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="93" pageNumber="93">AMNH</collectionCode>
