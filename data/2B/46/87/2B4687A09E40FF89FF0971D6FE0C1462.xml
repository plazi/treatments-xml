<location LSID="urn:lsid:plazi:treatment:2B4687A09E40FF89FF0971D6FE0C1462:A630606D9E40FF88FD51762FFCB41407" box="[738,834,565,586]" county="Type'" municipality="Mearns. He" name="Mearns'" pageId="68" pageNumber="68" stateProvince="Arizona">Mearns’</location>
