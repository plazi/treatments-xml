<paragraph blockId="42.[93,620,815,1632]" pageId="42" pageNumber="42">
<materialsCitation ID-GBIF-Occurrence="3059738781" collectionCode="AMNH" collectorName="John R. Galbraith. From &amp; George N." country="Panama" location="Panama" pageId="42" pageNumber="42" specimenCode="AMNH 40132" specimenCount="1" typeStatus="holotype">
