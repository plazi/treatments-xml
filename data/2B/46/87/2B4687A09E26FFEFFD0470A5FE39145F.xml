<taxonomicName authority="P. L. Sclater, 1859" authorityName="P. L. Sclater" authorityYear="1859" class="Aves" family="Vireonidae" genus="Cyclarhis" kingdom="Animalia" order="Passeriformes" pageId="34" pageNumber="34" phylum="Chordata" rank="subSpecies" species="gujanensis" subSpecies="flavipectus">
