<geoCoordinate box="[365,445,851,873]" degrees="23.58" direction="south" orientation="latitude" pageId="99" pageNumber="99" precision="555" value="-23.58">23.58S</geoCoordinate>
, 
<geoCoordinate box="[465,558,851,872]" degrees="132.37" direction="east" orientation="longitude" pageId="99" pageNumber="99" precision="555" value="132.37">132.37E</geoCoordinate>
