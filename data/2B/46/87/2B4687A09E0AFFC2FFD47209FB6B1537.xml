</materialsCitation>
; 
<materialsCitation ID-GBIF-Occurrence="3059738438" collectingDate="1927-12" collectingDateMax="1928-01" collectingDateMin="1927-12" collectionCode="CP" pageId="14" pageNumber="14" specimenCode="AMNH 237053-237058" specimenCount="1" typeStatus="paratype">
