<collectionCode box="[700,797,547,568]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="14" pageNumber="14">AMNH</collectionCode>
