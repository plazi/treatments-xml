<taxonomicName authority="Berlepsch and Stolzmann, 1894" authorityName="Berlepsch and Stolzmann" authorityYear="1894" class="Aves" family="Fringillidae" genus="Spinus" higherTaxonomySource="CoL" kingdom="Animalia" order="Passeriformes" pageId="68" pageNumber="68" phylum="Chordata" rank="species" species="olivaceus">
<emphasis box="[146,341,1310,1333]" italics="true" pageId="68" pageNumber="68">Spinus olivaceus</emphasis>
<bibRefCitation author="Berlepsch, H. von &amp; J. Stolzmann" pageId="68" pageNumber="68" pagination="385 - 405" refId="ref112887" refString="Berlepsch, H. von, and J. Stolzmann. 1894. Descriptions de quelques especes nouvelles d'oiseaux du Perou central. Ibis (6) 6: 385 - 405, pl. XI." type="journal article" year="1894">Berlepsch and Stolzmann, 1894</bibRefCitation>
</taxonomicName>
[preoccupied by 
<taxonomicName authority="Rafinesque, 1810" authorityName="Rafinesque" authorityYear="1810" class="Aves" family="Fringillidae" genus="Fringilla" kingdom="Animalia" order="Passeriformes" pageId="68" pageNumber="68" phylum="Chordata" rank="species" species="olivacea">
<emphasis box="[424,619,1339,1362]" italics="true" pageId="68" pageNumber="68">Fringilla olivacea</emphasis>
Rafinesque, 1810
</taxonomicName>
, now 
<taxonomicName authority="(Pallas, 1770)" baseAuthorityName="Pallas" baseAuthorityYear="1770" class="Aves" family="Fringillidae" genus="Carpodacus" kingdom="Animalia" order="Passeriformes" pageId="68" pageNumber="68" phylum="Chordata" rank="species" species="erythrinus">
<emphasis box="[364,619,1368,1391]" italics="true" pageId="68" pageNumber="68">Carpodacus erythrinus</emphasis>
(Pallas, 1770)
</taxonomicName>
, if 
<taxonomicName authorityName="Kaup" authorityYear="1829" box="[298,432,1398,1421]" class="Aves" family="Fringillidae" genus="Carpodacus" kingdom="Animalia" order="Passeriformes" pageId="68" pageNumber="68" phylum="Chordata" rank="genus">
<emphasis box="[298,432,1398,1421]" italics="true" pageId="68" pageNumber="68">Carpodacus</emphasis>
