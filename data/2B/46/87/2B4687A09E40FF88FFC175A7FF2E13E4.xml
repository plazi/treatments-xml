<taxonomicName authorityName="Brisson" authorityYear="1760" box="[93,201,1427,1450]" class="Aves" family="Fringillidae" genus="Carduelis" kingdom="Animalia" order="Passeriformes" pageId="68" pageNumber="68" phylum="Chordata" rank="genus">
