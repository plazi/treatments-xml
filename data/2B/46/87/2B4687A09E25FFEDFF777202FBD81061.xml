<location LSID="urn:lsid:plazi:treatment:2B4687A09E25FFEDFF777202FBD81061:A630606D9E25FFEDFC6B7689FBF514E5" box="[984,1027,659,680]" county="Amadon" municipality="Unique-the" name="No" pageId="33" pageNumber="33" stateProvince="Hawaii">No</location>
