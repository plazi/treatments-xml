are: 
<collectingCountry box="[1085,1224,301,323]" name="Saint Helena, Ascension and Tristan da Cunha" pageId="39" pageNumber="39">Santa Elena</collectingCountry>
, 
<emphasis bold="true" box="[701,881,330,351]" pageId="39" pageNumber="39">
