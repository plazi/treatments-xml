</emphasis>
, 
<emphasis bold="true" box="[896,978,330,351]" pageId="39" pageNumber="39">
<specimenCode box="[896,978,330,351]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="39" pageNumber="39">134059</specimenCode>
