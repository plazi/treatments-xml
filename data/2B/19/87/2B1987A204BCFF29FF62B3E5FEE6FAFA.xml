? 
<taxonomicName authority="Rakitov &amp; Godoy" authorityName="Rakitov &amp; Godoy" authorityYear="2005" box="[287,737,1272,1298]" class="Insecta" family="Cicadellidae" genus="Quichira" kingdom="Animalia" order="Hemiptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="species" species="parallela">
<emphasis box="[287,511,1272,1297]" italics="true" pageId="212" pageNumber="213">Quichira parallela</emphasis>
Rakitov &amp; Godoy
</taxonomicName>
(
<taxonomicName box="[755,908,1272,1298]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="family">Cicadellidae</taxonomicName>
). Both 
<taxonomicName box="[997,1123,1272,1297]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="species" species="rakitovi">
<emphasis box="[997,1123,1272,1297]" italics="true" pageId="212" pageNumber="213">G. rakitovi</emphasis>
</taxonomicName>
and 
<taxonomicName class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="species" species="blefuscu" status="sp. n." subGenus="Cosmocomoidea">
<emphasis box="[1186,1211,1272,1297]" italics="true" pageId="212" pageNumber="213">G.</emphasis>
(
<emphasis box="[1231,1426,1272,1297]" italics="true" pageId="212" pageNumber="213">Cosmocomoidea</emphasis>
) 
<emphasis box="[151,249,1312,1337]" italics="true" pageId="212" pageNumber="213">blefuscu</emphasis>
