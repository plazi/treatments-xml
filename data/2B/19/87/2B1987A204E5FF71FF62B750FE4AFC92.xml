<taxonomicName baseAuthorityName="Ogloblin" baseAuthorityYear="1959" box="[334,799,312,338]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="140" pageNumber="141" phylum="Arthropoda" rank="species" species="coxalis" subGenus="Cosmocomoidea">
