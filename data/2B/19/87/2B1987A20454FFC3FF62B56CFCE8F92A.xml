<paragraph blockId="60.[151,1437,1402,2028]" pageId="60" pageNumber="61">
<materialsCitation ID-GBIF-Occurrence="1639354031" collectionCode="BPBM" collectorName="A. Koebele's" location="Morelos" pageId="60" pageNumber="61" specimenCount="3" stateProvince="Morelos" typeStatus="lectotype">
