<collectingRegion box="[643,785,1925,1951]" country="Argentina" name="Misiones" pageId="150" pageNumber="151">MISIONES</collectingRegion>
, 
<collectingMunicipality box="[795,876,1925,1951]" pageId="150" pageNumber="151">Loreto</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:2B1987A204FAFF6AFF62B2F0FF09F822:A66F606F04F9FF6BFC8DB5E2FB9FF807" box="[888,1091,1925,1951]" country="Argentina" county="Material" municipality="Loreto" name="Ruinas Jesuiticas" pageId="150" pageNumber="151" stateProvince="MISIONES">Ruinas Jesuíticas</location>
, 
<date box="[1103,1247,1925,1951]" pageId="150" pageNumber="151" value="2000-08-24">
<collectingDate box="[1103,1247,1925,1951]" pageId="150" pageNumber="151" value="2000-08-24">24.viii.2000</collectingDate>
