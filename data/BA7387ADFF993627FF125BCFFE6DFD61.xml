<quantity id="F5229B5EFF983620FAD4589AFA31FA66" box="[1361,1429,1459,1483]" metricMagnitude="0" metricUnit="m" metricValue="9.1" pageId="19" pageNumber="320" unit="m" value="9.1">9.1 m</quantity>
