<quantity id="F5229B5EFF983620FDE158FEFD13FA42" box="[612,695,1495,1519]" metricMagnitude="1" metricUnit="m" metricValue="1.83" pageId="19" pageNumber="320" unit="m" value="18.3">18.3 m</quantity>
