<quantity id="F5229B5EFF983620FB3F58FEFAEDFA42" box="[1210,1353,1495,1519]" metricMagnitude="1" metricUnit="m" metricValue="2.465" metricValueMax="2.7399999999999998" metricValueMin="2.19" pageId="19" pageNumber="320" unit="m" value="24.65" valueMax="27.4" valueMin="21.9">21.9-27.4 m</quantity>
