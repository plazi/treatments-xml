species: 
<taxonomicName id="4C1BDBEB96168F3CFE908070FD1EF3A3" authority="Stoll, 1781" authorityName=", Stoll" authorityYear="1781" box="[309,660,871,897]" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="239" phylum="Arthropoda" rank="species" species="euphemia">
<emphasis id="B96F7C7A96168F3CFE908070FD99F3A3" box="[309,531,871,897]" italics="true" pageId="2" pageNumber="239">Phalaena euphemia</emphasis>
<bibRefCitation id="EF8ADD9996168F3CFDBF8070FD1EF3A3" author="Stoll, C." box="[538,660,871,897]" pageId="2" pageNumber="239" pagination="91 - 182" refId="ref11431" refString="Stoll, C. (1781) s. n. In: De Uitlandsche Kapellen voorkomende in de drie Waereld-deelen, Asia, Africa en America. Vol. IV. S. J. Baalde, Amsterdam, pp. 91 - 182, pls. CCCXXXVII - CCCLXXXIV." type="book chapter" year="1781">Stoll, 1781</bibRefCitation>
</taxonomicName>
[by subsequent designation, 
<bibRefCitation id="EF8ADD9996168F3CFC7C8070FB44F3A3" author="Hampson, G. F." box="[985,1230,870,897]" pageId="2" pageNumber="239" pagination="42 - 71" refId="ref10462" refString="Hampson, G. F. (1920) Catalogue of the Lepidoptera Phalaenae in the British Museum. Supplement Volume II. Catalogue of the Lithosiadae (Arctianae) and Phalaenoididae in the collection of the British Museum. Trustees of the British Museum (Natural History), London, 619 pp., pls. 42 - 71." type="book chapter" year="1920">Hampson (1920: 549)</bibRefCitation>
but cited as 
<taxonomicName id="4C1BDBEB96168F3CFAFC8070FE69F387" authority="(Fabricius, 1781)" baseAuthorityName="Fabricius" baseAuthorityYear="1781" class="Insecta" family="Noctuidae" genus="Heraclia" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="239" phylum="Arthropoda" rank="species" species="geryon">
<emphasis id="B96F7C7A96168F3CFAFC8070FE92F386" italics="true" pageId="2" pageNumber="239">Heraclia geryon</emphasis>
(
<bibRefCitation id="EF8ADD9996168F3CFE8D809CFE51F387" author="Fabricius, J. C." box="[296,475,907,933]" pageId="2" pageNumber="239" refId="ref10177" refString="Fabricius, J. C. (1781) Species Insectorum, Exhibentes Eorum Differentias Specificas, Synonyma Auctorum, Loca Natalia, Metamorphosin Adiectis, Observationibus, Descriptionibus 2. Impensis C. E. Bohnii, Hamburgi et Kilonii, 517 pp. https: // doi. org / 10.5962 / bhl. title. 36509" type="book" year="1781">Fabricius, 1781</bibRefCitation>
)
</taxonomicName>
and referred back to 
<bibRefCitation id="EF8ADD9996168F3CFD79809CFC5CF387" author="Hampson, G. F." box="[732,982,906,933]" pageId="2" pageNumber="239" pagination="36 - 54" refId="ref10401" refString="Hampson, G. F. (1901) Catalogue of the Lepidoptera Phalaenae in the British Museum. Volume III. Catalogue of the Acrtiadae (Arctianae) and Agaristidae in the collection of the British Museum. Trustees of the British Museum (Natural History), London, 690 pp., pls. 36 - 54." type="book chapter" year="1901">Hampson (1901: 565)</bibRefCitation>
where 
<taxonomicName id="4C1BDBEB96168F3CFB89809CFBF0F386" baseAuthorityName="Fabricius" baseAuthorityYear="1781" box="[1068,1146,907,932]" class="Insecta" family="Noctuidae" genus="Heraclia" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="239" phylum="Arthropoda" rank="species" species="geryon">
<emphasis id="B96F7C7A96168F3CFB89809CFBF0F386" box="[1068,1146,907,932]" italics="true" pageId="2" pageNumber="239">geryon</emphasis>
</taxonomicName>
was treated as the senior synonym of 
<taxonomicName id="4C1BDBEB96168F3CFE8680B8FE1AF3EB" authorityName=", Stoll" authorityYear="1781" box="[291,400,943,969]" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="239" phylum="Arthropoda" rank="species" species="euphemia">
<emphasis id="B96F7C7A96168F3CFE8680B8FE1AF3EB" box="[291,400,943,969]" italics="true" pageId="2" pageNumber="239">euphemia</emphasis>
</taxonomicName>
]
</paragraph>
</subSubSection>
