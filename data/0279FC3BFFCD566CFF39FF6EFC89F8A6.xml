<taxonomicName id="4DD036AEFFCD566CFF39FF6EFD8BFF05" authority="Winnertz, 1863: 779" authorityName="Winnertz" authorityPageNumber="779" authorityYear="1863" box="[151,517,151,178]" class="Insecta" family="Mycetophilidae" genus="Phthinia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="304" phylum="Arthropoda" rank="genus">
