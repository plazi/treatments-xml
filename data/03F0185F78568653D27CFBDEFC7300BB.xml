<typeStatus id="54E217EB78538653D24CFE4BFE9203F0" box="[312,382,443,469]" pageId="4" pageNumber="5">Types</typeStatus>
.
</emphasis>
<typeStatus id="54E217EB78538653D2D5FE4CFDFD03F3" box="[417,529,444,470]" pageId="4" pageNumber="5">Holotype</typeStatus>
, male, 
<collectingCountry id="F34EE9D978538653D11BFE4CFD0003F3" box="[623,748,444,470]" name="Bolivia" pageId="4" pageNumber="5">BOLIVIA</collectingCountry>
: Santa Cruz Dept., 
<taxonomicName id="4C59D2CA78538653D097FE4CFBCB03F3" box="[995,1063,444,470]" class="Insecta" family="Cerambycidae" genus="Stizocera" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="8" phylum="Arthropoda" rank="species" species="ichilo">Ichilo</taxonomicName>
Prov., Hotel Flora y Fauna, 
<quantity id="4CA104AC78538653D215FE14FE5103DB" box="[353,445,484,510]" metricMagnitude="3" metricUnit="m" metricValue="5.0" metricValueMax="6.0" metricValueMin="4.0" pageId="4" pageNumber="5" unit="km" value="5.0" valueMax="6.0" valueMin="4.0">4–6 km</quantity>
SSE Buena Vista, 
<geoCoordinate id="EE6DCF8E78538653D1D1FE14FCC103DB" box="[677,813,484,510]" direction="south" orientation="latitude" pageId="4" pageNumber="5" precision="9" value="-17.499166">
<number id="3D91BB6E78538653D1D1FE14FD2E03DB" box="[677,706,484,510]" pageId="4" pageNumber="5" value="17.0">17</number>
° 
<number id="3D91BB6E78538653D1B9FE14FCF403DB" box="[717,792,484,510]" pageId="4" pageNumber="5" value="29.95">29.95</number>
’S
</geoCoordinate>
, 
<geoCoordinate id="EE6DCF8E78538653D048FE14FC2703DB" box="[828,971,484,510]" direction="west" orientation="longitude" pageId="4" pageNumber="5" precision="9" value="-63.5525">
<number id="3D91BB6E78538653D048FE14FCB503DB" box="[828,857,484,510]" pageId="4" pageNumber="5" value="63.0">63</number>
