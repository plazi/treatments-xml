<taxonomicName id="ADB67DA33B61FF81FDF1FD207B60E98C" authority="Nilsson 2011" authorityName="Nilsson" authorityYear="2011" box="[541,828,652,677]" class="Insecta" family="Noteridae" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="190" phylum="Arthropoda" rank="family">
