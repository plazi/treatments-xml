, and 
<collectingCountry id="F348C5FEFFAFA162FBE3FEB0FB6CFEBD" box="[1087,1183,338,364]" name="Panama" pageId="100" pageNumber="101">Panama</collectingCountry>
; 
<quantity id="4CA7288BFFAFA162FB77FEB0FAD0FEBD" box="[1195,1315,338,364]" metricMagnitude="2" metricUnit="m" metricValue="5.5" metricValueMax="11.0" metricValueMin="0.0" pageId="100" pageNumber="101" unit="m" value="550.0" valueMax="1100.0" valueMin="0.0">0–1100 m</quantity>
