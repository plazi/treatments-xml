<taxonomicName id="B8B64D37FFB5FF9C6DCD97F6FE105294" authority="albania" authorityName="albania (H. W. Bates" authorityYear="1864" box="[199,480,911,936]" class="Insecta" family="Pieridae" genus="Enantia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="subSpecies" species="albania" subSpecies="albania">
