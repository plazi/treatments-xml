<collectionCode id="0699AE3FD7419D59FC8DFA1317D653FD" box="[817,914,1430,1454]" pageId="5" pageNumber="796">USMHC</collectionCode>
2449, 
<collectionCode id="0699AE3FD7419D59FC55FA13105853FD" box="[1001,1052,1430,1454]" pageId="5" pageNumber="796">SVL</collectionCode>
<quantity id="A7709B1FD7419D59FB97FA13103A53FD" box="[1067,1150,1430,1454]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="5" pageNumber="796" unit="mm" value="19.0">19 mm</quantity>
