<tr id="353834CDBA5DE4BB4BB695A2FA3FF8C0" box="[151,1427,1745,1803]" gridrow="6" pageId="2" pageNumber="64">
<th id="76E95DB1BA5DE4BB4BB695A2FE6EF8C0" box="[151,450,1745,1803]" gridcol="0" gridrow="6" pageId="2" pageNumber="64">Basal projection of retrorse lobe of gonocoxite</th>
<td id="76E95DB1BA5DE4BB4AF495A2FC87F8C0" box="[469,811,1745,1803]" gridcol="1" gridrow="6" pageId="2" pageNumber="64">strong</td>
<td id="76E95DB1BA5DE4BB486395A2FBE2F8C0" box="[834,1102,1745,1803]" gridcol="2" gridrow="6" pageId="2" pageNumber="64">strong</td>
<td id="76E95DB1BA5DE4BB4F5695A2FA3FF8C0" box="[1143,1427,1745,1803]" gridcol="3" gridrow="6" pageId="2" pageNumber="64">weak</td>
</tr>
<tr id="353834CDBA5DE4BB4BB6946DFA3FF8FE" box="[151,1427,1822,1845]" gridrow="7" pageId="2" pageNumber="64">
<th id="76E95DB1BA5DE4BB4BB6946DFE6EF8FE" box="[151,450,1822,1845]" gridcol="0" gridrow="7" pageId="2" pageNumber="64">Inner brush of retrorse lobe</th>
