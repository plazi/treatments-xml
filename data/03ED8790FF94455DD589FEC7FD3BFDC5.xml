<collectingCountry id="F3537616FF94455DD5C7FD94FD83FD9A" box="[452,513,520,543]" name="Japan" pageId="4" pageNumber="233">Japan</collectingCountry>
(
<bibRefCitation id="EFD54B77FF94455DD613FD94FD2BFDA5" author="YAMANE S." box="[528,681,520,544]" pageId="4" pageNumber="233" pagination="1 - 189" refId="ref4530" refString="YAMANE S. 1990: A revision of the Japanese Eumenidae (Hymenoptera, Vespoidea). Insecta Matsumurana, New Series 43: 1 - 189." type="book chapter" year="1990">
<collectionCode id="ED55AE43FF94455DD613FD94FDEAFD9B" box="[528,616,520,544]" pageId="4" pageNumber="233">YAMANE</collectionCode>
1990
</bibRefCitation>
, as 
<taxonomicName id="4C444D05FF94455DD6D0FD95FC25FDA5" authorityName="Sk. Yamane" authorityYear="1990" box="[723,935,521,544]" class="Insecta" family="Eumenidae" genus="Symmorphus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="233" phylum="Arthropoda" rank="species" species="iwatai">
<emphasis id="B930EA94FF94455DD6D0FD95FC25FDA5" box="[723,935,521,544]" italics="true" pageId="4" pageNumber="233">Symmorphus iwatai</emphasis>
</taxonomicName>
