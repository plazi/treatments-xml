<quantity id="4CDC7ABDFF93FFB3175AAB75FE556635" box="[272,385,1324,1347]" metricMagnitude="2" metricUnit="m" metricValue="5.385" metricValueMax="9.3" metricValueMin="1.47" pageId="10" pageNumber="927" unit="m" value="538.5" valueMax="930.0" valueMin="147.0">
147- 
<specimenCount id="9D221CD1FF93FFB31776AB75FE556635" box="[316,385,1324,1347]" pageId="10" pageNumber="927" type="generic">930 m</specimenCount>
