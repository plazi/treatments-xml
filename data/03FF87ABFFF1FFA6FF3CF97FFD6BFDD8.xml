<heading id="D0A181D1FFF1FFA5FF3CF97FFCF6F8E6" bold="true" box="[151,788,1776,1802]" fontSize="11" level="1" pageId="5" pageNumber="45" reason="1">
<taxonomicName id="4C564D3EFFF1FFA5FF3CF97FFCF6F8E6" authority="Diesing, 1851" authorityName="Diesing" authorityYear="1851" box="[151,788,1776,1802]" class="Phascolosomatidea" family="Aspidosiphonidae" genus="Aspidosiphon" kingdom="Animalia" order="Aspidosiphoniformes" pageId="5" pageNumber="45" phylum="Sipuncula" rank="species" species="muelleri" subGenus="Aspidosiphon">
<emphasis id="B922EAAFFFF1FFA5FF3CF97FFCF6F8E6" bold="true" box="[151,788,1776,1802]" pageId="5" pageNumber="45">
<emphasis id="B922EAAFFFF1FFA5FF3CF97FFED9F8E6" bold="true" box="[151,315,1776,1802]" italics="true" pageId="5" pageNumber="45">Aspidosiphon</emphasis>
(
<emphasis id="B922EAAFFFF1FFA5FEE7F97FFE12F8E6" bold="true" box="[332,496,1776,1802]" italics="true" pageId="5" pageNumber="45">Aspidosiphon</emphasis>
) 
<emphasis id="B922EAAFFFF1FFA5FDA9F97FFD85F8E6" bold="true" box="[514,615,1776,1802]" italics="true" pageId="5" pageNumber="45">muelleri</emphasis>
Diesing, 
<number id="3D9E249AFFF1FFA5FD71F97FFCF6F8E6" box="[730,788,1776,1802]" pageId="5" pageNumber="45" value="1851.0">1851</number>
</emphasis>
