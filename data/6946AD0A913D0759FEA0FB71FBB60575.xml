<taxonomicName id="26EF679F913D0759FBC1FB71FB0904F7" box="[1129,1230,1238,1264]" class="Insecta" family="Aphididae" genus="Sitobion" kingdom="Animalia" order="Aphidomorpha" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
