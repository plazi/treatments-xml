<taxonomicName id="4C35FEFD3274FC26FBC3D1592891A770" authority="L." box="[1151,1410,1888,1906]" class="Magnoliopsida" family="Apocynaceae" genus="Asclepias" kingdom="Plantae" order="Gentianales" pageId="3" pageNumber="2" phylum="Tracheophyta" rank="species" species="verticillata">
