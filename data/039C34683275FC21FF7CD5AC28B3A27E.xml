<taxonomicName id="4C35FEFD3274FC26FE28D0682F99A667" authorityName="Skillman" authorityYear="2007" box="[404,650,1617,1637]" class="Insecta" family="Cerambycidae" genus="Tetraopes" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="huetheri">
