<number id="FA2AED38BF67FF8FA6D5F99AFDFFF97B" box="[508,529,1653,1677]" pageId="15" pageNumber="316" value="3.0">3</number>
. 
<collectingCountry id="34F5BF8FBF67FF8FA536F99AFD5AF97B" box="[543,692,1653,1677]" name="Myanmar" pageId="15" pageNumber="316">MYANMAR</collectingCountry>
, N.E. 
<collectingCountry id="34F5BF8FBF67FF8FA5D0F999FCA6F97B" box="[761,840,1654,1677]" name="Myanmar" pageId="15" pageNumber="316">Burma</collectingCountry>
