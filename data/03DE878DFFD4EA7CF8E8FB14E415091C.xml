<taxonomicName id="4C774D18FFD4EA7CF8E8FB14E3B608D3" authority="Laidlaw, 1902" authorityName="Laidlaw" authorityYear="1902" box="[151,652,1157,1183]" class="Rhabditophora" family="Pseudocerotidae" genus="Pseudobiceros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="3" pageNumber="104" phylum="Platyhelminthes" rank="species" species="punctatus">
