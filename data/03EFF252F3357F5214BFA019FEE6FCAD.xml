<taxonomicName id="4C4638C7F3357F5214BFA019FDE7FD3B" authority="Basilewsky, 1950" authorityName="Basilewsky" authorityYear="1950" box="[151,532,618,644]" class="Insecta" family="Carabidae" genus="Termophilum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="250" phylum="Arthropoda" rank="genus">
