<taxonomicName id="4C21848B606DFFCDFF33166D5C86FEF0" authority="Mez, Bot. Jahrb. Syst." authorityName="Mez, Bot. Jahrb. Syst." box="[227,706,330,354]" class="Liliopsida" family="Poaceae" genus="Panicum" kingdom="Plantae" order="Poales" pageId="77" pageNumber="77" phylum="Tracheophyta" rank="species" species="ecuadorense">
