<taxonomicName id="439CB6C8FF9CBE4C4617FA7DFDDFFA3E" box="[269,528,1455,1479]" class="Magnoliopsida" family="Elaeagnaceae" genus="Elaeagnus" kingdom="Plantae" order="Rosales" pageId="14" pageNumber="117" phylum="Tracheophyta" rank="species" species="angustifolia">
