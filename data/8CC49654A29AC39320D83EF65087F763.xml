<taxonomicName id="325316BC663C01DFD1E743AE550DDEFD" authority="Minet, 2003" authorityName="Minet" authorityYear="2003" class="Insecta" family="Epicopeiidae" genus="Deuveia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Deuveia" order="Lepidoptera" pageId="9" pageNumber="42" phylum="Arthropoda" rank="genus">Deuveia</taxonomicName>
