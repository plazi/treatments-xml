; PW 
<quantity id="4CEF12DBDB33FFAEFCC6B479FC4EFEFA" box="[799,907,272,298]" metricMagnitude="-3" metricUnit="m" metricValue="1.85" pageId="63" pageNumber="64" unit="mm" value="1.85">1.85 mm</quantity>
; PL 
<quantity id="4CEF12DBDB33FFAEFC19B479FBE9FEFA" box="[960,1068,272,298]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="63" pageNumber="64" unit="mm" value="0.8">0.80 mm</quantity>
