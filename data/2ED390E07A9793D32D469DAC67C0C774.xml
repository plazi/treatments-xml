<taxonomicName id="DCFA955DBD2F8A27EB5026C81DB441D6" class="Insecta" family="Crabronidae" genus="Gorytes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gorytes matangensis" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="matangensis">Gorytes matangensis</taxonomicName>
