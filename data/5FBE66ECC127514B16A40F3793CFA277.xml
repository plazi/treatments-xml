<taxonomicName id="B0C84653B758B06C66C4B119B0ECB84B" class="Insecta" family="Bombyliidae" genus="Lepidanthrax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lepidanthrax disiunctus" order="Diptera" pageId="16" pageNumber="65" phylum="Arthropoda" rank="species" species="disiunctus">Lepidanthrax disiunctus</taxonomicName>
