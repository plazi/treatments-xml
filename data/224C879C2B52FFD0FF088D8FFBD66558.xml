<paragraph id="AA5A368A2B52FFD0FF088DC4FBD66558" blockId="37.[151,1436,1517,1614]" pageId="37" pageNumber="38">
<emphasis id="9891EA982B52FFD0FF088DC4FEC96510" bold="true" box="[151,278,1517,1542]" pageId="37" pageNumber="38">Definition.</emphasis>
<taxonomicName id="6DE54D092B52FFD0FE808DC4FE6B6510" authorityName="Dendy" authorityYear="1922" box="[287,436,1517,1542]" class="Demospongiae" family="Iotrochotidae" kingdom="Animalia" order="Poecilosclerida" pageId="37" pageNumber="38" phylum="Porifera" rank="family">Iotrochotidae</taxonomicName>
