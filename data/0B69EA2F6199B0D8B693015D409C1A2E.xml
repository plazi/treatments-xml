<td id="D8134F8CEC06FB9CB1A2A020CCA6DBFC" align="left" pageId="null" pageNumber="746" valign="top">
2. 
<normalizedToken id="1A28AD66351264CCAD0DC0F1EE05B3C9" originalValue="Äußere" pageId="null" pageNumber="746">Aeussere</normalizedToken>
<normalizedToken id="D1B70EB566155A229976B545816CA28A" originalValue="Perigonblätter" pageId="null" pageNumber="746">Perigonblaetter</normalizedToken>
