<tr id="E6DDE953C8A119CE06D6D02FF333F9CE" pageId="null" pageNumber="746">
<td id="BA4AEAD1250E27343A0C7619B8309510" align="left" pageId="null" pageNumber="746" valign="top">
2*. 
<normalizedToken id="EFA89A6F074383FE52E2D7DF51E74742" originalValue="Äußere" pageId="null" pageNumber="746">Aeussere</normalizedToken>
