<taxonomicName id="CBDE4D7F7061FFB0FECFF856FD6FF84F" authority="Lameere, 1912" authorityName="Lameere" authorityYear="1912" box="[348,712,1934,1956]" class="Insecta" family="Cerambycidae" genus="Quercivir" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="134" phylum="Arthropoda" rank="species" species="gounellei">
<emphasis id="3EAAEAEE7061FFB0FECFF856FE19F84F" box="[348,446,1935,1956]" italics="true" pageId="21" pageNumber="134">Quercivir</emphasis>
<taxonomicName id="CBDE4D7F7061FFB0FE56F857FD21F84F" authority="Lameere" authorityName="Lameere" authorityYear="1912" box="[453,646,1934,1956]" pageId="21" pageNumber="134" rank="species" species="gounellei">
<taxonomicName id="CBDE4D7F7061FFB0FE56F857FD84F84F" box="[453,547,1934,1956]" pageId="21" pageNumber="134" rank="species" species="gounellei">
