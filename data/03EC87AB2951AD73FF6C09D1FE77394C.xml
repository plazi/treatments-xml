<taxonomicName id="4C454D3E2956AD72FE470DE1FDA63E85" box="[444,577,1727,1749]" pageId="12" pageNumber="13" rank="species" species="weddellia">
cf. 
<taxonomicName id="4C454D3E2956AD72FE240DE1FDA63E85" box="[479,577,1727,1749]" pageId="12" pageNumber="13" rank="species" species="weddellia">
<emphasis id="B931EAAF2956AD72FE240DE1FDA63E85" box="[479,577,1727,1749]" italics="true" pageId="12" pageNumber="13">weddellia</emphasis>
