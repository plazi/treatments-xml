<taxonomicName id="4C454D3E2956AD72FB240D9FFAD63E86" box="[1247,1329,1729,1750]" class="Polychaeta" family="Spionidae" genus="Laonice" higherTaxonomySource="CoL" kingdom="Animalia" order="Spionida" pageId="12" pageNumber="13" phylum="Annelida" rank="genus">Laonice</taxonomicName>
