species; 
<emphasis id="B926EAD03C18FFC4FD5CFD5C4ECBFCC1" box="[747,1162,766,810]" italics="true" pageId="1" pageNumber="36">
<taxonomicName id="4C524D413C18FFC4FD5CFD5C4E0CFCC1" authority="Wu &amp; Wang" authorityName="Wu &amp; Wang" box="[747,1101,766,810]" class="Insecta" family="Formicidae" genus="Dolophra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="36" phylum="Arthropoda" rank="species" species="politae">Dolophra pol√≠tae</taxonomicName>
