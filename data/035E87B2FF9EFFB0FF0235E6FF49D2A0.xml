<materialsCitation id="3B9F3CF9FF9EFFB0FF6C34F9FC69D0E1" ID-GBIF-Occurrence="3026467306" box="[249,978,392,419]" collectionCode="NHMUK" pageId="46" pageNumber="47" specimenCount="1" typeStatus="holotype">
