Cercis 
<normalizedToken id="E599BA9CE466D9854190DECB6BFEA17B" originalValue="Siliquástrum" pageId="null" pageNumber="482">Siliquastrum</normalizedToken>
<authorityName id="C638245170E942FE8F45A9244B94BABC" pageId="null" pageNumber="482">L.</authorityName>
