<taxonomicName id="4C3D4D0CFFD3FFC8FDDBFA78FB98FA0E" authority="SHILENKOV &amp; HEINZ, 1988" authorityName="SHILENKOV &amp; HEINZ" authorityYear="1988" box="[583,1064,1425,1449]" class="Insecta" family="Carabidae" genus="Nebria" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="chitralensis">
