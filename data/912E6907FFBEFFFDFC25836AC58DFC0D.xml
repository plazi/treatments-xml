<heading id="42706F7DFFBEFFFCFC25836AC0F1F9B4" box="[906,1418,1609,1634]" centered="true" fontSize="8" level="2" pageId="6" pageNumber="85" reason="2">
<taxonomicName id="DE87A392FFBEFFFCFC25836AC0F1F9B4" authority="PRIBYL et VANEK, 1981" authorityName="PRIBYL et VANEK" authorityYear="1981" box="[906,1418,1609,1634]" class="Trilobita" family="Aulacopleuridae" genus="Otarion" kingdom="Animalia" order="Proetida" pageId="6" pageNumber="85" phylum="Arthropoda" rank="species" species="lacrimarum">
<emphasis id="2BF30403FFBEFFFCFC25836AC0F1F9B4" bold="true" box="[906,1418,1609,1634]" pageId="6" pageNumber="85">
<emphasis id="2BF30403FFBEFFFCFC25836AC69BF9B4" bold="true" box="[906,992,1610,1634]" italics="true" pageId="6" pageNumber="85">Otarion</emphasis>
cf. 
<emphasis id="2BF30403FFBEFFFCFBA68369C1F0F9B7" bold="true" box="[1033,1163,1609,1633]" italics="true" pageId="6" pageNumber="85">lacrimarum</emphasis>
