<paragraph id="A20A36A3FFCC4D37FF5FDB8EFCFF7E10" blockId="90.[174,816,1277,1302]" box="[174,816,1277,1302]" pageId="90" pageNumber="91">
<emphasis id="90C1EAB1FFCC4D37FF5FDB8EFF2E7E12" bold="true" box="[174,225,1277,1300]" pageId="90" pageNumber="91">SIZE„</emphasis>
Males, 10.0- 
<quantity id="654D9B46FFCC4D37FE8BDB8DFE177E10" box="[378,472,1278,1302]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" pageId="90" pageNumber="91" unit="mm" value="12.5">12.5 mm</quantity>
; ovigerous females, 
<quantity id="654D9B46FFCC4D37FD56DB8DFCE37E10" box="[679,812,1278,1302]" metricMagnitude="-2" metricUnit="m" metricValue="1.045" metricValueMax="1.18" metricValueMin="0.9099999999999999" pageId="90" pageNumber="91" unit="mm" value="10.45" valueMax="11.8" valueMin="9.1">9.1-11.8 mm</quantity>
