<number id="3DBE24EC9B75FFDF01D974546B227FD6" box="[484,546,1393,1419]" pageId="2" pageNumber="3" value="1855.0">1855</number>
; 
<emphasis id="B902EAD99B75FFDF021074546BDD7FD7" box="[557,733,1393,1418]" italics="true" pageId="2" pageNumber="3">Pachodynerius</emphasis>
