<taxonomicName id="4C764D489B75FFDF023E74BC6A337FEE" authority="Fabricius" authorityName="Fabricius" box="[515,819,1433,1459]" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="analis">
