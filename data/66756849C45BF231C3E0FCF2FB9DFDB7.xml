<taxonomicName id="29DCA2DCC45AF23EC33FFAB8FDF9FAA9" authority=", SEM" authorityName="SEM" box="[331,612,1292,1311]" class="Gordioida" family="Chordodidae" genus="Neochordodes" kingdom="Animalia" order="Gordioidea" pageId="7" pageNumber="520" phylum="Nematomorpha" rank="species" species="occidentalis">
