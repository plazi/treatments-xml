<taxonomicName id="29DCA2DCC45AF23EC369FB63FE6EFB5C" authorityName="Carvalho" authorityYear="1942" box="[285,499,1239,1258]" class="Gordioida" family="Chordodidae" genus="Pseudochordodes" kingdom="Animalia" order="Gordioidea" pageId="7" pageNumber="520" phylum="Nematomorpha" rank="species" species="manteri">
