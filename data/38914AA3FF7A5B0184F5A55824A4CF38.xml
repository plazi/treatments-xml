<normalizedToken id="64D1142B6C1CB8766B7B3909B11D0FC6" originalValue="Hiwa-chô">Hiwa-cho</normalizedToken>
, 
<normalizedToken id="B7C73FD5B24920DBF54607618710A9BF" originalValue="Shôbara-shi">Shobara-shi</normalizedToken>
