<taxonomicName id="4C304DEDD04AFFFEFCA7FEA933E40204" box="[871,922,339,362]" class="Magnoliopsida" family="Amaranthaceae" genus="Beta" kingdom="Plantae" order="Caryophyllales" pageId="31" pageNumber="367" phylum="Tracheophyta" rank="genus">
