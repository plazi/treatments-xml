<taxonomicName id="4C424D37FF86FFD29790FB4FFF45FB25" authorityName="Shipman &amp; Gosliner" authorityYear="2015" box="[151,242,1228,1253]" class="Gastropoda" family="Dotidae" genus="Kabeiro" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Nudibranchia" pageId="86" pageNumber="87" phylum="Mollusca" rank="genus">
