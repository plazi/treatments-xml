<number id="FB76D27077708B23FECA9212FE3CE31D" box="[271,393,1874,1900]" pageId="9" pageNumber="10" value="0.33999999999999997" valueMax="0.35" valueMin="0.33">0.33–0.35</number>
; PW, 
<number id="FB76D27077708B23FE099212FDF0E31D" box="[460,581,1874,1900]" pageId="9" pageNumber="10" value="0.33" valueMax="0.34" valueMin="0.32">0.32–0.34</number>
; MW, 
<number id="FB76D27077708B23FD579212FCBEE31D" box="[658,779,1874,1900]" pageId="9" pageNumber="10" value="0.5" valueMax="0.52" valueMin="0.48">0.48–0.52</number>
; 
<collectionCode id="2BAF589277708B23FCD29212FCD1E31D" box="[791,868,1874,1900]" pageId="9" pageNumber="10">AWIV</collectionCode>
, 
<number id="FB76D27077708B23FCAA9212FC13E31D" box="[879,934,1874,1900]" pageId="9" pageNumber="10" value="0.68">0.68</number>
; ANW, 
<number id="FB76D27077708B23FBC19212FBC9E31D" box="[1028,1148,1874,1900]" pageId="9" pageNumber="10" value="0.245" valueMax="0.26" valueMin="0.23">0.23–0.26</number>
; TL, 
<number id="FB76D27077708B23FB7E9212FA81E31D" box="[1211,1332,1874,1900]" pageId="9" pageNumber="10" value="1.67" valueMax="1.68" valueMin="1.66">1.66–1.68</number>
.
</paragraph>
<paragraph id="4D01C05777708B20FF02923AFBAAE55B" blockId="9.[151,1436,1597,2020]" lastBlockId="10.[151,1436,152,298]" lastPageId="10" lastPageNumber="11" pageId="9" pageNumber="10">
<emphasis id="7FCA1C4577708B23FF02923AFE82E3E5" bold="true" box="[199,311,1914,1940]" pageId="9" pageNumber="10">
Male (
<number id="FB76D27077708B23FED2923AFE9DE3E5" box="[279,296,1914,1940]" pageId="9" pageNumber="10" value="2.0">2</number>
