<taxonomicName id="4C194D705B0BFFFDFDB6F7E6FD43FA75" authorityName="Perkins" authorityYear="2011" box="[600,757,1483,1507]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="110" pageNumber="111" phylum="Arthropoda" rank="species" species="bicarinova">
