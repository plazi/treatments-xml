<taxonomicName id="4C3A15EFFFDFFFF6FED4FBEEFE368536" authorityName="Roggero, Barbero, &amp; Palestrini" authorityYear="2015" box="[274,416,1049,1074]" class="Insecta" family="Scarabaeidae" genus="Epidrepanus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
