<taxonomicName id="BDFEA3BDC536FFA1FC3444DFD87AF93B" box="[943,1057,1686,1704]" class="Insecta" family="Curculionidae" genus="Ambrosiophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="218" phylum="Arthropoda" rank="species" species="peregrinus">
