<taxonomicName id="DE6B235818C7D74417FAFD30AD47A47B" authorityName="F. Liu, L. Cai &amp; Crous, in Liu, Bonthond, Groenewald, Cai &amp; Crous, Stud. Mycol. 92: 402" authorityYear="2018" class="Sordariomycetes" family="Discosiaceae" genus="Sporocadus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Sporocadus rosigena" order="Amphisphaeriales" pageId="0" pageNumber="173" phylum="Ascomycota" rank="species" species="rosigena">
