<taxonomicName id="CCD34D19FF9BFFB9FF09DCEFFC78FB7E" authority="Gomez, Fleeger, Rocha-Olivares and Foltz, 2004" authorityName="Gomez, Fleeger, Rocha-Olivares and Foltz" authorityYear="2004" box="[233,905,1208,1227]" class="Hexanauplia" family="Canthocamptidae" genus="Cletocamptus" kingdom="Animalia" order="Harpacticoida" pageId="11" pageNumber="3111" phylum="Arthropoda" rank="species" species="sinaloensis">
