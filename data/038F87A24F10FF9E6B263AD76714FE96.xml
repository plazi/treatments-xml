<emphasis id="B952EAA64F10FF906B7A38EF651FFC38" box="[340,358,988,1013]" italics="true" pageId="6" pageNumber="7">l’</emphasis>
) 
<number id="3DEE24934F10FF906B5938EF65FFFC3B" box="[375,390,988,1014]" pageId="6" pageNumber="7" value="6.0">6</number>
(
<number id="3DEE24934F10FF906BB838EF65BFFC3B" box="[406,454,988,1014]" pageId="6" pageNumber="7" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
