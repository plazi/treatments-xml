<emphasis id="B952EAA64F10FF906884388766BBFC00" box="[682,706,948,973]" italics="true" pageId="6" pageNumber="7">l’’</emphasis>
) 
<number id="3DEE24934F10FF9068F93887668DFC03" box="[727,756,948,974]" pageId="6" pageNumber="7" value="26.0">26</number>
(
<number id="3DEE24934F10FF9069263887672CFC03" box="[776,853,948,974]" pageId="6" pageNumber="7" value="26.5" valueMax="27.0" valueMin="26.0">26–27</number>
