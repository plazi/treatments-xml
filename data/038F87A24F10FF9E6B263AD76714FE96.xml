<number id="3DEE24934F10FF90685738EF66FDFC3B" box="[633,644,988,1014]" pageId="6" pageNumber="7" value="1.0">1</number>
/ 
<number id="3DEE24934F10FF9068BE38EF66E6FC3B" box="[656,671,988,1014]" pageId="6" pageNumber="7" value="3.0">3</number>
from dorsal base; tarsus 
<number id="3DEE24934F10FF9069EB38EF67ADFC3B" box="[965,980,988,1014]" pageId="6" pageNumber="7" value="8.0">8</number>
(
<number id="3DEE24934F10FF9069CA38EF606AFC3B" box="[996,1043,988,1014]" pageId="6" pageNumber="7" value="7.5" valueMax="8.0" valueMin="7.0">7–8</number>
