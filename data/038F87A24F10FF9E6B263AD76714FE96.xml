<number id="3DEE24934F10FF906B473F37650FFBD3" box="[361,374,1028,1054]" pageId="6" pageNumber="7" value="5.0">5</number>
­rayed, tarsal solenidion knobbed. Leg 
<date id="FF9810744F10FF9069793F3767EAFBD3" box="[855,915,1028,1054]" pageId="6" pageNumber="7" value="1938-02">
II 
<number id="3DEE24934F10FF9069583F3767EAFBD3" box="[886,915,1028,1054]" pageId="6" pageNumber="7" value="38.0">38</number>
</date>
(
<number id="3DEE24934F10FF9069893F37678AFBD3" box="[935,1011,1028,1054]" pageId="6" pageNumber="7" value="37.5" valueMax="39.0" valueMin="36.0">36–39</number>
) long, femur 
<number id="3DEE24934F10FF906E8E3F3760C4FBD3" box="[1184,1213,1028,1054]" pageId="6" pageNumber="7" value="10.0">10</number>
(
<number id="3DEE24934F10FF906EFF3F376167FBD3" box="[1233,1310,1028,1054]" pageId="6" pageNumber="7" value="10.5" valueMax="11.0" valueMin="10.0">10–11</number>
), basiventral femoral setae (
<emphasis id="B952EAA64F10FF9068113F1F6622FB88" box="[575,603,1068,1093]" italics="true" pageId="6" pageNumber="7">bv</emphasis>
) 
<number id="3DEE24934F10FF9068423F1F66F0FB8B" box="[620,649,1068,1094]" pageId="6" pageNumber="7" value="14.0">14</number>
(
<number id="3DEE24934F10FF9068B43F1F669EFB8B" box="[666,743,1068,1094]" pageId="6" pageNumber="7" value="13.5" valueMax="15.0" valueMin="12.0">12–15</number>
); genu 
