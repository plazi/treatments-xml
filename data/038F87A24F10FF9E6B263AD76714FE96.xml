<number id="3DEE24934F10FF9069C138876072FC03" box="[1007,1035,948,974]" pageId="6" pageNumber="7" value="11.0">11</number>
(
<number id="3DEE24934F10FF906E3138876015FC03" box="[1055,1132,948,974]" pageId="6" pageNumber="7" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
