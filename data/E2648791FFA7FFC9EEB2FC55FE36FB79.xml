<taxonomicName id="ADCD4D04FFA7FFC9EF4CFB8EFD27FBC9" authority="Zavattari, 1913" authorityName="Zavattari" authorityYear="1913" box="[361,730,1078,1104]" class="Insecta" family="Mutillidae" genus="Mutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="584" phylum="Arthropoda" rank="species" species="gutrunae">
