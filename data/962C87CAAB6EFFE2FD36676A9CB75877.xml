<taxonomicName id="D9854D5FAB6EFFE2FFB5659F9CF05C17" authority="Michener" authorityName="Michener" box="[918,1206,508,532]" class="Insecta" family="Halictidae" genus="Corynura" kingdom="Animalia" order="Hymenoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="subGenus" subGenus="Callistochlora">
