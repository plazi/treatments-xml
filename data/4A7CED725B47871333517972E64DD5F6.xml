<taxonomicName id="91ABFDE0442C795778D82B786EE55BA3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181963" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole jeannei Wilson" lsidName-HNS="Pheidole jeannei Wilson" order="Hymenoptera" pageNumber="538" phylum="Arthropoda" rank="species" species="jeannei">Pheidole jeannei</taxonomicName>
