<taxonomicName id="4C4E4D2A0628FFA0FECAF8A1FE24F8FC" authority="Li &amp; Yin, 2021" authorityName="Li &amp; Yin" authorityYear="2021" box="[280,497,1863,1887]" class="Insecta" family="Staphylinidae" genus="Syndicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="117" phylum="Arthropoda" rank="species" species="wangjisheni" status="sp. nov.">
<emphasis id="B93AEABB0628FFA0FECAF8A1FE24F8FC" box="[280,497,1863,1887]" italics="true" pageId="3" pageNumber="117">Syndicus wangjisheni</emphasis>
</taxonomicName>
<emphasis id="B93AEABB0628FFA0FE25F8AEFD92F8C3" bold="true" box="[503,583,1864,1888]" pageId="3" pageNumber="117">
