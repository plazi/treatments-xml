<taxonomicName id="9F8BCBA25E05D108342082CB80E43D4C" authorityName="Monniot C &amp; Monniot F" authorityYear="1991" box="[282,472,1773,1795]" class="Ascidiacea" family="Ascidiidae" genus="Fimbrora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enterogona" pageId="3" pageNumber="532" phylum="Chordata" rank="species" species="calsubia">
<emphasis id="6AFF6C335E05D108342082CB80E43D4C" box="[282,472,1773,1795]" italics="true" pageId="3" pageNumber="532">Fimbrora calsubia</emphasis>
