<paragraph id="8C9E7D2307F387C659DF1520FF37F512" pageId="4" pageNumber="49">
<taxonomicName id="6A2BA817D1BA50F2AB62151454F30EFE" LSID="urn:lsid:zoobank.org:act:3B810496-A96D-4D12-904F-B3C695EA48FD" class="Polychaeta" family="Sabellidae" genus="Amphicorina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amphicorina ezoensis" order="Sabellida" pageId="4" pageNumber="49" phylum="Annelida" rank="species" species="ezoensis">Amphicorina ezoensis</taxonomicName>
