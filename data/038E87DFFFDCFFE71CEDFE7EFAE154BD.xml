<heading id="D0D081A5FFDCFFE61CEDFE7EFE2F5597" bold="true" box="[151,400,439,466]" fontSize="11" level="1" pageId="10" pageNumber="11" reason="1">
<emphasis id="B953EADBFFDCFFE61CEDFE7EFE2F5597" bold="true" box="[151,400,439,466]" pageId="10" pageNumber="11">
<taxonomicName id="4C274D4AFFDCFFE61CEDFE7EFEF85594" box="[151,327,439,465]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="ciliata" status="sp. n.">
