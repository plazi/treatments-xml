; clypeal index 
<number id="304824F8793F0C223FB7FCE6FCEAFC86" box="[726,845,852,878]" pageId="15" pageNumber="16" value="0.8600000000000001" valueMax="1.11" valueMin="0.61">0.61–1.11</number>
; MOQ index 
<number id="304824F8793F0C223E8FFCE6FBC0FC86" box="[1006,1127,852,878]" pageId="15" pageNumber="16" value="6.355" valueMax="6.88" valueMin="5.83">5.83–6.88</number>
; legs length: I 
<number id="304824F8793F0C223872FCE6FADAFC86" box="[1299,1405,852,878]" pageId="15" pageNumber="16" value="8.25" valueMax="10.2" valueMin="6.3">6.3–10.2</number>
, II 
<number id="304824F8793F0C223DF6FCC9FEA7FC7D" box="[151,256,891,917]" pageId="15" pageNumber="16" value="8.350000000000001" valueMax="10.3" valueMin="6.4">6.4–10.3</number>
, III 
<number id="304824F8793F0C223C50FCC9FE3DFC7D" box="[305,410,891,917]" pageId="15" pageNumber="16" value="8.7" valueMax="10.8" valueMin="6.6">6.6–10.8</number>
, IV 
<number id="304824F8793F0C223CADFCC9FD91FC7D" box="[460,566,891,917]" pageId="15" pageNumber="16" value="11.05" valueMax="13.8" valueMin="8.3">8.3–13.8</number>
