<quantity id="242F1C8EFFB6F07BD90DFF2A9F34BBFC" box="[370,542,152,178]" metricMagnitude="-3" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="8" pageNumber="45" unit="mm" value="1.65" valueMax="1.8" valueMin="1.5">1.50–1.80 mm</quantity>
