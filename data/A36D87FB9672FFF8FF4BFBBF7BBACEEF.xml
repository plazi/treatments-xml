<paragraph id="2B7B36ED9672FFF8FF4BFBBF7984CF3F" blockId="3.[151,1437,1054,1510]" box="[151,184,1054,1078]" pageId="3" pageNumber="398">
<heading id="703381819672FFF8FF4BFBBF7984CF3F" box="[151,184,1054,1078]" pageId="3" pageNumber="398">
<emphasis id="19B0EAFF9672FFF8FF4BFBBF7984CF3F" bold="true" box="[151,184,1054,1078]" pageId="3" pageNumber="398">
<taxonomicName id="ECC44D6E9672FFF8FF4BFBBF798FCF3F" authority="(Kolombatovic, 1891)" baseAuthorityName="Kolombatovic" baseAuthorityYear="1891" box="[151,179,1054,1078]" class="Actinopterygii" family="Gobiidae" genus="Millerigobius" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="398" phylum="Chordata" rank="species" species="macrocephalus">17</taxonomicName>
.
