<taxonomicName id="4C064D52FC57FFAFFF13B83A2CCFFCC3" authorityName="WALL" authorityYear="2007" box="[249,390,836,857]" class="Insecta" family="Miridae" genus="Morobea" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="spectabilis">
