<taxonomicName id="4C0BC2F7FFEBFFC7D411885F83B9F674" box="[1365,1436,1847,1868]" class="Insecta" family="Cicadellidae" genus="Delopa" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="573" phylum="Arthropoda" rank="genus">
