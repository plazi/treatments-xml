<taxonomicName id="09FC4D14FFF8FFCCFBB6F940E5FCF956" authority="Ujvári, 2012" authorityName="Ujvári" authorityYear="2012" box="[1100,1270,1717,1738]" class="Arachnida" family="Zerconidae" genus="Aspar" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="279" phylum="Arthropoda" rank="species" species="tamalpaisensis" status="sp. n.">
