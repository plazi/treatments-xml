<taxonomicName id="09FC4D14FFF9FFCDFB6EFC3EE5C7FC7C" box="[1172,1229,971,992]" class="Arachnida" family="Zerconidae" genus="Aspar" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="280" phylum="Arthropoda" rank="genus">
<emphasis id="FC88EA85FFF9FFCDFB6EFC3EE5C7FC7C" box="[1172,1229,971,992]" italics="true" pageId="5" pageNumber="280">Aspar</emphasis>
</taxonomicName>
, of 
<taxonomicName id="09FC4D14FFF9FFCDFEC7FC1DE0A7FC61" authorityName="TRAGARDH" authorityYear="1931" box="[317,429,1000,1021]" class="Arachnida" family="Zerconidae" genus="Parazercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="280" phylum="Arthropoda" rank="genus">
<emphasis id="FC88EA85FFF9FFCDFEC7FC1DE0A7FC61" box="[317,429,1000,1021]" italics="true" pageId="5" pageNumber="280">Parazercon</emphasis>
</taxonomicName>
