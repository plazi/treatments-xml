<taxonomicName id="4C314D35FFF2FFF2FB46F900FAEE0628" baseAuthorityName="Bonaparte" baseAuthorityYear="1835" box="[1267,1402,1769,1794]" class="Chondrichthyes" family="Torpedinidae" genus="Torpedo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="16" pageNumber="475" phylum="Chordata" rank="species" species="nobiliana">
