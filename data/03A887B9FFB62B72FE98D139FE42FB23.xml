<taxonomicName id="4C014D2CFFB62B72FBB8D172FB62FB92" authorityName="Agren" authorityYear="1903" box="[1064,1248,1119,1144]" class="Entognatha" family="Brachystomellidae" genus="Brachystomella" kingdom="Animalia" order="Collembola" pageId="1" phylum="Arthropoda" rank="genus">
