<geoCoordinate id="C6DB50719E45FF8DFD81719DFD7F13D0" box="[562,649,1415,1437]" degrees="39.55" direction="north" orientation="latitude" pageId="65" pageNumber="65" precision="555" value="39.55">39.55N</geoCoordinate>
