<quantity id="4CD89B34D41FF85E5DBEFE8DE36AFEC7" box="[1195,1295,332,356]" metricMagnitude="-2" metricUnit="m" metricValue="2.18" pageId="7" pageNumber="500" unit="mm" value="21.8">21.8 mm</quantity>
long, 
<number id="3DE824F6D41FF85E5C43FE8DE3F9FEC7" box="[1366,1436,332,356]" pageId="7" pageNumber="500" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
