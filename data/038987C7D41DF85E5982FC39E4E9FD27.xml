<taxonomicName id="4C204D52D41FF85E5AECFE8CE239FEC7" box="[1017,1116,333,356]" class="Gastropoda" family="Triphoridae" genus="Eutriphora" kingdom="Animalia" pageId="7" pageNumber="500" phylum="Mollusca" rank="species" species="costai">
