<taxonomicName id="89814D13FFB0FB70FE5CDDBCFA91FF37" authority="Hirschmann &amp; Zirngiebl-Nicol, 1961: 24" authorityName="Hirschmann &amp; Zirngiebl-Nicol" authorityPageNumber="24" authorityYear="1961" box="[391,1390,197,222]" class="Arachnida" family="Trematuridae" genus="Trichouropoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="16" pageNumber="117" phylum="Arthropoda" rank="species" species="dalarnaensis" subGenus="Ipiduropoda">
