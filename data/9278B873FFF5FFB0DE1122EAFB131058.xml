<materialsCitation id="AAB90338FFF5FFB0DEE82133FC581022" collectingDate="2017-11" collectorName="La Cruz" country="Argentina" latitude="-29.09639" location="Corrientes" longLatPrecision="20" longitude="-56.9175" pageId="11" pageNumber="426" specimenCount="1" stateProvince="Corrientes">
