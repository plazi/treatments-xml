<quantity id="4CAC5FB3FFC11823FBB50A63FBD8FABA" box="[1029,1128,1364,1391]" metricMagnitude="0" metricUnit="m" metricValue="1.8" pageId="123" pageNumber="124" unit="m" value="1.8">1,800 m</quantity>
