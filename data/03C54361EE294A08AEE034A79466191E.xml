<taxonomicName id="4C6C89F4EE294A08AF4D357D92841C36" authority="Liang, 2009" authorityName="Liang" authorityYear="2009" box="[314,732,323,349]" class="Insecta" family="Tetrigidae" genus="Aspiditettix" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="405" phylum="Arthropoda" rank="species" species="yunnanensis">
