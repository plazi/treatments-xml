<taxonomicName id="4C0D4D670524FFBEFC916D0D0EE024CA" authority="Sss., Hololectotypus" authorityName="Sss., Hololectotypus" class="Insecta" family="Eumastacidae" genus="Paramastax" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="206" phylum="Arthropoda" rank="species" species="brevipennis">Paramastax brevipennis Sss., Hololectotypus</taxonomicName>
