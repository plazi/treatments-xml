<paragraph id="3B20F566C8A6A39A92D01F0466C3578B" pageId="0" pageNumber="85">
<taxonomicName id="65944375A3CEA1EC13AC0D0097E769B2" class="Magnoliopsida" family="Euphorbiaceae" genus="Acalypha" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acalypha diminuta" order="Malpighiales" pageId="0" pageNumber="85" phylum="Tracheophyta" rank="species" species="diminuta">
<emphasis id="CC46C530D035CF95AC1D8AC4F85631F9" italics="true" pageId="0" pageNumber="85">Acalypha diminuta</emphasis>
</taxonomicName>
