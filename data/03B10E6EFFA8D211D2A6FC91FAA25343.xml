<taxonomicName id="4C18C4FBFFA8D211D2A6FC91FE455403" authority="Morgan, 1925" authorityName="Morgan" authorityYear="1925" box="[151,487,797,823]" class="Insecta" family="Thripidae" genus="Cercyothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="389" phylum="Arthropoda" rank="genus">
<emphasis id="B96C636AFFA8D211D2A6FC91FE455403" bold="true" box="[151,487,797,823]" pageId="2" pageNumber="389">
<emphasis id="B96C636AFFA8D211D2A6FC91FE905403" bold="true" box="[151,306,797,823]" italics="true" pageId="2" pageNumber="389">Cercyothrips</emphasis>
<bibRefCitation id="EF89C289FFA8D211D308FC91FE455403" author="Morgan" box="[313,487,797,823]" pageId="2" pageNumber="389" refString="Morgan, A. C. (1925) A new genus, a new subgenus and seven new species of Thysanoptera from Porto Rico. Florida Entomologist, 9 (1), 1 - 9. http: // dx. doi. org / 10.2307 / 3492636" type="journal article" year="1925">
Morgan, 
<number id="3DD0AD5FFFA8D211D39DFC91FE455403" box="[428,487,797,823]" pageId="2" pageNumber="389" value="1925.0">1925</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
