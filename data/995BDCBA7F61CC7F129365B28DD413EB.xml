<taxonomicName id="CCD65A1F01BE1190CA977095D8CBD6BC" LSID="http://species-id.net/wiki/Zealeuctra_wachita" authority="Ricker &amp; Ross" class="Insecta" family="Leuctridae" genus="Zealeuctra" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zealeuctra wachita" order="Plecoptera" pageId="23" pageNumber="40" phylum="Arthropoda" rank="species" species="wachita">
