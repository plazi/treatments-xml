<taxonomicName id="6031C736B1A6DA3384C23D1A478661BA" authorityName="Gosliner &amp; Johnson" authorityYear="2018" class="Gastropoda" family="Chromodorididae" genus="Hypselodoris" kingdom="Animalia" lsidName="Hypselodoris juniperae" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="species" species="juniperae">Hypselodoris cf. juniperae</taxonomicName>
6 mm 
<emphasis id="41660C15148285B6181BF2366E0FD853" bold="true" pageId="0" pageNumber="73">B</emphasis>
<taxonomicName id="E1440D2CE18FB4C3FCC2D49BA01B8C28" authorityName="Rudman" authorityYear="1977" class="Gastropoda" family="Chromodorididae" genus="Hypselodoris" kingdom="Animalia" lsidName="Hypselodoris kanga" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="species" species="kanga">Hypselodoris cf. kanga</taxonomicName>
40 mm 
<emphasis id="7288405138967EE5175A05738F770C6A" bold="true" pageId="0" pageNumber="73">C</emphasis>
<taxonomicName id="C07B3F24D1A89215FDAF1281FA54A174" authorityName="Gosliner &amp; Johnson" authorityYear="2018" class="Gastropoda" family="Chromodorididae" genus="Hypselodoris" kingdom="Animalia" lsidName="Hypselodoris katherinae" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="species" species="katherinae">
