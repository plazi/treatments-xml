<taxonomicName id="B23436EEC4993956181C79290A488233" class="Magnoliopsida" family="Burseraceae" genus="Bursera" kingdom="Plantae" lsidName="Bursera" order="Sapindales" pageId="0" pageNumber="65" phylum="Tracheophyta" rank="genus">
