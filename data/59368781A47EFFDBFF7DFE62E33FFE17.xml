<emphasis id="E3EBEA85A47FFFDBFB08FE83E310FEF4" box="[1250,1336,272,297]" italics="true" pageId="33" pageNumber="34">Chubut</emphasis>
, 
<quantity id="16679B72A47FFFDBFAA2FE83E354FEF7" box="[1352,1404,272,298]" metricMagnitude="3" metricUnit="m" metricValue="8.0" pageId="33" pageNumber="34" unit="km" value="8.0">8km</quantity>
S. of Rada Tilly, 
<number id="675724B0A47FFFDBFEA2FEABE740FE8F" box="[328,360,312,338]" pageId="33" pageNumber="34" value="45.0">45</number>
° 
<number id="675724B0A47FFFDBFE98FEABE7BAFE8F" box="[370,402,312,338]" pageId="33" pageNumber="34" value="59.0">59</number>
’043S, 
<number id="675724B0A47FFFDBFE0DFEABE42EFE8F" box="[487,518,312,338]" pageId="33" pageNumber="34" value="72.0">72</number>
° 
<number id="675724B0A47FFFDBFDE5FEABE405FE8F" box="[527,557,312,338]" pageId="33" pageNumber="34" value="36.0">36</number>
’ 
<number id="675724B0A47FFFDBFDD8FEABE449FE8F" box="[562,609,312,338]" pageId="33" pageNumber="34" value="322.0">322</number>
W, 
<quantity id="16679B72A47FFFDBFD67FEABE4EDFE8F" box="[653,709,312,338]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="33" pageNumber="34" unit="m" value="30.0">30m</quantity>
, 
<date id="A5211057A47FFFDBFD3EFEABE57BFE8F" box="[724,851,312,338]" pageId="33" pageNumber="34" value="2003-11-24">
<number id="675724B0A47FFFDBFD3EFEABE4DCFE8F" box="[724,756,312,338]" pageId="33" pageNumber="34" value="24.0">24</number>
.xi. 
<number id="675724B0A47FFFDBFCF9FEABE57BFE8F" box="[787,851,312,338]" pageId="33" pageNumber="34" value="2003.0">2003</number>
