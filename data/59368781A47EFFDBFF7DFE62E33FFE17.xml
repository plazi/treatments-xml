<collectionCode id="B78EAE52A47FFFDBFF7DFE1BE77AFE7F" box="[151,338,392,418]" pageId="33" pageNumber="34">EXTRACTION</collectionCode>
E.A.B.Almeida # 
<number id="675724B0A47FFFDBFDCCFE1BE46DFE7F" box="[550,581,392,418]" pageId="33" pageNumber="34" value="62.0">62</number>
