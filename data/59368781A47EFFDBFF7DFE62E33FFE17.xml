<collectionCode id="B78EAE52A47FFFDBFF7DFEF3E6DAFEA7" box="[151,242,352,378]" httpUri="http://grbio.org/cool/pyng-6456" name="Museo Argentino de Ciencias Naturales Bernardino Rivadavia" pageId="33" pageNumber="34">MACN</collectionCode>
