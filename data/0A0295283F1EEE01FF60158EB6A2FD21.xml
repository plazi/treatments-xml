, ex. 
<taxonomicName id="45AB5FBD3F1EEE01FB94173EB38BFD00" box="[1076,1277,630,654]" class="Liliopsida" family="Araceae" genus="Pistia" kingdom="Plantae" order="Alismatales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="stratiodes">
