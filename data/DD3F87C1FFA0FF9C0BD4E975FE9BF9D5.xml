<taxonomicName id="92964D54FFA0FF9C0BD4E975FDE0FBA4" authority="Dufour, 1843" authorityName="Dufour" authorityYear="1843" box="[151,545,1086,1112]" class="Insecta" family="Curculionidae" genus="Cleopus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="34" phylum="Arthropoda" rank="species" species="uncinatus">
<emphasis id="67E2EAC5FFA0FF9C0BD4E975FDE0FBA4" bold="true" box="[151,545,1086,1112]" pageId="3" pageNumber="34">
<emphasis id="67E2EAC5FFA0FF9C0BD4E975FEB8FBA4" bold="true" box="[151,377,1086,1112]" italics="true" pageId="3" pageNumber="34">Cleopus uncinatus</emphasis>
<bibRefCitation id="31074B26FFA0FF9C0AC3E975FDE0FBA4" author="Dufour" box="[384,545,1086,1112]" pageId="3" pageNumber="34" refString="Dufour, L. (1843) Excursion entomologique dans les montagnes de la vallee d'Ossau. Bulletin de la Societe des Sciences, Lettres et Arts de Pau, 5 - 118." type="book chapter" year="1843">
Dufour, 
<number id="E35E24F0FFA0FF9C0AA4E975FDE0FBA4" box="[487,545,1086,1112]" pageId="3" pageNumber="34" value="1843.0">1843</number>
</bibRefCitation>
</emphasis>
