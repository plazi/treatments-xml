<taxonomicName id="4C114D5BB549FED5C6C272F7E706F933" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[948,1149,1688,1709]" class="Arachnida" family="Lamponidae" genus="Notsodipus" kingdom="Animalia" order="Araneae" pageId="274" pageNumber="275" phylum="Arthropoda" rank="species" species="upstart" status="new species">
<emphasis id="B965EACAB549FED5C6C272F7E706F933" box="[948,1149,1688,1709]" italics="true" pageId="274" pageNumber="275">Notsodipus upstart</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A25657B1B549FED5C1FB72F7E18CF956" pageId="274" pageNumber="275" rank="species">new species</taxonomicNameLabel>
(circle), 
<taxonomicName id="4C114D5BB549FED5C60D72DCE77CF956" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[891,1031,1715,1736]" class="Arachnida" family="Lamponidae" genus="Notsodipus" kingdom="Animalia" order="Araneae" pageId="274" pageNumber="275" phylum="Arthropoda" rank="species" species="muckera" status="new species">
<emphasis id="B965EACAB549FED5C60D72DCE77CF956" box="[891,1031,1715,1736]" italics="true" pageId="274" pageNumber="275">N. muckera</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A25657B1B549FED5C15072DCE7C2F956" box="[1062,1209,1715,1736]" pageId="274" pageNumber="275" rank="species">new species</taxonomicNameLabel>
(squares), and 
<taxonomicName id="4C114D5BB549FED5C63272A1E0CAF97D" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[836,945,1742,1763]" class="Arachnida" family="Lamponidae" genus="Notsodipus" kingdom="Animalia" order="Araneae" pageId="274" pageNumber="275" phylum="Arthropoda" rank="species" species="quobba" status="new species">
<emphasis id="B965EACAB549FED5C63272A1E0CAF97D" box="[836,945,1742,1763]" italics="true" pageId="274" pageNumber="275">N. quobba</emphasis>
</taxonomicName>
