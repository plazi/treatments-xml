<taxonomicName id="4C014D2AFFCE7C03FF39FEB4FE520997" authorityName="Randell" authorityYear="1964" box="[239,418,366,392]" class="Insecta" family="Gryllidae" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="373" phylum="Arthropoda" rank="tribe" tribe="Anurogryllina">Anurogryllina</taxonomicName>
