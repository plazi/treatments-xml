<emphasis id="B94FEA9F732EAB7C4385F8A5FE85F815" bold="true" box="[199,273,1909,1934]" pageId="91" pageNumber="92">Notes.</emphasis>
<taxonomicName id="4C3B4D0E732EAB7C425BF8A5FDA9F815" baseAuthorityName="Crawford" baseAuthorityYear="1907" box="[281,573,1909,1934]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="quebecense">
