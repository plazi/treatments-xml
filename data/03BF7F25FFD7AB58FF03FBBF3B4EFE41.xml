<taxonomicName id="4C16B5B0FFD7AB26FF61FBBF3ED1FB9F" baseAuthorityName="Wood" baseAuthorityYear="1985" box="[245,421,1125,1150]" class="Insecta" family="Tachinidae" genus="Erythromelana" kingdom="Animalia" order="Diptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="genus">
