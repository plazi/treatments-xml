<taxonomicName id="4C5CB18EFFD1FFFDFFA55287FEF1F3C4" baseAuthorityName="Rage &amp; Rocek" baseAuthorityYear="1983" box="[113,314,424,447]" class="Reptilia" family="Colubridae" genus="Palaeonatrix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="19" pageNumber="432" phylum="Chordata" rank="species" species="lehmani">
