<taxonomicName id="4C5D96A6B72FFFD8FE12FBC26532D686" authorityName="Mcalpine" authorityYear="2019" box="[432,635,701,723]" class="Insecta" family="Lauxaniidae" genus="Paranomina" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="79" phylum="Arthropoda" rank="species" species="danielsi">
