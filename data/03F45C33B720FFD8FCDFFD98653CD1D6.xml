<quantity id="4CA540C0B72FFFD8FC15FBA4634AD6B9" box="[951,1027,728,749]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="8" pageNumber="79" unit="mm" value="0.2">0.2 mm</quantity>
