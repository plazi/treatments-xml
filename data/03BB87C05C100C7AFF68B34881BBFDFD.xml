in the 
<location id="8ECD600D5C120C7AFBFEB4268149FDFD" LSID="urn:lsid:plazi:treatment:03BB87C05C100C7AFF68B34881BBFDFD:8ECD600D5C120C7AFBFEB4268149FDFD" box="[1025,1117,547,573]" country="China" name="Oriental" pageId="10" pageNumber="217" stateProvince="Yunnan">Oriental</location>
