<taxonomicName id="0B4B766A01D08C649DD1F226C757F911" class="Chlorophyceae" family="Volvocaceae" genus="Eudorina" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eudorina" order="Chlamydomonadales" pageId="0" pageNumber="1" phylum="Chlorophyta" rank="genus">
