<paragraph id="8BFC43205115011200C9F87653A9F860" blockId="1.[151,1435,1985,2035]" pageId="1" pageNumber="34">
<keyLead id="30B2E0155115011200C9F87653A9F860" pageId="1" pageNumber="34">
<number id="3D8B51075115011200C9F8765696F845" box="[151,164,1985,2006]" pageId="1" pageNumber="34" value="1.0">1</number>
