<taxonomicName id="64EF4D359E6EFFA6FB9C72D2FB571092" authorityName="G.R.Gray" authorityYear="1849" box="[1071,1185,1736,1759]" class="Aves" family="Estrildidae" genus="Neochmia" kingdom="Animalia" order="Passeriformes" pageId="106" pageNumber="106" phylum="Chordata" rank="genus">
