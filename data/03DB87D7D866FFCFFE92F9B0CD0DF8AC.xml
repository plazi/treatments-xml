<emphasis id="B906EAD3D866FFCBFE03F975CCD3F91E" box="[409,671,1748,1771]" italics="true" pageId="3" pageNumber="4">
<taxonomicName id="4C724D42D866FFCBFE03F975CCD7F91E" authority="Alderslade, 2003" authorityName="Alderslade" authorityYear="2003" box="[409,667,1748,1771]" class="Anthozoa" family="Alcyoniidae" genus="Notodysiferus" kingdom="Animalia" order="Alcyonacea" pageId="3" pageNumber="4" phylum="Cnidaria" rank="species" species="dhondtae" status="n. gen., n. sp.">Notodysiferus dhondtae</taxonomicName>
