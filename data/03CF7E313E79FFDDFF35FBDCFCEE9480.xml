<taxonomicName id="4C66B4A43E78FFDDFB6FFCAEFAB1939B" box="[1229,1319,872,897]" class="Gastropoda" family="Orthalicidae" genus="Orthalicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="25" pageNumber="35" phylum="Mollusca" rank="species" species="sultana">
