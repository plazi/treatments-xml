<emphasis id="B9618A28B461FFC3FF39550CFF68F885" bold="true" box="[136,157,1846,1872]" italics="true" pageId="5" pageNumber="220">6.</emphasis>
<taxonomicName id="4C152DB9B461FFC3FF0C550CFD37F885" authority="Kutzing 1844, p. 138" authorityName="Kutzing" authorityPageNumber="138" authorityYear="1844" box="[189,706,1846,1873]" class="Bacillariophyceae" family="Biddulphiaceae" genus="Biddulphia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Biddulphiales" pageId="5" pageNumber="220" phylum="Ochrophyta" rank="species" species="trilocularis">
