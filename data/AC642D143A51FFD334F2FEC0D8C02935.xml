<specimenCount id="32CB578B3A51FFD337F9FE3ADEB72AEC" box="[924,1004,438,463]" pageId="24" pageNumber="317" type="generic">
<quantity id="E33531E73A51FFD337F9FE3ADEB32AEC" box="[924,1000,438,463]" metricMagnitude="2" metricUnit="m" metricValue="7.95" pageId="24" pageNumber="317" unit="m" value="795.0">795 m</quantity>
