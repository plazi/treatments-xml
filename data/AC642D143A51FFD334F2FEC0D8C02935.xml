<quantity id="E33531E73A51FFD33667FE3ADF132AEC" box="[514,584,438,463]" metricMagnitude="-1" metricUnit="m" metricValue="6.1" pageId="24" pageNumber="317" unit="cm" value="61.0">61 cm</quantity>
