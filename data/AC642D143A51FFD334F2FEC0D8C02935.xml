<collectingCountry id="5CDADC923A51FFD334B1FE73DC752935" box="[212,302,511,535]" name="Norway" pageId="24" pageNumber="317">Norway</collectingCountry>
and 
<collectingCountry id="5CDADC923A51FFD33500FE72DCE32935" box="[357,440,510,535]" name="Iceland" pageId="24" pageNumber="317">Iceland</collectingCountry>
to 
<collectingCountry id="5CDADC923A51FFD335BEFE72DF0D2935" box="[475,598,510,535]" name="Mauritania" pageId="24" pageNumber="317">Mauritania</collectingCountry>
