<quantity id="E33531E73A51FFD3303DFE56D9942AD0" box="[1112,1231,474,499]" metricMagnitude="2" metricUnit="m" metricValue="2.1" metricValueMax="3.7" metricValueMin="0.5" pageId="24" pageNumber="317" unit="m" value="210.0" valueMax="370.0" valueMin="50.0">
50– 
<specimenCount id="32CB578B3A51FFD330E7FE56D9942AD0" box="[1154,1231,474,499]" pageId="24" pageNumber="317" type="generic">370 m</specimenCount>
</quantity>
