<taxonomicName id="E6C342E2B41DFF8C667CF0D9FC72892F" authority="(du Buysson)" baseAuthorityName="du Buysson" baseAuthorityYear="1908" box="[550,949,495,521]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="bhoutanensis">
