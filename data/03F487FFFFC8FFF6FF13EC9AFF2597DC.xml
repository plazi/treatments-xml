<taxonomicName id="4C5D4D6AFFC6FFF7FEA5E8C9FD609685" authority="Berents 1983" authorityName="Berents" authorityYear="1983" box="[289,678,1976,2000]" class="Malacostraca" family="Maeridae" genus="Elasmopus" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="651" phylum="Arthropoda" rank="species" species="crenulatus">
<emphasis id="B929EAFBFFC6FFF7FEA5E8C9FDC8969A" box="[289,526,1977,2000]" italics="true" pageId="8" pageNumber="651">Elasmopus crenulatus</emphasis>
