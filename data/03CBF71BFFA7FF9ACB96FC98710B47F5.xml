<emphasis id="B9169A1FFFA7FF95CA47FC25713A46FB" box="[355,553,937,958]" italics="true" pageId="7" pageNumber="1144">
<taxonomicName id="4C623D8EFFA7FF95CA47FC2572DA46FB" box="[355,457,937,958]" class="Insecta" family="Ichneumonidae" genus="Mesoleius" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="1144" phylum="Arthropoda" rank="genus">Mesoleius</taxonomicName>
