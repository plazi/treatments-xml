<quantity id="4C889B385E27FFD31AD25505FEC9FD90" box="[213,361,542,567]" metricMagnitude="-1" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="31" pageNumber="186" unit="mm" value="200.0" valueMax="300.0" valueMin="100.0">100-300 mm</quantity>
