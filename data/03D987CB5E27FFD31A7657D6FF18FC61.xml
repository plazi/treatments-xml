<quantity id="4C889B385E27FFD31BDF5561FD90FD34" box="[472,560,634,659]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="31" pageNumber="186" unit="mm" value="4.5" valueMax="5.0" valueMin="4.0">4-5 mm</quantity>
