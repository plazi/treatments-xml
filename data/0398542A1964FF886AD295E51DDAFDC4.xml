[
<emphasis id="B945392E1964FF886AC995E41D79F986" box="[181,600,1514,1537]" italics="true" pageId="91" pageNumber="91">
<taxonomicName id="4C319EBF1964FF886AC995E41D3BF986" box="[181,538,1514,1537]" class="Aves" family="Ptilonorhynchidae" genus="Alphachlamydera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="91" pageNumber="91" phylum="Chordata" rank="species" species="cerviniventris">Alphachlamydera cerviniventris</taxonomicName>
