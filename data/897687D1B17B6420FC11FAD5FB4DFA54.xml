in CD, 
<quantity id="C6279B22B17B6421FA89F8C8FA6CF8EA" box="[1348,1445,1885,1908]" metricMagnitude="-2" metricUnit="m" metricValue="3.6799999999999997" pageId="0" pageNumber="7" unit="mm" value="36.8">36.8 mm</quantity>
