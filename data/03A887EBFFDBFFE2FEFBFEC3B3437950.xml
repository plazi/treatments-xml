<number id="3DC924DAFFDBFFE2FAEDFDF7B3457E71" box="[1310,1325,559,585]" pageId="11" pageNumber="12" value="3.0">3</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep id="30F52E58FFDBFFE2FEFBFD8FB3437ED1" pageId="11" pageNumber="12">
<paragraph id="8BBE36FDFFDBFFE2FEFBFD8FB3437EA1" blockId="11.[264,1325,359,1384]" pageId="11" pageNumber="12">
<keyLead id="30F095C8FFDBFFE2FEFBFD8FB3437EA1" pageId="11" pageNumber="12">
<number id="3DC924DAFFDBFFE2FEFBFD8FB7717E49" box="[264,281,599,625]" pageId="11" pageNumber="12" value="3.0">3</number>
