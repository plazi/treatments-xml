<taxonomicName id="4C204D1298015A03DE98FEDCD378FE77" authority="(Seabra, 1898)" baseAuthorityName="Seabra" baseAuthorityYear="1898" class="Mammalia" family="Pteropodidae" genus="Myonycteris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="angolensis">
