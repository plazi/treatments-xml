<taxonomicName id="711205B377911EF0E6940CDEA6D8073C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179602" authority="Forel (Atopogyne)" authorityName="Forel (Atopogyne)" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cremastogaster menilekii Forel" lsidName-HNS="Cremastogaster menilekii Forel" order="Hymenoptera" pageNumber="97" phylum="Arthropoda" rank="species" species="menilekii">Cremastogaster Menilekii</taxonomicName>
, 
<taxonomicNameLabel id="8F90D7E9B0647C8C55A3AC853AEC0CFA" rank="species">n. spec.</taxonomicNameLabel>
