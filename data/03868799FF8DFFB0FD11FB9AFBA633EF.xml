<taxonomicName id="4C2F4D0CFF8DFFB0FCE4FA0BFCD930C3" box="[856,1020,1496,1517]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="354" phylum="Tracheophyta" rank="species" species="velutina">
