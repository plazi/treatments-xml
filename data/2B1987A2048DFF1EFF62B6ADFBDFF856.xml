<taxonomicName id="64B04D37048CFF1EFEE9B48BFE65F89D" authorityName="Stål" authorityYear="1869" box="[284,441,1772,1797]" class="Insecta" family="Cicadellidae" genus="Homalodisca" kingdom="Animalia" order="Hemiptera" pageId="227" pageNumber="228" phylum="Arthropoda" rank="genus">
