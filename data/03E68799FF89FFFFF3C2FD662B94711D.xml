<figureCitation id="13742A0AFF89FFFFF02DFBF82DDD7025" box="[888,981,1029,1050]" captionStart="FIGURE 196" captionStartId="246.[151,244,1301,1322]" captionTargetBox="[207,1367,561,1277]" captionTargetId="figure-47@246.[200,1381,548,1288]" captionTargetPageId="246" captionText="FIGURE 196. Rhotana formosana, lectotype ♂." pageId="106" pageNumber="107">
Fig. 
<quantity id="4CB79B6AFF89FFFFF0F1FBF82DDD7025" box="[932,981,1029,1050]" metricMagnitude="-1" metricUnit="kg" metricValue="1.96" pageId="106" pageNumber="107" unit="g" value="196.0">196g</quantity>
</figureCitation>
)............................................................................................... 
<taxonomicName id="4C4F4D0CFF89FFFFF0C7FBDF2A827008" authority="Matsumura" authorityName="Matsumura" authorityYear="1914" box="[914,1162,1058,1079]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="formosana">
<emphasis id="B93BEA9DFF89FFFFF0C7FBDF2A107008" box="[914,1048,1058,1079]" italics="true" pageId="106" pageNumber="107">R. formosana</emphasis>
Matsumura
</taxonomicName>
(
<taxonomicName id="4C4F4D0CFF89FFFFF7CCFBDF2B127008" authorityName="Matsumura" authorityYear="1914" box="[1177,1306,1058,1079]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="formosana">
