<quantity id="4CF79B33FFB2FF9BC321FB64FE0CFB3D" box="[344,410,1213,1233]" metricMagnitude="3" metricUnit="m" metricValue="1.75" pageId="93" pageNumber="248" unit="m" value="1750.0">1750m</quantity>
, 8941/1, 
<date id="FFB11016FFB2FF9BC384FB64FDF0FB3D" box="[509,614,1213,1233]" pageId="93" pageNumber="248" value="2005-07-13">13.07.2005</date>
