<quantity id="4CF79B33FFB2FF9BC07AFAD4FDD2FACD" box="[515,580,1293,1313]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="93" pageNumber="248" unit="m" value="2300.0">2300m</quantity>
