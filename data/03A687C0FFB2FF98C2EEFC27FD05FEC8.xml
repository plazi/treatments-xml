<quantity id="4CF79B33FFB2FF9BC3C8FB5EFE65FB77" box="[433,499,1159,1179]" metricMagnitude="3" metricUnit="m" metricValue="1.95" pageId="93" pageNumber="248" unit="m" value="1950.0">1950m</quantity>
