<subSubSection id="C32FF4D3FFECFFE0DCA8F910FD877E02" box="[154,541,1031,1054]" pageId="109" pageNumber="110" type="nomenclature">
<paragraph id="8B8AA758FFECFFE0DCA8F910FD877E02" blockId="109.[154,541,1031,1054]" box="[154,541,1031,1054]" pageId="109" pageNumber="110">
<heading id="D0C21034FFECFFE0DCA8F910FD877E02" box="[154,541,1031,1054]" centered="true" fontSize="9" level="2" pageId="109" pageNumber="110" reason="2">
<taxonomicName id="4C35DCDBFFECFFE0DCA8F910FE1C7E02" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[154,390,1031,1054]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="109" pageNumber="110" phylum="Arthropoda" rank="species" species="axeli" status="new species">
<emphasis id="B9417B4AFFECFFE0DCA8F910FE1C7E02" bold="true" box="[154,390,1031,1054]" italics="true" pageId="109" pageNumber="110">Paramblynotus axeli</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFECFFE0DDA4F91EFD877E02" box="[406,541,1032,1054]" pageId="109" pageNumber="110" rank="species">new species</taxonomicNameLabel>
</heading>
</paragraph>
