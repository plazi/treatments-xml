<taxonomicName id="EFD54D74FFAEFFA1FDBFDB3BCC54F8F1" authority="(See TuzovSkij 2017)" authorityName="See TuzovSkij" authorityYear="2017" baseAuthorityName="See TuzovSkij" baseAuthorityYear="2017" box="[636,1086,1890,1915]" class="Arachnida" family="Hygrobatidae" genus="Hygrobates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="8" pageNumber="257" phylum="Arthropoda" rank="species" species="nigromaculatus">
