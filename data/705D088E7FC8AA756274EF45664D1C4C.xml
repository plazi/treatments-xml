<taxonomicName id="32BD157256437F43117113C469AC075E" LSID-ZBK="urn:lsid:zoobank.org:act:D6E2004D-AAC6-4A97-9D32-DC5186F09138" class="Actinopterygii" family="Balitoridae" genus="Gastromyzon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Gastromyzon Günther 1874:454" order="Cypriniformes" pageNumber="1" phylum="Chordata" rank="genus">Gastromyzon</taxonomicName>
