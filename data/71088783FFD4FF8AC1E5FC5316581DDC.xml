<number id="4F6924B2FFD5FF8AC254FE9916A31B14" box="[806,924,352,378]" pageId="21" pageNumber="22" value="7.265" valueMax="7.64" valueMin="6.89">6.89–7.64</number>
(mean = 
<number id="4F6924B2FFD5FF8AC57DFE9911781B14" box="[1039,1095,352,378]" pageId="21" pageNumber="22" value="7.2">7.20</number>
