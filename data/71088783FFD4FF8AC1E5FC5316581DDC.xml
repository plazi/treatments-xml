<typeStatus id="261A8837FFD5FF8AC098FE9917631B14" box="[490,604,352,378]" pageId="21" pageNumber="22">Paratypes</typeStatus>
(n = 
<number id="4F6924B2FFD5FF8AC3ECFE9917811B14" box="[670,702,352,378]" pageId="21" pageNumber="22" value="10.0">10</number>
