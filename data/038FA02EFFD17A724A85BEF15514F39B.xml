<taxonomicName id="4C266ABBFFD17A714848BFD05713F4AE" box="[602,867,1991,2016]" class="Heterotardigrada" family="Macrobiotidae" genus="Minibiotus" kingdom="Animalia" order="Parachela" pageId="77" pageNumber="78" phylum="Tardigrada" rank="species" species="constellatus">
