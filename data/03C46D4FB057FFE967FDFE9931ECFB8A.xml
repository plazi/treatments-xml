<taxonomicName id="4C6DA7DAB057FFE965E9FC41308EFC12" authorityName="Herman" authorityYear="1983" box="[912,993,928,953]" class="Insecta" family="Staphylinidae" genus="Pseudopsis" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="948" phylum="Arthropoda" rank="species" species="suchi">
