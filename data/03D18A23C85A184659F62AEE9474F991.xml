</emphasis>
<taxonomicName id="4C7840B6C85A184658672AAE97FCF8B1" authority="Patterson &amp; Kraglievich 1960" authorityName="Patterson &amp; Kraglievich" authorityYear="1960" class="Aves" family="Phorusrhacidae" genus="Andalgalornis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gruiformes" pageId="24" pageNumber="79" phylum="Chordata" rank="species" species="ferox">
