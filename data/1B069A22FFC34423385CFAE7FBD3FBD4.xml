<taxonomicName id="54AF50B7FFC344233837FA24FD76FA6B" authority="Boeck, 1871" authorityName="Boeck" authorityYear="1871" box="[328,667,1499,1523]" class="Malacostraca" family="Podoceridae" genus="Dulichia" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="40" phylum="Arthropoda" rank="species" species="nordlandica">
