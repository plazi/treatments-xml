<taxonomicName id="40BA8151DC17C3618C7DDBAE0CAEDE7B" class="Insecta" family="Chalcididae" genus="Brachymeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachymeria" order="Hymenoptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="genus">Brachymeria</taxonomicName>
