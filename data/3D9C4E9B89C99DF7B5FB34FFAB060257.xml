<taxonomicName id="F2F6D238371C1BE1ADDE887E5A516E4B" class="Insecta" family="Chalcididae" genus="Chalcis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcis" order="Hymenoptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="genus">Chalcis</taxonomicName>
