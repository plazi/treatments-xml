<taxonomicName id="A7F75C664D54C60CCBB94186E2925AB2" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Miconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Miconia incurva" order="Myrtales" pageId="189" pageNumber="190" phylum="Tracheophyta" rank="species" species="incurva" status="comb. nov.">Miconia incurva</taxonomicName>
