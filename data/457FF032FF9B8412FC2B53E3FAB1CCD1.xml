<collectingCountry id="B5C101B4FF9B8412FC66515AFB76CC73" box="[988,1162,1842,1867]" name="New Caledonia" pageId="19" pageNumber="148">NEW CALEDONIA</collectingCountry>
. — 
<taxonomicName id="0AD63AA7FF9B8412FB01515AFAA9CC72" box="[1211,1365,1842,1865]" class="Liliopsida" family="Bromeliaceae" genus="Ananas" kingdom="Plantae" order="Poales" pageId="19" pageNumber="148" phylum="Tracheophyta" rank="species" species="comosus">
<emphasis id="FFA29D36FF9B8412FB01515AFAA9CC72" box="[1211,1365,1842,1865]" italics="true" pageId="19" pageNumber="148">Ananas comosus</emphasis>
</taxonomicName>
, 
<taxonomicName id="0AD63AA7FF9B8412FADE515AFA4CCC72" box="[1380,1456,1842,1865]" class="Magnoliopsida" family="Annonaceae" genus="Annona" kingdom="Plantae" order="Magnoliales" pageId="19" pageNumber="148" phylum="Tracheophyta" rank="genus">
