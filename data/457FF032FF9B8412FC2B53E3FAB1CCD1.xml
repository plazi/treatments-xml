</taxonomicName>
spp. and 
<taxonomicName id="0AD63AA7FF9B8412FC3F5124FBE9CC5F" box="[901,1045,1868,1892]" class="Magnoliopsida" family="Cucurbitaceae" genus="Cucurbita" kingdom="Plantae" order="Cucurbitales" pageId="19" pageNumber="148" phylum="Tracheophyta" rank="species" species="pepo">
