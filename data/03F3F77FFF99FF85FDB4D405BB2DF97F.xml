<taxonomicName id="4C5A3DEAFF99FF85FDB4D6F2BDF3F935" authorityName="Gray" authorityYear="1843" box="[565,696,1663,1688]" class="Mammalia" family="Pteropodidae" genus="Cynopterus" kingdom="Animalia" order="Chiroptera" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="horsfieldii">
<emphasis id="B92E9A7BFF99FF85FDB4D6F2BD0DF935" box="[565,582,1664,1687]" italics="true" pageId="7" pageNumber="8">C</emphasis>
. 
<emphasis id="B92E9A7BFF99FF85FDD3D60DBDF3F935" box="[594,696,1663,1687]" italics="true" pageId="7" pageNumber="8">horsfieldii</emphasis>
</taxonomicName>
