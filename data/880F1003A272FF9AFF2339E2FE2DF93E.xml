<taxonomicName id="C7A6DA96A272FF9AFB503B70FABAF9F9" box="[1252,1406,1593,1616]" class="Insecta" family="Reduviidae" genus="Triatoma" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="patagonica">
