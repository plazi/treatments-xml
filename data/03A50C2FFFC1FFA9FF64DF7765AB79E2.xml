species, 
<number id="3DC4AF1EFFC1FFA9FC4EDF5562A27976" box="[957,971,294,319]" pageId="2" pageNumber="354" value="1.0">1</number>
endemic genus, 
<number id="3DC4AF1EFFC1FFA9FB6BDF5565DD7976" box="[1176,1204,294,319]" pageId="2" pageNumber="354" value="28.0">28</number>
endemic species, 
<number id="3DC4AF1EFFC1FFA9FA7DDF5564F57976" box="[1422,1436,294,319]" pageId="2" pageNumber="354" value="1.0">1</number>
