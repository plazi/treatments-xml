<taxonomicName id="BA94A089A33565555F23F8F56CF25E95" class="Magnoliopsida" family="Fabaceae" genus="Senegalia" kingdom="Plantae" order="Fabales" pageId="4" pageNumber="499" phylum="Tracheophyta" rank="species" species="mellifera">
