<taxonomicName id="BA94A089A335655559F1F8C968685E5D" box="[1020,1126,1868,1894]" class="Pucciniomycetes" genus="Aecidium" kingdom="Chromista" order="Pucciniales" pageId="4" pageNumber="499" phylum="Basidiomycota" rank="genus">
