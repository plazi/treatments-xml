<taxonomicName id="EC34613D8B1C60617EEF79CA16768D92" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="17" pageNumber="18" phylum="Angiospermae" rank="species" species="shawii">
