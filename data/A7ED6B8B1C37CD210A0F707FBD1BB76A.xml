<taxonomicName id="EBBD28349F44D9322CF58E8B5DC0F2B4" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="gerrardii">
<emphasis id="C3532F95366F65343F0E405DC16A24E7" italics="true" pageId="17" pageNumber="18">Acacia gerrardii</emphasis>
