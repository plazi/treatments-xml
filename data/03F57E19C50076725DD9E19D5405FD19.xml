<taxonomicName id="4C5CB48CC50076725F11E0ED56A4FE51" authority="(WARNCKE 1992)" baseAuthorityName="WARNCKE" baseAuthorityYear="1992" box="[619,926,440,462]" class="Insecta" family="Megachilidae" genus="Coelioxys" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="1763" phylum="Arthropoda" rank="species" species="argentea">
