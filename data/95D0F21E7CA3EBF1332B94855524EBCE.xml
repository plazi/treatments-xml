<taxonomicName id="990EE91B4831A80909C064708C1F276A" class="Insecta" family="Curculionidae" genus="Dactylipalpus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dactylipalpus" order="Coleoptera" pageId="272" pageNumber="273" phylum="Arthropoda" rank="genus">Dactylipalpus</taxonomicName>
