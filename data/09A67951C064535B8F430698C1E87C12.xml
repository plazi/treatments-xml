are 
<taxonomicName id="8C2F78BC3305B3A43F2FE4DCE1CE7B1B" class="Magnoliopsida" family="Vitaceae" genus="Cissus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cissus pseudosicyoidea" order="Vitales" pageId="0" pageNumber="45" phylum="Tracheophyta" rank="species" species="pseudosicyoidea">
