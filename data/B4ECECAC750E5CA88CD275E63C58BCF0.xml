<emphasis id="ACA4ADBA6CFE46F61FA9035DF11566EF" bold="true" pageId="0" pageNumber="31">Figure 13.</emphasis>
<taxonomicName id="7273F169FBAAF4ED55C8EF336F598CFC" class="Diplopoda" family="Fuhrmannodesmidae" genus="Hemisphaeroparia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemisphaeroparia avis" order="Polydesmida" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="avis">
