, 
<geoCoordinate id="7A686E31642598699EE672D9EADDBEE6" direction="west" orientation="longitude" precision="925" value="-82.23333">82°14'W</geoCoordinate>
, 1150m, 9-12 VI 1995, J. Ashe &amp; R. Brooks#186, ex: flight intercept trap&quot;/ 
<normalizedToken id="6AA9B23A2F1B283F4BE739DDA67BE6B2" originalValue="“SEMC0903667”">&quot;SEMC0903667&quot;</normalizedToken>
