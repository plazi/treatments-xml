<collectionCode id="A08384FFFFC6FFE49371F88BFE03F8CB" box="[320,385,1835,1858]" httpUri="http://grbio.org/cool/9bph-kj5p" name="Institute of Zoology and Botany" pageId="4" pageNumber="30">IZBE</collectionCode>
