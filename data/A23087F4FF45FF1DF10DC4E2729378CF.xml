<taxonomicName id="ED994D61FF42FF1DF38CC77673847F69" authority="(Balss, 1916)" baseAuthorityName="Balss" baseAuthorityYear="1916" class="Malacostraca" family="Albuneidae" genus="Paralbunea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="180" pageNumber="181" phylum="Arthropoda" rank="species" species="intermedia">
