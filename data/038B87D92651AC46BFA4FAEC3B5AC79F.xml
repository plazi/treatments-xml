<quantity id="4CDA9B2A2651AC46BE6DF96C3B8FC743" box="[350,489,1788,1812]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" metricValueMax="6.4" metricValueMin="5.8" pageId="20" pageNumber="381" unit="mm" value="6.1" valueMax="6.4" valueMin="5.8">5.8–6.4 mm</quantity>
