<paragraph id="DA3569C52768348520617DEC3BEA4C88" pageId="0" pageNumber="6577">
<taxonomicName id="9CD37047FC119CA38C63CD2338B11026" authority="Belogurov &amp; Belogurova, 1979" authorityName="Belogurov &amp; Belogurova" authorityYear="1979" class="Enoplea" family="Oncholaimidae" genus="Admirandus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Admirandus" order="Oncholaimida" pageId="0" pageNumber="6577" phylum="Nematoda" rank="genus">Admirandus</taxonomicName>
