<paragraph id="11D123D8402BC5E92F884833F4F95009" pageId="0" pageNumber="6577">
<taxonomicName id="7C6A8A839A2D202F669A1A9EBF6AA593" authority="Belogurov &amp; Belogurova, 1979" authorityName="Belogurov &amp; Belogurova" authorityYear="1979" class="Enoplea" family="Oncholaimidae" genus="Admirandus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Admirandus" order="Oncholaimida" pageId="0" pageNumber="6577" phylum="Nematoda" rank="genus">Admirandus</taxonomicName>
