<taxonomicName id="4C054D41BF5DD16E8427FBA52EC84C3A" authorityName="Zimin" authorityYear="1951" box="[295,436,1085,1110]" class="Insecta" family="Muscidae" genus="Dasyphora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="meridionalis">
