<taxonomicName id="4C6CA1CF664AB432C4154DFE3D8327F6" box="[1321,1433,1678,1703]" class="Magnoliopsida" family="Cistaceae" kingdom="Plantae" order="Malvales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="family">Cistaceae</taxonomicName>
