<taxonomicName id="4C6F1183FFEFFFAEFE26F982FD1CF99B" baseAuthorityName="Alpheraky" baseAuthorityYear="1908" box="[511,656,1581,1607]" class="Insecta" family="Ethmiidae" genus="Ethmia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="23" phylum="Arthropoda" rank="species" species="candidella">
