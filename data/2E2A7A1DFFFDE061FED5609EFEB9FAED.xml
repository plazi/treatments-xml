<taxonomicName id="6183B088FFFEE061FD6765CAFCEFFAC6" box="[698,806,1256,1281]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="audaciae">
