<taxonomicName id="7BB21BD6FF8B7462FC81F8AF6E57FD39" authority="Gravenhorst, 1802" authorityName="Gravenhorst" authorityYear="1802" box="[860,1403,1848,1873]" class="Agaricomycetes" family="Staphylinidae" genus="Oxyporus" higherTaxonomySource="CoL" kingdom="Fungi" order="Coleoptera" pageId="16" pageNumber="261" phylum="Basidiomycota" rank="species" species="vittatus" subGenus="Oxyporus">
