<taxonomicName id="4C444D7B3B36446C35E8FD5EFB3FC532" box="[1170,1266,752,779]" class="Insecta" family="Bethylidae" genus="Odontepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="slide">
