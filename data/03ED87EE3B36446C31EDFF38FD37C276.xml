<taxonomicName id="4C444D7B3B36446C3374FC96FDBFC56A" box="[526,626,824,851]" class="Insecta" family="Bethylidae" genus="Odontepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="82" phylum="Arthropoda" rank="species" species="slides">on slides</taxonomicName>
