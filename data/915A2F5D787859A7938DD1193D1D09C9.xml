<taxonomicName id="4D6D057D2442C837480DA907ACFE5DA4" authorityName="Kilburn" authorityYear="1978" class="Gastropoda" family="Fissurellidae" genus="Emarginula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Emarginula thorektes" order="Lepetellida" pageId="0" pageNumber="49" phylum="Mollusca" rank="species" species="thorektes">
