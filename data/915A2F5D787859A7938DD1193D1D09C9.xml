<taxonomicName id="3493A4F1AFB98C06D6951B3C1128D637" authorityName="Kilburn" authorityYear="1978" class="Gastropoda" family="Fissurellidae" genus="Emarginula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Emarginula connelli" order="Lepetellida" pageId="0" pageNumber="49" phylum="Mollusca" rank="species" species="connelli">
