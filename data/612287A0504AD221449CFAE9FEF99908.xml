<collectingRegion id="2B4FF854504AD22646F4F92FFC8D9DA6" box="[767,865,1693,1718]" country="Costa Rica" name="Alajuela" pageId="19" pageNumber="512">Alajuela</collectingRegion>
, 
<collectingRegion id="2B4FF854504AD2264767F92CFC1C9DA6" box="[876,1008,1694,1718]" country="Costa Rica" name="Guanacaste" pageId="19" pageNumber="512">Guanacaste</collectingRegion>
, 
<collectingRegion id="2B4FF854504AD22647F7F92FFBA59DA6" box="[1020,1097,1693,1718]" country="Costa Rica" name="Limon" pageId="19" pageNumber="512">Limón</collectingRegion>
, and 
<collectingRegion id="2B4FF854504AD226408DF92CFAED9DA5" box="[1158,1281,1694,1717]" country="Costa Rica" name="Puntarenas" pageId="19" pageNumber="512">Puntarenas</collectingRegion>
provinces. In 
<collectingCountry id="919C7626504AD226449CF970FEFF9DCA" box="[151,275,1729,1754]" name="Costa Rica" pageId="19" pageNumber="512">Costa Rica</collectingCountry>
