<collectingRegion id="2B4FF854504AD22647FDFA77FBA79ECE" box="[1014,1099,1477,1502]" country="Mexico" name="Mexico" pageId="19" pageNumber="512">
<collectingCountry id="919C7626504AD22647FDFA77FBA79ECE" box="[1014,1099,1477,1502]" name="Mexico" pageId="19" pageNumber="512">Mexico</collectingCountry>
</collectingRegion>
