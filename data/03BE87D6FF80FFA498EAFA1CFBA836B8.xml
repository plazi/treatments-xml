<quantity id="4CEF9B25FF80FFA49B3AFA1DF9C9360C" box="[839,904,1519,1545]" metricMagnitude="0" metricUnit="m" metricValue="4.2" pageId="17" pageNumber="18" unit="m" value="4.2">4.2 m</quantity>
