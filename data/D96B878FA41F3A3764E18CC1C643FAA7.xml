<collectorName id="FC37534FA41A3A3765BB8E48C084FBCF" box="[461,955,1093,1118]" pageId="4" pageNumber="477">All Royal Entomological Society London</collectorName>
, 
<collectorName id="FC37534FA41A3A3767BB8E48C7B9FBCF" box="[973,1158,1093,1118]" pageId="4" pageNumber="477">Project Wallace</collectorName>
, (
<collectionCode id="37D3AE5CA41A3A3760E88E4BC7CBFBCF" box="[1182,1268,1094,1118]" pageId="4" pageNumber="477">BMNH</collectionCode>
