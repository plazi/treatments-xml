<quantity id="4C8A9B700E42FF8AFCE064B3EA687283" box="[796,889,1707,1734]" metricMagnitude="-1" metricUnit="m" metricValue="2.05" pageId="22" pageNumber="323" unit="mm" value="205.0">205 mm</quantity>
