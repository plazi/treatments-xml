<paragraph id="B6D66947D50D62BF28A6AC45AAB3C76F" pageId="0" pageNumber="69">
<taxonomicName id="9D71827A2012A96564E3ACE74FAA1695" class="Dicotyledoneae" family="Verbenaceae" genus="C." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="C. emorsus" order="Tubiflorae" pageId="0" pageNumber="69" phylum="Angiospermae" rank="species" species="emorsus">
