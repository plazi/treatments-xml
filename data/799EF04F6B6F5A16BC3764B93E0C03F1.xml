<taxonomicName id="A266170A70EB3E7EAFCB0BBB4F24A2AC" class="Dicotyledoneae" family="Verbenaceae" genus="C." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="C. terminatus" order="Tubiflorae" pageId="0" pageNumber="69" phylum="Angiospermae" rank="species" species="terminatus">
