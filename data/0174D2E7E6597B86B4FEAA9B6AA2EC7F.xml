In 
<normalizedToken id="03A2ACD315942294C403D16A1C3A72AD" originalValue="Feldhölzern">Feldhoelzern</normalizedToken>
. 
<materialsCitation id="35647B12C3BBB234561DB5C05A9E2437" ID-GBIF-Occurrence="923930447" location="Regensburg">
Bei 
<location id="778AF4F82C3FA6BDF21DFED38846AA77" LSID="urn:lsid:plazi:treatment:0174D2E7E6597B86B4FEAA9B6AA2EC7F:778AF4F82C3FA6BDF21DFED38846AA77" country="Germany" county="Upper Palatinate" latitude="49.015" longitude="12.09556" name="Regensburg" stateProvince="Bavaria">Regensburg</location>
