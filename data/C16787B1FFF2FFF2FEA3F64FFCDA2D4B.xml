<taxonomicName id="8ECE4D24FFF2FFF2FDC2F6BFFC0E2D6B" authority="(Trjapitzin, 1978)" baseAuthorityName="Trjapitzin" baseAuthorityYear="1978" box="[540,971,492,515]" class="Insecta" family="Encyrtidae" genus="Syrphophagus" kingdom="Animalia" order="Hymenoptera" pageId="48" pageNumber="393" phylum="Arthropoda" rank="species" species="vicinus">
