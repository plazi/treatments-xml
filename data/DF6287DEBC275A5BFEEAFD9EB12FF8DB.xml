<caption id="03B46640BC205A59FEEAF927B071F8DE" ID-DOI="http://doi.org/10.5281/zenodo.5051224" ID-Zenodo-Dep="5051224" httpUri="https://zenodo.org/record/5051224/files/figure.png" pageId="5" pageNumber="52" startId="5.[264,368,1765,1789]" targetBox="[396,1150,284,1743]" targetPageId="5">
