<number id="3D94CF038328FF89B951FC185DC82EC9" box="[891,923,959,985]" pageId="25" pageNumber="26" value="14.0">14</number>
(
<number id="3D94CF038328FF89B98EFC185DE02EC9" box="[932,947,959,985]" pageId="25" pageNumber="26" value="3.0">3</number>
), 
<number id="3D94CF038328FF89B9E1FC185DBB2EC9" box="[971,1000,959,985]" pageId="25" pageNumber="26" value="15.0">15</number>
<number id="3D94CF038328FF89B9D3FC185A442EC9" box="[1017,1047,959,985]" pageId="25" pageNumber="26" value="14.0">14</number>
(
<number id="3D94CF038328FF89BE0AFC185A632EC9" box="[1056,1072,959,985]" pageId="25" pageNumber="26" value="2.0">2</number>
) or 
<number id="3D94CF038328FF89BE4EFC185AD22EC9" box="[1124,1153,959,985]" pageId="25" pageNumber="26" value="15.0">15</number>
<number id="3D94CF038328FF89BEBBFC185AFC2EC9" box="[1169,1199,959,985]" pageId="25" pageNumber="26" value="15.0">15</number>
(
<number id="3D94CF038328FF89BE93FC185A992EC9" box="[1209,1226,959,985]" pageId="25" pageNumber="26" value="4.0">4</number>
); pelvic fin I, 
<number id="3D94CF038328FF89BB6CFC405F052911" box="[326,342,999,1025]" pageId="25" pageNumber="26" value="5.0">5</number>
; principal caudal rays 
<number id="3D94CF038328FF89B85BFC405CD32911" box="[625,640,999,1025]" pageId="25" pageNumber="26" value="9.0">9</number>
