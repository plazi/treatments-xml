spp. 1-2 
<taxonomicName id="ABAB4D55E9C2B3B5147F6713B9B5BF20" genus="B." lsidName="B. (Metabolbochromus) catenatus" pageId="0" pageNumber="135" rank="species" species="catenatus" subGenus="Metabolbochromus">B. (Metabolbochromus) catenatus</taxonomicName>
