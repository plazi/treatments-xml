<taxonomicName id="4C2E71DCFFB23D12FCD7CD9EFC56FCB0" authorityName="de Christol" authorityYear="1832" box="[861,970,755,780]" class="Mammalia" family="Equidae" genus="Hipparion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="10" pageNumber="461" phylum="Chordata" rank="genus">
