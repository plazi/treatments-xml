<quantity id="4CE66E991357532AFC8EFE65FC5EFE3A" box="[895,984,415,441]" metricMagnitude="3" metricUnit="m" metricValue="2.1" pageId="31" pageNumber="32" unit="m" value="2100.0">2100 m</quantity>
