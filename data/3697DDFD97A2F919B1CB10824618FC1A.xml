<taxonomicName id="83168C10D9B942C2B9C2AA9CACBA0849" class="Insecta" family="Tenthredinidae" genus="Pachynematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pachynematus fallax" order="Hymenoptera" pageId="0" pageNumber="1168" phylum="Arthropoda" rank="species" species="fallax">Pachynematus fallax</taxonomicName>
