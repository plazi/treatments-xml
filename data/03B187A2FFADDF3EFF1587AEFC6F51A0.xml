<taxonomicName id="4C184D37FFA3DF3FFEF0841AFCAB574A" authority="(Osella, 1983)" authorityName="Osella" authorityYear="1983" baseAuthorityName="Osella" baseAuthorityYear="1983" box="[370,787,1866,1892]" class="Insecta" family="Curculionidae" genus="Baldorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="vailatii">
