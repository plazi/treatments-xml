<taxonomicName id="C3970006A22FEFF6C943FE3FAA4DCB6C" class="Insecta" family="Erotylidae" genus="Microsternus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microsternus" order="Coleoptera" pageId="4" pageNumber="83" phylum="Arthropoda" rank="genus">Microsternus</taxonomicName>
