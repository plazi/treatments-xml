<taxonomicName id="EE081956E553B6D51363E45782716250" class="Insecta" family="Curculionidae" genus="Rhynchaenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhynchaenus" order="Coleoptera" pageId="251" pageNumber="252" phylum="Arthropoda" rank="genus">Rhynchaenus</taxonomicName>
