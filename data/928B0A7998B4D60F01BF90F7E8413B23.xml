<taxonomicName id="6BE712B3D38644ACF43394A61E42A81C" class="Insecta" family="Brachyceridae" genus="Notaris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Notaris" order="Coleoptera" pageId="252" pageNumber="253" phylum="Arthropoda" rank="genus">Notaris</taxonomicName>
