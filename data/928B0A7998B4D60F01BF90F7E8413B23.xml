<taxonomicName id="6C4E85998ECC5FB73B7EFEB28A0DDEAD" class="Insecta" family="Brachyceridae" genus="Tournotaris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tournotaris" order="Coleoptera" pageId="252" pageNumber="253" phylum="Arthropoda" rank="genus">Tournotaris</taxonomicName>
