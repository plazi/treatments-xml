<taxonomicName id="4C1F41AB8B47FFF294C8A332FEC2FD55" box="[151,353,620,644]" class="Insecta" family="Staphylinidae" genus="Pselaphomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
