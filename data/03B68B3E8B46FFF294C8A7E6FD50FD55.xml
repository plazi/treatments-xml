<emphasis id="B96BE63A8B47FFF2961AA333FD50FD55" box="[581,755,620,644]" italics="true" pageId="33" pageNumber="34">
<taxonomicName id="4C1F41AB8B47FFF2961AA333FD4DFD55" box="[581,750,620,644]" class="Insecta" family="Staphylinidae" genus="Pselaphomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="sculpturatus">P. sculpturatus</taxonomicName>
