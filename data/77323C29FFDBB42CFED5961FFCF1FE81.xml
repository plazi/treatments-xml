<taxonomicName id="389BF6BCFFDBB42DFED5961FFBB7F76C" authority="Ji, Currie, Norell and Ji, 1998" authorityName="Ji, Currie, Norell and Ji" authorityYear="1998" box="[288,1095,2159,2195]" class="Reptilia" family="Caudipterygidae" genus="Caudipteryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="28" pageNumber="28" phylum="Chordata" rank="species" species="zoui">
