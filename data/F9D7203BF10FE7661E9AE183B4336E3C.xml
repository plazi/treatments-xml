<taxonomicName id="9C05FEE256ECF6E654798E90A4ED7B9A" authority="L." class="Magnoliopsida" family="Adoxaceae" genus="Adoxa" kingdom="Plantae" order="Dipsacales" pageId="null" pageNumber="322" phylum="Tracheophyta" rank="species" species="moschatellina">
