<caption id="DF0E6677A928FFBDFF643452672E0B11" httpUri="https://zenodo.org/record/223221/files/figure.png" pageId="11" pageNumber="460" targetBox="[248,1346,231,1529]" targetPageId="11">
<paragraph id="8BCE36FFA928FFBDFF643452672E0B11" blockId="11.[151,1436,1588,1641]" pageId="11" pageNumber="460">
<emphasis id="B905EAEDA928FFBDFF643452676E0B31" bold="true" box="[153,278,1588,1610]" pageId="11" pageNumber="460">FIGURE 8.</emphasis>
<taxonomicName id="4C714D7CA928FFBDFEE3345264DF0B32" authority="Borkent, 1984" authorityName="Borkent" authorityYear="1984" box="[286,679,1588,1610]" class="Insecta" family="Chironomidae" genus="Stenochironomus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="460" phylum="Arthropoda" rank="species" species="reissi">
