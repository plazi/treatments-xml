<taxonomicName id="10ED4D39FFA3FFA1FF6B7D87FD5C78C9" authority="Whitfield, 1886" authorityName="Whitfield" authorityYear="1886" box="[189,642,659,685]" class="Liliopsida" family="Orchidaceae" genus="Orthoceras" kingdom="Plantae" order="Asparagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="brainerdi">
