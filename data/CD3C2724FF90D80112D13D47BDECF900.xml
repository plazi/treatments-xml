<taxonomicName id="8295EDB1FF90D80112D13DDCBD49FA0B" authority="Malicky 1993: 1114" authorityName="Malicky" authorityPageNumber="1114" authorityYear="1993" box="[151,583,1475,1497]" class="Insecta" family="Polycentropodidae" genus="Polyplectropus" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="jonam">
