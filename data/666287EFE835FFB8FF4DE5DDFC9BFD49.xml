<emphasis id="DCBFEAEBE835FFBAFE78E4DEFDBBFE61" box="[418,544,403,429]" italics="true" pageId="49" pageNumber="50">T. scudderi</emphasis>
Bolivar, 1887
</taxonomicName>
; 
<taxonomicName id="29CB4D7AE835FFBAFD0DE4DEFBB9FE61" authority="Bruner, 1910" authorityName="Bruner" authorityYear="1910" box="[727,1058,403,430]" class="Insecta" family="Tetrigidae" genus="Tettigidea" kingdom="Animalia" order="Orthoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="chapadensis">
<emphasis id="DCBFEAEBE835FFBAFD0DE4DEFC1BFE61" box="[727,896,403,429]" italics="true" pageId="49" pageNumber="50">T. chapadensis</emphasis>
