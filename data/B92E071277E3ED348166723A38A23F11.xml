<paragraph id="B487653DA0588A2957AFFFDF97AEC58A" pageId="171" pageNumber="172">
Holotype 
<taxonomicName id="842C7F788FAAF46BADD980B6F89AADEA" class="Insecta" family="Scelionidae" genus="Scelio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scelio striatus" order="Hymenoptera" pageId="171" pageNumber="172" phylum="Arthropoda" rank="species" species="striatus">Scelio striatus</taxonomicName>
