<quantity id="4CA59B109D6EFFD9FEE0FD1C309AFCD8" box="[330,417,754,778]" metricMagnitude="3" metricUnit="m" metricValue="1.128" pageId="18" pageNumber="669" unit="m" value="1128.0">1128 m</quantity>
