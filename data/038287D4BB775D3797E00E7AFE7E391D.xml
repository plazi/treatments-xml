<emphasis id="B95FEAD0BB765D3797B00A02FEFA3AA8" bold="true" box="[199,295,366,392]" pageId="103" pageNumber="104">Biology.</emphasis>
<taxonomicName id="4C2B4D41BB765D3796590A03FDEE3AA9" authorityName="Det. H. S. Lopes" baseAuthorityName="Wulp" baseAuthorityYear="1895" box="[302,563,367,393]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="trivialis">
