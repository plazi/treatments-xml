<taxonomicName id="4C2B4D41BB765D3796B10AFFFD093A8D" authority="Mill." authorityName="Mill." box="[454,724,403,429]" class="Magnoliopsida" family="Lauraceae" genus="Persea" kingdom="Plantae" order="Laurales" pageId="103" pageNumber="104" phylum="Tracheophyta" rank="species" species="americana">
<emphasis id="B95FEAD0BB765D3796B10AFFFD4E3A8C" box="[454,659,403,428]" italics="true" pageId="103" pageNumber="104">Persea americana</emphasis>
Mill.
</taxonomicName>
, 
<taxonomicName id="4C2B4D41BB765D3795A80AFFFC893A8D" box="[735,852,403,429]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="103" pageNumber="104" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
) (
<bibRefCitation id="EFBA4B33BB765D37941D0AFFFB0A3A8D" author="Castaneda-Vildozola, A. &amp; Equihua-Martinez, A. &amp; Valdes-Carrasco, J. &amp; Barrientos-Priego, A. F. &amp; Ish-Am, G. &amp; Gazit, S." box="[874,1239,403,429]" pageId="103" pageNumber="104" pagination="129 - 136" refId="ref105278" refString="Castaneda-Vildozola, A., Equihua-Martinez, A., Valdes-Carrasco, J., Barrientos-Priego, A. F., Ish-Am, G. &amp; Gazit, S. (1999) Insectos polinizadores del aguacareto em los estados de Mexico y Michoacan. Revista Chapingo Serie Horticultura, 5, 129 - 136." type="journal article" year="1999">
