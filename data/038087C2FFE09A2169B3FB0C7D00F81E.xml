<taxonomicName id="4C294D57FFE09A216B86F8157B09F843" authorityName="Anderson" authorityYear="2018" box="[674,876,1956,1980]" class="Insecta" family="Curculionidae" genus="Sicoderus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="species" species="striatolateralis">
