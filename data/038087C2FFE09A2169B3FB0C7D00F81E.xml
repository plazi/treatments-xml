<taxonomicName id="4C294D57FFE09A216A96F8157CA9F843" authorityName="Anderson" authorityYear="2018" box="[946,1228,1956,1980]" class="Insecta" family="Curculionidae" genus="Sicoderus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="species" species="pseudostriatolateralis">
