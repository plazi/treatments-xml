<location id="A630606D9E2BFFE3FC8B7556FC3E172F" LSID="urn:lsid:plazi:treatment:2B4687A09E2BFFE3FF0970FFFBAB106A:A630606D9E2BFFE3FC8B7556FC3E172F" box="[824,968,332,354]" country="Guatemala" county="Griscom" municipality="Dwight Collection" name="Guatemalan" pageId="47" pageNumber="47">Guatemalan</location>
