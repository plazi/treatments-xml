<collectingCountry id="1D880BD7A034FF8AFC5BFA6CE62DFA43" box="[983,1112,1499,1524]" name="Philippines" pageId="8" pageNumber="485">Philippines</collectingCountry>
, 
<collectingCountry id="1D880BD7A034FF8AFBE9FA6BE735FA43" box="[1125,1344,1499,1524]" name="Papua New Guinea" pageId="8" pageNumber="485">Papua New Guinea</collectingCountry>
