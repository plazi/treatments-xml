<taxonomicName id="A29F30C4A034FF8AFD0DFA21E083FA1A" box="[641,758,1428,1453]" class="Gastropoda" family="Aeolidiidae" genus="Baeolidia" kingdom="Animalia" order="Nudibranchia" pageId="8" pageNumber="485" phylum="Mollusca" rank="species" species="gracilis" status="sp. nov.">
<emphasis id="57EB9755A034FF8AFD0DFA21E083FA1A" box="[641,758,1428,1453]" italics="true" pageId="8" pageNumber="485">B. gracilis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4CD82A2EA034FF8AFD72FA23E12DFA1A" box="[766,856,1428,1453]" pageId="8" pageNumber="485" rank="species">
<emphasis id="57EB9755A034FF8AFD72FA23E12DFA1A" bold="true" box="[766,856,1428,1453]" pageId="8" pageNumber="485">sp. nov.</emphasis>
</taxonomicNameLabel>
