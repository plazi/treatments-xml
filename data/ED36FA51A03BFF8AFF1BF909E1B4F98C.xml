<taxonomicName id="A29F30C4A034FF8AFE16F993E06BF98C" authority="" authorityName="" box="[410,542,1570,1595]" class="Gastropoda" family="Aeolidiidae" genus="Aeolis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nudibranchia" pageId="8" pageNumber="485" phylum="Mollusca" rank="species" species="macleayi">
