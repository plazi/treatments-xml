<collectingCountry id="1D880BD7A034FF8AFAB8FA0EE702FA67" box="[1332,1399,1465,1488]" name="Japan" pageId="8" pageNumber="485">Japan</collectingCountry>
, 
<taxonomicName id="A29F30C4A034FF8AFA08FA0EE298FA43" class="Gastropoda" family="Aeolidiidae" genus="Baeolidia" kingdom="Animalia" order="Nudibranchia" pageId="8" pageNumber="485" phylum="Mollusca" rank="species" species="gracilis" status="sp. nov.">
<emphasis id="57EB9755A034FF8AFA08FA0EE298FA43" italics="true" pageId="8" pageNumber="485">B. gracilis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4CD82A2EA034FF8AFF79FA6CE325FA43" box="[245,336,1499,1524]" pageId="8" pageNumber="485" rank="species">
<emphasis id="57EB9755A034FF8AFF79FA6CE325FA43" bold="true" box="[245,336,1499,1524]" pageId="8" pageNumber="485">sp. nov.</emphasis>
</taxonomicNameLabel>
in the 
<collectingCountry id="1D880BD7A034FF8AFE2EFA6CE055FA43" box="[418,544,1499,1524]" name="Philippines" pageId="8" pageNumber="485">Philippines</collectingCountry>
and 
<taxonomicName id="A29F30C4A034FF8AFDD6FA6AE090FA43" box="[602,741,1499,1524]" class="Gastropoda" family="Aeolidiidae" genus="Baeolidia" kingdom="Animalia" order="Nudibranchia" pageId="8" pageNumber="485" phylum="Mollusca" rank="species" species="variabilis" status="sp. nov.">
<emphasis id="57EB9755A034FF8AFDD6FA6AE090FA43" box="[602,741,1499,1524]" italics="true" pageId="8" pageNumber="485">B. variabilis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4CD82A2EA034FF8AFD62FA6CE13CFA43" box="[750,841,1499,1524]" pageId="8" pageNumber="485" rank="species">
<emphasis id="57EB9755A034FF8AFD62FA6CE13CFA43" bold="true" box="[750,841,1499,1524]" pageId="8" pageNumber="485">sp. nov.</emphasis>
</taxonomicNameLabel>
