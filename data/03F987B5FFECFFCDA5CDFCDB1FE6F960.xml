<taxonomicName id="4C504D20FFECFFCDA463F9BA1FD8F9BC" authorityName="Persoon" authorityYear="1806" baseAuthorityName="Linnaeus" box="[294,487,1634,1660]" class="Magnoliopsida" family="Verbenaceae" genus="Priva" kingdom="Plantae" order="Lamiales" pageId="28" pageNumber="29" phylum="Tracheophyta" rank="species" species="lappulacea">
