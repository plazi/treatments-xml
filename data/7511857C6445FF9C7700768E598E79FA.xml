<taxonomicName id="3AB84FE96446FF927799716B58E37E04" authority="Sars" authorityName="Sars" box="[401,651,1353,1376]" class="Malacostraca" family="Phoxocephalidae" genus="Harpinia" higherTaxonomySource="CoL" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="truncata">
<taxonomicName id="3AB84FE96446FF927799716B583B7E04" box="[401,595,1353,1376]" class="Malacostraca" family="Phoxocephalidae" genus="Harpinia" higherTaxonomySource="CoL" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="truncata">
