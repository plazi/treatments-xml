<taxonomicName id="3AB84FE96446FF9277F1716B583B7E04" authority="Sars, 1891" authorityName="Sars" authorityYear="1891" box="[505,595,1353,1376]" pageId="6" pageNumber="7" rank="species" species="truncata">truncata</taxonomicName>
