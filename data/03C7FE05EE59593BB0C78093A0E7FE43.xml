<number id="3DA65D34EE5A593BB3398093A099FF6B" box="[873,908,151,175]" pageId="22" pageNumber="23" value="2.5">2.5</number>
(variation 
<number id="3DA65D34EE5A593BB45B8093A74AFF6B" box="[1035,1119,151,175]" pageId="22" pageNumber="23" value="2.3" valueMax="2.5" valueMin="2.1">
2.1 
<emphasis id="B91A9301EE5A593BB47E809EA72FFF6B" bold="true" box="[1070,1082,154,175]" pageId="22" pageNumber="23">–</emphasis>
2.5
</number>
, n= 
<number id="3DA65D34EE5A593BB4DE8093A788FF6B" box="[1166,1181,151,175]" pageId="22" pageNumber="23" value="2.0">2</number>
