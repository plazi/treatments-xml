<subSubSection id="C3754BA6FF97FF94FF7CFB5985C23D77" pageId="65" pageNumber="66" type="description">
<paragraph id="8BD0182DFF97FF94FF7CFB5985C23D77" blockId="65.[151,1436,1233,1294]" pageId="65" pageNumber="66">
<taxonomicName id="4C6F63AEFF97FF94FF7CFB5987EE3C93" authorityName="Simon" authorityYear="1884" box="[151,449,1233,1258]" class="Arachnida" family="Linyphiidae" genus="Porrhomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="subterraneum">
