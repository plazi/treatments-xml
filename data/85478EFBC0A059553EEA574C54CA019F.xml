<taxonomicName id="3EEDC827429A91D9AF9E4EE6CC29B5CD" class="Arachnida" family="Selenopidae" genus="Makdiops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Makdiops nilgirensis" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="nilgirensis">Makdiops nilgirensis</taxonomicName>
