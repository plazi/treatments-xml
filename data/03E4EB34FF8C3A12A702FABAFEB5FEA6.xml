<quantity id="4CB5F7C7FF8C3A11A46EF90FFCFCF8F6" box="[612,771,1780,1806]" metricMagnitude="-3" metricUnit="m" metricValue="3.185" metricValueMax="3.2" metricValueMin="3.17" pageId="8" pageNumber="43" unit="mm" value="3.185" valueMax="3.2" valueMin="3.17">3.17–3.2 mm</quantity>
