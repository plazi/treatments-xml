<paragraph id="87C1F93B575D13A9C2A00455C16C0C18" pageNumber="670">
<taxonomicName id="5D0582BE63493CAEDDF6B3181DB8D87B" class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageNumber="670" phylum="Arthropoda" rank="species" species="scotti-group">scotti-group</taxonomicName>
