<taxonomicName id="B3922EBBFFE93A13FBD7FCF9FE91FCF6" authorityName="Herbst" authorityYear="1806" class="Insecta" family="Carabidae" genus="Cicindela" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="717" phylum="Arthropoda" rank="species" species="tranquebarica">
