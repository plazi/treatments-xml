<taxonomicName id="E5FD22A2D7C75321BEEBA7C217DB977D" class="Insecta" family="Platygastridae" genus="Helava" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Helava alticola" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="alticola">Helava alticola</taxonomicName>
