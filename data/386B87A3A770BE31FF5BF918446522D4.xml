<taxonomicName id="77C24D36A771BE3EFDC2F9054277258F" authority="Westwood, 1873" authorityName="Westwood" authorityYear="1873" box="[526,811,1722,1746]" class="Insecta" family="Cetoniidae" genus="Platysodes" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="199" phylum="Arthropoda" rank="genus">
