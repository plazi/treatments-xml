and 
<taxonomicName id="9B97C9DEFFBB414AFF26FC836530FCB1" authority="Mannerheim. The" authorityName="Mannerheim. The" box="[189,552,835,862]" class="Insecta" family="Staphylinidae" genus="Gnypeta" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="49" phylum="Arthropoda" rank="species" species="carbonaria">
