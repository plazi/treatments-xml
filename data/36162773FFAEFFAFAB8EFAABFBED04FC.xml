<taxonomicName id="79BFEDE6FFAEFFAFA872FA03FC7206BC" box="[875,1015,1419,1444]" class="Actinopterygii" family="Cichlidae" genus="Theraps" kingdom="Animalia" order="Perciformes" pageId="13" pageNumber="224" phylum="Chordata" rank="species" species="coeruleus">
