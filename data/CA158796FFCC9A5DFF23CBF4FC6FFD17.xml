and 
<taxonomicName id="85BC4D03FFCD9A5DFE06CB79FD25FDAD" authorityName="Hampson" authorityYear="1926" box="[485,642,613,639]" class="Insecta" family="Noctuidae" genus="Litoprosopus" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="species" species="bahamensis">
