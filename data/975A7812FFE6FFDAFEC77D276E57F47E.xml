<collectingCountry id="67E48994FFE6FFD5FC7B7C406F70F478" box="[948,1070,798,824]" name="Australia" pageId="55">Australia</collectingCountry>
: 
<collectingRegion id="DD3707E6FFE6FFD5FB8A7C406E7BF478" box="[1093,1317,798,824]" country="Australia" name="New South Wales" pageId="55">New South Wales</collectingRegion>
:
</emphasis>
<collectingCounty id="F62DB188FFE6FFD5FEC77C196A3EF421" box="[264,352,839,865]" pageId="55">Sydney</collectingCounty>
[labels: &quot;North Sydney flood refuse 9.06 HJC&quot;; &quot;Type T­4192&quot;]. 
<collectingMunicipality id="FF28537EFFE6FFD5FBA47C196FBCF421" box="[1131,1250,839,865]" pageId="55">Deposited</collectingMunicipality>
in the MVMA. 
<location id="1A2C9FDFFFE6FFD5FEB17C316AF5F4C9" LSID="urn:lsid:plazi:treatment:975A7812FFE6FFDAFEC77D276E57F47E:1A2C9FDFFFE6FFD5FEB17C316AF5F4C9" box="[382,427,879,905]" country="Australia" county="Sydney" municipality="Deposited" name="The" pageId="55" stateProvince="New South Wales">The</location>
type series in the 
<location id="1A2C9FDFFFE6FFD5FD497C3169B2F4C9" LSID="urn:lsid:plazi:treatment:975A7812FFE6FFDAFEC77D276E57F47E:1A2C9FDFFFE6FFD5FD497C3169B2F4C9" box="[646,748,879,905]" country="Australia" county="Sydney" municipality="Deposited" name="Museum" pageId="55" stateProvince="New South Wales">Museum</location>
of 
<collectingRegion id="DD3707E6FFE6FFD5FCDA7C31682DF4C9" box="[789,883,879,905]" country="Australia" name="Victoria" pageId="55">Victoria</collectingRegion>
consists of 
<specimenCount id="09F5028DFFE6FFD5FBCD7C316FD8F4C9" box="[1026,1158,879,905]" pageId="55" type="female">two female</specimenCount>
