<taxonomicName id="4C744D26FF85FF8AFF2FB5C42BA2FA09" authority="Diego N. Barbosa &amp; Ricardo Kawada &amp; Celso O. Azevedo, 2017" authorityName="Diego N. Barbosa &amp; Ricardo Kawada &amp; Celso O. Azevedo" authorityYear="2017" box="[221,404,1468,1489]" class="Insecta" family="Bethylidae" genus="Glenosema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="326" phylum="Arthropoda" rank="species" species="paulae" status="sp. nov.">
