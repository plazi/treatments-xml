<paragraph id="9C2E75879BF4AE4AF173D6818C2767A0" pageId="5" pageNumber="38">
<taxonomicName id="90D0BE75BBFC5AB17AFE6B51E55D9561" class="Insecta" family="Curculionidae" genus="Microborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microborus camerunus" order="Coleoptera" pageId="5" pageNumber="38" phylum="Arthropoda" rank="species" species="camerunus">Microborus camerunus</taxonomicName>
