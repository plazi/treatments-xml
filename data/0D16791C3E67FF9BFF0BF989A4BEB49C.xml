<taxonomicName id="42BFB3893E67FF9AFDF6F804A302B3A3" authorityName="Broun" authorityYear="1903" box="[618,877,1968,1992]" class="Insecta" family="Zopheridae" genus="Pycnomerus" kingdom="Animalia" order="Coleoptera" pageId="70" pageNumber="71" phylum="Arthropoda" rank="species" species="nitiventris">
