<paragraph id="EC96E5500F261C229FF182735E92D399" pageId="12" pageNumber="77">
<taxonomicName id="AD64B97A1BB805C28DB072C5B1BE7CB6" authority="Berg, 1877 c: 228" authorityName="Bar" authorityYear="1873" class="Insecta" family="Erebidae" genus="Palustra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Palustra burmeisteri" order="Lepidoptera" pageId="12" pageNumber="77" phylum="Arthropoda" rank="species" species="burmeisteri">Palustra burmeisteri Berg, 1877c: 228</taxonomicName>
