<quantity id="334B9B37FFAC6B6EFBD7F3B3FB6DF399" box="[1074,1206,3135,3170]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="12" pageNumber="45" unit="mm" value="4.5">4.5 mm</quantity>
