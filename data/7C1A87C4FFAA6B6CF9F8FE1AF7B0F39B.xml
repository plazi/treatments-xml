6. 
<taxonomicName id="33B34D51FFAB6B69FDA8F3D8FC8EF38C" authorityName="Bradbury &amp; Williams" authorityYear="1996" box="[589,853,3156,3191]" class="Malacostraca" family="Eriopisidae" genus="Nedsia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="44" phylum="Arthropoda" rank="species" species="hurlberti">
