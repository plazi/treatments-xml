<taxonomicName id="4C1C4D72FF94E97EFEBCF992ACB9A369" box="[277,411,1548,1573]" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="199" phylum="Arthropoda" rank="species" species="olympica">
