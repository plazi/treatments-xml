<taxonomicName id="4C1C4D72FF94E97EFD5BFA76AE53A34C" box="[754,881,1511,1536]" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="199" phylum="Arthropoda" rank="species" species="jolantae">
