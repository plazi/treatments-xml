A 
<specimenCount id="658EFD69FFC2FFCEFF45FE77F6CAFE5F" box="[189,211,421,447]" pageId="16" pageNumber="255">♀</specimenCount>
<taxonomicName id="B4884D63FFC2FFCEFF22FE77F72EFE5F" authorityName="Motschulsky" authorityYear="1844" box="[218,311,421,447]" class="Insecta" family="Ptiliidae" genus="Ptinella" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="255" phylum="Arthropoda" rank="genus">
<emphasis id="41FCEAF2FFC2FFCEFF22FE77F72EFE5F" box="[218,311,421,447]" italics="true" pageId="16" pageNumber="255">Ptinella</emphasis>
</taxonomicName>
, missing its head, from 
<collectingCountry id="0B9F7670FFC2FFCEFDB7FE77F4B4FE5F" box="[591,685,421,447]" name="Uganda" pageId="16" pageNumber="255">Uganda</collectingCountry>
, Katabana Papyrus Swamp, 
<quantity id="B4709B05FFC2FFCEFC00FE77F245FE5F" box="[1016,1116,421,447]" metricMagnitude="4" metricUnit="m" metricValue="2.2530816" pageId="16" pageNumber="255" unit="mi" value="14.0">14 miles</quantity>
N. 
<collectingRegion id="B14CF802FFC2FFCEFF55FE16F703FE3E" box="[173,282,452,478]" country="Uganda" name="Kampala" pageId="16" pageNumber="255">Kampala</collectingRegion>
, 
<geoCoordinate id="16BC5027FFC2FFCEFED4FE16F79BFE3E" box="[300,386,452,478]" degrees="0" direction="north" minutes="29" orientation="latitude" pageId="16" pageNumber="255" precision="925" value="0.48333332">0°29′N</geoCoordinate>
, 
<geoCoordinate id="16BC5027FFC2FFCEFE6DFE16F7EEFE3E" box="[405,503,452,478]" degrees="32" direction="east" minutes="37" orientation="longitude" pageId="16" pageNumber="255" precision="925" value="32.616665">32°37′E</geoCoordinate>
, 
<emphasis id="41FCEAF2FFC2FFCEFDF1FE16F434FE3E" box="[521,557,452,478]" italics="true" pageId="16" pageNumber="255">leg</emphasis>
