10. 
<taxonomicName id="4C6DBEA5FFE5FF8FFD32185DFB32FBCD" authority="(Gahan, 1890)" baseAuthorityName="Gahan" baseAuthorityYear="1890" box="[742,1155,1081,1102]" class="Insecta" family="Cerambycidae" genus="Derancistrus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="358" phylum="Arthropoda" rank="species" species="anthracinus">
