<geoCoordinate box="[1018,1171,613,637]" degrees="19" direction="north" minutes="44" orientation="latitude" pageId="35" pageNumber="427" precision="1" seconds="55.9" value="19.748861">19°44’55.9&quot;N</geoCoordinate>
