<caption ID-DOI="http://doi.org/10.5281/zenodo.6588314" ID-Zenodo-Dep="6588314" httpUri="https://zenodo.org/record/6588314/files/figure.png" inLine="true" pageId="21" pageNumber="532" targetBox="[1482,2073,2458,2872]" targetPageId="21">
<paragraph blockId="21.[2096,2690,2452,2878]" box="[2097,2687,2727,2760]" pageId="21" pageNumber="532">
