<caption ID-DOI="http://doi.org/10.5281/zenodo.6588280" ID-Zenodo-Dep="6588280" httpUri="https://zenodo.org/record/6588280/files/figure.png" inLine="true" pageId="18" pageNumber="529" targetBox="[1398,1989,554,967]" targetPageId="18">
