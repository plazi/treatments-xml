<number box="[1249,1298,854,880]" pageId="6" pageNumber="28" value="174.0">174</number>
º 
<number box="[1309,1339,854,880]" pageId="6" pageNumber="28" value="17.0">17</number>
’E
</geoCoordinate>
, 
<collectingCountry name="New Zealand" pageId="6" pageNumber="28">New Zealand</collectingCountry>
], A.T. Urquhart (lost; 
<bibRefCitation author="Court" box="[509,764,893,919]" pageId="6" pageNumber="29" refString="Court, D. J. &amp; Forster, R. R. (1988) Family Araneidae. Subfamily Araneidae [sic]. Otago Museum Bulletin, 6, 70 - 124." type="journal article" year="1988">
