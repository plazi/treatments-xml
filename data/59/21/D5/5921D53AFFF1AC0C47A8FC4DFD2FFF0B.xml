</taxonomicName>
and 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[184,295,1490,1516]" class="Magnoliopsida" family="Lentibulariaceae" genus="Utricularia" kingdom="Plantae" order="Lamiales" pageId="11" pageNumber="239" phylum="Tracheophyta" rank="species" species="foliosa">
