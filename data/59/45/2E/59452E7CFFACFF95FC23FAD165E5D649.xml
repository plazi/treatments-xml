<caption box="[155,728,1732,1756]" pageId="6" pageNumber="6" startId="6.[155,201,1732,1756]">
<paragraph blockId="6.[155,1437,1732,2076]" box="[155,728,1732,1756]" pageId="6" pageNumber="6">
Figs 1-12: 
<emphasis box="[277,566,1732,1756]" italics="true" pageId="6" pageNumber="6">
<taxonomicName authorityName="BASSLER" authorityYear="1952" box="[277,391,1732,1755]" class="Stenolaemata" family="Fistuliporidae" genus="Canutrypa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cystoporida" pageId="6" pageNumber="6" phylum="Bryozoa" rank="genus">Canutrypa</taxonomicName>
