<paragraph blockId="0.[151,1436,1403,1586]" pageId="0" pageNumber="66">
<accessionNumber box="[151,271,1403,1426]" httpUri="http://www.uniprot.org/uniprot/UF121604" pageId="0" pageNumber="66">
