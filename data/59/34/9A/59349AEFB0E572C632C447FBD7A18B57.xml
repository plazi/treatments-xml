<subSubSection type="synonymic_list">
<paragraph pageNumber="403">
-? 
<taxonomicName class="Insecta" family="Crambidae" genus="Notaspis" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepidoptera" pageNumber="403" phylum="Arthropoda" rank="species" species="schuetzi">
