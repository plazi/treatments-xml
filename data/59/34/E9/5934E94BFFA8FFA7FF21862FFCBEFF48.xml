<subSubSection box="[151,404,614,640]" pageId="1" pageNumber="50" type="nomenclature">
<paragraph blockId="1.[151,404,614,640]" box="[151,404,614,640]" pageId="1" pageNumber="50">
<heading bold="true" box="[151,404,614,640]" fontSize="11" level="1" pageId="1" pageNumber="50" reason="1">
<emphasis bold="true" box="[151,404,614,640]" pageId="1" pageNumber="50">
<taxonomicName authority="Hendrickx, 2019" authorityName="Hendrickx" authorityYear="2019" box="[151,288,614,640]" class="Malacostraca" family="Pandalidae" genus="Heteronika" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="50" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,288,614,640]" italics="true" pageId="1" pageNumber="50">Heteronika</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[294,404,614,640]" pageId="1" pageNumber="50" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
