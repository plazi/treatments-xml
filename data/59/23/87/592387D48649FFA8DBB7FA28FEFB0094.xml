<subSubSection pageId="4" pageNumber="59" type="etymology">
<paragraph blockId="4.[151,1436,152,487]" box="[197,1288,267,294]" pageId="4" pageNumber="59">
<emphasis bold="true" box="[197,338,267,293]" pageId="4" pageNumber="59">Etymology.</emphasis>
