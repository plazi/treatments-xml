(
<bibRefCitation author="HANZAWA S." box="[296,425,1985,2003]" pageId="8" pageNumber="281" pagination="85 - 96" refId="ref6032" refString="HANZAWA S. 1930. - Note on Foraminifera found in the Lepidocyclina-limestone from Pabehasan, Java. Science Reports of the Tohoku Imperial University, 2 nd series, Geology 14 (1): 85 - 96." type="journal article" year="1930">Hanzawa,1930</bibRefCitation>
)
</taxonomicName>
,axial section,megalospheric form (HYM-36A/1/1); 
<emphasis bold="true" box="[847,882,1984,2003]" pageId="8" pageNumber="281">Q -R</emphasis>
, 
<taxonomicName authority="(Henson, 1950)" baseAuthorityName="Henson" baseAuthorityYear="1950" box="[891,1218,1985,2003]" class="Tubothalamea" family="Soritidae" genus="Penarchaias" higherTaxonomySource="GBIF" kingdom="Chromista" order="Miliolida" pageId="8" pageNumber="281" phylum="Foraminifera" rank="species" species="glynnjonesi">
