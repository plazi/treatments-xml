<taxonomicName authorityName="Matsumaru" authorityYear="1976" box="[682,834,1962,1980]" class="Globothalamea" family="Planorbulinidae" genus="Neoplanorbulinella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Rotaliida" pageId="7" pageNumber="280" phylum="Foraminifera" rank="genus">
