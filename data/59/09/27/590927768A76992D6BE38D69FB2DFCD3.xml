<collectingCountry box="[835,903,837,863]" name="Palau" pageId="3" pageNumber="16">Palau</collectingCountry>
, 
<collectingCountry box="[915,1032,837,863]" name="Indonesia" pageId="3" pageNumber="16">Indonesia</collectingCountry>
, Marianas and 
<collectingCountry box="[1209,1254,837,863]" name="Fiji" pageId="3" pageNumber="16">Fiji</collectingCountry>
.
</paragraph>
