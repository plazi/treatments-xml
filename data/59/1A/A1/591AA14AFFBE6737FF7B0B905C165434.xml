<subSubSection pageId="9" pageNumber="84" type="materials_examined">
<paragraph blockId="9.[140,1108,342,616]" pageId="9" pageNumber="84">
<emphasis bold="true" box="[140,372,342,368]" pageId="9" pageNumber="84">
<emphasis box="[140,241,342,368]" inLineHeading="true" pageId="9" pageNumber="84" reason="1">Material</emphasis>
