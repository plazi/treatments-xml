<paragraph blockId="9.[140,1108,870,1672]" pageId="9" pageNumber="84">
<emphasis bold="true" box="[140,453,870,897]" pageId="9" pageNumber="84">
<emphasis box="[140,198,871,897]" inLineHeading="true" pageId="9" pageNumber="84" reason="1">Type</emphasis>
material (examined).
</emphasis>
<materialsCitation ID-GBIF-Occurrence="2608997617" collectionCode="GNM" collectorName="K. Lindberg" country="Afghanistan" location="Kabul" pageId="9" pageNumber="84" specimenCount="2" specimenCount-female="2" stateProvince="Kabul" typeStatus="holotype">
