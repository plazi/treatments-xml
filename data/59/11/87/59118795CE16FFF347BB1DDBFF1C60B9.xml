ist unglaublich weit verbreitet, in ganz 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF344921A7DFBCA617C" box="[923,1037,1109,1131]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Nordafrika" pageId="9" pageNumber="362">Nordafrika</location>
, 
<collectorName pageId="9" pageNumber="362">von Iberien</collectorName>
bis 
<collectorName box="[302,431,1139,1161]" pageId="9" pageNumber="362">zum Pazifik</collectorName>
, nach 
<collectingCountry box="[506,582,1139,1161]" name="Taiwan" pageId="9" pageNumber="362">Taiwan</collectingCountry>
und 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF3458A1A5BFD77619F" box="[643,688,1139,1160]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Java" pageId="9" pageNumber="362">Java</location>
(SCHEUCHL &amp; WILLNER 2016: 323). 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF347BB1AB8FE5661B2" box="[178,401,1164,1190]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Innerhalb Osterreichs" pageId="9" pageNumber="362">Innerhalb Österreichs</location>
