<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF3455A1AB8FD1461B1" box="[595,723,1168,1190]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Pannonikum" pageId="9" pageNumber="362">Pannonikum</location>
viele präzise 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF344671AB8FC1E61B1" box="[878,985,1168,1190]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Funddaten" pageId="9" pageNumber="362">Funddaten</location>
aus dem 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF347BB1A85FE8861D4" box="[178,335,1197,1219]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Wiener Becken" pageId="9" pageNumber="362">Wiener Becken</location>
und östlich davon (
<bibRefCitation author="ZETTEL H &amp; SCHODL S" box="[538,783,1197,1219]" pageId="9" pageNumber="362" pagination="99 - 124" refId="ref14263" refString="ZETTEL H., SCHODL S. &amp; H. WIESBAUER (2004): Zur Kenntnis der Wildbienen (Hymenoptera: Apidae) in Wien, Niederosterreich und dem Burgenland (Osterreich) - 1. - Beitr. zur Entomofaunistik 5: 99 - 124." type="journal article" year="2004">ZETTEL et al. 2004: 109</bibRefCitation>
; hier auch ein Foto des 
<specimenCount box="[1027,1046,1203,1224]" pageId="9" pageNumber="362" type="female">♀</specimenCount>
auf Seite 119). 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF346261AE3FDFF61F6" box="[303,568,1227,1249]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Doch in Niederosterreich" pageId="9" pageNumber="362">Doch in Niederösterreich</location>
kommt 
<taxonomicName authorityName="Lepeletier" authorityYear="1841" box="[661,737,1227,1248]" class="Insecta" family="Megachilidae" genus="Coelioxys" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="afra">
<emphasis box="[661,737,1227,1248]" italics="true" pageId="9" pageNumber="362">C. afra</emphasis>
</taxonomicName>
auch weiter westlich vor, als in dieser 
<location LSID="urn:lsid:plazi:treatment:59118795CE16FFF347BB1DDBFF1C60B9:D4676058CE16FFF347FC1AC0FEAD61E9" box="[245,362,1256,1278]" country="Taiwan" county="Weinbergwege" latitude="48.439445" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" name="Publikation" pageId="9" pageNumber="362">Publikation</location>
angeführt: 
<collectorName box="[480,646,1256,1278]" pageId="9" pageNumber="362">NW Gedersdorf</collectorName>
, 
<collectingMunicipality box="[657,778,1256,1278]" pageId="9" pageNumber="362">Spiegelweg</collectingMunicipality>
, 
<collectingCounty box="[789,945,1256,1278]" pageId="9" pageNumber="362">Weinbergwege</collectingCounty>
, 
<geoCoordinate box="[955,1085,1256,1278]" degrees="48" direction="north" minutes="26" orientation="latitude" pageId="9" pageNumber="362" precision="15" seconds="22" value="48.439445">N48°26‘22‘‘</geoCoordinate>
<geoCoordinate box="[178,307,1285,1307]" degrees="15" direction="east" minutes="41" orientation="longitude" pageId="9" pageNumber="362" precision="15" seconds="36" value="15.693334">E15°41‘36‘‘</geoCoordinate>
, 
<date box="[319,422,1285,1307]" pageId="9" pageNumber="362" value="2003-10-06">
