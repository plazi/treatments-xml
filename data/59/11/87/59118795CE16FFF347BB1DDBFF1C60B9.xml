<materialsCitation ID-GBIF-Occurrence="2848974305" collectingDate="2003-10-06" collectingDateMax="2016-09-08" collectingDateMin="2003-10-06" collectorName="von Iberien &amp; zum Pazifik &amp; NW Gedersdorf" country="Taiwan" county="Weinbergwege" latitude="48.439445" location="Die Schuppenhaarige Kegelbiene" longLatPrecision="18" longitude="15.693334" municipality="Spiegelweg" pageId="9" pageNumber="362" specimenCount="7" specimenCount-female="6" specimenCount-male="1">
