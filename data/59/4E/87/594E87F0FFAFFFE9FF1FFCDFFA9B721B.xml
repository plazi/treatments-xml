<emphasis box="[698,852,1643,1665]" italics="true" pageId="6" pageNumber="207">P. kalligraphos</emphasis>
</taxonomicName>
, ♀; 10, 
<taxonomicName box="[935,1069,1643,1665]" class="Insecta" family="Yponomeutidae" genus="Prays" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="207" phylum="Arthropoda" rank="species" species="rectangula" status="sp. nov.">
<emphasis box="[935,1069,1643,1665]" italics="true" pageId="6" pageNumber="207">P. rectangula</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1076,1157,1643,1664]" pageId="6" pageNumber="207" rank="species">
<emphasis bold="true" box="[1076,1157,1643,1664]" pageId="6" pageNumber="207">sp. nov.</emphasis>
