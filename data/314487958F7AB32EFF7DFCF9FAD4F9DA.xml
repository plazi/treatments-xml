<taxonomicName id="7EED4D008F7AB32EFE04FAF3FD63FAA6" box="[494,679,1322,1347]" class="Gastropoda" family="Pupinidae" genus="Pseudopomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="19" pageNumber="20" phylum="Mollusca" rank="genus">
<emphasis id="8B99EA918F7AB32EFE04FAF3FD63FAA6" box="[494,679,1322,1347]" italics="true" pageId="19" pageNumber="20">Pseudopomatias</emphasis>
</taxonomicName>
