<taxonomicName id="4C674D596B06FFBB6E02FAE9FC3EFF45" box="[795,904,219,243]" class="Insecta" family="Coccinellidae" genus="Scymnus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="26" phylum="Arthropoda" rank="species" species="malleatus">
