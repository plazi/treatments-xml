<subSubSection id="C37D65516B06FFBB6DDCFAE9FE8BFEA2" pageId="4" pageNumber="26" type="etymology">
<paragraph id="8BD836DA6B06FFBB6DDCFAE9FE8BFEA2" blockId="4.[151,1436,151,310]" pageId="4" pageNumber="26">
<emphasis id="B913EAC86B06FFBB6DDCFAE9FEFAFF45" bold="true" box="[197,332,219,243]" pageId="4" pageNumber="26">Etymology.</emphasis>
