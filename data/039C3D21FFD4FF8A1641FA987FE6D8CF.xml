<quantity id="4CCD21D2FFD1FF8A1774FD4E7DCCDFBF" box="[814,913,705,726]" metricMagnitude="-3" metricUnit="m" metricValue="4.67" pageId="4" pageNumber="316" unit="mm" value="4.67">4.67 mm</quantity>
