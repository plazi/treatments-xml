<taxonomicName id="4C7B4D46FFA9FFC6808BFEE9FD923454" authority="Lobl, 1984b: 184" authorityName="Lobl" authorityPageNumber="184" authorityYear="1984" box="[151,514,350,373]" class="Insecta" family="Staphylinidae" genus="Baeocera" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="324" phylum="Arthropoda" rank="species" species="micros">
