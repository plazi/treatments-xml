<taxonomicName id="D83A4EEBF1C25ADA44AD094166BAA73B" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Diptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Diptera" order="Malvales" pageId="150" pageNumber="151" phylum="Tracheophyta" rank="genus">Diptera</taxonomicName>
