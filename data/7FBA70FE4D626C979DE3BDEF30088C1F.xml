<taxonomicName id="27738F4559579015224DB884CFE51AC4" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Diptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Diptera" order="Malvales" pageId="150" pageNumber="151" phylum="Tracheophyta" rank="genus">Diptera</taxonomicName>
name is 
<taxonomicName id="03CCDF12C698F97A560DC3B07770ED34" genus="Drapetid" lsidName="Drapetid" pageId="150" pageNumber="151" rank="genus">Drapetid</taxonomicName>
