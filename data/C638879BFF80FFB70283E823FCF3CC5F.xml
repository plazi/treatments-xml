<paragraph id="4E2E368DFF82FFB7035CECEEFC9CCB5C" blockId="15.[125,1172,662,814]" pageId="15" pageNumber="3964">
<materialsCitation id="FEF93CD0FF82FFB7035CECEEFEC9CABC" collectingDate="2002-09-13" collectionCode="SCAU" collectorName="H. - W. Chen" elevation="940" location="Menglun" municipality="Xishuangbanna" pageId="15" pageNumber="3964" specimenCount="1" stateProvince="Yunnan" typeStatus="holotype">
