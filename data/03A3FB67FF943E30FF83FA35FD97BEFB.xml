<specimenCount id="9D0C81F8FF943E30FB09F8E8FBFFBED7" box="[1053,1106,1915,1941]" pageId="6" pageNumber="521" type="generic">2 sp.</specimenCount>
n. 1 and 
<taxonomicName id="4C0A31F2FF943E30FBACF8E8FA30BED7" authority="Curini-Galletti &amp; Schockaert, 2021" authorityName="Curini-Galletti &amp; Schockaert" authorityYear="2021" box="[1208,1437,1915,1941]" class="Rhabditophora" family="Archimonocelididae" kingdom="Animalia" order="Seriata" pageId="6" pageNumber="521" phylum="Platyhelminthes" rank="family" status="gen. n.">Archimonocelididae</taxonomicName>
<taxonomicNameLabel id="A24D2B18FF943E30FF83F80CFF4EBEFB" box="[151,227,1951,1977]" pageId="6" pageNumber="521" rank="genus">gen. n.</taxonomicNameLabel>
