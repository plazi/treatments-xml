<collectorName id="26FF2FA7FF943E30FDC8F930FCDEBFFF" box="[732,883,1699,1725]" pageId="6" pageNumber="521">Las Perlas Is.</collectorName>
, 
<collectingCountry id="F31D0AE1FF943E30FC6BF930FC77BFFF" box="[895,986,1699,1725]" name="Panama" pageId="6" pageNumber="521">Panama</collectingCountry>
