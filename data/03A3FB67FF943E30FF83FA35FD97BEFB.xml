<subSubSection id="C31019FAFF943E30FFD3F8A0FD97BEFB" pageId="6" pageNumber="521" type="etymology">
<paragraph id="8BB54A71FF943E30FFD3F8A0FC34BE0F" blockId="6.[151,1437,1554,1977]" box="[199,921,1843,1869]" pageId="6" pageNumber="521">
<emphasis id="B97E9663FF943E30FFD3F8A0FEE5BE0F" bold="true" box="[199,328,1843,1869]" pageId="6" pageNumber="521">Etymology</emphasis>
