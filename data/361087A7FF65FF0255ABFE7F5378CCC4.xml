<quantity id="79419B54FF65FF005748FC54528EC919" box="[628,740,1009,1036]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="160" pageNumber="161" unit="mm" value="0.75" valueMax="1.0" valueMin="0.5">0.5–1 µm</quantity>
