<heading id="697C81A5C229A34FFF2E06A2FC0FFB49" bold="true" box="[151,989,1181,1207]" fontSize="11" level="1" pageId="9" pageNumber="10" reason="1">
<emphasis id="00FFEADBC229A34FFF2E06A2FC0FFB49" bold="true" box="[151,989,1181,1207]" pageId="9" pageNumber="10">
<taxonomicName id="F58B4D4AC229A34FFF2E06A2FCD8FB49" authority="Goncalves, Mejdalani" authorityName="Goncalves, Mejdalani" box="[151,778,1181,1207]" class="Insecta" family="Cicadellidae" genus="Chromagallia" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="paraguayensis">
<emphasis id="00FFEADBC229A34FFF2E06A2FE26FB49" bold="true" box="[151,500,1181,1207]" italics="true" pageId="9" pageNumber="10">Chromagallia paraguayensis</emphasis>
