<taxonomicName id="E6814D7BFF807003FC6DFAFD5B107486" authority="L. BENICK. Aedoeagus" authorityName="L. BENICK. Aedoeagus" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="1324" phylum="Arthropoda" rank="species" species="simulans">
