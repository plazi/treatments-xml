<taxonomicName id="4C0F4D457B4F0331EC1CFEE6FD07FE9F" box="[547,705,329,354]" class="Insecta" family="Coccidae" genus="Discochiton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="paucipedis" status="spec. n.">
