<quantity id="4CB39B27FFBEFF90262D5E56FD8FF9C4" box="[542,628,1614,1636]" metricMagnitude="-5" metricUnit="m" metricValue="6.0" pageId="16" pageNumber="335" unit="mm" value="0.06">0.06 mm</quantity>
for 42, 
<quantity id="4CB39B27FFBEFF9026F75E56FCF5F9C4" box="[708,782,1614,1636]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="16" pageNumber="335" unit="mm" value="0.1">0.1 mm</quantity>
for 38, 
<quantity id="4CB39B27FFBEFF90276D5E56FC4FF9C4" box="[862,948,1614,1636]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="16" pageNumber="335" unit="mm" value="0.13">0.13 mm</quantity>
for 40, 43, 
<quantity id="4CB39B27FFBEFF9024BB5E70FF25F9DE" box="[136,222,1640,1662]" metricMagnitude="-4" metricUnit="m" metricValue="1.6" pageId="16" pageNumber="335" unit="mm" value="0.16">0.16 mm</quantity>
for 39, 41, 
<quantity id="4CB39B27FFBEFF9025635E70FE61F9DE" box="[336,410,1640,1662]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="16" pageNumber="335" unit="mm" value="0.2">0.2 mm</quantity>
