<taxonomicName id="4C4B4D41FFBEFF9027F65E2EFF16F9C4" authority="Gildenkov" authorityName="Gildenkov" authorityYear="1998" class="Insecta" family="Staphylinidae" genus="Thinobius" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="335" phylum="Arthropoda" rank="species" species="zerchei">
