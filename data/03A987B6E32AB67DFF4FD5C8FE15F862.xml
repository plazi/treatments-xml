</taxonomicName>
, F. 
<taxonomicName id="4C004D23E329B67CFCA0D293FC08F81C" baseAuthorityName="Schintlmeister" baseAuthorityYear="1997" box="[888,986,1923,1947]" class="Insecta" family="Notodontidae" genus="Syntypistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="201" phylum="Arthropoda" rank="species" species="witoldi">
<emphasis id="B974EAB2E329B67CFCA0D293FC08F81C" box="[888,986,1923,1947]" italics="true" pageId="10" pageNumber="201">S. witoldi</emphasis>
</taxonomicName>
, G. 
<taxonomicName id="4C004D23E329B67CFBD3D294FEE8F838" authority=", H. Himalodontosia" authorityName="H. Himalodontosia" class="Insecta" family="Notodontidae" genus="Pseudosomera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="201" phylum="Arthropoda" rank="subSpecies" species="noctuiformis" subSpecies="yunwu">
