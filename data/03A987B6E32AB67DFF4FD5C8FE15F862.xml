<emphasis id="B974EAB2E329B67CFB61D248FA44F8F0" box="[1209,1430,1887,1911]" italics="true" pageId="10" pageNumber="201">Ogulina ochrocinerea</emphasis>
</taxonomicName>
, C. 
<taxonomicName id="4C004D23E329B67CFF6FD294FDC5F81C" box="[183,535,1923,1947]" class="Insecta" family="Notodontidae" genus="Odnarda" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="201" phylum="Arthropoda" rank="variety" species="leechi" variety="lucida">
<emphasis id="B974EAB2E329B67CFF6FD294FE85F81C" box="[183,343,1923,1947]" italics="true" pageId="10" pageNumber="201">Odnarda leechi</emphasis>
, D. 
<emphasis id="B974EAB2E329B67CFE50D293FDC5F81C" box="[392,535,1923,1947]" italics="true" pageId="10" pageNumber="201">Torona lucida</emphasis>
</taxonomicName>
, E. 
<taxonomicName id="4C004D23E329B67CFD9DD293FC9CF81C" baseAuthorityName="Kiriakoff" baseAuthorityYear="1964" box="[581,846,1923,1947]" class="Insecta" family="Notodontidae" genus="Syntypistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="201" phylum="Arthropoda" rank="species" species="synechochlora">
