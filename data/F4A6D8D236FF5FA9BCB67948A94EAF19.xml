<taxonomicName id="77189711290A7520A85C0C193FF32A30" class="Coniferopsida" family="Taxodiaceae" genus="Cunninghamia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Pinales" pageId="0" pageNumber="89" phylum="Gymnospermae" rank="family">Taxodiaceae</taxonomicName>
