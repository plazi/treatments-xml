<taxonomicName id="4C4F4D0CFF66FF10F21AFAEC2C607116" authorityName="Zelazny &amp; Webb" authorityYear="2011" baseAuthorityName="Fennah" baseAuthorityYear="1956" box="[335,616,1297,1321]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="palauensis">
