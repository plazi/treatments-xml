<taxonomicName id="59C6A61D1BBB178E051801907970A8C8" class="Insecta" family="Staphylinidae" genus="Domene" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Domene reitteri" order="Coleoptera" pageId="9" pageNumber="118" phylum="Arthropoda" rank="species" species="reitteri">Domene reitteri</taxonomicName>
Koch / 
<taxonomicName id="AAA67DF52456E4DB3C2CD0690B1DEA7F" class="Insecta" family="Staphylinidae" genus="Domene" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Domene reitteri" order="Coleoptera" pageId="9" pageNumber="118" phylum="Arthropoda" rank="species" species="reitteri">Domene reitteri</taxonomicName>
