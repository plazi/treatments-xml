r. 
<taxonomicName id="85F89FF638FBC0D3F1AF2C0D3FAA824A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27219" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus sylvaticus (Olivier)" order="Hymenoptera" pageNumber="66" phylum="Arthropoda" rank="species" species="sylvaticus">C. sylvaticus Oliv</taxonomicName>
