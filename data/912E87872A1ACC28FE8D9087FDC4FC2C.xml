<taxonomicName id="DE874D122A1ACC28FE8D9087FD70FD36" authority="Navas" authorityName="Navas" box="[264,665,737,763]" class="Insecta" family="Aeshnidae" genus="Triacanthagyna" kingdom="Animalia" order="Odonata" pageId="10" pageNumber="47" phylum="Arthropoda" rank="species" species="nympha">
<emphasis id="2BF3EA832A1ACC28FE8D9087FD70FD36" bold="true" box="[264,665,737,763]" pageId="10" pageNumber="47">
<emphasis id="2BF3EA832A1ACC28FE8D9087FDDBFD36" bold="true" box="[264,562,737,763]" italics="true" pageId="10" pageNumber="47">Triacanthagyna nympha</emphasis>
