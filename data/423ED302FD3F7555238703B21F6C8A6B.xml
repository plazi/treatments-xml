<taxonomicName id="A7036EB6316BF8795DD8073D5088E4C1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:239843" class="Insecta" family="Formicidae" genus="Atta" kingdom="Animalia" lsidName-HNS="Atta (Acromyrmex) coronata (Fabricius)" order="Hymenoptera" pageNumber="290" phylum="Arthropoda" rank="species" species="coronata" subGenus="Acromyrmex">Atta (Acromyrmex) coronata F.</taxonomicName>
