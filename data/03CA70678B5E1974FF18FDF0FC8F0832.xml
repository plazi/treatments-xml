<taxonomicName id="4C63BAF28B5E1974FF75FCBDFEF90832" authorityName="Wang &amp; Zhu &amp; Zhao &amp; Yang" authorityYear="2020" baseAuthorityName="Wang" baseAuthorityYear="2004" box="[250,379,801,827]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="377" phylum="Arthropoda" rank="species" species="acutiuscula">
<emphasis id="B9171D638B5E1974FF75FCBDFEF90832" box="[250,379,801,827]" italics="true" pageId="6" pageNumber="377">acutiuscula</emphasis>
