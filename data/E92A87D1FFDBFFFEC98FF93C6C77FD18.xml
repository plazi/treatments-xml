, 
<taxonomicName id="A6834D44FFDCFFFFCBA0FB776B1BFB16" authorityName="Matsuo" authorityYear="2020" box="[696,825,1253,1277]" class="Insecta" family="Torymidae" genus="Torymus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="405" phylum="Arthropoda" rank="species" species="aciculatus">
<emphasis id="53F7EAD5FFDCFFFFCBA0FB776B1BFB16" box="[696,825,1253,1277]" italics="true" pageId="4" pageNumber="405">T. aciculatus</emphasis>
</taxonomicName>
; 
<emphasis id="53F7EAD5FFDCFFFFCA5EFB746B73FB16" bold="true" box="[838,849,1253,1277]" pageId="4" pageNumber="405">c</emphasis>
, 
<taxonomicName id="A6834D44FFDCFFFFCA46FB776BCFFB16" authorityName="Matsuo" authorityYear="2020" box="[862,1005,1254,1277]" class="Insecta" family="Torymidae" genus="Torymus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="405" phylum="Arthropoda" rank="species" species="acutissimus">
<emphasis id="53F7EAD5FFDCFFFFCA46FB776BCFFB16" box="[862,1005,1254,1277]" italics="true" pageId="4" pageNumber="405">T. acutissimus</emphasis>
</taxonomicName>
; 
<emphasis id="53F7EAD5FFDCFFFFCAE3FB746C2BFB16" bold="true" box="[1019,1033,1253,1277]" pageId="4" pageNumber="405">d</emphasis>
, 
<taxonomicName id="A6834D44FFDCFFFFCD0DFB776C9EFB16" authorityName="Matsuo" authorityYear="2020" box="[1045,1212,1253,1277]" class="Insecta" family="Torymidae" genus="Torymus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="405" phylum="Arthropoda" rank="species" species="angustitemple">
<emphasis id="53F7EAD5FFDCFFFFCD0DFB776C9EFB16" box="[1045,1212,1253,1277]" italics="true" pageId="4" pageNumber="405">T. angustitemple</emphasis>
