<quantity id="4CC19B042C5BFFA703D6FF79FD55FE8E" box="[695,754,247,267]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="32" pageNumber="153" unit="m" value="700.0">700 m</quantity>
