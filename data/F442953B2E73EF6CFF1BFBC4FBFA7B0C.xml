<taxonomicName id="BBEB5FAE2E73EF6CFF54FBC4FDDF7A6F" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[242,618,1067,1094]" class="Elasmobranchii" family="Rajidae" genus="Dipturus" kingdom="Animalia" order="Rajiformes" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="batis">
<emphasis id="4E9FF83F2E73EF6CFF54FBC4FE2E7A6C" box="[242,411,1067,1093]" italics="true" pageId="14" pageNumber="15">Dipturus batis</emphasis>
