<heading id="A605819CFFC7FF816EC0F57AFD4B92CE" box="[387,734,892,914]" centered="true" fontSize="9" level="2" pageId="17" pageNumber="430" reason="2">
<taxonomicName id="3AF24D73FFC7FF816EC0F57AFD4B92CE" authority="Triplehorn, 2007" authorityName="Triplehorn" authorityYear="2007" box="[387,734,892,914]" class="Insecta" family="Tenebrionidae" genus="Eleodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="430" phylum="Arthropoda" rank="species" species="mirabilis">
