<taxonomicName id="F4E74D5EFFC058474F27A66C3013FA1D" box="[288,532,1393,1416]" class="Malacostraca" family="Melitidae" genus="Quadrimaera" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="chaelata" status="sp. nov.">
<emphasis id="0193EACFFFC058474F27A66C3013FA1D" box="[288,532,1393,1416]" italics="true" pageId="4" pageNumber="59">
Quadrimaera 
<taxonomicName id="F4E74D5EFFC058474FBFA66C3013FA1D" box="[440,532,1393,1416]" pageId="4" pageNumber="59" rank="species" species="chaelata" status="sp. nov.">chaelata</taxonomicName>
