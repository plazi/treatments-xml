<taxonomicName id="6BC194C65A3B1393FC991D0BBA6EB581" LSID="urn:lsid:zoobank.org:act:BD1A264E-6EDB-4F69-A2D7-5EB0FBB57583" class="Insecta" family="Histeridae" genus="Afroprinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Afroprinus" order="Coleoptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="genus">Afroprinus</taxonomicName>
