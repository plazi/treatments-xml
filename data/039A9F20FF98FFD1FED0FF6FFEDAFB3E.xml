<quantity id="4CCB83D3FF98FFD6FE69FD36FDECFD10" box="[456,568,733,757]" metricMagnitude="1" metricUnit="m" metricValue="7.4" metricValueMax="13.0" metricValueMin="1.8" pageId="35" pageNumber="820" unit="m" value="74.0" valueMax="130.0" valueMin="18.0">
18± 
<specimenCount id="9D35E5BFFF98FFD6FE52FD36FDECFD10" box="[499,568,733,757]" pageId="35" pageNumber="820" type="generic">130 m</specimenCount>
