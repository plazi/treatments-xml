<taxonomicName id="2E64BEDF87951439174E76BACF18D53D" class="Insecta" family="Chrysomelidae" genus="Typophorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Typophorus" order="Coleoptera" pageId="239" pageNumber="240" phylum="Arthropoda" rank="genus">Typophorus</taxonomicName>
