<number id="3D85248DE572FFB644F7FB67FD416AD6" box="[736,752,1196,1222]" pageId="7" pageNumber="50" value="2.0">2</number>
. Trochanters 
<number id="3D85248DE572FFB64599FB67FC2E6AD6" box="[910,927,1196,1222]" pageId="7" pageNumber="50" value="6.0">6</number>
- 
<number id="3D85248DE572FFB645BFFB67FC066AD6" box="[936,951,1196,1222]" pageId="7" pageNumber="50" value="5.0">5</number>
- 
<number id="3D85248DE572FFB645D6FB67FC616AD6" box="[961,976,1196,1222]" pageId="7" pageNumber="50" value="5.0">5</number>
