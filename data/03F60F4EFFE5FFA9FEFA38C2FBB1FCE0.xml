: 
<taxonomicName id="4C5FC5DBFFE5FFABFE5838B5FCB0FC7D" authority="Silvestri, 1899" authorityName="Silvestri" authorityYear="1899" box="[426,868,993,1019]" class="Chilopoda" family="Henicopidae" genus="Anopsobius" kingdom="Animalia" order="Lithobiomorpha" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="productus">
