<taxonomicName id="F8760F206E486CB72546086F30C13C47" LSID-ZBK="urn:lsid:zoobank.org:act:1C60A92D-D5DE-4D43-99D9-16D1565EDA2E" authority="McClelland, 1839" authorityName="McClelland" authorityYear="1839" class="Actinopterygii" family="Cobitidae" genus="Hymenphysa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-ZBK="Hymenphysa McClelland 1839:443" order="Cypriniformes" pageNumber="11" phylum="Chordata" rank="genus">Hymenphysa M'Clelland, 1839</taxonomicName>
