<paragraph id="9E66DC1CF4CB0E124C779820E3D7C3F7" pageId="13" pageNumber="14">
<taxonomicName id="502D34B9D3AE5CB53AC3672481A50476" authority="Smith, 2011" authorityName="Smith" authorityYear="2011" class="Aves" family="Alcidae" genus="Miomancalla" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Miomancalla howardi" order="Charadriiformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="howardi">Miomancalla howardi</taxonomicName>
