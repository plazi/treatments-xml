<taxonomicName id="4C6915BEB936B517FEBEFB61FDE9FAFF" box="[293,553,1271,1293]" class="Insecta" family="Formicidae" genus="Adelomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="bispeculum" status="sp. nov.">
