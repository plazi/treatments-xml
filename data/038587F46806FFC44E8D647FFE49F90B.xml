<taxonomicName id="4C2C4D616806FFC44D0962B5FC43FA33" authorityName="sensu Stal" authorityYear="1854" box="[787,958,1434,1459]" class="Insecta" family="Pentatomidae" genus="Paramecocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="547" phylum="Arthropoda" rank="genus">
