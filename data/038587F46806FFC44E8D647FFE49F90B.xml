<taxonomicName id="4C2C4D616806FFC44DF96355FB73FB13" authorityName="sensu Stal" authorityYear="1854" box="[995,1166,1146,1171]" class="Insecta" family="Pentatomidae" genus="Paramecocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="547" phylum="Arthropoda" rank="genus">
