<treatmentCitationGroup id="96F8119817774E2DFF54FC55FC2AFC4C" box="[151,954,906,928]" pageId="36" pageNumber="37">
<treatmentCitation id="374910A717774E2DFF54FC55FDC7FC4C" author="Jennings, J. T. &amp; Austin, A. D. &amp; Stevens, N. B." box="[151,599,906,928]" page="116" pageId="36" pageNumber="37" year="2004">
