<taxonomicName id="4C464D516927FFA4FE8BFEEA3C4AFB2D" authorityName="Cook" authorityYear="1895" box="[379,546,1219,1243]" class="Diplopoda" family="Chelodesmidae" kingdom="Animalia" order="Polydesmida" pageId="7" pageNumber="644" phylum="Arthropoda" rank="family">Chelodesmidae</taxonomicName>
