<taxonomicName id="4C464D516927FFA4FEB2FECF3FA3FB0B" box="[322,459,1253,1277]" class="Diplopoda" family="Chelodesmidae" genus="Maatidesmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="7" pageNumber="644" phylum="Arthropoda" rank="species" species="paachtun">
