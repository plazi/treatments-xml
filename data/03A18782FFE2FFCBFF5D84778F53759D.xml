<emphasis id="B97CEA86FFE3FFC8FF5D80858BB470DE" bold="true" box="[151,164,1969,1990]" pageId="5" pageNumber="182">9</emphasis>
, 
<taxonomicName id="4C084D17FFE3FFC8FF7B80858AB770DF" authorityName="Santos-Silva" authorityYear="2017" baseAuthorityName="Chemsak &amp; Hovore" baseAuthorityYear="2010" box="[177,423,1969,1991]" class="Insecta" family="Cerambycidae" genus="Monneellus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="182" phylum="Arthropoda" rank="species" species="semilunatus">
