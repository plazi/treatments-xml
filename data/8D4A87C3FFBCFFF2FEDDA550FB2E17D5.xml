<quantity id="C21B9B30FFBDFFF2FD71A268FEE117B6" box="[329,371,628,654]" metricMagnitude="0" metricUnit="m" metricValue="6.0" pageId="5" pageNumber="250" unit="m" value="6.0">6 m</quantity>
