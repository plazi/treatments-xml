<normalizedToken id="D8683CAB46E6CAF0CF24F9D070E7D666" originalValue="Männchen">Maennchen</normalizedToken>
<normalizedToken id="011BB4AA93CC1758EC9AB0633B1CB582" originalValue="führte">fuehrte</normalizedToken>
