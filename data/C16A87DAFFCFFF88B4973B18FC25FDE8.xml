<emphasis id="7BB7EADEFFCFFF89B4973BACFEFCFC14" bold="true" box="[151,330,978,1003]" pageId="4" pageNumber="134">
<typeStatus id="9678886EFFCFFF89B4973BACFF64FC14" box="[151,210,979,1003]" pageId="4" pageNumber="134">Type</typeStatus>
material.
</emphasis>
<typeStatus id="9678886EFFCFFF89B5563BADFE08FC15" box="[342,446,978,1002]" pageId="4" pageNumber="134">Holotype</typeStatus>
<number id="FF0B24EBFFCFFF89B5CB3BACFE56FC14" box="[459,480,979,1003]" pageId="4" pageNumber="134" value="3.0">3</number>
. Queensland: Annan River, 
<quantity id="8E3B9B29FFCFFF89B7323BADFCC7FC15" box="[818,881,978,1002]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="4" pageNumber="134" unit="km" value="3.0">3 km</quantity>
