<taxonomicName id="4C7B3AB2FFCFFFF5FE89F8706443D5BD" box="[348,537,1964,1985]" class="Insecta" family="Ichneumonidae" genus="Stethantyx" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="100" phylum="Arthropoda" rank="species" species="radiata" status="sp. nov.">
<emphasis id="B90F9D23FFCFFFF5FE89F8706443D5BD" box="[348,537,1964,1985]" italics="true" pageId="5" pageNumber="100">
Stethantyx 
<taxonomicName id="4C7B3AB2FFCFFFF5FE1BF8706443D5BD" box="[462,537,1964,1985]" pageId="5" pageNumber="100" rank="species" species="radiata" status="sp. nov.">radiata</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23C2058FFCFFFF5FDF7F871642FD5BE" box="[546,629,1965,1986]" pageId="5" pageNumber="100" rank="species">
