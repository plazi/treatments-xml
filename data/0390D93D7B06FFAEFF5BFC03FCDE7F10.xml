<taxonomicName id="4C3913A87B06FFAFFEEAFC5DFD887FED" authority="Cramer" authorityName="Cramer" authorityYear="1775" box="[294,560,978,1000]" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="125" phylum="Arthropoda" rank="species" species="imperialis">
