<taxonomicName id="5E57BEFD5E5BB2C7BD9E6BF2EEC0EC86" class="Insecta" family="Lampyridae" genus="Lampyris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lampyris" order="Coleoptera" pageId="159" pageNumber="160" phylum="Arthropoda" rank="genus">Lampyris</taxonomicName>
