<taxonomicName id="4C6DF3DBC313FFF0FE7ABFF4FDB7F97D" box="[401,532,1761,1783]" class="Insecta" family="Phoridae" genus="Apocephalus" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="21" phylum="Arthropoda" rank="genus">
