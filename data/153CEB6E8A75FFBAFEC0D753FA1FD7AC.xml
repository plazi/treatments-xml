<number id="2B5D485F8A75FFB9FD85D451FD6CD73E" box="[589,600,537,563]" pageId="49" pageNumber="50" value="1.0">1</number>
/ 
<number id="2B5D485F8A75FFB9FDABD451FD46D73E" box="[611,626,537,563]" pageId="49" pageNumber="50" value="3.0">3</number>
distance from base (
<figureCitation id="05AE46FD8A75FFB9FCADD451FCFDD73E" box="[869,969,537,563]" captionStart="FIGURE 126" captionStartId="49.[264,368,1625,1649]" captionTargetBox="[272,1250,1431,1600]" captionTargetId="figure@49.[264,1260,1417,1619]" captionTargetPageId="49" captionText="FIGURE 126. Ochlerotatus (Gymnometopa) mediovittatus." httpUri="https://zenodo.org/record/169261/files/figure.png" pageId="49" pageNumber="50">
