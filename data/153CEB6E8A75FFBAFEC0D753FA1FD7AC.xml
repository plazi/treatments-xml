<figureCitation id="05AE46FD8A75FFB9FC95D494FCF3D7FB" box="[861,967,732,758]" captionStart="FIGURE 127" captionStartId="49.[819,923,1315,1339]" captionTargetBox="[817,1188,841,1296]" captionTargetId="figure@49.[804,1193,841,1304]" captionTargetPageId="49" captionText="FIGURE 127. Aedes (Stegomyia) aegypti." httpUri="https://zenodo.org/record/169260/files/figure.png" pageId="49" pageNumber="50">
