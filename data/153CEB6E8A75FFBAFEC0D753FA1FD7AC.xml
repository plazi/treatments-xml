<figureCitation id="05AE46FD8A75FFB9FDBFD781FDE8D4EE" box="[631,732,457,483]" captionStart="FIGURE 125" captionStartId="49.[264,368,1315,1339]" captionTargetBox="[355,684,849,1304]" captionTargetId="figure@49.[341,688,849,1310]" captionTargetPageId="49" captionText="FIGURE 125. Ochlerotatus (Gymnometopa) mediovittatus." httpUri="https://zenodo.org/record/169259/files/figure.png" pageId="49" pageNumber="50">
