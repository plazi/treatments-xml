<taxonomicName id="4C011396D22F0261FBD5FA78542C36BD" box="[1148,1326,1419,1443]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="32" pageNumber="2814" phylum="Firmicutes" rank="species" species="thailandensis">
