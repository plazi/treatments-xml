<taxonomicName id="4C714D2281BAFF16FE15FCDFFD36FC5A" authority="Turrill" authorityName="Turrill" box="[409,645,878,904]" class="Liliopsida" family="Orchidaceae" genus="Habenaria" kingdom="Plantae" order="Asparagales" pageId="149" pageNumber="150" phylum="Tracheophyta" rank="species" species="medioflexa">
