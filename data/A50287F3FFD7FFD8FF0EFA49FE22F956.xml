<taxonomicName id="EAAB4D66FFD7FFD9FF44FA49FD83FA4E" authority="Latreille, 1824" authorityName="Latreille" authorityYear="1824" box="[221,614,1487,1512]" class="Insecta" family="Hesperiidae" genus="Gegenes" kingdom="Animalia" order="Lepidoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="hottentota">
