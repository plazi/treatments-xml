<taxonomicName id="8FAE58F7FFC9FF871D58FD8BFD08FDD2" authority="Baviera, 2017" authorityName="Baviera" authorityYear="2017" box="[628,756,548,573]" class="Insecta" family="Curculionidae" genus="Dodomeira" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="53" phylum="Arthropoda" rank="genus" status="gen. n.">
