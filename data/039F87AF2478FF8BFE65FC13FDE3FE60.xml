<taxonomicName id="4C364D3A2467FF8AFDBCF88CFCD1F8B5" authority="Horvath, 1905" authorityName="Horvath" authorityYear="1905" box="[523,817,1844,1871]" class="Insecta" family="Corixidae" genus="Micronecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="sedula">
