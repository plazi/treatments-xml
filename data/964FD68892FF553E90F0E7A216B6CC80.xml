<paragraph id="C23D584158C1CA8F4213271F44D7EC04" pageId="10" pageNumber="11">
<taxonomicName id="FCE161D811E76F9C881360637BD60B16" authorityName="Fujii &amp; Reimer" authorityYear="2011" class="Anthozoa" family="Nanozoanthidae" genus="Microzoanthus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microzoanthus" order="Zoantharia" pageId="10" pageNumber="11" phylum="Cnidaria" rank="genus">
