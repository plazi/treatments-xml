<taxonomicName id="4C654D71FFB4436BFB06C2498DADF8D2" box="[1171,1428,1833,1858]" class="Insecta" family="Aphalaridae" genus="Ctenarytaina" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="eucalypti">
