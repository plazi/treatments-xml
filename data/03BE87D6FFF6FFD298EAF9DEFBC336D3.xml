<geoCoordinate id="EE235007FFF6FFD29AF9F9A2F8BD366F" box="[644,764,1616,1642]" degrees="23" direction="north" minutes="24.6" orientation="latitude" pageId="103" pageNumber="104" precision="92" value="23.41">23°24.6’N</geoCoordinate>
