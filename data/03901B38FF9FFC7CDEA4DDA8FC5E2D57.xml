<heading id="D0CE1D42FF9FFC7CDEA4DDA8FDFB2FEB" bold="true" box="[151,515,1494,1520]" fontSize="11" level="1" pageId="8" pageNumber="39" reason="1">
<taxonomicName id="4C39D1ADFF9FFC7CDEA4DDA8FDFB2FEB" authority="Takaoka, 2001" authorityName="Takaoka" authorityYear="2001" box="[151,515,1494,1520]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="39" phylum="Arthropoda" rank="species" species="weji">
