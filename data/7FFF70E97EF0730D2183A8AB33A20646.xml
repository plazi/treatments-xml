<paragraph id="9471C874644F875636C1B1A7F473980F" pageNumber="1">
<taxonomicName id="58844C29564BE5EC18BDCA034D9D03E1" authority="Koch, 1835" authorityName="Koch" authorityYear="1835" class="Arachnida" family="Phthiracaridae" genus="Hoplophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageNumber="1" phylum="Arthropoda" rank="species" species="ardua">Hoplophora ardua</taxonomicName>
