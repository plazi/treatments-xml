<taxonomicName id="4C008C8FFF8AFF985364B8E04F79C06F" box="[380,527,1010,1033]" class="Actinopterygii" family="Bramidae" genus="Taractichthys" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageId="19" pageNumber="20" phylum="Chordata" rank="genus">Taractichthys</taxonomicName>
