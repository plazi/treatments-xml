<heading id="D09C81B5FFB1C816FF36FE1FFD45FE55" allCaps="true" box="[246,664,413,437]" centered="true" fontSize="10" level="1" pageId="2" pageNumber="345" reason="1">
<emphasis id="B91FEACBFFB1C816FF36FE1FFD45FE55" bold="true" box="[246,664,413,437]" pageId="2" pageNumber="345">
KEY TO SPECIES OF 
<taxonomicName id="4C6B4D5AFFB1C816FDC8FE1FFD45FE55" authorityName="Latreille" authorityYear="1807" box="[520,664,413,437]" class="Insecta" family="Ochteridae" genus="Ochterus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="345" phylum="Arthropoda" rank="genus">
