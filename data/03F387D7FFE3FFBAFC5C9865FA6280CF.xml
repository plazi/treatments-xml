<taxonomicName id="4C5A4D42FFE3FFBAFB469EA2FA6383CE" authorityName="Carter in Longridge, Carter, Smith &amp; Tipper" authorityYear="2007" box="[1210,1457,1427,1453]" class="Polycystina" genus="Tipperella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Spumellaria" pageId="21" pageNumber="512" phylum="Radiozoa" rank="species" species="kennecottensis">
