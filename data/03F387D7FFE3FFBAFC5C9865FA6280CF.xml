<taxonomicName id="4C5A4D42FFE3FFBAFB3B9EC2FA7D806E" authorityName="Carter in Longridge, Carter, Smith &amp; Tipper" authorityYear="2007" box="[1223,1455,1523,1549]" class="Polycystina" genus="Tipperella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Spumellaria" pageId="21" pageNumber="512" phylum="Radiozoa" rank="species" species="kennecottensis">
<emphasis id="B92EEAD3FFE3FFBAFB3B9EC2FA7D806E" box="[1223,1455,1523,1549]" italics="true" pageId="21" pageNumber="512">Tipperella kennecottensis</emphasis>
