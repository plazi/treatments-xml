locality in 
<collectingCountry id="C6BF02D89E0B936BA014FE53FC7DDBA2" box="[891,959,439,463]" name="Brazil" pageId="82" pageNumber="83">Brazil</collectingCountry>
(
<figureCitation id="26935ECD9E0B936BA0A1FE53FBF3DBA2" box="[974,1073,439,463]" captionStart="FIGURE 126" captionStartId="86.[151,250,1870,1893]" captionTargetBox="[283,1304,337,1849]" captionTargetId="figure-105@86.[283,1304,337,1849]" captionTargetPageId="86" captionText="FIGURE 126. Geographical distributions of Epimetopus species." pageId="82" pageNumber="83">Fig. 126</figureCitation>
).
</paragraph>
</subSubSection>
