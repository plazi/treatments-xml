<taxonomicName id="9CFA4D38FB53FFFCFEA82DAC2DF2229E" authority="(Daday, 1913)" baseAuthorityName="Daday" baseAuthorityYear="1913" box="[275,640,1945,1968]" class="Branchiopoda" family="Cyzicidae" genus="Ozestheria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="2" pageNumber="248" phylum="Arthropoda" rank="species" species="annandalei">
