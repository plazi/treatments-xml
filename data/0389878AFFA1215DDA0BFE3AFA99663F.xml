</taxonomicName>
(her.ba’rum. L. gen. pl. n. 
<taxonomicName id="4C204D1FFFA1215DDA0BFD8FFC0B6056" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[828,932,537,561]" class="Bacilli" family="Lactobacillaceae" genus="Lactiplantibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="44" pageNumber="2826" phylum="Firmicutes" rank="species" species="herbarum">
