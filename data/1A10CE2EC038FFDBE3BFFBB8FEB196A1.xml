<quantity id="5541D2DDC038FFD8E62DFB0BFBB893B3" box="[1063,1131,1191,1218]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="13" pageNumber="14" unit="mm" value="1.0">1 mm</quantity>
