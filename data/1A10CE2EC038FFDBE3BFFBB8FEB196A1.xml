<quantity id="5541D2DDC038FFD8E2B7FB67FF2A9394" box="[189,249,1227,1253]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="13" pageNumber="14" unit="mm" value="7.0">7 µm</quantity>
long; amphidial fovea an inverted U-shape, 
<quantity id="5541D2DDC038FFD8E0FCFB67FC939394" box="[758,832,1227,1253]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="13" pageNumber="14" unit="mm" value="11.0">11 µm</quantity>
