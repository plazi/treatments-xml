; 3 – 
<taxonomicName id="4C364D428C21FFD034E5F9AF80CEF9F3" authority="Nakane, 1989" authorityName="Nakane" authorityYear="1989" box="[478,713,1543,1562]" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="727" phylum="Arthropoda" rank="species" species="kitamurai">
