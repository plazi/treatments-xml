; 4–5 – 
<taxonomicName id="4C364D428C21FFD0363CF9AF8606F9F3" authority="(Miyatake, 1965)" baseAuthorityName="Miyatake" baseAuthorityYear="1965" box="[775,1025,1543,1562]" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="727" phylum="Arthropoda" rank="species" species="shibatai">
