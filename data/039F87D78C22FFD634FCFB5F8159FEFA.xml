<taxonomicName id="4C364D428C21FFD037EFFA4481CFFA16" authority="Murakami, 2016" authorityName="Murakami" authorityYear="2016" box="[724,968,1516,1535]" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="727" phylum="Arthropoda" rank="species" species="yonaguniensis" status="sp. nov.">
