<taxonomicName id="8C21A506C55272E1F8C116D0B1EAB384" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia punctatissima" order="Coleoptera" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="punctatissima">Cephaloleia punctatissima</taxonomicName>
