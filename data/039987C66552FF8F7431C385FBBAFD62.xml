<subSubSection id="C32A655B6551FF8F7401C4EFFBBAFD62" box="[312,1065,684,710]" pageId="22" pageNumber="23" type="etymology">
<paragraph id="8B8F36D06551FF8F7401C4EFFBBAFD62" blockId="22.[264,1324,284,710]" box="[312,1065,684,710]" pageId="22" pageNumber="23">
