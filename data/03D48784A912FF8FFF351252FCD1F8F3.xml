<emphasis id="B909EA80A912FF8FFBEE1089FB67F967" box="[1100,1251,1765,1788]" italics="true" pageId="57" pageNumber="58">E. morimotoi</emphasis>
Imamura, 
<number id="3DB524B5A912FF8FFAC61088FA18F966" box="[1380,1436,1764,1789]" pageId="57" pageNumber="58" value="1958.0">1958</number>
</taxonomicName>
(
<collectingCountry id="F36A7602A912FF8FFF3D1164FF67F8BB" box="[159,227,1800,1824]" name="Japan" pageId="57" pageNumber="58">Japan</collectingCountry>
, only female known) and 
<taxonomicName id="4C7D4D11A912FF8FFDB11165FCF4F8BB" authority="Imamura, 1961" authorityName="Imamura" authorityYear="1961" box="[531,880,1799,1824]" class="Arachnida" family="Aturidae" genus="Erebaxonopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="amamiensis">
<emphasis id="B909EA80A912FF8FFDB11165FD3DF8BB" box="[531,697,1801,1824]" italics="true" pageId="57" pageNumber="58">E. amamiensis</emphasis>
Imamura, 
<number id="3DB524B5A912FF8FFC9A116BFCF4F8BB" box="[824,880,1799,1824]" pageId="57" pageNumber="58" value="1961.0">1961</number>
