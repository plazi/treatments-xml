; 
<taxonomicName id="4C5D4D22FFAFFFF56BC51D98D6C7A6D2" authority="Renaud-Debyser, 1959" authorityName="Renaud-Debyser" authorityYear="1959" class="Heterotardigrada" family="Halechiniscidae" genus="Dipodarctus" kingdom="Animalia" order="Arthrotardigrada" pageId="8" pageNumber="409" phylum="Tardigrada" rank="species" species="subterraneus">
