<taxonomicName id="4C7215C4A77EFF97FED3FF64366E5FBD" authorityName="Abdalla &amp; Mansell &amp; Sole" authorityYear="2019" baseAuthorityName="Tjeder" baseAuthorityYear="1967" box="[329,535,224,251]" class="Insecta" family="Nemopteridae" genus="Afroptera" kingdom="Animalia" order="Neuroptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="dyscrita">
