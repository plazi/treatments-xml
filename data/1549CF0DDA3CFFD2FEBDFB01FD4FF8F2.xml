, 
<typeStatus id="425BC0B9DA3FFFD4FCBFF97DFCBFF949" box="[778,870,1765,1788]" pageId="2" pageNumber="3">paratype</typeStatus>
female, ventral view; 
<number id="2B286C3CDA3FFFD4FBEEF97DFBB3F949" box="[1115,1130,1765,1788]" pageId="2" pageNumber="3" value="2.0">2</number>
, 
