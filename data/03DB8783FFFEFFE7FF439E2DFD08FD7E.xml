<taxonomicName id="4C724D16FFFEFFE7FE849D61FD8AFD37" authorityName="Koch" authorityYear="1939" box="[336,517,619,642]" class="Insecta" family="Staphylinidae" genus="Domene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="461" phylum="Arthropoda" rank="species" species="reitteri">
