, 
<collectingCountry id="668C767CFFE3FF837454FA04FDA8F9C7" box="[554,639,1530,1555]" name="Taiwan" pageId="10" pageNumber="111">Taiwan</collectingCountry>
, 
<collectingCountry id="668C767CFFE3FF8374F4FA04FD37F9C7" box="[650,736,1530,1555]" name="Tunisia" pageId="10" pageNumber="111">Tunisia</collectingCountry>
, 
<collectingCountry id="668C767CFFE3FF837495FA04FCEBF9C7" box="[747,828,1530,1555]" name="Turkey" pageId="10" pageNumber="111">Turkey</collectingCountry>
