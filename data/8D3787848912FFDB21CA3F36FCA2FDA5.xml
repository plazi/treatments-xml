<collectingRegion id="C75AF8708912FFC425A33E1EFAFBFA98" box="[1278,1351,1354,1378]" country="New Zealand" name="Otago" pageId="15" pageNumber="16">Otago</collectingRegion>
: Symes Road at Old Man Range, 
<locationDeviation id="14932FB08912FFC420933E39FD17FA7F" box="[462,683,1389,1414]" pageId="15" pageNumber="16">
<quantity id="C2669B778912FFC420933E39FDB1FA7C" box="[462,525,1389,1414]" metricMagnitude="4" metricUnit="m" metricValue="1.0" pageId="15" pageNumber="16" unit="km" value="10.0">10km</quantity>
S Beaumont
</locationDeviation>
, 
<location id="004160498912FFC423E63E39FC4CFA7C" LSID="urn:lsid:plazi:treatment:8D3787848912FFDB21CA3F36FCA2FDA5:004160498912FFC423E63E39FC4CFA7C" box="[699,1008,1389,1414]" name="Flagstaff-Whare Flat Road" pageId="15" pageNumber="16" stateProvince="Otago">Flagstaff-Whare Flat Road</location>
