<materialsCitation id="B5F63CCF8912FFC421CA3E1DFEEAFA50" ID-GBIF-Occurrence="1341350632" location="Flagstaff-Whare Flat Road" pageId="15" pageNumber="16" specimenCount="1" stateProvince="Otago">
