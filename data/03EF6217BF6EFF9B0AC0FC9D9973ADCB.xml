</emphasis>
: 
<collectingRegion id="49821DE3BF60FF9B0B20F9F79E5AADE0" box="[375,513,1663,1687]" country="Madagascar" name="Antsiranana" pageId="89" pageNumber="90">Antsiranana</collectingRegion>
, Parc National de Marojejy, Antranohofa, 
<quantity id="4CBE7EE4BF60FF9B09A5F9F79809ADE0" box="[1010,1106,1663,1687]" metricMagnitude="4" metricUnit="m" metricValue="2.66" pageId="89" pageNumber="90" unit="km" value="26.6">26.6 km</quantity>
31° NNE Andapa, 
<quantity id="4CBE7EE4BF60FF9B0F6BF9F799C7ADE0" box="[1340,1436,1663,1687]" metricMagnitude="4" metricUnit="m" metricValue="1.0699999999999998" pageId="89" pageNumber="90" unit="km" value="10.7">10.7 km</quantity>
318° NW Manantenina, 
<geoCoordinate id="EE72B5C6BF60FF9B0BFEF92C9E72ADCB" box="[425,553,1700,1724]" degrees="14.44333" direction="south" orientation="latitude" pageId="89" pageNumber="90" precision="1" value="-14.44333">14.44333 S</geoCoordinate>
