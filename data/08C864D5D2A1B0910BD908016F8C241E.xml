<taxonomicName id="FDD85583CFBBCACBE7BBDF65E493A1B9" class="Insecta" family="Curculionidae" genus="Parallelodemas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parallelodemas" order="Coleoptera" pageId="6" pageNumber="111" phylum="Arthropoda" rank="genus">Parallelodemas</taxonomicName>
