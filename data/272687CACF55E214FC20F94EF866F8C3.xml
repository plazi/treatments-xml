<taxonomicName id="688F4D5FCF54E214FCB7F93EFF95F921" authorityName="Petersen" authorityYear="1962" box="[831,921,1728,1751]" class="Hydrozoa" family="Eirenidae" genus="Neotima" kingdom="Animalia" order="Leptothecata" pageId="11" pageNumber="362" phylum="Cnidaria" rank="genus">
