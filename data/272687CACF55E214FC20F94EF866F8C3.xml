<taxonomicName id="688F4D5FCF54E214FCEEF900F860F8E1" authority="Haeckel, 1879" authorityName="Haeckel" authorityYear="1879" box="[870,1132,1790,1815]" class="Hydrozoa" family="Eirenidae" kingdom="Animalia" order="Leptothecata" pageId="11" pageNumber="362" phylum="Cnidaria" rank="family">
