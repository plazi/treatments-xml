<number id="3D998A70AD55FFEE4600FE0AEBC3DECA" box="[633,670,429,454]" pageId="24" pageNumber="25" value="4.0" valueMax="6.0" valueMin="2.0">2-6</number>
: 45, 46, 48, 66, 
<number id="3D998A70AD55FFEE472DFE09EA2EDECA" box="[852,883,430,454]" pageId="24" pageNumber="25" value="82.0">82</number>
; I-L- 
<number id="3D998A70AD55FFEE47D7FE0AEAE6DECA" box="[942,955,429,454]" pageId="24" pageNumber="25" value="6.0">6</number>
H 
<number id="3D998A70AD55FFEE47A6FE09EAA0DECA" box="[991,1021,430,454]" pageId="24" pageNumber="25" value="34.0">34</number>
, I-L- 
<number id="3D998A70AD55FFEE404EFE0AED19DECA" box="[1079,1092,429,454]" pageId="24" pageNumber="25" value="6.0">6</number>
L/H ratio 
<number id="3D998A70AD55FFEE40C5FE09EDBFDECA" box="[1212,1250,430,454]" pageId="24" pageNumber="25" value="2.4">2.4</number>
