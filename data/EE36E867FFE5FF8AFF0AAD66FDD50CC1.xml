<elevation id="EDB2BE42FFE5FF8BFF0AACFBFEF209C7" box="[151,280,1929,1953]" metricMagnitude="2" metricUnit="m" metricValue="8.965" metricValueMax="9.24" metricValueMin="8.69" pageId="96" pageNumber="97" unit="m" value="896.5" valueMax="924.0" valueMin="869.0">869–924 m</elevation>
