<specimenCount id="9D7373C3FFC83310CB94E4398546FE96" box="[475,607,261,287]" pageId="18" pageNumber="19" type="female">one female</specimenCount>
and 
<specimenCount id="9D7373C3FFC83310C8EFE4398416FE96" box="[672,783,261,287]" pageId="18" pageNumber="19" type="male">two male</specimenCount>
adults of 
<taxonomicName id="4C75C3C9FFC83310C9C1E43984EBFE97" authorityName="Melika &amp; Pujade-Villar &amp; Abe &amp; Tang &amp; Nicholls &amp; Wachi &amp; Ide &amp; Yang &amp; Pénzes &amp; Csóka &amp; Stone" authorityYear="2010" baseAuthorityName="Monzen" baseAuthorityYear="1954" box="[910,1010,261,286]" class="Insecta" family="Cynipidae" genus="Latuspina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="stirps">
