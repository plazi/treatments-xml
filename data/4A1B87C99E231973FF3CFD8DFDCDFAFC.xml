<quantity id="054A9B3A9E231972FF3CFEDBFF58F925" box="[151,238,1746,1770]" metricMagnitude="3" metricUnit="m" metricValue="2.4" pageId="48" pageNumber="49" unit="m" value="2400.0">
<elevation id="499FD1EC9E231972FF3CFEDBFF58F925" box="[151,238,1746,1770]" metricMagnitude="3" metricUnit="m" metricValue="2.4" pageId="48" pageNumber="49" unit="m" value="2400.0">2400 m</elevation>
