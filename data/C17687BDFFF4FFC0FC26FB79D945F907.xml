, 
<quantity id="8E279B4EFFEBFFC1FDC4F950D823F8E3" box="[528,609,1779,1803]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.7" metricValueMin="0.4" pageId="48" pageNumber="273" unit="m" value="10.5" valueMax="17.0" valueMin="4.0">4-17 m</quantity>
