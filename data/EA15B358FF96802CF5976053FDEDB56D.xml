<taxonomicName id="A5BC79CDFF96802EF597609BFE78B7B2" authority="Yazaki, 1992" authorityName="Yazaki" authorityYear="1992" box="[151,410,940,962]" class="Insecta" family="Geometridae" genus="Metaterpna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="502" phylum="Arthropoda" rank="genus">
