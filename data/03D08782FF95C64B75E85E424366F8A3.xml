<taxonomicName id="4C794D17FF96C64B77EB597B4141FE76" box="[227,347,1120,1142]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="58" pageNumber="58" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B90DEA86FF96C64B77EB597B412BFE75" box="[227,305,1120,1141]" italics="true" pageId="58" pageNumber="58">Acacia</emphasis>
sp.
</taxonomicName>
(
<taxonomicName id="4C794D17FF96C64B7664597B41FAFE76" box="[364,480,1120,1142]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="58" pageNumber="58" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
