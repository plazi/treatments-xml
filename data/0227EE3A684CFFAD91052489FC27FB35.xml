<taxonomicName id="4D8E24AF684CFFAD91052489FB3CFE1D" authorityName="HEER" authorityYear="1881" box="[1066,1220,475,499]" class="Pinopsida" family="Araucariaceae" genus="Pagiophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="4" pageNumber="321" phylum="Tracheophyta" rank="genus">
<emphasis id="B8FA833E684CFFAD91052489FB3CFE1D" bold="true" box="[1066,1220,475,499]" italics="true" pageId="4" pageNumber="321">Pagiophyllum</emphasis>
