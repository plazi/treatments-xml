<taxonomicName id="4C7E4D27564B7216FE6B3113C95DF9B9" box="[452,584,1575,1598]" class="Arachnida" family="Salticidae" genus="Goleba" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="lyra">
