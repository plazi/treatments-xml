, 
<number id="3DA3A959FF93FFC4542AF8D3FE6DF870" box="[346,398,1915,1940]" pageId="3" pageNumber="486" value="2.66">2.66</number>
, 
<number id="3DA3A959FF93FFC454EAF8D4FE2DF870" box="[410,462,1916,1940]" pageId="3" pageNumber="486" value="1.88">1.88</number>
, 
<number id="3DA3A959FF93FFC454ABF8D4FDECF870" box="[475,527,1916,1940]" pageId="3" pageNumber="486" value="1.38">1.38</number>
), II 
<number id="3DA3A959FF93FFC4574FF8D4FD93F870" box="[575,624,1916,1940]" pageId="3" pageNumber="486" value="7.33">7.33</number>
