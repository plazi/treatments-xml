<taxonomicName id="4C060095A17490133CEA0F34B563CB7B" baseAuthorityName="Meyrick" baseAuthorityYear="1930" box="[722,892,1047,1071]" class="Insecta" family="Pterophoridae" genus="Michaelophorus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="indentatus">
<emphasis id="B972A704A17490133CEA0F34B563CB7B" box="[722,892,1047,1071]" italics="true" pageId="8" pageNumber="7">M. indentatus</emphasis>
