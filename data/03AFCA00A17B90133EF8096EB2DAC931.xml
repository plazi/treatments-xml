<taxonomicName id="4C060095A17490133F410F14B40FCB1B" authority="Raf." authorityName="Raf." box="[377,528,1079,1103]" class="Magnoliopsida" family="Fabaceae" genus="Delonix" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="genus">
<emphasis id="B972A704A17490133F410F14B7C9CB1B" box="[377,470,1079,1103]" italics="true" pageId="8" pageNumber="7">Delonix</emphasis>
Raf.
</taxonomicName>
(
<taxonomicName id="4C060095A17490133C270F14B48CCB1B" box="[543,659,1079,1103]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
