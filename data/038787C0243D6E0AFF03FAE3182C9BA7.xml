</emphasis>
<taxonomicName id="4C2E4D55243D6E0AFEBEFAC219B89887" authority="Dendy, 1922: 132" authorityName="Dendy" authorityPageNumber="132" authorityYear="1922" box="[298,679,1401,1425]" class="Demospongiae" family="Mycalidae" genus="Barbozia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poecilosclerida" pageId="0" pageNumber="596" phylum="Porifera" rank="species" species="primitiva">
