° 
<number id="AD1024FFFF93FF8DFDAFFB7F8C296565" box="[576,608,1275,1299]" pageId="38" pageNumber="239" value="27.0">27</number>
'0&quot;N
</geoCoordinate>
; 
<geoCoordinate id="7EEC501FFF93FF8DFD4FFB7F8D406565" box="[672,777,1275,1299]" direction="east" orientation="longitude" pageId="38" pageNumber="239" precision="15" value="7.65">
