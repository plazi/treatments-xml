<taxonomicName id="4C144D736A34FE90AC94FFEDFB493AD3" authority="(Brenske, 1894)" baseAuthorityName="Brenske" baseAuthorityYear="1894" box="[906,1175,1927,1951]" class="Insecta" family="Melolonthidae" genus="Autoserica" kingdom="Animalia" order="Coleoptera" pageId="341" pageNumber="342" phylum="Arthropoda" rank="species" species="exigua">
