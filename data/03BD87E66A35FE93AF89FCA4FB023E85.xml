<taxonomicName id="4C144D736A34FE90AE7DFFEDFD123AD3" authority="(Brenske, 1898)" baseAuthorityName="Brenske" baseAuthorityYear="1898" box="[355,716,1927,1951]" class="Insecta" family="Melolonthidae" genus="Maladera" kingdom="Animalia" order="Coleoptera" pageId="341" pageNumber="342" phylum="Arthropoda" rank="species" species="flammea">
