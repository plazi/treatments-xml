<taxonomicName id="4C714D59FFE3FFAB2159FE37FF04FE31" authority="Lacepède, 1788" authorityName="Lacepède" authorityYear="1788" class="Reptilia" family="Lacertidae" genus="Lacertus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="aquatilis">
