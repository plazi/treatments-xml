<taxonomicName id="4C714D59FFE3FFAB2671FE51FD2CFE31" authority="Lacepède, 1788" authorityName="Lacepède" authorityYear="1788" box="[541,726,454,479]" class="Reptilia" family="Lacertidae" genus="Lacertus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="viridis">
