, 
<collectingRegion id="49D8F824D91F4D67029FE68EFA2BDEA2" box="[1208,1432,655,681]" country="Russia" name="Kaliningrad" pageId="15" pageNumber="157">Kaliningrad Region</collectingRegion>
, 
<collectingCountry id="F30B7656D91F4D6706B0E6B2FF56DEC6" box="[151,229,691,717]" name="Russia" pageId="15" pageNumber="157">Russia</collectingCountry>
