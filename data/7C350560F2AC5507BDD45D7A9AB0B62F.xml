<taxonomicName id="4C21B0E999B3FD0FCCA94AFF72382BCB" class="Dicotyledoneae" family="Onagraceae" genus="Oenothera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Myrtiflorae" pageId="2" pageNumber="17" phylum="Angiospermae" rank="species" species="fruticosa">
