d. 
<taxonomicName id="BC9B53E7FFCDAE4BF1F7C4F6D716F89B" authorityName="Say" authorityYear="1817" box="[454,593,1784,1805]" class="Insecta" family="Carabidae" genus="Cicindela" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="species" species="decemnotata">decemnotata</taxonomicName>
