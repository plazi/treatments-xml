<subSubSection id="C3657AE64479B124FFFE1363FF5BE6AC" pageId="8" pageNumber="573" type="materials_examined">
<paragraph id="8BC0296D4479B124FFFE1363FF78E630" blockId="8.[113,646,780,831]" pageId="8" pageNumber="573">
<collectionCode id="ED6EB1A84479B124FFFE1363FF6EE62D" box="[113,239,780,803]" pageId="8" pageNumber="573">
<typeStatus id="54C497CF4479B124FFFE1363FF23E62E" box="[113,162,780,803]" pageId="8" pageNumber="573">TYPE</typeStatus>
SPECIES
</collectionCode>
. — 
<taxonomicName id="4C7F52EE4479B124FE951363FD07E62B" authority="(Linnaeus, 1766)" baseAuthorityName="Linnaeus" baseAuthorityYear="1766" box="[282,646,780,804]" class="Reptilia" family="Anguidae" genus="Ophisaurus" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="573" phylum="Chordata" rank="species" species="ventralis">
