<materialsCitation id="1DAD1323FFA3FFEAFA84BD76FE86FEAD" collectionCode="IV" collectorName="St" pageId="57" pageNumber="58" specimenCount="1">
off shield, 
<collectorName id="00307CA8FFA3FFEAFF1FBD55FF59FEAD" box="[231,254,295,319]" pageId="57" pageNumber="58">St</collectorName>
<collectionCode id="CBD481BBFFA3FFEAFEFCBD5AFE86FEAD" box="[260,289,296,319]" pageId="57" pageNumber="58">IV</collectionCode>
</materialsCitation>
<materialsCitation id="1DAD1323FFA3FFEAFED0BD5AFCBEFE5D" collectionCode="IV" pageId="57" pageNumber="58" specimenCode="IV 37" specimenCount="1">
