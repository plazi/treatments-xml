<number id="3DB6E839ED1EFFD89BE94FBEEB639C37" box="[226,365,246,268]" pageId="4" pageNumber="171" value="215354.5" valueMax="430384.0" valueMin="325.0">325–430, 384</number>
<number id="3DB6E839ED1EFFD89A964FBEE8279C37" box="[413,553,246,268]" pageId="4" pageNumber="171" value="265451.0" valueMax="530492.0" valueMin="410.0">410–530, 492</number>
<number id="3DB6E839ED1EFFD8997B4FBEE8F59C37" box="[624,763,246,268]" pageId="4" pageNumber="171" value="140223.0" valueMax="280246.0" valueMin="200.0">200–280, 246</number>
