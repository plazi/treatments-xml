(
<bibRefCitation id="EFD1AF8E4356C95427A123CFFD14FC4F" author="Klapalek" box="[467,648,981,1006]" pageId="49" pageNumber="50" refString="Klapalek, F. (1921) Plecopteres nouveaux. I. Sous-familie des Perlinae et Neoperlinae. Annales de la Societe Entomologique de Belgique, 61, 57 - 67." type="journal article" year="1921">
