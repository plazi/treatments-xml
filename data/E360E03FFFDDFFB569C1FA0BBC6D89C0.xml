<materialsCitation id="DBA15B74FFDDFFB569E3FAB2BC7189C0" collectingDate="1995-07-24" collectingDateMax="2016-11-17" collectingDateMin="1995-07-24" collectionCode="VS, MHNG" collectorName="Chittaro Y. &amp; Besuchet C." country="Switzerland" determinerName="Sanchez A." elevation="610" location="Suisse" pageId="4" pageNumber="49" specimenCount="1">
