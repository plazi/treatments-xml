<taxonomicName id="4C47D3B2C507140AFEDBF9EBFCE51868" authority="Pall-Gergely, A. Reischutz &amp; Maassen" authorityName="Pall-Gergely, A. Reischutz &amp; Maassen in Pall-Gergely, A. Reischutz, Maassen, Grego &amp; Hunyadi" authorityYear="2020" box="[257,883,1579,1601]" class="Gastropoda" family="Streptaxidae" genus="Sinoennea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="3" pageNumber="4" phylum="Mollusca" rank="species" species="angustistoma" status="new species">
