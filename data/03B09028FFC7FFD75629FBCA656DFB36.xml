<paragraph id="8BA6213EFFC7FFD65629FBA9619AFBB9" blockId="24.[264,509,1084,1145]" box="[264,509,1119,1145]" pageId="24" pageNumber="25">
(
<figureCitation id="13223DBBFFC7FFD65630FBA96116FBB9" box="[273,369,1119,1145]" captionStart="FIGURES 10 – 12" captionStartId="44.[264,382,1768,1792]" captionTargetBox="[312,1196,323,1702]" captionTargetId="figure@44.[312,1289,302,1764]" captionTargetPageId="44" captionText="FIGURES 10 – 12. Wing. — 10. Carpophthoromyia interrupta. — 11. C. schoutedeni. — 12. C. tritea." httpUri="https://zenodo.org/record/172784/files/figure.png" pageId="24" pageNumber="25">Figs. 11</figureCitation>
, 
<figureCitation id="13223DBBFFC7FFD6565FFBA961FAFBB9" box="[382,413,1119,1145]" captionStart="FIGURES 13 – 22" captionStartId="45.[264,382,1790,1814]" captionTargetBox="[266,1276,321,1790]" captionTargetId="figure@45.[253,1307,305,1790]" captionTargetPageId="45" captionText="FIGURES 13 – 22. Aculeus full length. — 13. Carpophthoromyia flavofasciata. — 14. C. radulata. — 15. C. tritea. — 16. C. interrupta. — 17. C. schoutedeni. — 18. C. dividua. — 19. C. tessmanni. — 20. C. pseudotritea. — 21. C. virgata. — 22. C. vittata." httpUri="https://zenodo.org/record/172785/files/figure.png" pageId="24" pageNumber="25">17</figureCitation>
, 
<figureCitation id="13223DBBFFC7FFD6568BFBA961ADFBB9" box="[426,458,1119,1145]" captionStart="FIGURES 28 – 33" captionStartId="46.[264,382,1845,1869]" captionTargetBox="[298,1244,875,1844]" captionTargetId="figure@46.[297,1289,864,1844]" captionTargetPageId="46" captionText="FIGURES 28 – 33. Apical part of aculeus. — 28. Carpophthoromyia flavofasciata. — 29. C. radulata. — 30. C. virgata. — 31. C. interrupta. — 32. C. schoutedeni. — 33. C. tritea." httpUri="https://zenodo.org/record/172787/files/figure.png" pageId="24" pageNumber="25">32</figureCitation>
, 
<figureCitation id="13223DBBFFC7FFD656F7FBA96192FBB9" box="[470,501,1119,1145]" captionStart="FIGURES 36 – 38" captionStartId="47.[264,382,1339,1363]" captionTargetBox="[319,1260,321,1230]" captionTargetId="figure@47.[283,1393,310,1264]" captionTargetPageId="47" captionText="FIGURES 36 – 38. Spermatheca. — 36. Carpophthoromyia dimidiata. — 37. C. schoutedeni. — 38. C. interrupta." httpUri="https://zenodo.org/record/172788/files/figure.png" pageId="24" pageNumber="25">37</figureCitation>
