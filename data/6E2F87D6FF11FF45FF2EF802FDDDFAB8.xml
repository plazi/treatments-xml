Red Sea, Indo-West Pacific: East Africa, 
<collectingCountry id="9E917650FF16FF45FC8DFF35FC34FBEC" box="[820,965,225,251]" name="South Africa" pageId="148" pageNumber="149">South Africa</collectingCountry>
, 
<collectingCountry id="9E917650FF16FF45FC69FF35FBD9FBEC" box="[976,1064,225,251]" name="Yemen" pageId="148" pageNumber="149">Socotra</collectingCountry>
, 
<collectingCountry id="9E917650FF16FF45FB8DFF35FB5EFBEC" box="[1076,1199,225,251]" name="Seychelles" pageId="148" pageNumber="149">Seychelles</collectingCountry>
