<taxonomicName id="D7F64D08DF1B725AC079F9F7FDB17185" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[563,593,1662,1686]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="121" pageNumber="122" phylum="Arthropoda" rank="species" species="fio">
