: 
<taxonomicName id="6A2C8CD2CB82BE0E8999064A59D512B5" lsidName="" order="Coleoptera" pageId="0" pageNumber="1042" rank="order">Coleoptera</taxonomicName>
: 
<taxonomicName id="31B7FEAA48B8A435C9B90478DF0473F9" family="Coccinellidae" lsidName="" pageId="0" pageNumber="1042" rank="family">Coccinellidae</taxonomicName>
: 
<taxonomicName id="42D4E0BB9CC609D6F4BA611F1E2E72C8" authority="Casey, 1908" authorityName="Casey" authorityYear="1908" class="Insecta" family="Coccinellidae" genus="Anegleis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Anegleis cardoni" order="Coleoptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="cardoni">Anegleis cardoni</taxonomicName>
(Weise); 
<taxonomicName id="94D6CA6AEFB2E57FCE09F35BDBAA87AC" class="Insecta" family="Coccinellidae" genus="Hyperaspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperaspis maindronia" order="Coleoptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="maindronia">Hyperaspis maindronia</taxonomicName>
; 
<taxonomicName id="E12E8B9678278000E9669C166859A0E9" class="Insecta" family="Coccinellidae" genus="Nephus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nephus regularis" order="Coleoptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="regularis">Nephus regularis</taxonomicName>
; 
<taxonomicName id="5CAAF080253FCF1558E8EB52D690DCD3" lsidName="" order="Lepidoptera" pageId="0" pageNumber="1042" rank="order">Lepidoptera</taxonomicName>
