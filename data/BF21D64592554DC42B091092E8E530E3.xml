<taxonomicName id="9104B754CD965608A3AE3D0B8E9F98CF" class="Insecta" family="Pseudococcidae" genus="Coccidohystrix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coccidohystrix insolita" order="Hemiptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="insolita">Coccidohystrix insolita</taxonomicName>
