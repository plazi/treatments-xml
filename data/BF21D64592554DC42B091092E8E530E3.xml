, 
<taxonomicName id="FBED4A6FF335C30A0267706E2072D67C" family="Moraceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Moraceae</taxonomicName>
, 
<taxonomicName id="F3E99E268D81D0BE3571B1CB13CDB689" family="Poaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Poaceae</taxonomicName>
, 
<taxonomicName id="E3BA584DA89ECE3245FF78CB545C30AA" family="Rhamnaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Rhamnaceae</taxonomicName>
