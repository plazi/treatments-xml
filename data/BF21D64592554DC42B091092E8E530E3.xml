<taxonomicName id="5AC0C564330EFA72B31D2DC3491CC8B6" family="Acanthaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Acanthaceae</taxonomicName>
, 
<taxonomicName id="73012E94629B897F8A8F639B1C2837FC" family="Amaranthaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Amaranthaceae</taxonomicName>
, 
<taxonomicName id="36ECBA7C513F584F370D4E008EBEFC3F" family="Apocynaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Apocynaceae</taxonomicName>
, 
<taxonomicName id="428D99F6A02971BCBFEAAEA814894F42" family="Araceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Araceae</taxonomicName>
, 
<taxonomicName id="6E8D8246E0E8D5743FB103B423A1053F" family="Arecaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Arecaceae</taxonomicName>
, 
<taxonomicName id="728FC804C36A177EB7C855DB67BED3BA" family="Aristolochiaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Aristolochiaceae</taxonomicName>
, 
<taxonomicName id="BC853705C8F48CC6B90063951A79C543" family="Asteraceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Asteraceae</taxonomicName>
, 
<taxonomicName id="FD0D2747FC1984A86137A944007E5A46" family="Chenopodiaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Chenopodiaceae</taxonomicName>
, 
<taxonomicName id="E8CB4ED92DB90F792DEEC65D8BCED6CF" family="Cucurbitaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Cucurbitaceae</taxonomicName>
, 
<taxonomicName id="AEC3D0C636FAD20F23E6BF95A2F33D5C" family="Euphorbiaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Euphorbiaceae</taxonomicName>
, 
<taxonomicName id="A3B208FDC03F664FCBB59E098D3D769F" family="Fabaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Fabaceae</taxonomicName>
, 
<taxonomicName id="A268953F0D23C8B48365EB6683FD264C" family="Malvaceae" lsidName="" pageId="0" pageNumber="1042" rank="family">Malvaceae</taxonomicName>
, 
