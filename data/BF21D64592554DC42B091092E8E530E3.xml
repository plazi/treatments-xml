<taxonomicName id="CD1AC32D20A538306F572863F20CA690" class="Insecta" family="Pseudococcidae" genus="Coccidohystrix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coccidohystrix insolita" order="Hemiptera" pageId="0" pageNumber="1042" phylum="Arthropoda" rank="species" species="insolita">Coccidohystrix insolita</taxonomicName>
: 
<taxonomicName id="4657EC5E79DA82A26ED1FE23890DFE26" kingdom="Fungi" lsidName="" pageId="0" pageNumber="1042" rank="kingdom">Fungi</taxonomicName>
: 
<taxonomicName id="36712967B30E268BF141454291DD9090" class="Sordariomycetes" family="Clavicipitaceae" genus="Metarhizium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Metarhizium anisopliae" order="Hypocreales" pageId="0" pageNumber="1042" phylum="Ascomycota" rank="species" species="anisopliae">Metarhizium anisopliae</taxonomicName>
; 
