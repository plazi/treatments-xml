<subSubSection id="C32393C7FFBCEB0EFC9D89C644A3FB50" pageId="24" pageNumber="1138" type="etymology">
<paragraph id="8B86C04CFFBCEB0EFC9D89C644A3FB50" blockId="24.[827,1443,924,1222]" pageId="24" pageNumber="1138">
<emphasis id="B94D1C5EFFBCEB0EFC9D89C643ACFC27" box="[827,955,924,945]" italics="true" pageId="24" pageNumber="1138">Etymology:</emphasis>
