<subSubSection id="C30A652B9653FFE030A3FC046D2B6497" pageId="8" pageNumber="43" type="materials_examined">
<paragraph id="8BAF36A09653FFE030A3FC046D106394" blockId="8.[151,1436,151,1220]" pageId="8" pageNumber="43">
