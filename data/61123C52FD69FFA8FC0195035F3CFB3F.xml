<taxonomicName id="2EBBF6C7FD69FFA9FC0195035B1FFCDB" authority="Jankowski, 1986" authorityName="Jankowski" authorityYear="1986" box="[979,1327,772,796]" class="Oligohymenophorea" family="Operculariidae" genus="Setonophrys" kingdom="Protozoa" order="Peritrichida" pageId="8" pageNumber="161" phylum="Ciliophora" rank="genus">
