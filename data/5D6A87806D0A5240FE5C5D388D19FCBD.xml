<quantity id="123B9B736D0B5246FD705D688ED3F9AA" box="[657,752,1631,1655]" metricMagnitude="-3" metricUnit="m" metricValue="2.69" pageId="27" pageNumber="1198" unit="mm" value="2.69">2.69 mm</quantity>
