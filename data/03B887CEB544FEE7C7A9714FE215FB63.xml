<taxonomicName id="4C114D5BB544FED8C7E87238E18CF9F0" authorityName="PLATNICK" authorityYear="2000" box="[670,759,1623,1646]" class="Arachnida" family="Lamponidae" genus="Notsodipus" kingdom="Animalia" order="Araneae" pageId="287" pageNumber="288" phylum="Arthropoda" rank="species" species="visio">
