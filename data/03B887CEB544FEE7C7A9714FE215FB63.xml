<taxonomicName id="4C114D5BB544FED8C67C72A3E011F97D" authorityName="PLATNICK" authorityYear="2000" box="[778,874,1740,1763]" class="Arachnida" family="Lamponidae" genus="Notsodipus" kingdom="Animalia" order="Araneae" pageId="287" pageNumber="288" phylum="Arthropoda" rank="species" species="visio">
