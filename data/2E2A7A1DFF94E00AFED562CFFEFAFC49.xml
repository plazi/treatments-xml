<taxonomicName id="6183B088FF95E00AFD9563DEFCFDFCD2" box="[584,820,764,789]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="cenae">
