<emphasis id="B969EAFBFF8CFFE6FC59B797FB7CA693" bold="true" box="[937,1262,2465,2497]" italics="true" pageId="18" pageNumber="355">
<taxonomicName id="4C1D4D6AFF8CFFE6FC59B797FB77A693" authorityName="M.A. Lieftinck" authorityYear="1940" box="[937,1253,2465,2497]" class="Insecta" family="Platycnemididae" genus="Coeliccia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="18" pageNumber="355" phylum="Arthropoda" rank="species" species="coomansi" status="sp. n.">Coeliccia coomansi</taxonomicName>
