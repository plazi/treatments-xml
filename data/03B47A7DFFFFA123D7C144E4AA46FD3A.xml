. 
<taxonomicName id="4C1DB0E8FFFEA12DD60A46E7ABE3F822" authority="Diskus &amp; Stonis" authorityName="Diskus &amp; Stonis" box="[348,718,1927,1949]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="108" phylum="Arthropoda" rank="species" species="bipartita" status="sp. nov.">
<emphasis id="B9691779FFFEA12DD60A46E7AB31F823" box="[348,540,1927,1948]" italics="true" pageId="7" pageNumber="108">Stigmella bipartita</emphasis>
Diškus &amp; Stonis
</taxonomicName>
, 
<emphasis id="B9691779FFFEA12DD58F46E7AA06F823" bold="true" box="[729,811,1927,1948]" pageId="7" pageNumber="108">
