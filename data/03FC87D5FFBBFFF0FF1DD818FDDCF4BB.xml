<taxonomicName id="4C554D40FFBBFFF0FF1DD818FFE8F59B" authority="Bagnall" authorityName="Bagnall" authorityYear="1932" box="[151,424,618,645]" class="Insecta" family="Thripidae" genus="Hercinothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="216" phylum="Arthropoda" rank="genus">
