<emphasis id="B93EEAE3E75D2305FF7AFB2EFF4AFB23" bold="true" box="[151,221,1152,1174]" pageId="19" pageNumber="20">45–46.</emphasis>
<taxonomicName id="4C4A4D72E75D2305FF0FFB2FFD89FB20" authority="Townsend" authorityName="Townsend" box="[226,542,1152,1174]" class="Insecta" family="Sarcophagidae" genus="Argoravinia" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="aurea">
