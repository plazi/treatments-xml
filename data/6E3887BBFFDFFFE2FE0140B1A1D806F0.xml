<taxonomicName id="21914D2EFFDFFFE2FEB4419AA1D406F0" authority="(Thunberg, 1794)" baseAuthorityName="Thunberg" baseAuthorityYear="1794" box="[355,743,909,934]" class="Insecta" family="Kateretidae" genus="Kateretes" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="231" phylum="Arthropoda" rank="species" species="pusillus">
