<emphasis id="98DBF9547810FF64CE73F95CFB02CAC7" box="[851,1212,1758,1779]" italics="true" pageId="110">
<taxonomicName id="6DAF5EC57810FF64CE73F95CFB06CAC7" authority="ENGEL, 2001" authorityName="Engel" authorityYear="2001" box="[851,1208,1758,1779]" class="Insecta" family="Apidae" genus="Thaumastobombus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="110" phylum="Arthropoda" rank="species" species="andreniformis" status="new species">Thaumastobombus andreniformis</taxonomicName>
