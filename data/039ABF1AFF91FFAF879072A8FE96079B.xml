<taxonomicName id="4C33758FFF91FFAF86A27322FDAD0751" baseAuthorityName="Leach" baseAuthorityYear="1815" box="[384,521,339,361]" class="Insecta" family="Geometridae" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="1312" phylum="Arthropoda" rank="family">Geometridae</taxonomicName>
