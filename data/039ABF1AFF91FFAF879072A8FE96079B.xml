<taxonomicName id="4C33758FFF91FFAF854A7344FC5C0773" authority="(DE VILLERS)" baseAuthorityName="DE VILLERS" box="[616,1016,309,331]" class="Insecta" family="Geometridae" genus="Nychiodes" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="1312" phylum="Arthropoda" rank="species" species="obscuraria">
