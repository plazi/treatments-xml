<taxonomicName id="2187F5ED74B35F74FA0AAE5379EAFF2C" class="Insecta" family="Eumenidae" genus="Zethus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zethus (Calligaster) cyanopterus" order="Hymenoptera" pageId="10" pageNumber="29" phylum="Arthropoda" rank="species" species="cyanopterus" subGenus="Calligaster">
