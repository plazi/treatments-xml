<taxonomicName id="4C180B85FF9DFF9DFDF89F2ECD83F98C" box="[593,752,1556,1580]" class="Insecta" family="Tortricidae" genus="Loboschiza" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="87" phylum="Arthropoda" rank="species" species="subrectangula">
