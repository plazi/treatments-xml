<emphasis id="36CE3A1F82145D78FEFBFEB37583A968" bold="true" box="[267,414,349,373]" pageId="10" pageNumber="213">Distribution.</emphasis>
<taxonomicName id="C3BA9D8E82145D78FE53FEB07637A968" box="[419,554,349,373]" class="Insecta" family="Carabidae" genus="Nebria" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="213" phylum="Arthropoda" rank="species" species="hiekeiana" status="n. sp.">
<emphasis id="36CE3A1F82145D78FE53FEB07637A968" box="[419,554,349,373]" italics="true" pageId="10" pageNumber="213">N. hiekeiana</emphasis>
</taxonomicName>
<taxonomicNameLabel id="2DFD876482145D78FDC0FEB07674A968" box="[560,617,350,373]" pageId="10" pageNumber="213" rank="species">n. sp.</taxonomicNameLabel>
