<emphasis id="2A8F795D8554FFAAFE8C2FC917C8F967" bold="true" box="[264,702,1753,1779]" pageId="6" pageNumber="7">
Genus 
<taxonomicName id="DFFBDECC8554FFAAFEDB2FC917C8F967" authority="Lafresnaye, 1841" authorityName="Lafresnaye" authorityYear="1841" box="[351,702,1753,1779]" class="Aves" family="Rallidae" genus="Gallirallus" kingdom="Animalia" order="Gruiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">
<emphasis id="2A8F795D8554FFAAFEDB2FC91497F967" bold="true" box="[351,481,1753,1779]" italics="true" pageId="6" pageNumber="7">Gallirallus</emphasis>
<bibRefCitation id="7C6AD8BE8554FFAAFE6D2FC917C8F967" author="Lafresnaye" box="[489,702,1753,1779]" pageId="6" pageNumber="31" refString="Lafresnaye, F. (1841) Revue Zoologique, par la Societe Cuvierienne, IV. F. E. Guerin-Meneville, Paris." type="book" year="1841">
Lafresnaye, 
<number id="AE33B7688554FFAAFD072FC917C8F967" box="[643,702,1753,1779]" pageId="6" pageNumber="7" value="1841.0">1841</number>
