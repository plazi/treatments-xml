<paragraph id="352E137DFFCFAC58FF27FBD3FF7884AD" blockId="2.[151,566,1150,1172]" box="[151,566,1150,1172]" pageId="2" pageNumber="403">
<taxonomicName id="F29168FEFFCFAC58FF27FBD3FCA384AD" authority="Distant, 1893" authorityName="Distant" authorityYear="1893" box="[151,493,1150,1172]" class="Insecta" family="Cydnidae" genus="Amnestus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="403" phylum="Arthropoda" rank="species" species="bergrothi">
