<emphasis id="B907D766FF91EA2E6158A3A8FE18F916" bold="true" box="[264,410,1712,1736]" pageId="3" pageNumber="4">Figures 1­8.</emphasis>
<taxonomicName id="4C7370F7FF91EA2E61E2A3A8FD66F919" authorityName="Pitts. The" authorityYear="2003" box="[434,740,1712,1735]" class="Insecta" family="Mutillidae" genus="Schusterphotopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="barghesti">
<emphasis id="B907D766FF91EA2E61E2A3A8FD66F919" box="[434,740,1712,1735]" italics="true" pageId="3" pageNumber="4">Schusterphotopsis barghesti</emphasis>
</taxonomicName>
