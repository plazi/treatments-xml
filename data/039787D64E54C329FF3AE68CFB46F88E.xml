<taxonomicName id="4C3E4D434E55C32AFE72E33EFC8EF809" authorityName="Bundhun, Camporesi &amp; K. D. Hyde" authorityYear="2021" box="[448,683,1919,1941]" class="Sordariomycetes" family="Diaporthaceae" genus="Diaporthe" kingdom="Fungi" order="Diaporthales" pageId="5" pageNumber="6" phylum="Ascomycota" rank="species" species="forlicesenica">
