<taxonomicName id="4C4D4D06FFE1FF8EFF0AFB4EB819BCB9" box="[178,271,1168,1194]" class="Magnoliopsida" family="Fabaceae" genus="Mimosa" higherTaxonomySource="CoL" kingdom="Plantae" order="Fabales" pageId="12" pageNumber="213" phylum="Tracheophyta" rank="genus">Mimosa</taxonomicName>
