<taxonomicName id="4C014D08FFFEFFFD7BB8F7FD11F0970B" authorityName="Stubbs, Karin, Arifin, Iskandar, Arida, Reilly, Bloch, Kusnadi &amp; McGuire" authorityYear="2017" box="[427,665,2059,2083]" class="Reptilia" family="Gekkonidae" genus="Lepidodactylus" kingdom="Animalia" order="Squamata" pageId="15" pageNumber="719" phylum="Chordata" rank="species" species="pantai">
