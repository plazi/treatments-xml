<bibRefCitation id="EFEF4B56FFFC2C50FBE6AC8BFB17F88D" author="Bhaud" box="[1107,1266,1782,1807]" pageId="4" pageNumber="188" refString="Bhaud, M. (1975 b) Nouvelles observations de Sabellariidae (Annelides Polychetes) dans la region malgache. Cahiers ORSTOM, Serie Oceanographie, 13, 69 - 77." type="journal article" year="1975" yearSuffix="b">
Bhaud 
<number id="3DB62480FFFC2C50FB1CAC8BFB06F88D" box="[1193,1251,1782,1807]" pageId="4" pageNumber="188" value="1975.0">1975</number>
b
</bibRefCitation>
; 
<bibRefCitation id="EFEF4B56FFFC2C50FAB7AC8BFA72F88D" author="Kirtley" box="[1282,1431,1782,1807]" pageId="4" pageNumber="188" refString="Kirtley, D. W. (1994) A review and taxonomic revision of the family Sabellariidae Johnston, 1865 (Annelida; Polychaeta). Sabecon Press Science Series, 1, 1 - 223." type="journal article" year="1994">
Kirtley 
<number id="3DB62480FFFC2C50FAE9AC8AFA72F88D" box="[1372,1431,1783,1807]" pageId="4" pageNumber="188" value="1994.0">1994</number>
</bibRefCitation>
; Hutchings 
<emphasis id="B90AEAB5FFFC2C50FEA4AD61FEACF8B1" box="[273,329,1818,1843]" italics="true" pageId="4" pageNumber="188">et al.</emphasis>
<number id="3DB62480FFFC2C50FEE5AD66FE6EF8B1" box="[336,395,1819,1843]" pageId="4" pageNumber="188" value="2012.0">2012</number>
