<paragraph id="8BBF369D2526FFA3FF4F8BFA383F99C3" blockId="2.[151,1436,1212,1270]" pageId="2" pageNumber="145">
<emphasis id="B974EA8F2526FFA3FF4F8BFA3A5999E1" bold="true" box="[151,325,1213,1236]" pageId="2" pageNumber="145">FIGURES 1–4.</emphasis>
<taxonomicName id="4C004D1E2526FFA3FE978BFB396C99E1" box="[335,624,1212,1236]" class="Insecta" family="Staphylinidae" genus="Elacatophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="145" phylum="Arthropoda" rank="species" species="euconnoides" status="sp. n.">
<emphasis id="B974EA8F2526FFA3FE978BFB396C99E1" box="[335,624,1212,1236]" italics="true" pageId="2" pageNumber="145">Elacatophora euconnoides</emphasis>
</taxonomicName>
<emphasis id="B974EA8F2526FFA3FDA28BFA39A599E1" bold="true" box="[634,697,1213,1236]" pageId="2" pageNumber="145">
