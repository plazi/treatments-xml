<paragraph id="8BDD7933FFE5FFFBFE938A4CFC29FD6B" blockId="34.[249,1082,598,699]" pageId="34" pageNumber="187">
<taxonomicName id="4C6202B0FFE5FFFBFE938A4CFE48FDBB" authority="Sasaji" authorityName="Sasaji" authorityYear="1967" box="[275,465,598,619]" class="Insecta" family="Coccinellidae" genus="Shirozuella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="187" phylum="Arthropoda" rank="genus">
