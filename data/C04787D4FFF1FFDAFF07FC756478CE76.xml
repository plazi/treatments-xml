<taxonomicName id="8FEE4D41FFF1FFDAFBBAFC7561B7C8AB" authorityName="Arthur" authorityYear="1958" box="[1066,1268,978,1004]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="diomedeae">
