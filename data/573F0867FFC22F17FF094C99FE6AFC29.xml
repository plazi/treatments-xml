<emphasis id="EDE26563FFC42F0AFE514FFEFDBAF863" box="[463,551,1936,1958]" italics="true" pageId="13" pageNumber="368">O. elium</emphasis>
</taxonomicName>
, 
<taxonomicName id="1896C2F2FFC42F0AFDAB4FFEFD34F863" authorityName="Melzer &amp; Bell &amp; Patterson" authorityYear="2017" box="[565,681,1936,1958]" class="Reptilia" family="Scincidae" genus="Oligosoma" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="368" phylum="Chordata" rank="species" species="kokowai">
<emphasis id="EDE26563FFC42F0AFDAB4FFEFD34F863" box="[565,681,1936,1958]" italics="true" pageId="13" pageNumber="368">O. kokowai</emphasis>
</taxonomicName>
, 
<taxonomicName id="1896C2F2FFC42F0AFD294FFEFCF0F863" box="[695,877,1936,1958]" class="Reptilia" family="Scincidae" genus="Oligosoma" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="368" phylum="Chordata" rank="species" species="lineoocellatum">
<emphasis id="EDE26563FFC42F0AFD294FFEFCF0F863" box="[695,877,1936,1958]" italics="true" pageId="13" pageNumber="368">O. lineoocellatum</emphasis>
