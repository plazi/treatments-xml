<emphasis id="FED4EAE32F0AFF83D68B5912FDA3FBEF" box="[509,539,1051,1075]" italics="true" pageId="30" pageNumber="331">
Z 
<number id="7A6824D62F0AFF83D5785912FDA3FBEF" box="[526,539,1051,1075]" pageId="30" pageNumber="331" value="4.0">4</number>
</emphasis>
and 
<emphasis id="FED4EAE32F0AFF83D5215912FDCDFBEE" box="[599,629,1051,1074]" italics="true" pageId="30" pageNumber="331">
Z 
<number id="7A6824D62F0AFF83D51E5912FDCDFBEE" box="[616,629,1051,1074]" pageId="30" pageNumber="331" value="5.0">5</number>
</emphasis>
