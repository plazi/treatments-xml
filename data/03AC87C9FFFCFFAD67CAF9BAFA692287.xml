<taxonomicName id="4C054D5CFFFDFFAD6364FF7BFABD25A2" authority="Toma &amp; Guimaraes" authorityName="Toma &amp; Guimaraes" authorityYear="2002" box="[1081,1352,884,906]" class="Insecta" family="Tachinidae" genus="Leschenaultia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="106" phylum="Arthropoda" rank="species" species="brooksi">
