<taxonomicName id="4C3152ECFF97FF9EFD5742CFFC878BD5" authorityName="ASSING" authorityYear="2012" box="[647,797,1471,1492]" class="Insecta" family="Staphylinidae" genus="Rugilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="75" phylum="Arthropoda" rank="species" species="gonggaicus">
