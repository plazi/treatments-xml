<taxonomicName id="2F9A4D4D905CFF94E0A1A30514F5F8FF" authority="Chace, 1985" authorityName="Chace" authorityYear="1985" box="[151,508,1839,1866]" class="Malacostraca" family="Pandalidae" genus="Parapandalus" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="586" phylum="Arthropoda" rank="species" species="philippinensis">
