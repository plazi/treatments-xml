<taxonomicName id="EC387BD7D3F4C9590082BA56A093A8A5" class="Insecta" family="Zygophyllaceae" genus="Zygophyllum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Thysanoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="family">Zygophyllaceae</taxonomicName>
