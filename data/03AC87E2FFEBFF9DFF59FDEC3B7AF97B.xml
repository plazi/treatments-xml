<paragraph id="8BBA36F4FFEBFF9DFD1CFF543D07FF4E" blockId="80.[739,1120,212,234]" box="[739,1120,211,234]" pageId="80" pageNumber="80">
<taxonomicName id="4C054D77FFEBFF9DFD1CFF543D07FF4E" authority="Mathews" authorityName="Mathews" box="[739,1120,211,234]" class="Aves" family="Meliphagidae" genus="Ptilotis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="80" pageNumber="80" phylum="Chordata" rank="subSpecies" species="sonora" subSpecies="broomei">
