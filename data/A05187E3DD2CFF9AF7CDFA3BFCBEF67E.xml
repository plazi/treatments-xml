<paragraph id="284736F5DD2CFF99F7CDFA64FEAEF7F9" blockId="1.[151,601,1470,1530]" box="[151,262,1505,1530]" pageId="1" pageNumber="282">
<figureCitation id="B0C32A70DD2CFF99F7CDFA64FEAAF7F9" box="[151,258,1505,1530]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-2="FIGURE 3" captionStart-3="FIGURE 4" captionStart-4="FIGURE 5" captionStart-5="FIGURE 6" captionStartId-0="2.[151,250,2011,2033]" captionStartId-1="3.[151,250,1336,1358]" captionStartId-2="3.[151,250,2010,2032]" captionStartId-3="4.[151,250,1102,1124]" captionStartId-4="4.[151,250,1993,2015]" captionStartId-5="5.[151,250,1637,1659]" captionTargetBox-1="[151,1403,193,1314]" captionTargetBox-3="[151,1435,193,1057]" captionTargetBox-4="[151,1408,1174,1971]" captionTargetBox-5="[151,1436,177,1599]" captionTargetId-1="figure@3.[151,1436,193,1314]" captionTargetId-3="figure@4.[151,1436,193,1081]" captionTargetId-4="figure@4.[151,1436,1174,1971]" captionTargetId-5="figure@5.[151,1436,177,1630]" captionTargetPageId-1="3" captionTargetPageId-3="4" captionTargetPageId-4="4" captionTargetPageId-5="5" captionText-0="FIGURE 1. Baltileuctra foraminis, sp. nov. Adult habitus, ventrolateral view;" captionText-1="FIGURE 2. Baltileuctra foraminis, sp. nov. Thoracic sclerites, ventral view;" captionText-2="FIGURE 3. Baltileuctra foraminis, sp. nov. Right foreleg, ventral view;" captionText-3="FIGURE 4. Baltileuctra foraminis, sp. nov. Left forewing,ventral view;" captionText-4="FIGURE 5. Baltileuctra foraminis, sp. nov. Terminalia, ventral view;" captionText-5="FIGURE 6. Baltileuctra foraminis, sp. nov. Terminalia, lateral view;" httpUri-1="https://zenodo.org/record/1216412/files/figure.png" httpUri-3="https://zenodo.org/record/1216414/files/figure.png" httpUri-4="https://zenodo.org/record/1216416/files/figure.png" httpUri-5="https://zenodo.org/record/1216418/files/figure.png" pageId="1" pageNumber="282">Figs. 1–6</figureCitation>
