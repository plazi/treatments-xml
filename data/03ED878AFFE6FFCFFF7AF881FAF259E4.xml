<quantity id="4CBC9B79FFE5FFCEFC20F8EFFBF0597C" box="[973,1055,1895,1920]" metricMagnitude="-2" metricUnit="m" metricValue="9.5" pageId="2" pageNumber="257" unit="mm" value="95.0">95 mm</quantity>
