<quantity id="4CBC9B79FFE5FFCEFD0DF8E0FCDD597C" box="[736,818,1896,1920]" metricMagnitude="-2" metricUnit="m" metricValue="9.0" pageId="2" pageNumber="257" unit="mm" value="90.0">90 mm</quantity>
