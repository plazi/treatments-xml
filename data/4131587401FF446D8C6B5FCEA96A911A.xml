<paragraph id="16D474EB993F6FF89F32266E44CD1905" pageId="null" pageNumber="336">
<taxonomicName id="86758D323236AB1ADAA1B7DFEB7C9224" authority="L." class="Liliopsida" family="Poaceae" genus="Poa" kingdom="Plantae" order="Poales" pageId="null" pageNumber="336" phylum="Tracheophyta" rank="species" species="nemoralis">
Poa 
<normalizedToken id="7DFC3263B75E438E075026B890FF9928" originalValue="nemorális" pageId="null" pageNumber="336">nemoralis</normalizedToken>
<authorityName id="2FDFE1D12E7CC725ABAD5243BBA8982B" pageId="null" pageNumber="336">L.</authorityName>
</taxonomicName>
