<heading id="6E5681E9FFF9FFBDFF72FB17FE36FA97" bold="true" box="[151,471,1263,1289]" fontSize="11" level="1" pageId="1" pageNumber="216" reason="1">
<emphasis id="07D5EA97FFF9FFBDFF72FB17FE36FA97" bold="true" box="[151,471,1263,1289]" pageId="1" pageNumber="216">
<taxonomicName id="F2A14D06FFF9FFBDFF72FB17FEA6FA97" box="[151,327,1263,1289]" class="Insecta" family="Leptoceridae" genus="Magadacerina" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="216" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="07D5EA97FFF9FFBDFF72FB17FEA6FA97" bold="true" box="[151,327,1263,1289]" italics="true" pageId="1" pageNumber="216">Magadacerina</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="1CE657ECFFF9FFBDFEB0FB17FE36FA97" box="[341,471,1263,1289]" pageId="1" pageNumber="216" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
