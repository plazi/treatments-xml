<emphasis id="A1562AB5AE5500F85A9CA3525DCB1E20" bold="true" pageId="3" pageNumber="172">York Co.</emphasis>
, Charters Settlement, 
<geoCoordinate id="1FDEE6F649391E669C045BA7C43E4DD7" degrees="45.8395" direction="north" orientation="latitude" precision="5" value="45.8395">45.8395°N</geoCoordinate>
, 
<geoCoordinate id="361D1F49577D1C573089651A96BA204C" degrees="66.7391" direction="west" orientation="longitude" precision="5" value="-66.7391">66.7391°W</geoCoordinate>
