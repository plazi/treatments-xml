<taxonomicName id="44C34D39FF938D059DF37E8EFEEBF963" box="[151,306,1722,1747]" class="Insecta" family="Sesiidae" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="584" phylum="Arthropoda" rank="tribe" tribe="Paranthrenini">Paranthrenini</taxonomicName>
