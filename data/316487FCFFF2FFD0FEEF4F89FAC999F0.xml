<taxonomicName id="7ECD4D69FFF2FFD0FF0D4C16FD949826" authority="Emeljanov, 2008" authorityName="Emeljanov" authorityYear="2008" box="[164,626,561,583]" class="Insecta" family="Dictyopharidae" genus="Pippax" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="380" phylum="Arthropoda" rank="species" species="opilionoides">
