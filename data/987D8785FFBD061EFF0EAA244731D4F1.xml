* 
<taxonomicName id="D7D44D10FFBD061FFE81AAFC44DBD2CC" authorityName="" authorityYear="1820" box="[280,409,1743,1768]" class="Magnoliopsida" family="Lamiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="6" pageNumber="307" phylum="Magnoliopsida" rank="family">Lamiaceae</taxonomicName>
