<taxonomicName id="3C934D6F0337FFB5FEEEFAF5FC841063" authority="Mateu &amp; Deuve, 1979" authorityName="Mateu &amp; Deuve" authorityYear="1979" box="[342,840,1349,1375]" class="Insecta" family="Carabidae" genus="Trechus" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="franzianus">
