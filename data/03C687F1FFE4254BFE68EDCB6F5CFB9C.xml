<taxonomicName id="4C6F4D64FFE6254BFCBBEBEC6F5DFA2E" authorityName="Melichar" authorityYear="1923" box="[877,1089,1470,1493]" class="Insecta" family="Nogodinidae" genus="Orthophana" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="spinata">
