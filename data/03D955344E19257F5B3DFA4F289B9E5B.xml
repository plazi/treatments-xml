2007 
<taxonomicName id="4C709FA14E19257F5AC3F8C32FD39C95" authority="(STERNB.) KVACEK et H. WALTHER" authorityName="KVACEK et H. WALTHER" baseAuthorityName="STERNB." box="[204,737,1905,1926]" class="Magnoliopsida" family="Juglandaceae" genus="Carya" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="303" phylum="Tracheophyta" rank="species" species="fragiliformis">
