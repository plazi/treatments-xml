<taxonomicName id="D6963D834456F4E95F41D37C8D36AFEC" class="Insecta" family="Braconidae" genus="Centistes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Centistes (Ancylocentrus)" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="subGenus" subGenus="Ancylocentrus">Centistes (Ancylocentrus)</taxonomicName>
