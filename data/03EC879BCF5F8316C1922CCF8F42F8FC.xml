<collectionCode id="ED54AE48CF5F8316C5A32EA58B4BF8D1" box="[1190,1291,1772,1796]" pageId="12" pageNumber="485">SALMA</collectionCode>
, 
<collectingRegion id="4981F86FCF5F8316C41E2EA58EBFF8FC" country="Argentina" name="Santa Cruz" pageId="12" pageNumber="485">
Santa 
