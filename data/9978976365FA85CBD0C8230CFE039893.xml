<collectionCode id="77C0BEB065E585CBD0BE2489FE1E9EC2" box="[398,486,416,442]" country="0" httpUri="http://grbio.org/cool/1d5a-4efs" name="Natural History Museum Bucharest" pageId="176" pageNumber="177">NHMB</collectionCode>
<accessionNumber id="0E82BB9665E585CBD0DE2489FDAC9EC2" box="[494,596,416,442]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/M11223" pageId="176" pageNumber="177">M11223</accessionNumber>
