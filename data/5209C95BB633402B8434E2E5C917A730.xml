<paragraph id="59EA5FDB02908E4BC9DE1B010B6106AC" pageId="285" pageNumber="286">
<taxonomicName id="B4A8E909028A22D62B8CF7A705266195" authorityName="LeConte" authorityYear="1857" class="Hexapoda" family="Carabidae" genus="Clivina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="285" pageNumber="286" phylum="Arthropoda" rank="species" species="ferrea">Clivina ferrea</taxonomicName>
