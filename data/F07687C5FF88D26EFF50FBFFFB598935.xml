<quantity id="BF279B36FF88D26EFE9DFADEFE248919" box="[346,405,1340,1367]" metricMagnitude="1" metricUnit="m" metricValue="3.5" pageId="23" pageNumber="324" unit="m" value="35.0">35 m</quantity>
