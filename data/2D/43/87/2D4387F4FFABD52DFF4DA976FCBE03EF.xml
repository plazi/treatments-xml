<collectingCountry box="[143,270,950,974]" name="Kyrgyzstan" pageId="2" pageNumber="205">Kyrgyzstan</collectingCountry>
, 
<collectingCountry box="[280,388,950,974]" name="Tajikistan" pageId="2" pageNumber="205">Tajikistan</collectingCountry>
