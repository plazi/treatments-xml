<quantity box="[1150,1226,1699,1727]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="14" pageNumber="215" unit="mm" value="2.0">2 mm</quantity>
<emphasis box="[199,252,1958,1981]" italics="true" pageId="14" pageNumber="215">Eyes</emphasis>
