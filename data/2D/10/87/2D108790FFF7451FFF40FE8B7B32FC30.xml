<paragraph blockId="3.[151,328,295,321]" box="[151,328,295,321]" pageId="3" pageNumber="4">
<heading bold="true" box="[151,328,295,321]" fontSize="11" level="1" pageId="3" pageNumber="4" reason="1">
<emphasis bold="true" box="[151,328,295,321]" pageId="3" pageNumber="4">
<taxonomicName box="[151,278,295,321]" class="Reptilia" family="Liolaemidae" genus="Liolaemus" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
