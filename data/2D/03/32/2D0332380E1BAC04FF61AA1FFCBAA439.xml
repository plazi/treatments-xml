<paragraph blockId="20.[151,1437,1770,2011]" box="[151,929,1770,1795]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[151,297,1770,1795]" pageId="20" pageNumber="21">Description.</emphasis>
Female. Body length 
<quantity box="[544,632,1771,1795]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="20" pageNumber="21" unit="mm" value="3.1">3.1 mm</quantity>
, fore wing length 
<quantity box="[836,925,1771,1795]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="20" pageNumber="21" unit="mm" value="3.2">3.2 mm</quantity>
