<taxonomicName authority="Burmeister, 1855" authorityName="Burmeister" authorityYear="1855" box="[196,636,1134,1156]" class="Insecta" family="Melolonthidae" genus="Macrodactylus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="195" phylum="Arthropoda" rank="species" species="mexicanus">
