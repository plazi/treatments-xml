<taxonomicName authority="(Theobald)" baseAuthorityName="Theobald" baseAuthorityYear="1901" box="[151,427,1371,1395]" class="Insecta" family="Culicidae" genus="Mansonia" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="385" phylum="Arthropoda" rank="species" species="uniformis">
