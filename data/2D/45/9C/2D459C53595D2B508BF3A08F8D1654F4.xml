<collectingCountry box="[656,702,1336,1359]" name="Iran" pageId="0" pageNumber="385">Iran</collectingCountry>
. 
<materialsCitation ID-GBIF-Occurrence="2830980304" collectingDate="1999-06-08" collectingDateMax="1999-07-10" collectingDateMin="1999-06-08" collectorName="The &amp; Mattingly" country="Iran" county="Rasht" elevation="500" latitude="26.2556" location="Mohammad-Reza Abai in Boghan Village" longLatPrecision="6" longitude="60.7556" municipality="University of Medical Sciences" pageId="0" pageNumber="385" specimenCount="11" specimenCount-female="11" stateProvince="Gilan">
