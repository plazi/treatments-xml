<taxonomicName authority="Blanchard" authorityName="Blanchard" authorityYear="1901" box="[1015,1239,1335,1359]" class="Insecta" family="Culicidae" genus="Mansonia" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="385" phylum="Arthropoda" rank="genus">
<emphasis box="[1015,1122,1336,1359]" italics="true" pageId="0" pageNumber="385">Mansonia</emphasis>
