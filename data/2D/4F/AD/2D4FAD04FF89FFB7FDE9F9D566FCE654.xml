<paragraph blockId="8.[594,993,1589,1616]" box="[594,993,1589,1616]" pageId="8" pageNumber="9">
<heading box="[594,993,1589,1616]" centered="true" fontSize="11" level="2" pageId="8" pageNumber="9" reason="2">
<emphasis bold="true" box="[594,993,1589,1616]" pageId="8" pageNumber="9">
