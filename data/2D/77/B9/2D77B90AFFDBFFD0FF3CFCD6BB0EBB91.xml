<taxonomicName authorityName="Meyrick" authorityYear="1928" box="[479,586,1635,1660]" class="Insecta" family="Tortricidae" genus="Eucosma" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="sparactis">
