<quantity box="[744,848,440,464]" metricMagnitude="-4" metricUnit="m" metricValue="3.8" pageId="6" pageNumber="599" unit="mm" value="0.38">0.38 mm</quantity>
; PL 
<quantity box="[900,1002,440,464]" metricMagnitude="-4" metricUnit="m" metricValue="2.4" pageId="6" pageNumber="599" unit="mm" value="0.24">0.24 mm</quantity>
