<subSubSection pageId="9" pageNumber="10" type="discussion">
<paragraph blockId="9.[189,303,1731,1757]" box="[189,303,1731,1757]" pageId="9" pageNumber="10">
<heading bold="true" box="[189,303,1731,1757]" fontSize="11" level="3" pageId="9" pageNumber="10" reason="3">
<emphasis bold="true" box="[189,303,1731,1757]" pageId="9" pageNumber="10">Remarks</emphasis>
</heading>
</paragraph>
<paragraph blockId="9.[189,1203,1778,1804]" box="[189,1203,1778,1804]" pageId="9" pageNumber="10">
