<taxonomicName baseAuthorityName="Okushima" baseAuthorityYear="1996" box="[661,804,1718,1744]" class="Insecta" family="Cantharidae" genus="Lycocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="hanatanii">
