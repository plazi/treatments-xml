<geoCoordinate box="[1107,1250,873,899]" degrees="14" direction="south" minutes="38" orientation="latitude" pageId="71" pageNumber="72" precision="15" seconds="46" value="-14.646111">14°38’46” S</geoCoordinate>
, 
<geoCoordinate box="[1264,1423,873,900]" degrees="145" direction="east" minutes="27" orientation="longitude" pageId="71" pageNumber="72" precision="15" seconds="13" value="145.45361">145°27’13” E</geoCoordinate>
), 
<quantity box="[151,238,909,936]" metricMagnitude="1" metricUnit="m" metricValue="1.0" metricValueMax="1.2" metricValueMin="0.8" pageId="71" pageNumber="72" unit="m" value="10.0" valueMax="12.0" valueMin="8.0">8–12 m</quantity>
, coral rubble in sand, 
<date box="[486,640,909,936]" pageId="71" pageNumber="72" value="2013-08-20">20 Aug. 2013</date>
, M. Capa 
<emphasis box="[755,804,909,935]" italics="true" pageId="71" pageNumber="72">et al</emphasis>
., coll. [Complete 
<quantity box="[1006,1084,909,936]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="71" pageNumber="72" unit="mm" value="11.0">11 mm</quantity>
long, 
<quantity box="[1154,1240,909,936]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="71" pageNumber="72" unit="mm" value="1.5">1.5 mm</quantity>
