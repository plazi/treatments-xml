<subSubSection type="reference_group">
<paragraph pageNumber="25">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136575" authority="Emery, 1889" authorityName="Emery" authorityYear="1889" class="Insecta" family="Formicidae" genus="Spalacomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Spalacomyrmex feae Emery" order="Hymenoptera" pageNumber="25" phylum="Arthropoda" rank="species" species="feae">Spalacomyrmex feae Emery</taxonomicName>
