<quantity box="[670,818,398,420]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" metricValueMax="4.45" metricValueMin="3.95" pageId="2" unit="mm" value="4.2" valueMax="4.45" valueMin="3.95">3.95–4.45 mm</quantity>
