<paragraph id="122D5C0DAD450F6865B47422555804A8" pageId="null" pageNumber="395">
<taxonomicName id="95875F6E2E58973019C3ABA50A78B569" authority="L." class="Magnoliopsida" family="Campanulaceae" genus="Campanula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="395" phylum="Tracheophyta" rank="species" species="patula">
