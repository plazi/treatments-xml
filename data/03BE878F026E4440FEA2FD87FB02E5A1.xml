<quantity id="4CEF9B7C026E4440FD56FC0EFCB9E614" box="[717,847,1372,1395]" metricMagnitude="-4" metricUnit="m" metricValue="9.25" metricValueMax="9.5" metricValueMin="9.0" pageId="106" pageNumber="248" unit="mm" value="0.925" valueMax="0.95" valueMin="0.9">0.9–0.95 mm</quantity>
