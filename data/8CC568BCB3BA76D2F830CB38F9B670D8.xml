<paragraph id="9D4C4D203E6CF2C460855F1B990500C2" pageId="10" pageNumber="11">
<taxonomicName id="737FD4098D6448C6B2FDD2D0C98E95C9" LSID="http://zoobank.org/5F3C2042-451E-4B27-8058-711F055D6834" authority="Brothers &amp; Lelej" family="Mutillidae" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="10" pageNumber="11" rank="tribe" tribe="Dasymutillini">Dasymutillini Brothers &amp; Lelej</taxonomicName>
