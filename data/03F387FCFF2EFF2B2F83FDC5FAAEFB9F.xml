<taxonomicName id="4C5A4D69FF2EFF2B2FC6FCFFFD94FCEF" authority=": Yasunaga, 1999: 186" authorityName=": Yasunaga" authorityPageNumber="186" authorityYear="1999" box="[694,1222,768,790]" class="Insecta" family="Miridae" genus="Plagiognathus" kingdom="Animalia" order="Hemiptera" pageId="250" pageNumber="251" phylum="Arthropoda" rank="species" species="senjoensis">
<emphasis id="B92EEAF8FF2EFF2B2FC6FCFFFA9AFCEC" box="[694,968,769,790]" italics="true" pageId="250" pageNumber="251">Plagiognathus senjoensis</emphasis>
