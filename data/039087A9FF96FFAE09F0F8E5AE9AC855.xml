<paragraph id="8B8636BFFF95FFAF09F0FABDAD89CFF8" blockId="58.[151,1436,1401,2038]" box="[151,716,1401,1425]" pageId="58" pageNumber="59">
Host: 
<taxonomicName id="4C394D3CFF95FFAF09BCFABEAE3ACFF8" authorityName="Linnaeus" authorityYear="1758" box="[219,383,1401,1425]" class="Reptilia" family="Anguidae" genus="Anguis" kingdom="Animalia" order="Squamata" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="fragilis">
<emphasis id="B94DEAADFF95FFAF09BCFABEAE3ACFF8" box="[219,383,1401,1425]" italics="true" pageId="58" pageNumber="59">Anguis fragilis</emphasis>
</taxonomicName>
(
<taxonomicName id="4C394D3CFF95FFAF08E8FABEAEABCFF8" box="[399,494,1401,1425]" class="Reptilia" kingdom="Animalia" pageId="58" pageNumber="59" phylum="Chordata" rank="class">Reptilia</taxonomicName>
: 
<taxonomicName id="4C394D3CFF95FFAF089FFABEAD01CFF8" box="[504,580,1401,1425]" class="Reptilia" kingdom="Animalia" order="Squamata" pageId="58" pageNumber="75" phylum="Chordata" rank="subOrder" subOrder="Sauria">Sauria</taxonomicName>
: 
<taxonomicName id="4C394D3CFF95FFAF0B29FABEAD85CFF8" authorityName="Gray" authorityYear="1825" box="[590,704,1401,1425]" class="Reptilia" family="Anguidae" kingdom="Animalia" order="Squamata" pageId="58" pageNumber="59" phylum="Chordata" rank="family">Anguidae</taxonomicName>
