(
<bibRefCitation id="67C79A5AB51B4624067352B62B70EF71" author="Bondar, G" journalOrPublisher="Revista de Entomologia, Rio de Janeiro" pageId="52" pageNumber="103" pagination="78 - 113" refId="B10" refString="Bondar, G, 1946. Notas Entomologicas da Baia. XVII. Revista de Entomologia, Rio de Janeiro 17 (1-2): 78 - 113" title="Notas Entomologicas da Baia. XVII." volume="17" year="1946">Bondar 1946</bibRefCitation>
) and 
<taxonomicName id="6A7E3E91ECB948587E75428308C7E58A" authorityName="Schoenherr" authorityYear="1833" lsidName="" pageId="13" pageNumber="64" rank="subFamily" subFamily="Conoderinae">Conoderinae</taxonomicName>
(
<bibRefCitation id="F4F755C91A7E9620D661A750FBF0AB39" author="Wibmer, GJ" journalOrPublisher="Southwestern Entomologist, supplement" pageId="58" pageNumber="109" pagination="1 - 49" refId="B154" refString="Wibmer, GJ, O'Brien, CW, 1989. Additions and corrections to annotated checklists of the weevils of North America, Central America, the West Indies, and South America. Southwestern Entomologist, supplement 13: 1 - 49" title="Additions and corrections to annotated checklists of the weevils of North America, Central America, the West Indies, and South America." volume="13" year="1989">
