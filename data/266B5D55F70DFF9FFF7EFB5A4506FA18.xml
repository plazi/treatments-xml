, 
<collectingRegion id="6C0622A1F70DFF9FFB82FA3643C5FA53" box="[1131,1250,1434,1457]" country="United States of America" name="Tennessee" pageId="77" pageNumber="78">Tennessee</collectingRegion>
, 
<collectingRegion id="6C0622A1F70DFF9FFB04FA3542BFFA50" box="[1261,1432,1433,1458]" country="United States of America" name="North Carolina" pageId="77" pageNumber="78">North Carolina</collectingRegion>
