<taxonomicName id="4FE00387FF85AE26FEE0F9FC3DE9F563" box="[320,452,1541,1558]" class="Insecta" family="Curculionidae" genus="Macrocryphalus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="genus">Macrocryphalus</taxonomicName>
