<taxonomicName id="A39973FFFF80FFCD55DC8EEBFD2BFE46" authorityName="W. Y. Hsia" authorityYear="1934" box="[449,650,444,470]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="paohsingensis">
