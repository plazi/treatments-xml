<collectionCode id="ED65AE0E6D13FFFFF15FFB69893BFB3A" box="[808,889,1227,1251]" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="10" pageNumber="473">SMNH</collectionCode>
