<number id="3DE81450B714AD3EFE1EC0081AFF3050" box="[423,480,1077,1101]" pageId="6" pageNumber="7" value="1973.0">1973</number>
)
</bibRefCitation>
described and figured 
<taxonomicName id="4C207DF4B714AD3EFD57C00818CB3050" box="[750,980,1077,1101]" class="Myxomycetes" family="Tubiferaceae" genus="Reticularia" kingdom="Fungi" order="Liceales" pageId="6" pageNumber="7" phylum="Mycetozoa" rank="species" species="annulata">
