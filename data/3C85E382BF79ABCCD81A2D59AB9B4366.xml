<taxonomicName id="3B2FE268140A55234C64AD00FF4A15AD" LSID="http://zoobank.org/040348F0-28F6-4B49-AAB8-8BB746453F58" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus mopanmaya" order="Hymenoptera" pageId="254" pageNumber="255" phylum="Arthropoda" rank="species" species="mopanmaya">Heterospilus mopanmaya Marsh</taxonomicName>
