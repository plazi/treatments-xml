<materialsCitation id="4886EE156A59FFF4FD14FCB80B2D2C88" ID-GBIF-Occurrence="3053832301" collectionCode="MATERIEL, TYPE" pageId="23" pageNumber="932" specimenCount="1" typeStatus="holotype">
