<paragraph id="BD492C104E196180A151734D7BD4DFF2" pageId="0" pageNumber="1">
<taxonomicName id="606D1C113CB2B287F595496E1DCB3B12" class="Insecta" family="Curculionidae" genus="Eurydactylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eurydactylus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Eurydactylus</taxonomicName>
