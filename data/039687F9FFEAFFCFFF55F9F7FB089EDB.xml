<taxonomicName id="4C3F4D6CFFEDFFCFFECBFA31FE3F9DBF" box="[338,446,1447,1468]" class="Actinopterygii" family="Cichlidae" genus="Congochromis" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="sabinae">
