<taxonomicName id="4C3F4D6CFFEDFFCFFBDBFA31FB4A9DBF" baseAuthorityName="Pellegrin" baseAuthorityYear="1900" box="[1090,1227,1447,1468]" class="Actinopterygii" family="Cichlidae" genus="Congochromis" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="dimidiatus">
