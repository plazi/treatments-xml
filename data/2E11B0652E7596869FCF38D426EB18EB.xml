</taxonomicName>
, 
<taxonomicName id="5A600326D9D5F691FD67E5A7D257D78A" authorityName="Mattera, T. Molnar &amp; Struwe" authorityYear="2015" class="Magnoliopsida" family="Cornaceae" genus="Cornus" higherTaxonomySource="CoL" hybrid-sign="x" kingdom="Plantae" lsidName="Cornus rutgersensis" order="Cornales" pageId="7" pageNumber="100" phylum="Tracheophyta" rank="species" species="rutgersensis">
Cornus 
<normalizedToken id="BE162EE4907BFEC275C1D8C01C6EFF85" originalValue="×">x</normalizedToken>
rutgersensis
</taxonomicName>
, and 
<taxonomicName id="E514A2F3B3AED57B002A1D93B363ED4E" class="Magnoliopsida" family="Cornaceae" genus="Cornus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cornus kousa" order="Cornales" pageId="7" pageNumber="100" phylum="Tracheophyta" rank="species" species="kousa">
<emphasis id="27F7CE17ACBD05F1AB69216BC16FDEE6" italics="true" pageId="7" pageNumber="100">Cornus kousa</emphasis>
