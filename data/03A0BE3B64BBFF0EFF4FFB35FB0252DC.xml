<heading id="D0FEB84164BBFF0EFF4FFB35FDCF53D7" bold="true" box="[151,526,1187,1213]" fontSize="11" level="1" pageId="177" pageNumber="178" reason="1">
<emphasis id="B97DD33F64BBFF0EFF4FFB35FDCF53D7" bold="true" box="[151,526,1187,1213]" pageId="177" pageNumber="178">
Genus 
<taxonomicName id="4C0974AE64BBFF0EFF36FB35FDCF53D7" authority="Miller, 1900" authorityName="Miller" authorityYear="1900" box="[238,526,1187,1213]" class="Mammalia" family="Cricetidae" genus="Craseomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="177" pageNumber="178" phylum="Chordata" rank="genus">
<emphasis id="B97DD33F64BBFF0EFF36FB35FEB353D7" bold="true" box="[238,370,1187,1213]" italics="true" pageId="177" pageNumber="178">Craseomys</emphasis>
Miller, 1900
</taxonomicName>
</emphasis>
