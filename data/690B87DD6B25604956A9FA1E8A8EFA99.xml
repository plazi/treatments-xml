</emphasis>
habitus: 
<emphasis id="D3D6EAD96B26604854ABF8008B81F809" bold="true" box="[661,674,2002,2024]" pageId="6" pageNumber="167">4</emphasis>
, 
<taxonomicName id="26A24D486B2660485490F8008A52F809" authorityName="Literak" authorityYear="2017" box="[686,881,2002,2024]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="6" pageNumber="167" phylum="Arthropoda" rank="species" species="flaveolae">
<emphasis id="D3D6EAD96B2660485490F8008A52F809" box="[686,881,2002,2024]" italics="true" pageId="6" pageNumber="167">Myrsidea flaveolae</emphasis>
</taxonomicName>
female; 
<emphasis id="D3D6EAD96B26604855F4F8008AF5F809" bold="true" box="[970,982,2002,2024]" pageId="6" pageNumber="167">5</emphasis>
, 
<taxonomicName id="26A24D486B26604855DDF8008D85F809" authorityName="Literak" authorityYear="2017" box="[995,1190,2002,2024]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="6" pageNumber="167" phylum="Arthropoda" rank="species" species="flaveolae">
<emphasis id="D3D6EAD96B26604855DDF8008D85F809" box="[995,1190,2002,2024]" italics="true" pageId="6" pageNumber="167">Myrsidea flaveolae</emphasis>
</taxonomicName>
