<collectionCode id="13844BA9FFB1FF91FD37F89BFC56F8B0" box="[895,961,1836,1862]" country="USA" httpUri="http://grbio.org/cool/nw7q-krra" name="University of California, Davis" pageId="19" pageNumber="20">UCD</collectionCode>
