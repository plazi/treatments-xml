<taxonomicName id="B295A8EFFFB1FF91FDACF8B3FBCCF8EB" box="[996,1115,1796,1821]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="araujoi">
