<taxonomicName id="B295A8EFFFB1FF91FFFEF8E3FDD4F89B" box="[438,579,1876,1901]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
