<paragraph id="752AD36CFFB1FF91FF70F8B3FD30F898" blockId="19.[264,1323,1756,1902]" pageId="19" pageNumber="20">
<collectionCode id="13844BA9FFB1FF91FF70F8B3FE75F8E8" box="[312,482,1796,1822]" pageId="19" pageNumber="20">COMMENTS</collectionCode>
