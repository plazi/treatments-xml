<taxonomicName id="4C5C5091FFFEFF8811EED0A6E12B5B99" box="[203,310,680,702]" class="Maxillopoda" family="Lernaeopodidae" genus="Brachiella" kingdom="Animalia" order="Siphonostomatoida" pageId="1" pageNumber="52" phylum="Arthropoda" rank="genus">
