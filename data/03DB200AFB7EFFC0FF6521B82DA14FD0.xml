(
<bibRefCitation id="EFE3ECEDFB7EFFC0FC3825A32CB74CD4" box="[970,1154,1508,1532]" pageId="7" pageNumber="476" refId="ref14465" refString="Hampson, G. F. (1891) The lepidoptera of heterocera of the Nilgiri district. Illustrations of typical specimens of Lepidoptera Heterocera in the collection of the British Museum, 8, 1 - 144, pls. 139 - 156." type="journal article">Hampson, 1891</bibRefCitation>
)
</taxonomicName>
with question mark by 
<bibRefCitation id="EFE3ECEDFB7EFFC0FF652640296D4F08" box="[151,344,1543,1568]" pageId="7" pageNumber="476" refId="ref14592" refString="Holloway, J. D. (2001) The Moths of Borneo: Family Arctiidae, Subfamily Lithosiinae. Malayan Nature Journal, 7, 279 - 486." type="journal article">Holloway (2001)</bibRefCitation>
and synonym of 
<taxonomicName id="4C72EA9FFB7EFFC0FDEF264E2B454F08" authority="Bucsek, 2008" authorityName="Bucsek" authorityYear="2008" box="[541,880,1543,1568]" class="Insecta" family="Erebidae" genus="Eugoa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="476" phylum="Arthropoda" rank="species" species="cesaneki">
