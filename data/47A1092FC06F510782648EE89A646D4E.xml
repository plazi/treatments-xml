<paragraph id="8B40F86DBEC4B68E48BC4DC99C7D17A4" pageId="24" pageNumber="41">
<taxonomicName id="022320DB80D81F434A3CDD3AD4A5A27F" baseAuthorityName="Obenberger" baseAuthorityYear="1913" class="Insecta" family="Buprestidae" genus="Brasilaxia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="24" pageNumber="41" phylum="Arthropoda" rank="species" species="costifera">
