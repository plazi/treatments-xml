<heading id="4C6081A75633FFCBE89AFE58FDC7FE2C" bold="true" box="[151,588,475,502]" fontSize="11" level="1" pageId="29" pageNumber="30" reason="1">
<emphasis id="25E3EAD95633FFCBE89AFE58FDC7FE2C" bold="true" box="[151,588,475,502]" pageId="29" pageNumber="30">
<emphasis id="25E3EAD95633FFCBE89AFE58FE3EFE2F" bold="true" box="[151,437,476,502]" italics="true" pageId="29" pageNumber="30">
<taxonomicName id="D0974D485633FFCBE89AFE58FE24FE2F" box="[151,431,476,502]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="naitasirensis" status="new species">Chimarra naitasirensis</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="3ED057A25633FFCBE9B1FE5FFDC7FE2C" box="[444,588,475,501]" pageId="29" pageNumber="30" rank="species">new species</taxonomicNameLabel>
</emphasis>
