<taxonomicName id="ADD74D55FFB3FFA9FEB0FA490C7DD0F8" authority="Bergh, 1888" authorityName="Bergh" authorityYear="1888" box="[298,603,1502,1527]" class="Gastropoda" family="Facelinidae" genus="Moridilla" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Nudibranchia" pageId="3" pageNumber="268" phylum="Mollusca" rank="species" species="brockii">
