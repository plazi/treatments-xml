<specimenCount id="9D73FD44FFA3FF9070B5FE9DFF59FB7A" box="[151,248,260,284]" pageId="47" pageNumber="48" type="female">1 female</specimenCount>
<typeStatus id="54CE886FFFA3FF9070DDFE9DFECBFB7A" box="[255,362,260,284]" pageId="47" pageNumber="48">paratypes</typeStatus>
in the 
<collectionCode id="ED64AE08FFA3FF907195FE9DFE55FB7A" box="[439,500,260,284]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="47" pageNumber="48" type="Museum">ZSM</collectionCode>
. 
<collectingRegion id="49B1F82FFFA3FF9071DDFE9DFD95FB7D" box="[511,564,260,283]" country="Afghanistan" name="Khost" pageId="47" pageNumber="48">Host</collectingRegion>
