<heading id="2653395AFFB2D358EBE6FA3CFDEEFA0C" bold="true" box="[151,535,1482,1509]" fontSize="11" level="1" pageId="30" pageNumber="181" reason="1">
<emphasis id="4FD05224FFB2D358EBE6FA3CFDEEFA0C" bold="true" box="[151,535,1482,1509]" pageId="30" pageNumber="181">
<taxonomicName id="BAA4F5B5FFB2D358EBE6FA3CFE3FFA0D" authority="Zwick, 2016" authorityName="Zwick" authorityYear="2016" box="[151,454,1482,1508]" class="Insecta" family="Scirtidae" genus="Prionocyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="181" phylum="Arthropoda" rank="species" species="bidentatus" status="n. sp.">
<emphasis id="4FD05224FFB2D358EBE6FA3CFE3FFA0D" bold="true" box="[151,454,1482,1508]" italics="true" pageId="30" pageNumber="181">Prionocyphon bidentatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="54E3EF5FFFB2D358EAA4FA3DFDEEFA0C" box="[469,535,1483,1509]" pageId="30" pageNumber="181" rank="species">n. sp.</taxonomicNameLabel>
