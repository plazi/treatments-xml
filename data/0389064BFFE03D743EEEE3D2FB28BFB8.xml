</taxonomicName>
sp. cf. 
<taxonomicName id="4C20CCDEFFE03D743F25E3D2FD6FBBA8" authority="d'Orbigny 1826" authorityName="d'Orbigny" authorityYear="1826" box="[348,674,151,176]" class="Tubothalamea" family="Cribrolinoididae" genus="Adelosina" kingdom="Chromista" order="Miliolida" pageId="26" pageNumber="27" phylum="Foraminifera" rank="species" species="pulchella">
