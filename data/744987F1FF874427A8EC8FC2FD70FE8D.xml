<caption id="A89F666FFF844421A8EC8B42FEC9F94F" ID-DOI="http://doi.org/10.5281/zenodo.3813095" ID-Zenodo-Dep="3813095" httpUri="https://zenodo.org/record/3813095/files/figure.png" pageId="26" pageNumber="73" startId="26.[140,212,1590,1612]" targetBox="[248,988,958,1564]" targetPageId="26">
