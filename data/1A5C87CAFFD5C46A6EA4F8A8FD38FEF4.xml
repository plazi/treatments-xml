<quantity id="550D9B39FFD7C46C6AECFED2FAEEFEA4" box="[1247,1347,272,298]" metricMagnitude="-3" metricUnit="m" metricValue="2.68" pageId="11" pageNumber="33" unit="mm" value="2.68">2.68mm</quantity>
; 
