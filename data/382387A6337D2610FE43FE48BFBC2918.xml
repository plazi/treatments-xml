<heading id="EB7D81DC337D2611FF7DFD42BF642F85" box="[160,362,697,723]" fontSize="11" level="3" pageId="10" pageNumber="2321" reason="5">
<emphasis id="82FEEAA2337D2611FF7DFD42BF642F85" bold="true" box="[160,362,697,723]" inLineHeading="true" italics="true" pageId="10" pageNumber="2321" reason="3">
<taxonomicName id="778A4D33337D2611FF7DFD42BEC32F85" authorityName="Diakonoff" authorityYear="1982" box="[160,205,697,723]" class="Insecta" family="Tortricidae" genus="Age" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="2321" phylum="Arthropoda" rank="genus">Age</taxonomicName>
