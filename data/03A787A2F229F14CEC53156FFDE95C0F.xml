<taxonomicName id="4C0E4D37F229F14FEC8F1003FB4A5BF5" box="[981,1227,1647,1670]" class="Aves" family="Pachycephalidae" genus="Pachycephala" kingdom="Animalia" order="Passeriformes" pageId="29" pageNumber="29" phylum="Chordata" rank="species" species="littayei">
