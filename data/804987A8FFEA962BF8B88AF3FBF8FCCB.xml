<taxonomicName id="CFE04D3DFFEA962BFA3D8B6EFD30FE8B" box="[530,651,354,377]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="68" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
