<emphasis id="B90DEAFCFF8BFF9EAADDF859FE17F84B" bold="true" box="[151,397,1923,1949]" pageId="24" pageNumber="25">Material examined</emphasis>
. 
<specimenCount id="9D7FFD67FF8BFF9EABD6F85EFDBDF848" box="[412,551,1924,1950]" pageId="24" pageNumber="25" type="generic">
<number id="3DB124C9FF8BFF9EABD6F85EFE31F848" box="[412,427,1924,1950]" pageId="24" pageNumber="25" value="1.0">1</number>
specimen
</specimenCount>
