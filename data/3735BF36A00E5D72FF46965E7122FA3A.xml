<taxonomicName id="789C75A3A00E5D72FD0891417619FB8E" baseAuthorityName="Lea" baseAuthorityYear="1912" box="[729,889,1029,1056]" class="Insecta" family="Lymexylidae" genus="Australymexylon" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="217" phylum="Arthropoda" rank="species" species="fuscipennis">
