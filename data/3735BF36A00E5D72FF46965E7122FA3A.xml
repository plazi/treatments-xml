. 
<taxonomicName id="789C75A3A00E5D72FECA9142775DFB8E" baseAuthorityName="Erichson" baseAuthorityYear="1842" box="[283,573,1029,1055]" class="Insecta" family="Lymexylidae" genus="Australymexylon" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="217" phylum="Arthropoda" rank="species" species="australe">
