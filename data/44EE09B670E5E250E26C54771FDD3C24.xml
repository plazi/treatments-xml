<bibRefCitation id="CA5F880814BC6C38781BB670672BFCB6" pageId="0" pageNumber="73">Simon 1885</bibRefCitation>
) = 
<taxonomicName id="CC5C22D1FCDD5B154742D0FDF9B3C9CC" class="Arachnida" family="Salticidae" genus="Chalcoscirtus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcoscirtus elegans" order="Araneae" pageId="0" pageNumber="73" phylum="Arthropoda" rank="species" species="elegans">Chalcoscirtus elegans</taxonomicName>
<bibRefCitation id="395AEAD1E4D7F62DBB33EF39038A722C" pageId="0" pageNumber="73">Simon 1885</bibRefCitation>
, originally designated by 
<bibRefCitation id="AF5E24FA9A40727F43F99EEC9213C487" author="Simon, E" journalOrPublisher="Annales de la Societe Entomologique de Belgique" pageId="10" pageNumber="83" pagination="381 - 407" title="Descriptions d'arachnides nouveaux de la famille des Attidae." volume="44" year="1900">Simon (1900)</bibRefCitation>
