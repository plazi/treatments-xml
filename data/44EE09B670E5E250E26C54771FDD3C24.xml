<paragraph id="A99AFE6AB31D56B38ACAC21C1A448A23" pageId="0" pageNumber="73">Type species.</paragraph>
<paragraph id="70AD0C410418CC03C6AA2DF7ECA3C43A" pageId="0" pageNumber="73">
<taxonomicName id="5D371E02B8027BEB659316A181E045C1" genus="R." lsidName="R. elegans" pageId="0" pageNumber="73" rank="species" species="elegans">R. elegans</taxonomicName>
