<emphasis id="B926E407F876FFE6FE38FEFE67D8FEB9" box="[395,957,284,309]" italics="true" pageId="20" pageNumber="21">.............................................................................</emphasis>
<taxonomicName id="4C524396F876FFE6FC77FEFE60B7FEBA" authority="Villeneuve" authorityName="Villeneuve" box="[964,1234,284,310]" class="Insecta" family="Calliphoridae" genus="Chrysomya" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="laxifrons">
