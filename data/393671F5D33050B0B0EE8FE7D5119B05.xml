<taxonomicName id="3E7D52474E4E13C0C633E1CAAC3411A4" genus="Platygastroidea" lsidName="Platygastroidea puncticollis" pageId="0" pageNumber="7991" rank="species" species="puncticollis">puncticollis</taxonomicName>
