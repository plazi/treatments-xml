<taxonomicName id="4C1192EF3D3CFB365A74FD12FA75FD37" authority="Walker, 1854" authorityName="Elwes" authorityYear="1890" box="[1316,1429,693,719]" class="Insecta" family="Erebidae" genus="Bizone" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="molleri">
