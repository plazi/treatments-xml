<subSubSection id="B3D9DE7F12EC99D537B3BFD2D85B6C0A" type="reference_group">
<paragraph id="36EB306268C1B2AF2B587B0D503AEBF6" pageNumber="51">
<taxonomicName id="7AF06B51DA6A8E769659577BB05309E9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32382" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica hirsuta Elmes" order="Hymenoptera" pageNumber="51" phylum="Arthropoda" rank="species" species="hirsuta">Myrmica hirsuta Elmes</taxonomicName>
