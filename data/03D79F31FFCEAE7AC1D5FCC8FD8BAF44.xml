<taxonomicName id="4C7E55A4FFCEAE7AC466FCE0FE3AA852" authority="Rondani, 1861" authorityName="Rondani" authorityYear="1861" class="Insecta" family="Tachinidae" genus="Clytiomya" kingdom="Animalia" order="Diptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="genus">
