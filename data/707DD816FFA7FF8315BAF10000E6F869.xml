<subSubSection id="B0CE3A8BFFA7FF8315BAF100025CFCA1" pageId="41" pageNumber="42" type="nomenclature">
<paragraph id="F86B6900FFA7FF8315BAF1000135FC84" blockId="41.[151,562,831,892]" box="[151,562,831,857]" pageId="41" pageNumber="42">
<heading id="A323DE6CFFA7FF8315BAF1000135FC84" bold="true" box="[151,562,831,857]" fontSize="11" level="1" pageId="41" pageNumber="42" reason="1">
<emphasis id="CAA0B512FFA7FF8315BAF1000135FC84" bold="true" box="[151,562,831,857]" pageId="41" pageNumber="42">
<taxonomicName id="3FD41283FFA7FF8315BAF10002EFFC84" authority="Troubridge" authorityName="Troubridge" authorityYear="2008" box="[151,488,831,857]" class="Insecta" family="Noctuidae" genus="Sympistis" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="khem" status="sp. n.">
