<paragraph id="8BC936CDEB60FFA8BAB0F679FC0BFDD5" blockId="3.[151,1437,547,1366]" box="[151,981,547,574]" pageId="3" pageNumber="336">
<emphasis id="B902EADFEB60FFA8BAB0F679FEC3FDD6" bold="true" box="[151,285,547,573]" pageId="3" pageNumber="336">
<typeStatus id="54CD886FEB60FFA8BAB0F679FF0CFDD6" box="[151,210,547,573]" pageId="3" pageNumber="336">Type</typeStatus>
genus
</emphasis>
. 
<taxonomicName id="4C764D4EEB60FFA8BB0CF679FE72FDD6" authorityName="Saussure" authorityYear="1862" box="[299,428,547,573]" class="Insecta" family="Tetrigidae" genus="Cladonotus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="336" phylum="Arthropoda" rank="genus">
