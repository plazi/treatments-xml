<taxonomicName id="6AD247ADFFF0FFCDFEBCFBA8E8E6FB9E" baseAuthorityName="Fabricius" baseAuthorityYear="1803" box="[277,509,1123,1149]" class="Insecta" family="Pentatomidae" genus="Hypoxys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="quadridens">
