<taxonomicName id="4C2BAA87FFCFFFBF2BC8067BCB6AFBAB" authorityName="OHAUS" authorityYear="1914" box="[967,1144,1024,1048]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="349" phylum="Arthropoda" rank="species" species="vitalisi">
