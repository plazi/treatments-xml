<taxonomicName id="4C2F02C9FFD32A2A2F7C4D72FD34FE1C" box="[607,733,361,385]" class="Insecta" family="Hybotidae" genus="Hybos" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="175" phylum="Arthropoda" rank="species" species="pallidus">
