<taxonomicName id="4C2F02C9FFD32A2A2E354D72FC26FE1C" box="[790,975,361,385]" class="Insecta" family="Hybotidae" genus="Hybos" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="175" phylum="Arthropoda" rank="species" species="liupanshanus">
