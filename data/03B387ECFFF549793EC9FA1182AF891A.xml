<date id="FFA4103AFFF749793AA7FC6F8382894C" box="[1273,1425,1002,1028]" pageId="7" pageNumber="8" value="1986-12-02">
02/XII/ 
<number id="3DD224DDFFF749793B08FC6F8382894C" box="[1366,1425,1002,1028]" pageId="7" pageNumber="8" value="1986.0">1986</number>
</date>
” (
<collectionCode id="ED0BAE3FFFF749793EC1FB9486F88963" box="[159,235,1041,1067]" pageId="7" pageNumber="8">DZRJ</collectionCode>
), one male “ 
<date id="FFA4103AFFF749793F20FB94841D8963" box="[382,526,1041,1067]" pageId="7" pageNumber="8" value="1987-09-22">
<number id="3DD224DDFFF749793F20FB94878E8963" box="[382,413,1041,1067]" pageId="7" pageNumber="8" value="22.0">22</number>
/IX/ 
<number id="3DD224DDFFF749793F8CFB94841D8963" box="[466,526,1041,1067]" pageId="7" pageNumber="8" value="1987.0">1987</number>
</date>
”, one male “ 
<date id="FFA4103AFFF749793CFDFB94852E8963" box="[675,829,1041,1067]" pageId="7" pageNumber="8" value="1987-07-31">
<number id="3DD224DDFFF749793CFDFB9484AD8963" box="[675,702,1041,1067]" pageId="7" pageNumber="8" value="31.0">31</number>
/VII/ 
<number id="3DD224DDFFF749793CA3FB94852E8963" box="[765,829,1041,1067]" pageId="7" pageNumber="8" value="1987.0">1987</number>
</date>
”, one male “ 
<date id="FFA4103AFFF749793D8DFB9482718963" box="[979,1122,1041,1067]" pageId="7" pageNumber="8" value="1992-11-05">
05/XI/ 
<number id="3DD224DDFFF749793A7BFB9482718963" box="[1061,1122,1041,1067]" pageId="7" pageNumber="8" value="1992.0">1992</number>
</date>
”, one female “ 
<date id="FFA4103AFFF749793B50FB9483828963" box="[1294,1425,1041,1067]" pageId="7" pageNumber="8" value="1982-05-26">
<number id="3DD224DDFFF749793B50FB94833F8963" box="[1294,1324,1041,1067]" pageId="7" pageNumber="8" value="26.0">26</number>
/V/ 
<number id="3DD224DDFFF749793B0BFB9483828963" box="[1365,1425,1041,1067]" pageId="7" pageNumber="8" value="1982.0">1982</number>
</date>
” (UFV), one male “ 
<date id="FFA4103AFFF749793F2EFBBD87E0891A" box="[368,499,1080,1106]" pageId="7" pageNumber="8" value="1987-02-20">
<number id="3DD224DDFFF749793F2EFBBD879C891A" box="[368,399,1080,1106]" pageId="7" pageNumber="8" value="20.0">20</number>
/II/ 
<number id="3DD224DDFFF749793FE9FBBD87E0891A" box="[439,499,1080,1106]" pageId="7" pageNumber="8" value="1987.0">1987</number>
</date>
”, one male “ 
<date id="FFA4103AFFF749793CD8FBBD8518891A" box="[646,779,1080,1106]" pageId="7" pageNumber="8" value="1987-10-14">
<number id="3DD224DDFFF749793CD8FBBD84B0891A" box="[646,675,1080,1106]" pageId="7" pageNumber="8" value="14.0">14</number>
/X/ 
<number id="3DD224DDFFF749793C90FBBD8518891A" box="[718,779,1080,1106]" pageId="7" pageNumber="8" value="1987.0">1987</number>
</date>
”, one female “ 
<date id="FFA4103AFFF749793DEBFBBD8252891A" box="[949,1089,1080,1106]" pageId="7" pageNumber="8" value="1988-04-26">
