<typeStatus id="54A18858FFF749793E9BFC4687108E95" box="[197,259,963,989]" pageId="7" pageNumber="8">Type</typeStatus>
material
</emphasis>
. 
<typeStatus id="54A18858FFF749793FD8FC4187EA8E96" box="[390,505,964,990]" pageId="7" pageNumber="8">Holotype</typeStatus>
: male, 
<collectingCountry id="F30D766AFFF749793C13FC41848B8E96" box="[589,664,964,990]" name="Brazil" pageId="7" pageNumber="8">Brazil</collectingCountry>
, “ 
<taxonomicName id="4C1A4D79FFF749793CEEFC4185158E96" box="[688,774,964,990]" class="Insecta" family="Cicadellidae" genus="Agalliopsis" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="15" phylum="Arthropoda" rank="species" species="vicosa">Viçosa</taxonomicName>
, MG [Minas Gerais State], 
<collectingCountry id="F30D766AFFF749793A0EFC4182858E96" box="[1104,1174,964,990]" name="Brazil" pageId="7" pageNumber="8">Brasil</collectingCountry>
\ Data 
<date id="FFA4103AFFF749793AB9FC4183458E96" box="[1255,1366,964,990]" pageId="7" pageNumber="8" value="1987-10-02">
