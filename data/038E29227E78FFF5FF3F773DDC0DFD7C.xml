</emphasis>
<taxonomicName id="4C27E3B77E79FFF5FE8173F3DDDFFF09" authority="Plotz, 1882" authorityName="Plotz" authorityYear="1882" box="[382,705,217,242]" class="Insecta" family="Hesperiidae" genus="Pellicia" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="10" phylum="Arthropoda" rank="species" species="licisca">
