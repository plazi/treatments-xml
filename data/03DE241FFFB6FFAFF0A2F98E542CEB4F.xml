<emphasis id="B903491BFFB5FFAFF0F2F9BE5167EAB7" bold="true" box="[199,354,1657,1682]" inLineHeading="true" pageId="27" pageNumber="528" reason="1">Depth range.</emphasis>
<quantity id="4C8F38ECFFB5FFAFF15CF9BE51EDEAB4" box="[361,488,1657,1682]" metricMagnitude="2" metricUnit="m" metricValue="8.715" metricValueMax="9.23" metricValueMin="8.2" pageId="27" pageNumber="528" unit="m" value="871.5" valueMax="923.0" valueMin="820.0">820–923 m</quantity>
