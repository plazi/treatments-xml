</taxonomicName>
/ 
<taxonomicName id="4C1B04CCFF8FFFE6FC0CFAC524EDCA92" authority="Hartungi Heer" authorityName="Hartungi Heer" class="Insecta" family="Curculionidae" genus="Otiorrhynchus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="2007" phylum="Arthropoda" rank="genus">
<emphasis id="B96FA35DFF8FFFE6FC0CFAC5208BCA72" box="[918,1185,1362,1386]" italics="true" pageId="7" pageNumber="2007">Otiorrhynchus Hartungi</emphasis>
