<paragraph id="6F0336A7C7618B1C0FD2F951CB23F93E" blockId="8.[189,334,1738,1764]" box="[189,334,1738,1764]" pageId="8" pageNumber="67">
<heading id="344B81CBC7618B1C0FD2F951CB23F93E" bold="true" box="[189,334,1738,1764]" fontSize="11" level="3" pageId="8" pageNumber="67" reason="3">
