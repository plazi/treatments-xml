<paragraph id="FA91639C7871EB9D3EA3CC48B8337902" pageId="10" pageNumber="93">
<taxonomicName id="2EE514575766AA38DC0F160EBCE05E7B" LSID="http://zoobank.org/0E8B453F-D81F-484B-9897-7F615908507C" class="Insecta" family="Melolonthidae" genus="Tetraserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetraserica shunbiensis" order="Coleoptera" pageId="10" pageNumber="93" phylum="Arthropoda" rank="species" species="shunbiensis">Tetraserica shunbiensis</taxonomicName>
