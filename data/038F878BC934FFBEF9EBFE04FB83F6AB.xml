<geoCoordinate id="EE12505AC934FFBEF9EBFB4CFEA8F6AA" box="[151,305,1173,1200]" degrees="54" direction="south" minutes="25" orientation="latitude" pageId="39" pageNumber="40" precision="1" seconds="0.12" value="-54.416702">54º25’0.12”S</geoCoordinate>
