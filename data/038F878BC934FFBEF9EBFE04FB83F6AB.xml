<quantity id="4CDE9B78C934FFBEFB16FBABFD23F697" box="[618,698,1138,1165]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="39" pageNumber="40" unit="m" value="100.0">100 m</quantity>
to 
