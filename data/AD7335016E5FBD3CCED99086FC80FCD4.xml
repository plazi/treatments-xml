<collectionCode id="43CB1CD26E5FBD3CCD9B927AFB91FD45" LSID="urn:lsid:biocol.org:col:34925" box="[981,1066,619,643]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="2" pageNumber="66">AMNH</collectionCode>
