<geoCoordinate id="CFF0108F35187C98E0D8108C4EFD6E74" degrees="23" direction="north" minutes="32" orientation="latitude" precision="1" seconds="28.73" value="23.541313">23°32'28.73&quot;N</geoCoordinate>
