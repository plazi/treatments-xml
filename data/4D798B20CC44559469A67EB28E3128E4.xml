<geoCoordinate id="4F4EBA3F7B8382CC45A2486B8F8BD399" degrees="114" direction="east" minutes="35" orientation="longitude" precision="1" seconds="8.46" value="114.585686">114°35'8.46&quot;E</geoCoordinate>
