species.
</emphasis>
<taxonomicName id="4C3E7CAE5276FFDFFEEDFB3C6771FB29" authority="Hess, 1970" authorityName="Hess" authorityYear="1970" box="[361,614,1159,1185]" genus="Chaetasterina" kingdom="Fungi" pageId="4" pageNumber="148" rank="species" species="gracilis">
