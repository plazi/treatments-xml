<taxonomicName id="F0AB4D37FFF43A36086DFB93944BAB70" authority="Eichler, 1949" authorityName="Eichler" authorityYear="1949" box="[274,608,1052,1079]" class="Insecta" family="Philopteridae" genus="Guimaraesiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="26" pageNumber="278" phylum="Arthropoda" rank="genus">
