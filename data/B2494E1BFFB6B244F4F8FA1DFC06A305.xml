<taxonomicName id="FDE0848EFFB6B244F56AF9C5FF40A331" authority="Ellis &amp; Solander, 1786" authorityName="Ellis &amp; Solander" authorityYear="1786" class="Demospongiae" family="Spongiidae" genus="Spongia" kingdom="Animalia" order="Dictyoceratida" pageId="13" pageNumber="216" phylum="Porifera" rank="species" species="botryoides">
