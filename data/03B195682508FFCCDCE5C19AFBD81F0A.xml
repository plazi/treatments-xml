<subSubSection id="C30277F52508FFC3DCE5C652FD201A79" box="[151,672,1082,1106]" pageId="23" pageNumber="24" type="materials_examined">
<paragraph id="8BA7247E2508FFC3DCE5C652FD201A79" blockId="23.[151,672,1082,1106]" box="[151,672,1082,1106]" pageId="23" pageNumber="24">
<emphasis id="B96CF86C2508FFC3DCE5C652FEAB1A7A" bold="true" box="[151,299,1082,1106]" pageId="23" pageNumber="24">
<typeStatus id="54A39ADC2508FFC3DCE5C652FF4F1A7A" box="[151,207,1082,1106]" pageId="23" pageNumber="24">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C185FFD2508FFC3DD43C652FD1C1A79" authority="Butler, 1876: 547" authorityName="Butler" authorityPageNumber="547" authorityYear="1876" box="[305,668,1082,1105]" class="Insecta" family="Tettigoniidae" genus="Phisis" kingdom="Animalia" order="Orthoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="spinifera">
