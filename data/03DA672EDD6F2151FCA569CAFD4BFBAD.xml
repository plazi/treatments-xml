<taxonomicName id="4C73ADBBDD6F2151FD9E6E35FE54FBAD" authorityName="FORSTER" authorityYear="1853" box="[393,510,1129,1151]" class="Insecta" family="Megachilidae" genus="Coelioxys" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="607" phylum="Arthropoda" rank="species" species="echinata">
