1849.12.3.75–78, 
<collectionCode id="ADCDAE11FFED6528FCB8EBC8D7BD17D7" box="[813,919,919,945]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="41" pageNumber="42" type="Museum">NHMUK</collectionCode>
