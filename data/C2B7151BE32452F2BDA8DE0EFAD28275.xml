<taxonomicName id="4E939287EC5D23FDC8D5721B9E558C7A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179465" class="Insecta" family="Formicidae" genus="Ponera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Ponera punctatissima jugata Forel" order="Hymenoptera" pageNumber="251" phylum="Arthropoda" rank="subSpecies" species="punctatissima" subSpecies="jugata">PONERA PUNCTATISSIMA, Roger. RACE: P. JUGATA</taxonomicName>
