<taxonomicName id="4C4BD09E8915FF949683FC5FFCD46ADB" authority="Bezzi. A. Epandrium" box="[471,870,911,933]" class="Insecta" family="Asteiidae" genus="Asteia" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="nigriceps">
