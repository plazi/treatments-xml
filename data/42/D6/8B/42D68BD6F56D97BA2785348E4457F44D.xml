, 
<taxonomicName authority="Steinhauser, 1989-22" authorityName="Steinhauser, 1989-22" class="Insecta" family="Hesperiidae" genus="Thessia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Thessia jalapus" order="Lepidoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="jalapus">Thessia jalapus</taxonomicName>
