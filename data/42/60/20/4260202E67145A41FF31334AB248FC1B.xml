<emphasis bold="true" box="[151,584,354,380]" pageId="1" pageNumber="413">
<taxonomicName box="[151,483,354,380]" class="Arachnida" family="Galumnidae" genus="Pergalumna" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="413" phylum="Arthropoda" rank="species" species="paratsurusakii" status="sp. nov.">
<emphasis bold="true" box="[151,483,354,380]" italics="true" pageId="1" pageNumber="413">Pergalumna paratsurusakii</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[491,584,354,380]" pageId="1" pageNumber="413" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
