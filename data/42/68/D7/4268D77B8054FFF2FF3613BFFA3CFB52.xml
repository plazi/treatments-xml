<collectingCountry box="[985,1067,151,172]" name="Trinidad and Tobago" pageId="18" pageNumber="509">Trinidad</collectingCountry>
(length 
<quantity box="[1147,1288,151,172]" metricMagnitude="-2" metricUnit="m" metricValue="3.6599999999999993" metricValueMax="3.8299999999999996" metricValueMin="3.4899999999999998" pageId="18" pageNumber="509" unit="mm" value="36.599999999999994" valueMax="38.3" valueMin="34.9">34.9–38.3 mm</quantity>
