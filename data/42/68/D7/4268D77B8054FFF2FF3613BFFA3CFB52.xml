<taxonomicName authority="Percheron" authorityName="Percheron" box="[1141,1436,180,202]" class="Insecta" family="Passallidae" genus="Passalus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="509" phylum="Arthropoda" rank="species" species="coarctatus">
<emphasis box="[1141,1332,180,202]" italics="true" pageId="18" pageNumber="509">Passalus coarctatus</emphasis>
