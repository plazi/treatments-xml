<number box="[646,658,2009,2030]" pageId="17" pageNumber="508" value="8.0">8</number>
and 
<number box="[704,716,2009,2030]" pageId="17" pageNumber="508" value="9.0">9</number>
glabrous, 
<number box="[816,840,2009,2030]" pageId="17" pageNumber="508" value="10.0">10</number>
pubescent in basal third (length 
<quantity box="[1153,1258,2009,2030]" metricMagnitude="-2" metricUnit="m" metricValue="3.8" metricValueMax="4.2" metricValueMin="3.4" pageId="17" pageNumber="508" unit="mm" value="38.0" valueMax="42.0" valueMin="34.0">34–42 mm</quantity>
)............. 
<number box="[1424,1436,2009,2030]" pageId="17" pageNumber="508" value="8.0">8</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="18" pageNumber="509">
<paragraph blockId="18.[151,1436,151,1170]" pageId="18" pageNumber="509">
<keyLead pageId="18" pageNumber="509">
<number box="[151,165,151,172]" pageId="18" pageNumber="509" value="8.0">8</number>
