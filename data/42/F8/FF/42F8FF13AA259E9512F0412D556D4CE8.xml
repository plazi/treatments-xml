<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:26DDB809-23F4-4AFD-BC9C-2573E1A85E4C" class="Actinopterygii" family="Cichlidae" genus="Chromidotilapia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Chromidotilapia nana Lamboj 2003:7" order="Perciformes" phylum="Chordata" rank="species" species="nana">Chromidotilapia nana</taxonomicName>
