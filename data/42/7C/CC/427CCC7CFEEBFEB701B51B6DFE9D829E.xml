Southern 
<collectingRegion box="[404,493,272,298]" country="Canada" name="Ontario" pageId="357" pageNumber="358">Ontario</collectingRegion>
south to 
<collectingRegion box="[602,779,272,298]" country="United States of America" name="South Carolina" pageId="357" pageNumber="358">South Carolina</collectingRegion>
(
<figureCitation box="[794,898,272,298]" captionStart="FIGURE 239" captionStartId="356.[151,255,1306,1330]" captionTargetBox="[172,1365,186,1282]" captionTargetId="figure-230@356.[172,1415,186,1282]" captionTargetPageId="356" captionText="FIGURE 239. Distribution of Lasioglossum weemsi (light grey) and L. yukonae (dark grey)." pageId="357" pageNumber="358">Fig. 239</figureCitation>
).
</paragraph>
<paragraph blockId="357.[151,1437,152,1018]" pageId="357" pageNumber="358">
