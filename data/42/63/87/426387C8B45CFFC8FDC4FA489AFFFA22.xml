<paragraph blockId="35.[189,1399,267,329]" pageId="35" pageNumber="36">
<quantity box="[189,322,267,294]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.2" metricValueMin="1.9" pageId="35" pageNumber="36" unit="mm" value="2.05" valueMax="2.2" valueMin="1.9">1.9–2.2 µm</quantity>
