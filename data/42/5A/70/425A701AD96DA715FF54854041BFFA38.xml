<typeStatus box="[680,783,1410,1436]" pageId="12" pageNumber="13">holotype</typeStatus>
collected in 
<collectingCountry box="[937,986,1410,1436]" name="Iceland" pageId="12" pageNumber="13">Ada</collectingCountry>
