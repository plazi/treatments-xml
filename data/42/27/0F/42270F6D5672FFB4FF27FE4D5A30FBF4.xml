, 
<taxonomicName authority="Ris, 1918" authorityName="Ris" authorityYear="1918" box="[295,530,1698,1723]" class="Insecta" family="Polythoridae" genus="Cora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="7" pageNumber="122" phylum="Arthropoda" rank="species" species="irene">
