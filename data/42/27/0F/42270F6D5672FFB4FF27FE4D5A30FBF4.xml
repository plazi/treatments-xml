, 
<taxonomicName authority="Selys, 1868" authorityName="Selys" authorityYear="1868" class="Insecta" family="Polythoridae" genus="Cora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="7" pageNumber="122" phylum="Arthropoda" rank="species" species="marina">
