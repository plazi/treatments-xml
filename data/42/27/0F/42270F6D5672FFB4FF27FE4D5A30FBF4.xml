<taxonomicName authorityName="Munz" authorityYear="1919" box="[1180,1326,1878,1903]" class="Insecta" family="Polythoridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="7" pageNumber="122" phylum="Arthropoda" rank="family">Polythoridae</taxonomicName>
