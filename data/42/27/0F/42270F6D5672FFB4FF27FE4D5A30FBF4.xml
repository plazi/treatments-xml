<taxonomicName authority="(Selys, 1873)" authorityName="Selys" authorityYear="1873" baseAuthorityName="Selys" baseAuthorityYear="1873" box="[641,908,1626,1651]" class="Insecta" family="Polythoridae" genus="Cora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="7" pageNumber="122" phylum="Arthropoda" rank="species" species="inca">
