Parry, 
<number id="031C2498FFE8FFD69118FB70CD2EADB5" box="[1067,1125,1181,1206]" pageId="1" pageNumber="287" value="1986.0">1986</number>
</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="72D44D3CFFE8FFD69197FB73C993ADD9" authority="Lazorko, 1974" authorityName="Lazorko" authorityYear="1974" class="Insecta" family="Chrysomelidae" genus="Crepidodera" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="287" phylum="Arthropoda" rank="species" species="spenceri">
<emphasis id="87A0EAADFFE8FFD69197FB73CC6EADB6" box="[1188,1317,1182,1205]" italics="true" pageId="1" pageNumber="287">C. spenceri</emphasis>
