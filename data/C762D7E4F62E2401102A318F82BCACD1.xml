<taxonomicName id="08A0EBA5FFDEE346060FAD8DEBE3F66D" class="Insecta" family="Coccinellidae" genus="Exochomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exochomus pubescens" order="Coleoptera" pageId="11" pageNumber="54" phylum="Arthropoda" rank="species" species="pubescens">Exochomus pubescens</taxonomicName>
