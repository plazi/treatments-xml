<taxonomicName id="4C3DFF8C0B4AC876FD8AFE35FEBF58AE" authority="Kieffer, 1924" authorityName="Kieffer" authorityYear="1924" box="[629,1004,396,419]" class="Insecta" family="Chironomidae" genus="Orthocladius" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="738" phylum="Arthropoda" rank="species" species="corax">
