</taxonomicName>
and 
<taxonomicName id="4C064D081916FFD1692EA7CE7391F856" authority="Steenis &amp; Hippa &amp; Mutin, 2018" authorityName="Steenis &amp; Hippa &amp; Mutin" authorityYear="2018" box="[792,957,1960,1987]" class="Insecta" family="Syrphidae" genus="Sphegina" kingdom="Animalia" order="Diptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="hauseri" status="sp. nov." subGenus="Asiosphegina">
