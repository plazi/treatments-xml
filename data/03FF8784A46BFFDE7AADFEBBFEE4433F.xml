<taxonomicName id="4C564D11A46BFFDE7BCDFE24FF4C4240" class="Mammalia" family="Talpidae" genus="Myogale" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="9" phylum="Chordata" rank="subSpecies" species="platysma" subSpecies="myoides">M. platysma myoides</taxonomicName>
