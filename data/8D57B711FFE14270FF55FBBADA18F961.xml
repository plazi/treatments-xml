<specimenCode id="5558AE7CFFE14270FE04FB48D906FB59" box="[454,597,1214,1240]" collectionCode="EBRG" country="Venezuela" httpUri="http://grbio.org/cool/ar2q-hqc7" name="Museo de la Estacion Biologia de Rancho Grande" pageId="34" pageNumber="35">EBRG 4666</specimenCode>
