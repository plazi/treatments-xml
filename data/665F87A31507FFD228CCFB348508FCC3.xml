<quantity id="290E9B501501FFD32BECF8F88069F8CF" box="[951,1054,1821,1846]" metricMagnitude="-4" metricUnit="m" metricValue="5.3" pageId="4" pageNumber="156" unit="mm" value="0.53">0.53 mm</quantity>
