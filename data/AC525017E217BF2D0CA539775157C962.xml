<taxonomicName id="4D1FEDBD5C66C42BFE4109D883DBC917" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181947" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole subreticulata Emery" lsidName-HNS="Pheidole subreticulata Emery" order="Hymenoptera" pageNumber="514" phylum="Arthropoda" rank="species" species="subreticulata">subreticulata</taxonomicName>
