<materialsCitation id="3B289D15FFDAB219F44D99D569256ABE" ID-GBIF-Occurrence="3428576305" collectingDate="1909-10" collectionCode="R, K, BLAT, CAL, BSI, MH, ICN" collectorName="Bhide" country="India" location="Bombay" pageId="6" pageNumber="109" specimenCode="K000245830, K000245831, K000245653" specimenCount="2" typeStatus="syntype">
<bibRefCitation id="EFD1EAB9FFDAB219F44D99D56AC16B87" author="Bhide, R. K." box="[283,434,1238,1264]" pageId="6" pageNumber="109" pagination="513 - 520" refId="ref1640" refString="Bhide, R. K. (1911) New and revised species of Gramineae from Bombay (with four plates). Journal and Proceedings of the Asiatic Society of Bengal 7: 513 - 520." type="journal article" year="1911">Bhide (1911)</bibRefCitation>
describing 
<taxonomicName id="4C40ECCBFFDAB219F76A99D568F16B87" box="[572,898,1238,1264]" class="Liliopsida" family="Poaceae" genus="Andropogon" kingdom="Plantae" order="Poales" pageId="6" pageNumber="109" phylum="Tracheophyta" rank="species" species="paranjpyeanum">
