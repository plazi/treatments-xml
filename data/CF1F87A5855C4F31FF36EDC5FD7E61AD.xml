<collectingCountry id="3FA17623855C4F31FC3EEE57FBE86121" box="[927,1038,1605,1631]" name="Australia" pageId="11" pageNumber="49">Australia</collectingCountry>
, and 
<collectingCountry id="3FA17623855C4F31FBEFEE57FB0A6121" box="[1102,1260,1605,1631]" name="New Zealand" pageId="11" pageNumber="49">New Zealand</collectingCountry>
