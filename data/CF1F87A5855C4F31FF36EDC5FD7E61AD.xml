<typeStatus id="980D8811855C4F31FB5CEE57FAD76121" box="[1277,1329,1605,1631]" pageId="11" pageNumber="49">type</typeStatus>
locality) (
<bibRefCitation id="23274B42855C4F31FF3EEE7EFE9861F8" author="Cunningham, G." box="[159,382,1644,1670]" pageId="11" pageNumber="49" refId="ref8163" refString="Cunningham, G. H, (1963). The Thelephoraceae of Australia &amp; New Zealand. New Zealand Department of Scientific and Industrial Research Bulletin 145, 359 pp." type="book" year="1963">Cunningham 1963</bibRefCitation>
