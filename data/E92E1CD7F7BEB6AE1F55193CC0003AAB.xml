<taxonomicName id="8C32B664330DD3BC5BED20D81D3D4854" authority="Wollebaek, 1912" authorityName="Wollebaek" authorityYear="1912" class="Polychaeta" family="Terebellidae" genus="Polycirrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polycirrus plumosus" order="Terebellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="plumosus">
Polycirrus plumosus 
<normalizedToken id="BCA6AE22F13DAAAC081197B5F7AA8586" originalValue="Wollebæk">Wollebaek</normalizedToken>
