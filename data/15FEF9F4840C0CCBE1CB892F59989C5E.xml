<taxonomicName id="D29769253FFD426BE084E2BDB0E014F2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:135273" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus (Myrmoturba) maculatus st. desantii Santschi" lsidName-HNS="Camponotus (Myrmoturba) maculatus st. desantii Santschi" order="Hymenoptera" pageNumber="280" phylum="Arthropoda" rank="subSpecies" species="maculatus" subGenus="Myrmoturba" subSpecies="desantii">Desantii</taxonomicName>
