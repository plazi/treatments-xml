<taxonomicName id="D9AD4D160E66FFC4FF7CA8D0FE5BFC3D" authority="Gerecke, 2003" authorityName="Gerecke" authorityYear="2003" box="[245,422,960,976]" class="Arachnida" family="Hygrobatidae" genus="Megapus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="93" pageNumber="234" phylum="Arthropoda" rank="variety" species="spinipes" status="n.v." variety="caucasica">
