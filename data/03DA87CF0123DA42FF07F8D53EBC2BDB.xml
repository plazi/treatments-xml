<taxonomicName id="4C734D5A0122DA42FC22FEDD38532BFF" authorityName="Carriker" authorityYear="1944" box="[946,1061,367,392]" class="Insecta" family="Philopteridae" genus="Pseudolipeurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="similis">
<emphasis id="B907EACB0122DA42FC22FEDD38532BFF" box="[946,1061,367,392]" italics="true" pageId="41" pageNumber="42">Ps. similis</emphasis>
</taxonomicName>
from 
<taxonomicName id="4C734D5A0122DA42FBF8FEDD39662BFF" baseAuthorityName="J. F. Gmelin" baseAuthorityYear="1789" box="[1128,1296,369,392]" class="Aves" family="Tinamidae" genus="Tinamus" kingdom="Animalia" order="Tinamiformes" pageId="41" pageNumber="42" phylum="Chordata" rank="species" species="major">
