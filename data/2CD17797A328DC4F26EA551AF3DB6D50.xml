<paragraph id="33CD603E3994F5AD866C537E4A8EAC8C" pageId="8" pageNumber="73">
<taxonomicName id="8AAC534B5B26B15131B359AE8292F64E" class="Sordariomycetes" family="Arctiidae" genus="Isia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Isia alcumena subsp. alcumena" order="Sordariales" pageId="8" pageNumber="73" phylum="Ascomycota" rank="subSpecies" species="alcumena" subSpecies="alcumena">Isia alcumena alcumena</taxonomicName>
