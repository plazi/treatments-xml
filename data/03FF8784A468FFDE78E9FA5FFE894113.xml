<taxonomicName id="4C564D11A468FFDD7E38F9B2FB2246CD" authorityName="Temminck" authorityYear="1841" box="[1036,1164,1660,1683]" class="Mammalia" family="Talpidae" genus="Urotrichus" kingdom="Animalia" order="Soricomorpha" pageId="9" pageNumber="10" phylum="Chordata" rank="genus">
