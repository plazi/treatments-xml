<emphasis id="B922EA80A46BFFDE7810FED7FD23416E" box="[548,653,281,304]" italics="true" pageId="10" pageNumber="11">Scalopus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C564D11A46BFFDE7A84FEF9FEB14110" box="[176,287,311,334]" class="Mammalia" family="Talpidae" genus="Scapanus" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="11" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46BFFDE7A84FEF9FEB14110" box="[176,287,311,334]" italics="true" pageId="10" pageNumber="11">Scapanus</emphasis>
