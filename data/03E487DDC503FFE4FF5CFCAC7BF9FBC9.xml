<taxonomicName id="4C4D4D48C503FFE4FF5CFCAC786EFC96" authority="(Zeller, 1847)" baseAuthorityName="Zeller" baseAuthorityYear="1847" box="[151,579,832,859]" class="Insecta" family="Tortricidae" genus="Pelochrista" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="15" phylum="Arthropoda" rank="species" species="mollitana">
