<taxonomicName id="DFE9784F83BA9E1B4C8B94F330755C4D" class="Insecta" family="Histeridae" genus="Dendrophilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dendrophilus kiteleyi" order="Coleoptera" pageId="4" pageNumber="15" phylum="Arthropoda" rank="species" species="kiteleyi">Dendrophilus kiteleyi</taxonomicName>
