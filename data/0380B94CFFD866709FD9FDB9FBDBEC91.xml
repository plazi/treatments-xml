<emphasis id="B95DD448FFD966709F89FF20FE8FEE98" bold="true" box="[199,353,222,248]" pageId="21" pageNumber="472">Distribution.</emphasis>
<taxonomicName id="4C2973D9FFD966709E23FF21FDC9EE99" baseAuthorityName="H. Milne Edwards" baseAuthorityYear="1853" box="[365,551,223,249]" class="Malacostraca" family="Sesarmidae" genus="Armases" kingdom="Animalia" order="Decapoda" pageId="21" pageNumber="472" phylum="Arthropoda" rank="species" species="ricordi">
