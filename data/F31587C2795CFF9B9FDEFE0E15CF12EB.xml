Vaney, 
<number id="CD7424F3795CFF9B9CFBFDFE12D01330" box="[946,1006,573,599]" pageId="5" pageNumber="6" value="1908.0">1908</number>
</bibRefCitation>
</taxonomicName>
with 
<taxonomicName id="BCBC4D57795CFF9B9B7BFDFE10061319" authority="Vaney, 1908" authorityName="Vaney" authorityYear="1908" class="Holothuroidea" family="Psolidae" genus="Psolidium" kingdom="Animalia" order="Dendrochirotida" pageId="5" pageNumber="6" phylum="Echinodermata" rank="species" species="coatsi" subGenus="Cucumaria">
