<taxonomicName id="4C2901BE046E153DFEAE4E92FE0AFF21" authorityName="Deshayes" authorityYear="1834" box="[397,481,187,213]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="103" pageNumber="104" phylum="Mollusca" rank="genus">
