<taxonomicName id="4C2901BE046E153DFB134E92FAF7FF21" box="[1072,1308,187,213]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="103" pageNumber="104" phylum="Mollusca" rank="species" species="lilaceocincta">
