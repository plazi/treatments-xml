<paragraph id="0F607C2A2254EE5646AC2AD61982732A" blockId="8.[151,688,1014,1040]" box="[151,688,1014,1040]" pageId="8" pageNumber="328">
<emphasis id="3DABA0382254EE5646AC2AD61AEC732A" bold="true" box="[151,478,1014,1040]" pageId="8" pageNumber="328">
<taxonomicName id="C8DF07A92254EE5646AC2AD61A48732A" box="[151,378,1014,1040]" class="Insecta" family="Cerambycidae" genus="Cacostola" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="328" phylum="Arthropoda" rank="species" species="carinata" status="sp. nov.">
<emphasis id="3DABA0382254EE5646AC2AD61A48732A" bold="true" box="[151,378,1014,1040]" italics="true" pageId="8" pageNumber="328">Cacostola carinata</emphasis>
