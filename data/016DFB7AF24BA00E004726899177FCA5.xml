sp. n. 4 
<taxonomicName id="E956909D98CF7C05F9B8E42A9B3A7DF5" genus="P." lsidName="P. pacchanensis" pageId="10" pageNumber="23" rank="species" species="pacchanensis">P. pacchanensis</taxonomicName>
sp. n. 5 
<taxonomicName id="63DF0F2DDE2803EF3D825CCC1EAE45F1" genus="P." lsidName="P. spirulata" pageId="10" pageNumber="23" rank="species" species="spirulata">P. spirulata</taxonomicName>
sp. n. 6 
<taxonomicName id="179B439505C216EF986526406008689A" genus="P." lsidName="P. xiezi" pageId="10" pageNumber="23" rank="species" species="xiezi">P. xiezi</taxonomicName>
sp. n. 7 
<taxonomicName id="F5C6A83928876F4182B1024ED139C7FD" genus="P." lsidName="P. zhenzhuang" pageId="10" pageNumber="23" rank="species" species="zhenzhuang">P. zhenzhuang</taxonomicName>
sp. n.
</paragraph>
</caption>
