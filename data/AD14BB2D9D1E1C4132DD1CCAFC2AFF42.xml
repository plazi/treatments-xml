<taxonomicName id="E2BD71B89D1E1C4E30761B17FD23F8F6" box="[572,707,1866,1889]" class="Insecta" family="Cecidomyiidae" genus="Anocha" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="282" phylum="Arthropoda" rank="species" species="moraviensis">
