<collectingRegion id="4990F84EFFFC2E3B668BFF05D11BCAE2" box="[739,852,188,213]" country="Japan" name="Hokkaido" pageId="16" pageNumber="17">Hokkaido</collectingRegion>
, 
<collectingCountry id="F343763CFFFC2E3B6737FF04D1EDCAE3" box="[863,930,189,212]" name="Japan" pageId="16" pageNumber="17">Japan</collectingCountry>
