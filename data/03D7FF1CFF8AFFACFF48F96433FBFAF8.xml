<taxonomicName id="4C7E3589FF8AFFACFD7CF93635E7FD80" authorityName="Laporte" authorityYear="1840" box="[756,1179,600,624]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="64" phylum="Arthropoda" rank="species" species="rutilans" subGenus="Francmonrosia">
