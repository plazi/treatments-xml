, and 
<emphasis id="25938230FFBC100DFD6283D373AE5ADE" box="[730,759,1765,1790]" italics="true" pageId="4" pageNumber="61">
h 
<number id="A12F4C05FFBC100DFD5183D373AE5ADE" box="[745,759,1765,1790]" pageId="4" pageNumber="61" value="2.0">2</number>
