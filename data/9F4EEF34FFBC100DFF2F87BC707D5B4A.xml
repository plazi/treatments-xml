<emphasis id="25938230FFBC100DFBB083D075ED5ADE" box="[1032,1204,1765,1790]" italics="true" pageId="4" pageNumber="61">
<taxonomicName id="D0E725A1FFBC100DFBB083D075E95ADE" box="[1032,1200,1765,1790]" class="Arachnida" family="Syringophilidae" genus="Syringophiloidus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="bombycillae">S. bombycillae</taxonomicName>
