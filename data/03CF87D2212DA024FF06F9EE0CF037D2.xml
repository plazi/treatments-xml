<taxonomicName id="4C664D472123A024FA10FDD308273723" authority="Dyar." authorityName="Dyar." class="Insecta" family="Lymantriidae" genus="Olene" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="213" phylum="Arthropoda" rank="species" species="grisefacta">
