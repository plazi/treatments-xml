<geoCoordinate id="92003B12FC7A571408EC3618FED15639" direction="north" orientation="latitude" precision="925" value="26.366667">26°22'N</geoCoordinate>
, 
<geoCoordinate id="2047317987F54E56DD794F8EA430E5FF" direction="east" orientation="longitude" precision="925" value="107.38333">107°23'E</geoCoordinate>
