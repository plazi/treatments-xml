(
<taxonomicName id="37CED1953B74E014F8194CB8DEF98272" class="Polypodiopsida" family="Dryopteridaceae" genus="Dryopteris" kingdom="Plantae" order="Polypodiales" pageId="null" pageNumber="320" phylum="Tracheophyta" rank="species" species="lobata">
