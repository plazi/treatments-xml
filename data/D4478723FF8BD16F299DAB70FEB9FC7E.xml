<taxonomicName id="9BEE4DB6FF88D16F283DAD22FDB6FE8E" authority="Masi" authorityName="Masi" box="[311,560,260,285]" class="Insecta" family="Pteromalidae" genus="Callocleonymus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="genus">
<emphasis id="6E9AEA27FF88D16F283DAD22FE6AFE8E" box="[311,492,260,285]" italics="true" pageId="2" pageNumber="424">Callocleonymus</emphasis>
Masi
</taxonomicName>
, 
<taxonomicName id="9BEE4DB6FF88D16F2B37AD22FCA3FE8E" authority="Latreille" authorityName="Latreille" box="[573,805,260,285]" class="Insecta" family="Pteromalidae" genus="Cleonymus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="genus">
<emphasis id="6E9AEA27FF88D16F2B37AD22FD3CFE8E" box="[573,698,260,285]" italics="true" pageId="2" pageNumber="424">Cleonymus</emphasis>
Latreille
</taxonomicName>
, 
<taxonomicName id="9BEE4DB6FF88D16F2A38AD22FBC6FE8E" authority="Gibson" authorityName="Gibson" box="[818,1088,260,285]" class="Insecta" family="Pteromalidae" genus="Dasycleonymus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="genus">
<emphasis id="6E9AEA27FF88D16F2A38AD22FC64FE8E" box="[818,994,260,285]" italics="true" pageId="2" pageNumber="424">Dasycleonymus</emphasis>
Gibson
</taxonomicName>
, 
<taxonomicName id="9BEE4DB6FF88D16F2D47AD23FB3BFE8F" box="[1101,1213,261,284]" class="Insecta" family="Pteromalidae" genus="Notanisus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="genus">
<emphasis id="6E9AEA27FF88D16F2D47AD23FB3BFE8F" box="[1101,1213,261,284]" italics="true" pageId="2" pageNumber="424">Notanisus</emphasis>
</taxonomicName>
