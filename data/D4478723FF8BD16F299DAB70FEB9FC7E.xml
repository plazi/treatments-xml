<taxonomicName id="9BEE4DB6FF88D16F2A6CAD0FFC50FED3" box="[870,982,297,320]" class="Insecta" family="Pteromalidae" genus="Notanisus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="genus">
<emphasis id="6E9AEA27FF88D16F2A6CAD0FFC50FED3" box="[870,982,297,320]" italics="true" pageId="2" pageNumber="424">Notanisus</emphasis>
</taxonomicName>
