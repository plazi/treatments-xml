<taxonomicName id="9BEE4DB6FF88D16F2C88AD0FFE9CFEF7" class="Insecta" family="Pteromalidae" genus="Notanisus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="species" species="sexramosus">
