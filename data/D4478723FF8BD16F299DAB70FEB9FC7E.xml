<taxonomicName id="9BEE4DB6FF88D16F2DF2AD22FE9EFEAC" authority="Risbec. He" authorityName="Risbec. He" class="Insecta" family="Pteromalidae" genus="Zolotarewskya" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="424" phylum="Arthropoda" rank="genus">
<emphasis id="6E9AEA27FF88D16F2DF2AD22FA1AFE8E" box="[1272,1436,260,285]" italics="true" pageId="2" pageNumber="424">Zolotarewskya</emphasis>
