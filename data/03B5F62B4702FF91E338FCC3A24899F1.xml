<quantity id="4CE4EAD84702FF90E14DFB74A5839BDF" box="[856,909,1185,1209]" metricMagnitude="1" metricUnit="m" metricValue="2.6" pageId="22" pageNumber="852" unit="m" value="26.0">26 m</quantity>
