<taxonomicName id="8A4C9AD70AA35C08798878FFDF0DF2B7" authorityName="Hu" authorityYear="1940" baseAuthorityName="Pierre" class="Magnoliopsida" family="Magnoliaceae" genus="Paramichelia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Paramichelia baillonii" order="Magnoliales" pageId="0" pageNumber="43" phylum="Tracheophyta" rank="species" species="baillonii">
<emphasis id="D48A2AC7A29F26BC6ED617A7130EFB2D" italics="true" pageId="0" pageNumber="43">Paramichelia baillonii</emphasis>
</taxonomicName>
