<taxonomicName id="E2618B9C93B05638A0D6F7F314B8BB0A" authorityName="Fabricius" authorityYear="1794" class="Hexapoda" family="Carabidae" genus="Cychrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="genus">
