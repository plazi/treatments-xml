<number id="3DCC24E6FFB4FFE2FAF5E679FAB9D66A" box="[1309,1361,295,319]" pageId="6" pageNumber="45" value="1.18">1.18</number>
; metatarsus, 
<number id="3DCC24E6FFB4FFE2FF0FE612FEF3D631" box="[231,283,332,356]" pageId="6" pageNumber="45" value="0.92">0.92</number>
; tarsus, 
<number id="3DCC24E6FFB4FFE2FE90E612FE45D631" box="[376,429,332,356]" pageId="6" pageNumber="45" value="0.59">0.59</number>
