<collectionCode id="ED38AE18FFC30722FB19922FFAC7A777" box="[1275,1358,1574,1601]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="45" pageNumber="46" type="Museum">BPBM</collectionCode>
