<taxonomicName id="59CF53F9FFD1FFADFEE4FA679DC4F9DA" authority="Assing, 2012" authorityName="Assing" authorityYear="2012" box="[290,459,1520,1542]" class="Insecta" family="Staphylinidae" genus="Masuria" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="1028" phylum="Arthropoda" rank="species" species="subnitens" status="nov.sp.">
