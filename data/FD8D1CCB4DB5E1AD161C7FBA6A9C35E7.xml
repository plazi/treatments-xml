<taxonomicName id="FBEBE25B1324BF6E48C0D22130BD8557" class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="571" phylum="Tracheophyta" rank="genus">Chrysanthemum</taxonomicName>
