<subSubSection id="C32195D678441072B6E85235FE96FA3F" pageId="17" pageNumber="364" type="diagnosis">
<paragraph id="8B84C65D78441072B6E85235FE96FA3F" blockId="17.[140,1108,1258,1672]" pageId="17" pageNumber="364">
<emphasis id="B94F1A4F78441072B6E85235FEF4FA1B" bold="true" box="[185,312,1398,1424]" pageId="17" pageNumber="364">
<emphasis id="B94F1A4F78441072B6E85235FEFFFA1B" box="[185,307,1398,1424]" inLineHeading="true" pageId="17" pageNumber="364" reason="1">Diagnosis</emphasis>
.
</emphasis>
Fossil 
<taxonomicName id="4C3BBDDE78441072B7DB5234FE23FA1A" authorityName="Martens" authorityYear="1969" box="[394,495,1399,1425]" class="Arachnida" family="Phalangiidae" genus="Amilenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="17" pageNumber="364" phylum="Arthropoda" rank="genus">
