<emphasis id="BAFDEAC20117FFE23108F4F6FDACF4FB" box="[307,595,2840,2885]" italics="true" pageId="0" pageNumber="272">
<taxonomicName id="4F894D530117FFE23108F4F6FDB2F4FB" authority="Fisher, 1850" authorityName="Fisher" authorityYear="1850" box="[307,589,2840,2885]" class="Insecta" family="Cicadellidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="272" phylum="Arthropoda" rank="species" species="cassinii" status="sp. nov.">Cicada Cassinii</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A1CE57B90117FFE2325FF4F6FD31F4FB" box="[612,718,2840,2885]" pageId="0" pageNumber="272">nobis</taxonomicNameLabel>
