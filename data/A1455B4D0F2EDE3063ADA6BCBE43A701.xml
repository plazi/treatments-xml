<geoCoordinate id="011BF6085BA0E8A430713A4B384F53A7" direction="north" orientation="latitude" precision="9" value="34.086334">34°05.18'N</geoCoordinate>
, 
<geoCoordinate id="E2D335517A6183B3B7DDF443FC5AC239" direction="east" orientation="longitude" precision="9" value="107.70133">107°42.08'E</geoCoordinate>
