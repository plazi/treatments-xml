14. 
<taxonomicName id="FC6A0FFA985EBD4EE34A5BAD2048B927" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145404" class="Insecta" family="Formicidae" genus="Eciton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eciton rufonigrum Jerdon" lsidName-HNS="Eciton rufonigrum Jerdon" order="Hymenoptera" pageNumber="111" phylum="Arthropoda" rank="species" species="rufonigrum">ECITON? rufonigrum</taxonomicName>
