(
<figureCitation id="130E86EDBD2CFF84FF7BFEADFEA24310" box="[159,294,356,381]" captionStart="FIGURES 61 – 66" captionStartId="30.[151,264,1911,1934]" captionTargetBox="[179,1407,599,1883]" captionTargetId="figure@30.[166,1420,589,1890]" captionTargetPageId="30" captionText="FIGURES 61 – 66. Boreantrops male terminalia. B. subfoveolatus: 61) phallus, postgonite, and phallapodeme, lateral view, 62) sternite 5; B. suchixtepecensis: 63) phallus, postgonite, and phallapodeme, lateral view, 64) sternite 5; B. zacapa: 65) phallus, postgonite, and phallapodeme, lateral view, 66) sternite 5." httpUri="https://zenodo.org/record/240910/files/figure.png" pageId="31" pageNumber="332">Figs. 63, 64</figureCitation>
, 
<number id="3DFD884FBD2CFF84FED6FEADFE034311" box="[306,391,356,381]" pageId="31" pageNumber="332" value="76119.0">76, 119</number>
)
