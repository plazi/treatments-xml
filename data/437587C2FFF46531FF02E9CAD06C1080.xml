<taxonomicName id="0CDC4D57FFF46531FBE0EA1CD0F7163B" authorityName="Linnaeus" authorityYear="1758" box="[1141,1245,579,605]" class="Bivalvia;Caudofoveata;Cephalopoda;Gastropoda;Monoplacophora;Polyplacophora;Scaphopoda;Solenogastres" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mollusca" pageId="48" pageNumber="49" phylum="Mollusca" rank="order">Mollusca</taxonomicName>
