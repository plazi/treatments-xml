<collectionCode id="ADCDAE11FFF46531FD68EA1BD74D1638" box="[765,871,580,606]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="48" pageNumber="49" type="Museum">NHMUK</collectionCode>
