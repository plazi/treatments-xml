<taxonomicName id="4C4D4D6AE45EEE55FF49FB6CFA5656ED" authority="(J. Sahlberg 1901)" baseAuthorityName="J. Sahlberg" baseAuthorityYear="1901" box="[151,580,1172,1199]" class="Insecta" family="Ciidae" genus="Orthocis" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="117" phylum="Arthropoda" rank="species" species="linearis">
<emphasis id="B939EAFBE45EEE55FF49FB6CF97356EC" bold="true" box="[151,353,1172,1199]" italics="true" pageId="6" pageNumber="117">Orthocis linearis</emphasis>
(J. Sahlberg 1901)
</taxonomicName>
(
<taxonomicName id="4C4D4D6AE45EEE55FD8DFB6CFAB756ED" authorityName="Leach" authorityYear="1819" box="[595,677,1172,1198]" class="Insecta" family="Ciidae" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="117" phylum="Arthropoda" rank="family">Ciidae</taxonomicName>
