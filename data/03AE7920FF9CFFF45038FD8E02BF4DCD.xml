, 
<taxonomicName id="4C07B3B5FF9CFFFA5591F9FB012B4AAD" authority="(Pic, 1922)" baseAuthorityName="Pic" baseAuthorityYear="1922" class="Insecta" family="Staphylinidae" genus="Bironium" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="234" phylum="Arthropoda" rank="species" species="tonkineum">
