and 
<taxonomicName id="4C07B3B5FF9CFFFA530EF9DB00544AAD" authority="Lobl, 2020" authorityName="Lobl" authorityYear="2020" box="[595,865,1641,1665]" class="Insecta" family="Staphylinidae" genus="Bironium" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="234" phylum="Arthropoda" rank="species" species="yunnanum">
