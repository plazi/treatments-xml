<taxonomicName id="4C427CC8693DFFD6FD8D377A8A3AFD9B" authority="Soto-Adames, 2017" authorityName="Soto-Adames" authorityYear="2017" box="[615,830,579,603]" class="Entognatha" family="Neanuridae" genus="Paleonura" kingdom="Animalia" order="Collembola" pageId="4" pageNumber="392" phylum="Arthropoda" rank="species" species="boricana" status="sp. nov.">
