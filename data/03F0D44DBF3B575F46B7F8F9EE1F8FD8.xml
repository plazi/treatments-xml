<taxonomicName id="4C591ED8BF3B575F46B7F88AED418F91" box="[151,356,1916,1940]" class="Magnoliopsida" family="Acanthaceae" genus="Avicennia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="2" pageNumber="223" phylum="Tracheophyta" rank="species" species="marina">
