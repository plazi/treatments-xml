<taxonomicName id="4C755FDA7F02FFCAFEE47018BC61E3BC" box="[377,591,1132,1156]" class="Magnoliopsida" family="Solanaceae" genus="Nicotiana" kingdom="Plantae" order="Solanales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="tabacum">
