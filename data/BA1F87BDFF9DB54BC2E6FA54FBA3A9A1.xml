<taxonomicName id="F5B64D28FF9DB54BC344FA1DFD95ABB9" box="[309,605,1498,1523]" class="Insecta" family="Mantoididae" genus="Paramantoida" kingdom="Animalia" order="Mantodea" pageId="3" pageNumber="197" phylum="Arthropoda" rank="species" species="amazonica" status="n. sp.">
<emphasis id="00C2EAB9FF9DB54BC344FA1DFD95ABB9" bold="true" box="[309,605,1498,1523]" italics="true" pageId="3" pageNumber="197">Paramantoida amazonica</emphasis>
</taxonomicName>
<taxonomicNameLabel id="1BF157C2FF9DB54BC015FA1CFD68ABB8" box="[612,672,1499,1522]" pageId="3" pageNumber="197" rank="species">n. sp.</taxonomicNameLabel>
