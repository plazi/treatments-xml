<collectionCode id="ED26AE33D37AD14F1F03F958CFB231D7" LSID="urn:lsid:biocol.org:col:34925" box="[831,922,1723,1747]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="3" pageNumber="63">AMNH</collectionCode>
