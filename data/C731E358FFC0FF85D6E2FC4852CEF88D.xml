<taxonomicName id="889829CDFFC7FF86D632F85B5013F859" authority="Belokobylskij" authorityName="Belokobylskij" authorityYear="2021" box="[284,864,1952,1979]" class="Insecta" family="Braconidae" genus="Doryctes" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="arrujumi" status="sp. nov." subGenus="Neodoryctes">
