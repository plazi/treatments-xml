subgenus 
<taxonomicName id="4C144D2DFF9DFF86FED6FDF8CC0B1D27" authority="Gehin, 1876" authorityName="Gehin" authorityYear="1876" box="[274,587,548,574]" class="Insecta" family="Carabidae" genus="Oreocarabus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis id="B960EABCFF9DFF86FED6FDF8CFEE1D27" bold="true" box="[274,430,548,574]" italics="true" pageId="33" pageNumber="34">Oreocarabus</emphasis>
Géhin, 
<number id="3DDC2489FF9DFF86FDD4FDF8CC0B1D27" box="[528,587,548,574]" pageId="33" pageNumber="34" value="1876.0">1876</number>
</taxonomicName>
