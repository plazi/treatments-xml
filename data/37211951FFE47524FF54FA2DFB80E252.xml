<location id="BA57FE9CFFE47524FC06FA78FBDFE0AC" LSID="urn:lsid:plazi:treatment:37211951FFE47524FF54FA2DFB80E252:BA57FE9CFFE47524FC06FA78FBDFE0AC" box="[1020,1111,1442,1466]" country="Turkey" county="Olivier" municipality="Michaux" name="Cullen" pageId="20" pageNumber="107">Cullen</location>
