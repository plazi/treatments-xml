], 
<collectionCode id="D9993082FFE47524FCC2FA78FCC0E0AF" box="[824,840,1442,1465]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="20" pageNumber="107" type="Herbarium">P</collectionCode>
