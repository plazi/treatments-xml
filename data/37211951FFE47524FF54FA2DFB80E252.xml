<location id="BA57FE9CFFE47524FB84FA18FB3FE0CC" LSID="urn:lsid:plazi:treatment:37211951FFE47524FF54FA2DFB80E252:BA57FE9CFFE47524FB84FA18FB3FE0CC" box="[1150,1207,1474,1498]" country="Turkey" county="Olivier" municipality="Michaux" name="Flora" pageId="20" pageNumber="107">Flora</location>
