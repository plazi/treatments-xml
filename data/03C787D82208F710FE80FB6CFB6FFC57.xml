<date id="FFD0100E2208F711FC47FB1FFBE1FAD6" box="[975,1117,1279,1305]" pageId="8" pageNumber="9" value="1998-09-15">
<number id="3DA624E92208F711FC47FB1FFC57FAD6" box="[975,1003,1279,1305]" pageId="8" pageNumber="9" value="15.0">15</number>
­IX­ 
<number id="3DA624E92208F711FB97FB1FFBE1FAD6" box="[1055,1117,1279,1305]" pageId="8" pageNumber="9" value="1998.0">1998</number>
</date>
. J. Márquez col./ 
<taxonomicName id="4C6E4D4D2208F711FE80FAC7FD05FA8E" authority="Campbell, 1974" authorityName="Campbell" authorityYear="1974" box="[264,697,1319,1345]" class="Agaricomycetes" family="Staphylinidae" genus="Oxyporus" higherTaxonomySource="CoL" kingdom="Fungi" order="Coleoptera" pageId="8" pageNumber="9" phylum="Basidiomycota" rank="species" species="lawrencei">
