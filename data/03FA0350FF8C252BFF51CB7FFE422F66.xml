<taxonomicName id="4C53C9C5FF8B252AFEEACAD2FDCA2AFB" authorityName="Grismer, Sumontha, Cota, Grismer, Wood, Pauwels &amp; Kunya" authorityYear="2010" box="[300,548,1679,1701]" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="niyomwanae">
