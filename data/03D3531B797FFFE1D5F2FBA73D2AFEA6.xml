, 
<collectingRegion id="49BE2CEF797DFFE0D603FACE3C41FAC6" box="[784,874,1337,1360]" country="Canada" name="Alberta" pageId="18" pageNumber="19">Alberta</collectingRegion>
, 
<collectingMunicipality id="6BA17877797DFFE0D693FACE3B2CFAC6" box="[896,1031,1337,1360]" pageId="18" pageNumber="19">Campanian</collectingMunicipality>
, 
<location id="8EA5B4D6797DFFE0D10EFACE3C3BFAFB" LSID="urn:lsid:plazi:treatment:03D3531B797FFFE1D5F2FBA73D2AFEA6:8EA5B4D6797DFFE0D10EFACE3C3BFAFB" county="Grassy Lake" municipality="Campanian" name="Foremost Formation" pageId="18" pageNumber="19" stateProvince="Alberta">Foremost Formation</location>
, 
<location id="8EA5B4D6797DFFE0D60DFAA13C84FAFB" LSID="urn:lsid:plazi:treatment:03D3531B797FFFE1D5F2FBA73D2AFEA6:8EA5B4D6797DFFE0D60DFAA13C84FAFB" box="[798,943,1366,1389]" county="Grassy Lake" municipality="Campanian" name="Formicidae'" pageId="18" pageNumber="19" stateProvince="Alberta">Formicidae’</location>
’ (figs. 2, 10). 
<location id="8EA5B4D6797DFFE0D144FAA13B86FAFB" LSID="urn:lsid:plazi:treatment:03D3531B797FFFE1D5F2FBA73D2AFEA6:8EA5B4D6797DFFE0D144FAA13B86FAFB" box="[1111,1197,1366,1389]" county="Grassy Lake" municipality="Campanian" name="Worker" pageId="18" pageNumber="19" stateProvince="Alberta">Worker</location>
