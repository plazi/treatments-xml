<taxonomicName id="4C659A98F3053429FD94F2783C93BFA7" box="[559,648,1914,1937]" class="Insecta" family="Hesperiidae" genus="Lobocla" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="49" phylum="Arthropoda" rank="genus">Lobocla</taxonomicName>
