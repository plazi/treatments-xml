<taxonomicName id="4C659A98F3053429FD23F2783D21BFA7" authority="Sonan" authorityName="Sonan" box="[664,826,1914,1937]" class="Insecta" family="Hesperiidae" genus="Lobocla" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="49" phylum="Arthropoda" rank="species" species="kodairai">kodairai Sonan</taxonomicName>
/ DET. J. 
<collectionCode id="ED7479DEF3053429FC1FF2783A1EBFA7" box="[932,1029,1914,1937]" pageId="1" pageNumber="49">SONAN</collectionCode>
,” “Hori/ 
<date id="FFDBC7DBF3053429FBD3F2783AF7BFA7" box="[1128,1260,1914,1937]" pageId="1" pageNumber="49" value="1934-07-27">
<number id="3DADF33CF3053429FBD3F2783A98BFA7" box="[1128,1155,1914,1937]" pageId="1" pageNumber="49" value="27.0">27</number>
VII 
<number id="3DADF33CF3053429FB03F2783AF7BFA7" box="[1208,1260,1914,1937]" pageId="1" pageNumber="49" value="1934.0">1934</number>
</date>
/ K. Kodaira,” “ No. 
<number id="3DADF33CF3053429FF4EF2983F08BF87" box="[245,275,1946,1969]" pageId="1" pageNumber="49" value="49.0">49</number>
