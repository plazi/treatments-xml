<taxonomicName id="8B70B1AAEE454C56F562D1073CE20C5E" class="Insecta" family="Derodontidae" genus="Laricobius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Laricobius" order="Coleoptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="genus">Laricobius</taxonomicName>
