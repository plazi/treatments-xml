<quantity id="4CB80997FF8D973C46EA5AF6FDF6F86B" box="[446,517,1996,2022]" metricMagnitude="2" metricUnit="m" metricValue="2.4" pageId="7" pageNumber="510" unit="m" value="240.0">240 m</quantity>
– 
<specimenCount id="9D466FFBFF8D973C45475AF6FD92F86A" box="[531,609,1996,2023]" pageId="7" pageNumber="510" type="generic">
<quantity id="4CB80997FF8D973C45475AF6FDAEF86A" box="[531,605,1996,2023]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="7" pageNumber="510" unit="m" value="600.0">600 m</quantity>
