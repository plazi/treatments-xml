<number id="3D8DCC490C0E7535A2EBDBD4FE32FF5F" box="[423,479,223,248]" pageId="7" value="2005.0">2005</number>
</taxonomicName>
and 
<taxonomicName id="4C45A5ED0C0E7535A16CDBEAFC16FF5F" authority="Fomichev &amp; Marusik, 2013" authorityName="Fomichev &amp; Marusik" authorityYear="2013" box="[544,1019,223,248]" class="Arachnida" family="Filistatidae" genus="Zaitunia" kingdom="Animalia" order="Araneae" pageId="7" phylum="Arthropoda" rank="species" species="zonsteini">
