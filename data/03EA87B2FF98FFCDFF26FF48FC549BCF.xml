<taxonomicName id="4C434D27FF98FFCDFB3BFEDCFB7A9B2B" box="[1162,1233,330,355]" class="Entognatha" family="Paronellidae" genus="Salina" kingdom="Animalia" order="Collembola" pageId="38" pageNumber="339" phylum="Arthropoda" rank="genus">
