of 
<taxonomicName id="4C1A4D238622CF6F98F73C3DFB025069" authority="Jedlicka 1955" authorityName="Jedlicka" authorityYear="1955" box="[902,1181,1826,1851]" class="Insecta" family="Carabidae" genus="Euschizomerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="365" phylum="Arthropoda" rank="species" species="indicus">
<emphasis id="B96EEAB28622CF6F98F73C3DFC675069" box="[902,1016,1827,1851]" italics="true" pageId="4" pageNumber="365">E. indicus</emphasis>
