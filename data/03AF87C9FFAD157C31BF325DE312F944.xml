<taxonomicName id="4C064D5CFFAD157C33173220E5A8F96F" box="[575,716,1656,1681]" class="Entognatha" family="Isotomidae" genus="Isotomiella" kingdom="Animalia" order="Collembola" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="granulata">
