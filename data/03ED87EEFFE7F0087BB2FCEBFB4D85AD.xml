<taxonomicName id="4C444D7BFFE7F0087BD2FBC8FD70861D" box="[247,703,1102,1128]" class="Palaeacanthocephala" family="Polymorphidae" genus="Polymorphus" kingdom="Animalia" order="Polymorphida" pageId="8" pageNumber="65" phylum="Acanthocephala" rank="species" species="cetaceum" subGenus="Polymorphus">
<emphasis id="B930EAEAFFE7F0087BD2FBC8FE5C861D" box="[247,403,1103,1128]" italics="true" pageId="8" pageNumber="65">Polymorphus</emphasis>
