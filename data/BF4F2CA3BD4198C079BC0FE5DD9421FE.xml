<taxonomicName id="976892D4FD5CF0A34E367F9AD369478E" authority="Busck" class="Insecta" family="Ethmiidae" genus="Ethmia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ethmia festiva" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="festiva">Ethmia festiva Busck</taxonomicName>
