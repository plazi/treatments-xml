<taxonomicName id="4C664D098B17FFA4FDF4F9CCFCA2F92A" authority="Meigen" authorityName="Meigen" box="[595,891,1655,1681]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="aurulans">
<emphasis id="B912EA988B17FFA4FDF4F9CCFCCEF92B" box="[595,791,1655,1680]" italics="true" pageId="89" pageNumber="90">Phasia aurulans</emphasis>
