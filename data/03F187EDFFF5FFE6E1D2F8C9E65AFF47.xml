<taxonomicName id="4C584D78FFEAFFE4E063FB0AE149FB23" authority="Buckle" authorityName="Buckle" box="[294,633,1158,1180]" class="Insecta" family="Staphylinidae" genus="Brachygluta" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="miccosukee">
