<taxonomicName id="F5CCB988FFE5FFA771F9271AFB8A213B" box="[1006,1064,1564,1587]" class="Pinopsida" family="Taxaceae" genus="Taxus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="2" pageNumber="595" phylum="Tracheophyta" rank="genus">
<emphasis id="00B81E19FFE5FFA771F9271AFB8A213B" box="[1006,1064,1564,1587]" italics="true" pageId="2" pageNumber="595">Taxus</emphasis>
