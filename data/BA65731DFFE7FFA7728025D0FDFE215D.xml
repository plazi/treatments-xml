<quantity id="F5346FEEFFE5FFA7761D24FFFB122118" box="[1034,1200,1529,1552]" metricMagnitude="3" metricUnit="m" metricValue="2.45" metricValueMax="2.7" metricValueMin="2.2" pageId="2" pageNumber="595" unit="m" value="2450.0" valueMax="2700.0" valueMin="2200.0">
<date id="4672E4CBFFE5FFA7761D24FFFB372118" box="[1034,1173,1529,1552]" pageId="2" pageNumber="595" value="2200" valueMax="2700">2200 to 2700</date>
m
