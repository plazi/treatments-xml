<taxonomicName id="F5CCB988FFE5FFA7766E271AFB1C213B" box="[1145,1214,1564,1587]" class="Insecta" family="Diapriidae" genus="Betula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="595" phylum="Arthropoda" rank="genus">
