<taxonomicName id="73ECAFBE6866FF9FFC1E3062FA8FFB73" authority="(Cameron)" baseAuthorityName="Cameron" box="[925,1343,1594,1619]" class="Insecta" family="Mutillidae" genus="Traumatomutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="species" species="phya">
