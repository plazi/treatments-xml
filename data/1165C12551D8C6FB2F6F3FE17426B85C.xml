<taxonomicName id="710EB6BAD2D5DE8341850DBCCDCCD4A0" family="Euphorbiaceae" genus="A." kingdom="Plantae" lsidName="A. madagascariensis" order="Malpighiales" pageId="0" pageNumber="85" rank="species" species="madagascariensis">
