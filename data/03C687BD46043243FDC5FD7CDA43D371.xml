<taxonomicName id="4C6F4D2846073243FD2FFE4BDC71D371" authorityName="Kirby" authorityYear="1837" box="[662,837,491,517]" class="Insecta" family="Scarabaeidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="tribe" tribe="Macrodactylini">Macrodactylini</taxonomicName>
