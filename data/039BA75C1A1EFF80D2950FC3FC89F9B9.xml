<quantity id="4CCABBAF1A1FFF80D1E30DFBFC61FB25" box="[954,1010,1170,1187]" metricMagnitude="-2" metricUnit="m" metricValue="2.2" pageId="65" pageNumber="376" unit="mm" value="22.0">22 mm</quantity>
