<specimenCount id="DDA8FD5FA87CFFA3FCB1FAB2FC700C67" box="[804,973,1383,1405]" pageId="100" pageNumber="100" type="female">eight females</specimenCount>
</materialsCitation>
; Abrilongo, 
<specimenCount id="DDA8FD5FA87CFFA3FBE8FABDFD5D0C81" pageId="100" pageNumber="100" type="male">two males</specimenCount>
; Tapirapoan, 
<specimenCount id="DDA8FD5FA87CFFA3FC1AFA50FBB00C81" box="[911,1037,1413,1435]" pageId="100" pageNumber="100" type="male">two males</specimenCount>
, 
<specimenCount id="DDA8FD5FA87CFFA3FB8BFA50FB180C81" box="[1054,1189,1413,1435]" pageId="100" pageNumber="100" type="female">one female</specimenCount>
; 
<materialsCitation id="7BC63C8BA87CFFA3FD0EFA77FB260FC4" collectionCode="AMNH" collectorName="Utiarity &amp; Many &amp; H. H. Smith &amp; Zimmer &amp; The &amp; Smith" pageId="100" pageNumber="100" specimenCode="AMNH 31348-31350, 31353, 31354, 31356, 31357, 31359, 31364" specimenCount="1" specimenCount-female="1" typeStatus="paratype">
<collectorName id="665B5300A87CFFA3FD0EFA77FD460CA2" box="[667,763,1442,1464]" pageId="100" pageNumber="100">Utiarity</collectorName>
, 
<specimenCount id="DDA8FD5FA87CFFA3FC98FA76FC290CA2" box="[781,916,1442,1464]" pageId="100" pageNumber="100" type="female">one female</specimenCount>
. 
<collectorName id="665B5300A87CFFA3FC30FA76FC560CA2" box="[933,1003,1443,1464]" pageId="100" pageNumber="100">Many</collectorName>
of the original large series of these 
<collectorName id="665B5300A87CFFA3FC0CFA15FB9A0CCC" box="[921,1063,1472,1494]" pageId="100" pageNumber="100">H.H. Smith</collectorName>
