Saussure, 1859a: 324
</taxonomicName>
[
<taxonomicName id="4C6C4D61FFE94610FB0BF8E3851C8145" box="[1213,1340,1794,1817]" class="Diplopoda" family="Polydesmidae" genus="Polydesmus" kingdom="Animalia" order="Polydesmida" pageId="3" pageNumber="206" phylum="Arthropoda" rank="genus">
