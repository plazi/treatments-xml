(
<taxonomicName id="4C6C4D61FFE94610FAE1F8E283AE8165" authorityName="Saussure" authorityYear="1859" class="Diplopoda" family="Chelodesmidae" genus="Leptodesmus" kingdom="Animalia" order="Polydesmida" pageId="3" pageNumber="206" phylum="Arthropoda" rank="genus">
<emphasis id="B918EAF0FFE94610FAE1F8E283AE8165" italics="true" pageId="3" pageNumber="206">Leptodesmus</emphasis>
