<elevation id="00068074FB5D6E60FD9BFA7C8099FA28" box="[543,644,1458,1483]" metricMagnitude="1" metricUnit="m" metricValue="2.25" metricValueMax="3.0" metricValueMin="1.5" pageId="8" pageNumber="139" unit="m" value="22.5" valueMax="30.0" valueMin="15.0">
