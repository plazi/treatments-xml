, 
<taxonomicName id="00A54D0AEC4D0229FCD7FE6E592FFE61" authority="(Stal, 1865)" authorityName="Brailovsky" authorityYear="2009" baseAuthorityName="Stal" baseAuthorityYear="1865" box="[842,1089,443,468]" class="Insecta" family="Coreidae" genus="Phthiadema" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="70" phylum="Arthropoda" rank="species" species="ornata" status="comb. nov.">
<emphasis id="F5D1EA9BEC4D0229FCD7FE6E5ED2FE66" box="[842,956,445,468]" italics="true" pageId="11" pageNumber="70">Ph. ornata</emphasis>
(
<bibRefCitation id="A3344B78EC4D0229FC56FE685957FE61" author="STAL C." box="[971,1081,443,468]" pageId="11" pageNumber="70" pagination="163 - 188" refId="ref6548" refString="STAL C. 1865: Hemiptera nova vel minus cognita. Annales de la Societe Entomologique de France, Serie 4: 163 - 188" type="journal article" year="1865">Stål, 1865</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel id="EEE257E0EC4D0229FFECFE0F5D86FE41" box="[113,232,476,500]" pageId="11" pageNumber="70" rank="species">comb. nov.</taxonomicNameLabel>
and 
<taxonomicName id="00A54D0AEC4D0229FE86FE0E5F02FE46" authority="(Walker, 1871)" authorityName="Brailovsky" authorityYear="2009" baseAuthorityName="Walker" baseAuthorityYear="1871" box="[283,620,476,500]" class="Insecta" family="Coreidae" genus="Phthiadema" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="70" phylum="Arthropoda" rank="species" species="smaragdina" status="comb. nov.">
