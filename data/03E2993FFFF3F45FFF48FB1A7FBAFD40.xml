<heading id="D0BC9F45FFF3F45EFF48FB1A7C6EFAF9" bold="true" box="[151,823,1259,1286]" fontSize="11" level="1" pageId="16" pageNumber="17" reason="1">
<emphasis id="B93FF43BFFF3F45EFF48FB1A7C6EFAF9" bold="true" box="[151,823,1259,1286]" pageId="16" pageNumber="17">
<taxonomicName id="4C4B53AAFFF3F45EFF48FB1A7D31FAF9" authority="Wiegmann" authorityName="Wiegmann" box="[151,616,1259,1286]" class="Reptilia" family="Dactyloidae" genus="Anolis" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="nebulosus" subGenus="Norops">
<emphasis id="B93FF43BFFF3F45EFF48FB1A7FBCFAFA" bold="true" box="[151,229,1260,1286]" italics="true" pageId="16" pageNumber="17">Anolis</emphasis>
