<taxonomicName id="4C204D1FFFA1215DDA0BFB55FB7766BC" authorityName="Zheng, Wittouck, Salvetti, Franz, Harris, Mattarelli, O'Toole, Pot, Vandamme, Walter, Watanabe, Wuyts, Felis, Ganzle &amp; Lebeer" authorityYear="2020" box="[828,1240,1219,1243]" class="Bacilli" family="Lactobacillaceae" genus="Lactiplantibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="44" pageNumber="2826" phylum="Firmicutes" rank="species" species="modestisalitolerans">
