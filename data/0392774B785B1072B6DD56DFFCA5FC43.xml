<paragraph id="8B84C65D785B106DB6E85533FE92FD7E" blockId="14.[140,1108,518,1672]" pageId="14" pageNumber="361">
<emphasis id="B94F1A4F785B106DB6E85533FE67FD01" bold="true" box="[185,427,624,650]" pageId="14" pageNumber="361">
<emphasis id="B94F1A4F785B106DB6E85533FEF5FD01" box="[185,313,624,650]" inLineHeading="true" pageId="14" pageNumber="361" reason="1">Additional</emphasis>
material.
</emphasis>
<collectionCode id="ED2A5E98785B106DB7E05533FE22FD01" box="[433,494,624,650]" country="Germany" httpUri="http://grbio.org/cool/jp8y-i5nv" name="Museum für Naturkunde" pageId="14" pageNumber="361" type="Museum">MfN</collectionCode>
