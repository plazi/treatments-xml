<taxonomicName id="4C3BBDDE78451073B7625283FD38FA5D" authority="(Koch &amp; Berendt, 1854)" baseAuthorityName="Koch &amp; Berendt" baseAuthorityYear="1854" box="[307,756,1472,1494]" class="Arachnida" family="Phalangiidae" genus="Dicranopalpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="16" pageNumber="363" phylum="Arthropoda" rank="species" species="ramiger">
<emphasis id="B94F1A4F78451073B7625283FE33FA5E" box="[307,511,1472,1493]" italics="true" pageId="16" pageNumber="363">Dicranopalpus ramiger</emphasis>
