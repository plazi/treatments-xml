<taxonomicName id="4C0515E3C434FFBDCABDFF21D3CCFF2E" box="[151,394,151,177]" class="Insecta" family="Tettigoniidae" genus="Cyrtopsis" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="365" phylum="Arthropoda" rank="species" species="furcicerca" status="sp. nov.">
<emphasis id="B971B272C434FFBDCABDFF21D3CCFF2E" bold="true" box="[151,394,151,177]" italics="true" pageId="12" pageNumber="365">Cyrtopsis furcicerca</emphasis>
</taxonomicName>
