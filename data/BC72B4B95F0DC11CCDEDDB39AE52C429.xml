<taxonomicName id="B86C57EF6901EEAACD052EDEF8E484AB" class="Magnoliopsida" family="Rosaceae" genus="Agrimonia" higherTaxonomySource="CoL" kingdom="Plantae" order="Rosales" pageId="7" pageNumber="32" phylum="Tracheophyta" rank="species" species="odorata">A. odorata</taxonomicName>
