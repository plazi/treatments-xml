<taxonomicName id="64FF4D14A6714C58E41CF9AFFF263C50" authority="Wang &amp; Yu &amp; Zhang, 2013" authorityName="Wang &amp; Yu &amp; Zhang" authorityYear="2013" class="Collembola" family="Tomoceridae" genus="Pogonognathellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="1244" phylum="Arthropoda" rank="species" species="heterochros" status="sp. nov.">
