<taxonomicName id="4C204DCEA850FFFF5FB9FA6EAEEEFA61" authority="Cook, 1967" authorityName="Cook" authorityYear="1967" box="[979,1234,1444,1469]" class="Arachnida" family="Aturidae" genus="Javalbiopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="14" pageNumber="354" phylum="Arthropoda" rank="species" species="antama">
