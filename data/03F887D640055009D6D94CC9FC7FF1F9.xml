<taxonomicName id="4C514D4340055008D6D94E9CFDC0F695" authority="(Walker, 1870: 104)" baseAuthorityName="Walker" baseAuthorityPageNumber="104" baseAuthorityYear="1870" box="[151,551,1951,1977]" class="Insecta" family="Achilidae" genus="Booneta" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="41" phylum="Arthropoda" rank="species" species="lurida">
