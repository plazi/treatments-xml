<taxonomicName id="4C4F77CBFFA3FFCDFE8020003222CFC5" box="[285,302,1915,1937]" class="Chilopoda" family="Cryptopidae" genus="Cryptops" kingdom="Animalia" order="Scolopendromorpha" pageId="4" pageNumber="436" phylum="Arthropoda" rank="species" species="iporangensis" status="n. sp." subGenus="Trigonocryptops">
<emphasis id="B93BD05AFFA3FFCDFE8020003222CFC5" bold="true" box="[285,302,1915,1937]" italics="true" pageId="4" pageNumber="436">C</emphasis>
