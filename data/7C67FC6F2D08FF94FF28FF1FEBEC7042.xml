<taxonomicName id="33CE36FA2D08FF93FF51FF18EEBB714F" authority="Fibiger" authorityName="Fibiger" authorityYear="2010" box="[238,483,1635,1662]" class="Insecta" family="Erebidae" genus="Asyprocessa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="genus" status="new genus">
