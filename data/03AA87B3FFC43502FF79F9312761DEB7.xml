<taxonomicName id="4C034D26FFC43503FF79F9312497D864" authority="C. Felder &amp; R. Felder, 1860" authorityName="C. Felder &amp; R. Felder" authorityYear="1860" box="[151,660,1769,1795]" class="Insecta" family="Hesperiidae" genus="Astictopterus" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
<emphasis id="B977EAB7FFC43503FF79F9312497D864" bold="true" box="[151,660,1769,1795]" pageId="2" pageNumber="303">
<emphasis id="B977EAB7FFC43503FF79F9312736D864" bold="true" box="[151,309,1769,1795]" italics="true" pageId="2" pageNumber="303">Astictopterus</emphasis>
<bibRefCitation id="EF924B54FFC43503FED2F9312497D864" author="Felder" box="[316,660,1769,1795]" pageId="2" pageNumber="303" refString="Felder, C. &amp; Felder, R, (1860) Lepidoptera nova in paeninsula Malayica collecta diagnosibus instructa. Series prima. Wiener Entomologische Monatsschrijt, 4, 394 - 402." type="journal article" year="1860">
C. Felder &amp; R. Felder, 
<number id="3DCB2482FFC43503FDB7F9312497D864" box="[601,660,1769,1795]" pageId="2" pageNumber="303" value="1860.0">1860</number>
</bibRefCitation>
</emphasis>
