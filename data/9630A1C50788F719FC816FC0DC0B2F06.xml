<subSubSection id="2A85A96D2E9C1DB5A86F7B6B798ED6E6" pageId="132" pageNumber="133" type="type material">
<paragraph id="E62D20E1EE44002E1AADCFE39599ECD0" pageId="132" pageNumber="133">Type material studied</paragraph>
