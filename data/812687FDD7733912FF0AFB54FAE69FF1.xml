<number id="BF4724CCD7733912FD5DF863FD3F9FA9" box="[704,764,1938,1962]" pageId="21" pageNumber="22" value="1984.0">1984</number>
). 
<taxonomicName id="CE8F4D68D7733912FC8FF865FC6C9FA8" box="[786,943,1940,1963]" class="Insecta" family="Pseudococcidae" genus="Eurycoccus" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="tamariscus">
<emphasis id="3BFBEAF9D7733912FC8FF865FC6C9FA8" box="[786,943,1940,1963]" italics="true" pageId="21" pageNumber="22">E. tamariscus</emphasis>
