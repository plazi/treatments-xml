<taxonomicName id="CE8F4D68D7733912FB6BF865FA939FA8" box="[1270,1360,1940,1963]" class="Magnoliopsida" family="Tamaricaceae" genus="Tamarix" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Caryophyllales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="genus">
