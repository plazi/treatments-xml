<taxonomicName id="05B5F1850AAC5F74129A45230B268AF2" family="Gasteruptiidae" genus="G." kingdom="Animalia" lsidName="G. subtile" order="Hymenoptera" pageId="0" pageNumber="1" rank="species" species="subtile">
