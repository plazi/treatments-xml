<taxonomicName id="6E4638600DC49B62A3DB930DE65B25F9" family="Gasteruptiidae" genus="G." kingdom="Animalia" lsidName="G. poecilothecum" order="Hymenoptera" pageId="0" pageNumber="1" rank="species" species="poecilothecum">
