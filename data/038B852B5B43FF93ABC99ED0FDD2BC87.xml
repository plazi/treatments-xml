<taxonomicName id="4C224FBE5B40FF93AA199FFFFD3FBCE2" box="[327,671,152,176]" class="Insecta" family="Corydalidae" genus="Protohermes" kingdom="Animalia" order="Megaloptera" pageId="6" pageNumber="575" phylum="Arthropoda" rank="species" species="decemmaculatus">
<emphasis id="B956E82F5B40FF93AA199FFFFD3FBCE2" box="[327,671,152,176]" italics="true" pageId="6" pageNumber="575">Protohermes decemmaculatus</emphasis>
