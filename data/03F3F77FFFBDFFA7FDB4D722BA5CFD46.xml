<taxonomicName id="4C5A3DEAFFBBFFA7FDB4D2DBBDF8FD62" authorityName="Francis, Kingston and Zubaid" authorityYear="2007" box="[565,691,680,705]" class="Mammalia" family="Vespertilionidae" genus="Kerivoula" kingdom="Animalia" order="Chiroptera" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="krauensis">
<emphasis id="B92E9A7BFFBBFFA7FDB4D2DBBD0DFD62" box="[565,582,681,704]" italics="true" pageId="37" pageNumber="38">K</emphasis>
