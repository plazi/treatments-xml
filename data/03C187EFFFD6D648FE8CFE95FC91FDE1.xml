<taxonomicName id="4C684D7AFFD6D64EFE1EFB56FB77FB2C" authority="Mannerheim, Point Pleasant Park, Halifax" authorityName="Mannerheim, Point Pleasant Park, Halifax" box="[410,1167,1188,1212]" class="Insecta" family="Staphylinidae" genus="Phloeocharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="subtilissima">
