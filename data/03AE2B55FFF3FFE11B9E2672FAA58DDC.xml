<taxonomicName id="4C07E1C0FFF3FFE11F972005FA9C8D6F" box="[1182,1325,1376,1401]" class="Insecta" family="Formicidae" genus="Odontophotopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="dentifera">
