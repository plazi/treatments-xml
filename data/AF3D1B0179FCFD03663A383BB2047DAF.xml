<taxonomicName id="2FFA9A973E81549E1A3DF4571EB897BC" authority="Foerster, 1869" authorityName="Foerster" authorityYear="1869" class="Insecta" family="Ichneumonidae" genus="Stenomacrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stenomacrus" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">
