<taxonomicName id="4C714D30FF9D8925FB56FD85CD02FD5D" baseAuthorityName="Ruiz-Carranza, Hernandez-Camacho &amp; Rueda-Almonacid" baseAuthorityYear="1988" class="Amphibia" family="Phyllomedusidae" genus="Hylomantis" kingdom="Animalia" order="Anura" pageId="114" pageNumber="115" phylum="Chordata" rank="species" species="danieli">
<emphasis id="B905EAA1FF9D8925FB56FD85CD02FD5D" italics="true" pageId="114" pageNumber="115">Hylomantis danieli</emphasis>
</taxonomicName>
(Ruiz­Carranza, Hernández­ Camacho, and Rueda­Almonacid, 1988), new comb.; 
<taxonomicName id="4C714D30FF9D8925FC8CFD6DCDECFD25" authority="(Duellman and Medelson, 1995)" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Duellman and Medelson" baseAuthorityYear="1995" class="Amphibia" family="Phyllomedusidae" genus="Hylomantis" kingdom="Animalia" order="Anura" pageId="114" pageNumber="115" phylum="Chordata" rank="species" species="hulli" status="new comb.">
<emphasis id="B905EAA1FF9D8925FC8CFD6DCA7DFD06" box="[848,1044,692,715]" italics="true" pageId="114" pageNumber="115">Hylomantis hulli</emphasis>
