<taxonomicName id="4C714D30FF9D8925FBFDFD08CDB1FCC8" authority="(Boulenger, 1882)" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Boulenger" baseAuthorityYear="1882" class="Amphibia" family="Phyllomedusidae" genus="Hylomantis" kingdom="Animalia" order="Anura" pageId="114" pageNumber="115" phylum="Chordata" rank="species" species="lemur" status="new comb.">
<emphasis id="B905EAA1FF9D8925FBFDFD08CC82FCCB" italics="true" pageId="114" pageNumber="115">Hylomantis lemur</emphasis>
(Boulenger, 1882)
</taxonomicName>
, 
<taxonomicNameLabel id="A23657DAFF9D8925FC30FD37CA1EFCC8" box="[1004,1143,750,773]" pageId="114" pageNumber="115" rank="species">new comb.</taxonomicNameLabel>
; 
<taxonomicName id="4C714D30FF9D8925FB56FD36CAEAFCEE" authority="(Funkhouser, 1962)" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Funkhouser" baseAuthorityYear="1962" class="Amphibia" family="Phyllomedusidae" genus="Hylomantis" kingdom="Animalia" order="Anura" pageId="114" pageNumber="115" phylum="Chordata" rank="species" species="medinai" status="new comb.">
