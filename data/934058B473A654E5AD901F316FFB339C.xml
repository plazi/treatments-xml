<taxonomicName id="E7ECA4C73182118E2EB242CAA328E29E" authority="D.S.Tennakoon, C.-H.Kuo &amp; K.D.Hyde, 2020" authorityName="Tennakoon, C. H. Kuo &amp; K. D. Hyde" authorityYear="2020" class="Dothideomycetes" family="Phaeosphaeriaceae" genus="Elongaticollum" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Elongaticollum" order="Pleosporales" pageId="0" pageNumber="59" phylum="Ascomycota" rank="genus">
