<taxonomicName id="4C144D37C67AA208F9E2F083F9454C06" baseAuthorityName="Horsfield" baseAuthorityYear="1823" box="[1562,1706,3865,3904]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="239" phylum="Chordata" rank="species" species="larvatus">
