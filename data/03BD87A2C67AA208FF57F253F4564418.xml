<taxonomicName id="4C144D37C67AA208FC1BF083FAB14C06" baseAuthorityName="Horsfield" baseAuthorityYear="1823" box="[995,1374,3865,3904]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="239" phylum="Chordata" rank="species" species="larvatus">
