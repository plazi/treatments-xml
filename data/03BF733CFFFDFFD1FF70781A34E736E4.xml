<collectionCode id="ED075AEFFFFCFFD1FE1D7C30370E3014" box="[506,567,811,834]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="7" pageNumber="8">ZMB</collectionCode>
