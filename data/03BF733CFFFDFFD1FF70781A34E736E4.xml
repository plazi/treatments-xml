<taxonomicName id="4C16B9A9FFFCFFD1FC197C5631CC3033" box="[1022,1269,845,869]" class="Aves" family="Motacillidae" genus="Motacilla" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="amphileuca">
