<collectionCode id="ED075AEFFFFCFFD1FAEE7C88307F30FC" box="[1289,1350,915,938]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="7" pageNumber="8">ZMB</collectionCode>
as 
<taxonomicName id="4C16B9A9FFFCFFD1FA8D7C883460309B" class="Aves" family="Muscicapidae" genus="Saxicola" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="amphileuca">
<emphasis id="B9621E38FFFCFFD1FA8D7C883460309B" italics="true" pageId="7" pageNumber="8">Saxicola amphileuca</emphasis>
</taxonomicName>
