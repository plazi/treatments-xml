<taxonomicName id="4C16B9A9FFFCFFD1FF707C3034403015" box="[151,377,811,835]" class="Aves" family="Muscicapidae" genus="Oenanthe" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="hispanica">
