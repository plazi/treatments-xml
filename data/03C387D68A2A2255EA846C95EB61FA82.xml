<collectingCountry id="F37D76508A2C2255E94A6C0AEAE1FBF0" box="[857,918,1124,1149]" name="India" pageId="20" pageNumber="21">India</collectingCountry>
. 
<taxonomicName id="4C6A4D438A2C2255E9B16C0AED0FFBF1" box="[930,1144,1124,1149]" class="Insecta" family="Nothybidae" genus="Nothybus" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="procerus">
