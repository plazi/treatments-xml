<quantity id="13609B79FFE3FFD36B22FE595641A580" box="[280,360,485,511]" metricMagnitude="-2" metricUnit="m" metricValue="6.6" pageId="25" pageNumber="26" unit="mm" value="66.0">66 µm</quantity>
