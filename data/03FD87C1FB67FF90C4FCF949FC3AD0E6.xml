<taxonomicName id="4C544D54FB67FF90C7BEF8FBFAEED0C3" authority="Sjostedt 1913" authorityName="Sjostedt" authorityYear="1913" box="[981,1284,1911,1936]" class="Insecta" family="Tettigoniidae" genus="Lanistoides" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis id="B920EAC5FB67FF90C7BEF8FBFBBCD0C3" box="[981,1110,1911,1936]" italics="true" pageId="6" pageNumber="7">Lanistoides</emphasis>
