[[ 
<taxonomicName id="25EE6D49F7C4C07D840C68DDB151567C" class="Arachnida" family="Damaeidae" genus="Damaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="94" phylum="Arthropoda" rank="species" species="bicostatus">Damaeus bicostatus Berlese</taxonomicName>
