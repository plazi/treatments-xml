<taxonomicName id="4DE361B4507371E5D6B1E342E70E4447" LSID="http://zoobank.org/CF024D84-DF1F-449E-ADBC-25E025FC585E" class="Collembola" family="Neanuridae" genus="Paralobella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paralobella tianmuna" order="Collembola" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="tianmuna">Paralobella tianmuna</taxonomicName>
