<caption id="66DEBF87FFAEFF9A0699F838993EBED1" httpUri="https://zenodo.org/record/254267/files/figure.png" pageId="10" pageNumber="125" targetBox="[211,1375,193,1901]" targetPageId="10">
<paragraph id="321EEF0FFFAEFF9A0699F838993EBED1" blockId="10.[151,1435,1928,1980]" pageId="10" pageNumber="125">
