<subSubSection box="[312,1042,523,550]" pageId="12" pageNumber="13" type="etymology">
<paragraph blockId="12.[264,1323,284,550]" box="[312,1042,523,550]" pageId="12" pageNumber="13">
<emphasis bold="true" box="[312,445,523,549]" pageId="12" pageNumber="13">Etymology</emphasis>
