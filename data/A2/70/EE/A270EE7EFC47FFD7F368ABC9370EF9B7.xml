<subSubSection pageId="61" pageNumber="62" type="biology_ecology">
<paragraph blockId="61.[189,288,874,900]" box="[189,288,874,900]" pageId="61" pageNumber="62">
<heading bold="true" box="[189,288,874,900]" fontSize="11" level="3" pageId="61" pageNumber="62" reason="3">
