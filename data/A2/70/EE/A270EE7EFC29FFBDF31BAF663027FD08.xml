<subSubSection box="[189,971,267,340]" pageId="86" pageNumber="87" type="etymology">
<paragraph blockId="86.[189,324,267,293]" box="[189,324,267,293]" pageId="86" pageNumber="87">
<heading bold="true" box="[189,324,267,293]" fontSize="11" level="3" pageId="86" pageNumber="87" reason="3">
