<quantity box="[1145,1287,1770,1796]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="3.6" metricValueMin="3.4" pageId="38" pageNumber="39" unit="mm" value="3.5" valueMax="3.6" valueMin="3.4">3.4–3.6 mm</quantity>
