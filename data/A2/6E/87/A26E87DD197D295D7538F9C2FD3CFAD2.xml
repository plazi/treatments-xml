<paragraph blockId="46.[212,766,1270,1333]" box="[212,766,1309,1333]" pageId="46" pageNumber="621">
<collectingCountry box="[212,320,1309,1333]" name="Brazil" pageId="46" pageNumber="621">BRAZIL</collectingCountry>
. 
