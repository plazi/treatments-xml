<taxonomicName authorityName="Sokolov" authorityYear="2020" box="[1216,1309,1449,1475]" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="549" phylum="Arthropoda" rank="species" species="clinei">
<emphasis box="[1216,1309,1449,1475]" italics="true" pageId="2" pageNumber="549">A. clinei</emphasis>
</taxonomicName>
, this group includes 
<taxonomicName authority="Sokolov" authorityName="Sokolov" authorityYear="2012" box="[251,478,1485,1511]" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="549" phylum="Arthropoda" rank="species" species="cavicola">
<emphasis box="[251,378,1485,1511]" italics="true" pageId="2" pageNumber="549">A. cavicola</emphasis>
Sokolov
</taxonomicName>
, 
<taxonomicName authority="Sokolov" authorityName="Sokolov. The" authorityYear="2012" box="[490,712,1485,1511]" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="549" phylum="Arthropoda" rank="species" species="hirsutus">
<emphasis box="[490,611,1485,1511]" italics="true" pageId="2" pageNumber="549">A. hirsutus</emphasis>
Sokolov
</taxonomicName>
