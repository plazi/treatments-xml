<taxonomicName baseAuthorityName="Forel" baseAuthorityYear="1913" box="[579,682,702,726]" class="Insecta" family="Formicidae" genus="Chrysapace" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="479" phylum="Arthropoda" rank="species" species="sauteri">
