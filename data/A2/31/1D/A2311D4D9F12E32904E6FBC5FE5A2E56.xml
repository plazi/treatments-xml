in length, 
<quantity box="[596,731,1689,1714]" metricMagnitude="-3" metricUnit="m" metricValue="5.1499999999999995" metricValueMax="9.1" metricValueMin="1.2" pageId="11" pageNumber="12" unit="mm" value="5.1499999999999995" valueMax="9.1" valueMin="1.2">1.2–9.1 mm</quantity>
in height, 
