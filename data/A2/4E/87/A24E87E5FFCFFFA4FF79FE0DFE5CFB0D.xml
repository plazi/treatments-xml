<subSubSection pageId="36" type="description">
<paragraph blockId="36.[151,638,646,1198]" pageId="36">
<emphasis bold="true" box="[175,298,646,667]" pageId="36">Description.</emphasis>
<taxonomicName authorityName="Lord &amp; Ivie" authorityYear="2016" baseAuthorityName="Franz" baseAuthorityYear="1969" box="[310,513,646,667]" class="Insecta" family="Zopheridae" genus="Rapthius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="36" phylum="Arthropoda" rank="species" species="peruvianus">
<emphasis box="[310,513,646,667]" italics="true" pageId="36">Rapthius peruvianus</emphasis>
</taxonomicName>
