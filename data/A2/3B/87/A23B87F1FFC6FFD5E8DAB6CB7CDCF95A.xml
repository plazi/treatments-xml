<subSubSection pageId="52" pageNumber="53" type="nomenclature">
<paragraph blockId="52.[192,588,711,764]" pageId="52" pageNumber="53">
<heading centered="true" fontSize="9" level="2" pageId="52" pageNumber="53" reason="2">
<taxonomicName authority="Dimitrov &amp; Hormiga, 2009" authorityName="Dimitrov &amp; Hormiga" authorityYear="2009" box="[192,437,711,734]" class="Arachnida" family="Tetragnathidae" genus="Cyrtognatha" kingdom="Animalia" order="Araneae" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="orphana" status="new species">
<emphasis bold="true" box="[192,437,711,734]" italics="true" pageId="52" pageNumber="53">Cyrtognatha orphana</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[454,588,712,734]" pageId="52" pageNumber="53" rank="species">new species</taxonomicNameLabel>
<figureCitation box="[307,433,742,764]" captionStart="Fig" captionStartId="53.[116,155,1352,1371]" captionTargetBox="[99,1184,210,1322]" captionTargetId="figure-134@53.[99,1188,209,1322]" captionTargetPageId="53" captionText="Fig. 22. Cyrtognatha orphana, n. sp. Male palp: (A), retrolateral (B), prolateral (C). Vulva: lateral (D), dorsal (E). Scale bars 0.5 mm." pageId="52" pageNumber="53">Figures 22</figureCitation>
, 
<figureCitation box="[447,474,742,763]" captionStart="Fig" captionStartId="54.[150,188,816,835]" captionTargetBox="[131,1224,211,786]" captionTargetId="figure-343@54.[131,1224,211,786]" captionTargetPageId="54" captionText="Fig. 23. Cyrtognatha orphana, n. sp. Male habitus: dorsal (A), lateral (C), ventral (D). Male chelicerae (B)." pageId="52" pageNumber="53">23</figureCitation>
