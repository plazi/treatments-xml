<subSubSection box="[199,1035,223,248]" pageId="17" pageNumber="88" type="etymology">
<paragraph blockId="17.[151,1437,151,1184]" box="[199,1035,223,248]" pageId="17" pageNumber="88">
<emphasis bold="true" box="[199,326,223,248]" pageId="17" pageNumber="88">Etymology</emphasis>
