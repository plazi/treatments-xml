<paragraph blockId="17.[151,1437,151,1184]" pageId="17" pageNumber="88">
<emphasis bold="true" box="[199,337,367,392]" pageId="17" pageNumber="88">Description</emphasis>
. 
<emphasis box="[351,435,368,392]" italics="true" pageId="17" pageNumber="88">Female</emphasis>
(n = 
<number box="[497,511,368,392]" pageId="17" pageNumber="88" value="1.0">1</number>
): TL = 8.0; CL = 
<number box="[716,754,367,392]" pageId="17" pageNumber="88" value="3.5">3.5</number>
; CW = 
<number box="[842,880,367,392]" pageId="17" pageNumber="88" value="2.6">2.6</number>
; OAL = 
<number box="[980,1018,367,392]" pageId="17" pageNumber="88" value="0.5">0.5</number>
; OAW = 
<number box="[1125,1163,367,392]" pageId="17" pageNumber="88" value="1.6">1.6</number>
; CLL = 
<number box="[1259,1297,368,392]" pageId="17" pageNumber="88" value="0.1">0.1</number>
