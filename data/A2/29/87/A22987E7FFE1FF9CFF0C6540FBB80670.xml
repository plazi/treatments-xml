<collectingCountry box="[597,683,1341,1367]" name="Algeria" pageId="5" pageNumber="107">Algeria</collectingCountry>
and 
<collectingCountry box="[736,824,1341,1368]" name="Tunisia" pageId="5" pageNumber="107">Tunisia</collectingCountry>
