<geoCoordinate direction="north" orientation="latitude" precision="15" value="26.022223">26°01'20&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="15" value="99.888054">99°53'17&quot;E</geoCoordinate>
