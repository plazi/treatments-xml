(= 
<taxonomicName box="[616,747,1676,1697]" class="Ostracoda" family="Thaumatocyprididae" genus="Pokornyopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Halocyprida" pageId="5" pageNumber="52" phylum="Arthropoda" rank="genus">
