<collectingCountry box="[144,219,1053,1077]" name="Kenya" pageId="14" pageNumber="1880">Kenya</collectingCountry>
, 
<collectingCountry box="[230,338,1052,1076]" name="Tanzania" pageId="14" pageNumber="1880">Tanzania</collectingCountry>
