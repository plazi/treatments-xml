<quantity box="[363,493,1994,2020]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="4.1" metricValueMin="3.9" pageId="4" pageNumber="357" unit="mm" value="4.0" valueMax="4.1" valueMin="3.9">3.9–4.1mm</quantity>
, female 
