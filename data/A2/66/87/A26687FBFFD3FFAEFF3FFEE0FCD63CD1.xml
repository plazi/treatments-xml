arboribus 
<quantity box="[466,534,384,407]" metricMagnitude="0" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="4" pageNumber="23" unit="m" value="4.5" valueMax="5.0" valueMin="4.0">4–5 m</quantity>
