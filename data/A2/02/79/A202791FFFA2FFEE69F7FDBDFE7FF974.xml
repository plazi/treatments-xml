<taxonomicName authorityName="Fieber" authorityYear="1844" box="[746,905,1029,1055]" class="Insecta" family="Tingidae" genus="Physatocheila" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
