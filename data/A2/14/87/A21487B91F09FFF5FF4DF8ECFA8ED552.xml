<emphasis bold="true" box="[199,309,1159,1184]" pageId="11" pageNumber="506">Variation</emphasis>
. Body length 
<quantity box="[480,650,1160,1184]" metricMagnitude="-3" metricUnit="m" metricValue="3.58" metricValueMax="3.83" metricValueMin="3.33" pageId="11" pageNumber="506" unit="mm" value="3.58" valueMax="3.83" valueMin="3.33">3.33–3.83 mm</quantity>
