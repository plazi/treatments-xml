<paragraph id="8B90A945FFB0FFE4D93BFDFAFA8EFB36" blockId="12.[843,1371,1142,1193]" box="[843,1371,1171,1193]" pageId="12" pageNumber="21">
1822a 
<taxonomicName id="4C2FD2C6FFB0FFE4D9E6FDFDFAD9FB38" authority="SCHLOTHEIM ex BRONGNIART" authorityName="SCHLOTHEIM ex BRONGNIART" authorityYear="1822" box="[918,1292,1171,1193]" class="Magnoliopsida" family="Arecaceae" genus="Palmacites" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecales" pageId="12" pageNumber="21" phylum="Tracheophyta" rank="genus">
