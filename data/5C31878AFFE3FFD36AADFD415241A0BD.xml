<materialsCitation id="64F03CC1FFE3FFD36832FBE4524DA0BD" country="Brazil" county="The" location="The" municipality="Parque Estadual Intervales" pageId="25" pageNumber="26" specimenCount="1" stateProvince="Sao Paulo">
<location id="D1476047FFE3FFD36832FBE4551CA00D" LSID="urn:lsid:plazi:treatment:5C31878AFFE3FFD36AADFD415241A0BD:D1476047FFE3FFD36832FBE4551CA00D" box="[520,565,1112,1138]" country="Brazil" county="The" municipality="Parque Estadual Intervales" name="The" pageId="25" pageNumber="26" stateProvince="Sao Paulo">The</location>
species has only been taken in the 
<collectingMunicipality id="3443ACE6FFE3FFD369EDFBE45239A00D" box="[983,1296,1112,1138]" pageId="25" pageNumber="26">Parque Estadual Intervales</collectingMunicipality>
in southern 
<collectingRegion id="165CF87EFFE3FFD36AADFB3C5678A0E5" box="[151,337,1152,1178]" country="Brazil" name="Sao Paulo" pageId="25" pageNumber="26">São Paulo State</collectingRegion>
, 
<collectingCountry id="AC8F760CFFE3FFD36B67FB3C5681A0E5" box="[349,424,1152,1178]" name="Brazil" pageId="25" pageNumber="26">Brazil</collectingCountry>
, at the border with 
<collectingRegion id="165CF87EFFE3FFD368B0FB3C55F4A0E5" box="[650,733,1152,1178]" country="Brazil" name="Parana" pageId="25" pageNumber="26">Paraná</collectingRegion>
