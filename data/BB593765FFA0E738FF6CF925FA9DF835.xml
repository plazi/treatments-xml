<collectingCountry id="4BE7C6E3FFA0E738FEA6F870FE2AF86D" box="[349,478,1940,1966]" name="Micronesia" pageId="9" pageNumber="10">Micronesia</collectingCountry>
, 
<collectingCountry id="4BE7C6E3FFA0E738FE12F870FDDEF86D" box="[489,554,1940,1966]" name="Palau" pageId="9" pageNumber="10">Palau</collectingCountry>
