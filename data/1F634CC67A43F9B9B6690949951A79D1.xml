<paragraph id="712CBA1197F53162C8B05F056623B126" pageId="26" pageNumber="27">
<taxonomicName id="7CFA8178F740AAB4DC6A5FFC093E7552" authority="Jacoby, 1887" authorityName="Jacoby" authorityYear="1887" class="Insecta" family="Chrysomelidae" genus="Chabria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chabria nigroplagiata" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="nigroplagiata">Chabria nigroplagiata</taxonomicName>
