<taxonomicName id="798E4D018A250C6DFEBBFA8451C58F2E" authority="Haeckel, 1872" authorityName="Haeckel" authorityYear="1872" box="[294,651,1371,1394]" class="Calcarea" family="Leucettidae" genus="Leucetta" kingdom="Animalia" order="Clathrinida" pageId="5" pageNumber="154" phylum="Porifera" rank="species" species="primigenia">
