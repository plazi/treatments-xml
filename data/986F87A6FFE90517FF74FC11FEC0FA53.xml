<taxonomicName id="D7C64D33FFE90517FEFEFAFBFE63FAB1" baseAuthorityName="Boucek" baseAuthorityYear="1949" box="[285,485,1375,1400]" class="Insecta" family="Chalcididae" genus="Bucekia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="258" phylum="Arthropoda" rank="species" species="differens">
