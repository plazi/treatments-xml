<taxonomicName id="4C524D76FFBBFF983B86FEBEFE747D60" box="[254,415,260,284]" class="Insecta" family="Syrphidae" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="family">SYRPHIDAE</taxonomicName>
.
