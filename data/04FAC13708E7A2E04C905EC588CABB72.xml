<taxonomicName id="ECE456A4D42EA4CD42BA4D95F387C31C" authority="Adans." class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="null" pageNumber="288" phylum="Tracheophyta" rank="genus">
