</taxonomicName>
(
<collectionCode id="0D80FFA635182160FF0CF92676BD68EF" box="[158,245,1726,1750]" pageId="6" pageNumber="436">BMNH</collectionCode>
) (
<figureCitation id="F3AA7BE635182160FE80F925771368EF" box="[274,347,1725,1750]" captionStart="FIGURE 6" captionStartId="7.[151,250,710,732]" captionTargetBox="[151,1436,193,688]" captionTargetId="figure@7.[151,1436,193,689]" captionTargetPageId="7" captionText="FIGURE 6. Guyalna brisa (Walker, 1850) n. comb. A, holotype male of junior synonym Fidicina amazona, Brazil (BMNH). Scale bar: 2 cm; B, timbal cover, Colombia (AFSC). Scale bar: 2 mm." httpUri="https://zenodo.org/record/255431/files/figure.png" pageId="6" pageNumber="436">
