<taxonomicName id="AC911CE035182160FF05F97976BF68C3" box="[151,247,1761,1786]" class="Insecta" family="Cicadidae" genus="Guyalna" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="genus">
