<typeStatus id="B42AD9C135182160FB17F90172AF688B" box="[1157,1255,1689,1714]" pageId="6" pageNumber="436">holotype</typeStatus>
of 
<taxonomicName id="AC911CE035182160FA84F90273D46888" box="[1302,1436,1690,1713]" class="Insecta" family="Cicadidae" genus="Fidicina" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="species" species="amazona">
