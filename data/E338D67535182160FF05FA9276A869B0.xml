<taxonomicName id="AC911CE035182160FB92F9EE723A68B4" box="[1024,1138,1654,1677]" class="Insecta" family="Cicadidae" genus="Dorisiana" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="genus">
