<taxonomicName id="AC911CE035182160FD20F9ED755B68B7" box="[690,787,1653,1678]" class="Insecta" family="Cicadidae" genus="Guyalna" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="genus">
