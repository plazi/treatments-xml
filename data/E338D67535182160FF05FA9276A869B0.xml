<taxonomicName id="AC911CE035182160FB23F979733668C0" box="[1201,1406,1761,1786]" class="Insecta" family="Cicadidae" genus="Guyalna" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="species" species="amazona">
