<subSubSection id="C35A74BCFF85FFFAF5D7FAB1FDA0FABE" box="[151,612,1335,1362]" pageId="1" pageNumber="131" type="nomenclature">
<paragraph id="8BFF2737FF85FFFAF5D7FAB1FDA0FABE" blockId="1.[151,612,1335,1362]" box="[151,612,1335,1362]" pageId="1" pageNumber="131">
<heading id="D0B7905BFF85FFFAF5D7FAB1FDA0FABE" bold="true" box="[151,612,1335,1362]" fontSize="11" level="1" pageId="1" pageNumber="131" reason="1">
<emphasis id="B934FB25FF85FFFAF5D7FAB1FDA0FABE" bold="true" box="[151,612,1335,1362]" pageId="1" pageNumber="131">
<taxonomicName id="4C405CB4FF85FFFAF5D7FAB1FE2AFABE" authority="Seeman &amp; Baker" authorityName="Seeman &amp; Baker" box="[151,494,1335,1362]" class="Arachnida" family="Discozerconidae" genus="Berzercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="131" phylum="Arthropoda" rank="genus">
<emphasis id="B934FB25FF85FFFAF5D7FAB1FED7FABD" bold="true" box="[151,275,1335,1361]" italics="true" pageId="1" pageNumber="131">Berzercon</emphasis>
Seeman &amp; 
<collectingCountry id="F35767A7FF85FFFAF4E2FABEFE2AFABE" box="[418,494,1336,1362]" name="United States Pacific Island Wildlife Refuges" pageId="1" pageNumber="131">Baker</collectingCountry>
