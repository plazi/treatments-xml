<taxonomicName id="3707FBA0D04D9697E6D21664EA17E3F3" authorityName="Morrone" authorityYear="1994" class="Insecta" family="Curculionidae" genus="Nacodius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="9" pageNumber="24" phylum="Arthropoda" rank="species" species="martitae">
