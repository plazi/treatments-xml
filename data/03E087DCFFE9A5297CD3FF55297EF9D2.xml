<location id="8E966011FFE9A5287EECFC6D2B17FC0C" LSID="urn:lsid:plazi:treatment:03E087DCFFE9A5297CD3FF55297EF9D2:8E966011FFE9A5287EECFC6D2B17FC0C" box="[766,862,964,985]" country="United States of America" county="Harris Co." latitude="29.586111" longLatPrecision="20" longitude="-95.134445" municipality="Collection of Arthropods. Data" name="BugGuide" pageId="0" pageNumber="24" stateProvince="California">BugGuide</location>
