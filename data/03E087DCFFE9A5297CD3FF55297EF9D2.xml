<location id="8E966011FFE9A5287853FC6D2C2CFC0C" LSID="urn:lsid:plazi:treatment:03E087DCFFE9A5297CD3FF55297EF9D2:8E966011FFE9A5287853FC6D2C2CFC0C" box="[1089,1125,964,985]" country="United States of America" county="Harris Co." latitude="29.586111" longLatPrecision="20" longitude="-95.134445" municipality="Collection of Arthropods. Data" name="The" pageId="0" pageNumber="24" stateProvince="California">The</location>
