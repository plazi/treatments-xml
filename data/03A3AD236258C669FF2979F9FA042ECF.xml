</taxonomicName>
group species of 
<taxonomicName id="4C0A67B66258C669FD0C79F9FC9729AD" authorityName="Spinola" authorityYear="1837" box="[690,783,691,717]" class="Insecta" family="Chalcididae" genus="Conura" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="413" phylum="Arthropoda" rank="genus">
<emphasis id="B97EC0276258C669FD0C79F9FC9729AD" bold="true" box="[690,783,691,717]" italics="true" pageId="4" pageNumber="413">Conura</emphasis>
