<taxonomicName id="7E934D42FFF36B69AFD52459FD274E3F" authority="Fiatte and Joyon, 1965" authorityName="Fiatte and Joyon" authorityYear="1965" box="[232,641,359,384]" class="Imbricatea" family="Abolliferidae" genus="Heterochromonas" higherTaxonomySource="GBIF" kingdom="Chromista" order="Marimonadida" pageId="71" pageNumber="552" phylum="Cercozoa" rank="species" species="hovassei">
