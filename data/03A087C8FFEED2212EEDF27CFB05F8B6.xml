<taxonomicName id="4C094D5DFFEFD2212FA3F261FD60FAE6" box="[473,653,1349,1374]" class="Insecta" family="Oecophoridae" genus="Punctulata" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="381" phylum="Arthropoda" rank="species" species="undefined-4">
