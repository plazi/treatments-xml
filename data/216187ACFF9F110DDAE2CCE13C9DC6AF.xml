<collectingCountry id="D1DF762AFF9F110DDF13CE1438ACC68B" box="[1382,1436,870,896]" name="United States of America" pageId="45" pageNumber="98">USA</collectingCountry>
and 
<collectingCountry id="D1DF762AFF9F110DDAB2CEF83C2DC6AF" box="[199,285,906,932]" name="Mexico" pageId="45" pageNumber="98">Mexico</collectingCountry>
to 
<collectingCountry id="D1DF762AFF9F110DDB35CEF83C9DC6AF" box="[320,429,906,932]" name="Honduras" pageId="45" pageNumber="98">Honduras</collectingCountry>
