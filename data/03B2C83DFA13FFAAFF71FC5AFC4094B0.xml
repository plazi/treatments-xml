<taxonomicName id="4C1B02A8FA12FFAAFB4BFEBEFACF9453" box="[1197,1295,332,356]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="palmi">
