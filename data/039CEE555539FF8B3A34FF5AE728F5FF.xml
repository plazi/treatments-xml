<taxonomicName id="4C3524C05539FF8B3951FC27E765F0DA" authority="Serejo &amp; Lowry, 2008" authorityName="Serejo &amp; Lowry" authorityYear="2008" box="[514,611,999,1022]" class="Malacostraca" family="Talitridae" genus="Notorchestia" kingdom="Animalia" order="Amphipoda" pageId="14" pageNumber="175" phylum="Arthropoda" rank="species" species="lobata" status="n.sp.">
<emphasis id="B94183515539FF8B3951FC27E765F0DA" box="[514,611,999,1022]" italics="true" pageId="14" pageNumber="175">N. lobata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2723E2A5539FF8B393BFC27E79AF0DA" box="[616,668,999,1022]" pageId="14" pageNumber="175" rank="species">n.sp.</taxonomicNameLabel>
; 
<taxonomicName id="4C3524C05539FF8B39F5FC27E5CDF73F" authority="Serejo &amp; Lowry, 2008" authorityName="Serejo &amp; Lowry" authorityYear="2008" class="Malacostraca" family="Talitridae" genus="Notorchestia" kingdom="Animalia" order="Amphipoda" pageId="14" pageNumber="175" phylum="Arthropoda" rank="species" species="naturaliste" status="n.sp.">
<emphasis id="B94183515539FF8B39F5FC27E5CDF73F" italics="true" pageId="14" pageNumber="175">N. naturaliste</emphasis>
