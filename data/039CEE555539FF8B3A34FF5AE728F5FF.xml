<taxonomicNameLabel id="A2723E2A5539FF8B3B81FBC4E403F73F" box="[210,261,1028,1051]" pageId="14" pageNumber="175" rank="species">n.sp.</taxonomicNameLabel>
and 
<taxonomicName id="4C3524C05539FF8B3A6BFBC4E7C0F73F" authority="(Stebbing, 1899)" baseAuthorityName="Stebbing" baseAuthorityYear="1899" box="[312,710,1028,1051]" class="Malacostraca" family="Talitridae" genus="Notorchestia" kingdom="Animalia" order="Amphipoda" pageId="14" pageNumber="175" phylum="Arthropoda" rank="species" species="novaehollandiae">
