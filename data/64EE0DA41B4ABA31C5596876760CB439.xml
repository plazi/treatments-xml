<paragraph id="D762E27A9CDC079D09A558289156D618" pageId="54" pageNumber="55">
<taxonomicName id="92030647AEB66CF1948F2F0FE43D280E" class="Arachnida" family="Lohmanniidae" genus="Lohmannia" kingdom="Animalia" order="Sarcoptiformes" pageId="54" pageNumber="55" phylum="Arthropoda" rank="subGenus" subGenus="Eulohmannia">Eulohmannia</taxonomicName>
