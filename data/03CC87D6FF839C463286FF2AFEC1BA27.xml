<taxonomicName id="4C654D43FF839C463734FE4FFDE6BA4E" authority="(Spruce) J. J. Engel et G. L. Merr." authorityName="J. J. Engel et G. L. Merr." baseAuthorityName="Spruce" class="Jungermanniopsida" family="Lepidoziaceae" genus="Telaranea" kingdom="Plantae" order="Jungermanniales" pageId="18" pageNumber="19" phylum="Bryophyta" rank="species" species="coactilis">
