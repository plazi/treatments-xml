<quantity id="4C9D9B25FF839C4631D1FE71FB9DB99B" box="[960,1033,460,486]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="18" pageNumber="19" unit="m" value="300.0">300 m</quantity>
alt. 
<date id="FFDB1000FF839C46362DFE71FB27B99B" box="[1084,1203,460,486]" pageId="18" pageNumber="19" value="2008-09-08">8.09.2008</date>
, 
<emphasis id="B911EAD2FF839C4636AEFE71FF6EBA76" italics="true" pageId="18" pageNumber="19">Ellis &amp; Wilbraham R08-71c</emphasis>
