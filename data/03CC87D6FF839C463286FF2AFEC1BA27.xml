<geoCoordinate id="EE515007FF839C4633B7FE71FDB7B99B" box="[422,547,460,486]" degrees="21" direction="south" minutes="21" orientation="latitude" pageId="18" pageNumber="19" precision="15" seconds="0" value="-21.35">21°21'0&quot; S</geoCoordinate>
