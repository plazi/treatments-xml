<emphasis id="B97DEAE40E58FFA3FF7FFEF4FEF8FE9B" bold="true" box="[151,290,282,304]" pageId="17" pageNumber="118">
<typeStatus id="54B288540E58FFA3FF7FFEF4FF16FE9B" box="[151,204,282,304]" pageId="17" pageNumber="118">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C094D750E58FFA3FEC1FEF4FDB7FE9B" authority="Lamarck, 1811" authorityName="Lamarck" authorityYear="1811" box="[297,621,282,304]" class="Gastropoda" family="Volutidae" genus="Voluta" kingdom="Animalia" order="Neogastropoda" pageId="17" pageNumber="118" phylum="Mollusca" rank="species" species="rarispina">
<emphasis id="B97DEAE40E58FFA3FEC1FEF4FE14FE9B" box="[297,462,282,304]" italics="true" pageId="17" pageNumber="118">Voluta rarispina</emphasis>
Lamarck, 
<number id="3DC124D10E58FFA3FDD3FEF4FDB7FE9B" box="[571,621,282,304]" pageId="17" pageNumber="118" value="1811.0">1811</number>
</taxonomicName>
