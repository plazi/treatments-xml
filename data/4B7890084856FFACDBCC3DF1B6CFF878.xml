<number id="751933394856FFAAD9433A01B017FBC5" box="[536,584,1076,1102]" pageId="18" pageNumber="19" value="531.0">531</number>
; 
<typeStatus id="1C6A9FBC4856FFAAD90E3A01B098FBC5" box="[597,711,1076,1102]" pageId="18" pageNumber="19">paratypes</typeStatus>
(mean and range; n = 
<number id="751933394856FFAAD88E3A01B1B8FBC5" box="[981,999,1076,1102]" pageId="18" pageNumber="19" value="7.0">7</number>
): 
<number id="751933394856FFAAD8A53A01B674FBC5" box="[1022,1067,1076,1102]" pageId="18" pageNumber="19" value="623.0">623</number>
(
<number id="751933394856FFAADF603A01B6F5FBC5" box="[1083,1194,1076,1102]" pageId="18" pageNumber="19" value="611.5" valueMax="678.0" valueMin="545.0">545–678</number>
). Breadth: 
<typeStatus id="1C6A9FBC4856FFAADE773A01B7C7FBC5" box="[1324,1432,1076,1102]" pageId="18" pageNumber="19">holotype</typeStatus>
: 
