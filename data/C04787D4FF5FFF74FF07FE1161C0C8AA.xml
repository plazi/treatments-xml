<taxonomicName id="8FEE4D41FF5FFF74FF67FE1166C2CA96" authority="Hoogstraal and Kohls, 1959" authorityName="Hoogstraal and Kohls" authorityYear="1959" box="[247,897,438,465]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="182" pageNumber="183" phylum="Arthropoda" rank="species" species="ornithophila">
