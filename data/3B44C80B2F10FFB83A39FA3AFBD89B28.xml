locality, 
<location id="B6322FC62F11FFB839D8FC67FE909C0A" LSID="urn:lsid:plazi:treatment:3B44C80B2F10FFB83A39FA3AFBD89B28:B6322FC62F11FFB839D8FC67FE909C0A" box="[198,381,1014,1038]" country="Vietnam" municipality="Lac Duong District" name="Bidoup Mountain" pageId="3" pageNumber="260" stateProvince="Lam Dong">Bidoup Mountain</location>
, 
<collectingMunicipality id="5336E3672F11FFB83897FC66FDA29C0A" box="[393,591,1014,1039]" pageId="3" pageNumber="260">Lac Duong District</collectingMunicipality>
