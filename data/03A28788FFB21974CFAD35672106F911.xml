<paragraph id="8BB4369EFFB11974CF9831EA2106F911" blockId="2.[136,779,1608,2092]" lastBlockId="2.[810,1452,1604,1740]" pageId="2" pageNumber="68">
Abdomen 
<quantity id="4CF39B7BFFB11974CE0831EA2406F864" box="[301,376,1953,1977]" metricMagnitude="-3" metricUnit="m" metricValue="4.826" pageId="2" pageNumber="68" unit="in" value="0.19">0.19 in</quantity>
length, 
<quantity id="4CF39B7BFFB11974CEEA31EA2767F864" box="[463,537,1953,1977]" metricMagnitude="-3" metricUnit="m" metricValue="5.587999999999999" pageId="2" pageNumber="68" unit="in" value="0.22">0.22 in</quantity>
