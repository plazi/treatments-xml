<taxonomicName id="50DB568E08C61CED4D6AC3B35C30F943" LSID="urn:lsid:zoobank.org:act:7F51E3D0-3408-4469-BC83-EFD22E47750F" authority="Pollock &amp; Majka" class="Insecta" family="Mycteridae" genus="Lacconotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lacconotus (Alcconotus)" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="subGenus" subGenus="Alcconotus">Alcconotus Pollock &amp; Majka</taxonomicName>
<taxonomicNameLabel id="753BFD33F071C8C23D7597F3CE02DC17" pageId="10" pageNumber="11">subgen. n.</taxonomicNameLabel>
