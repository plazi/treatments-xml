<taxonomicName id="F2DE98EFFF823A731D04414D8BCBFEA2" authority="Galleni, 1976" authorityName="Galleni" authorityYear="1976" box="[302,698,361,383]" class="Rhabditophora" family="Stylochidae" genus="Stylochus" kingdom="Animalia" order="Polycladida" pageId="16" pageNumber="124" phylum="Platyhelminthes" rank="species" species="mediterraneus">
