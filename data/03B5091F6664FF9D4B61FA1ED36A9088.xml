<quantity id="4CE415EC6664FF9B4810F9E6D0E99060" box="[552,653,1599,1623]" metricMagnitude="-4" metricUnit="m" metricValue="9.5" pageId="98" pageNumber="910" unit="mm" value="0.95">0.95 mm</quantity>
