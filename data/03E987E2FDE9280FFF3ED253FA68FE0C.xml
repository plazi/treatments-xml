<paragraph id="8BFF36F4FDE9280EFF3ED2C6FABBFB24" blockId="582.[151,1437,1160,2015]" box="[151,1370,1160,1187]" pageId="582" pageNumber="583">
<emphasis id="B934EAE6FDE9280EFF3ED2C6FEEBFB24" bold="true" box="[151,266,1160,1187]" pageId="582" pageNumber="583">Diagnosis</emphasis>
. 
<specimenCount id="9D46FD7DFDE9280EFEB1D2C7FECCFB24" box="[280,301,1161,1187]" pageId="582" pageNumber="583" type="male">♂</specimenCount>
