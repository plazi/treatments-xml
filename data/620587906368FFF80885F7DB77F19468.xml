<taxonomicName id="2DAC4D056368FFFB0A09F70271DE9FAC" authorityName="Hardwicke" authorityYear="1821" box="[588,684,2271,2296]" class="Mammalia" family="Viverridae" genus="Viverra" kingdom="Animalia" order="Carnivora" pageId="1" pageNumber="172" phylum="Chordata" rank="species" species="linsang">linsang</taxonomicName>
