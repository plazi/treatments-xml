<caption id="BED3660E6368FFFB08F9F6FB77009EE8" ID-DOI="http://doi.org/10.5281/zenodo.5714316" ID-Zenodo-Dep="5714316" httpUri="https://zenodo.org/record/5714316/files/figure.png" inLine="true" pageId="1" pageNumber="172" targetBox="[1461,2043,1924,2332]" targetPageId="1">
