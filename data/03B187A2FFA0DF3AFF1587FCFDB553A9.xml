<taxonomicName id="4C184D37FFA6DF3AFEC281ADFC5E533D" authority="reissi Ganglbauer (1911)" authorityName="Ganglbauer" authorityYear="1911" box="[320,998,764,789]" class="Insecta" family="Carabidae" genus="Laemostenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="subSpecies" species="reissi" subGenus="Antisphodrus" subSpecies="reissi">
<emphasis id="B96CEAA6FFA6DF3AFEC281ADFE6A533D" box="[320,466,765,788]" italics="true" pageId="61" pageNumber="62">Laemostenus</emphasis>
