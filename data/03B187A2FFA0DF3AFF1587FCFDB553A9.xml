<taxonomicName id="4C184D37FFA6DF3AFC038189FB9752D9" authorityName="Bello &amp; Osella" authorityYear="2016" box="[897,1071,729,752]" class="Insecta" family="Curculionidae" genus="Baldorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="magasaensis" status="sp. n.">
<emphasis id="B96CEAA6FFA6DF3AFC038189FB9752D9" box="[897,1071,729,752]" italics="true" pageId="61" pageNumber="62">B. magasaensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25F57DDFFA6DF3AFBB48188FBCB52C6" box="[1078,1139,728,751]" pageId="61" pageNumber="62" rank="species">sp. n.</taxonomicNameLabel>
