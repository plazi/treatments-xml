<taxonomicName id="4C184D37FFA6DF3AFF3481ACFE8E533C" authorityName="" authorityYear="1758" box="[182,310,764,789]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
