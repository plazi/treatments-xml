<quantity id="1E659B14366EFF99FEF6D4AEFE3B80E1" box="[298,402,1441,1462]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" metricValueMax="10.0" metricValueMin="9.0" pageId="15" pageNumber="16" unit="mm" value="9.5" valueMax="10.0" valueMin="9.0">9 – 10 mm</quantity>
........................................................ 
