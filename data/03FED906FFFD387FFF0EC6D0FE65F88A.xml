<collectingCountry id="F3402880FFFD387FFE4CC53DFDE9F8AE" box="[469,557,1849,1873]" name="Canada" pageId="31" pageNumber="32">Canada</collectingCountry>
: 
<collectingRegion id="4993A6F2FFFD387FFDA2C53DFD6DF8AE" box="[571,681,1849,1873]" country="Canada" name="Manitoba" pageId="31" pageNumber="32">Manitoba</collectingRegion>
