, 
<collectingRegion id="4993A6F2FFFD387FFC87C53EFC6BF8AE" box="[798,943,1849,1873]" country="Canada" name="Nova Scotia" pageId="31" pageNumber="32">Nova Scotia</collectingRegion>
. USA: Kansas, North and
</materialsCitation>
<collectingRegion id="4993A6F2FFFD387FFB70C53DFE59F88A" isEnumeration="true" pageId="31" pageNumber="32">South Carolina, Georgia and Tennessee</collectingRegion>
