<taxonomicName id="8AF04D34811DFFD7315AFC1309B2FC2D" authorityName="Gmelin" authorityYear="1791" box="[568,754,1002,1024]" class="Gastropoda" family="Patellidae" genus="Patella" kingdom="Animalia" order="Pezizales" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="magellanica">
