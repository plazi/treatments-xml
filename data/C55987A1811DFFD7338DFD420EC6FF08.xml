</taxonomicName>
, 
<taxonomicName id="8AF04D34811DFFD73251FBF20A89FBCC" baseAuthorityName="Philippi" baseAuthorityYear="1849" box="[307,457,1033,1055]" class="Gastropoda" family="Nacellidae" genus="Nacella" kingdom="Animalia" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="delesserti">
