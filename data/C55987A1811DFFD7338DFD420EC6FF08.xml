, 
<taxonomicName id="8AF04D34811DFFD733F3FBF20A62FBCC" baseAuthorityName="Gmelin" baseAuthorityYear="1791" box="[145,290,1033,1055]" class="Gastropoda" family="Nacellidae" genus="Nacella" kingdom="Animalia" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="deaurata">
