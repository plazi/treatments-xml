<taxonomicName id="8AF04D34811DFFD733FBFBBF0A22FB89" baseAuthorityName="Smith" baseAuthorityYear="1879" box="[153,354,1094,1116]" class="Gastropoda" family="Nacellidae" genus="Nacella" kingdom="Animalia" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="kerguelenensis">
<emphasis id="7F84EAA5811DFFD733FBFBBF0A22FB89" box="[153,354,1094,1116]" italics="true" pageId="4" pageNumber="307">N. kerguelenensis</emphasis>
</taxonomicName>
