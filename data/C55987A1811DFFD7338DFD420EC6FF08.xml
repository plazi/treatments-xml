<taxonomicName id="8AF04D34811DFFD7320DFBBF0AB3FB89" baseAuthorityName="Gmelin" baseAuthorityYear="1791" box="[367,499,1094,1116]" class="Gastropoda" family="Nacellidae" genus="Nacella" kingdom="Animalia" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="flammea">
<emphasis id="7F84EAA5811DFFD7320DFBBF0AB3FB89" box="[367,499,1094,1116]" italics="true" pageId="4" pageNumber="307">N. flammea</emphasis>
</taxonomicName>
, 
<taxonomicName id="8AF04D34811DFFD73162FBBF0929FB89" baseAuthorityName="Powell" baseAuthorityYear="1957" box="[512,617,1094,1116]" class="Gastropoda" family="Nacellidae" genus="Nacella" kingdom="Animalia" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="edgari">
<emphasis id="7F84EAA5811DFFD73162FBBF0929FB89" box="[512,617,1094,1116]" italics="true" pageId="4" pageNumber="307">N. edgari</emphasis>
</taxonomicName>
