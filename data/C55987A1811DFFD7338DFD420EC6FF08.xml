<emphasis id="7F84EAA5811DFFD7310BFBF209B9FBCD" box="[617,761,1034,1055]" italics="true" pageId="4" pageNumber="307">N. concinna</emphasis>
</taxonomicName>
and 
<taxonomicName id="8AF04D34811DFFD733AEFBD00A28FBEF" baseAuthorityName="Lesson" baseAuthorityYear="1831" box="[204,360,1064,1085]" class="Gastropoda" family="Nacellidae" genus="Nacella" kingdom="Animalia" pageId="4" pageNumber="307" phylum="Mollusca" rank="species" species="clypeater">
<emphasis id="7F84EAA5811DFFD733AEFBD00A28FBEF" box="[204,360,1064,1085]" italics="true" pageId="4" pageNumber="307">N. clypeater</emphasis>
