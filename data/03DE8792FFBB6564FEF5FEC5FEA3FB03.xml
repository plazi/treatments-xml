species: 
<taxonomicName id="4C774D07FFBB6564FEB1FE2CFDBBFE31" authority="Hooker &amp; Russell, 2012" authorityName="Hooker &amp; Russell" authorityYear="2012" box="[320,571,412,433]" class="Mammalia" family="Louisinidae" genus="Thryptodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="41" pageNumber="897" phylum="Chordata" rank="species" species="brailloni" status="sp. nov.">Thryptodon brailloni</taxonomicName>
</emphasis>
<taxonomicNameLabel id="A23057EDFFBB6564FDBEFE2CFD33FE31" box="[591,691,412,433]" pageId="41" pageNumber="897" rank="species">sp. nov.</taxonomicNameLabel>
, late Thanetian, Cernaysian 
