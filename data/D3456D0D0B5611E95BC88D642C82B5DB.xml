<td id="A329B46F5A50D595D83C445D12F8A0D8" colspan="1" pageId="6" pageNumber="45" rowspan="1">10</td>
<td id="BAFEFAA1CD1DA96D92065D46B70E9C67" colspan="1" pageId="6" pageNumber="45" rowspan="1">Sori 0.6 mm or more wide</td>
<td id="4DF2CEFE014382C91199D1242D0E7620" colspan="1" pageId="6" pageNumber="45" rowspan="1">
