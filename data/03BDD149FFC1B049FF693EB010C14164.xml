<paragraph id="8BAB605FFFC1B04BFF693F2517CA4449" blockId="13.[151,905,258,280]" box="[151,905,258,280]" pageId="13" pageNumber="74">
<taxonomicName id="4C141BDCFFC1B04BFF693F2515AA4449" authority="Jacoby, 1882" authorityName="Jacoby" authorityYear="1882" box="[151,489,258,280]" class="Insecta" family="Chrysomelidae" genus="Calligrapha" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="74" phylum="Arthropoda" rank="species" species="barda">
