<collectingCountry id="55AA7635C0764B14FC4D4CD0FC6A3588" box="[908,989,389,415]" name="Bolivia" pageId="23" pageNumber="22">Bolivia</collectingCountry>
, 
<collectingCountry id="55AA7635C0764B14FC294CD3FBA935B7" box="[1000,1054,390,416]" name="Peru" pageId="23" pageNumber="22">Peru</collectingCountry>
, 
<collectingCountry id="55AA7635C0764B14FBE94CD0FBDB3588" box="[1064,1132,389,415]" name="Brazil" pageId="23" pageNumber="22">Brazil</collectingCountry>
