<taxonomicName id="4C63FFC08871D171506A2388FE69FA62" authorityName="Grados" authorityYear="2018" box="[345,472,1464,1489]" class="Insecta" family="Arctiidae" genus="Paranerita" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="41" phylum="Arthropoda" rank="subSpecies" species="maculata" subSpecies="sandeepani">
