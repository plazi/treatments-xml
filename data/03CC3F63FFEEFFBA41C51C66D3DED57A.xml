<taxonomicName id="4C65F5F6FFEDFFBA41C51FC0D3C7D49A" authority="" box="[192,413,739,763]" class="Malacostraca" family="Paramelitidae" genus="Molina" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="5" phylum="Arthropoda" rank="subGenus" subGenus="Rosaceae">Molina (Rosaceae)</taxonomicName>
