<taxonomicName id="4C65F5F6FFEDFFBA40811FE7D04AD4BA" authority="Mill" authorityName="Mill" box="[388,528,707,731]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="genus">
<emphasis id="B9115267FFEDFFBA40811FE7D38DD4BA" box="[388,471,708,731]" italics="true" pageId="6" pageNumber="5">Acacia</emphasis>
Mill
</taxonomicName>
and 
<taxonomicName id="4C65F5F6FFEDFFBA434F1FE7D0E9D4BA" box="[586,691,708,731]" class="Magnoliopsida" family="Fabaceae" genus="Prosopis" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="genus">
<emphasis id="B9115267FFEDFFBA434F1FE7D0E9D4BA" box="[586,691,708,731]" italics="true" pageId="6" pageNumber="5">Prosopis</emphasis>
</taxonomicName>
L. (Leguminosae), 
<taxonomicName id="4C65F5F6FFEDFFBA42981FE7D64FD4BA" box="[925,1045,708,731]" class="Magnoliopsida" family="Celastraceae" genus="Maytenus" kingdom="Plantae" order="Celastrales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="genus">
<emphasis id="B9115267FFEDFFBA42981FE7D64FD4BA" box="[925,1045,708,731]" italics="true" pageId="6" pageNumber="5">Maytenus</emphasis>
