<taxonomicName id="4C65F5F6FFEDFFBA45191FE0D774D4BA" authority="" box="[1052,1326,707,731]" class="Malacostraca" family="Paramelitidae" genus="Molina" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="5" phylum="Arthropoda" rank="subGenus" subGenus="Celastraceae">Molina (Celastraceae)</taxonomicName>
