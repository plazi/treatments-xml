Rize (
<bibRefCitation id="EFD1AF8E4368C96A2443259DFCA8FA3E" author="Vincon" box="[561,820,1415,1440]" pageId="15" pageNumber="16" refString="Vincon, G. &amp; Sivec, I. (2001) Contribution to the knowledge of Turkish Leuctridae (Plecoptera). Nouvelle Revue d'Entomologie, 18 (3), 259 - 285." type="journal article" year="2001">
Vinçon &amp; Sivec 
<number id="3D88C0584368C96A248A259DFCA8FA3E" box="[760,820,1415,1439]" pageId="15" pageNumber="16" value="2001.0">2001</number>
</bibRefCitation>
); 
<emphasis id="B9340E6D4368C96A2539259DFBD4FA01" bold="true" box="[843,1096,1415,1440]" pageId="15" pageNumber="16">country record only:</emphasis>
