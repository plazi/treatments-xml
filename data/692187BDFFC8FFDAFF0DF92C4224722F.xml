<date id="9536106BFFC8FFD8FF0DF86B4205778A" box="[151,287,1973,1999]" pageId="8" pageNumber="42" value="2005-12-14">
<number id="5740248CFFC8FFD8FF0DF86B43AE778A" box="[151,180,1973,1999]" pageId="8" pageNumber="42" value="14.0">14</number>
