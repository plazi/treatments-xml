<heading id="234AF857FFBC4A31FF2FBA1DFDBCF80A" bold="true" box="[151,551,1906,1932]" fontSize="11" level="1" pageId="7" pageNumber="62" reason="1">
<taxonomicName id="BFBD34B8FFBC4A31FF2FBA1DFDBCF80A" authority="Westwood, 1840" authorityName="Westwood" authorityYear="1840" box="[151,551,1906,1932]" class="Insecta" family="Apidae" genus="Euglossa" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="analis">
<emphasis id="4AC99329FFBC4A31FF2FBA1DFDBCF80A" bold="true" box="[151,551,1906,1932]" pageId="7" pageNumber="62">
<emphasis id="4AC99329FFBC4A31FF2FBA1DFECDF80A" bold="true" box="[151,342,1906,1932]" italics="true" pageId="7" pageNumber="62">Euglossa analis</emphasis>
<bibRefCitation id="1C2C32CAFFBC4A31FEE5BA1DFDBCF80A" author="Westwood" box="[349,551,1906,1932]" pageId="7" pageNumber="62" refString="Westwood, J. O. (1840) Foreign Bees. Pp. 260 - 301. In: Duncan, J. (Ed.) The Natural History of Bees, in W. Jardine (ed.) The Naturalist's Library. Lizars, Edinburgh." type="book chapter" year="1840">
Westwood, 
<number id="CE755D1CFFBC4A31FE55BA1DFDBCF80A" box="[493,551,1906,1932]" pageId="7" pageNumber="62" value="1840.0">1840</number>
</bibRefCitation>
