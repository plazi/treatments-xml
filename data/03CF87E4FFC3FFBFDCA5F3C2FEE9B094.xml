<emphasis id="B912EAE0FFC3FFBFDCF5F091FE8BB0CF" bold="true" box="[199,324,655,681]" pageId="5" pageNumber="449">Comment.</emphasis>
<taxonomicName id="4C664D71FFC3FFBFDD79F08EFE3FB0CF" box="[331,496,656,681]" class="Insecta" family="Staphylinidae" genus="Domene" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="449" phylum="Arthropoda" rank="species" species="jiangi">
