<taxonomicName id="4C24E3F7FFBF1B355FE9F9DFFE20F994" box="[389,490,1610,1636]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="36" pageNumber="73" phylum="Arthropoda" rank="genus">Catocala</taxonomicName>
