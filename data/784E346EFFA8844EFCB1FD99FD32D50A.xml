<collectingCountry id="88F0C5E8FFA8844EFE54FC3FFD5DD3DE" box="[626,720,903,929]" name="Ukraine" pageId="9" pageNumber="111">Ukraine</collectingCountry>
</materialsCitation>
.
</paragraph>
<paragraph id="F0588578FFA8844EFCE1FC14FD5FD3B9" blockId="9.[151,1437,867,1398]" box="[199,722,939,966]" pageId="9" pageNumber="111">
<materialsCitation id="408F8F25FFA8844EFCE1FC14FD43D3B9" ID-GBIF-Occurrence="3019621301" box="[199,718,939,966]" collectionCode="EMAMS" pageId="9" pageNumber="111" specimenCount="1" typeStatus="holotype">
