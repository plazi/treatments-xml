<taxonomicName id="BE8A4D75FFA3B35B82B7E7988DCDFB5A" authorityName="Guichenot" authorityYear="1855" box="[162,437,1189,1211]" class="Reptilia" family="Gymnophthalmidae" genus="Neusticurus" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="410" phylum="Chordata" rank="species" species="bicarinatus">
