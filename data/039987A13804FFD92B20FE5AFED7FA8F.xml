<quantity id="4CC89B523804FFD82B73FC3DFD39FC75" box="[570,677,979,1005]" metricMagnitude="-4" metricUnit="m" metricValue="6.7" pageId="22" pageNumber="23" unit="mm" value="0.67">0.67 mm</quantity>
