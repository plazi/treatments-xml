<taxonomicName id="4C53ACA1FC52FFF182A3F83033F3D9DB" authority="Tong &amp; Li, 2008" authorityName="Tong &amp; Li" authorityYear="2008" box="[226,403,2020,2041]" class="Arachnida" family="Pholcidae" genus="Belisana" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="48" phylum="Arthropoda" rank="species" species="aliformis" status="new species">
<emphasis id="B9270B30FC52FFF182A3F83033F3D9DB" box="[226,403,2020,2041]" italics="true" pageId="3" pageNumber="48">Belisana aliformis</emphasis>
</taxonomicName>
