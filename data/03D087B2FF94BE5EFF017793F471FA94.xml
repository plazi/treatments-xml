<typeStatus id="54C28806FF94BE5EFDF974F0F51EFD57" box="[522,567,671,692]" pageId="112" pageNumber="699">Type</typeStatus>
species: 
<taxonomicName id="4C794D27FF94BE5EFD7B74CFF4D6FD57" authority="Schepman, 1913" authorityName="Schepman" authorityYear="1913" box="[648,1023,670,693]" class="Gastropoda" family="Scaphandridae" genus="Volvulopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="112" pageNumber="699" phylum="Mollusca" rank="species" species="ancillarioides">
