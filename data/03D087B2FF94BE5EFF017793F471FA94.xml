<taxonomicName id="4C794D27FF94BE5EFF2774CFF52BFD57" authority="Schepman, 1913: 463" authorityName="Schepman" authorityPageNumber="463" authorityYear="1913" box="[212,514,670,693]" class="Gastropoda" family="Scaphandridae" genus="Volvulopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="112" pageNumber="699" phylum="Mollusca" rank="genus">
