<taxonomicName id="4C1B4D37424FFF86FEA8F975FCA3F916" authority="Borgmeier, 1967 a: 230" authorityName="Borgmeier" authorityPageNumber="230" authorityYear="1967" box="[313,773,1671,1693]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="531" phylum="Arthropoda" rank="species" species="armipectus">
