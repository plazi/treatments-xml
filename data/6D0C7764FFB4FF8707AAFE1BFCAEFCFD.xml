<taxonomicName id="22A5BDF1FFB4FF8706FCFDA3FD12FDA6" authority="(Thorell)" baseAuthorityName="Thorell" baseAuthorityYear="1894" box="[449,703,583,610]" class="Arachnida" family="Theraphosidae" genus="Eupalaestrus" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="weijenberghi">
<emphasis id="D7D11A60FFB4FF8706FCFDA3FDFFFDA5" box="[449,594,583,609]" italics="true" pageId="7" pageNumber="8">weijenberghi</emphasis>
(Thorell)
</taxonomicName>
(
<taxonomicName id="22A5BDF1FFB4FF8705F0FDA3FCDBFDA6" box="[717,886,583,610]" class="Arachnida" family="Theraphosidae" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="family">Theraphosidae</taxonomicName>
