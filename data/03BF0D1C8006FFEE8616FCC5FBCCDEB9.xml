<taxonomicName id="4C16C7898006FFED8616FC69FD48DFC4" authority="Hermann, 1917: 33" authorityName="Hermann" authorityPageNumber="33" authorityYear="1917" box="[264,694,929,953]" class="Insecta" family="Asilidae" genus="Astochia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="virgatipes">
