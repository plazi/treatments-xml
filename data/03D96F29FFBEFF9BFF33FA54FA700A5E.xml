</materialsCitation>
<geoCoordinate id="EE44B8F8FFBEFF9BFE64F98BFE190851" box="[398,458,1591,1612]" degrees="74" direction="north" orientation="latitude" pageId="27" pageNumber="472" precision="55555" value="74.0">74°N</geoCoordinate>
, 
<geoCoordinate id="EE44B8F8FFBEFF9BFE3FF98BFDD60851" box="[469,517,1591,1612]" degrees="7" direction="west" orientation="longitude" pageId="27" pageNumber="472" precision="55555" value="-7.0">7°W</geoCoordinate>
