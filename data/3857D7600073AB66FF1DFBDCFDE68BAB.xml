<subSubSection id="F8E435FD0073AB66FF1DFB07FEF18A81" pageId="0" pageNumber="399" type="materials_examined">
<paragraph id="B04166760073AB66FF1DFB07FD1B88C3" blockId="0.[151,1437,1204,1956]" box="[151,756,1204,1228]" pageId="0" pageNumber="399">
