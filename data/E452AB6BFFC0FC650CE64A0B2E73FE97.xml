<taxonomicName id="ABFB61FEFFC3FC640CDD4EB8288CF869" authority="Lacordaire, 1848" authorityName="Lacordaire" authorityYear="1848" box="[474,952,1948,1974]" class="Insecta" family="Chrysomelidae" genus="Clythra" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="quadraticollis">
