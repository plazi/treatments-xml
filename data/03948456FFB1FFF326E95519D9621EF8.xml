<taxonomicName id="4C3D4EC3FFB1FFF32207504BDC411E23" authority="Werneck 1948: 58" authorityName="Werneck" authorityPageNumber="58" authorityYear="1948" class="Mammalia" family="Muridae" genus="Oryzomys" kingdom="Animalia" order="Rodentia" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="laticeps">
