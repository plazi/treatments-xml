<emphasis id="B930190932387E6AFF6BF88A6C16F8EC" bold="true" box="[151,267,1889,1914]" pageId="47" pageNumber="48">Remarks.</emphasis>
<taxonomicName id="4C44BE9832387E6AFEEBF88A6CE0F8EC" authorityName="Murray (Murray" authorityYear="1913" box="[279,509,1889,1914]" class="Eutardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="47" pageNumber="48" phylum="Tardigrada" rank="species" species="rubens">
