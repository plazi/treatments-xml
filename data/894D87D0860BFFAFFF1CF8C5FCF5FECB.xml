<taxonomicName id="C6E44D458609FFAFFE63FE81FDD9FEE8" authorityName="Reuter. BMNH" authorityYear="1871" box="[488,631,295,320]" class="Insecta" family="Anthocoridae" genus="Lasiochilus" kingdom="Animalia" order="Hemiptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="pallidulus">
