<emphasis id="3390EAD48608FFAEFEF2F91CFD83F961" box="[377,557,1715,1737]" italics="true" pageId="54" pageNumber="55">
<taxonomicName id="C6E44D458608FFAEFEF2F91CFD87F961" authorityName="Carpintero" authorityYear="2014" box="[377,553,1715,1737]" class="Insecta" family="Anthocoridae" genus="Lasiochilus" kingdom="Animalia" order="Hemiptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="colpoides" subGenus="Lasiochilus">L. (L.) colpoides</taxonomicName>
