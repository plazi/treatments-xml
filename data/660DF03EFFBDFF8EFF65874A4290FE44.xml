<taxonomicName id="29A43AABFFB2FF8CFEA9853144A1F89B" authority="Medler." authorityName="Medler." box="[347,643,1857,1879]" class="Insecta" family="Cicadellidae" genus="Erythrogonia" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="283" phylum="Arthropoda" rank="species" species="hertha">
