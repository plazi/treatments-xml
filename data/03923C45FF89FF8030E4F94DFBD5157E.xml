<taxonomicName id="4C3BF6D0FF89FF8030E4F94DFD1A10C3" authority="(White, 1955)" baseAuthorityName="White" baseAuthorityYear="1955" box="[252,655,1682,1709]" class="Elasmobranchii" family="Carcharhinidae" genus="Galeocerdo" kingdom="Animalia" order="Carcharhiniformes" pageId="8" pageNumber="127" phylum="Chordata" rank="species" species="eaglesomei">
