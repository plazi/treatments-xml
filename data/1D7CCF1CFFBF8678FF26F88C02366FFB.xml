<typeStatus id="4A6EC0A8FFBF867BFF26F87600306DA9" box="[151,259,1974,1999]" pageId="8" pageNumber="363">Holotype</typeStatus>
. ♂, 
<collectingCountry id="EDC23E9AFFBF867BFE81F87600456DA9" box="[304,374,1974,1999]" name="China" pageId="8" pageNumber="363">China</collectingCountry>
, Fujian Prov., Chongan, Sangang, 
<date id="E16B58CAFFBF867BFD48F87702B76DA9" box="[761,900,1975,1999]" pageId="8" pageNumber="363" value="1989-12-14">
<number id="231D6C2DFFBF867BFD48F87702266DA9" box="[761,789,1975,1999]" pageId="8" pageNumber="363" value="14.0">14</number>
.XII. 
<number id="231D6C2DFFBF867BFCF9F87702B76DA9" box="[840,900,1975,1999]" pageId="8" pageNumber="363" value="1989.0">1989</number>
</date>
, Wang Jiashe, No. 
<number id="231D6C2DFFBF867BFBEBF87705F96DA9" box="[1114,1226,1975,1999]" pageId="8" pageNumber="363" value="2.0008147E7">20008147</number>
(
<collectionCode id="F3C4E6CFFFBF867BFB68F87704176DA9" box="[1241,1316,1975,1999]" pageId="8" pageNumber="363">ZJUH</collectionCode>
). 
<emphasis id="A7A1A218FFBF867BFF76F81A006A6D95" bold="true" box="[199,345,2010,2035]" pageId="8" pageNumber="363">Description.</emphasis>
