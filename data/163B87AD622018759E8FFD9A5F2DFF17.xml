<location id="9B4D6060622018749A88F97E5AB7F930" LSID="urn:lsid:plazi:treatment:163B87AD622018759E8FFD9A5F2DFF17:9B4D6060622018749A88F97E5AB7F930" box="[1168,1254,1735,1759]" country="Brazil" county="Prussian" municipality="One" name="Germar" pageId="10" pageNumber="93" stateProvince="Sao Paulo">Germar</location>
