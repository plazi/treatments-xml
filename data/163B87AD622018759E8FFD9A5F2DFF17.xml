<typeStatus id="41298819622018749C90F97E5CE9F930" box="[648,696,1735,1759]" pageId="10" pageNumber="93">type</typeStatus>
locality ‘ 
<emphasis id="ACE6EAA9622018749D3FF97E5A40F930" box="[807,1041,1734,1759]" italics="true" pageId="10" pageNumber="93">
<location id="9B4D6060622018749D3FF97E5DDEF930" LSID="urn:lsid:plazi:treatment:163B87AD622018759E8FFD9A5F2DFF17:9B4D6060622018749D3FF97E5DDEF930" box="[807,911,1734,1759]" country="Brazil" county="Prussian" municipality="One" name="St. Paulo" pageId="10" pageNumber="93" stateProvince="Sao Paulo">St. Paulo</location>
