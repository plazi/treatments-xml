<taxonomicName id="A4CDD3ABE227A3558716F9C3FD1D0DB3" box="[297,657,1977,1999]" class="Actinopterygii" family="Bythitidae" genus="Parasaccogaster" kingdom="Animalia" order="Ophidiiformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="rhamphidognatha">
<emphasis id="51B9743AE227A3558716F9C3FD1D0DB3" box="[297,657,1977,1999]" italics="true" pageId="31" pageNumber="32">Parasaccogaster rhamphidognatha</emphasis>
