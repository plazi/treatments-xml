<taxonomicName id="A2AD8DB0D95AFFDFFEA5FA26447A0594" authority="Turrisi &amp; Smith, 2011" authorityName="Turrisi &amp; Smith" authorityYear="2011" box="[373,624,1441,1463]" class="Insecta" family="Aulacidae" genus="Pristaulacus" kingdom="Animalia" order="Hymenoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="vilhelmseni" status="sp. nov.">
