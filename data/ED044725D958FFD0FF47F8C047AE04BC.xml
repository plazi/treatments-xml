<caption id="31D2A6BBD95AFFDFFF47FA2644B105F5" ID-DOI="http://doi.org/10.5281/zenodo.5281522" ID-Zenodo-Dep="5281522" httpUri="https://zenodo.org/record/5281522/files/figure.png" pageId="55" pageNumber="56" startId="55.[151,264,1441,1464]" targetBox="[228,1361,160,1419]" targetPageId="55">
