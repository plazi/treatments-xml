<taxonomicName id="2CB94D3615709006FB1E4DCB59557BB7" box="[1154,1352,1231,1252]" class="Actinopterygii" family="Chaunacidae" genus="Chaunax" kingdom="Animalia" order="Lophiiformes" pageId="18" pageNumber="193" phylum="Chordata" rank="species" species="atimovatae" status="sp. nov.">
<emphasis id="D9CDEAA715709006FB1E4DCB59557BB7" box="[1154,1352,1231,1252]" italics="true" pageId="18" pageNumber="193">Chaunax atimovatae</emphasis>
</taxonomicName>
<taxonomicNameLabel id="C2FE57DC15709006FAD34DCB59817BB6" box="[1359,1436,1231,1253]" pageId="18" pageNumber="193" rank="species">
<emphasis id="D9CDEAA715709006FAD34DCB59817BB6" bold="true" box="[1359,1436,1231,1253]" pageId="18" pageNumber="193">sp. nov.</emphasis>
</taxonomicNameLabel>
</keyLead>
