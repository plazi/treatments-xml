<taxonomicName id="4C754D4EFEA9FE9A7487F93AFA95FCDD" box="[1189,1332,1699,1723]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="293" pageNumber="294" phylum="Nematoda" rank="genus">
