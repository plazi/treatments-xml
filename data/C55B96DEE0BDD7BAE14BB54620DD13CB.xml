<taxonomicName id="B31B82DEC0CED4C1F486AF299CFBD690" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32970" class="Insecta" family="Formicidae" genus="Odontomachus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Odontomachus angulatus Mayr" lsidName-HNS="Odontomachus angulatus Mayr" order="Hymenoptera" pageNumber="17" phylum="Arthropoda" rank="species" species="angulatus">O. angulatus</taxonomicName>
