Among 
<taxonomicName id="4C294D5EFFB70756FE459088FDA9A5AD" box="[423,544,1153,1179]" class="Insecta" family="Gracillariidae" genus="Philodoria" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="genus">
