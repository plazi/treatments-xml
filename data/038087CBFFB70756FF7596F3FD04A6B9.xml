</taxonomicName>
, 
<taxonomicName id="4C294D5EFFB70756FD7D90ACFC91A589" box="[671,792,1189,1215]" class="Insecta" family="Gracillariidae" genus="Philodoria" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="genus">
