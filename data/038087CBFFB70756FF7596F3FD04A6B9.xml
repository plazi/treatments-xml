</taxonomicName>
, 
<emphasis id="B95DEACFFFB70756FA999088FF27A589" bold="true" pageId="89" pageNumber="90">
<taxonomicNameLabel id="A26E57B4FFB70756FA999088FF27A589" pageId="89" pageNumber="90" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C294D5EFFB70756FF5E90AFFEABA589" box="[188,290,1189,1215]" class="Insecta" family="Gracillariidae" genus="Philodoria" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="funkae" status="sp. n.">
<emphasis id="B95DEACFFFB70756FF5E90AFFEABA589" box="[188,290,1189,1215]" italics="true" pageId="89" pageNumber="90">P. funkae</emphasis>
</taxonomicName>
<emphasis id="B95DEACFFFB70756FECB90ACFEE0A589" bold="true" box="[297,361,1189,1215]" pageId="89" pageNumber="90">
<taxonomicNameLabel id="A26E57B4FFB70756FECB90ACFEE0A589" box="[297,361,1189,1215]" pageId="89" pageNumber="90" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C294D5EFFB70756FE9490AFFD1AA589" authority="(Swezey)" baseAuthorityName="Swezey" baseAuthorityYear="1928" box="[374,659,1189,1215]" class="Insecta" family="Gracillariidae" genus="Philodoria" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="pittosporella">
