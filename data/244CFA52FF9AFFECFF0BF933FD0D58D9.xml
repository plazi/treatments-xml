(
<bibRefCitation id="C87436B5FF9BFFECFDEAFF21FC8E5879" author="Wagener" box="[630,809,232,258]" pageId="19" pageNumber="38" refString="Wagener, G. (1857) Helminthologische Bemerkungen aus einem Sandsschreibe an C. Th. v. Siebold. Zeitschrift fur Wissenschaftliche Zoologie, 9, 73 - 90." type="journal article" year="1857">
