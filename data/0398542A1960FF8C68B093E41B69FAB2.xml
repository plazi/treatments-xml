[
<taxonomicName id="4C319EBF1960FF8C68AF93E61B96FC79" authority="Mathews" authorityName="Mathews" box="[723,1207,1000,1023]" class="Aves" family="Paradisaeidae" genus="Ptiloris" kingdom="Animalia" order="Passeriformes" pageId="95" pageNumber="95" phylum="Chordata" rank="subSpecies" species="paradisea" subSpecies="queenslandica">
<emphasis id="B945392E1960FF8C68AF93E61B60FC79" box="[723,1089,1000,1023]" italics="true" pageId="95" pageNumber="95">Ptiloris paradisea queenslandica</emphasis>
