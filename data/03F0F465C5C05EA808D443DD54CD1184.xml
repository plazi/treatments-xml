<taxonomicName id="4C593EF0C5C05EA80BED42C254BF11E8" authority="Kuroko, 1982" authorityName="Kuroko" authorityYear="1982" box="[942,1196,909,935]" class="Insecta" family="Elachistidae" genus="Whitebreadia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="species" species="andoi">
