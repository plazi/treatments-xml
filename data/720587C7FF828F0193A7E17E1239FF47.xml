<collectingCountry id="82BB7641FF828F0E9797E02F1434F8DF" box="[1191,1313,1842,1867]" name="Guatemala" pageId="55" pageNumber="56">Guatemala</collectingCountry>
, 
<collectingCountry id="82BB7641FF828F0E961BE02F148CF8DF" box="[1323,1433,1842,1867]" name="Honduras" pageId="55" pageNumber="56">Honduras</collectingCountry>
