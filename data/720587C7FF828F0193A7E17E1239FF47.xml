<collectingCountry id="82BB7641FF828F0E91F5E02F121FF8DF" box="[709,778,1842,1867]" name="Brazil" pageId="55" pageNumber="56">Brazil</collectingCountry>
, 
<collectingCountry id="82BB7641FF828F0E9026E02F1290F8DF" box="[790,901,1842,1867]" name="Colombia" pageId="55" pageNumber="56">Colombia</collectingCountry>
