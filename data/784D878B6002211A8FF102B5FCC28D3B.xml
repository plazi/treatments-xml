<subSubSection id="B8FE65166003211A8FA101B0FCC28D3B" pageId="15" pageNumber="198" type="discussion">
<paragraph id="F05B369D6003211A8FA101B0FCC28D3B" blockId="15.[151,1436,153,539]" pageId="15" pageNumber="198">
<emphasis id="C290EA8F6003211A8FA101B0FE868ED7" bold="true" box="[199,308,477,503]" pageId="15" pageNumber="198">Remarks</emphasis>
: 
<typeStatus id="2F5F883F6003211A8E2301B0FECF8ED7" box="[325,381,477,503]" pageId="15" pageNumber="198">Type</typeStatus>
specimens from 
<collectingCountry id="88F3760D6003211A8D2401B0FD248ED7" box="[578,662,477,503]" name="Algeria" pageId="15" pageNumber="198">Algeria</collectingCountry>
