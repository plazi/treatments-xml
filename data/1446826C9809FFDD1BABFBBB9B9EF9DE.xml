<taxonomicName id="5BEF48F99809FFDD1998FB189C5FFB39" authority="Carvalho 1992" authorityName="Carvalho" authorityYear="1992" box="[676,1118,1243,1268]" class="Insecta" family="Miridae" genus="Mecistoceloides" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="160" phylum="Arthropoda" rank="species" species="tonkinensis">
