<subSubSection id="C32C6528FF83476FFE9FFDC7FEF1FDF1" pageId="71" pageNumber="170" type="nomenclature">
<paragraph id="8B8936A3FF83476FFE9FFDC7FD99FD9C" blockId="71.[242,595,543,602]" box="[272,595,543,567]" pageId="71" pageNumber="170">
<heading id="D0C181CFFF83476FFE9FFDC7FD99FD9C" box="[272,595,543,567]" fontSize="10" level="2" pageId="71" pageNumber="170" reason="2">
<taxonomicName id="4C364D20FF83476FFE9FFDC7FDD9FD9D" authority="Kuschel, 2008" authorityName="Kuschel" authorityYear="2008" box="[272,531,543,566]" class="Insecta" family="Curculionidae" genus="Trigonops" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="170" phylum="Arthropoda" rank="species" species="samuelsoni" status="n. sp.">
<emphasis id="B942EAB1FF83476FFE9FFDC7FDD9FD9D" bold="true" box="[272,531,543,566]" italics="true" pageId="71" pageNumber="170">Trigonops samuelsoni</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27157CAFF83476FFD95FDF8FD99FD9C" box="[538,595,544,567]" pageId="71" pageNumber="170" rank="species">n. sp.</taxonomicNameLabel>
