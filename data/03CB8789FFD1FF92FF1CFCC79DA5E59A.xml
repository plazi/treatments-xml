<taxonomicName id="4C624D1CFFD1FF92FC3EFBCD9C74E50A" authority="Wiedemann, 1830" authorityName="Wiedemann" authorityYear="1830" box="[949,1405,1085,1111]" class="Insecta" family="Tachinidae" genus="Tachina" kingdom="Animalia" order="Diptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="leucophrys">
