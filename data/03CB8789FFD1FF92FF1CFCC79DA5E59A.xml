<quantity id="4C9A9B7AFFD1FF92FAC8FBCD9C74E50A" box="[1347,1405,1085,1111]" metricMagnitude="1" metricUnit="m" metricValue="4.6482" pageId="96" pageNumber="97" unit="in" value="1830.0">1830</quantity>
</bibRefCitation>
</taxonomicName>
