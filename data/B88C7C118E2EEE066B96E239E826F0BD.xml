<taxonomicName id="CFC15566689B18E75E4C143131BEFFD7" authority="L." authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Asteraceae" genus="Hypochaeris" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="radicata">
