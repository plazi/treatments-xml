<taxonomicName id="351DD7D5960F5B103D93B063CF978CE8" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Asteraceae" genus="Hypochaeris" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="glabra">Hypochaeris glabra</taxonomicName>
