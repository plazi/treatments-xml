<taxonomicName id="4C533BD6BA6CFFADFE813C17F4B7F91C" box="[288,453,1657,1681]" class="Insecta" family="Ectobiidae" genus="Phyllodromica" kingdom="Animalia" order="Blattodea" pageId="9" pageNumber="360" phylum="Arthropoda" rank="genus">
