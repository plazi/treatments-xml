<quantity id="BE6D8CA0FFA8FFA4FD7FFB4EB457FB24" box="[716,881,1159,1184]" metricMagnitude="-4" metricUnit="m" metricValue="6.75" metricValueMax="6.8999999999999995" metricValueMin="6.6" pageId="12" pageNumber="595" unit="mm" value="0.675" valueMax="0.69" valueMin="0.66">0.66–0.69 mm</quantity>
, height 
<quantity id="BE6D8CA0FFA8FFA4FC79FB41B344FB24" box="[970,1122,1160,1184]" metricMagnitude="-4" metricUnit="m" metricValue="3.9" metricValueMax="4.0" metricValueMin="3.8" pageId="12" pageNumber="595" unit="mm" value="0.39" valueMax="0.4" valueMin="0.38">0.38–0.4 mm</quantity>
