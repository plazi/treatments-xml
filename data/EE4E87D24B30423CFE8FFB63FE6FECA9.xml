<heading id="3D1081A84B304238FE8FFB63FCA8EB14" box="[288,836,1270,1291]" centered="true" fontSize="9" level="2" pageId="1" pageNumber="238" reason="2">
<taxonomicName id="A1E74D474B304238FE8FFB63FD2EEB14" authority="Romero and Johnson" authorityName="Romero and Johnson" box="[288,706,1270,1291]" class="Insecta" family="Chrysomelidae" genus="Meganeltumius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="238" phylum="Arthropoda" rank="species" species="juani" status="new species">
<emphasis id="5493EAD64B304238FE8FFB63FE07EB14" box="[288,491,1270,1291]" italics="true" pageId="1" pageNumber="238">Meganeltumius juani</emphasis>
Romero and Johnson
</taxonomicName>
, 
<emphasis id="5493EAD64B304238FD62FB63FCA8EB14" bold="true" box="[717,836,1270,1291]" pageId="1" pageNumber="238">
<taxonomicNameLabel id="4FA057AD4B304238FD62FB63FCA8EB14" box="[717,836,1270,1291]" pageId="1" pageNumber="238" rank="species">new species</taxonomicNameLabel>
