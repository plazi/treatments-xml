Brazil (Er. Marcus 1957; 
<bibRefCitation id="4C72B28A9C0CFFC04465FDA7FCDF1C14" author="Garcia" box="[571,774,583,608]" pageId="67" pageNumber="68" refString="Garcia, F. J., Dominguez, M. &amp; Troncoso, J. S. (2008) Opistobranquios de Brasil. Descripcion y Distribucion de Opistobranquios del Litoral de Brasil y del Archipielago Fernando de Noronha. The Authors, Vigo, Spain, 215 pp." type="book" year="2008">
García 
<emphasis id="1A9713699C0CFFC044D2FDA6FD1D1C14" box="[652,708,583,608]" italics="true" pageId="67" pageNumber="68">et al.</emphasis>
2008
</bibRefCitation>
), Costa Rica (
<bibRefCitation id="4C72B28A9C0CFFC045FEFDA8FB731C14" author="Espinosa" box="[928,1194,583,608]" pageId="67" pageNumber="68" refString="Espinosa, J. &amp; Ortea, J. (2001) Moluscos del Mar Caribe de Costa Rica: Desde Cahuita hasta Gandoca. Avicennia, suplemento 4, 1 - 77." type="journal article" year="2001">Espinosa &amp; Ortea 2001</bibRefCitation>
), 
<materialsCitation id="988BC5269C0CFFC042E3FDA8FC921CF0" ID-GBIF-Occurrence="1291048974" location="Florida" pageId="67" pageNumber="68" specimenCount="1" stateProvince="Florida">
<collectingRegion id="EA2701999C0CFFC042E3FDA8FAD71C14" box="[1213,1294,583,608]" country="United States of America" name="Florida" pageId="67" pageNumber="68">Florida</collectingRegion>
(
<bibRefCitation id="4C72B28A9C0CFFC04342FDA7FEA91CF1" author="Ev" pageId="67" pageNumber="68" refString="Marcus, Ev. &amp; Marcus, Er. (1960) Opisthobranchs from American Atlantic warm waters. Bulletin of Marine Science of the Gulf and Caribbean, 10, 129 - 203." type="journal article" year="1960">Ev. Marcus &amp; Er. Marcus 1960</bibRefCitation>
, 
<bibRefCitation id="4C72B28A9C0CFFC04722FD83FE6E1CF1" author="Marcus" box="[380,439,620,645]" pageId="67" pageNumber="68" refString="Marcus, Ev. &amp; Marcus, Er. (1967) Tropical American opisthobranchs. Studies in Tropical Oceanography, 6, 1 - 137." type="journal article" year="1967">1967</bibRefCitation>
; 
<bibRefCitation id="4C72B28A9C0CFFC0479CFD82FD681CF0" author="Jensen" box="[450,689,620,645]" pageId="67" pageNumber="68" refString="Jensen, K. R. &amp; Clark, K. N. (1983) Annotated checklist of Florida ascoglossan Opisthobranchia. The Nautilus, 97, 1 - 13." type="journal article" year="1983">Jensen &amp; Clark 1983</bibRefCitation>
; 
<bibRefCitation id="4C72B28A9C0CFFC044E3FD83FCE71CF0" author="Clark" box="[701,830,620,645]" pageId="67" pageNumber="68" refString="Clark, K. B. (1994) Ascoglossan (= Sacoglossa) molluscs in the Florida Keys: Rare marine invertebrates at special risk. Bulletin of Marine Science, 54, 900 - 916." type="journal article" year="1994">Clark 1994</bibRefCitation>
).
