<collectingCountry id="15FAF78BFF88F437FC6FFF43FBA4FF6D" box="[930,1111,151,175]" name="Czech Republic" pageId="8" pageNumber="62">Czech Republic</collectingCountry>
; 
<collectingCountry id="15FAF78BFF88F437FBAFFF43FB11FF6D" box="[1122,1250,151,175]" name="Serbia" pageId="8" pageNumber="62">Yugoslavia</collectingCountry>
