<taxonomicName id="F0A051F48738753DFE85DA3BFE3DFD92" authorityName="Roewer" authorityYear="1929" box="[338,507,528,553]" class="Arachnida" family="Gonyleptidae" genus="Discocyrtanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis id="05D4F6658738753DFE85DA3BFE3DFD92" box="[338,507,528,553]" italics="true" pageId="13" pageNumber="14">Discocyrtanus</emphasis>
</taxonomicName>
and 
<taxonomicName id="F0A051F48738753DFDECDA3BFD6DFD92" authorityName="Mello-Leitao" authorityYear="1923" box="[571,683,528,553]" class="Arachnida" family="Gonyleptidae" genus="Roeweria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis id="05D4F6658738753DFDECDA3BFD6DFD92" box="[571,683,528,553]" italics="true" pageId="13" pageNumber="14">Roeweria</emphasis>
</taxonomicName>
