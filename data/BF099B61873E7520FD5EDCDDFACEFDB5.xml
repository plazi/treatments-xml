and 
<taxonomicName id="F0A051F48738753DFB22D948FAA3FEC7" authorityName="Mello-Leitao" authorityYear="1923" box="[1269,1381,355,380]" class="Arachnida" family="Gonyleptidae" genus="Roeweria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
