<taxonomicName id="F0A051F48738753DFBC6D9A5FB7AFE1C" authorityName="Holmberg" authorityYear="1876" box="[1041,1212,397,423]" class="Arachnida" family="Gonyleptidae" genus="Discocyrtus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="testudineus">
