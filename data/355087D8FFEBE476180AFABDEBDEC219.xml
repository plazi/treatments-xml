<taxonomicName id="7AF94D4DFFEAE4701B7AF8EAED64C372" authority="T.Y.Yu" authorityName="T. Y. Yu" authorityYear="2021" box="[295,591,1858,1885]" class="Magnoliopsida" family="Rubiaceae" genus="Chassalia" kingdom="Plantae" order="Gentianales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="species" species="chewii" status="sp. nov.">
