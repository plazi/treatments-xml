<taxonomicName id="4C5BF3C6FFAEFFAB02BDE58FFC46FD0B" authority="sp." authorityName="Sp." box="[737,902,672,694]" class="Insecta" family="Braconidae" genus="Microctonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="58" pageNumber="1053" phylum="Arthropoda" rank="genus">
