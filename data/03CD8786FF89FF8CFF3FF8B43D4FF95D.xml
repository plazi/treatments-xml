<subSubSection id="C37E651BFF89FF8CFF6FFA7C3CB0F929" pageId="6" pageNumber="213" type="description">
<paragraph id="8BDB3690FF89FF8CFF6FFA7C3CB0F929" blockId="6.[151,1437,1465,1778]" pageId="6" pageNumber="213">
<emphasis id="B910EA82FF89FF8CFF6FFA7C3F20F992" bold="true" box="[199,345,1573,1597]" pageId="6" pageNumber="213">Description.</emphasis>
Length 
<quantity id="4C9C9B75FF89FF8CFE69FA7F3C55F992" box="[449,556,1574,1598]" metricMagnitude="-2" metricUnit="m" metricValue="1.03" pageId="6" pageNumber="213" unit="mm" value="10.3">10.3 mm</quantity>
, width 
<quantity id="4C9C9B75FF89FF8CFD22FA7F3D6EF992" box="[650,791,1574,1598]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" metricValueMax="4.7" metricValueMin="4.5" pageId="6" pageNumber="213" unit="mm" value="4.6" valueMax="4.7" valueMin="4.5">4.5–4.7 mm</quantity>
(
<typeStatus id="54DF8832FF89FF8CFC83FA7C3DF7F991" box="[811,910,1573,1598]" pageId="6" pageNumber="213">holotype</typeStatus>
<number id="3DAC24B7FF89FF8CFC33FA7C3DB8F991" box="[923,961,1573,1598]" pageId="6" pageNumber="213" value="4.7">4.7</number>
), length/width 
<number id="3DAC24B7FF89FF8CFBDDFA7F3AB1F991" box="[1141,1224,1574,1598]" pageId="6" pageNumber="213" value="2.25" valueMax="2.3" valueMin="2.2">2.2-2.3</number>
