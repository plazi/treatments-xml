79. 
<taxonomicName id="9B844D64FF84622DD797FCC2FD23FC8F" authority="(Linnaeus, 1767)" authorityName="Linnaeus" authorityYear="1767" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[193,680,822,847]" class="Insecta" family="Acrididae" genus="Sphingonotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="caerulans">
