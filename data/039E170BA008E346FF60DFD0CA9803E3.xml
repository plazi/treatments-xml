<taxonomicName id="4C37DD9EA008E346FD30D953CAEB0360" authorityName="Brolemann" authorityYear="1916" box="[656,927,1058,1082]" class="Diplopoda" family="Macrosternodesmidae" kingdom="Animalia" order="Polydesmida" pageId="7" pageNumber="6" phylum="Arthropoda" rank="family">Macrosternodesmidae</taxonomicName>
