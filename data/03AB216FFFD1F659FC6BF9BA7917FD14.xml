, 
<elevation id="002F774AFFAFF659FD8EFE4C7EA5FE08" box="[622,755,412,434]" metricMagnitude="1" metricUnit="m" metricValue="1.2" pageId="64" pageNumber="741" unit="m" value="12.0">
<quantity id="4CFA3D9CFFAFF659FD8EFE4C7EF0FE0B" box="[622,678,412,433]" metricMagnitude="1" metricUnit="m" metricValue="1.2" pageId="64" pageNumber="741" unit="m" value="12.0">12 m</quantity>
depth
