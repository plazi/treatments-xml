<accessionNumber id="94510D9AFFAFF659FD7EFEAD7EA5FE29" box="[670,755,381,403]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/G60255" pageId="64" pageNumber="741">G60255</accessionNumber>
, 
<location id="8EDDC6A2FFAFF659FF70FE4C7D0BFE08" LSID="urn:lsid:plazi:treatment:03AB216FFFD1F659FC6BF9BA7917FD14:8EDDC6A2FFAFF659FF70FE4C7D0BFE08" box="[144,349,412,434]" country="Philippines" name="Calamian Islands" pageId="64" pageNumber="741" stateProvince="Palawan">Calamian Islands</location>
