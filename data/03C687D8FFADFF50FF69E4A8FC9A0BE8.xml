<number id="3DA724E9FFADFF50FCC2E5C7FCDF0D6B" box="[828,856,504,530]" pageId="14" pageNumber="15" value="10.0">10</number>
/ 
<number id="3DA724E9FFADFF50FC9CE5C7FC180D6B" box="[866,927,504,530]" pageId="14" pageNumber="15" value="1984.0">1984</number>
</date>
