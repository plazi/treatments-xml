<paragraph id="C0ED263C487C41C357361985885D79C1" pageId="28" pageNumber="29">
<taxonomicName id="3462D1ECDB9CBA5774F566B5E902A93E" class="Arachnida" family="Protoplophoridae" genus="Protoplophora" kingdom="Animalia" order="Sarcoptiformes" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="palpalis">Protoplophora palpalis Berlese</taxonomicName>
