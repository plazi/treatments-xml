<number id="3DDC7E25E47C553CFE39FADABD5CFAB7" box="[495,527,1304,1330]" pageId="17" pageNumber="18" value="48.0">48</number>
'E
</geoCoordinate>
, 
<quantity id="4CECC1E7E47C553CFDE5FADABDC9FAB7" box="[563,666,1304,1330]" metricMagnitude="1" metricUnit="m" metricValue="3.55" metricValueMax="3.7" metricValueMin="3.4" pageId="17" pageNumber="18" unit="m" value="35.5" valueMax="37.0" valueMin="34.0">34-37 m</quantity>
, mud), Sta. 
<number id="3DDC7E25E47C553CFCFFFADABC06FAB7" box="[809,853,1304,1330]" pageId="17" pageNumber="18" value="230.0">230</number>
(
<geoCoordinate id="EE200AC5E47C553CFCB1FADABCEFFAB7" box="[871,956,1304,1330]" direction="north" orientation="latitude" pageId="17" pageNumber="18" precision="925" value="23.05">
<number id="3DDC7E25E47C553CFCB1FADABCDBFAB7" box="[871,904,1304,1330]" pageId="17" pageNumber="18" value="23.0">23</number>
