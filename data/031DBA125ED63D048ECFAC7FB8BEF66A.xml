<paragraph id="9B10BBD6BCF730CFA7F4794B30AFA688" pageId="18" pageNumber="97">
<taxonomicName id="E768E6B22A7702495C39CFF54F88159A" LSID="http://zoobank.org/EB5CD657-F22C-4E1B-8352-995242B9531D" class="Insecta" family="Formicidae" genus="Cerapachys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cerapachys wighti" order="Hymenoptera" pageId="18" pageNumber="97" phylum="Arthropoda" rank="species" species="wighti">Cerapachys wighti</taxonomicName>
