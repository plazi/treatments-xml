<emphasis id="B9237632474FD44FFF25FB28FE0EB2AA" bold="true" box="[151,436,1212,1237]" italics="true" pageId="5" pageNumber="570">
<taxonomicName id="4C57D1A3474FD44FFF25FB28FEB3B2AA" box="[151,265,1212,1237]" class="Actinopterygii" family="Terapontidae" genus="Therapon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="5" pageNumber="570" phylum="Chordata" rank="genus">Therapon</taxonomicName>
terrae-reginae
</emphasis>
Castelnau [= 
<taxonomicName id="4C57D1A3474FD44FFDE8FB28FCF7B2AA" baseAuthorityName="Gunther" baseAuthorityYear="1864" box="[602,845,1212,1237]" class="Actinopterygii" family="Terapontidae" genus="Amniataba" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="5" pageNumber="570" phylum="Chordata" rank="species" species="percoides">
