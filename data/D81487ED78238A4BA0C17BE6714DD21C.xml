<taxonomicName id="97BD4D7878208A4AA1F3782E7038D419" authority="Viets, 1931" authorityName="Viets" authorityYear="1931" box="[421,524,1793,1817]" class="Arachnida" family="Athienemanniidae" genus="Africasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="14" pageNumber="83" phylum="Arthropoda" rank="genus">
