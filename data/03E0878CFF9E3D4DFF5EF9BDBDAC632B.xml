<subSubSection id="C3536511FF9C3D4DFF0FFC9EBA856440" box="[198,980,785,811]" pageId="6" pageNumber="35" type="etymology">
<paragraph id="8BF6369AFF9C3D4DFF0FFC9EBA856440" blockId="6.[151,1437,152,1092]" box="[198,980,785,811]" pageId="6" pageNumber="35">
<emphasis id="B93DEA88FF9C3D4DFF0FFC9EB8026440" bold="true" box="[198,339,785,811]" pageId="6" pageNumber="35">Etymology.</emphasis>
