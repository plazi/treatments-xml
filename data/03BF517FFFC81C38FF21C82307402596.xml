<taxonomicName id="4C169BEAFFC81C38FE9AC8E807AE23DD" authority="Duges, 1834" authorityName="Duges" authorityYear="1834" box="[343,667,645,669]" class="Arachnida" family="Raphignathidae" genus="Raphignathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="426" phylum="Arthropoda" rank="genus">
