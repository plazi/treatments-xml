<paragraph id="087FBFB0940517340BD9EB25B64E59FC" pageId="9" pageNumber="10">
<taxonomicName id="E890B794FC4E5A99D67C3F90A17DA827" authorityName="Reimer &amp; Fujii" authorityYear="2010" class="Anthozoa" family="Microzoanthidae" genus="Terrazoanthus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Terrazoanthus onoi" order="Zoantharia" pageId="9" pageNumber="10" phylum="Cnidaria" rank="species" species="onoi">
