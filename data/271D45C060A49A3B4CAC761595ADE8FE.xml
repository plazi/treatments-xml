<paragraph id="0CC45CBCB8307F8068B4989F3714A1E7" pageId="23" pageNumber="76">
<taxonomicName id="E1CA1DEBE94A6F9C15DB6DD2F9124A76" authority="" authorityName="" class="Crustacea" family="Atyidae" genus="Atyaephyra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Atyaephyra desmarestii subsp. orientalis" order="Decapoda" pageId="23" pageNumber="76" phylum="Arthropoda" rank="subSpecies" species="desmarestii" subSpecies="orientalis">Atyaephyra desmarestii orientalis</taxonomicName>
