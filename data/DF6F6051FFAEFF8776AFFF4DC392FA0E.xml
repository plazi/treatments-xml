<emphasis id="65B20D55FFAEFF8676D4FE87C640FEA5" box="[164,309,269,290]" italics="true" pageId="78" pageNumber="778">Type species:</emphasis>
<taxonomicName id="90C6AAC4FFAEFF86774EFE86C472FEA5" authority="Edwards" authorityName="Edwards" authorityYear="1923" box="[318,775,268,290]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="778" phylum="Arthropoda" rank="species" species="iyengari" subGenus="Skusea">
<taxonomicName id="90C6AAC4FFAEFF86774EFE86C53AFEA5" box="[318,591,268,290]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="778" phylum="Arthropoda" rank="species" species="iyengari" subGenus="Skusea">
<emphasis id="65B20D55FFAEFF86774EFE86C60AFEA6" box="[318,383,268,289]" italics="true" pageId="78" pageNumber="778">Aedes</emphasis>
