(
<taxonomicName id="EED3B1C0BCF032ED791EFCA728F9A92E" family="Sphingidae" lsidName="" pageId="106" pageNumber="107" rank="family">Sphingidae</taxonomicName>
) (
<bibRefCitation id="CECD8160F831E0D93343ED360A981AA6" author="Gahan, AB" journalOrPublisher="Proceedings of the United States National Museum" pageId="120" pageNumber="121" pagination="1 - 39" title="Miscellaneous descriptions of new parasitic Hymenoptera with some synonymical notes." url="10.5479/si.00963801.71-2676.1" volume="71" year="1927">Gahan 1927</bibRefCitation>
).
</paragraph>
</subSubSection>
<subSubSection id="F890B6BFEA64A1E3A68129BE73C4149E" pageId="106" pageNumber="107" type="distribution">
<paragraph id="95A28CFDEAD1E07E2F1AD6203998FAA2" pageId="106" pageNumber="107">Distribution.</paragraph>
<paragraph id="AD8D788C489E88E98CC9FB3E725ECC08" pageId="106" pageNumber="107">
