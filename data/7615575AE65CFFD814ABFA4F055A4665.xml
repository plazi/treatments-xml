<taxonomicName id="39BC9DCFE65BFFD817F9FAF701744648" authority="Gunther, 1864" authorityName="Gunther" authorityYear="1864" box="[965,1382,1340,1365]" class="Actinopterygii" family="Plotosidae" genus="Euristhmus" kingdom="Animalia" order="Siluriformes" pageId="45" pageNumber="46" phylum="Chordata" rank="species" species="lepturus">
