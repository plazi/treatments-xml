<location id="694CD0DDFFF97E60FD9466F93894EF1D" LSID="urn:lsid:plazi:treatment:E43A3710FFF97E60FF4E66D33943ED09:694CD0DDFFF97E60FD9466F93894EF1D" box="[626,720,1263,1287]" country="India" latitude="13.3142" longLatPrecision="7" longitude="74.8014" municipality="Udupi" name="Manipal" pageId="10" pageNumber="695" stateProvince="Karnataka">Manipal</location>
, Pra- gathi nagar, site code H (
<geoCoordinate id="09A7E0C1FFF97E60FE2E6707387DEF33" box="[456,569,1297,1321]" degrees="13.3142" direction="north" orientation="latitude" pageId="10" pageNumber="695" precision="5" value="13.3142">13.3142°N</geoCoordinate>
