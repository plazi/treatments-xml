<number id="5A0524D6FF8E8A41ADC7EEC832DACEEF" box="[936,950,1974,1998]" pageId="5" pageNumber="6" value="1.0">1</number>
strong preapical 
<emphasis id="DEB9EAE3FF8E8A41AA17EEC835F8CEEF" box="[1144,1172,1974,1998]" italics="true" pageId="5" pageNumber="6">ad</emphasis>
and 
<number id="5A0524D6FF8E8A41AAA3EEC835B6CEEF" box="[1228,1242,1974,1998]" pageId="5" pageNumber="6" value="2.0">2</number>
