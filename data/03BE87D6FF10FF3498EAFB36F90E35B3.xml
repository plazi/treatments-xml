<taxonomicName id="4C174D43FF10FF3498EAFB36F8FA34DB" authority="(Valenciennes, 1829)" baseAuthorityName="Valenciennes" baseAuthorityYear="1829" box="[151,699,1219,1246]" class="Actinopterygii" family="Agonidae" genus="Hypsagonus" kingdom="Animalia" order="Scorpaeniformes" pageId="129" pageNumber="130" phylum="Chordata" rank="species" species="quadricornis">
<emphasis id="B963EAD2FF10FF3498EAFB36FB8234DB" bold="true" box="[151,451,1219,1246]" italics="true" pageId="129" pageNumber="130">Hypsagonus quadricornis</emphasis>
