<taxonomicName id="4C70A5BCFFBDFF98FAE6FD69FC4A0D14" authorityName="Kikuchi &amp; Hashimoto" authorityYear="2000" class="Malacostraca" family="Alvinocarididae" genus="Alvinocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="24" pageNumber="469" phylum="Arthropoda" rank="species" species="leurokolos">
<emphasis id="B904022DFFBDFF98FAE6FD69FC4A0D14" italics="true" pageId="24" pageNumber="469">Alvinocaris leurokolos</emphasis>
</taxonomicName>
(see above under 
<taxonomicName id="4C70A5BCFFBDFF98FB86FD48FB2E0D14" authorityName="Kikuchi &amp; Hashimoto" authorityYear="2000" box="[1132,1277,756,777]" class="Malacostraca" family="Alvinocarididae" genus="Alvinocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="24" pageNumber="469" phylum="Arthropoda" rank="species" species="leurokolos">
<emphasis id="B904022DFFBDFF98FB86FD48FB2E0D14" box="[1132,1277,756,777]" italics="true" pageId="24" pageNumber="469">A. leurokolos</emphasis>
</taxonomicName>
