<taxonomicName id="4C70A5BCFFBDFF98FC4EFD0BFBE50CD1" authorityName="KOMAI &amp; SEGONZAC" authorityYear="2005" box="[932,1078,695,716]" class="Malacostraca" family="Alvinocarididae" genus="Shinkaicaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="24" pageNumber="469" phylum="Arthropoda" rank="genus">
