<normalizedToken id="F177EA2570BC932AFC59957782689DE2" originalValue="Blütenständen" pageId="null" pageNumber="792">Bluetenstaenden</normalizedToken>
; Kelch schmal 
<normalizedToken id="C4AA70DD5E88EC987C11C03C9B5983E1" originalValue="glockenförmig" pageId="null" pageNumber="792">glockenfoermig</normalizedToken>
