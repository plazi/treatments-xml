<taxonomicName id="4C0B899CFFFF616AFEE9FABFBD7AFE3B" authorityName="Ros-Farre &amp; Pujade-Villar" authorityYear="2013" box="[328,570,1301,1325]" class="Insecta" family="Figitidae" genus="Aspicera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="santamariai">
