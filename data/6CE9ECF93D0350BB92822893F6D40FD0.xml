<subSubSection id="0CD5178C3BCF53F7A2F0D2DD1A69B030" pageId="0" pageNumber="1" type="reference_group">
<paragraph id="C9D1ABD6CF882C8C5D20C2D6D7226C56" pageId="0" pageNumber="1">
≡ 
<taxonomicName id="5CEF670AE7B04A51855110595CC05F88" authorityName="C. K. Pradeep &amp; Matheny" authorityYear="2016" class="Agaricomycetes" family="Inocybaceae" genus="Inocybe" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Inocybe alboflavella" order="Agaricales" pageId="0" pageNumber="1" phylum="Basidiomycota" rank="species" species="alboflavella">Inocybe alboflavella</taxonomicName>
