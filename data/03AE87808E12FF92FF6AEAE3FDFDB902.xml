<taxonomicName id="4C074D158E12FF93FF6AEAE3FD7FBFD1" authority="(Cunningham, 1871)" authorityName="Cunningham" authorityYear="1871" baseAuthorityName="Cunningham" baseAuthorityYear="1871" box="[151,653,1974,1999]" class="Malacostraca" family="Pandalidae" genus="Austropandalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="20" pageNumber="221" phylum="Arthropoda" rank="species" species="grayi">
