<taxonomicName id="4C204D1FFFCC2130D9DFFC29FD0E61B0" authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[232,673,959,984]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="achengensis">
