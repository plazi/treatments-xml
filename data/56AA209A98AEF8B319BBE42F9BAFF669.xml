<taxonomicName id="1FBCF40ADBB5470E91404E5FC7AE4A5D" class="Arachnida" family="Euctenizidae" genus="Aptostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aptostichus fornax" order="Araneae" pageId="137" pageNumber="138" phylum="Arthropoda" rank="species" species="fornax">Aptostichus fornax</taxonomicName>
, 
<taxonomicName id="31A51F011E0E95C6454E931EA3EB0B8A" class="Arachnida" family="Euctenizidae" genus="Aptostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aptostichus cajalco" order="Araneae" pageId="137" pageNumber="138" phylum="Arthropoda" rank="species" species="cajalco">Aptostichus cajalco</taxonomicName>
, 
<taxonomicName id="7705FF0F612FCBF7F62A6D815553B4BF" class="Arachnida" family="Euctenizidae" genus="Aptostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aptostichus chavezi" order="Araneae" pageId="137" pageNumber="138" phylum="Arthropoda" rank="species" species="chavezi">Aptostichus chavezi</taxonomicName>
