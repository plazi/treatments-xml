<taxonomicName id="7428CB6257D3BE5BC4DBE27B1AB96CF7" class="Arachnida" family="Euctenizidae" genus="Aptostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aptostichus fisheri" order="Araneae" pageId="137" pageNumber="138" phylum="Arthropoda" rank="species" species="fisheri">Aptostichus fisheri</taxonomicName>
, 
<taxonomicName id="D50FF5BBE8EDFFA1413C5BA78537E5B1" class="Arachnida" family="Euctenizidae" genus="Aptostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aptostichus elisabethae" order="Araneae" pageId="137" pageNumber="138" phylum="Arthropoda" rank="species" species="elisabethae">Aptostichus elisabethae</taxonomicName>
, and 
<taxonomicName id="1637E46D821A47AC2C73329285F41E14" class="Arachnida" family="Euctenizidae" genus="Aptostichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aptostichus lucerne" order="Araneae" pageId="137" pageNumber="138" phylum="Arthropoda" rank="species" species="lucerne">Aptostichus lucerne</taxonomicName>
