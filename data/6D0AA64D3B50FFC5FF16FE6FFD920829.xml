<collectingCountry id="9DB457CB3B51FFC5FEDEFD62FE2409F1" box="[351,427,693,716]" name="France" pageId="17" pageNumber="18">France</collectingCountry>
and 
<taxonomicName id="22A36CD83B51FFC5FE67FD62FC3509F1" authority="Nolf &amp; Brzobohaty, 2002" authorityName="Nolf &amp; Brzobohaty" authorityYear="2002" box="[486,954,692,717]" class="Actinopterygii" family="Acropomatidae" genus="Parascombrops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="17" pageNumber="18" phylum="Chordata" rank="species" species="epigonoides">
