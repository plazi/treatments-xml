<quantity id="4CAC9B0CFF9D0D00FEB4FDBE152AFDA4" box="[720,881,522,546]" metricMagnitude="-3" metricUnit="m" metricValue="1.7799999999999998" metricValueMax="1.88" metricValueMin="1.68" pageId="5" pageNumber="206" unit="mm" value="1.7799999999999998" valueMax="1.88" valueMin="1.68">1.68–1.88 mm</quantity>
