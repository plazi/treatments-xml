<quantity id="4CAC9B0CFF9D0D00FD53FD9E178AFDC4" box="[311,465,554,578]" metricMagnitude="-3" metricUnit="m" metricValue="1.72" metricValueMax="1.84" metricValueMin="1.6" pageId="5" pageNumber="206" unit="mm" value="1.72" valueMax="1.84" valueMin="1.6">1.6–1.84 mm</quantity>
, avg. 
<quantity id="4CAC9B0CFF9D0D00FE79FD9E1420FDC4" box="[541,635,554,578]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="5" pageNumber="206" unit="mm" value="1.7">1.7 mm</quantity>
