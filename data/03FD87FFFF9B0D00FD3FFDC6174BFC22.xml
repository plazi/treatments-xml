<quantity id="4CAC9B0CFF9D0D00FFD5FDBE1252FDA4" box="[945,1033,522,546]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="5" pageNumber="206" unit="mm" value="1.8">1.8 mm</quantity>
