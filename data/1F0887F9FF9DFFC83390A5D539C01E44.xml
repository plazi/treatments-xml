<taxonomicNameLabel id="BEE65786FF9DFFC830B1A6E83ED31988" box="[950,1011,933,956]" pageId="17" pageNumber="52" rank="species">n. sp.</taxonomicNameLabel>
<taxonomicName id="50A14D6CFF9DFFC830FDA6E939C51989" box="[1018,1253,932,957]" class="Polychaeta" family="Syllidae" genus="Trypanobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="17" pageNumber="52" phylum="Annelida" rank="species" species="depressa">
