<taxonomicName id="93B04D29FFC4FFE2FE43F83EFD23F801" authorityName="Kuan Zhao et Zhu L. Yang" authorityYear="2014" box="[421,652,1994,2015]" class="Agaricomycetes" family="Boletaceae" genus="Rubroboletus" kingdom="Fungi" order="Boletales" pageId="7" pageNumber="68" phylum="Basidiomycota" rank="species" species="latisporus">
