<taxonomicName id="5A8B2BE9BB84C51AFCCFF796636DBFBA" authority="Gillies, 1990" authorityName="Gillies" authorityYear="1990" baseAuthorityName="Gattolliat" baseAuthorityYear="2001" class="Insecta" family="Baetidae" genus="Potamocloeon" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Potamocloeon freitagae" order="Ephemeroptera" pageId="0" pageNumber="427" phylum="Arthropoda" rank="species" species="freitagae">Potamocloeon freitagae</taxonomicName>
