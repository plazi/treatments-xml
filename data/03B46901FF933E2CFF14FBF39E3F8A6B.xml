<emphasis id="B9690405FF933E2CFF14FBF39E2B887B" bold="true" box="[151,590,1118,1144]" pageId="1" pageNumber="532">
Subgenus 
<taxonomicName id="4C1DA394FF933E2CFE94FBF39E2B887B" authority="Verhoeff 1930" authorityName="Verhoeff" authorityYear="1930" box="[279,590,1118,1144]" class="Malacostraca" family="Agnaridae" genus="Hemilepistus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="532" phylum="Arthropoda" rank="subGenus" subGenus="Desertellio">
<emphasis id="B9690405FF933E2CFE94FBF39DFC887B" bold="true" box="[279,409,1118,1144]" italics="true" pageId="1" pageNumber="532">Desertellio</emphasis>
