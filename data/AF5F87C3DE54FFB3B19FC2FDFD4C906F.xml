<taxonomicName id="E0F64D56DE29FFB5B022C622FD90936C" authorityName="Drumm &amp; Heard" authorityYear="2011" box="[298,560,1622,1644]" class="Malacostraca" family="Kalliapseudidae" genus="Cristapseudes" kingdom="Animalia" order="Tanaidacea" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="exocladum" status="n. sp.">
<emphasis id="1582EAC7DE29FFB5B022C622FD90936C" box="[298,560,1622,1644]" italics="true" pageId="66" pageNumber="67">Cristapseudes exocladum</emphasis>
</taxonomicName>
<emphasis id="1582EAC7DE29FFB5B331C622FDDA936B" bold="true" box="[569,634,1622,1643]" pageId="66" pageNumber="67">
<taxonomicNameLabel id="0EB157BCDE29FFB5B331C622FDD5936B" box="[569,629,1622,1643]" pageId="66" pageNumber="67" rank="species">n. sp.</taxonomicNameLabel>
