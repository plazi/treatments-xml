(
<collectingRegion id="49D7D1EFFFACFFBAFF65FE5F5219FDCD" box="[159,326,506,532]" country="Brazil" name="Espirito Santo" pageId="25" pageNumber="583">Espírito Santo</collectingRegion>
, 
<collectingRegion id="49D7D1EFFFACFFBAFEAEFE5F52A0FDCD" box="[340,511,506,532]" country="Brazil" name="Rio de Janeiro" pageId="25" pageNumber="583">Rio de Janeiro</collectingRegion>
, 
<collectingRegion id="49D7D1EFFFACFFBAFDF6FE5F51DAFDCC" box="[524,645,506,533]" country="Brazil" name="Sao Paulo" pageId="25" pageNumber="583">São Paulo</collectingRegion>
