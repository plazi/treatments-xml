<subSubSection id="8CDEF7CEFF96676BFF17129F2503FF5E" box="[199,1373,151,176]" pageId="10" pageNumber="29" type="etymology">
<paragraph id="C47BA445FF96676BFF17129F2503FF5E" blockId="10.[151,1436,151,385]" box="[199,1373,151,176]" pageId="10" pageNumber="29">
<emphasis id="F6B07857FF96676BFF17129F2111FF41" bold="true" box="[199,335,152,176]" pageId="10" pageNumber="29">Etymology.</emphasis>
