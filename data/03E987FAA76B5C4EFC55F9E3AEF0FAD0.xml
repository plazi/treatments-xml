<caption id="DF3F6664A76A5C4EFF54FDC3AEFCFD21" ID-DOI="http://doi.org/10.5281/zenodo.5312926" ID-Zenodo-Dep="5312926" httpUri="https://zenodo.org/record/5312926/files/figure.png" pageId="15" pageNumber="16" startId="15.[164,243,544,563]" targetBox="[421,1187,198,511]" targetPageId="15">
<paragraph id="8BFF36ECA76A5C4EFF54FDC3AEFCFD21" blockId="15.[164,1441,544,710]" pageId="15" pageNumber="16">
