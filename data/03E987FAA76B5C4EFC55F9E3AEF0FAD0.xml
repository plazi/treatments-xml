; 
<taxonomicName id="4C404D6FA76A5C4EFB4CFDB8AE31FD8A" authorityName="Roewer" authorityYear="1943" box="[1212,1361,603,622]" class="Arachnida" family="Manaosbiidae" kingdom="Animalia" order="Opiliones" pageId="15" pageNumber="16" phylum="Arthropoda" rank="family">Manaosbiidae</taxonomicName>
; abbreviation of clade NOT named after Gonzalo Giribet; it is just a coincidence); ME-CO, 
<taxonomicName id="4C404D6FA76A5C4EFBCCFD9BAFA9FD6F" authorityName="Kury" authorityYear="1994" box="[1084,1225,632,651]" class="Arachnida" family="Metasarcidae" kingdom="Animalia" order="Opiliones" pageId="15" pageNumber="16" phylum="Arthropoda" rank="family">Metasarcidae</taxonomicName>
+ 
<taxonomicName id="4C404D6FA76A5C4EFB13FD9BAE00FD6F" authorityName="Koch" authorityYear="1839" box="[1251,1376,632,651]" class="Arachnida" family="Cosmetidae" kingdom="Animalia" order="Opiliones" pageId="15" pageNumber="16" phylum="Arthropoda" rank="family">Cosmetidae</taxonomicName>
; STY, 
<taxonomicName id="4C404D6FA76A5C4EFF54FD76AA70FD4C" box="[164,272,661,680]" class="Insecta" family="Stygnidae" kingdom="Animalia" order="Protorthoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="family">Stygnidae</taxonomicName>
