<taxonomicName id="4BA558BBFF817917FB5ADBB2ACC66D80" authority="Chen, 1957" authorityName="Chen" authorityYear="1957" box="[1163,1430,967,988]" class="Insecta" family="Mutillidae" genus="Sinotilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="166" phylum="Arthropoda" rank="species" species="contractula">
