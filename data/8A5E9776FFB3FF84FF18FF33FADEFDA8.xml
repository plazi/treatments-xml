<collectingCountry id="7AE066F0FFB3FF84FBB8FED8FB3DFE3D" box="[1079,1184,371,395]" name="Comoros" pageId="9" pageNumber="244">Comoros</collectingCountry>
, 
<collectingCountry id="7AE066F0FFB3FF84FB22FED9FE9FFE06" name="Democratic Republic of the Congo" pageId="9" pageNumber="244">Democratic Republic of Congo</collectingCountry>
, 
