<taxonomicName id="E2715A40193C03ED5305C0B361393CDD" class="Magnoliopsida" family="Annonaceae" genus="Cremastosperma" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cremastosperma napoense" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="napoense">Cremastosperma napoense</taxonomicName>
