<collectingCountry id="29F075CF464AFF8BFD07E01F5453FBC0" box="[764,852,1044,1070]" name="India" pageId="1" pageNumber="2">INDIA</collectingCountry>
: 
<collectingRegion id="9323FBBD464AFF8BFC9AE01F54EFFBC0" box="[865,1000,1044,1070]" country="India" name="Meghalaya" pageId="1" pageNumber="2">Meghalaya</collectingRegion>
