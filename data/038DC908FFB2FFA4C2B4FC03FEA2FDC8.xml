<quantity id="4CDCD5FBFFB0FFA4C1C0FE4DFC9BFE5F" box="[683,809,470,494]" metricMagnitude="2" metricUnit="m" metricValue="2.94" metricValueMax="4.25" metricValueMin="1.63" pageId="7" pageNumber="497" unit="m" value="294.0" valueMax="425.0" valueMin="163.0">163–425 m</quantity>
