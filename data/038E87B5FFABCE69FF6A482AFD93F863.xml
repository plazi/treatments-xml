<taxonomicName id="4C274D20FFAACE69FB584D6FFAB5F8E8" authority="Lopes-Andrade, 2008" authorityName="Lopes-Andrade" authorityYear="2008" box="[1189,1392,1792,1817]" class="Insecta" family="Ciidae" genus="Scolytocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="panamensis" status="sp. nov.">
