<taxonomicName id="4C274D20FFAACE69FCB24D6FFC00F8E8" authorityName=", Blair" authorityYear="1928" box="[847,965,1792,1817]" class="Insecta" family="Ciidae" genus="Scolytocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
