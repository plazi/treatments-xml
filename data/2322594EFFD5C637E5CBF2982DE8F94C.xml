<collectionCode id="CD9A709DFFD5C637E42DF32D29B0FEC1" box="[369,451,290,313]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="1" pageNumber="595">USNM</collectionCode>
). One additional ♂ 
<typeStatus id="743056FAFFD5C637E7FFF32D2A8CFEC1" box="[675,767,290,313]" pageId="1" pageNumber="595">paratype</typeStatus>
