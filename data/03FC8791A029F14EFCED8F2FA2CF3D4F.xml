<geoCoordinate id="EE615040A029F14EFAC68FF2A3C93ED6" box="[1286,1415,1482,1507]" degrees="09" direction="south" minutes="16.510" orientation="latitude" pageId="4" pageNumber="131" precision="1" value="-9.2751665">09°16.510′S</geoCoordinate>
