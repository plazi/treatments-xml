<geoCoordinate id="EE615040A029F14EFAC68F9EA3C63E8A" box="[1286,1416,1446,1471]" degrees="09" direction="south" minutes="07.714" orientation="latitude" pageId="4" pageNumber="131" precision="1" value="-9.128567">09°07.714′S</geoCoordinate>
