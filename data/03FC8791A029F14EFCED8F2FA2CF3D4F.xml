<geoCoordinate id="EE615040A029F14EFCED8FD5A5863D33" box="[813,968,1517,1542]" degrees="068" direction="west" minutes="57.432" orientation="longitude" pageId="4" pageNumber="131" precision="1" value="-68.9572">068°57.432′W</geoCoordinate>
