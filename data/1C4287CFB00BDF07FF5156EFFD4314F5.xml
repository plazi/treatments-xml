<quantity id="53139B3CB00BDF07FEAF5454FE06157E" box="[361,445,556,580]" metricMagnitude="3" metricUnit="m" metricValue="4.011" pageId="19" pageNumber="20" unit="m" value="4011.0">4011 m</quantity>
