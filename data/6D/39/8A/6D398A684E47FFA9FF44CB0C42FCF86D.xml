<taxonomicName box="[374,645,1759,1782]" class="Insecta" family="Psocidae" genus="Setopsocus" kingdom="Animalia" order="Psocodea" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="leuserensis" status="sp. nov.">
<taxonomicName box="[374,504,1759,1782]" class="Insecta" family="Psocidae" genus="Setopsocus" higherTaxonomySource="CoL" kingdom="Animalia" order="Psocodea" pageId="6" pageNumber="49" phylum="Arthropoda" rank="genus">Setopsocus</taxonomicName>
<taxonomicName box="[512,645,1759,1782]" pageId="6" pageNumber="49" rank="species" species="leuserensis" status="sp. nov.">leuserensis</taxonomicName>
</taxonomicName>
,
</emphasis>
<emphasis bold="true" box="[658,753,1759,1783]" pageId="6" pageNumber="49">
