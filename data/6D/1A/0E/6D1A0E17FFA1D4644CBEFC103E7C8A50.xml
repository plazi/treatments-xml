<quantity box="[1108,1217,432,458]" metricMagnitude="-5" metricUnit="m" metricValue="1.0" pageId="18" pageNumber="19" unit="mm" value="0.01">0.01 mm</quantity>
; LB = 
<number box="[1301,1352,432,458]" pageId="18" pageNumber="19" value="2.39">2.39</number>
± 
<quantity metricMagnitude="-5" metricUnit="m" metricValue="2.0" pageId="18" pageNumber="19" unit="mm" value="0.02">0.02 mm</quantity>
