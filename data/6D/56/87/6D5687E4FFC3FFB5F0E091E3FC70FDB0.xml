<taxonomicName authority="Wiedemann" authorityName="Wiedemann" class="Insecta" family="Culicidae" genus="Culex" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="sitiens" subGenus="Culex">
