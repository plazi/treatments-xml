</bibRefCitation>
records 
<taxonomicName authorityName="Rougemont" authorityYear="1996" box="[984,1138,1576,1600]" class="Insecta" family="Staphylinidae" genus="Stilicoderus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="123" phylum="Arthropoda" rank="species" species="formosanus">
<emphasis box="[984,1138,1576,1600]" italics="true" pageId="10" pageNumber="123">
<collectionCode box="[984,1001,1576,1599]" country="Sweden" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15668" name="Department of Botany, Swedish Museum of Natural History" pageId="10" pageNumber="123" type="Herbarium">S</collectionCode>
