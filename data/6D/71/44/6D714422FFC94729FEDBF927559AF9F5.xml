(described from 
<collectingRegion box="[997,1084,1545,1568]" country="China" name="Taiwan)" pageId="10" pageNumber="123">
<collectingCountry box="[997,1076,1545,1568]" name="Taiwan" pageId="10" pageNumber="123">Taiwan</collectingCountry>
