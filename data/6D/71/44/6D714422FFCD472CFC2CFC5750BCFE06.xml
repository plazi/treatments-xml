, 
<taxonomicName authority="(Kraatz)" baseAuthorityName="Kraatz" box="[261,610,182,206]" class="Insecta" family="Staphylinidae" genus="Lithocharis" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="128" phylum="Arthropoda" rank="species" species="sororcula">
