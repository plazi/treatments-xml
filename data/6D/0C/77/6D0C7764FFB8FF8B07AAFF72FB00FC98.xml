cf 
<taxonomicName authority="Smith" authorityName="Smith" authorityYear="1995" box="[1045,1193,654,680]" class="Arachnida" family="Theraphosidae" genus="Aphonopelma" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="gabeli">
