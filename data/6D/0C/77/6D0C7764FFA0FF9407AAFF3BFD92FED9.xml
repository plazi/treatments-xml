<paragraph blockId="19.[151,1437,223,2014]" pageId="19" pageNumber="20">
<materialsCitation ID-GBIF-Occurrence="2990869465" collectingDate="2018-09-12" collectionCode="CA" collectorName="B. Garrett &amp; Host &amp; The" county="Obispo County" location="Paso Robles" pageId="19" pageNumber="20" specimenCount="1" stateProvince="San Luis">
