<materialsCitation ID-GBIF-Occurrence="2990869444" collectingDate="2019-09-29" collectionCode="R" country="Chile" location="Cachapoal Province" pageId="36" pageNumber="37" specimenCount="1" stateProvince="Libertador General Bernardo O'Higgins">
