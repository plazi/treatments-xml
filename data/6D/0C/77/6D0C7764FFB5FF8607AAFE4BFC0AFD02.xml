<date box="[525,744,467,493]" pageId="6" pageNumber="7" value="2019-11-20">20 November 2019</date>
; B. Wier (bobby_wier). Host: 
<taxonomicName authorityName="Ausserer" authorityYear="1875" box="[1083,1193,468,493]" class="Arachnida" family="Cyrtaucheniidae" genus="Eucteniza" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis box="[1083,1193,468,493]" italics="true" pageId="6" pageNumber="7">Eucteniza</emphasis>
</taxonomicName>
cf 
<taxonomicName baseAuthorityName="O. Pickard-Cambridge" baseAuthorityYear="1895" box="[1229,1295,467,493]" class="Arachnida" family="Cyrtaucheniidae" genus="Eucteniza" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="relata">
<emphasis box="[1229,1295,467,493]" italics="true" pageId="6" pageNumber="7">relata</emphasis>
</taxonomicName>
(O. P.-Cambridge) (
<taxonomicName authorityName="Raven" authorityYear="1985" box="[292,440,503,530]" class="Arachnida" family="Euctenizidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="family">Euctenizidae</taxonomicName>
