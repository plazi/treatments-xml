, 
<collectingMunicipality box="[548,666,1987,2013]" pageId="14" pageNumber="15">Manizales</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:6D0C7764FFBDFF8F07AAF90FFBEFFEA1:E07A90A9FFBDFF8E059AF827FC02F819" box="[679,943,1987,2013]" country="Colombia" municipality="Manizales" name="Los Alcazares Ecopark" pageId="14" pageNumber="15" stateProvince="Caldas">Los Alcázares Ecopark</location>
; posted 
