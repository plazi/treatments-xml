<date box="[734,955,511,537]" pageId="31" pageNumber="32" value="2017-09-27">27 September 2017</date>
; S. Dean. Host: 
<taxonomicName authorityName="Hentz" authorityYear="1844" box="[1138,1391,511,537]" class="Arachnida" family="Pisauridae" genus="Dolomedes" kingdom="Animalia" order="Araneae" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="tenebrosus">
<emphasis box="[1138,1391,511,537]" italics="true" pageId="31" pageNumber="32">Dolomedes tenebrosus</emphasis>
</taxonomicName>
