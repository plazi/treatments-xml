<collectingMunicipality box="[268,324,943,969]" pageId="44" pageNumber="45">State</collectingMunicipality>
of 
<collectingRegion box="[364,531,943,969]" country="Brazil" name="Rio de Janeiro" pageId="44" pageNumber="45">Rio de Janeiro</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:6D0C7764FF9FFFAC07AAFCFAFBAEFBB9:E07A90A9FF9FFFAC051DFC4BFDC6FC0D" box="[544,619,943,969]" country="Brazil" municipality="State" name="Paraty" pageId="44" pageNumber="45" stateProvince="Rio de Janeiro">Paraty</location>
; 
<date box="[631,854,943,969]" pageId="44" pageNumber="45" value="2009-09-26">
<collectingDate box="[631,854,943,969]" pageId="44" pageNumber="45" value="2009-09-26">26 September 2009</collectingDate>
</date>
; 
<collectorName box="[867,1028,943,969]" pageId="44" pageNumber="45">D. Atuy. Host</collectorName>
: Unidentified species (
<taxonomicName baseAuthorityName="Martins" baseAuthorityYear="1991" box="[1286,1424,943,970]" class="Arachnida" family="Sparassidae" kingdom="Animalia" order="Araneae" pageId="44" pageNumber="45" phylum="Arthropoda" rank="family">Sparassidae</taxonomicName>
