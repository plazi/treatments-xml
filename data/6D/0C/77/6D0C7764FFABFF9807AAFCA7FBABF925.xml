<location LSID="urn:lsid:plazi:treatment:6D0C7764FFABFF9807AAFCA7FBABF925:E07A90A9FFABFF98051DFC83FCE2FC45" box="[544,847,871,897]" country="Costa Rica" name="Bosque Nuboso El Cocora" pageId="24" pageNumber="25" stateProvince="Alajuela">Bosque Nuboso El Cocora</location>
</collectorName>
; 
<date box="[859,1014,871,897]" pageId="24" pageNumber="25" value="2012-05-10">
<collectingDate box="[859,1014,871,897]" pageId="24" pageNumber="25" value="2012-05-10">10 May 2012</collectingDate>
