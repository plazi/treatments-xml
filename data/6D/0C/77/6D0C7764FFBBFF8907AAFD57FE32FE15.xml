<taxonomicName authorityName="C.L.Koch" authorityYear="1850" box="[1291,1406,727,753]" class="Arachnida" family="Theraphosidae" genus="Lasiodora" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
<emphasis box="[1291,1406,727,753]" italics="true" pageId="8" pageNumber="9">Lasiodora</emphasis>
</taxonomicName>
cf 
<taxonomicName authority="Mello-Leit" authorityName="Mello-Leit" box="[198,465,763,789]" class="Arachnida" family="Theraphosidae" genus="Lasiodora" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="parahybana">
