, 
<location LSID="urn:lsid:plazi:treatment:6D0C7764FFA2FF9207AAFA0BFE21FF3E:E07A90A9FFA2FF910476F9F7FC40F9E9" box="[843,1005,1555,1581]" country="Chile" county="Concepcion Province" municipality="Talcahuano" name="Cerro Macera" pageId="17" pageNumber="18" stateProvince="Biobio">Cerro Macera</location>
