: 
<collectingRegion box="[250,415,1771,1797]" country="Chile" name="Maule" pageId="17" pageNumber="18">Maule Region</collectingRegion>
; 
<date box="[428,633,1771,1797]" pageId="17" pageNumber="18" value="2007-02-11">11 February 2007</date>
; C. M. Munoz. Host: 
<taxonomicName box="[886,986,1771,1797]" class="Arachnida" family="Theraphosidae" genus="Euathlus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis box="[886,986,1771,1797]" italics="true" pageId="17" pageNumber="18">Euathlus</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[1043,1214,1771,1798]" class="Arachnida" family="Theraphosidae" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="18" phylum="Arthropoda" rank="family">Theraphosidae</taxonomicName>
