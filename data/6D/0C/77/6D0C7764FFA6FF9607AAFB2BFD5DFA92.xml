<paragraph blockId="21.[151,1437,1231,2014]" pageId="21" pageNumber="22">
<materialsCitation ID-GBIF-Occurrence="2990869388" collectingDate="2020-01-21" collectorName="C. Neto. Host" country="Brazil" location="Ibitipoca" municipality="State" pageId="21" pageNumber="22" specimenCount="1" stateProvince="Minas Gerais">
