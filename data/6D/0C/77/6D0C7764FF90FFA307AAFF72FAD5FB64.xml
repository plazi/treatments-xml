<collectingMunicipality box="[507,709,510,536]" pageId="35" pageNumber="36">Malleco Province</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:6D0C7764FF90FFA307AAFF72FAD5FB64:E07A90A9FF90FFA305EEFE1AFC51FDDC" box="[723,1020,510,536]" country="Chile" county="La" municipality="Malleco Province" name="Nahuelbuta National Park" pageId="35" pageNumber="36" stateProvince="Araucania Region">Nahuelbuta National Park</location>
; 
<date box="[1033,1181,510,537]" pageId="35" pageNumber="36" value="2015-04-04">
<collectingDate box="[1033,1181,510,537]" pageId="35" pageNumber="36" value="2015-04-04">4 April 2015</collectingDate>
</date>
; 
<collectorName pageId="35" pageNumber="36">P. Vargas Betancourt. Host</collectorName>
