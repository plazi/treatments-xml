<collectingCountry box="[940,1107,897,923]" name="New Zealand" pageId="31" pageNumber="42">New Zealand</collectingCountry>
, 
<collectingCountry box="[1124,1235,897,923]" name="Australia" pageId="31" pageNumber="42">Australia</collectingCountry>
, 
<collectingCountry box="[1252,1319,897,923]" name="Chile" pageId="31" pageNumber="42">Chile</collectingCountry>
, 
<collectingCountry box="[264,339,937,963]" name="Brazil" pageId="31" pageNumber="42">Brazil</collectingCountry>
, 
<collectingCountry box="[352,472,937,963]" name="Argentina" pageId="31" pageNumber="42">Argentina</collectingCountry>
, and 
<collectingCountry box="[535,718,937,963]" name="New Caledonia" pageId="31" pageNumber="42">New Caledonia</collectingCountry>
(
<bibRefCitation author="Womersley" box="[735,946,937,963]" pageId="31" pageNumber="64" refString="Womersley, H. (1956) On some new Acarina-Mesostigmata from Australia, New Zealand and New Guinea. Journal of the Linnean Society (Zoology), 42, 505 - 599." type="journal article" year="1956">
