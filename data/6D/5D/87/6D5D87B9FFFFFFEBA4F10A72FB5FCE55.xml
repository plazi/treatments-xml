<taxonomicName authority="Franz" authorityName="Franz" box="[690,1049,915,938]" class="Insecta" family="Staphylinidae" genus="Lathomicrus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="27" phylum="Arthropoda" rank="species" species="sumatranus">
<emphasis box="[690,959,915,938]" italics="true" pageId="2" pageNumber="27">Lathomicrus sumatranus</emphasis>
(Franz)
</taxonomicName>
; 2― 
<taxonomicName authority="Jaloszynski" authorityName="Jaloszynski" class="Insecta" family="Staphylinidae" genus="Cephennomicrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="27" phylum="Arthropoda" rank="species" species="fesumatranus" status="sp. n.">
<emphasis box="[1101,1436,915,938]" italics="true" pageId="2" pageNumber="27">Cephennomicrus fesumatranus</emphasis>
Jałoszyński
</taxonomicName>
, 
