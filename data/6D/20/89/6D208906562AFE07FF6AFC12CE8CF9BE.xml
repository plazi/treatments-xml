<taxonomicName authority="Thomas" authorityName="Thomas" box="[344,662,1256,1282]" class="Mammalia" family="Muridae" genus="Oryzomys" kingdom="Animalia" order="Rodentia" pageId="10" pageNumber="49" phylum="Chordata" rank="species" species="wavrini">
<emphasis box="[344,559,1256,1281]" italics="true" pageId="10" pageNumber="49">Oryzomys wavrini</emphasis>
Thomas
</taxonomicName>
(
<typeStatus box="[677,729,1256,1282]" pageId="10" pageNumber="49">type</typeStatus>
locality in the Paraguayan Chaco) and 
<taxonomicName authority="Pine and Wetzel" authorityName="Pine and Wetzel" class="Mammalia" family="Muridae" genus="Oryzomys" kingdom="Animalia" order="Rodentia" pageId="10" pageNumber="49" phylum="Chordata" rank="subSpecies" species="wavrini" subSpecies="reigi">
