<taxonomicName authorityName="Bonelli" authorityYear="1810" class="Hexapoda" family="Carabidae" genus="Dromius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="804" pageNumber="805" phylum="Arthropoda" rank="genus">
