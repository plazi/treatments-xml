4. 
<taxonomicName box="[1319,1451,389,411]" class="Magnoliopsida" family="Euphorbiaceae" genus="Cnidoscolus" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="182" phylum="Tracheophyta" rank="species" species="mcvaughii">
