<taxonomicName authorityName="Fernandez Casas" authorityYear="2004" box="[1368,1451,505,526]" class="Magnoliopsida" family="Euphorbiaceae" genus="Cnidoscolus" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="182" phylum="Tracheophyta" rank="species" species="cervii">
