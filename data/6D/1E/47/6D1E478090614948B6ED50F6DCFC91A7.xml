</subSubSection>
<subSubSection pageId="5" pageNumber="6" type="nomenclature">
<paragraph pageId="5" pageNumber="6">
<taxonomicName LSID="http://zoobank.org/7CF47809-19D3-4348-9876-0D63D8EF95AB" class="Arachnida" family="Nesticidae" genus="Hamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hamus cornutus" order="Araneae" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="cornutus">Hamus cornutus</taxonomicName>
