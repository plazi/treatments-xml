<materialsCitation id="7EF13CA16529FFE5FE88A973FCAEFBF0" ID-GBIF-Occurrence="2987373309" collectingDate="XII. 21.1911" collectionCode="QM" pageId="5" pageNumber="685" specimenCode="QM Hy 773 (T773; T4412)" specimenCount="1" specimenCount-male="1" typeStatus="holotype">
<typeStatus id="1122885E6529FFE5FE88A973FE0EFC6A" box="[352,455,880,905]" pageId="5" pageNumber="685">Holotype</typeStatus>
<specimenCount id="D89FFD756529FFE5FE3CA973FDC4FC6A" box="[468,525,880,905]" pageId="5" pageNumber="685" type="male">male</specimenCount>
, 
<collectionCode id="A888AE396529FFE5FDF5A973FC9AFC6A" box="[541,851,880,905]" pageId="5" pageNumber="685">QM Hy 773 (T773; T4412)</collectionCode>
, under cracked resin and coverslip, along with a specimen of 
<taxonomicName id="09994D7F6529FFE5FDD7A991FC33FC48" authority="Gir. Transcription" authorityName="Gir. Transcription" box="[575,1018,914,939]" class="Insecta" family="Trichogrammatidae" genus="Aphelinoidea" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="685" phylum="Arthropoda" rank="species" species="howardi">
