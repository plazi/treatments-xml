<taxonomicName id="4C77C3B8FFFFD707FE8BF88C2CB4DB9A" authority="Archey, 1923" authorityName="Archey" authorityYear="1923" box="[271,497,1900,1925]" class="Chilopoda" family="Scolopocryptopidae" genus="Kartops" kingdom="Animalia" order="Scolopendromorpha" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
