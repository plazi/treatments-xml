<taxonomicName id="2DD44D74FFF7F713FF5DFC36FE687ECF" authorityName="Oláh &amp; Johanson &amp; Barnard" authorityYear="2008" box="[180,440,941,967]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="pali">
<emphasis id="D8A0EAE5FFF7F713FF5DFC36FE687ECF" bold="true" box="[180,440,941,967]" italics="true" pageId="9" pageNumber="10">Cheumatopsyche pali</emphasis>
