<taxonomicName id="850020EC4405614616380CECE96E5BB6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:31685" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName-HNS="Monomorium pharaonis (Linnaeus)" order="Hymenoptera" pageNumber="238" phylum="Arthropoda" rank="species" species="pharaonis" subGenus="Pharaophanes">M. (Pharaophanes) pharaonis (L.)</taxonomicName>
