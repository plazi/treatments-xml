<taxonomicName id="4C424D4F3B32E35FFF14F7D9FDA19563" authority="(Fabricius, 1798)" baseAuthorityName="Fabricius" baseAuthorityYear="1798" box="[210,546,2063,2085]" class="Malacostraca" family="Hexapodidae" genus="Hexapus" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="405" phylum="Arthropoda" rank="species" species="sexpes">
