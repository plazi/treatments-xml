<collectingRegion id="C042C9871457CD20FC5B5382FBEEF84A" box="[950,1136,1999,2025]" country="China" name="Fujian" pageId="16" pageNumber="17">Fujian Province</collectingRegion>
in 
<collectingCountry id="7A9147F51457CD20FB785382FB41F84A" box="[1173,1247,1999,2025]" name="China" pageId="16" pageNumber="17">China</collectingCountry>
