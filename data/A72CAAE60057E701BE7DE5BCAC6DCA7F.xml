<subSubSection id="15C840625A9603C75672F236A307203D" pageId="null" pageNumber="569" type="nomenclature">
<paragraph id="4E969737F8B4CF66C7ECDC8527697110" pageId="null" pageNumber="569">
<pageBreakToken id="BA330A3B36E57F6DBA93EB1EE326A866" pageId="null" pageNumber="569" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="0D96C6375899D62C8FB680E706953D2A" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="569" phylum="Tracheophyta" rank="species" species="alpinum">
Chrysanthemum 
<normalizedToken id="ED487E523BB13E7AD4D8D80C415DB4CA" originalValue="alpínum" pageId="null" pageNumber="569">alpinum</normalizedToken>
<authorityName id="5B0146214C73E9D58ACB0850E85FD52F" pageId="null" pageNumber="569">L.</authorityName>
</taxonomicName>
</paragraph>
