<quantity id="4C839B45911A3B4CFABFFC82FF14FCE4" metricMagnitude="3" metricUnit="m" metricValue="3.105" metricValueMax="4.36" metricValueMin="1.85" pageId="84" pageNumber="85" unit="m" value="3105.0" valueMax="4360.0" valueMin="1850.0">1850– 4360 m</quantity>
