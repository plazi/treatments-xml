<caption id="DF27BDA7D63A0628FF4EF91AC708F83F" ID-DOI="http://doi.org/10.5281/zenodo.6359127" ID-Zenodo-Dep="6359127" httpUri="https://zenodo.org/record/6359127/files/figure.png" pageId="31" pageNumber="32" startId="31.[189,232,1785,1811]" targetBox="[189,1398,464,1754]" targetPageId="31">
