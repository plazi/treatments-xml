<taxonomicName authorityName="Lauren A. Esposito &amp; Humberto Y. Yamaguti &amp; Cláudio A. Souza &amp; Ricardo Pinto-Da-Rocha &amp; Lorenzo Prendini" authorityYear="2017" baseAuthorityName="Karsch" baseAuthorityYear="1879" box="[818,1065,427,450]" class="Arachnida" family="Buthidae" genus="Heteroctenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scorpiones" pageId="10" pageNumber="10" phylum="Arthropoda" rank="species" species="princeps" status="comb. nov.">
