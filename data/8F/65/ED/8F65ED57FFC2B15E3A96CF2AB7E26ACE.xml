<taxonomicName authorityName="Thorell" authorityYear="1876" box="[373,637,658,682]" class="Arachnida" family="Buthidae" genus="Rhopalurus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Scorpiones" pageId="10" pageNumber="10" phylum="Arthropoda" rank="species" species="laticauda">
