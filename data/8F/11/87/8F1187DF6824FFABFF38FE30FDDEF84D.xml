<paragraph blockId="103.[151,1436,151,1976]" pageId="103" pageNumber="104">
<emphasis bold="true" box="[197,513,1627,1652]" inLineHeading="true" pageId="103" pageNumber="104" reason="1">Other material examined.</emphasis>
<materialsCitation collectingDate="1957-01-27" collectorName="Nova Teutonia" country="Brazil" elevation="400" location="Santa Catarina" pageId="103" pageNumber="104" specimenCount="1" specimenCount-male="1" stateProvince="Santa Catarina">
