<taxonomicName authorityName="Netolitzky" authorityYear="1914" class="Insecta" family="Carabidae" genus="Chrysobracteon" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="354" pageNumber="355" phylum="Arthropoda" rank="genus">
