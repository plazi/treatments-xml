<subSubSection pageId="12" pageNumber="27" type="nomenclature">
<paragraph pageId="12" pageNumber="27">
<taxonomicName LSID="http://zoobank.org/9E3094C1-2958-4E4B-9D02-C58FCEDC7594" authority="Waterhouse, 1879" authorityName="Waterhouse" authorityYear="1879" class="Insecta" family="Lycidae" genus="Eniclases" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eniclases pseudoluteolus" order="Coleoptera" pageId="12" pageNumber="27" phylum="Arthropoda" rank="species" species="pseudoluteolus">Eniclases pseudoluteolus</taxonomicName>
