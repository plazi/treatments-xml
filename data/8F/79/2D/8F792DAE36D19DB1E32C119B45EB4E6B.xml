<paragraph pageNumber="413">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:140869" class="Insecta" family="Formicidae" genus="Eciton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Eciton cristatum André" order="Hymenoptera" pageNumber="413" phylum="Arthropoda" rank="species" species="cristatum" subGenus.completionEvidence="missing, matching species" subGenus.innerRound="1" subGenus.outerRound="1">
