<heading bold="true" box="[151,943,1001,1028]" fontSize="11" level="1" pageId="13" pageNumber="500" reason="1">
<emphasis bold="true" box="[151,943,1001,1028]" pageId="13" pageNumber="500">
<taxonomicName authority="(Wells &amp; Rao, 1987)" authorityName="Mu &amp; Huys" authorityYear="2021" baseAuthorityName="Wells &amp; Rao" baseAuthorityYear="1987" box="[151,801,1001,1028]" class="Hexanauplia" family="Miraciidae" genus="Pseudobalucopsylla" kingdom="Animalia" order="Harpacticoida" pageId="13" pageNumber="500" phylum="Arthropoda" rank="species" species="triarticulata" status="comb. nov.">
