<location LSID="urn:lsid:plazi:treatment:8F5DE56C2804B10AFF2AF967320C853A:022B02A12805B10AFE47FE35342786F6" box="[506,584,403,429]" country="Costa Rica" county="Punta Morales" municipality="Gulf of Nicoya" name="Pacific" pageId="15" pageNumber="502">Pacific</location>
