<collectingCountry box="[304,359,1562,1586]" name="India" pageId="4" pageNumber="245">India</collectingCountry>
(
<collectingRegion box="[372,513,1562,1586]" country="India" name="West Bengal" pageId="4" pageNumber="245">West Bengal</collectingRegion>
).
</paragraph>
