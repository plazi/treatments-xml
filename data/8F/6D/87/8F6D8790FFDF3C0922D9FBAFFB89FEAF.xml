<taxonomicName box="[151,451,223,248]" class="Aves" family="Phalacrocoracidae" genus="Phalacrocorax" kingdom="Animalia" order="Suliformes" pageId="8" pageNumber="109" phylum="Chordata" rank="species" species="brasilianus">
