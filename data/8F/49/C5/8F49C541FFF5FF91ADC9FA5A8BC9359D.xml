<subSubSection pageId="7" pageNumber="48" type="etymology">
<paragraph blockId="7.[151,1436,152,738]" pageId="7" pageNumber="48">
<emphasis bold="true" box="[198,343,671,697]" pageId="7" pageNumber="48">Etymology:</emphasis>
