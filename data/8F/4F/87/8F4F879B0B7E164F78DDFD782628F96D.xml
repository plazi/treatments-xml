— 
<materialsCitation box="[538,1448,1526,1552]" collectingDate="2001-02" collectionCode="E" country="Brazil" location="Linha Copueiro" municipality="Machadinho" pageId="14" pageNumber="119" specimenCount="1" stateProvince="Rio Grande do Sul">
<collectingCountry box="[538,642,1526,1552]" name="Brazil" pageId="14" pageNumber="119">BRAZIL</collectingCountry>
. 
<collectingRegion box="[654,876,1526,1552]" country="Brazil" name="Rio Grande do Sul" pageId="14" pageNumber="119">Rio Grande do Sul</collectingRegion>
. 
