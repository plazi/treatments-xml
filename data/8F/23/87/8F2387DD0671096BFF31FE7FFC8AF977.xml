<collectingCountry box="[359,454,1695,1720]" name="Ecuador" pageId="102" pageNumber="103">Ecuador</collectingCountry>
, 
<collectingCountry box="[465,519,1696,1719]" name="Peru" pageId="102" pageNumber="103">Peru</collectingCountry>
, and 
<collectingCountry box="[578,641,1695,1720]" name="Chile" pageId="102" pageNumber="103">Chile</collectingCountry>
, intertidal to 
<specimenCount box="[790,854,1695,1720]" pageId="102" pageNumber="103" type="generic">
<quantity box="[790,849,1695,1720]" metricMagnitude="1" metricUnit="m" metricValue="2.5" pageId="102" pageNumber="103" unit="m" value="25.0">25 m</quantity>
.
</specimenCount>
</paragraph>
