) ... 
<taxonomicName authority="Ng, Padate &amp; Saravanane, 2019" authorityName="Ng, Padate &amp; Saravanane" authorityYear="2019" box="[946,1474,304,326]" class="Malacostraca" family="Homolidae" genus="Gordonopsis" kingdom="Animalia" order="Decapoda" pageId="38" pageNumber="305" phylum="Arthropoda" rank="species" species="robusta">
