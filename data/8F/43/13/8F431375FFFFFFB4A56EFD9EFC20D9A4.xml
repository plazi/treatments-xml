<specimenCount box="[399,446,1522,1549]" pageId="102" pageNumber="103" type="male">1 ♂</specimenCount>
; same collection data as for holotype; 
<specimenCode box="[897,1045,1522,1549]" collectionCode="MNRJ" country="Brazil" httpUri="http://grbio.org/cool/zi1i-a0b5" name="Museu Nacional/Universidade Federal de Rio de Janeiro" pageId="102" pageNumber="103">MNRJ 9201</specimenCode>
