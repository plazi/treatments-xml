<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Cercis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cercis" order="Fabales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="genus">Cercis</taxonomicName>
sp., 
<taxonomicName class="Magnoliopsida" family="Rutaceae" genus="Citrus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Citrus" order="Sapindales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="genus">Citrus</taxonomicName>
sp., 
<taxonomicName class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eucalyptus" order="Myrtales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="genus">Eucalyptus</taxonomicName>
sp., 
<taxonomicName class="Gastropoda" family="Ficidae" genus="Ficus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ficus carica" order="Littorinimorpha" pageId="12" pageNumber="81" phylum="Mollusca" rank="species" species="carica">Ficus carica</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Malvaceae" genus="Hibiscus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Hibiscus sabdariffa" order="Malvales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="species" species="sabdariffa">Hibiscus sabdariffa</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Lauraceae" genus="Laurus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Laurus nobilis" order="Laurales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="species" species="nobilis">Laurus nobilis</taxonomicName>
, 
<taxonomicName class="Aves" family="Sulidae" genus="Morus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Morus alba" order="Suliformes" pageId="12" pageNumber="81" phylum="Chordata" rank="species" species="alba">Morus alba</taxonomicName>
