<taxonomicName class="Insecta" family="Tingidae" genus="Ulmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ulmus" order="Hemiptera" pageId="12" pageNumber="81" phylum="Arthropoda" rank="genus">Ulmus</taxonomicName>
