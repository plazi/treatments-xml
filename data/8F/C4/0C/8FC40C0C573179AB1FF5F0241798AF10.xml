<taxonomicName class="Liliopsida" family="Poaceae" genus="Arundo" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Arundo" order="Poales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="genus">Arundo</taxonomicName>
sp., 
<taxonomicName class="Liliopsida" family="Poaceae" genus="Bambusa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Bambusa" order="Poales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="genus">Bambusa</taxonomicName>
sp., 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Ceratonia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ceratonia siliqua" order="Fabales" pageId="12" pageNumber="81" phylum="Tracheophyta" rank="species" species="siliqua">Ceratonia siliqua</taxonomicName>
