<taxonomicName box="[444,661,669,693]" class="Arachnida" family="Uloboridae" genus="Orinomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="203" phylum="Arthropoda" rank="species" species="lamprus">
<emphasis box="[444,661,669,693]" italics="true" pageId="2" pageNumber="203">Orinomus lamprus</emphasis>
</taxonomicName>
was synonymized by 
<bibRefCitation author="Opell" box="[932,1082,669,694]" pageId="2" pageNumber="203" refString="Opell, B. D. (1979) Revision of the genera and tropical American species of the spider family Uloboridae. Bulletin of the Museum of Comparative Zoology Harvard, 148, 443 - 549." type="journal article" year="1979">
Opell (
<number box="[1015,1073,670,694]" pageId="2" pageNumber="203" value="1979.0">1979</number>
)
</bibRefCitation>
with 
<taxonomicName authority="Keyserling, 1882" authorityName="Keyserling" authorityYear="1882" class="Arachnida" family="Uloboridae" genus="Uloborus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="203" phylum="Arthropoda" rank="species" species="bituberculatus">
<emphasis box="[1157,1436,669,693]" italics="true" pageId="2" pageNumber="203">Uloborus bituberculatus</emphasis>
Keyserling, 
<number box="[288,348,706,730]" pageId="2" pageNumber="203" value="1882.0">1882</number>
