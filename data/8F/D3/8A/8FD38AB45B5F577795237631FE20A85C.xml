<taxonomicName class="Insecta" genus="P." kingdom="Animalia" lsidName="P. a. subsp. pumilus" order="Coleoptera" pageId="0" pageNumber="193" phylum="Arthropoda" rank="subSpecies" species="a." subSpecies="pumilus">
