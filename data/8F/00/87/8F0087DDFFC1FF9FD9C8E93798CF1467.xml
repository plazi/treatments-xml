<subSubSection pageId="5" pageNumber="541" type="etymology">
<paragraph blockId="5.[151,1437,152,1797]" pageId="5" pageNumber="541">
<emphasis bold="true" box="[199,334,1735,1760]" pageId="5" pageNumber="541">Etymology.</emphasis>
