<taxonomicName authorityName="Matsumura" authorityYear="1916" box="[335,472,700,723]" class="Insecta" family="Issidae" genus="Sarima" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="489" phylum="Arthropoda" rank="species" species="rinkihonis">
