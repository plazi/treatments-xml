<paragraph id="02080659897DFF95C7F3645BFA08973E" blockId="2.[151,1437,1580,2037]" box="[151,1425,1580,1605]" pageId="2" pageNumber="531">
<emphasis id="30C3DA4B897DFF95C7F3645BFE16973E" bold="true" box="[151,399,1580,1605]" pageId="2" pageNumber="531">Specimens examined</emphasis>
. 
<specimenCount id="14B1CDD0897DFF95C6F9645BFE6F973E" box="[413,502,1580,1605]" pageId="2" pageNumber="531" type="male">
<number id="B47F147E897DFF95C6F9645BFE32973F" box="[413,427,1580,1604]" pageId="2" pageNumber="531" value="9.0">9</number>
