</taxonomicName>
sp. nov., and 
<taxonomicName id="5421E8D4A3EAE3BEE81EDA1174BE1A7F" genus="P." lsidName="P. sauteri" pageId="0" pageNumber="65" rank="species" species="sauteri">
<emphasis id="0743CB7EEBAA25766A9C4FB13D375456" italics="true" pageId="0" pageNumber="65">P. sauteri</emphasis>
</taxonomicName>
