<taxonomicName id="4C2B4D61CB0BFF93555C0342FD0EFD4B" authority="Boulard, 1975: 316" authorityName="Boulard" authorityPageNumber="316" authorityYear="1975" box="[343,668,626,650]" class="Insecta" family="Cicadidae" genus="Zammara" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="1585" phylum="Arthropoda" rank="species" species="columbia">
