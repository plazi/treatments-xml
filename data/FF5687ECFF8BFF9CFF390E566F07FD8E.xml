<taxonomicName id="B0FF4D79FF88FF92FEF40D426818F8CF" authority="Ahmad &amp; Ramamurthy" authorityName="Ahmad &amp; Ramamurthy" box="[346,804,1895,1918]" class="Insecta" family="Kerriidae" genus="Kerria" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="524" phylum="Arthropoda" rank="species" species="maduraiensis" status="sp. nov.">
