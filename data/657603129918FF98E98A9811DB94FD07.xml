<quantity id="2A271FE19918FF9FED369D89DB30F90F" box="[1067,1105,1701,1731]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="43" pageNumber="44" unit="in" value="1.0">1 in</quantity>
