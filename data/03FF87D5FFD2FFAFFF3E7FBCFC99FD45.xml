<quantity id="4CAE9B26FFD6FFAFFD557BCFFC85FD45" box="[764,846,658,682]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="4" pageNumber="77" unit="mm" value="19.0">19 mm</quantity>
