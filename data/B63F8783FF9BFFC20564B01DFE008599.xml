<emphasis id="0CE2EA87FF9BFFFD06F9B66CF956822C" box="[1493,1685,3107,3147]" italics="true" pageId="30" pageNumber="69">
<taxonomicName id="F9964D16FF9BFFFD06F9B66CF94C822C" authorityName="Haswell. B" authorityYear="1881" box="[1493,1679,3107,3147]" class="Malacostraca" family="Stenetriidae" genus="Stenetrium" kingdom="Animalia" order="Isopoda" pageId="30" pageNumber="69" phylum="Arthropoda" rank="genus">Stenetrium</taxonomicName>
