<taxonomicName id="6DFD27A9BF78FFC45AC2F90D37414C68" box="[445,559,1770,1796]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
