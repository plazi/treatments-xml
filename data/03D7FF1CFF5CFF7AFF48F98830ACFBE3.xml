<taxonomicName id="4C7E3589FF5CFF7AFBC2F8D535C3FC23" authorityName="MacLeay" authorityYear="1819" box="[1098,1215,955,979]" class="Insecta" family="Rutelidae" genus="Pelidnota" kingdom="Animalia" order="Coleoptera" pageId="151" pageNumber="150" phylum="Arthropoda" rank="genus">
