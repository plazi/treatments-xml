<taxonomicName id="4C7E3589FF5CFF7AFC9BF8D532FEFC23" authorityName="Burmeister" authorityYear="1844" box="[787,898,955,979]" class="Insecta" family="Rutelidae" genus="Strigidia" kingdom="Animalia" order="Coleoptera" pageId="151" pageNumber="150" phylum="Arthropoda" rank="genus">
