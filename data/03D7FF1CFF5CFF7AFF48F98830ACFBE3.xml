<taxonomicName id="4C7E3589FF5CFF7AFC0AF8B5351CFC03" authorityName="Ohaus" authorityYear="1912" box="[898,1120,987,1011]" class="Insecta" family="Rutelidae" genus="Pelidnota" kingdom="Animalia" order="Coleoptera" pageId="151" pageNumber="150" phylum="Arthropoda" rank="species" species="pennata">
