<taxonomicNameLabel id="95DE78485667D54AFD3FC38406A9F804" box="[658,715,1977,1998]" pageId="21" pageNumber="22" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 82 = 
<emphasis id="8EEDC5335667D54AFCA1C3870732F805" box="[780,848,1976,1999]" italics="true" pageId="21" pageNumber="22">P. t h a i i</emphasis>
<emphasis id="8EEDC5335667D54AFCF4C38407F0F804" bold="true" box="[857,914,1977,1998]" pageId="21" pageNumber="22">sp. n.</emphasis>
