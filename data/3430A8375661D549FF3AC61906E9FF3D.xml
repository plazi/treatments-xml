</emphasis>
, 78 = 
<emphasis id="8EEDC5335667D54AFEDAC3A605DDF87A" box="[375,447,1946,1968]" italics="true" pageId="21" pageNumber="22">P. t h a i i</emphasis>
<emphasis id="8EEDC5335667D54AFE61C3A7066AF865" bold="true" box="[460,520,1946,1967]" pageId="21" pageNumber="22">sp. n.</emphasis>
, 79 = 
<taxonomicName id="7B9962A25667D54AFDF9C3A60696F87A" box="[596,756,1946,1968]" class="Insecta" family="Teratomyzidae" genus="Poecilovitila" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="hindustanica" status="sp. n.">
<emphasis id="8EEDC5335667D54AFDF9C3A60603F87A" box="[596,609,1947,1968]" italics="true" pageId="21" pageNumber="22">P</emphasis>
. 
<emphasis id="8EEDC5335667D54AFDDCC3A70696F87A" box="[625,756,1946,1968]" italics="true" pageId="21" pageNumber="22">hindustanica</emphasis>
</taxonomicName>
<emphasis id="8EEDC5335667D54AFD53C3A70759F865" bold="true" box="[766,827,1946,1967]" pageId="21" pageNumber="22">
<taxonomicNameLabel id="95DE78485667D54AFD53C3A70759F865" box="[766,827,1946,1967]" pageId="21" pageNumber="22" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
80 
<emphasis id="8EEDC5335667D54AFCC5C3A6078FF87A" box="[872,1005,1946,1968]" italics="true" pageId="21" pageNumber="22">
= 
<taxonomicName id="7B9962A25667D54AFC2EC3A6078FF87A" box="[899,1005,1946,1968]" class="Insecta" family="Teratomyzidae" genus="Poecilovitila" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="elegans" status="sp. n.">P. elegans</taxonomicName>
</emphasis>
