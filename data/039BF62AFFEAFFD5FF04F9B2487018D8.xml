<emphasis id="B9469B2EFFEBFFD6FF04F9404AD41D4D" bold="true" box="[151,214,1786,1808]" pageId="41" pageNumber="42">
<number id="3DFA551BFFEBFFD6FF04F9404AD41D4D" box="[151,214,1786,1808]" pageId="41" pageNumber="42" value="97.5" valueMax="98.0" valueMin="97.0">97–98</number>
