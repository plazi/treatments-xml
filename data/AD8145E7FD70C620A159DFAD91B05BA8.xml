, 
<taxonomicName id="0E4EA20CE929173D252AB69A5B07AE91" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181920" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole peltastes Wilson" lsidName-HNS="Pheidole peltastes Wilson" order="Hymenoptera" pageNumber="535" phylum="Arthropoda" rank="species" species="peltastes">peltastes</taxonomicName>
, 
<taxonomicName id="13B931F5B1BD7208C1FC2D853193F3F2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181921" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole perkinsi Wilson" lsidName-HNS="Pheidole perkinsi Wilson" order="Hymenoptera" pageNumber="535" phylum="Arthropoda" rank="species" species="perkinsi">perkinsi</taxonomicName>
