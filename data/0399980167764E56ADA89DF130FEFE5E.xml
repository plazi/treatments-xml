<taxonomicName id="4C30529467764E56ADA89DF131E2FF1D" authority="Berg, 1889" authorityName="Berg" authorityYear="1889" box="[151,646,151,177]" class="Insecta" family="Tenebrionidae" genus="Praocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="42" phylum="Arthropoda" rank="species" species="sellata" subGenus="Hemipraocis">
<emphasis id="B944F50567764E56ADA89DF131E2FF1D" bold="true" box="[151,646,151,177]" pageId="3" pageNumber="42">
<emphasis id="B944F50567764E56ADA89DF13396FF1D" bold="true" box="[151,242,151,177]" italics="true" pageId="3" pageNumber="42">Praocis</emphasis>
(
<emphasis id="B944F50567764E56AC3C9DF132FAFF1D" bold="true" box="[259,414,151,177]" italics="true" pageId="3" pageNumber="42">Hemipraocis</emphasis>
) 
<emphasis id="B944F50567764E56AC909DF13299FF1D" bold="true" box="[431,509,151,177]" italics="true" pageId="3" pageNumber="42">sellata</emphasis>
<bibRefCitation id="EFA154E667764E56AF3B9DF131E2FF1D" author="Berg" box="[516,646,151,177]" pageId="3" pageNumber="42" refString="Berg, F. G. C. (1889) Quadraginta Coleoptera nova Argentina (Tenebrionidae). Anales de la Universidad de Buenos Aires, 4, 127 - 145." type="journal article" year="1889">
Berg, 
<number id="3DF83B3067764E56AF729DF131E2FF1D" box="[589,646,151,177]" pageId="3" pageNumber="42" value="1889.0">1889</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
