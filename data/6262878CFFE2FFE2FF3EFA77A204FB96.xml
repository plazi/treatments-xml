<subSubSection id="A2D16511FFE2FFE0FF3EF9CBA75CF9BA" pageId="13" pageNumber="464" type="etymology">
<paragraph id="EA74369AFFE2FFE0FF3EF9CBA75CF9BA" blockId="13.[151,1437,1557,1979]" pageId="13" pageNumber="464">
<emphasis id="D8BFEA88FFE2FFE0FF3EF9CBA711F9C6" bold="true" box="[151,286,1557,1583]" pageId="13" pageNumber="464">Etymology.</emphasis>
Latin 
<emphasis id="D8BFEA88FFE2FFE0FEC1F9C8A7B9F9C6" box="[360,438,1558,1583]" italics="true" pageId="13" pageNumber="464">pansus</emphasis>
