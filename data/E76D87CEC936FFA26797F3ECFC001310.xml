<collectingCountry id="17D37648C934FFA26711F272FE2F1317" box="[302,383,859,883]" name="Angola" pageId="10" pageNumber="61">Angola</collectingCountry>
, 
<collectingCountry id="17D37648C934FFA267B5F272FE8D1317" box="[394,477,859,883]" name="Uganda" pageId="10" pageNumber="61">Uganda</collectingCountry>
(
