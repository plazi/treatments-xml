<geoCoordinate id="A1ECA249AF52158B800F0AD65EC04498" degrees="9" direction="south" minutes="22" orientation="latitude" precision="925" value="-9.366667">9°22'S</geoCoordinate>
, 
<geoCoordinate id="2BAD867795EFAE5FBCC447A2E9C4C0C2" degrees="147" direction="east" minutes="32" orientation="longitude" precision="925" value="147.53334">147°32'E</geoCoordinate>
