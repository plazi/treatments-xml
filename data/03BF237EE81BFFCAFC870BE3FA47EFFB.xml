<keyLead id="30E7315DE81CFFCAFC870C61FA47EEC3" pageId="4" pageNumber="14">
<number id="3DDE804FE81CFFCAFC870C61FF7AEE13" box="[151,169,592,618]" pageId="4" pageNumber="14" value="5.0">5</number>
