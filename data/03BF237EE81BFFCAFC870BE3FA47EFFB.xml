(
<bibRefCitation id="EF87EF99E81BFFCDF91008D4FA5FEA86" author="Kano" box="[1280,1420,1765,1791]" pageId="3" pageNumber="24" refString="Kano, T. (1931) Some new or unrecorded cetoniid-beetles from Formosa. Annotationes Zoologicae Japonenses, 17, 127 - 134." type="journal article" year="1931">
