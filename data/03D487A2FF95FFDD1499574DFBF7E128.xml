<taxonomicName id="4C7D4D37FF94FFDD165A5143FD4FE128" box="[596,720,260,285]" class="Insecta" family="Tettigoniidae" genus="Phlugiolopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="350" phylum="Arthropoda" rank="species" species="tribranchis">
