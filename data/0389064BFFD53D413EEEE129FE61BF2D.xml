sp. cf. 
<taxonomicName id="4C20CCDEFFD53D413FD5E129FD25B99C" authority="Collins 1974" authorityName="Collins" authorityYear="1974" box="[428,744,620,645]" class="Tubothalamea" family="Hauerinidae" genus="Quinqueloculina" kingdom="Chromista" order="Miliolida" pageId="47" pageNumber="48" phylum="Foraminifera" rank="species" species="victoriensis">
