<taxonomicName id="4C534D7CE537FF9F6ED5B95DFEB36977" box="[151,282,817,842]" class="Aves" family="Alaudidae" genus="Eremophila" kingdom="Animalia" order="Passeriformes" pageId="75" pageNumber="76" phylum="Chordata" rank="genus">
