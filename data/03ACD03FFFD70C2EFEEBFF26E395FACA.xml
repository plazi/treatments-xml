<taxonomicName id="4C051AAAFFD60C2EFF19FBAAE1B3FBFB" authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[251,452,1059,1080]" class="Insecta" family="Staphylinidae" genus="Lobrathium" kingdom="Animalia" order="Coleoptera" pageId="55" pageNumber="356" phylum="Arthropoda" rank="species" species="spoliatum" status="sp. n.">
