<taxonomicName id="4C7215C4A77EFF97FD6FFBC437395B1D" authorityName="Tjeder" authorityYear="1967" box="[757,832,1089,1114]" class="Insecta" family="Nemopteridae" genus="Nemopterella" kingdom="Animalia" order="Neuroptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="exigua">
