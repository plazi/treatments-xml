<taxonomicName id="FD6EFEDECF28DE03CF818478D0218628" genus="T." lsidName="T. larvata" pageId="0" pageNumber="1" rank="species" species="larvata">T. larvata</taxonomicName>
, 
<taxonomicName id="6F7D14DFFF66BBFE12CB0F75657FF539" genus="T." lsidName="T. unimaculata" pageId="0" pageNumber="1" rank="species" species="unimaculata">T. unimaculata</taxonomicName>
