<taxonomicName id="45D2DD90CD930041D9D9655BE1C2E8EB" genus="T." lsidName="T. kringi" pageId="0" pageNumber="1" rank="species" species="kringi">T. kringi</taxonomicName>
, and 
<taxonomicName id="E597140E38AC92C74D6B6F14AEFCC894" genus="T." lsidName="T. mohawk" pageId="0" pageNumber="1" rank="species" species="mohawk">T. mohawk</taxonomicName>
). 
<taxonomicName id="4D3A877CC90C00040CB06A5699F28C1A" genus="T." lsidName="T. trimaculata" pageId="0" pageNumber="1" rank="species" species="trimaculata">T. trimaculata</taxonomicName>
