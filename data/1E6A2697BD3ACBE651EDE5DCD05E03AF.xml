<taxonomicName id="E35E7584BF800E12173C252E87AA3151" genus="T." lsidName="T. larvata" pageId="0" pageNumber="1" rank="species" species="larvata">T. larvata</taxonomicName>
by being rounder (dorsum length/width = 1.2-1.37 in 
<taxonomicName id="C5ED437919FD5D7EF8475317A6574C34" genus="T." lsidName="T. trimaculata" pageId="0" pageNumber="1" rank="species" species="trimaculata">T. trimaculata</taxonomicName>
