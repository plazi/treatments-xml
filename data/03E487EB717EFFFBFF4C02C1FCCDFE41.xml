<taxonomicName id="4C4D4D7E717EFFFBFBD40342FEF2FE46" authorityName="JAQUELIN DU VAL" authorityYear="1850" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="149" phylum="Arthropoda" rank="species" species="guynemeri">
<emphasis id="B939EAEF717EFFFBFBD40342FEF2FE46" italics="true" pageId="12" pageNumber="149">S. guynemeri</emphasis>
</taxonomicName>
