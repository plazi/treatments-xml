<taxonomicName id="4C3B4D34955EFFB9CFF75F8FFE4125F3" authority="Linck, 1790: 31" authorityName="Linck" authorityPageNumber="31" authorityYear="1790" box="[151,440,1410,1436]" class="Elasmobranchii" family="Triakidae" genus="Mustelus" kingdom="Animalia" order="Carcharhiniformes" pageId="55" pageNumber="56" phylum="Chordata" rank="genus">
<emphasis id="B94FEAA5955EFFB9CFF75F8FFF0525F3" box="[151,252,1410,1436]" italics="true" pageId="55" pageNumber="56">Mustelus</emphasis>
<bibRefCitation id="EFAA4B46955EFFB9CE635F8FFE4125F3" author="Linck" box="[259,440,1410,1436]" firstAuthor="Linck" page="31" pageId="55" pageNumber="56" pagination="28 - 38" refId="ref108742" refString="Linck, H. F. (1790) Versuch einter Einthei 1 ung der Fische nach den Ziihnen. Magazinfiir das Neueste Physik und Naturggeschichte, Gotha, 6, 28 - 38." type="journal article" year="1790">Linck, 1790: 31</bibRefCitation>
