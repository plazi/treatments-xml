<taxonomicName id="1CC38486DB756ACF15186A47BA71F2B5" authority="L." class="Liliopsida" family="Poaceae" genus="Dactylis" kingdom="Plantae" order="Poales" pageId="null" pageNumber="320" phylum="Tracheophyta" rank="species" species="glomerata">
