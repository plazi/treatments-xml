<taxonomicName id="4C624D1CFFE8FFABFD6EFB089DE7E44F" authority="Robineau-Desvoidy, 1830" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[741,1262,1272,1298]" class="Insecta" family="Tachinidae" genus="Carcelia" kingdom="Animalia" order="Diptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="nigripes">
