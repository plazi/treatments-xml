<taxonomicName id="4C624D1CFFE8FFABFCCFFB5D9D34E59A" authority=", Fall." authorityName="Fall." box="[836,1085,1197,1223]" class="Insecta" family="Tachinidae" genus="Tachina" kingdom="Animalia" order="Diptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="affinis">
