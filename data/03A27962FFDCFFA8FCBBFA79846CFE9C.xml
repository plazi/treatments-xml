<taxonomicName id="4C0BB3F7FFDCFFA9FBC5FA7A81CEFDF3" box="[1051,1233,517,541]" class="Arachnida" family="Tenuipalpidae" genus="Raoiella" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="434" phylum="Arthropoda" rank="species" species="indica">
