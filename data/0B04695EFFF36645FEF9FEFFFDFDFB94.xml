<emphasis id="E26B88386B02F4BEFA8D94EDC2F1D50D" bold="true" pageId="6" pageNumber="137">Genus</emphasis>
<taxonomicName id="44ADA3CBFFF36645FE40FEFFFD4BFEFC" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[391,646,306,327]" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="137" phylum="Arthropoda" rank="genus">
