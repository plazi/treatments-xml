(
<taxonomicName id="D0C708AA6DFF6915FE05D685BDAE829E" authority="Thuill., R. Stevenii" authorityName="Thuill., R. Stevenii" class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="99" phylum="Tracheophyta" rank="species" species="silvaticus">
