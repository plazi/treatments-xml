</taxonomicName>
: 
<taxonomicName id="DDDC4D12FFC6FF82FB7FFDB5FAC1722C" authority="insignis" authorityName="insignis (Fox" authorityYear="1899" box="[1174,1394,571,594]" class="Insecta" family="Eumenidae" genus="Alphamenes" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="535" phylum="Arthropoda" rank="subSpecies" species="insignis" subSpecies="insignis">
