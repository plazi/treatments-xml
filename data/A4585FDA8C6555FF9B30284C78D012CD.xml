<paragraph id="C6B1BF69B6849AC1F9198ED5B5231F97" pageId="0" pageNumber="143">
<taxonomicName id="C569CF592A75ACBABA19F16614C8DC88" baseAuthorityName="Schlegel" baseAuthorityYear="1836" class="Reptilia" family="Gekkonidae" genus="Gekko" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gekko monarchus" order="Squamata" pageId="0" pageNumber="143" phylum="Chordata" rank="species" species="monarchus">
