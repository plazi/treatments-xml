SPECIES
</collectionCode>
. — 
<taxonomicName id="F8BF17A79B3CAA219512B304B4590572" authority="Johnston, 1836" authorityName="Johnston" authorityYear="1836" box="[302,603,823,847]" class="Polychaeta" family="Hesionidae" genus="Psamathe" kingdom="Animalia" order="Phyllodocida" pageId="4" pageNumber="3" phylum="Annelida" rank="species" species="fusca">
