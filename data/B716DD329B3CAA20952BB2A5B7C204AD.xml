<taxonomicName id="F8BF17A79B3CAA2190E5B286B33F04F2" authorityName="Johnston" authorityYear="1836" box="[1241,1341,693,719]" class="Polychaeta" family="Hesionidae" genus="Psamathe" kingdom="Animalia" order="Phyllodocida" pageId="4" pageNumber="3" phylum="Annelida" rank="genus">
