<quantity id="022F9B79BF74F920FDAE4E0EFD93FAAA" box="[532,620,1304,1328]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="9" pageNumber="154" unit="m" value="9.0" valueMax="10.0" valueMin="8.0">
