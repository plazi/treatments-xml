<taxonomicName id="DA85C3A0FFFAC85CD793DC9C8AC71DF3" box="[749,897,1224,1245]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="ningxiaense">
