<emphasis id="2FF16431FFFAC85CD6F0DC938AE61DF3" bold="true" box="[910,928,1223,1245]" pageId="2" pageNumber="3">C</emphasis>
— 
<taxonomicName id="DA85C3A0FFFAC85CD6C7DC9C8D001DF2" box="[953,1094,1223,1245]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="baiyunense">
<emphasis id="2FF16431FFFAC85CD6C7DC9C8D001DF2" box="[953,1094,1223,1245]" italics="true" pageId="2" pageNumber="3">L. baiyunense</emphasis>
</taxonomicName>
