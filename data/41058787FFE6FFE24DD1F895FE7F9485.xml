<taxonomicName id="0EAC4D12FFE5FFE34C65F842FE1A94F9" authority="Naveed &amp; Zhang, 2018" authorityName="Naveed &amp; Zhang" authorityYear="2018" box="[291,457,1929,1951]" class="Insecta" family="Cicadellidae" genus="Hecalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="582" phylum="Arthropoda" rank="species" species="erectus" status="sp. nov.">
