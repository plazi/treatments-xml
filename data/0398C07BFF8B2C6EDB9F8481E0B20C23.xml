<subSubSection id="C32B22E6FF8B2C6EDB9F84C6E7610B43" box="[151,881,508,534]" pageId="5" pageNumber="552" type="materials_examined">
<paragraph id="8B8E716DFF8B2C6EDB9F84C6E7610B43" blockId="5.[151,881,508,534]" box="[151,881,508,534]" pageId="5" pageNumber="552">
<typeStatus id="548ACFCFFF8B2C6EDB9F84C6E4D90B40" box="[151,201,510,534]" pageId="5" pageNumber="552">Type</typeStatus>
