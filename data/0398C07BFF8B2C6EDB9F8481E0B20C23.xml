<taxonomicName id="4C310AEEFF8B2C6EDA2E84C6E6740B40" authority="Deng" authorityName="Zhang &amp; Zhao &amp; Wu &amp; Deng" authorityYear="2020" box="[294,612,510,534]" class="Insecta" family="Tetrigidae" genus="Hainantettix" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="552" phylum="Arthropoda" rank="species" species="strictivertex" status="sp. nov.">
