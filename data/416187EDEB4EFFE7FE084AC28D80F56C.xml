<taxonomicName id="0EC84D78EB41FFE7FB614CF48E24FAA3" authority="Stal" authorityName="Stal" authorityYear="1865" box="[1233,1617,1955,1998]" class="Insecta" family="Cicadellidae" genus="Tartessus" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="131" phylum="Arthropoda" rank="species" species="fieberi">
<emphasis id="FBBCEAE9EB41FFE7FB614CF48D82FAA3" box="[1233,1527,1955,1998]" italics="true" pageId="24" pageNumber="131">Tartessus fieberi</emphasis>
