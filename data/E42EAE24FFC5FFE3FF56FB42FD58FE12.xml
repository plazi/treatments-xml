<taxonomicName id="AB8764B1FFC4FFE0FB6EF93CFAA9F8BD" authorityName="Robillard &amp; Desutter-Grandcolas" authorityYear="2008" box="[1226,1338,1777,1794]" class="Insecta" family="Gryllidae" genus="Matuanus" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="278" phylum="Arthropoda" rank="species" species="rectinervus">
