<emphasis id="A03CA7F3F074D219B931BD4B9806FE54" bold="true" pageId="88" pageNumber="89">B</emphasis>
<taxonomicName id="5FB3EDF0F7DD1FCFFFA02FE43AA61661" family="Chenopodiaceae" genus="C." kingdom="Plantae" lsidName="C. lepidocarpum" order="Caryophyllales" pageId="88" pageNumber="89" rank="species" species="lepidocarpum">
<emphasis id="14B7ECACA1F22573AD793A652048F86F" italics="true" pageId="88" pageNumber="89">C. lepidocarpum</emphasis>
