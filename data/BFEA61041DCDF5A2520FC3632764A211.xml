<emphasis id="3EA4C292E90D0E9029F1AA4887D291F7" bold="true" pageId="88" pageNumber="89">A</emphasis>
<taxonomicName id="CA2784FA6EAA6968A3505999E0C85166" family="Chenopodiaceae" genus="C." kingdom="Plantae" lsidName="C. lhasaense" order="Caryophyllales" pageId="88" pageNumber="89" rank="species" species="lhasaense">
<emphasis id="EC5CFEF10960DA0E7E92A07D9E38FCAB" italics="true" pageId="88" pageNumber="89">C. lhasaense</emphasis>
