<taxonomicName id="2531E7EE0FE65B5356628D7E727D6718" authorityName="L., Sp. Pl.: 4" authorityYear="1753" class="Magnoliopsida" family="Chenopodiaceae" genus="Corispermum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Corispermum" order="Caryophyllales" pageId="88" pageNumber="89" phylum="Tracheophyta" rank="genus">
<emphasis id="850369B018E05413429CA79993C0506E" italics="true" pageId="88" pageNumber="89">Corispermum</emphasis>
</taxonomicName>
