<materialsCitation id="A4CE3C8F1874FFE2FF65FCD74D55FC77" ID-GBIF-Occurrence="1326058681" collectionCode="USNM" county="Baker" location="Pauai" municipality="Luzon" pageId="3" pageNumber="489" specimenCount="1" stateProvince="Benguet Province" typeStatus="holotype">
