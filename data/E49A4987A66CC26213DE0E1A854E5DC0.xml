74. 
<taxonomicName id="A9DD389AC98ABA918A4DC7F7A44722F0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:135354" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica diligens Smith" order="Hymenoptera" pageId="22" pageNumber="22" phylum="Arthropoda" rank="species" species="diligens">Formica diligens</taxonomicName>
