<date id="FFECCFB2DD2C8B6FFDD7135CFD21C21D" box="[634,748,1296,1322]" pageId="10" pageNumber="57" value="1920-06-20">
<number id="3D9AFB55DD2C8B6FFDD7135CFD57C21D" box="[634,666,1296,1322]" pageId="10" pageNumber="57" value="20.0">20</number>
, VI 
<number id="3D9AFB55DD2C8B6FFD61135CFD21C21D" box="[716,748,1296,1322]" pageId="10" pageNumber="57" value="20.0">20</number>
</date>
, 
<date id="FFECCFB2DD2C8B6FFD55135CFC84C21D" box="[760,841,1296,1322]" pageId="10" pageNumber="57" value="1940-07">
