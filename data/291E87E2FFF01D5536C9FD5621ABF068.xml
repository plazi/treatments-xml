(
<emphasis id="93C3EAE6FFFF1D573799F9B42270F325" box="[424,589,1621,1642]" italics="true" pageId="40" pageNumber="1441">Hemigyroneuron</emphasis>
) 
<emphasis id="93C3EAE6FFFF1D57346AF9B42280F325" box="[603,701,1621,1642]" italics="true" pageId="40" pageNumber="1441">meruensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="88F0579DFFFF1D5734F1F9B42333F324" box="[704,782,1621,1643]" pageId="40" pageNumber="1441" rank="species">sp. nov.</taxonomicNameLabel>
, 
<superScript id="56C29BBCFFFF1D573524F9B4235CF32F" attach="left" box="[789,865,1617,1643]" fontSize="6" pageId="40" pageNumber="1441">CellD®</superScript>
