<taxonomicName id="4C0F028FFFFDE767FBEA1FB73F5A66A9" authority="Gladius" authorityName="Gladius" box="[1071,1331,1236,1258]" class="Actinopterygii" family="Kyphosidae" genus="Kyphosus" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="gladius">
