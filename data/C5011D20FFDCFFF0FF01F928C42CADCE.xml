<taxonomicName id="8AA8D7B5FFDCFFFEFF67F896C41CAB01" authority="Nolf D" authorityName="Nolf D" box="[241,560,1828,1850]" class="Actinopterygii" family="Paraulopidae" genus="Paraulopus" kingdom="Animalia" order="Aulopiformes" pageId="7" pageNumber="408" phylum="Chordata" rank="species" species="brevirostris">
