<taxonomicName id="4C384D40437C6B63FE85FA87FD1AFDE4" authority="M. R. Schomb., Reis. Br." authorityName="M. R. Schomb., Reis. Br." box="[290,734,499,526]" class="Polypodiopsida" family="Davalliaceae" kingdom="Plantae" order="Polypodiales" pageId="11" pageNumber="18" phylum="Tracheophyta" rank="family">
