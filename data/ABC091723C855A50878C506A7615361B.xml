<taxonomicName id="07A0DC60FC8B3B957559E0D75FF8107B" authorityName="Li &amp; Chang" authorityYear="2020" class="Arachnida" family="Psilodercidae" genus="Psiloderces" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psiloderces grohotensis" order="Araneae" pageId="0" pageNumber="61" phylum="Arthropoda" rank="species" species="grohotensis">
