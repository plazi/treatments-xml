, 
<taxonomicName class="Eutardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="6" pageNumber="30" phylum="Tardigrada" rank="species" species="semmelweisi">
<emphasis italics="true" pageId="6" pageNumber="30">M. semmelweisi</emphasis>
</taxonomicName>
and 
<taxonomicName box="[356,548,1142,1167]" class="Eutardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="6" pageNumber="30" phylum="Tardigrada" rank="species" species="drakensbergi">
<emphasis box="[356,548,1142,1167]" italics="true" pageId="6" pageNumber="30">M. drakensbergi</emphasis>
