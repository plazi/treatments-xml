, 
<taxonomicName authority="Dastych 1993" authorityName="Dastych" authorityYear="1993" class="Eutardigrada" family="Macrobiotidae" genus="Calcarobiotus" kingdom="Animalia" order="Parachela" pageId="1" pageNumber="25" phylum="Tardigrada" rank="species" species="filmeri" subGenus="Calcarobiotus">
