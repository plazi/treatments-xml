<taxonomicName authorityName="C.Linnaeus" authorityYear="1753" box="[433,617,860,886]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="168" pageNumber="169" phylum="Tracheophyta" rank="species" species="sylvatica">
