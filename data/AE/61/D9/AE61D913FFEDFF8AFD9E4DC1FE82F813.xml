</taxonomicName>
, 
<taxonomicName box="[341,427,1934,1960]" class="Magnoliopsida" family="Asteraceae" genus="Kleinia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="genus">
<emphasis box="[341,427,1934,1960]" italics="true" pageId="76" pageNumber="77">Kleinia</emphasis>
</taxonomicName>
