<taxonomicName authority=", Neochamaelia" authorityName="Neochamaelia" box="[445,784,1934,1961]" class="Magnoliopsida" family="Zygophyllaceae" genus="Zygophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Zygophyllales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="genus">
<emphasis box="[445,598,1934,1960]" italics="true" pageId="76" pageNumber="77">Zygophyllum</emphasis>
, 
<emphasis box="[615,784,1934,1960]" italics="true" pageId="76" pageNumber="77">Neochamaelia</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="W.Aiton" authorityYear="1789" box="[802,906,1934,1960]" class="Magnoliopsida" family="Rubiaceae" genus="Plocama" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="genus">
