<taxonomicName baseAuthorityName="Attems" baseAuthorityYear="1935" box="[705,878,425,451]" class="Magnoliopsida" family="Rubiaceae" genus="Megaphyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="122" pageNumber="123" phylum="Tracheophyta" rank="species" species="imbecillum">
