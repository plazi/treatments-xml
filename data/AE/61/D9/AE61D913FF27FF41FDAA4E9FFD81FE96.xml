<taxonomicName baseAuthorityName="Brolemann" baseAuthorityYear="1921" box="[841,978,267,294]" class="Diplopoda" family="Julidae" genus="Ommatoiulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Julida" pageId="135" pageNumber="136" phylum="Arthropoda" rank="species" species="lienharti">
