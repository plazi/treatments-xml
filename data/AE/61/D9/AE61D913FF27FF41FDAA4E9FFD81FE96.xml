<collectingRegion box="[512,582,1942,1968]" country="Portugal" name="Braga" pageId="134" pageNumber="135">Braga</collectingRegion>
in 
<collectingCountry box="[617,716,1941,1967]" name="Portugal" pageId="134" pageNumber="135">Portugal</collectingCountry>
(as 
<taxonomicName authorityName="Verhoeff" authorityYear="1925" box="[763,1057,1941,1967]" class="Magnoliopsida" family="Asteraceae" genus="Schizophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="134" pageNumber="135" phylum="Tracheophyta" rank="species" species="involutum">
<emphasis box="[763,1057,1941,1967]" italics="true" pageId="134" pageNumber="135">Schizophyllum involutum</emphasis>
</taxonomicName>
) and Pontevedra, La Coru√±a, Lugo, Oviedo, Segovia (as 
<taxonomicName authorityName="Verhoeff" authorityYear="1921" box="[530,787,1976,2003]" class="Magnoliopsida" family="Asteraceae" genus="Schizophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="134" pageNumber="135" phylum="Tracheophyta" rank="species" species="fissum">
<emphasis box="[530,787,1976,2003]" italics="true" pageId="134" pageNumber="135">Schizophyllum fissum</emphasis>
</taxonomicName>
) and 
<collectingRegion box="[864,950,1976,2002]" country="Spain" name="Madrid" pageId="134" pageNumber="135">Madrid</collectingRegion>
(as 
<taxonomicName authorityName="Verhoeff" authorityYear="1921" box="[1010,1266,1976,2003]" class="Magnoliopsida" family="Asteraceae" genus="Schizophyllum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="134" pageNumber="135" phylum="Tracheophyta" rank="species" species="fissum">
<emphasis box="[1010,1266,1976,2003]" italics="true" pageId="134" pageNumber="135">Schizophyllum fissum</emphasis>
</taxonomicName>
) in 
<collectingCountry box="[1324,1395,1976,2002]" name="Spain" pageId="134" pageNumber="135">Spain</collectingCountry>
.
</paragraph>
