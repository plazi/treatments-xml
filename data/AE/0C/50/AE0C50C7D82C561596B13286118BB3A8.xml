<emphasis bold="true" pageId="0" pageNumber="1">Figure 5.</emphasis>
<taxonomicName baseAuthorityName="Ashmead" baseAuthorityYear="1904" class="Insecta" family="Chalcididae" genus="Haltichella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haltichella clavicornis" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="clavicornis">
