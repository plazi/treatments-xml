<emphasis bold="true" box="[974,1015,1041,1065]" italics="true" pageId="5" pageNumber="4">Cy.</emphasis>
(
<emphasis bold="true" box="[1033,1076,1041,1065]" italics="true" pageId="5" pageNumber="4">Co.</emphasis>
) 
<emphasis bold="true" box="[1094,1225,1041,1065]" italics="true" pageId="5" pageNumber="4">conicollis</emphasis>
