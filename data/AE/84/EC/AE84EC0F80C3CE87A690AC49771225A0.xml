<subSubSection pageId="0" pageNumber="1" type="etymology">
<paragraph pageId="0" pageNumber="1">Etymology.</paragraph>
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Sharkey &amp; van Achterberg" authorityYear="2021" class="Insecta" family="Braconidae" genus="Dolichozele" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dolichozele josephinerodriguezae" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="josephinerodriguezae">
