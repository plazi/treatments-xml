). B 
<number box="[666,754,1558,1583]" pageId="9" pageNumber="170" value="6.300000000000001" valueMax="6.4" valueMin="6.2">6.2–6.4</number>
; A 
<number box="[792,879,1559,1583]" pageId="9" pageNumber="170" value="3.1500000000000004" valueMax="3.2" valueMin="3.1">3.1–3.2</number>
; F 
<number box="[914,999,1558,1583]" pageId="9" pageNumber="170" value="3.65" valueMax="3.8" valueMin="3.5">3.5–3.8</number>
(
<typeStatus box="[1006,1115,1558,1583]" pageId="9" pageNumber="170">Holotype</typeStatus>
: B 
<number box="[1152,1190,1558,1583]" pageId="9" pageNumber="170" value="6.4">6.4</number>
; A 
