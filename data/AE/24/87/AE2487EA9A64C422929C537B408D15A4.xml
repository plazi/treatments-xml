<subSubSection pageId="11" pageNumber="374" type="etymology">
<paragraph blockId="11.[151,1436,1761,2040]" pageId="11" pageNumber="374">
<emphasis bold="true" box="[199,338,1978,2004]" pageId="11" pageNumber="374">Etymology:</emphasis>
