A–I 
<taxonomicName authority="Donnell &amp; C. Takeuchi A" authorityName="Donnell &amp; C. Takeuchi A" authorityYear="2014" baseAuthorityName="Donnell &amp; C. Takeuchi A" baseAuthorityYear="2014" box="[301,740,2032,2054]" class="Magnoliopsida" family="Malvaceae" genus="Callianthe" kingdom="Plantae" order="Malvales" pageId="1" pageNumber="299" phylum="Tracheophyta" rank="species" species="montana">
