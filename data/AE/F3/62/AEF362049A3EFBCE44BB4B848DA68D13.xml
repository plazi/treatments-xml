and 
<taxonomicName class="Actinopterygii" family="Cichlidae" genus="Hypsophrys" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="63" phylum="Chordata" rank="species" species="nematopus">H. nematopus</taxonomicName>
