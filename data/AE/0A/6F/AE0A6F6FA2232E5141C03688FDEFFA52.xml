<materialsCitation ID-GBIF-Occurrence="1457407324" collectionCode="NHM" country="China" county="Wolong" location="The" municipality="Vietnamese" pageId="10" pageNumber="547" specimenCount="1" specimenCount-male="1" stateProvince="Sichuan province">
<specimenCount box="[997,1034,1123,1147]" pageId="10" pageNumber="547" type="male">1♂</specimenCount>
, 
