<taxonomicName authorityName="Schoenherr" authorityYear="1825" class="Insecta" family="Rutaceae" genus="Cratosomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="14" pageNumber="65" phylum="Arthropoda" rank="family">Rutaceae</taxonomicName>
