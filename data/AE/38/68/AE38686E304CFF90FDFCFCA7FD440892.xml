<collectionCode box="[1163,1251,1251,1277]" country="Spain" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35275" lsid="urn:lsid:biocol.org:col:35275" name="Museo Nacional de Ciencias Naturales" pageId="21" pageNumber="22" type="Museum">MNCN</collectionCode>
