, 
<collectingCountry id="81F4763DB24E12317C1EF988FD33F9D6" box="[589,747,1542,1566]" name="New Zealand" pageId="33" pageNumber="534">New Zealand</collectingCountry>
, 
<collectingCountry id="81F4763DB24E12317CAFF989FC4AF9D6" box="[764,914,1542,1566]" name="South Africa" pageId="33" pageNumber="534">South Africa</collectingCountry>
