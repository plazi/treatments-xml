<collectingCountry id="81F4763DB24E12317A73F989FB5CF9D6" box="[1056,1156,1542,1566]" name="United States of America" pageId="33" pageNumber="534">America</collectingCountry>
. 
<collectingCountry id="81F4763DB24E12317AC7F989FB2BF9D6" box="[1172,1267,1542,1566]" name="Réunion" pageId="33" pageNumber="534">Reunion</collectingCountry>
