<taxonomicName id="37881B19A047460E960DD7057C26FFF0" class="Agaricomycetes" family="Hydnaceae" genus="Hydnum" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hydnum subrufescens" order="Cantharellales" pageId="0" pageNumber="35" phylum="Basidiomycota" rank="species" species="subrufescens">Hydnum subrufescens</taxonomicName>
