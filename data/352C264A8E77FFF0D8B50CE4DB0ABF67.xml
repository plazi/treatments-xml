<quantity id="7A7D3AB98E77FFF1D9750A93D89DB801" box="[306,426,1514,1538]" metricMagnitude="2" metricUnit="m" metricValue="3.15" metricValueMax="4.3" metricValueMin="2.0" pageId="46" pageNumber="527" unit="m" value="315.0" valueMax="430.0" valueMin="200.0">
<elevation id="36A8706F8E77FFF1D9750A93D89DB801" box="[306,426,1514,1538]" metricMagnitude="2" metricUnit="m" metricValue="3.15" metricValueMax="4.3" metricValueMin="2.0" pageId="46" pageNumber="527" unit="m" value="315.0" valueMax="430.0" valueMin="200.0">200-430 m</elevation>
