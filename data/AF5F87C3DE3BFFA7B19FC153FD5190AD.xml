<taxonomicName id="E0F64D56DE3BFFA7B19FC2F6FDCF9798" authority="Bacescu, 1980: 371 - 374" authorityName="Bacescu" authorityPageNumber="371 - 374" authorityYear="1980" box="[151,623,642,665]" class="Malacostraca" family="Kalliapseudidae" genus="Kalliapseudes" kingdom="Animalia" order="Tanaidacea" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="borcei">
