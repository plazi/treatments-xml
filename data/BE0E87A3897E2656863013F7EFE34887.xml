<taxonomicName id="F1A74D36897E2656868C1246EE104D0C" authority="P. WESSEL et C. O. WEBER" authorityName="P. WESSEL et C. O. WEBER" box="[896,1303,850,871]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="298" phylum="Tracheophyta" rank="species" species="rhenana">
