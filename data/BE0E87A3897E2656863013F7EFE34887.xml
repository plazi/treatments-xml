<taxonomicName id="F1A74D36897E2656868C12BFEF7F4DD5" authority="SAPORTA" authorityName="SAPORTA" box="[896,1144,939,960]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="298" phylum="Tracheophyta" rank="species" species="rostaniana">
