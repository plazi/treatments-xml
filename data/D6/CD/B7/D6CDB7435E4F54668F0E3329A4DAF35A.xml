<subSubSection pageId="14" pageNumber="33" type="reference_group">
<paragraph pageId="14" pageNumber="33">
<taxonomicName class="Lecanoromycetes" family="Curculionidae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xyleborus interjectus" order="Coleoptera" pageId="14" pageNumber="33" phylum="Ascomycota" rank="species" species="interjectus">Xyleborus interjectus</taxonomicName>
