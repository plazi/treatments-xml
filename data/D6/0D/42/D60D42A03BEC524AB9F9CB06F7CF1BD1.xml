<subSubSection pageId="0" pageNumber="139" type="remarks">
<paragraph pageId="0" pageNumber="139">Remarks.</paragraph>
<paragraph pageId="0" pageNumber="139">
<taxonomicName authorityName="Q. Na &amp; Y. P. Ge" authorityYear="2021" class="Agaricomycetes" family="Marasmiaceae" genus="Atheniella" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Atheniella flavida" order="Agaricales" pageId="0" pageNumber="139" phylum="Basidiomycota" rank="species" species="flavida">
<emphasis italics="true" pageId="0" pageNumber="139">Atheniella flavida</emphasis>
