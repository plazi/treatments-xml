<number box="[151,202,1803,1827]" pageId="12" pageNumber="163" value="1.48">1.48</number>
, II 
<number box="[240,292,1803,1827]" pageId="12" pageNumber="163" value="1.02">1.02</number>
, III 
<number box="[339,391,1803,1827]" pageId="12" pageNumber="163" value="1.04">1.04</number>
, IV 
<number box="[439,491,1803,1827]" pageId="12" pageNumber="163" value="1.29">1.29</number>
; patella + tibia: I 
<number box="[690,742,1803,1827]" pageId="12" pageNumber="163" value="2.02">2.02</number>
, II 
<number box="[780,832,1803,1827]" pageId="12" pageNumber="163" value="1.2">1.20</number>
, III 
<number box="[879,930,1803,1827]" pageId="12" pageNumber="163" value="1.28">1.28</number>
, IV 
<number box="[979,1031,1802,1827]" pageId="12" pageNumber="163" value="1.57">1.57</number>
; metatarsus + tarsus: I 
<number box="[1291,1343,1803,1827]" pageId="12" pageNumber="163" value="1.4">1.40</number>
, II 
<number box="[1380,1432,1803,1827]" pageId="12" pageNumber="163" value="1.02">1.02</number>
, III 
<number box="[194,245,1838,1863]" pageId="12" pageNumber="163" value="1.15">1.15</number>
, IV 
<number box="[309,361,1838,1863]" pageId="12" pageNumber="163" value="1.46">1.46</number>
. Spination on ventral tibia I: 
<number box="[736,751,1839,1863]" pageId="12" pageNumber="163" value="2.0">2</number>
- 
<number box="[760,775,1839,1863]" pageId="12" pageNumber="163" value="2.0">2</number>
- 
<number box="[784,799,1839,1863]" pageId="12" pageNumber="163" value="2.0">2</number>
