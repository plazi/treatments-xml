</taxonomicName>
<taxonomicName authorityName="Marshall." box="[503,747,1402,1444]" class="Insecta" family="Curculionidae" genus="Aphanocorynes" kingdom="Animalia" order="Coleoptera" pageId="66" pageNumber="139" phylum="Arthropoda" rank="species" species="savaiiensis">
