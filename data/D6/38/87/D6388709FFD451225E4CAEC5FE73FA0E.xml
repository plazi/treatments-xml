from 
<taxonomicName box="[1302,1574,1338,1380]" class="Cycadopsida" family="Cycadaceae" genus="Cycas" kingdom="Plantae" order="Cycadales" pageId="36" pageNumber="109" phylum="Tracheophyta" rank="species" species="circinalis">
<emphasis box="[1302,1574,1338,1380]" italics="true" pageId="36" pageNumber="109">Cycas circinalis</emphasis>
