<taxonomicName class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis brevitarsis" order="Hymenoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="brevitarsis">
