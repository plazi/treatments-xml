<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:35417" class="Insecta" family="Formicidae" genus="Ponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Ponera coarctata (Latreille)" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="coarctata">coarctata</taxonomicName>
