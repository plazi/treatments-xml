<taxonomicName baseAuthorityName="Verrill" baseAuthorityYear="1880" box="[1135,1360,420,446]" class="Bivalvia" family="Lyonsiellidae" genus="Policordia" kingdom="Animalia" pageId="85" pageNumber="86" phylum="Mollusca" rank="species" species="gemma">
