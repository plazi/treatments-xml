<paragraph blockId="10.[144,933,293,317]" box="[144,933,293,317]" pageId="10" pageNumber="1470">
<materialsCitation box="[144,928,293,317]" country="Thailand" location="The" pageId="10" pageNumber="1470" specimenCount="1">
