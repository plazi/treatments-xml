<subSubSection lastPageId="2" lastPageNumber="58" pageId="1" pageNumber="57" type="materials_examined">
<paragraph blockId="1.[151,1436,1966,2031]" lastBlockId="2.[151,1437,152,371]" lastPageId="2" lastPageNumber="58" pageId="1" pageNumber="57">
<emphasis bold="true" box="[151,335,1966,1992]" pageId="1" pageNumber="57">
