<taxonomicName id="4BD808B0404AFFAFFEB2AF6DFC5B9213" authority="Ueckermann, Moraes &amp; Oliveira" authorityName="Ueckermann, Moraes &amp; Oliveira" box="[264,930,283,309]" class="Arachnida" family="Phytoseiidae" genus="Euseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="neomagucii" status="n. sp.">
