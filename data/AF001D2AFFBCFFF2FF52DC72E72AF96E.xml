. 
<bibRefCitation id="4338D1CDFF82FFF2FE82DB9DE3DCFE75" author="Peart" box="[327,474,432,458]" pageId="32" pageNumber="53" refString="Peart, R. A. (2004) A revision of the Cymadusa filosa complex (Crustacea: Amphipoda: Corophioidea: Ampithoidae). Journal of Natural History, 38, 301 - 336." type="journal article" year="2004">
Peart (
<number id="9161BE1BFF82FFF2FE51DB9DE3D4FE75" box="[404,466,432,458]" pageId="32" pageNumber="33" value="2004.0">2004</number>
)
</bibRefCitation>
redescribed 
<taxonomicName id="E0A9D7BFFF82FFF2FDB4DB9DE13BFE76" box="[625,829,432,457]" class="Malacostraca" family="Ampithoidae" genus="Cymadusa" kingdom="Animalia" order="Amphipoda" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="setosa">
