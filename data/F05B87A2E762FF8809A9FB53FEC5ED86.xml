<taxonomicName id="BFF24D37E762FF8809A9FB53F855EE59" authority="Zucchi, 1988" authorityName="Zucchi" authorityYear="1988" box="[151,592,1258,1285]" class="Insecta" family="Trichogrammatidae" genus="Trichogramma" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="species" species="jalmirezi">
