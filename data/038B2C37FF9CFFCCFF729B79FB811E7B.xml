<taxonomicName id="4C22E6A2FF9DFFCCFE549DCCFDEB1FBC" box="[433,582,224,248]" class="Insecta" family="Chironomidae" genus="Chironomus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="salinarius">
