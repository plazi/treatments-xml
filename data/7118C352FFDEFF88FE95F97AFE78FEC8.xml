<taxonomicName id="3EB109C7FFDFFF88FE86FE97FF2FFEC8" authority="Denis &amp; Schiffermuller, 1775" authorityName="Denis &amp; Schiffermuller" authorityYear="1775" class="Insecta" family="Papilionidae" genus="Papilio" kingdom="Animalia" order="Lepidoptera" pageId="45" pageNumber="328" phylum="Arthropoda" rank="species" species="medusa">
