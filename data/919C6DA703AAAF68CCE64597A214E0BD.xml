<taxonomicName id="55517824CC61CD2B332921888E5DE071" LSID-HNS="urn:lsid:biosci.ohio-state.edu:180257" authority="Mayr" authorityName="Mayr" class="Hexapoda" family="Formicidae" genus="Bothroponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Bothroponera soror Emery" order="Hymenoptera" pageNumber="188" phylum="Arthropoda" rank="species" species="soror">B. soror Em.</taxonomicName>
