</taxonomicName>
and 
<taxonomicName id="4C22AFCEFFCFB91F3C5D1A052D78BC22" authority="Audenaerde, 1991" authorityName="Audenaerde" authorityYear="1991" box="[856,1027,866,889]" class="Actinopterygii" family="Claroteidae" genus="Anaspidoglanis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Siluriformes" pageId="13" pageNumber="178" phylum="Chordata" rank="species" species="macrostoma">
