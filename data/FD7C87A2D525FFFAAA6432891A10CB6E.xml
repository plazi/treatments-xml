<subSubSection id="3DCF653FD526FFFAAA5434E91D35CA96" box="[312,1059,724,750]" pageId="14" pageNumber="15" type="etymology">
<paragraph id="756A36B4D526FFFAAA5434E91D35CA96" blockId="14.[264,1326,284,790]" box="[312,1059,724,750]" pageId="14" pageNumber="15">
<emphasis id="47A1EAA6D526FFFAAA5434E918F4CA96" bold="true" box="[312,482,724,750]" pageId="14" pageNumber="15">Etymology.—</emphasis>
