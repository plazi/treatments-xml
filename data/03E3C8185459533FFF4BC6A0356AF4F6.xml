<taxonomicName id="4C4A028D54595338FE2DC65C32B2F950" authorityName="Osborn" authorityYear="1906" box="[402,926,1656,1698]" class="Reptilia" family="Tyrannosauridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="11" pageNumber="2" phylum="Chordata" rank="family">TYRANNOSAURIDAE</taxonomicName>
