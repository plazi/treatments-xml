<taxonomicName id="77940FECFFD5745DFE55F8B3FD184CAA" box="[485,685,1915,1940]" class="Insecta" family="Termitidae" genus="Termes" kingdom="Animalia" order="Isoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="orthognathus">
