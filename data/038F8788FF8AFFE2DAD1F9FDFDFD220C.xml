<taxonomicName id="4C264D1DFF8AFFE2D894F8DBFE4724AF" authorityName="Anderson" authorityYear="1879" class="Reptilia" family="Emydidae" genus="Emys" kingdom="Animalia" order="Testudines" pageId="28" pageNumber="29" phylum="Chordata" rank="variety" species="trijuga" variety="burmana">
