<taxonomicName id="4C1D4D6FC2415268FE88814DCA30FAC7" authorityName="Enderlein" authorityYear="1935" box="[347,597,1296,1320]" class="Insecta" family="Simuliidae" genus="Psilocnetha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="206" phylum="Arthropoda" rank="species" species="scapulata">
<emphasis id="B969EAFEC2415268FE88814DCA30FAC7" box="[347,597,1296,1320]" italics="true" pageId="5" pageNumber="206">Psilocnetha scapulata</emphasis>
</taxonomicName>
