<paragraph id="8BDDD4593B4BBE23FEB3FB593E5EF8B7" blockId="2.[264,1323,1159,1825]" pageId="2" pageNumber="23">
<taxonomicName id="4C62AFDA3B4BBE23FEB3FB593F6AFB67" authority="Hypostomal" authorityName="Hypostomal" box="[312,625,1239,1265]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="2" pageNumber="23" phylum="Nematoda" rank="genus">
