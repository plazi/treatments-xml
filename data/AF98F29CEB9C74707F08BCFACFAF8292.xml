Current name: 
<taxonomicName id="6833B687B34483BB79DA7353EAF4F239" authority="(L.) G. J. Lewis" authorityName="G. J. Lewis" authorityYear="1941" baseAuthorityName="Linnaeus" baseAuthorityYear="1753" class="Liliopsida" family="Iridaceae" genus="Homeria" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" phylum="Tracheophyta" rank="species" species="breyniana">
