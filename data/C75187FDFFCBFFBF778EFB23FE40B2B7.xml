<subSubSection id="07E26560FFCBFFB9778EFA9CFAE3B7F5" pageId="8" pageNumber="61" type="materials_examined">
<paragraph id="4F4736EBFFCBFFB9778EFA9CFAE3B7F5" blockId="8.[151,1437,1365,2041]" pageId="8" pageNumber="61">
