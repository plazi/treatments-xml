<taxonomicName id="88F84D68FFCBFFB975DCFAB2FC92B7F4" baseAuthorityName="Brulle" baseAuthorityYear="1846" box="[709,822,1402,1429]" class="Insecta" family="Ichneumonidae" genus="Grotea" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="61" phylum="Arthropoda" rank="species" species="lineata">
