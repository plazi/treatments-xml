<taxonomicName id="12ED4D37FF80FFB64D1E3358FB9CA1E5" authorityName="C.Linnaeus" authorityYear="1753" box="[879,1080,367,393]" class="Gymnolaemata" family="Celleporidae" genus="Galeopsis" kingdom="Animalia" order="Cheilostomatida" pageId="14" pageNumber="315" phylum="Bryozoa" rank="species" species="tetrahit">
