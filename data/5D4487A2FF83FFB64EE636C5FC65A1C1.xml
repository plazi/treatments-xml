<taxonomicName id="12ED4D37FF80FFB64B5A3358FECEA1C1" authority="Burm." authorityName="Burm." class="Magnoliopsida" family="Geraniaceae" genus="Geranium" kingdom="Plantae" order="Geraniales" pageId="14" pageNumber="315" phylum="Tracheophyta" rank="species" species="pyrenaicum">
