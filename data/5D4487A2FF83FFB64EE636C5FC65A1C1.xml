<taxonomicName id="12ED4D37FF80FFB64EE6337CFE21A109" box="[151,389,331,357]" class="Magnoliopsida" family="Asteraceae" genus="Calendula" kingdom="Plantae" order="Asterales" pageId="14" pageNumber="315" phylum="Tracheophyta" rank="species" species="officinalis">
