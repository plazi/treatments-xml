<taxonomicName id="12ED4D37FF80FFB64A8A337CFAD7A109" authority="Linnaeus, 1753" authorityName="Linnaeus" authorityYear="1753" box="[1275,1395,331,357]" class="Magnoliopsida" family="Caryophyllaceae" genus="Cucubalus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="14" pageNumber="315" phylum="Tracheophyta" rank="genus">
