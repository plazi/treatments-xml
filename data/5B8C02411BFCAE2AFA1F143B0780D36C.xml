<taxonomicName id="249D764BBEB62852791D98285162FE2E" LSID="http://zoobank.org/34A1D33D-49A6-4914-8FBC-619D6C47EC84" authority="Kula" authorityName="Kula" authorityYear="2013" class="Hexapoda" family="Braconidae" genus="Heterospilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heterospilus michaeli" order="Hymenoptera" pageId="2" pageNumber="85" phylum="Arthropoda" rank="species" species="michaeli" status="sp. n.">Heterospilus michaeli Kula</taxonomicName>
