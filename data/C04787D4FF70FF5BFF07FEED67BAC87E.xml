<taxonomicName id="8FEE4D41FF70FF5BFAF4FD5C6419C87F" authorityName="Nuttall and Warburton" authorityYear="1915" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="houyi">
