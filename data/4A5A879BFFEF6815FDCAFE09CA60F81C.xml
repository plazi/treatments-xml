<taxonomicName id="05F34D0EFFEC6815FCF9FB1FCCA4FA98" authority="Verheye, 2017" authorityName="d’Acoz &amp; Verheye" authorityYear="2017" box="[782,947,1271,1297]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="106" pageNumber="183" phylum="Arthropoda" rank="subGenus" status="subgen. nov." subGenus="Laevepimeria">
