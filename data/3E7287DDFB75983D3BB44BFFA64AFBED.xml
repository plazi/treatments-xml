<geoCoordinate id="D3EF500CFB75983D38EE4BD7A316FE4C" box="[999,1080,429,453]" degrees="21. 35" direction="south" orientation="latitude" pageId="6" pageNumber="369" precision="555" value="-21.35">21.35 S</geoCoordinate>
