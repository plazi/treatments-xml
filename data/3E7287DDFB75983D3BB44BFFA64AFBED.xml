<geoCoordinate id="D3EF500CFB75983D3F4A4BD7A3B9FE4C" box="[1091,1175,429,453]" degrees="18. 10" direction="east" orientation="longitude" pageId="6" pageNumber="369" precision="555" value="18.1">18.10 E</geoCoordinate>
