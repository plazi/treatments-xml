<taxonomicName id="4C6EB2F94E26FFFE6207FD9E0FDF9703" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[1375,1436,584,607]" class="Mammalia" family="Atelidae" genus="Simia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="1" pageNumber="399" phylum="Chordata" rank="genus">
<emphasis id="B91A15684E26FFFE6207FD9E0FDF9703" box="[1375,1436,584,607]" italics="true" pageId="1" pageNumber="399">Simia</emphasis>
</taxonomicName>
