<paragraph id="ACCC6E41FE944746B722BE407225F9B0" pageId="1" pageNumber="40">
<taxonomicName id="2F73BC4603E2BC161F4CD02B4F8FF118" LSID="http://zoobank.org/9480CC00-367F-4F91-B88D-CE49D7F1FD4F" class="Insecta" family="Staphylinidae" genus="Tetrasticta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetrasticta gnatha" order="Coleoptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="gnatha">Tetrasticta gnatha</taxonomicName>
