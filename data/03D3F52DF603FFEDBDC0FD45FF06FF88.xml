<taxonomicName id="4C7A3FB8F600FFE9BE10FC59FBD6FA17" authority="(Yan and Chen, 2014)" baseAuthorityName="Yan and Chen" baseAuthorityYear="2014" box="[572,1041,1012,1038]" class="Insecta" family="Braconidae" genus="Wroughtonia" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="brevicarinata">
