<taxonomicName id="4C714D6AFFD3BF5708EDFC6E9452FC6D" box="[457,611,999,1021]" class="Insecta" family="Ichneumonidae" genus="Xanthopimpla" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="62" phylum="Arthropoda" rank="species" species="pucallpensis">
