<taxonomicName id="D8E64D609565FF869223ECB718D6FF64" box="[848,918,189,212]" class="Liliopsida" family="Arecaceae" genus="Cocos" kingdom="Plantae" order="Arecales" pageId="5" pageNumber="56" phylum="Tracheophyta" rank="genus">
