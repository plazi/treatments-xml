<taxonomicName id="D8E64D609564FF879028EB6B1945F8C8" box="[347,517,1888,1912]" class="Liliopsida" family="Arecaceae" genus="Cocos" kingdom="Plantae" order="Arecales" pageId="4" pageNumber="55" phylum="Tracheophyta" rank="species" species="nucifera">
<emphasis id="2D92EAF19564FF879028EB6B1945F8C8" box="[347,517,1888,1912]" italics="true" pageId="4" pageNumber="55">Cocos nucifera</emphasis>
</taxonomicName>
