<taxonomicName id="D8E64D609564FF879367EB6A19D0F8C8" box="[532,656,1888,1912]" class="Liliopsida" family="Arecaceae" kingdom="Plantae" order="Arecales" pageId="4" pageNumber="55" phylum="Tracheophyta" rank="family">Arecaceae</taxonomicName>
