<number id="A92E24C49564FF879188E9861A49FA14" box="[251,265,1420,1444]" pageId="4" pageNumber="55" value="8.0">8</number>
(A 
<number id="A92E24C49564FF87905CE9861A7CFA14" box="[303,316,1420,1444]" pageId="4" pageNumber="55" value="8.0">8</number>
) similar to A 
<number id="A92E24C49564FF8790A0E9871AA1FA15" box="[467,481,1421,1445]" pageId="4" pageNumber="55" value="7.0">7</number>
