<number id="A92E24C49564FF879188E9DE1A57FA5C" box="[251,279,1492,1516]" pageId="4" pageNumber="55" value="10.0">10</number>
(A 
<number id="A92E24C49564FF87904EE9DE1A1AFA5C" box="[317,346,1492,1516]" pageId="4" pageNumber="55" value="10.0">10</number>
