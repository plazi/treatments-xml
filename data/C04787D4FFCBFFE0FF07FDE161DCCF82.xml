<taxonomicName id="8FEE4D41FFCBFFE0FC9AFB206696CFE6" box="[778,981,1159,1185]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="holocyclus">
