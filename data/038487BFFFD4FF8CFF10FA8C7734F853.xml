<quantity id="4CD59B4CFFD6FF8CFF64F98B7330F996" box="[252,339,1632,1659]" metricMagnitude="-7" metricUnit="kg" metricValue="9.0" pageId="3" pageNumber="66" unit="mg" value="0.9">0.9 mg</quantity>
