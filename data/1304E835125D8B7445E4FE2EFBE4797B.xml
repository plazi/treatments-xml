<subSubSection id="D3B70AA8125D8B7445E4FDD7FDB97C37" box="[151,631,546,573]" pageId="1" pageNumber="46" type="type_taxon">
<paragraph id="9B125923125D8B7445E4FDD7FDB97C37" blockId="1.[151,1437,546,1905]" box="[151,631,546,573]" pageId="1" pageNumber="46">
<emphasis id="A9D98531125D8B7445E4FDD7FEE57C36" bold="true" box="[151,299,546,573]" pageId="1" pageNumber="46">
<typeStatus id="4416E781125D8B7445E4FDD7FF1C7C37" box="[151,210,547,573]" pageId="1" pageNumber="46">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="5CAD22A0125D8B744449FDD7FDFE7C36" authority="Eskov &amp; Marusik, 2021" authorityName="Eskov &amp; Marusik" authorityYear="2021" box="[314,560,547,573]" class="Arachnida" family="Theridiidae" genus="Glebych" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="46" phylum="Arthropoda" rank="species" species="minutissimus" status="sp. n.">
