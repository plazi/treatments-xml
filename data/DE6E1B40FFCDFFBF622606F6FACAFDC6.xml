<quantity id="913F07B3FFCCFFBD641005BCFA6BF971" box="[1354,1418,1714,1735]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" pageId="5" pageNumber="102" unit="mm" value="50.0">50 µm</quantity>
