<emphasis id="B9482E1DFFC14E0A72B2FBD98713CC13" bold="true" box="[168,330,1044,1068]" pageId="32" pageNumber="963">Identification.</emphasis>
<taxonomicName id="4C3C898CFFC14E0A7342FBD984E8CC13" box="[344,689,1044,1068]" class="Liliopsida" family="Araceae" genus="Schismatoglottis" kingdom="Plantae" order="Alismatales" pageId="32" pageNumber="963" phylum="Tracheophyta" rank="species" species="clemensiorum">
