</taxonomicName>
resembles
</emphasis>
<taxonomicName id="4C3C898CFFC14E0A72C5FBFB8426CC71" authority="Schott (Sumatera)" box="[223,639,1078,1102]" class="Liliopsida" family="Araceae" genus="Schismatoglottis" kingdom="Plantae" order="Alismatales" pageId="32" pageNumber="963" phylum="Tracheophyta" rank="species" species="acuminatissima">
