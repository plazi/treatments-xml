One 
<specimenCount id="9D69FD7D6F12D374FE58FD82D6C5FDD7" box="[384,404,519,543]" pageId="7" pageNumber="1929" type="male">♂</specimenCount>
, 
<specimenCount id="9D69FD7D6F12D374FE7CFD82D694FDD7" box="[420,453,519,543]" pageId="7" pageNumber="1929" type="female">2♀</specimenCount>
