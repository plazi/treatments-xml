<taxonomicName id="4C09D9AE2C67DA11FE04818028EFFE10" authority="Walckenaer, 1805" authorityName="Walckenaer" authorityYear="1805" box="[418,478,394,418]" class="Arachnida" family="Salticidae" genus="Attus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="88" phylum="Arthropoda" rank="genus">Attus</taxonomicName>
