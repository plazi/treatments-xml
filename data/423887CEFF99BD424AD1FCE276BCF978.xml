<treatmentCitation id="4B3010C9FF99BD424AD1FCE27777FC9B" author="Hoffmannsegg, J. C. von" box="[136,801,806,833]" page="102" pageId="6" pageNumber="235" year="1824">
<taxonomicName id="0D914D5BFF99BD424AD1FCE27777FC9B" authority="(Hoffmannsegg 1824: 102) G. Don" authorityName="G. Don" baseAuthorityName="Hoffmannsegg" baseAuthorityPageNumber="102" baseAuthorityYear="1824" box="[136,801,806,833]" class="Liliopsida" family="Araceae" genus="Anthurium" kingdom="Plantae" order="Alismatales" pageId="6" pageNumber="235" phylum="Tracheophyta" rank="species" species="longifolium">
<emphasis id="F8E5EACAFF99BD424AD1FCE275C1FC9A" bold="true" box="[136,407,806,833]" italics="true" pageId="6" pageNumber="235">Anthurium longifolium</emphasis>
(
<bibRefCitation id="AE004B29FF99BD424BFFFCE27691FC9B" author="Hoffmannsegg, J. C. von" box="[422,711,806,832]" pageId="6" pageNumber="235" refId="ref11201" refString="Hoffmannsegg, J. C. von (1824) Verzeichniss der Pflanzenkulturen. Commission der Arnoldischen Buchhandlung, Dresden." type="book" year="1824">Hoffmannsegg 1824: 102</bibRefCitation>
