<paragraph id="8BAC5B72B524406170F7F54EFA52D5D9" blockId="20.[713,1430,2756,2800]" box="[713,1430,2756,2800]" pageId="20" pageNumber="45">
<taxonomicName id="4C1320F1B524406170F7F54EFACED5C3" authority="Cope 1876" authorityName="Cope" authorityYear="1876" box="[713,1290,2756,2794]" class="Sauropsida" family="Champsosauridae" genus="Champsosaurus" kingdom="Animalia" order="Choristodera" pageId="20" pageNumber="45" phylum="Chordata" rank="species" species="annectens">Champsosaurus annectens</taxonomicName>
