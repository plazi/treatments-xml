<paragraph id="775D5542FFC2FFFE9AD8B5B8FD79E223" blockId="6.[168,775,1208,1635]" pageId="6" pageNumber="957">
<collectingCountry id="0FF515D2FFC2FFFE9AD8B5B8FEB8E3C2" box="[206,275,1242,1266]" name="Brazil" pageId="6" pageNumber="957">Brazil</collectingCountry>
, estuary of Maraú River: River Zone Station 1, 
<geoCoordinate id="12D63385FFC2FFFE9ABEB598FE8BE223" box="[168,288,1274,1299]" degrees="14" direction="south" minutes="06" orientation="latitude" pageId="6" pageNumber="957" precision="15" seconds="33" value="-14.109167">14°06′33″S</geoCoordinate>
, 
<geoCoordinate id="12D63385FFC2FFFE9B3DB598FE05E223" box="[299,430,1274,1299]" degrees="39" direction="west" minutes="02" orientation="longitude" pageId="6" pageNumber="957" precision="15" seconds="48" value="-39.046665">39°02′48″W</geoCoordinate>
