</taxonomicName>
(
<taxonomicName id="4C0E4D69FFA4FF8FE3F8F9E2FEAE257F" authorityName="Dumortier" authorityYear="1829" box="[206,316,1593,1619]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="77" pageNumber="78" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
), 
<taxonomicName id="4C0E4D69FFA4FF8FE279F9E2FD4E2578" authority="(Guttiferae)" baseAuthorityName="Guttiferae" box="[335,732,1593,1620]" class="Magnoliopsida" family="Clusiaceae" genus="Garcinia" kingdom="Plantae" order="Malpighiales" pageId="77" pageNumber="78" phylum="Tracheophyta" rank="species" species="portoricensis">
<emphasis id="B97AEAF8FFA4FF8FE279F9E2FDDC257F" box="[335,590,1593,1619]" italics="true" pageId="77" pageNumber="78">Garcinia portoricensis</emphasis>
(Guttiferae)
</taxonomicName>
, 
<taxonomicName id="4C0E4D69FFA4FF8FE1D1F9E2FC12257F" box="[743,896,1593,1619]" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" kingdom="Plantae" order="Fagales" pageId="77" pageNumber="78" phylum="Tracheophyta" rank="species" species="regia">
<emphasis id="B97AEAF8FFA4FF8FE1D1F9E2FC12257F" box="[743,896,1593,1619]" italics="true" pageId="77" pageNumber="78">Juglans regia</emphasis>
</taxonomicName>
(
<taxonomicName id="4C0E4D69FFA4FF8FE0B9F9E2FBB82578" box="[911,1066,1593,1620]" class="Magnoliopsida" family="Juglandaceae" kingdom="Plantae" order="Fagales" pageId="77" pageNumber="78" phylum="Tracheophyta" rank="family">Juglandaceae</taxonomicName>
