<paragraph id="8BCD94236F28FFC50BFCF8CDFECDF8DB" blockId="86.[189,381,1804,1831]" box="[189,381,1804,1831]" pageId="86" pageNumber="87">
<emphasis id="B90648316F28FFC50BFCF8CDFEABF8DB" allCaps="true" box="[189,283,1805,1831]" inLineHeading="true" pageId="86" pageNumber="87">LENGTH.</emphasis>
<quantity id="4C8A39C66F28FFC50A62F8CCFEC9F8DB" box="[291,377,1804,1831]" metricMagnitude="-2" metricUnit="m" metricValue="1.4" pageId="86" pageNumber="87" unit="mm" value="14.0">14 mm</quantity>
