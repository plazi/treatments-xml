described 
<taxonomicName id="4C4686DB6B68FFD3FDB617C3FCA6FE50" authorityName="Voss, R. Borovec &amp; J. Skuhrovec" authorityYear="2017" box="[583,864,476,501]" class="Insecta" family="Curculionidae" genus="Trachyphloeus" kingdom="Animalia" order="Coleoptera" pageId="11" phylum="Arthropoda" rank="species" species="pustulifer">
