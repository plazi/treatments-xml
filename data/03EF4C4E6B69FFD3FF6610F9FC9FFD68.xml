<taxonomicName id="4C4686DB6B68FFD3FB75141EFF09FD98" authority="Chevrolat, 1860" authorityName="Chevrolat" authorityYear="1860" class="Insecta" family="Curculionidae" genus="Trachyphloeus" kingdom="Animalia" order="Coleoptera" pageId="11" phylum="Arthropoda" rank="species" species="nodipennis">
