<quantity id="E553D4903163FFCAFAAC50DEFA45FE2D" box="[1383,1470,413,437]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="8" pageNumber="251" unit="m" value="1000.0">1000 m</quantity>
