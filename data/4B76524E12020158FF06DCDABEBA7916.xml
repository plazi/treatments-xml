<paragraph id="C360E3581202015BFF06DCDABD3F7F0E" blockId="5.[151,609,1791,1817]" box="[151,609,1791,1817]" pageId="5" pageNumber="6">
<heading id="982854341202015BFF06DCDABD3F7F0E" bold="true" box="[151,609,1791,1817]" fontSize="11" level="1" pageId="5" pageNumber="6" reason="1">
<emphasis id="F1AB3F4A1202015BFF06DCDABD3F7F0E" bold="true" box="[151,609,1791,1817]" pageId="5" pageNumber="6">
<taxonomicName id="04DF98DB1202015BFF06DCDABEEF7F0E" box="[151,433,1791,1817]" class="Insecta" family="Lycidae" genus="Sulabanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="mamasensis">
<emphasis id="F1AB3F4A1202015BFF06DCDABEEF7F0E" bold="true" box="[151,433,1791,1817]" italics="true" pageId="5" pageNumber="6">Sulabanus mamasensis</emphasis>
</taxonomicName>
