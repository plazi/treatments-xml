<taxonomicName id="34AF88EFFFC6FFCFBA4D4AB2BF2CA614" authorityName="Lindt &amp; Tasane &amp; Õunap &amp; Viidalepp" authorityYear="2017" box="[808,872,1267,1291]" class="Insecta" family="Geometridae" genus="Paromphacodes" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="402" phylum="Arthropoda" rank="species" species="alpha">
