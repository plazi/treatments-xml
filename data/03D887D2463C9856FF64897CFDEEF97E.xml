<paragraph id="8BCE36C4463C9854FF64895BFE3FF865" blockId="1.[151,542,1866,1926]" box="[151,385,1901,1926]" pageId="1" pageNumber="82">
(
<figureCitation id="134A2A41463C9854FF6D895BFE98F865" box="[158,294,1901,1926]" captionStart="FIGURES 1 – 4" captionStartId="3.[151,264,1365,1388]" captionTargetBox="[151,1422,195,1342]" captionTargetId="figure@3.[151,1436,195,1342]" captionTargetPageId="3" captionText="FIGURES 1 – 4. Centris aureiventris new species. Fig. 1. (holotype female). Head, frontal view (scale bar 1 mm). Fig. 2 (holotype female). Habitus, lateral view (scale bar 5 mm). Fig. 3. (paratype male). Head, frontal view (scale bar 1 mm). Fig. 4 (paratype male). Habitus, lateral view (scale bar 5 mm)." httpUri="https://zenodo.org/record/284323/files/figure.png" pageId="1" pageNumber="82">
Figures 
<number id="3DB924E3463C9854FF088958FE98F865" box="[251,294,1902,1926]" pageId="1" pageNumber="82" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
</figureCitation>
and 
<figureCitation id="134A2A41463C9854FEAF8958FEC6F865" box="[348,376,1902,1926]" captionStart="FIGURE 13" captionStartId="8.[151,250,1975,1997]" captionTargetBox="[189,1391,221,1900]" captionTargetId="figure@8.[173,1417,195,1951]" captionTargetPageId="8" captionText="FIGURE 13. Distribution records for the species of Centris (Xanthemisia) inhabiting Colombia." httpUri="https://zenodo.org/record/284326/files/figure.png" pageId="1" pageNumber="82">
