<taxonomicName id="4C5D89DA6E18C42DFB24FDA5A49FD025" authority="Forster" authorityName="Forster" authorityYear="1758" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="446" phylum="Arthropoda" rank="species" species="miniata">
