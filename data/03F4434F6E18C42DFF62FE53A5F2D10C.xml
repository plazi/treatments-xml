<taxonomicName id="4C5D89DA6E18C42DFEB4FDA4A67CD001" box="[321,527,576,600]" class="Insecta" family="Noctuidae" genus="Noctua" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="446" phylum="Arthropoda" rank="species" species="rubicunda">
