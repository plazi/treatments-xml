</taxonomicName>
ex gr. 
<taxonomicName id="FAD04D50FFD3FC55FB52D7EA5950F95D" baseAuthorityName="Selys-Longchamps" baseAuthorityYear="1836" box="[1239,1383,1755,1779]" class="Mammalia" family="Cricetidae" genus="Microtus" kingdom="Animalia" order="Rodentia" pageId="8" pageNumber="25" phylum="Chordata" rank="species" species="subterraneus">
