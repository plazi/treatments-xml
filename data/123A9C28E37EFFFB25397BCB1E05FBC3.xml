<taxonomicName id="5D9356BDE379FFFB25A47A2B19FCFA8A" authority="Danguy &amp; Choux" authorityName="Danguy &amp; Choux" box="[255,720,1355,1379]" class="Magnoliopsida" family="Sapindaceae" genus="Plagioscyphus" kingdom="Plantae" order="Sapindales" pageId="4" pageNumber="45" phylum="Tracheophyta" rank="species" species="louvelii">
<emphasis id="A8E7F12CE379FFFB25A47A2B1AB9FA8A" box="[255,405,1355,1379]" italics="true" pageId="4" pageNumber="45">Plagioscyphus</emphasis>
aff. 
<emphasis id="A8E7F12CE379FFFB249E7A2B193EFA8A" box="[453,530,1355,1379]" italics="true" pageId="4" pageNumber="45">louvelii</emphasis>
Danguy &amp; Choux
</taxonomicName>
and 
<taxonomicName id="5D9356BDE379FFFB25297A0B19AAFA6A" authority="(Sw.) Mett. var. attenuatum" box="[114,646,1387,1411]" class="Polypodiopsida" family="Blechnaceae" genus="Blechnum" kingdom="Plantae" order="Polypodiales" pageId="4" pageNumber="45" phylum="Tracheophyta" rank="variety" species="attenuatum" variety="attenuatum">
<emphasis id="A8E7F12CE379FFFB25297A0B1A77FA6A" box="[114,347,1387,1411]" italics="true" pageId="4" pageNumber="45">Blechnum attenuatum</emphasis>
(Sw.) Mett. var. 
<emphasis id="A8E7F12CE379FFFB27557A0B19AAFA6A" box="[526,646,1387,1411]" italics="true" pageId="4" pageNumber="45">attenuatum</emphasis>
