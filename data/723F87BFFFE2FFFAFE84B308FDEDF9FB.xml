<quantity id="3D6E9B4CFFE2FFFAFBFAB500FB34FBBF" box="[1134,1250,1098,1122]" metricMagnitude="2" metricUnit="m" metricValue="4.115" metricValueMax="6.13" metricValueMin="2.1" pageId="21" pageNumber="236" unit="m" value="411.5" valueMax="613.0" valueMin="210.0">
<elevation id="71BBD19AFFE2FFFAFBFAB500FB34FBBF" box="[1134,1250,1098,1122]" metricMagnitude="2" metricUnit="m" metricValue="4.115" metricValueMax="6.13" metricValueMin="2.1" pageId="21" pageNumber="236" unit="m" value="411.5" valueMax="613.0" valueMin="210.0">210-613 m</elevation>
