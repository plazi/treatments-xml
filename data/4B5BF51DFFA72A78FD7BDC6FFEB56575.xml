<taxonomicName id="04F23F88FFAA2A78FF32DAC8FEBA64E2" box="[236,302,1181,1202]" class="Liliopsida" family="Orchidaceae" genus="Bulbophyllum" kingdom="Plantae" order="Asparagales" pageId="26" pageNumber="327" phylum="Tracheophyta" rank="species" species="radula">
