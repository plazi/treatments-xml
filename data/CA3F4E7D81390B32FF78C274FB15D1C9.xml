<taxonomicName id="859684E881390B32FEFEC20BFDF3D203" authority="Klunzinger 1870" authorityName="Klunzinger" authorityYear="1870" box="[273,582,1122,1144]" class="Actinopterygii" family="Apogonidae" genus="Pristiapogon" kingdom="Animalia" order="Perciformes" pageId="33" pageNumber="184" phylum="Chordata" rank="genus">
