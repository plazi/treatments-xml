<quantity id="4C8D9B4DE103FFAFFE3EFAC89D5CF67D" box="[470,557,1375,1401]" metricMagnitude="-3" metricUnit="m" metricValue="7.1" pageId="28" pageNumber="29" unit="mm" value="7.1">7.1 mm</quantity>
