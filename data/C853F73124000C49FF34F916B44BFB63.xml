<emphasis id="728E9A3524030C48FB79F9F4B49EF91D" box="[1242,1436,1647,1669]" italics="true" pageId="18" pageNumber="541">Parapseudoconnus</emphasis>
</taxonomicName>
<taxonomicName id="87FA3DA424030C48FF34F915B0D9F93C" authority="Franz" authorityName="Franz" box="[151,475,1678,1700]" pageId="18" pageNumber="541" rank="species" species="fraudulentus" subGenus="Neuraphomimus">
(
<taxonomicName id="87FA3DA424030C48FF3CF915B044F93C" box="[159,326,1678,1700]" genus="Neuraphomimus" pageId="18" pageNumber="541" rank="genus">
