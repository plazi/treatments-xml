<collectionCode id="ED1EF9A7FFCF7E33FEEDFBD3FEA7FBB2" box="[256,305,1044,1068]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="32" pageNumber="725">SMF</collectionCode>
21.333- 
<collectionCode id="ED1EF9A7FFCF7E33FE69FBD3FE21FBB2" box="[388,439,1044,1068]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="32" pageNumber="725">SMF</collectionCode>
