<taxonomicName id="4C4998BBFFCF250F22F9FA25FE05FA50" authority="Simon, 1905" authorityName="Simon" authorityYear="1905" box="[151,442,1447,1472]" class="Arachnida" family="Zodariidae" genus="Hermippus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="115" phylum="Arthropoda" rank="species" species="cruciatus">
<emphasis id="B93D3F2AFFCF250F22F9FA25FE9EFA2F" box="[151,289,1448,1472]" italics="true" pageId="1" pageNumber="115">H. cruciatus</emphasis>
<bibRefCitation id="EFD89EC9FFCF250F2346FA2BFE05FA50" author="Simon" box="[296,442,1447,1472]" pageId="1" pageNumber="115" refString="Simon, E. (1905) Arachnides de Java, recueillis par le Prof. K. Kraepelin en 1904. Mitteilungen aus dem Naturhistorischen Museum in Hamburg, 22, 49 - 73." type="journal article" year="1905">
