<bibRefCitation id="EFD89EC9FFCF250F2112FA24FBB3FA50" author="Jocque" box="[892,1036,1448,1472]" pageId="1" pageNumber="115" refString="Jocque, R. (1989) A new African species and other new records of Hermippus (Araneae, Zodariidae). Biol. Jb. Dodonaea, 56, 105 - 110." type="journal article" year="1989">
Jocqué 
<number id="3D81F11FFFCF250F21BDFA24FBB3FA50" box="[979,1036,1448,1472]" pageId="1" pageNumber="115" value="1989.0">1989</number>
</bibRefCitation>
) and 
<taxonomicName id="4C4998BBFFCF250F2625FA25FADFFA50" authority="Gravely, 1921" authorityName="Gravely" authorityYear="1921" box="[1099,1376,1447,1472]" class="Arachnida" family="Zodariidae" genus="Hermippus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="115" phylum="Arthropoda" rank="species" species="arjuna">
