: 
<collectingCountry id="F37C56AA32395C7CFF1FD036AD55FE81" box="[192,285,258,282]" name="Malawi" pageId="33" pageNumber="32">Malawi</collectingCountry>
, 
<collectingCountry id="F37C56AA32395C7CFEF6D036AD82FE81" box="[297,458,258,282]" name="Mozambique" pageId="33" pageNumber="32">Mozambique</collectingCountry>
, 
<collectingCountry id="F37C56AA32395C7CFE0BD036AE7BFE81" box="[468,563,258,282]" name="Zambia" pageId="33" pageNumber="32">Zambia</collectingCountry>
, 
