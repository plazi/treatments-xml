<paragraph id="8C6773334070FF95FEB2AF1DFCBF928E" blockId="31.[264,1021,364,424]" pageId="31" pageNumber="32">
<taxonomicName id="4BD808B04070FF95FEB2AF1DFC0492A5" authority="Ueckermann &amp; Loots, 1988: 95" authorityName="Ueckermann &amp; Loots" authorityPageNumber="95" authorityYear="1988" box="[264,1021,364,387]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="myrobalanus" subGenus="Amblyseius">
<emphasis id="BEACAF214070FF95FEB2AF1DFE7892A5" box="[264,385,364,387]" italics="true" pageId="31" pageNumber="32">Amblyseius</emphasis>
(
<emphasis id="BEACAF214070FF95FE2BAF1DFDF392A5" box="[401,522,364,387]" italics="true" pageId="31" pageNumber="32">Amblyseius</emphasis>
