<taxonomicName id="4C333D8AFFAEA511DC17F84EF5DE699C" authority="Wang &amp; Soulier-Perkins" authorityName="Wang &amp; Soulier-Perkins" authorityYear="2016" box="[336,945,1965,1992]" class="Insecta" family="Lophopidae" genus="Serida" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="parenthesisflexuosa" status="sp. nov.">
