<subSubSection box="[199,1215,620,645]" pageId="6" pageNumber="235" type="etymology">
<paragraph blockId="6.[151,1436,151,645]" box="[199,1215,620,645]" pageId="6" pageNumber="235">
<emphasis bold="true" box="[199,334,620,645]" pageId="6" pageNumber="235">Etymology.</emphasis>
