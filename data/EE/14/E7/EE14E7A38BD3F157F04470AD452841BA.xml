<taxonomicName family="Agromyzidae" lsidName="" pageId="71" pageNumber="72" rank="family">Agromyzidae</taxonomicName>
, 
<taxonomicName family="Tephritidae" lsidName="" pageId="71" pageNumber="72" rank="family">Tephritidae</taxonomicName>
, 
<taxonomicName family="Ephydridae" lsidName="" pageId="71" pageNumber="72" rank="family">Ephydridae</taxonomicName>
, 
<taxonomicName family="Anthomyiidae" lsidName="" pageId="71" pageNumber="72" rank="family">Anthomyiidae</taxonomicName>
, 
<taxonomicName family="Scathophagidae" lsidName="" pageId="71" pageNumber="72" rank="family">Scathophagidae</taxonomicName>
, and 
<taxonomicName family="Drosophilidae" lsidName="" pageId="71" pageNumber="72" rank="family">Drosophilidae</taxonomicName>
