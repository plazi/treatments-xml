<paragraph blockId="26.[151,526,1511,1538]" box="[151,526,1511,1538]" pageId="26" pageNumber="27">
<heading bold="true" box="[151,526,1511,1538]" fontSize="11" level="1" pageId="26" pageNumber="27" reason="1">
<emphasis bold="true" box="[151,526,1511,1538]" pageId="26" pageNumber="27">
Genus 
<taxonomicName authority="Bush, 1905" authorityName="Bush" authorityYear="1905" box="[238,526,1511,1538]" class="Polychaeta" family="Sabellidae" genus="Parasabella" kingdom="Animalia" order="Sabellida" pageId="26" pageNumber="27" phylum="Annelida" rank="genus">
<emphasis bold="true" box="[238,381,1511,1537]" italics="true" pageId="26" pageNumber="27">Parasabella</emphasis>
<bibRefCitation author="Bush" box="[388,526,1512,1538]" pageId="26" pageNumber="27" refString="Bush, K. J. (1905) Tubicolous annelids of the tribes Sabellides and Serpulides from the Pacific Ocean. Harriman Alaska Expedition, 12, 169 - 346." type="journal article" year="1905">Bush, 1905</bibRefCitation>
</taxonomicName>
</emphasis>
