<emphasis box="[688,700,1441,1464]" italics="true" pageId="3" pageNumber="574">c</emphasis>
<emphasis box="[701,709,1456,1469]" italics="true" pageId="3" pageNumber="574">
<number box="[701,709,1456,1469]" pageId="3" pageNumber="574" value="2.0">2</number>
