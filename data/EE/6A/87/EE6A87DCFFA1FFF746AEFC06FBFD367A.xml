<emphasis box="[1353,1381,1557,1581]" italics="true" pageId="3" pageNumber="574">ad</emphasis>
<emphasis box="[1381,1389,1572,1585]" italics="true" pageId="3" pageNumber="574">
<number box="[1381,1389,1572,1585]" pageId="3" pageNumber="574" value="1.0">1</number>
</emphasis>
