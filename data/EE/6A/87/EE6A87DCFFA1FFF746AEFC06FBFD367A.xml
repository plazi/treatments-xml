<number box="[311,319,1572,1585]" pageId="3" pageNumber="574" value="6.0">6</number>
</emphasis>
, all setae 
<emphasis box="[429,443,1558,1581]" italics="true" pageId="3" pageNumber="574">g</emphasis>
<emphasis box="[443,464,1572,1585]" italics="true" pageId="3" pageNumber="574">
<number box="[443,464,1572,1585]" pageId="3" pageNumber="574" value="7.5" valueMax="9.0" valueMin="6.0">6-9</number>
</emphasis>
longer than setae 
<emphasis box="[665,679,1558,1581]" italics="true" pageId="3" pageNumber="574">g</emphasis>
<emphasis box="[679,699,1572,1585]" italics="true" pageId="3" pageNumber="574">
<number box="[679,699,1572,1585]" pageId="3" pageNumber="574" value="3.0" valueMax="5.0" valueMin="1.0">1-5</number>
</emphasis>
