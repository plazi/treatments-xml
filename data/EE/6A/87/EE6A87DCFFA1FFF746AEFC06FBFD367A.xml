<number box="[1267,1283,1518,1542]" pageId="3" pageNumber="574" value="4.0">4</number>
+ 
<number box="[1301,1317,1518,1542]" pageId="3" pageNumber="574" value="2.0">2</number>
: 
<number box="[1321,1335,1518,1542]" pageId="3" pageNumber="574" value="3.0">3</number>
, setae 
<emphasis box="[1409,1423,1519,1542]" italics="true" pageId="3" pageNumber="574">g</emphasis>
<emphasis italics="true" pageId="3" pageNumber="574">
<number pageId="3" pageNumber="574" value="8.0" valueMax="9.0" valueMin="7.0">7- 9</number>
</emphasis>
longer than 
<emphasis box="[297,311,1558,1581]" italics="true" pageId="3" pageNumber="574">g</emphasis>
