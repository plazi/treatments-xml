<emphasis box="[1083,1091,1378,1391]" italics="true" pageId="3" pageNumber="574">
<number box="[1083,1091,1378,1391]" pageId="3" pageNumber="574" value="1.0">1</number>
</emphasis>
), flagellate. Only setae 
<emphasis box="[1368,1380,1364,1387]" italics="true" pageId="3" pageNumber="574">c</emphasis>
<emphasis box="[1380,1388,1378,1391]" italics="true" pageId="3" pageNumber="574">
<number box="[1380,1388,1378,1391]" pageId="3" pageNumber="574" value="3.0">3</number>
</emphasis>
, 
<emphasis box="[1402,1429,1364,1387]" italics="true" pageId="3" pageNumber="574">cp</emphasis>
, 
<emphasis box="[151,165,1401,1425]" italics="true" pageId="3" pageNumber="574">h</emphasis>
<emphasis box="[165,173,1417,1430]" italics="true" pageId="3" pageNumber="574">
<number box="[165,173,1417,1430]" pageId="3" pageNumber="574" value="3.0">3</number>
</emphasis>
