<number box="[912,920,1417,1430]" pageId="3" pageNumber="574" value="1.0">1</number>
</emphasis>
(
<emphasis box="[939,953,1401,1425]" italics="true" pageId="3" pageNumber="574">h</emphasis>
<emphasis box="[953,961,1417,1430]" italics="true" pageId="3" pageNumber="574">
