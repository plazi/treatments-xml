<subSubSection lastPageId="1" lastPageNumber="61" pageId="0" pageNumber="60" type="description">
<paragraph blockId="0.[151,1437,1573,2012]" pageId="0" pageNumber="60">
<emphasis bold="true" box="[199,338,1816,1840]" pageId="0" pageNumber="60">Description.</emphasis>
Female (
<accessionNumber box="[445,590,1816,1839]" httpUri="http://www.uniprot.org/uniprot/MCN47515" pageId="0" pageNumber="60">
MCN 
<number box="[518,590,1816,1839]" pageId="0" pageNumber="60" value="47515.0">47515</number>
