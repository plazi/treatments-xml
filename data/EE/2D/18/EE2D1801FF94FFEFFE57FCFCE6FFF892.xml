<collectingCountry box="[520,634,1882,1908]" name="Mongolia" pageId="6" pageNumber="116">Mongolia</collectingCountry>
and 
<collectingRegion box="[691,874,1882,1909]" country="China" name="Nei Mongol" pageId="6" pageNumber="116">Inner Mongolia</collectingRegion>
