Gen. 
<taxonomicName authority="Brunner von Wattenwyl, 1891" authorityName="Brunner von Wattenwyl" authorityYear="1891" box="[218,771,655,682]" class="Insecta" family="Phaneropteridae" genus="Parapyrrhicia" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="301" phylum="Arthropoda" rank="genus">
