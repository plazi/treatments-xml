<emphasis box="[358,516,2003,2028]" italics="true" pageId="2" pageNumber="57">planetophora</emphasis>
</taxonomicName>
and 
<taxonomicName box="[573,701,2003,2028]" class="Bacillariophyceae" family="Stephanodiscaceae" genus="Cyclotella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Thalassiosirales" pageId="2" pageNumber="57" phylum="Ochrophyta" rank="species" species="ocellata">
<emphasis box="[573,701,2003,2028]" italics="true" pageId="2" pageNumber="57">C. ocellata</emphasis>
