<emphasis box="[330,442,1700,1717]" italics="true" pageId="4" pageNumber="5">Ectaetia betzi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[450,512,1700,1717]" pageId="4" pageNumber="5" rank="species">sp.nov.</taxonomicNameLabel>
, male holotype. 2. 
<taxonomicName box="[678,887,1700,1717]" class="Insecta" family="Scatopsidae" genus="Psectrosciara" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="gonzalezae" status="sp.nov.">
<emphasis box="[678,887,1700,1717]" italics="true" pageId="4" pageNumber="5">Psectrosciara gonzalezae</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[895,957,1700,1717]" pageId="4" pageNumber="5" rank="species">sp.nov.</taxonomicNameLabel>
, female paratype. 3. 
<taxonomicName authority="(Linnaeus)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[1139,1371,1700,1717]" class="Insecta" family="Scatopsidae" genus="Scatopse" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="notata">
<emphasis box="[1139,1274,1700,1717]" italics="true" pageId="4" pageNumber="5">Scatopse notata</emphasis>
(Linnaeus)
</taxonomicName>
, female. 4. 
<taxonomicName box="[165,326,1725,1742]" class="Insecta" family="Scatopsidae" genus="Aztecatopse" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="huertai" status="sp.nov.">
<emphasis box="[165,326,1725,1742]" italics="true" pageId="4" pageNumber="5">Aztecatopse huertai</emphasis>
</taxonomicName>
