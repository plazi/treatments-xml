<taxonomicName authorityName="Enderlein" authorityYear="1912" box="[410,511,1469,1488]" class="Insecta" family="Scatopsidae" genus="Holoplagia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">Holoplagia</taxonomicName>
[tax])
</paragraph>
