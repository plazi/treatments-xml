<materialsCitation ID-GBIF-Occurrence="1702506714" box="[199,1080,1950,1975]" country="Iceland" location="Concepcion" municipality="Chilean Patagonia" pageId="10" pageNumber="531" specimenCount="1" typeStatus="holotype">
<emphasis bold="true" box="[199,349,1950,1975]" pageId="10" pageNumber="531">
<typeStatus box="[199,258,1950,1975]" pageId="10" pageNumber="531">Type</typeStatus>
locality
</emphasis>
. 
<location LSID="urn:lsid:plazi:treatment:EE67F1203B7EFFC9FF32FF36FAF4F859:631116ED3B7EFFC9FEC9F83FFE28F835" box="[364,498,1950,1975]" country="Iceland" municipality="Chilean Patagonia" name="Concepcion" pageId="10" pageNumber="531">Concepción</location>
channel, Drumond Hay 
<collectingCountry box="[774,842,1950,1975]" name="Iceland" pageId="10" pageNumber="531">Island</collectingCountry>
