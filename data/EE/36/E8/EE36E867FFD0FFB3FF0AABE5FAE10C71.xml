, 
<location LSID="urn:lsid:plazi:treatment:EE36E867FFD0FFB3FF0AABE5FAE10C71:63400FAAFFD0FFBEFC1DAAD3FC180FDF" box="[896,1010,417,441]" municipality="Kyushu Island" name="Uji Island" pageId="85" pageNumber="86">Uji Island</location>
