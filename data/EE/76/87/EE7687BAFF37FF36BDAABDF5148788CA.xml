<taxonomicName baseAuthorityName="Goetghebuer" baseAuthorityYear="1921" box="[884,1023,959,983]" class="Insecta" family="Chironomidae" genus="Pseudosmittia" kingdom="Animalia" order="Diptera" pageId="192" pageNumber="193" phylum="Arthropoda" rank="species" species="forcipata">
