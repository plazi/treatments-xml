: 
<collectingRegion box="[492,670,574,598]" country="Brazil" name="Sao Paulo" pageId="283" pageNumber="284">São Paulo State</collectingRegion>
, 
<collectingMunicipality box="[681,777,574,598]" pageId="283" pageNumber="284">Cubatão</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:EE7687BAFEECFEEDBDAABCE311568CB2:63006077FEECFEEDBE29BF5C17048D6A" box="[788,892,574,598]" country="Brazil" municipality="Cubatao" name="Cambuci" pageId="283" pageNumber="284" stateProvince="Sao Paulo">Cambuci</location>
, chuvosa (rainy season), 
