: 
<materialsCitation collectionCode="MZUSP" collectorName="F. O. Roque" country="Brazil" location="Cambuci" municipality="Cubatao" pageId="283" pageNumber="284" specimenCount="2" stateProvince="Sao Paulo" typeStatus="holotype">
