<heading box="[288,836,1270,1291]" centered="true" fontSize="9" level="2" pageId="1" pageNumber="238" reason="2">
<taxonomicName authority="Romero and Johnson" authorityName="Romero and Johnson" box="[288,706,1270,1291]" class="Insecta" family="Chrysomelidae" genus="Meganeltumius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="238" phylum="Arthropoda" rank="species" species="juani" status="new species">
<emphasis box="[288,491,1270,1291]" italics="true" pageId="1" pageNumber="238">Meganeltumius juani</emphasis>
Romero and Johnson
</taxonomicName>
, 
<emphasis bold="true" box="[717,836,1270,1291]" pageId="1" pageNumber="238">
<taxonomicNameLabel box="[717,836,1270,1291]" pageId="1" pageNumber="238" rank="species">new species</taxonomicNameLabel>
