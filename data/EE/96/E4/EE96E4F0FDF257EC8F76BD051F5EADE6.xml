<geoCoordinate degrees="37" direction="north" minutes="00" orientation="latitude" precision="1" seconds="0.59" value="37.000164">37°00'0.59&quot;N</geoCoordinate>
, 
<geoCoordinate degrees="129" direction="east" minutes="26" orientation="longitude" precision="1" seconds="1.89" value="129.43385">129°26'1.89&quot;E</geoCoordinate>
