<geoCoordinate degrees="38" direction="north" minutes="18" orientation="latitude" precision="1" seconds="14.75" value="38.304096">38°18'14.75&quot;N</geoCoordinate>
, 
<geoCoordinate degrees="128" direction="east" minutes="34" orientation="longitude" precision="1" seconds="1.05" value="128.56696">128°34'1.05&quot;E</geoCoordinate>
