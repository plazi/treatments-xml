<subSubSection box="[199,545,1050,1077]" pageId="41" pageNumber="42" type="distribution">
<paragraph blockId="41.[151,1437,150,1149]" box="[199,545,1050,1077]" pageId="41" pageNumber="42">
<emphasis bold="true" box="[199,353,1050,1076]" pageId="41" pageNumber="42">Distribution.</emphasis>
<collectingCountry box="[359,426,1051,1077]" name="China" pageId="41" pageNumber="42">China</collectingCountry>
(
<collectingRegion box="[441,533,1051,1077]" country="China" name="Yunnan" pageId="41" pageNumber="42">Yunnan</collectingRegion>
).
</paragraph>
