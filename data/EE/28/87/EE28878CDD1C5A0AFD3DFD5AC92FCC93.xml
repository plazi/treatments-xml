(
<emphasis box="[237,374,427,452]" italics="true" pageId="33" pageNumber="34">Lautoconus</emphasis>
) 
<emphasis box="[390,511,427,453]" italics="true" pageId="33" pageNumber="34">eschewegi</emphasis>
<bibRefCitation author="Pereira da Costa F. A." box="[516,774,426,453]" pageId="33" pageNumber="34" refId="ref31319" refString="Pereira da Costa F. A. 1866. Gasteropodes dos depositos Terciarios do Portugal. Commissao Geologica de Portugal, Lisboa." type="book" year="1866">Pereira da Costa 1866</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Psarras &amp; Merle &amp; Koskeridou, 2022" authorityName="Psarras &amp; Merle &amp; Koskeridou" authorityYear="2022" box="[784,1089,427,453]" class="Gastropoda" family="Conidae" genus="Conus" kingdom="Animalia" order="Neogastropoda" pageId="33" pageNumber="34" phylum="Mollusca" rank="species" species="ictini" status="sp. nov." subGenus="Lautoconus">
