<taxonomicName authority="" authorityName="Morch" authorityYear="1852" box="[227,511,497,524]" class="Gastropoda" family="Conidae" genus="Conus" kingdom="Animalia" order="Neogastropoda" pageId="44" pageNumber="45" phylum="Mollusca" rank="subGenus" subGenus="Stephanoconus">
<emphasis box="[227,302,497,523]" italics="true" pageId="44" pageNumber="45">Conus</emphasis>
(
<emphasis box="[321,499,497,523]" italics="true" pageId="44" pageNumber="45">Stephanoconus</emphasis>
)
</taxonomicName>
