<subSubSection pageId="98" pageNumber="99" type="nomenclature">
<paragraph pageId="98" pageNumber="99">
<taxonomicName LSID="http://zoobank.org/2E6C1E2C-2A43-4A62-8C22-A1C4277B2E81" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina sturanyana" order="Stylommatophora" pageId="98" pageNumber="99" phylum="Mollusca" rank="subSpecies" species="sturanyana" subSpecies="gropana">Montenegrina sturanyana gropana</taxonomicName>
