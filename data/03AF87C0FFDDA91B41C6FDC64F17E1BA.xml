<subSubSection id="C31C655DFFDAA91B4198FA124EC9E121" pageId="20" pageNumber="21" type="etymology">
<paragraph id="8BB936D6FFDAA91B4198FA124EC9E121" blockId="20.[151,1437,151,968]" pageId="20" pageNumber="21">
<emphasis id="B972EAC4FFDAA91B4198FA124D38E145" bold="true" box="[201,328,800,824]" pageId="20" pageNumber="21">Etymology</emphasis>
