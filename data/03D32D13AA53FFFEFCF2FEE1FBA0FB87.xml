<quantity id="4C8231E0AA53FFFEFC64FC48FBABFC69" box="[966,1047,957,978]" metricMagnitude="3" metricUnit="m" metricValue="1.371" pageId="6" pageNumber="661" unit="m" value="1371.0">1371 m</quantity>
