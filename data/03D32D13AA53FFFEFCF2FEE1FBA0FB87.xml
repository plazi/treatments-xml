<collectingCountry id="F36DDC95AA53FFFEFC9CFD8BFCD1FD28" box="[830,877,638,659]" name="United States of America" pageId="6" pageNumber="661">USA</collectingCountry>
to 
<collectingCountry id="F36DDC95AA53FFFEFC35FD8BFC49FD28" box="[919,1013,638,659]" name="Colombia" pageId="6" pageNumber="661">Colombia</collectingCountry>
, 
<collectingCountry id="F36DDC95AA53FFFEFBA7FD8BFBD5FD28" box="[1029,1129,638,659]" name="Venezuela" pageId="6" pageNumber="661">Venezuela</collectingCountry>
