and 
<taxonomicName id="E4F54D44FFF76D0C0080FC283B84FC7C" authority="Packer &amp; Dumesh" authorityName="Packer &amp; Dumesh" class="Insecta" family="Colletidae" genus="Geodiscelis" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="288" phylum="Arthropoda" rank="species" species="phisquiri" status="sp. nov.">
