<collectingCountry id="32847640FFE6FF8DFEE1FAC91DD7FA8D" box="[359,465,1369,1394]" name="Bermuda" pageId="85" pageNumber="86">Bermuda</collectingCountry>
, 
<collectingCountry id="32847640FFE6FF8DFE5AFAC91E40FA8D" box="[476,582,1369,1394]" name="Bahamas" pageId="85" pageNumber="86">Bahamas</collectingCountry>
