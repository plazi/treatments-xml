<quantity id="8D6B9B35FFE6FF8DFC97FC5E1F4BFC1A" box="[785,845,974,998]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="85" pageNumber="86" unit="m" value="20.0">20 m</quantity>
