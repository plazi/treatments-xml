<collectionCode id="2C82AE15FFE6FF8DFE8AFC5E1D5BFC19" LSID="urn:lsid:biocol.org:col:34871" box="[268,349,974,998]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="85" pageNumber="86">USNM</collectionCode>
<number id="FC5B24F7FFE6FF8DFEE3FC5E1DCCFC19" box="[357,458,974,998]" pageId="85" pageNumber="86" value="1229121.0">1229121</number>
