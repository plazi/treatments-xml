In 
<taxonomicName id="4C3D4D6FFFCAAA3CFF44F955FE3BF920" authority="Assing, 2002" authorityName="Assing" authorityYear="2002" box="[235,567,1751,1776]" class="Insecta" family="Staphylinidae" genus="Astenus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="474" phylum="Arthropoda" rank="species" species="bicoloratus">
