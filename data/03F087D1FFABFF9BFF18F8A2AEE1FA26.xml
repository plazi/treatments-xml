<taxonomicName id="4C594D44FFA9FF9BFD85FD4EAC82FD32" authorityName="Prain A. Flowering" authorityYear="1898" box="[523,689,662,688]" class="Magnoliopsida" family="Fabaceae" genus="Pueraria" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="bella">
