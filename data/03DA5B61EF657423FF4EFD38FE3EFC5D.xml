<collectingCountry id="F364AAE7EF6B7423FE8AFC9FFE22FC73" box="[332,396,895,919]" name="China" pageId="9" pageNumber="543">China</collectingCountry>
(
<collectingRegion id="49B72495EF6B7423FE5FFC9FFE50FC73" box="[409,510,895,919]" country="China" name="Zhejiang" pageId="9" pageNumber="543">Zhejiang</collectingRegion>
), 
<collectingCountry id="F364AAE7EF6B7423FDD6FC9FFDFEFC72" box="[528,592,895,918]" name="Japan" pageId="9" pageNumber="543">Japan</collectingCountry>
.
</paragraph>
