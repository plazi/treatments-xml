<collectionCode id="35F030CC3323FFE2FE64FA01FE0C0CDE" box="[390,502,1471,1494]" country="Argentina" httpUri="http://grbio.org/cool/pnqa-7dnx" name="Muso Paleontologico Egidio Fergulio" pageId="3" pageNumber="250">MPEF-PV</collectionCode>
