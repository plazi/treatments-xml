<collectionCode id="35F030CC3323FFE2FE78FA63FDF90CFC" box="[410,515,1501,1524]" country="Argentina" httpUri="http://grbio.org/cool/pnqa-7dnx" name="Muso Paleontologico Egidio Fergulio" pageId="3" pageNumber="250">MPEF-PV</collectionCode>
