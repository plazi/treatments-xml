<subSubSection id="C2AA4370E16F6B977E25F52539C4F2AC" pageId="21" pageNumber="22" type="reference_group">
<paragraph id="20524BFC1D2E07506B1E165296EF2451" pageId="21" pageNumber="22">
<taxonomicName id="4BC1BC262E31FC0A2E630D7BCC4824B4" class="Demospongiae" family="Irciniidae" genus="Ircinia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ircinia paucifilamentosa" order="Dictyoceratida" pageId="21" pageNumber="22" phylum="Porifera" rank="species" species="paucifilamentosa">Ircinia paucifilamentosa</taxonomicName>
