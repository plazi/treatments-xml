<taxonomicName id="A3EEF5E9902995FABA5447C8FA0B0EE3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2394" class="Insecta" family="Formicidae" genus="Myrmicocrypta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Myrmicocrypta Smith" order="Hymenoptera" pageNumber="255" phylum="Arthropoda" rank="genus">Myrmicocrypta</taxonomicName>
