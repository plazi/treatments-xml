<normalizedToken id="C7D42C3A849E0724E8C7ECFD8C6054DB" originalValue="renonçant">renoncant</normalizedToken>
<normalizedToken id="138471288199F79ADF3411D6B9539D64" originalValue="à">a</normalizedToken>
