<taxonomicName id="AE80A9EBFFBCA465FD44FADDFBBAFAE0" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[660,1056,1281,1308]" class="Actinopterygii" family="Exocoetidae" genus="Exocoetus" kingdom="Animalia" order="Beloniformes" pageId="38" pageNumber="39" phylum="Chordata" rank="species" species="volitans">
<emphasis id="5BF40E7AFFBCA465FD44FADDFCF3FAE0" box="[660,873,1282,1308]" italics="true" pageId="38" pageNumber="39">Exocoetus volitans</emphasis>
<bibRefCitation id="0D11AF99FFBCA465FCA2FADCFBBAFAE0" author="Linnaeus, C. von" box="[882,1056,1281,1308]" pageId="38" pageNumber="39" refId="ref75066" refString="Linnaeus, C. von (1758) Systeme Naturale. Vol. 1. 10 th Edition. Impensis Direct. Laurentii Salvii, Holmiae, 824 pp." type="book" year="1758">Linnaeus, 1758</bibRefCitation>
</taxonomicName>
(
<taxonomicName id="AE80A9EBFFBCA465FBE1FADCFB25FAE1" authorityName="Risso" authorityYear="1827" box="[1073,1215,1282,1309]" class="Actinopterygii" family="Exocoetidae" kingdom="Animalia" order="Beloniformes" pageId="38" pageNumber="39" phylum="Chordata" rank="family">Exocoetidae</taxonomicName>
) and one specimen from 
<taxonomicName id="AE80A9EBFFBCA465FF04FAF8FDDBFABC" authority="(Day, 1878)" authorityName="Day" authorityYear="1878" baseAuthorityName="Day" baseAuthorityYear="1878" box="[212,577,1318,1344]" class="Actinopterygii" family="Clupeidae" genus="Sardinella" kingdom="Animalia" order="Clupeiformes" pageId="38" pageNumber="39" phylum="Chordata" rank="species" species="sindensis">
<emphasis id="5BF40E7AFFBCA465FF04FAF8FE2FFABC" box="[212,437,1318,1344]" italics="true" pageId="38" pageNumber="39">Sardinella sindensis</emphasis>
(Day, 1878)
</taxonomicName>
(
<taxonomicName id="AE80A9EBFFBCA465FD9FFAF8FD5EFABD" authorityName="Rafinesque" authorityYear="1810" box="[591,708,1318,1345]" class="Actinopterygii" family="Clupeidae" kingdom="Animalia" order="Clupeiformes" pageId="38" pageNumber="39" phylum="Chordata" rank="family">Clupeidae</taxonomicName>
) and he also suggesting that 
<taxonomicName id="AE80A9EBFFBCA465FBD5FAF8FB1DFABC" baseAuthorityName="Day" baseAuthorityYear="1878" box="[1029,1159,1318,1344]" class="Actinopterygii" family="Clupeidae" genus="Sardinella" kingdom="Animalia" order="Clupeiformes" pageId="38" pageNumber="39" phylum="Chordata" rank="species" species="sindensis">
<emphasis id="5BF40E7AFFBCA465FBD5FAF8FB1DFABC" box="[1029,1159,1318,1344]" italics="true" pageId="38" pageNumber="39">S. sindensis</emphasis>
</taxonomicName>
is not the normal host of 
<taxonomicName id="AE80A9EBFFBCA465FF47FA95FED6FA98" baseAuthorityName="Schioedte &amp; Meinert" baseAuthorityYear="1884" box="[151,332,1354,1380]" class="Malacostraca" family="Cymothoidae" genus="Mothocya" kingdom="Animalia" order="Isopoda" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="melanosticta">
<emphasis id="5BF40E7AFFBCA465FF47FA95FED6FA98" box="[151,332,1354,1380]" italics="true" pageId="38" pageNumber="39">M. melanosticta</emphasis>
</taxonomicName>
.
