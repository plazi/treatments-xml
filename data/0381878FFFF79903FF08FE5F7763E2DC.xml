<emphasis id="B95CEA8BFFE89903FF58FE17752FE1E4" bold="true" box="[199,315,476,501]" pageId="16" pageNumber="236">Remarks.</emphasis>
Although 
<taxonomicName id="4C284D1AFFE89903FE26FE177675E1E5" box="[441,609,476,501]" genus="Tubella" higherTaxonomySource="CoL" kingdom="Animalia" pageId="16" pageNumber="236" phylum="Calcitarcha" rank="species" species="repens">
