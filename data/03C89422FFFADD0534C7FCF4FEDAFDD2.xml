</emphasis>
<location id="8EBE73EFFFFADD03350BFC11FE3DFB9C" LSID="urn:lsid:plazi:treatment:03C89422FFFADD0534C7FCF4FEDAFDD2:8EBE73EFFFFADD03350BFC11FE3DFB9C" box="[347,399,1010,1036]" county="Later" latitude="45.183334" longLatPrecision="1132" longitude="152.46666" municipality="Kurile-Kamchatka region" name="This" pageId="43" pageNumber="44">This</location>
