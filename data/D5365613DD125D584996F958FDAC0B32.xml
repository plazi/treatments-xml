<collectionCode id="3B8E7FC0DD115D5949D4F9BAFC790E10" box="[929,949,1587,1613]" country="Netherlands" lsid="urn:lsid:biocol.org:col:15678" name="Nationaal Herbarium Nederland, Leiden University branch" pageId="22" pageNumber="87" type="Herbarium">L</collectionCode>
. 
<collectingMunicipality id="BD447D7FDD115D5949B6F9BBFBDF0E11" box="[963,1043,1586,1612]" pageId="22" pageNumber="87">Reveal</collectingMunicipality>
