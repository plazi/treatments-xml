<taxonomicName id="4D8D4D2AFFCDFF8FFF24FD8BFDF7FD66" authority="Bartsch, 2020" authorityName="Bartsch" authorityYear="2020" box="[151,577,618,645]" class="Arachnida" family="Halacaridae" genus="Lohmannella" kingdom="Animalia" order="Trombidiformes" pageId="16" pageNumber="217" phylum="Arthropoda" rank="species" species="njoerdri">
