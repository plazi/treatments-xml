<taxonomicName id="29E00982FFDDD845085AF8D457EA4020" box="[783,915,1881,1904]" class="Insecta" family="Tabanidae" genus="Myioscaptia" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Diptera" pageId="2" pageNumber="120" phylum="Arthropoda" rank="genus">
