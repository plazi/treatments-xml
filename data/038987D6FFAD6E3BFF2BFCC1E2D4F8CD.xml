<materialsCitation id="3B483C9DFFAD6E3BFB5EFB79E15DFB55" ID-GBIF-Occurrence="1427476144" country="Indonesia" location="Java" pageId="3" pageNumber="304" specimenCount="1" stateProvince="Java">
