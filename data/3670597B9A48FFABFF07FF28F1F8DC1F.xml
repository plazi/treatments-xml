<quantity id="792145889A48FFABFF0FFEE6F48CDC1F" box="[159,229,345,371]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="2" pageNumber="36" unit="m" value="400.0">400m</quantity>
