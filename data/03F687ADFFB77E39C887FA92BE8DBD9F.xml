<taxonomicName id="4C5F4D38FFB77E39C887F8CBBC3EBD2F" authority="Gunther, 1939" authorityName="Gunther" authorityYear="1939" box="[151,588,1793,1818]" class="Insecta" family="Tetrigidae" genus="Spadotettix" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="157" phylum="Arthropoda" rank="species" species="hainanensis">
