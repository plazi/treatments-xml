<taxonomicName id="4C114D3EFFDEFFF6FC5EFCDDFD13FC8F" authorityName="? (Davis" authorityYear="1887" class="Actinopterygii" family="Pycnodontiformes (awaiting allocation)" genus="Palaeobalistum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="10" pageNumber="149" phylum="Chordata" rank="species" species="ventralis">
<emphasis id="B965EAAFFFDEFFF6FC5EFCDDFD13FC8F" italics="true" pageId="10" pageNumber="149">Palaeobalistum ventralis</emphasis>
