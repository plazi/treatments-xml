<taxonomicName id="4C1741A1EB6FFFDCFCD4EC4FFC2DF904" baseAuthorityName="Rossmassler" baseAuthorityYear="1837" box="[823,961,1662,1688]" class="Gastropoda" family="Hygromiidae" genus="Cernuella" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="360" phylum="Mollusca" rank="species" species="cisalpina">
