<paragraph id="8BE7369BD63D026E8DC0FC50FB2FFC38" blockId="16.[151,1179,978,1000]" box="[151,1179,978,1000]" pageId="16" pageNumber="17">
<taxonomicName id="4C584D18D63D026E8DC0FC50FE69FC38" authority="Uhler 1894" authorityName="Uhler" authorityYear="1894" box="[151,477,978,1000]" class="Insecta" family="Curculionidae" genus="Conorhinus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="rubidus">
<emphasis id="B92CEA89D63D026E8DC0FC50FED5FC38" box="[151,353,978,1000]" italics="true" pageId="16" pageNumber="17">Conorhinus rubidus</emphasis>
Uhler 
<number id="3D9024BCD63D026E8CFFFC51FE69FC38" box="[424,477,979,1000]" pageId="16" pageNumber="17" value="1894.0">1894</number>
</taxonomicName>
