<subSubSection id="C31A6555FFCA781C1A86F9BE39528857" box="[199,1040,1584,1609]" pageId="5" pageNumber="397" type="etymology">
<paragraph id="8BBF36DEFFCA781C1A86F9BE39528857" blockId="5.[151,1436,1584,1969]" box="[199,1040,1584,1609]" pageId="5" pageNumber="397">
<emphasis id="B974EACCFFCA781C1A86F9BE3C048857" bold="true" box="[199,326,1584,1609]" pageId="5" pageNumber="397">Etymology</emphasis>
