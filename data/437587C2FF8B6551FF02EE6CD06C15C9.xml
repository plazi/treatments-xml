<paragraph id="CB6336D4FF8B654EFF52EF59D0351346" blockId="79.[151,1437,1761,2041]" box="[199,1055,1797,1825]" pageId="79" pageNumber="80">
<emphasis id="F9A8EAC6FF8B654EFF52EF59D5401346" bold="true" box="[199,362,1797,1824]" pageId="79" pageNumber="80">Label details:</emphasis>
‘ 
<taxonomicName id="0CDC4D57FF8B654EFEEFEF59D66B1346" authorityName="Reeve" authorityYear="1852" box="[378,577,1798,1824]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="79" pageNumber="80" phylum="Mollusca" rank="species" species="jeffreysiana">
