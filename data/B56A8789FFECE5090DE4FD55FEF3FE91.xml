): H = 
<quantity id="FA3B9B7AFFECE5080F16FA1AFD67FA31" box="[621,723,1474,1498]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" pageId="8" pageNumber="9" unit="mm" value="12.5">12.5 mm</quantity>
; W = 
<quantity id="FA3B9B7AFFECE5080E74FA1AFCC2FA31" box="[783,886,1474,1498]" metricMagnitude="-2" metricUnit="m" metricValue="3.82" pageId="8" pageNumber="9" unit="mm" value="38.2">38.2 mm</quantity>
; h = 12.0 mm; w = 
<quantity id="FA3B9B7AFFECE5080DAEFA30FE88F9EB" box="[213,316,1512,1536]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" pageId="8" pageNumber="9" unit="mm" value="11.5">11.5 mm</quantity>
. 
<typeStatus id="E278883DFFECE5080C32FA30FE04F9EB" box="[329,432,1512,1536]" pageId="8" pageNumber="9">Paratype</typeStatus>
(
<collectionCode id="5BD2AE5AFFECE5080CC5FA30FDB2F9EB" box="[446,518,1512,1536]" country="Switzerland" httpUri="http://grbio.org/cool/5hwi-0wgz" name="Naturhistorisches Museum der Burgergemeinde Bern" pageId="8" pageNumber="9">NMBE</collectionCode>
5018967): H = 
<quantity id="FA3B9B7AFFECE5080FB8FA30FC99F9EB" box="[707,813,1512,1536]" metricMagnitude="-2" metricUnit="m" metricValue="1.1199999999999999" pageId="8" pageNumber="9" unit="mm" value="11.2">11.2 mm</quantity>
; W = 
<quantity id="FA3B9B7AFFECE5080E08FA30FC6AF9EB" box="[883,990,1512,1536]" metricMagnitude="-2" metricUnit="m" metricValue="3.22" pageId="8" pageNumber="9" unit="mm" value="32.2">32.2 mm</quantity>
; h = 
<quantity id="FA3B9B7AFFECE5080DE4F9D5FEB1F9CE" box="[159,261,1549,1573]" metricMagnitude="-2" metricUnit="m" metricValue="1.1199999999999999" pageId="8" pageNumber="9" unit="mm" value="11.2">11.2 mm</quantity>
; w = 
<quantity id="FA3B9B7AFFECE5080C44F9D5FE10F9CE" box="[319,420,1549,1573]" metricMagnitude="-2" metricUnit="m" metricValue="1.03" pageId="8" pageNumber="9" unit="mm" value="10.3">10.3 mm</quantity>
