</taxonomicName>
(
<taxonomicName id="4C6CA1CF6720B558C4154AC73D8F2081" box="[1321,1429,439,464]" class="Magnoliopsida" family="Apiaceae" kingdom="Plantae" order="Apiales" pageId="373" pageNumber="374" phylum="Tracheophyta" rank="family">Apiaceae</taxonomicName>
