<emphasis id="97CCEAF3FF8991182BC4517EFE56FA2A" box="[409,458,1400,1424]" italics="true" pageId="2" pageNumber="60">cp -h</emphasis>
<emphasis id="97CCEAF3FF8991182B965180FE4FFA2E" box="[459,467,1415,1428]" italics="true" pageId="2" pageNumber="60">
<number id="137024C6FF8991182B965180FE4FFA2E" box="[459,467,1415,1428]" pageId="2" pageNumber="60" value="3.0">3</number>
