(
<figureCitation id="26BB2BF9FFAC2E75B8B7F9CEFF6AB550" box="[159,245,1585,1609]" captionStart="FIGURES 46–50" captionStartId="236.[151,258,1974,1995]" captionTargetBox="[172,1416,193,1953]" captionTargetId="figure-52@236.[172,1416,193,1953]" captionTargetPageId="236" captionText="FIGURES 46–50. Lateral view. 46, M. paralissonotus. 47, M. latissulcus*. 48, M. convexus*. 49, M. albiaterartus*. 50, M. nigriscutus." figureDoi="http://doi.org/10.5281/zenodo.5262147" httpUri="https://zenodo.org/record/5262147/files/figure.png" pageId="50" pageNumber="51">Figs 49</figureCitation>
, 
<figureCitation id="26BB2BF9FFAC2E75B929F9CEFEB2B550" box="[257,301,1585,1609]" captionStart="FIGURES 162–170" captionStartId="258.[151,258,1920,1941]" captionTargetBox="[151,1436,193,1899]" captionTargetId="figure-77@258.[151,1436,193,1899]" captionTargetPageId="258" captionText="FIGURES 162–170. Propodeum, dorsal view. 162, M. advenus*. 163, M. paradoxus*. 164, M. badius*. 165, M. albiaterartus*. 166, M. longicaudis*. 167, M. amarelus. 168, M. apertus. 169, M. convexus*. 170, M. laevilatus." figureDoi="http://doi.org/10.5281/zenodo.5262191" httpUri="https://zenodo.org/record/5262191/files/figure.png" pageId="50" pageNumber="51">165</figureCitation>
, 
<figureCitation id="26BB2BF9FFAC2E75B911F9CEFEF9B550" box="[313,358,1585,1609]" captionStart="FIGURES 236–241" captionStartId="268.[151,258,1967,1988]" captionTargetBox="[151,1436,193,1946]" captionTargetId="figure-56@268.[151,1436,193,1946]" captionTargetPageId="268" captionText="FIGURES 236–241. Hind leg. 236, M. mesonotator. 237, M. arcuatus. 238, M. keraiopetalus. 239, M. mesosternalis. 240, M. albiaterartus*. 241, M. versicolor*." figureDoi="http://doi.org/10.5281/zenodo.5262211" httpUri="https://zenodo.org/record/5262211/files/figure.png" pageId="50" pageNumber="51">240</figureCitation>
