<taxonomicName id="4C734D51FFAB1744FEE86925A87EFCD0" authority="Kramer, 1875" authorityName="Kramer" authorityYear="1875" box="[307,610,836,861]" class="Arachnida" family="Axonopsidae" genus="Brachypoda" kingdom="Animalia" order="Trombidiformes" pageId="35" pageNumber="146" phylum="Arthropoda" rank="genus">
<emphasis id="B907EAC0FFAB1744FEE86925ABA0FCD0" box="[307,444,836,861]" italics="true" pageId="35" pageNumber="146">Brachypoda</emphasis>
Kramer, 
<number id="3DBB24F5FFAB1744FDF16925A87EFCD0" box="[554,610,836,861]" pageId="35" pageNumber="146" value="1875.0">1875</number>
</taxonomicName>
to full generic status, transfer of species previously placed in subgenus 
<taxonomicName id="4C734D51FFAB1744FF4C6906ABF0FC0D" authority="Viets, 1937" authorityName="Viets" authorityYear="1937" box="[151,492,871,896]" class="Arachnida" family="Aturidae" genus="Hemibrachypoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="35" pageNumber="146" phylum="Arthropoda" rank="genus">
<emphasis id="B907EAC0FFAB1744FF4C6906AB46FC0D" box="[151,346,871,896]" italics="true" pageId="35" pageNumber="146">Hemibrachypoda</emphasis>
Viets, 
<number id="3DBB24F5FFAB1744FE6F6909ABF0FC0D" box="[436,492,872,896]" pageId="35" pageNumber="146" value="1937.0">1937</number>
</taxonomicName>
