<taxonomicName id="4C734D51FFAB1744FBF468B6AF71FD7D" authority="Habeeb, 1954" authorityName="Habeeb" authorityYear="1954" box="[1071,1389,727,752]" class="Arachnida" family="Axonopsidae" genus="Woolastookia" kingdom="Animalia" order="Trombidiformes" pageId="35" pageNumber="146" phylum="Arthropoda" rank="genus">
