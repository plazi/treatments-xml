<elevation id="05DED1C32A6CA425FD7FFAC8FCA20F42" box="[694,820,1365,1388]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="31" pageNumber="32" unit="m" value="20.0">
<quantity id="490B9B152A6CA425FD7FFAC8FD780F42" box="[694,750,1365,1388]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="31" pageNumber="32" unit="m" value="20.0">20 m</quantity>
