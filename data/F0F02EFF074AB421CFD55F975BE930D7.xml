<paragraph id="C399F700A4FA33050DC61D1B5D0886B6" pageId="null" pageNumber="541">
<taxonomicName id="E9FB4F288DAAACDB9495E898169A6706" authority="(DC.) Schleicher" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="541" phylum="Tracheophyta" rank="species" species="alpinus">
