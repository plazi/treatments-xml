<taxonomicName id="4C654D71FFB74368FD29C6948BA4FB9F" box="[700,925,1014,1039]" class="Insecta" family="Anthomyiidae" genus="Stomoxys" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="calcitrans">
