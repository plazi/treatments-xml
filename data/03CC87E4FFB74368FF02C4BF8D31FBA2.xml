<taxonomicName id="4C654D71FFB74368FC41C6948C93FB9F" box="[980,1194,1014,1039]" class="Insecta" family="Fanniidae" genus="Fannia" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="canicularis">
