<taxonomicName id="4C654D71FFB74368FBE3C17F8D38FBA3" authority="Robineau-Desvoidy, 1863" authorityName="Robineau-Desvoidy" authorityYear="1863" box="[1142,1281,1051,1075]" class="Insecta" family="Sarcophagidae" genus="Bercaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="cruentata">
