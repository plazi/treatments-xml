; E = 
<quantity id="5B3B9B30FFE1F82915C1F849FE288197" box="[415,508,1982,2006]" metricMagnitude="-5" metricUnit="m" metricValue="2.0" pageId="15" pageNumber="316" unit="mm" value="0.02">0.02 mm</quantity>
; F = 
