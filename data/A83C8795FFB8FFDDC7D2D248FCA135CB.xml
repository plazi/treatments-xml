<taxonomicName id="E7954D00FFB8FFDCC7D2D248FE3C3723" authority="Régimbart, 1882" authorityName="Régimbart" authorityYear="1882" box="[151,487,799,824]" class="Insecta" family="Gyrinidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="95" phylum="Arthropoda" rank="tribe" tribe="Enhydrini">
