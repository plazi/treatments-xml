<taxonomicName id="4C4F4D0CFFBEFFC8F21CFC9B2FF17741" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[329,505,870,894]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="similis">
