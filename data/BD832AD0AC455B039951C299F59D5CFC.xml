<taxonomicName id="E8FA0E7F874E75B8D91CD1BD2C59B9B5" class="Malacostraca" family="Corophiidae" genus="Paracorophium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracorophium" order="Amphipoda" pageId="0" pageNumber="35" phylum="Arthropoda" rank="genus">Paracorophium</taxonomicName>
