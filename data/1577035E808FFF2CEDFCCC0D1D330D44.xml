<quantity id="5A261FAD808FFF2CEE05CD361A9E0D4B" box="[878,963,799,824]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="3.5" metricValueMin="0.0" pageId="139" pageNumber="140" unit="m" value="17.5" valueMax="35.0" valueMin="0.0">0–35 m</quantity>
