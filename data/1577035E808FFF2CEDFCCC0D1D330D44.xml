<subSubSection id="D5C4E1C3808FFF2CEDACCCFE1D330D44" pageId="139" pageNumber="140" type="distribution">
<paragraph id="9D61B248808FFF2CEDACCCFE1D330D44" blockId="139.[151,1436,548,824]" pageId="139" pageNumber="140">
<emphasis id="AFAA6E5A808FFF2CEDACCCFE18AC0C8C" bold="true" box="[199,497,727,752]" pageId="139" pageNumber="140">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA808FFF2CEC93CCF11BD50C8C" box="[504,648,727,752]" country="Papua New Guinea" name="New Ireland" pageId="139" pageNumber="140">New Ireland</collectingRegion>
