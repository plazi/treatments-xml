<emphasis id="69DA3C4CFFCDFFEDFD9E8911B399CC86" bold="true" box="[622,705,1900,1924]" pageId="22" pageNumber="23">
<taxonomicNameLabel id="72E98137FFCDFFEDFD9E8911B399CC86" box="[622,705,1900,1924]" pageId="22" pageNumber="23" rank="species">sp. nov.</taxonomicNameLabel>
