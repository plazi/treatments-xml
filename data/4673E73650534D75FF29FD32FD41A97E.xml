; 16, 
<taxonomicName id="09DA2DA350534D7AFF2AF79CFDE6A665" authority="Samoderzhenkov" authorityName="Samoderzhenkov" box="[234,549,2094,2116]" class="Insecta" family="Chrysomelidae" genus="Apophylia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="clavicornis">
