<taxonomicName id="09DA2DA350534D7AFADBF7A1FAB4A601" authority="Bezdek, 2005" authorityName="Bezdek" authorityYear="2005" box="[1307,1399,2066,2087]" class="Insecta" family="Chrysomelidae" genus="Apophylia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="kubani" status="new species">
