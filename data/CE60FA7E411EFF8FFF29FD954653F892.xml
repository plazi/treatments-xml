<subSubSection id="0ED318E3411EFF8FFF79FD4D4567FCF6" pageId="8" pageNumber="31" type="etymology">
<paragraph id="46764B68411EFF8FFF79FD4D4567FCF6" blockId="8.[151,1437,620,1908]" pageId="8" pageNumber="31">
<emphasis id="74BD977A411EFF8FFF79FD4D4563FD12" bold="true" box="[199,338,729,755]" pageId="8" pageNumber="31">Etymology:</emphasis>
