<paragraph id="52A6B93435BABE81E401B1BB8FA7A085" pageId="2" pageNumber="3">
<emphasis id="DD88F57D790C9A79D1C8E60AFECA5E4C" bold="true" pageId="2" pageNumber="3">Shell</emphasis>
