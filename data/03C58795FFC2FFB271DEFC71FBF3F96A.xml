<figureCitation id="13572A06FFC2FFB275C7FA14FB19FA67" box="[1166,1261,1506,1530]" captionStart="FIGURES 16 – 23" captionStartId="5.[151,264,1906,1929]" captionTargetBox="[266,1270,179,1867]" captionTargetId="figure@5.[151,1436,171,1903]" captionTargetPageId="5" captionText="FIGURES 16 – 23. Platychauliodes capensis Barnard. 16. Male genitalia, dorsal view; 17. Male genitalia, ventral view; 18. Male genitalia, lateral view; 19. Male genitalia, caudal view; 20. Male tenth sternum, ventral view; 21. Sclerite beneath male anus, ventral view; 22. Female genitalia, lateral view; 23. Female eighth sternum, ventral view. S 8 – 10: eighth to tenth sternum, T 8 - 10: eighth to tenth tergum, Gx 9: ninth gonocoxite, c: cercus, lar: lateral arm, mpl: median plate. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277786/files/figure.png" pageId="1" pageNumber="2">Figs. 23</figureCitation>
, 
<figureCitation id="13572A06FFC2FFB275B2FA14FAEEFA67" box="[1275,1306,1506,1530]" captionStart="FIGURES 24 – 31" captionStartId="7.[151,264,1950,1973]" captionTargetBox="[247,1308,207,1818]" captionTargetId="figure@7.[151,1436,193,1929]" captionTargetPageId="7" captionText="FIGURES 24 – 31. Platychauliodes pusillus (McLachlan). 24. Male genitalia, dorsal view; 25. Male genitalia, ventral view; 26. Male genitalia, lateral view; 27. Male genitalia, caudal view. 28. Male tenth sternum, ventral view; 29. Female genitalia, lateral view; 30. Female eighth sternum, ventral view; 31. Female genitalia, dorsal view. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277787/files/figure.png" pageId="1" pageNumber="2">30</figureCitation>
, 
<figureCitation id="13572A06FFC2FFB2746EFA14FAB0FA67" box="[1319,1348,1506,1530]" captionStart="FIGURES 32 – 40" captionStartId="9.[151,264,1907,1930]" captionTargetBox="[219,1362,202,1864]" captionTargetId="figure@9.[151,1436,195,1886]" captionTargetPageId="9" captionText="FIGURES 32 – 40. Platychauliodes thornei Barnard. 32. Male genitalia, dorsal view; 33. Male genitalia, ventral view; 34. Male genitalia, lateral view; 35. Male genitalia, caudal view; 36. Male tenth sternum, ventral view; 37. Sclerite beneath male anus, ventral view; 38. Female genitalia, lateral view; 39. Female seventh and eighth sternum, ventral view; 40. Female genitalia, dorsal view. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277788/files/figure.png" pageId="1" pageNumber="2">
<number id="3DA424A4FFC2FFB2746EFA14FAB0FA67" box="[1319,1348,1506,1530]" pageId="1" pageNumber="2" value="39.0">39</number>
</figureCitation>
) broad, posteriorly with a membranous lobe; ninth gonocoxite (
<figureCitation id="13572A06FFC2FFB27259F9F1FC84F982" box="[784,880,1543,1567]" captionStart="FIGURES 16 – 23" captionStartId="5.[151,264,1906,1929]" captionTargetBox="[266,1270,179,1867]" captionTargetId="figure@5.[151,1436,171,1903]" captionTargetPageId="5" captionText="FIGURES 16 – 23. Platychauliodes capensis Barnard. 16. Male genitalia, dorsal view; 17. Male genitalia, ventral view; 18. Male genitalia, lateral view; 19. Male genitalia, caudal view; 20. Male tenth sternum, ventral view; 21. Sclerite beneath male anus, ventral view; 22. Female genitalia, lateral view; 23. Female eighth sternum, ventral view. S 8 – 10: eighth to tenth sternum, T 8 - 10: eighth to tenth tergum, Gx 9: ninth gonocoxite, c: cercus, lar: lateral arm, mpl: median plate. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277786/files/figure.png" pageId="1" pageNumber="2">Figs. 22</figureCitation>
, 
<figureCitation id="13572A06FFC2FFB27235F9F1FC6EF982" box="[892,922,1543,1567]" captionStart="FIGURES 24 – 31" captionStartId="7.[151,264,1950,1973]" captionTargetBox="[247,1308,207,1818]" captionTargetId="figure@7.[151,1436,193,1929]" captionTargetPageId="7" captionText="FIGURES 24 – 31. Platychauliodes pusillus (McLachlan). 24. Male genitalia, dorsal view; 25. Male genitalia, ventral view; 26. Male genitalia, lateral view; 27. Male genitalia, caudal view. 28. Male tenth sternum, ventral view; 29. Female genitalia, lateral view; 30. Female eighth sternum, ventral view; 31. Female genitalia, dorsal view. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277787/files/figure.png" pageId="1" pageNumber="2">29</figureCitation>
, 
<figureCitation id="13572A06FFC2FFB272E1F9F1FC31F982" box="[936,965,1543,1567]" captionStart="FIGURES 32 – 40" captionStartId="9.[151,264,1907,1930]" captionTargetBox="[219,1362,202,1864]" captionTargetId="figure@9.[151,1436,195,1886]" captionTargetPageId="9" captionText="FIGURES 32 – 40. Platychauliodes thornei Barnard. 32. Male genitalia, dorsal view; 33. Male genitalia, ventral view; 34. Male genitalia, lateral view; 35. Male genitalia, caudal view; 36. Male tenth sternum, ventral view; 37. Sclerite beneath male anus, ventral view; 38. Female genitalia, lateral view; 39. Female seventh and eighth sternum, ventral view; 40. Female genitalia, dorsal view. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277788/files/figure.png" pageId="1" pageNumber="2">
<number id="3DA424A4FFC2FFB272E1F9F1FC31F982" box="[936,965,1543,1567]" pageId="1" pageNumber="2" value="38.0">38</number>
</figureCitation>
) foliate without gonostylus; tenth tergum (
<figureCitation id="13572A06FFC2FFB271D6F9DCFF09F9DF" box="[159,253,1578,1602]" captionStart="FIGURES 16 – 23" captionStartId="5.[151,264,1906,1929]" captionTargetBox="[266,1270,179,1867]" captionTargetId="figure@5.[151,1436,171,1903]" captionTargetPageId="5" captionText="FIGURES 16 – 23. Platychauliodes capensis Barnard. 16. Male genitalia, dorsal view; 17. Male genitalia, ventral view; 18. Male genitalia, lateral view; 19. Male genitalia, caudal view; 20. Male tenth sternum, ventral view; 21. Sclerite beneath male anus, ventral view; 22. Female genitalia, lateral view; 23. Female eighth sternum, ventral view. S 8 – 10: eighth to tenth sternum, T 8 - 10: eighth to tenth tergum, Gx 9: ninth gonocoxite, c: cercus, lar: lateral arm, mpl: median plate. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277786/files/figure.png" pageId="1" pageNumber="2">Figs. 22</figureCitation>
, 
<figureCitation id="13572A06FFC2FFB27041F9DCFED2F9DF" box="[264,294,1578,1602]" captionStart="FIGURES 24 – 31" captionStartId="7.[151,264,1950,1973]" captionTargetBox="[247,1308,207,1818]" captionTargetId="figure@7.[151,1436,193,1929]" captionTargetPageId="7" captionText="FIGURES 24 – 31. Platychauliodes pusillus (McLachlan). 24. Male genitalia, dorsal view; 25. Male genitalia, ventral view; 26. Male genitalia, lateral view; 27. Male genitalia, caudal view. 28. Male tenth sternum, ventral view; 29. Female genitalia, lateral view; 30. Female eighth sternum, ventral view; 31. Female genitalia, dorsal view. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277787/files/figure.png" pageId="1" pageNumber="2">29</figureCitation>
, 
<figureCitation id="13572A06FFC2FFB2707BF9DCFEBBF9DF" box="[306,335,1578,1602]" captionStart="FIGURES 32 – 40" captionStartId="9.[151,264,1907,1930]" captionTargetBox="[219,1362,202,1864]" captionTargetId="figure@9.[151,1436,195,1886]" captionTargetPageId="9" captionText="FIGURES 32 – 40. Platychauliodes thornei Barnard. 32. Male genitalia, dorsal view; 33. Male genitalia, ventral view; 34. Male genitalia, lateral view; 35. Male genitalia, caudal view; 36. Male tenth sternum, ventral view; 37. Sclerite beneath male anus, ventral view; 38. Female genitalia, lateral view; 39. Female seventh and eighth sternum, ventral view; 40. Female genitalia, dorsal view. Scale bars = 1.0 mm." httpUri="https://zenodo.org/record/277788/files/figure.png" pageId="1" pageNumber="2">
