<th id="76C1EA58FFA8CAEFFDBCFDC8FC2CD22C" box="[572,988,538,591]" gridcol="1" gridrow="0" pageId="2" pageNumber="577">
<taxonomicName id="4C20FAE5FFA83512FDBCFDC9FCD8D253" authority="Lin &amp; Bi" authorityName="Lin &amp; Bi" box="[572,808,538,560]" class="Insecta" family="Cerambycidae" genus="Spiniphilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="577" phylum="Arthropoda" rank="species" species="spinicornis">
