<paragraph id="A97D36E4FFFE2B04FF1D30CCFC31EBA9" blockId="8.[151,1436,1422,2023]" pageId="8" pageNumber="9">
<typeStatus id="76798846FFFE2B04FF1D30CCFF00EBC5" box="[151,255,1422,1446]" pageId="8" pageNumber="9">Holotype</typeStatus>
