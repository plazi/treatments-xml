<number id="1F0A24C3FFFE2B04FCEF30CCFC8BEBC5" box="[869,884,1422,1446]" pageId="8" pageNumber="9" value="6.0">6</number>
° 
<number id="1F0A24C3FFFE2B04FCF530CCFC62EBC5" box="[895,925,1422,1446]" pageId="8" pageNumber="9" value="10.0">10</number>
'N
</geoCoordinate>
, 
<geoCoordinate id="CCF65023FFFE2B04FC4E30CCFBD1EBC5" box="[964,1070,1422,1446]" direction="east" orientation="longitude" pageId="8" pageNumber="9" precision="925" value="116.65">
<number id="1F0A24C3FFFE2B04FC4E30CCFC0EEBC5" box="[964,1009,1422,1446]" pageId="8" pageNumber="9" value="116.0">116</number>
