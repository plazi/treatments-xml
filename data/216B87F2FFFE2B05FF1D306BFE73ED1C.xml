<number id="1F0A24C3FFFF2B05FD1A3606FD50ED3F" box="[656,687,836,860]" pageId="9" pageNumber="10" value="32.0">32</number>
; body w/o wings: 
<number id="1F0A24C3FFFF2B05FCF43606FC62ED3F" box="[894,925,836,860]" pageId="9" pageNumber="10" value="15.0">15</number>
; pronotum: 
<number id="1F0A24C3FFFF2B05FBAF3606FBB4ED3F" box="[1061,1099,836,860]" pageId="9" pageNumber="10" value="3.8">3.8</number>
; tegmen: 
<number id="1F0A24C3FFFF2B05FB323606FB28ED3F" box="[1208,1239,836,860]" pageId="9" pageNumber="10" value="23.0">23</number>
; hind wing: 
<number id="1F0A24C3FFFF2B05FAE93606FA68ED3F" box="[1379,1431,836,860]" pageId="9" pageNumber="10" value="27.5">27.5</number>
; hind femur: 
<quantity id="6E3A9B01FFFF2B05FEAB3625FE77ED1C" box="[289,392,871,895]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" pageId="9" pageNumber="10" unit="mm" value="12.5">12.5 mm</quantity>
.
</paragraph>
