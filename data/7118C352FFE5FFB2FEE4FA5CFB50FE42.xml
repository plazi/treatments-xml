<taxonomicName id="3EB109C7FFE5FFB2FE5DFA72FF2CF9B2" authority="Cramer, 1777: 25" authorityName="CRAMER" authorityPageNumber="25" authorityYear="1777" class="Insecta" family="Papilionidae" genus="Trojanus" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="306" phylum="Arthropoda" rank="species" species="numitor">
