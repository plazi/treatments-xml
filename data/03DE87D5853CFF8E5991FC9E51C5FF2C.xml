<number id="3DBF24E4853CFF8F59D1FC7255A8FC77" box="[215,231,907,931]" pageId="14" pageNumber="465" value="2.8">2</number>
</figureCitation>
, 
<figureCitation id="134C2A46853CFF8F59F5FC72544CFC77" box="[243,259,907,931]" captionStart="FIGURES 4 – 11" captionStartId="38.[151,264,1745,1768]" captionTargetBox="[181,1434,225,1688]" captionTargetId="figure@38.[151,1435,195,1688]" captionTargetPageId="38" captionText="FIGURES 4 – 11. Aedeagus in Deltochilum (Aganhyboma). 4. D. titovidaurrei new species. 5. D. cupreicolle. 6. D. viridescens new status. 7. D. viridicatum new species. 8. D. amandaarcanjoae new species. 9. D. trisignatum. 10. D. cangalha new species. and D. alpercata new species. 11. D. finestriatum new species. Figures scale = 1 mm." httpUri="https://zenodo.org/record/244797/files/figure.png" pageId="14" pageNumber="465">8</figureCitation>
, 
<number id="3DBF24E4853CFF8F5809FC725419FC77" box="[271,342,907,931]" pageId="14" pageNumber="465" value="89.98">
<figureCitation id="134C2A46853CFF8F5809FC725462FC77" box="[271,301,907,931]" captionStart="FIGURES 89 – 92" captionStartId="46.[151,264,1748,1771]" captionTargetBox="[150,1406,193,1727]" captionTargetId="figure@46.[150,1437,193,1728]" captionTargetPageId="46" captionText="FIGURES 89 – 92. Known distribution of D. (Aganhyboma). 89. trisignatum complex. 90. cupreicolle complex. 91. acropyge complex. 92. finestriatum complex." httpUri="https://zenodo.org/record/244805/files/figure.png" pageId="14" pageNumber="465">89</figureCitation>
, 
<figureCitation id="134C2A46853CFF8F583FFC725419FC77" box="[313,342,907,931]" captionStart="FIGURES 95 – 102" captionStartId="48.[151,264,1951,1974]" captionTargetBox="[181,1434,202,1924]" captionTargetId="figure@48.[152,1434,193,1930]" captionTargetPageId="48" captionText="FIGURES 95 – 102. Species of D. (Aganhyboma). 95. D. trisignatum. 96. D. kolbei. 97. D. violaceum. 98. D. amandaarcanjoae new species. 99. D. cupreicolle. 100. D. titovidaurrei new species. 101. D. viridescens new status. 102. D. viridescens new status. Figures scale = 1 mm." httpUri="https://zenodo.org/record/244807/files/figure.png" pageId="14" pageNumber="465">98</figureCitation>
