<taxonomicName id="16AECEBCC357BA17FDA1FF57828FFECA" authority="Walker, 1852" authorityName="Walker" authorityYear="1852" box="[558,915,234,260]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="sinensis">
