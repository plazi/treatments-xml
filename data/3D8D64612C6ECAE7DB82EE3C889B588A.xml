<paragraph id="DD7F765F7C1AE13A9A796D26DCCF0C7A" pageId="null" pageNumber="317">
Blaues 
<normalizedToken id="C78405418876C645BB9B09A5E8165F41" originalValue="Geißblatt" pageId="null" pageNumber="317">Geissblatt</normalizedToken>
</paragraph>
