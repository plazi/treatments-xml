<subSubSection id="7AD504EA64AF548C3FA07C79C852ACD7" pageId="0" pageNumber="73" type="affinities">
<paragraph id="D9EB4119862C5D2CFCE2B4332166F8F4" pageId="0" pageNumber="73">Affinities.</paragraph>
<paragraph id="1AD1602D687CD6BDDACEEB5DC3D09362" pageId="0" pageNumber="73">
