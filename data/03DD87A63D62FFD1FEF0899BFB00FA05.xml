<taxonomicName id="4C744D333D62FFD1FEF0899BFE59F864" authority="Roberts, 2007" authorityName="Roberts" authorityYear="2007" box="[293,438,1953,1977]" class="Actinopterygii" family="Cyprinidae" genus="Celestichthys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="1" pageNumber="132" phylum="Chordata" rank="genus" status="new genus">
<emphasis id="B900EAA23D62FFD1FEF0899BFE59F864" bold="true" box="[293,438,1953,1977]" italics="true" pageId="1" pageNumber="132">Celestichthys</emphasis>
