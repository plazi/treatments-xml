; 
<taxonomicName id="E6C342E2B432FFA3669DF25DFBF788A3" authority="du Buysson, 1900" authorityName="du Buysson" authorityYear="1900" box="[711,1072,875,901]" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="planifrons">
<emphasis id="13B7E573B432FFA3669DF25DFC9C88A3" box="[711,859,875,901]" italics="true" pageId="14" pageNumber="15">H. planifrons</emphasis>
<bibRefCitation id="45524490B432FFA36739F25DFBF788A3" author="du Buysson, R." box="[867,1072,875,901]" pageId="14" pageNumber="15" pagination="125 - 165" refId="ref51553" refString="du Buysson, R. (1900) Contribution aux Chrysidides du Globe (4 e serie). Revue d'Entomologie, 19, 125 - 165." type="journal article" year="1900">du Buysson, 1900</bibRefCitation>
</taxonomicName>
; 
<taxonomicName id="E6C342E2B432FFA36066F25DFA5088A3" authority="Linsenmaier, 1959" authorityName="Linsenmaier" authorityYear="1959" box="[1084,1431,874,901]" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="iocosum">
