<taxonomicName id="4C264D41CA47FFA8C990340AFBB8F783" authorityName="Brisson" authorityYear="1762" box="[1013,1086,2050,2079]" class="Mammalia" family="Mustelidae" genus="Lutra" kingdom="Animalia" order="Carnivora" pageId="23" pageNumber="642" phylum="Chordata" rank="genus">Lutra</taxonomicName>
