, 
<emphasis id="ABABB67994CFB6ABD5A1457514FA652D" italics="true" pageId="0" pageNumber="1">lp</emphasis>
, 
<emphasis id="597DE6D930A51E263F37CFACFB2C5A44" italics="true" pageId="0" pageNumber="1">h</emphasis>
1, 
<emphasis id="AF1724C7DF19F69D21CEAB6503FE691C" italics="true" pageId="0" pageNumber="1">h</emphasis>
