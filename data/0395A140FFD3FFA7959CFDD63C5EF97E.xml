<collectingCountry id="F32B50C6FFD3FFA7965DFA983DFDFA8E" box="[854,944,1294,1319]" name="Jamaica" pageId="17" pageNumber="566">Jamaica</collectingCountry>
by 
<bibRefCitation id="EFAD6DA7FFD3FFA796D1FA983A25FA8E" author="Cobb" box="[986,1128,1294,1319]" pageId="17" pageNumber="566" refString="Cobb, N. A. (1920) One hundred new nemas (type species of 100 new genera). Contributions to a Science Nematology, 9, 217 - 343." type="journal article" year="1920">
Cobb (
<number id="3DF40271FFD3FFA7912DFA993A2CFA8E" box="[1062,1121,1295,1319]" pageId="17" pageNumber="566" value="1920.0">1920</number>
)
</bibRefCitation>
