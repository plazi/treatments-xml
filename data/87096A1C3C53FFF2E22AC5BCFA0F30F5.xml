<taxonomicName id="C8A0A0893C53FFF1E109C0BCFD2F3588" baseAuthorityName="Eigenmann &amp; Eigenmann" baseAuthorityYear="1888" box="[524,672,1922,1945]" class="Actinopterygii" family="Pimelodidae" genus="Steindachneridion" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="612" phylum="Chordata" rank="species" species="amblyurum">
