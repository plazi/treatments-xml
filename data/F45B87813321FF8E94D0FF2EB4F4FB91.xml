<number id="CA3A24B03321FF8E95A5FBBEB469FBE9" box="[482,534,1031,1055]" pageId="35" pageNumber="436" value="2.28">2.28</number>
, 
<collectingCountry id="04E576073321FF8E966FFBBEB430FBE9" box="[552,591,1031,1055]" name="Switzerland" pageId="35" pageNumber="436">CH</collectingCountry>
<number id="CA3A24B03321FF8E961BFBBEB4EFFBE9" box="[604,656,1031,1055]" pageId="35" pageNumber="436" value="1.24">1.24</number>
, AEW 
<number id="CA3A24B03321FF8E96A8FBBFB55DFBE9" box="[751,802,1030,1055]" pageId="35" pageNumber="436" value="1.68">1.68</number>
