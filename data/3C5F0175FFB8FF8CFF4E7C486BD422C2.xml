<collectionCode id="D2E728A6FFBBFF8CFAD778A66A7523CF" LSID="urn:lsid:biocol.org:col:34871" box="[1294,1381,224,248]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="22" pageNumber="103">USNM</collectionCode>
