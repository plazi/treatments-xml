<number id="38202489FFF6FF965469F897AD486607" box="[267,282,1850,1876]" pageId="15" pageNumber="62" value="1.0">1</number>
with foliaceous termination, originating subterminally from 
<number id="38202489FFF6FF965162F897A85D6607" box="[1024,1039,1850,1876]" pageId="15" pageNumber="62" value="1.0">1</number>
of the 
<number id="38202489FFF6FF965105F897A8246607" box="[1127,1142,1850,1876]" pageId="15" pageNumber="62" value="2.0">2</number>
bilateral branches; total length 
<number id="38202489FFF6FF965585F8CCAD566628" box="[231,260,1889,1915]" pageId="15" pageNumber="62" value="30.0">30</number>
(
<number id="38202489FFF6FF965476F8CCAD306628" box="[276,354,1889,1915]" pageId="15" pageNumber="62" value="30.0" valueMax="32.0" valueMin="28.0">28–32</number>
; n = 
<number id="38202489FFF6FF9654FEF8CCADEE6628" box="[412,444,1889,1915]" pageId="15" pageNumber="62" value="10.0">10</number>
); tube trace-length 
<number id="38202489FFF6FF9657C2F8CCAEEF6628" box="[672,701,1889,1915]" pageId="15" pageNumber="62" value="32.0">32</number>
(
<number id="38202489FFF6FF9657AFF8CCAF496628" box="[717,795,1889,1915]" pageId="15" pageNumber="62" value="31.0" valueMax="34.0" valueMin="28.0">28–34</number>
; n = 
<number id="38202489FFF6FF965637F8CCAF246628" box="[853,886,1889,1915]" pageId="15" pageNumber="62" value="10.0">10</number>
).
</paragraph>
