and 
<taxonomicName id="49E84D2DFFF6FF965437F802AFBA669B" authority="Guegan &amp; Lambert, 1991" authorityName="Guegan &amp; Lambert" authorityYear="1991" box="[341,1000,1966,1992]" class="Monogenea" family="Dactylogyridae" genus="Dactylogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="15" pageNumber="62" phylum="Platyhelminthes" rank="species" species="longiphalloides">
<emphasis id="BC9CEABCFFF6FF965437F802AEE3669B" box="[341,689,1967,1992]" italics="true" pageId="15" pageNumber="62">Dactylogyrus longiphalloides</emphasis>
<bibRefCitation id="EA794B5FFFF6FF9657D8F803AFBA669B" author="Guegan" box="[698,1000,1966,1992]" pageId="15" pageNumber="68" refString="Guegan, J. - F. &amp; Lambert, A. (1991) Dactylogyrids (Platyhelminthes: Monogenea) of Labeo (Teleostei: Cyprinidae) from West African coastal rivers. Journal of the Helminthological Society of Washington, 58, 85 - 99." type="journal article" year="1991">
Guégan &amp; Lambert, 
<number id="38202489FFF6FF9656CCF803AFBA669B" box="[942,1000,1966,1992]" pageId="15" pageNumber="62" value="1991.0">1991</number>
</bibRefCitation>
</taxonomicName>
from 
<taxonomicName id="49E84D2DFFF6FF965150F802A8E6669B" box="[1074,1204,1967,1992]" class="Actinopterygii" family="Cyprinidae" genus="Labeo" kingdom="Animalia" order="Cypriniformes" pageId="15" pageNumber="62" phylum="Chordata" rank="species" species="alluaudi">
<emphasis id="BC9CEABCFFF6FF965150F802A8E6669B" box="[1074,1204,1967,1992]" italics="true" pageId="15" pageNumber="62">L. alluaudi</emphasis>
</taxonomicName>
(see 
<bibRefCitation id="EA794B5FFFF6FF965191F803A9C5669B" author="Paperna" box="[1267,1431,1966,1992]" pageId="15" pageNumber="68" refString="Paperna, I. (1979) Monogenea of inland water fish in Africa. Annales de Musee Royal de l'Afrique Centrale, Science, Zoologie, 226, 1 - 131." type="journal article" year="1979">
Paperna 
<number id="38202489FFF6FF96503BF803A9C5669B" box="[1369,1431,1966,1992]" pageId="15" pageNumber="62" value="1979.0">1979</number>
</bibRefCitation>
; 
<bibRefCitation id="EA794B5FFFF6FF9655F5F878AD8A66BC" author="Guegan" box="[151,472,2005,2031]" pageId="15" pageNumber="68" refString="Guegan, J. - F. &amp; Lambert, A. (1991) Dactylogyrids (Platyhelminthes: Monogenea) of Labeo (Teleostei: Cyprinidae) from West African coastal rivers. Journal of the Helminthological Society of Washington, 58, 85 - 99." type="journal article" year="1991">
Guégan &amp; Lambert 
<number id="38202489FFF6FF9654F7F878AD8A66BC" box="[405,472,2005,2031]" pageId="15" pageNumber="62" value="1991.0">1991</number>
