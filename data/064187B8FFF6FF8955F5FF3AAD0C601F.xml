Paperna, 
<number id="38202489FFF6FF9651A9F825A95566F1" box="[1227,1287,1928,1954]" pageId="15" pageNumber="62" value="1973.0">1973</number>
</bibRefCitation>
</taxonomicName>
from 
<taxonomicName id="49E84D2DFFF6FF965033F825AD49669B" class="Actinopterygii" family="Cyprinidae" genus="Labeo" kingdom="Animalia" order="Cypriniformes" pageId="15" pageNumber="62" phylum="Chordata" rank="species" species="victorianus">
