and 
<taxonomicName id="4C7E3589FFD2FFF4FC72FACC35BFFE49" baseAuthorityName="Balthasar" baseAuthorityYear="1941" box="[1018,1219,417,441]" class="Insecta" family="Aphodiidae" genus="Labarrus" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="24" phylum="Arthropoda" rank="species" species="pseudolividus">
