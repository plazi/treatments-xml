); 
<bibRefCitation id="EFEF33FBFFD3FFF5FCDFFC05354CF873" author="Blackwelder, R. E." box="[855,1072,1899,1923]" journalOrPublisher="United States National Museum Bulletin" pageId="24" pageNumber="23" pagination="189 - 341" part="185" refId="ref193872" refString="Blackwelder, R. E. 1944. Checklist of the coleopterous insects of Mexico, Central America, the West Indies, and South America. Part 2. United States National Museum Bulletin 185 (2): 189 - 341." title="Checklist of the coleopterous insects of Mexico, Central America, the West Indies, and South America. Part 2" type="journal article" year="1944">Blackwelder 1944</bibRefCitation>
(not mentioned); 
<bibRefCitation id="EFEF33FBFFD3FFF4FA8CFC05306BFF01" author="Skelley" etAl="et al." firstAuthor="Skelley" lastPageId="25" lastPageNumber="24" pageId="24" pageNumber="23" pagination="1 - 14" refId="ref225232" refString="Skelley, P. E., M. Dellacasa, G. Dellacasa, and R. D. Gordon. 2007. Checklist of the Aphodiini of Mexico, Central and South America (Coleoptera: Scarabaeinae: Aphodiinae). Insecta Mundi 0014: 1 - 14." type="book chapter" year="2007">
