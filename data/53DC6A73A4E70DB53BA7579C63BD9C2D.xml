<paragraph id="5B7C33706DE7924E83C5AF085857B578" pageId="null" pageNumber="452">
<pageBreakToken id="96321FD720FB732FC3893E5E3A9B29D1" pageId="null" pageNumber="452" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="49490149BA4ADE6443FC7A5CFC1C8006" authority="J. Herrmann" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="452" phylum="Tracheophyta" rank="species" species="pomifera">
Rosa 
