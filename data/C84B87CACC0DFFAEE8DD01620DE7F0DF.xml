<location id="453D6007CC0DFFAEECEB05400A4DF073" LSID="urn:lsid:plazi:treatment:C84B87CACC0DFFAEE8DD01620DE7F0DF:453D6007CC0DFFAEECEB05400A4DF073" box="[1185,1362,1287,1312]" country="Iceland" county="Livingston Island" municipality="Trinity islands" name="Balleny Islands" pageId="27" pageNumber="28" stateProvince="Cape">Balleny Islands</location>
