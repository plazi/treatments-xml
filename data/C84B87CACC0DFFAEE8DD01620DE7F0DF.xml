<taxonomicName id="87E24D5FCC0DFFAEE9E8056C0D05F017" box="[418,538,1323,1348]" class="Hydrozoa" family="Campanulariidae" genus="Campanularia" kingdom="Animalia" order="Leptothecata" pageId="27" pageNumber="60" phylum="Cnidaria" rank="species" species="antarctica">
