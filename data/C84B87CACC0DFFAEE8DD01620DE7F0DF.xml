<taxonomicName id="87E24D5FCC0DFFAEEB3605400CEBF073" box="[892,1012,1287,1312]" class="Hydrozoa" family="Campanulariidae" genus="Campanularia" kingdom="Animalia" order="Leptothecata" pageId="27" pageNumber="60" phylum="Cnidaria" rank="species" species="antarctica">
