<taxonomicName id="4C734D12BA4EFFE4744DFF2EFBF3FF16" box="[879,1102,152,176]" class="Insecta" family="Andrenidae" genus="Stenocolletes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="495" phylum="Arthropoda" rank="species" species="pictus">
