; 22–24 – 
<taxonomicName id="4C364D428C24FFD534B6FCCE803FFC90" authority="Murakami, 2016" authorityName="Murakami" authorityYear="2016" box="[397,568,870,889]" class="Insecta" family="Cleridae" genus="Paracladiscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="732" phylum="Arthropoda" rank="species" species="choui" status="sp.nov.">
<emphasis id="B942EAD38C24FFD534B6FCCE803FFC90" box="[397,568,870,889]" italics="true" pageId="7" pageNumber="732">Paracladiscus choui</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27157A88C24FFD53707FCCE807AFC90" box="[572,637,870,889]" pageId="7" pageNumber="732" rank="species">sp.nov.</taxonomicNameLabel>
