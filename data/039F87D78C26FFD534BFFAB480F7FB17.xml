<taxonomicName id="4C364D428C24FFD536F0FCE48339FC90" authority="Nakane, 1989" authorityName="Nakane" authorityYear="1989" class="Insecta" family="Cleridae" genus="Diplopherusa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="732" phylum="Arthropoda" rank="species" species="kitamurai">
