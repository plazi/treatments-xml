<taxonomicName id="4C7215C4A777FF9EFED2FA38365B5A90" authorityName="Abdalla &amp; Mansell &amp; Sole" authorityYear="2019" baseAuthorityName="Navas" baseAuthorityYear="1910" box="[328,546,1469,1495]" class="Insecta" family="Nemopteridae" genus="Afroptera" kingdom="Animalia" order="Neuroptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="leptocera">
