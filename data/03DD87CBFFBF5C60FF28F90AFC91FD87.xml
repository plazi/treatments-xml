<taxonomicName id="4C744D5EFFBC5C61FEFDF891FD99F82F" authority="Ramaiah &amp; Meshram, 2021" authorityName="Ramaiah &amp; Meshram" authorityYear="2021" box="[322,632,1917,1941]" class="Insecta" family="Cicadellidae" genus="Myittana" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="191" phylum="Arthropoda" rank="species" species="bidentata" status="sp. nov." subGenus="Myittana">
