<collectingCountry id="F3181ECA02134A5F51A17FDEFA80FB5F" box="[1255,1345,1196,1220]" name="Guyana" pageId="20" pageNumber="21">Guyana</collectingCountry>
, 
<collectingCountry id="F3181ECA02134A5F50177FDEFA59FB5F" box="[1361,1432,1196,1220]" name="Brazil" pageId="20" pageNumber="21">Brazil</collectingCountry>
, 
<collectingCountry id="F3181ECA02134A5F55D17FA2FF0CFB7C" box="[151,205,1232,1255]" name="Peru" pageId="20" pageNumber="21">Peru</collectingCountry>
, 
<collectingCountry id="F3181ECA02134A5F559E7FBDFEECFB7C" box="[216,301,1231,1255]" name="Bolivia" pageId="20" pageNumber="21">Bolivia</collectingCountry>
