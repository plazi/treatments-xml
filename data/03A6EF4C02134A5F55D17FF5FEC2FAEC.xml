, 
<collectingCountry id="F3181ECA02134A5F56C07FDEFBABFB5F" box="[902,1130,1196,1220]" name="Trinidad and Tobago" pageId="20" pageNumber="21">Trinidad &amp; Tobago</collectingCountry>
, 
<collectingCountry id="F3181ECA02134A5F513D7FDEFB17FB58" box="[1147,1238,1196,1219]" name="Panama" pageId="20" pageNumber="21">Panama</collectingCountry>
