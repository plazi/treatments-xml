<treatmentCitationGroup id="FAB1DC5AFFA7FFACFF236EDA9E60FA99" box="[151,628,1272,1294]" pageId="21" pageNumber="22">
<taxonomicName id="1DA180F7FFA7FFACFF236EDA9D9BFA99" baseAuthorityName="Berlese" baseAuthorityYear="1903" box="[151,399,1272,1294]" class="Arachnida" family="Pachylaelapidae" genus="Pachydellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="angulatipes">
