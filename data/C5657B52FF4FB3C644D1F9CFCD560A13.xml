<taxonomicName id="8ACCB1C7FF48B3C7454AF852CA6F0C9D" authority="Rathbun, 1893" authorityName="Rathbun" authorityYear="1893" box="[268,617,1947,1969]" class="Malacostraca" family="Inachidae" genus="Erileptus" kingdom="Animalia" order="Decapoda" pageId="212" pageNumber="213" phylum="Arthropoda" rank="species" species="spinosus">
