<taxonomicName id="2DA1540CB6A8455E2EA294640E27BDF6" authority="L." class="Liliopsida" family="Amaryllidaceae" genus="Allium" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="571" phylum="Tracheophyta" rank="species" species="victorialis">
Allium 
<normalizedToken id="4346900B6C22AA39C4BB55A8E6B97ACF" originalValue="Victoriális" pageId="null" pageNumber="571">Victorialis</normalizedToken>
<authorityName id="C24DA2CCEB47A07006F8CBA5CEBB70C7" pageId="null" pageNumber="571">L.</authorityName>
