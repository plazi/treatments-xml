<paragraph id="8B9636B51819F33551E57D8165009AAE" blockId="14.[151,1436,1125,1221]" pageId="14" pageNumber="395">
<taxonomicName id="4C294D361819F33551E57D8160239ACA" box="[199,368,1160,1184]" class="Malacostraca" family="Galatheidae" genus="Munida" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="395" phylum="Arthropoda" rank="species" species="chacei">
