<quantity id="4CD19B501819F33552E77D81655D9ACA" box="[965,1038,1160,1185]" metricMagnitude="2" metricUnit="m" metricValue="4.46" pageId="14" pageNumber="395" unit="m" value="446.0">446 m</quantity>
