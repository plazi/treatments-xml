<collectionCode id="ED498D8BFFE37C7DFD39FCA7B57DFCA9" box="[739,817,838,861]" httpUri="http://grbio.org/cool/063z-3mpp" name="Museum of the Institute of Marine Biology" pageId="11" pageNumber="74">MIMB</collectionCode>
