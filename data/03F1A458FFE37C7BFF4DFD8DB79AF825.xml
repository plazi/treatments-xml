<quantity id="4CA0B8ABFFE37C7DFC64FD1BB259FCE6" box="[958,1045,762,787]" metricMagnitude="3" metricUnit="m" metricValue="3.3579999999999997" pageId="11" pageNumber="74" unit="m" value="3358.0">3358 m</quantity>
