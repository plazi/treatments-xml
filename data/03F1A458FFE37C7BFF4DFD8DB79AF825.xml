<number id="3D900769FFE37C7DFD0FFC88B557FC76" box="[725,795,873,898]" pageId="11" pageNumber="74" value="28869.0">28869</number>
and 
<collectionCode id="ED498D8BFFE37C7DFC88FC8BB5ECFC75" box="[850,928,874,897]" httpUri="http://grbio.org/cool/063z-3mpp" name="Museum of the Institute of Marine Biology" pageId="11" pageNumber="74">MIMB</collectionCode>
