<taxonomicName id="4C5228F46D50FFB16D88B7404937FBC6" authority="Viraktamath" authorityName="Viraktamath" box="[1176,1378,1067,1089]" class="Insecta" family="Cicadellidae" genus="Igerna" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="249" phylum="Arthropoda" rank="species" species="channa">
