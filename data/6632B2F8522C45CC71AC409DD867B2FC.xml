<geoCoordinate id="8D18BA303AAC073C8456CC24B860FF5F" direction="north" orientation="latitude" precision="1" value="33.9429">33°56'34.44&quot;N</geoCoordinate>
, 
<geoCoordinate id="33165E66607DE45860C05724C860EA27" direction="east" orientation="longitude" precision="1" value="104.121">104°07'15.59&quot;E</geoCoordinate>
