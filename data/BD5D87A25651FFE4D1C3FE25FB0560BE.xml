<collectionCode id="53E5AE715651FFE4D0E9FCF0FD926655" box="[466,549,786,810]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="77" pageNumber="77" type="Museum">FMNH</collectionCode>
