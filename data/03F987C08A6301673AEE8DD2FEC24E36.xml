<taxonomicName id="4C504D558A66016638AD8D1BFC984A5D" authorityName="Simon" authorityYear="1900" box="[724,848,1558,1579]" class="Arachnida" family="Salticidae" genus="Amphidraus" kingdom="Animalia" order="Araneae" pageId="20" pageNumber="471" phylum="Arthropoda" rank="genus">
