(
<bibRefCitation id="DD494B2AFFE67718BCB8A71C9A398F7D" author="Travassos" box="[671,855,1340,1364]" pageId="10" pageNumber="11" refString="Travassos, L. (1921) Contribucao ao conhecimento dos Cyclocoelidae Brazileros. Brazil-Medico, 1, 121 - 123." type="journal article" year="1921">
