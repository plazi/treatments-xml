<taxonomicName id="7ED84D58FFE67718BF9FA7A59BA38FB5" authority="Travassos, 1929" authorityName="Travassos" authorityYear="1929" box="[440,717,1412,1436]" class="Trematoda" family="Typhlocoelidae" genus="Neivaia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="10" pageNumber="11" phylum="Platyhelminthes" rank="genus">
