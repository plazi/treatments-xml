<bibRefCitation id="DD494B2AFFE67718BEB0A7A498368FB5" author="Travassos" box="[151,344,1412,1436]" pageId="10" pageNumber="11" refString="Travassos, L. (1929) Notas sobre Cyclocoelidae. Memorias do Instituto Oswaldo Cruz, 6 (Supplement), 54." type="journal article" year="1929">
Travassos (
<number id="0F1024FCFFE67718BF31A7A4983E8FB5" box="[278,336,1412,1436]" pageId="10" pageNumber="11" value="1929.0">1929</number>
)
</bibRefCitation>
