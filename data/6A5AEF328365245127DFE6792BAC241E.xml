<taxonomicName id="FF61E8AD90D20F15415A9B231467942A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27830" class="Insecta" family="Formicidae" genus="Cataulacus" kingdom="Animalia" lsidName-HNS="Cataulacus reticulatus Smith" order="Hymenoptera" pageNumber="81" phylum="Arthropoda" rank="species" species="reticulatus">Cataulacus reticulatus</taxonomicName>
