</taxonomicName>
(as 
<taxonomicName id="38FC4D15FFEAED200BC50B19FB32B912" box="[151,307,1308,1332]" class="Malacostraca" family="Macrophthalmidae" genus="Euplax" higherTaxonomySource="CoL" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="21" phylum="Arthropoda" rank="species" species="boscii">
