<taxonomicName id="ECD81B6F0768EF3B3106DEFC6687F2F2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32993" class="Insecta" family="Formicidae" genus="Odontomachus" kingdom="Animalia" lsidName-HNS="Odontomachus infandus Smith" order="Hymenoptera" pageId="81" pageNumber="81" phylum="Arthropoda" rank="species" species="infandus">Odontomachus infandus</taxonomicName>
