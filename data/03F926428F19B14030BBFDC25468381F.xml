<taxonomicName id="4C50ECD78F19B14330BBFDC257653B6B" authority="Broad" authorityName="Broad" box="[151,520,538,564]" class="Insecta" family="Ichneumonidae" genus="Orthocentrus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="206" phylum="Arthropoda" rank="species" species="anguillae" status="sp. nov.">
<emphasis id="B9244B468F19B14330BBFDC254DC3B6B" bold="true" box="[151,433,538,564]" italics="true" pageId="5" pageNumber="206">Orthocentrus anguillae</emphasis>
Broad
</taxonomicName>
, 
<taxonomicNameLabel id="A217F63D8F19B1433239FDC2571F3B6B" box="[533,626,538,564]" pageId="5" pageNumber="206" rank="species">sp. nov.</taxonomicNameLabel>
