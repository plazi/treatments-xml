<collectionCode id="ED1BAE52FFCCFFA5FF27FC3FFEC0C5D5" box="[192,277,980,1004]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="42" pageNumber="43">USNM</collectionCode>
