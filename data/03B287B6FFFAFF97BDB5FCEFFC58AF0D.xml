<paragraph id="8BA436A0FFFAFF96BDB5FCEFFD22AB1D" blockId="2.[151,713,834,861]" box="[151,713,834,861]" pageId="2" pageNumber="303">
<heading id="D0EC81CCFFFAFF96BDB5FCEFFD22AB1D" bold="true" box="[151,713,834,861]" fontSize="11" level="1" pageId="2" pageNumber="303" reason="1">
<emphasis id="B96FEAB2FFFAFF96BDB5FCEFFD22AB1D" bold="true" box="[151,713,834,861]" pageId="2" pageNumber="303">
Genus 
<taxonomicName id="4C1B4D23FFFAFF96BDCCFCEEFDC6AB1C" authority="Smirnov, 1971" authorityName="Smirnov" authorityYear="1971" box="[238,557,834,861]" class="Branchiopoda" family="Chydoridae" genus="Biapertura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
<emphasis id="B96FEAB2FFFAFF96BDCCFCEEFE99AB1D" bold="true" box="[238,370,835,861]" italics="true" pageId="2" pageNumber="303">Biapertura</emphasis>
<bibRefCitation id="EF8A4B51FFFAFF96BC58FCEFFDC6AB1C" author="Smirnov, N. N." box="[378,557,834,860]" pageId="2" pageNumber="303" pagination="1 - 531" refId="ref21612" refString="Smirnov, N. N. (1971) Chydoridae of the world fauna. Fauna SSSR. Rakoobraznie, 1 (2), 1 - 531. [in Russian]" type="journal article" year="1971">Smirnov, 1971</bibRefCitation>
