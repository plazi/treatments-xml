<subSubSection id="C3716549870CAA1C46A0FAD6D8D8A8E3" lastPageId="24" lastPageNumber="25" pageId="23" pageNumber="24" type="materials_examined">
<paragraph id="8BD436C2870CAA1346A0FAD6DA59AF1B" blockId="23.[189,798,1250,1342]" box="[189,313,1316,1342]" pageId="23" pageNumber="24">
<emphasis id="B91FEAD0870CAA1346A0FAD6DA59AF1B" bold="true" box="[189,313,1316,1342]" pageId="23" pageNumber="24">
<typeStatus id="54D08860870CAA1346A0FAD6DB9AAF1B" box="[189,250,1316,1342]" pageId="23" pageNumber="24">Type</typeStatus>
