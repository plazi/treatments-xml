<taxonomicName id="07D54D2DFFECB448F908F9CCFC1CF986" authorityName="Moraes et al." authorityYear="2004" baseAuthorityName="Collyer" baseAuthorityYear="1982" class="Arachnida" family="Phytoseiidae" genus="Neoseiulella" kingdom="Animalia" order="Mesostigmata" pageId="69" pageNumber="327" phylum="Arthropoda" rank="species" species="oleariae">
