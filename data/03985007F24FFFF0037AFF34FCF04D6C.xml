<taxonomicName id="4C319A92F24FFFF00300FEF4FB304F70" authority="Kirkegaard, 1996" authorityName="Kirkegaard" authorityYear="1996" box="[846,1206,280,304]" class="Polychaeta" family="Flabelligeridae" genus="Therochaeta" kingdom="Animalia" order="Terebellida" pageId="18" pageNumber="245" phylum="Annelida" rank="species" species="antoni">
