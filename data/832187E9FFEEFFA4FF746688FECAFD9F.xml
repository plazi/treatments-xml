<taxonomicName id="CC884D7CFFECFFA5FC6D658BFB24F802" baseAuthorityName="Ticehurst" baseAuthorityYear="1924" box="[910,1261,1931,1953]" class="Aves" family="Leiothrichidae" genus="Grammatoptila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="102" pageNumber="103" phylum="Chordata" rank="subSpecies" species="striata" subSpecies="sikkimensis">
