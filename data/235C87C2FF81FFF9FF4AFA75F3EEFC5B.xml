, FI= 
<quantity id="6C0D9B31FF86FFF9FC69FCD4F637FC77" box="[948,1028,877,902]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="4" pageNumber="100" unit="mm" value="3.5">3.5mm</quantity>
, TII= 
<quantity id="6C0D9B31FF86FFF9FB98FCD4F6A4FC77" box="[1093,1175,877,902]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="4" pageNumber="100" unit="mm" value="4.5">4.5mm</quantity>
, FII= 
