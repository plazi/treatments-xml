<collectionCode id="CDE4AE11FF86FFF9FF4AFC2BF2F7FC5B" box="[151,196,914,938]" pageId="4" pageNumber="100">FIII</collectionCode>
= 
<quantity id="6C0D9B31FF86FFF9FF0EFC2BF33DFC5B" box="[211,270,914,938]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="100" unit="mm" value="9.0">9mm</quantity>
, ovipositor= 
