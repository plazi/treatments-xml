, pronotum width= 
<quantity id="6C0D9B31FF86FFF9FF34FCD4F310FC77" box="[233,291,877,902]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" pageId="4" pageNumber="100" unit="mm" value="3.0">3mm</quantity>
, FW length= 
<quantity id="6C0D9B31FF86FFF9FE64FCD4F039FC77" box="[441,522,877,902]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="4" pageNumber="100" unit="mm" value="5.5">5.5mm</quantity>
