<taxonomicName id="4C1FF3B4FFE4FFE628FAD1FC430EFA12" box="[444,547,1428,1449]" class="Insecta" family="Miridae" genus="Fulvius" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="591" phylum="Arthropoda" rank="species" species="nozawai">
<emphasis id="B96B5425FFE4FFE628FAD1FC430EFA12" box="[444,547,1428,1449]" italics="true" pageId="3" pageNumber="591">F. nozawai</emphasis>
</taxonomicName>
. 
