<paragraph id="879FDCC6F67FE508DAF9E9397B78B3C0" pageId="18" pageNumber="491">
<taxonomicName id="591FA674B0A25B7F64648F67F2C7CEDF" class="Arachnida" family="Crotoniidae" genus="Camisia" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="18" pageNumber="491" phylum="Arthropoda" rank="species" species="segnis">Camisia segnis (Hermann)</taxonomicName>
