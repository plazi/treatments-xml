<taxonomicName id="2BA1C2F29837FFFB50A15F79FDC87641" box="[473,621,1637,1661]" class="Insecta" family="Noteridae" genus="Canthysellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="487" phylum="Arthropoda" rank="genus">
