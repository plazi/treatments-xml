<geoCoordinate id="25F00F9AFFE2FFB5FF1BCD92DE7EFA63" box="[245,349,1485,1511]" degrees="44" direction="north" minutes="28" orientation="latitude" pageId="5" pageNumber="31" precision="925" value="44.466667">44°28’N</geoCoordinate>
