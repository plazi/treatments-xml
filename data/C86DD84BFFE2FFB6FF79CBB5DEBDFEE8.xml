<geoCoordinate id="25F00F9AFFE2FFB5FE87CD92DEE3FA63" box="[361,448,1485,1511]" degrees="5" direction="east" minutes="25" orientation="longitude" pageId="5" pageNumber="31" precision="925" value="5.4166665">5°25’E</geoCoordinate>
