<emphasis id="B9514B34B358805F4084FAB682771E79" bold="true" box="[151,298,1320,1346]" italics="true" pageId="4" pageNumber="39">Homoneura</emphasis>
(
<emphasis id="B9514B34B358805F4129FAB682901E79" bold="true" box="[314,461,1320,1346]" italics="true" pageId="4" pageNumber="39">Homoneura</emphasis>
) 
<emphasis id="B9514B34B358805F41CDFAB6814D1E79" bold="true" box="[478,528,1320,1346]" italics="true" pageId="4" pageNumber="39">caoi</emphasis>
Wang 
<emphasis id="B9514B34B358805F427BFAB681201E79" bold="true" box="[616,637,1320,1346]" italics="true" pageId="4" pageNumber="39">et</emphasis>
Yang
</taxonomicName>
