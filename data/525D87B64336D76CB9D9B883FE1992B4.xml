<taxonomicName id="1DF44D234336D76BB8ACB8C6FB1C9A57" authority="Ventrillon, Bull. Mus. Paris" authorityName="Ventrillon, Bull. Mus. Paris" box="[414,1193,2266,2312]" class="Insecta" family="Culicidae" genus="Culex" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="6" phylum="Arthropoda" rank="species" species="giganteus">
