<taxonomicName id="80C44D3FE873D273FDB8F9EC0FF0F9FE" authority="d'Orbigny" authorityName="d'Orbigny" authorityYear="1852" box="[594,694,1634,1655]" pageId="61" pageNumber="62" rank="species" species="angustata">
