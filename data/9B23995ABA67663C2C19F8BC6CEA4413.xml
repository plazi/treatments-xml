<taxonomicName id="3658DEB09E98119C8EE389C75971817F" class="Insecta" family="Dolichopodidae" genus="Xanthochlorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xanthochlorus tibetensis" order="Diptera" pageId="7" pageNumber="148" phylum="Arthropoda" rank="species" species="tibetensis">Xanthochlorus tibetensis</taxonomicName>
