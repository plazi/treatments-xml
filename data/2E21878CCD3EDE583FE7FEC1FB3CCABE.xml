<taxonomicName id="61884D19CD3EDE5838C5FB27FAE7CBB3" baseAuthorityName="E. A. Smith" baseAuthorityYear="1885" box="[1197,1285,1195,1216]" class="Bivalvia" family="Propeamussiidae" genus="Propeamussium" kingdom="Animalia" order="Pectinoida" pageId="14" pageNumber="415" phylum="Mollusca" rank="species" species="watsoni">
