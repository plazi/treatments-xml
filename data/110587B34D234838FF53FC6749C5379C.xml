<emphasis id="ABD8EAB74D224838FF03FE644A4A34B5" bold="true" box="[199,315,403,429]" pageId="41" pageNumber="42">Remarks.</emphasis>
<taxonomicName id="5EAC4D264D224838FE85FE64490A34B5" baseAuthorityName="Duchassaing" baseAuthorityYear="1850" box="[321,635,403,429]" class="Demospongiae" family="Petrosiidae" genus="Neopetrosia" kingdom="Animalia" order="Haplosclerida" pageId="41" pageNumber="42" phylum="Porifera" rank="species" species="subtriangularis">
