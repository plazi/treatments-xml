<caption id="C6BF662621111F03FF3963DBFB39C4D2" ID-DOI="http://doi.org/10.5281/zenodo.5535955" ID-Zenodo-Dep="5535955" httpUri="https://zenodo.org/record/5535955/files/figure.png" pageId="17" pageNumber="18" startId="17.[189,232,1963,1989]" targetBox="[219,1373,347,1870]" targetPageId="17">
