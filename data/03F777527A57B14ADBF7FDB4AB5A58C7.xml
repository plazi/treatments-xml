at 
<quantity id="4CA66BA17A54B14AD9D8FF7BAB0658C7" box="[696,809,187,213]" metricMagnitude="1" metricUnit="m" metricValue="6.2" metricValueMax="10.2" metricValueMin="2.2" pageId="23" pageNumber="128" unit="m" value="62.0" valueMax="102.0" valueMin="22.0">22–102 m</quantity>
