<taxonomicName id="03FD4D09B60DCD655B85F8B9FCDFA332" authorityName="Boulard" authorityYear="1986" box="[684,810,1881,1907]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="lichiana">
<emphasis id="F689EA98B60DCD655B85F8B9FCDFA332" box="[684,810,1881,1907]" italics="true" pageId="80" pageNumber="81">C. lichiana</emphasis>
</taxonomicName>
with body lengths less than 
<quantity id="03059B6FB60DCD655D5BF8B9FB36A332" box="[1138,1219,1881,1907]" metricMagnitude="-2" metricUnit="m" metricValue="2.4" pageId="80" pageNumber="81" unit="mm" value="24.0">24 mm</quantity>
(although some 
<taxonomicName id="03FD4D09B60DCD655CAAF8B9FF06A3D6" authorityName="Boulard" authorityYear="1986" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="lichiana">
<emphasis id="F689EA98B60DCD655CAAF8B9FF06A3D6" italics="true" pageId="80" pageNumber="81">C. lichiana</emphasis>
</taxonomicName>
