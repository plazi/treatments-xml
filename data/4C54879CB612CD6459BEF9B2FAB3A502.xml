<taxonomicName id="03FD4D09B60DCD655AB2F89DFBCBA3D6" authorityName="Boulard" authorityYear="1986" box="[923,1086,1917,1943]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="aratayensis">
<emphasis id="F689EA98B60DCD655AB2F89DFBCBA3D6" box="[923,1086,1917,1943]" italics="true" pageId="80" pageNumber="81">C. aratayensis</emphasis>
</taxonomicName>
, 
