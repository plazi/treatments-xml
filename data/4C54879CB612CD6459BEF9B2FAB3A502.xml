<taxonomicName id="03FD4D09B60DCD655D28F8D5FAFEA30E" authority="Sanborn, 2020" authorityName="Sanborn" authorityYear="2020" box="[1025,1291,1845,1871]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="rumipataensis" status="n. sp.">
<emphasis id="F689EA98B60DCD655D28F8D5FAFEA30E" box="[1025,1291,1845,1871]" italics="true" pageId="80" pageNumber="81">Carineta rumipataensis</emphasis>
</taxonomicName>
