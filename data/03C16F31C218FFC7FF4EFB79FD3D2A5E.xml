<taxonomicName id="4C68A5A4C21EFFC7FCF6FD07FC492A16" authorityName="Frey" authorityYear="1924" box="[815,947,765,791]" class="Insecta" family="Sciomyzidae" genus="Tetanocera" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="92" phylum="Arthropoda" rank="species" species="latifibula">
<emphasis id="B91C0235C21EFFC7FCF6FD07FC492A16" box="[815,947,765,791]" italics="true" pageId="21" pageNumber="92">T. latifibula</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C68A5A4C21EFFC7FC33FD07FB7F2A16" authorityName="Rondani" authorityYear="1868" box="[1002,1157,765,791]" class="Insecta" family="Sciomyzidae" genus="Tetanocera" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="92" phylum="Arthropoda" rank="species" species="punctifrons">
