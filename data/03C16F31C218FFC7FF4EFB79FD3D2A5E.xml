<taxonomicName id="4C68A5A4C21EFFC7FB39FCDBFAA72A3A" authorityName="Dumeril" authorityYear="1800" box="[1248,1373,802,827]" class="Insecta" family="Sciomyzidae" genus="Tetanocera" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="92" phylum="Arthropoda" rank="genus">
<emphasis id="B91C0235C21EFFC7FB39FCDBFAA72A3A" box="[1248,1373,802,827]" italics="true" pageId="21" pageNumber="92">Tetanocera</emphasis>
