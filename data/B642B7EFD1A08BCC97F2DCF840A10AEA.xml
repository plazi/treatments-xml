<paragraph id="37255944762031B92D47DC9AE7283B74" pageId="15" pageNumber="16">
<taxonomicName id="5FF89BEF55BBE8F1728A3D7E0FAE35AC" class="Collembola" family="Onychiuridae" genus="Onychiurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Onychiurus flavescens" order="Poduromorpha" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="flavescens">Onychiurus flavescens</taxonomicName>
