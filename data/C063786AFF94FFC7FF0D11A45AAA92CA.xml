<number id="FE02DB5BFF94FFC7FAFC13DE5AB99645" box="[1382,1432,749,773]" pageId="58" pageNumber="59" value="0.43">0.43</number>
; pronotum 
<number id="FE02DB5BFF94FFC7FE9712215E1F966A" box="[269,318,786,810]" pageId="58" pageNumber="59" value="0.43">0.43</number>
