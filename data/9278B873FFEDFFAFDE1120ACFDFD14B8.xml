<collectingRegion id="D815C787FFEAFFAFDC3A25E4FE9E15C3" box="[257,374,736,760]" country="Argentina" name="Corrientes" pageId="20" pageNumber="435">Corrientes</collectingRegion>
, 
<collectorName id="B7246CB3FFEAFFAFDCBA25E4FE3715C3" box="[385,479,736,760]" pageId="20" pageNumber="435">La Cruz</collectorName>
, Paraje Tres Cerros; 
<geoCoordinate id="7FE56FA2FFEAFFAFDFFF25DAFF361422" degrees="29" direction="south" minutes="06" orientation="latitude" pageId="20" pageNumber="435" precision="15" seconds="32" value="-29.108889">29°06′ 32′′S</geoCoordinate>
, 
<geoCoordinate id="7FE56FA2FFEAFFAFDDD02404FE931422" box="[235,379,768,793]" degrees="056" direction="west" minutes="55" orientation="longitude" pageId="20" pageNumber="435" precision="15" seconds="39" value="-56.9275">056°55′39′′W</geoCoordinate>
