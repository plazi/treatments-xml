<taxonomicName id="4C7A4D3BC255FFF7F08AFA11FC435278" authority="K. O. Viets, 1977" authorityName="K. O. Viets" authorityYear="1977" box="[509,988,1533,1557]" class="Arachnida" family="Torrenticolidae" genus="Pseudotorrenticola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="259" phylum="Arthropoda" rank="species" species="boettgeri">
