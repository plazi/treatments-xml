</taxonomicName>
from 
<collectingRegion id="49BEF85AC25AFFF8F10AFF65FF4754CC" box="[125,216,137,161]" country="United States of America" name="Arizona" pageId="8" pageNumber="260">Arizona</collectingRegion>
, 
<collectingCountry id="F36D7628C25AFFF8F194FF65FE8454CC" box="[227,283,137,161]" name="United States of America" pageId="8" pageNumber="260">USA</collectingCountry>
