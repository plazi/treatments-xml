<taxonomicName id="E6EE81E12971FFDDC81F9471FC02C05D" authority="Lundblad, 1941" authorityName="Lundblad" authorityYear="1941" box="[587,975,571,599]" class="Arachnida" family="Hygrobatidae" genus="Aspidiobates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="13" pageNumber="915" phylum="Arthropoda" rank="genus">
