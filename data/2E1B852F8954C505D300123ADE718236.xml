<taxonomicName id="A637D614A24EAC0499EB44BDF18A6DB9" LSID="urn:lsid:zoobank.org:act:88F4D2D6-C9B6-4B74-9CC4-472673FC4EBE" authority="Yin &amp; Li" class="Insecta" family="Staphylinidae" genus="Labomimus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labomimus sarculus" order="Coleoptera" pageId="5" pageNumber="88" phylum="Arthropoda" rank="species" species="sarculus">Labomimus sarculus Yin &amp; Li</taxonomicName>
