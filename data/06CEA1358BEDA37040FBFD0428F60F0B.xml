<paragraph id="B36468BD742F26AFF1AB3376172CDCD4" pageId="1" pageNumber="58">
<taxonomicName id="AD8E910A5E8E821177B8480EC20B05D1" authority="Proszynski, 1976" authorityName="Proszynski" authorityYear="1976" class="Arachnida" family="Salticidae" genus="Yaginumaella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Yaginumaella" order="Araneae" pageId="1" pageNumber="58" phylum="Arthropoda" rank="genus">
Yaginumaella 
<normalizedToken id="B1BE6A8CB493E508753FA3CAB7309A5C" originalValue="Prószyński">Proszynski</normalizedToken>
, 1976
</taxonomicName>
