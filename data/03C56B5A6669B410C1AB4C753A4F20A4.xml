<taxonomicName id="4C6CA1CF6668B410C3884AC93B262081" baseAuthorityName="Dieckmann" baseAuthorityYear="1977" box="[692,828,439,464]" class="Magnoliopsida" family="Malvaceae" genus="Malva" kingdom="Plantae" order="Malvales" pageId="61" pageNumber="62" phylum="Tracheophyta" rank="species" species="sylvestris">
