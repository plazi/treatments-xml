</taxonomicName>
, 
<taxonomicName id="4C6CA1CF6668B410C2724AC93CA42081" authority="(Dieckmann 1977)" baseAuthorityName="Dieckmann" baseAuthorityYear="1977" box="[846,1214,439,464]" class="Magnoliopsida" family="Fabaceae" genus="Lathyrus" kingdom="Animalia" order="Fabales" pageId="61" pageNumber="62" phylum="Tracheophyta" rank="species" species="tuberosus">
