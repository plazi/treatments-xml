<taxonomicName id="A98311086620F0BEAD799F5855FBE21D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25498" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" lsidName-HNS="Aphaenogaster sardoa Mayr" order="Hymenoptera" pageNumber="71" phylum="Arthropoda" rank="species" species="sardoa">A. sardoa</taxonomicName>
