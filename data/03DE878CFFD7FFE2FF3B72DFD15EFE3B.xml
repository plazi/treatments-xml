<taxonomicName id="4C774D19FFC9FFE1FD1C775FD339F8E7" authorityName="Distant" authorityYear="1883" box="[688,809,1807,1833]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="apicalis">
