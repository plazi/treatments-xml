series collected 
<location id="8EA86041FFCAFFE2FBB771E7D4B7FE1F" LSID="urn:lsid:plazi:treatment:03DE878CFFD7FFE2FF3B72DFD15EFE3B:8EA86041FFCAFFE2FBB771E7D4B7FE1F" box="[1051,1191,439,465]" country="Ecuador" name="Balsapamba" pageId="50" pageNumber="51" stateProvince="Bolivar">Balsapamba</location>
, 
