<taxonomicName id="4C774D19FFC9FFE1FD8D7793D36DF813" authority="Distant, 1883 a" authorityName="Distant" authorityYear="1883" box="[545,893,1987,2013]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="cinara">
