<taxonomicName id="4C774D19FFC9FFE1FCA977CFD446F877" authority="Boulard, 1986 b" authorityName="Boulard" authorityYear="1986" box="[773,1110,1951,1977]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="imperfecta">
