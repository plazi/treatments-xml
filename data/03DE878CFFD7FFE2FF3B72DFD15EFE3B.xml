<taxonomicName id="4C774D19FFC9FFE1FEF676BBD238F8CB" authority="Sanborn, 2020" authorityName="Sanborn" authorityYear="2020" box="[346,552,1771,1797]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="coronida" status="n. sp.">
<emphasis id="B903EA88FFC9FFE1FEF676BBD238F8CB" box="[346,552,1771,1797]" italics="true" pageId="49" pageNumber="50">Carineta coronida</emphasis>
</taxonomicName>
<emphasis id="B903EA88FFC9FFE1FD8376BBD27DF8CB" bold="true" box="[559,621,1771,1797]" pageId="49" pageNumber="50">
<taxonomicNameLabel id="A23057F3FFC9FFE1FD8376BBD27DF8CB" box="[559,621,1771,1797]" pageId="49" pageNumber="50" rank="species">n. sp.</taxonomicNameLabel>
