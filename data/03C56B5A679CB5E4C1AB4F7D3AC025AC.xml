<taxonomicName id="4C6CA1CF679CB5E4C0D44FEC3A9325E4" baseAuthorityName="Mazur" baseAuthorityYear="2002" box="[488,649,1180,1205]" class="Magnoliopsida" family="Tamaricaceae" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Caryophyllales" pageId="457" pageNumber="458" phylum="Tracheophyta" rank="family">Tamaricaceae</taxonomicName>
