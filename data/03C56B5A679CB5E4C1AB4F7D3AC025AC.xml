<taxonomicName id="4C6CA1CF679CB5E4C06B4FED39AE25E5" box="[343,436,1181,1204]" class="Magnoliopsida" family="Tamaricaceae" genus="Tamarix" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Caryophyllales" pageId="457" pageNumber="458" phylum="Tracheophyta" rank="genus">
