</taxonomicName>
, 
<taxonomicName id="3CC9BEFDFFB1FF91FC65A917FB06FE8E" authorityName=": Weld" authorityYear="1922" box="[1014,1161,258,284]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="35" phylum="Arthropoda" rank="species" species="citriformis">
S. 
<emphasis id="C9BD196CFFB1FF91FB80A917FB06FE8E" box="[1043,1161,258,284]" italics="true" pageId="22" pageNumber="23">citriformis</emphasis>
</taxonomicName>
and 
<taxonomicName id="3CC9BEFDFFB1FF91FB2DA916FAE6FE8E" authorityName="Pujade-Villar &amp; Lobato-Vila" authorityYear="2017" box="[1214,1385,258,285]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="longimalaris">
<emphasis id="C9BD196CFFB1FF91FB2DA916FAE6FE8E" box="[1214,1385,258,285]" italics="true" pageId="22" pageNumber="23">S. longimalaris</emphasis>
</taxonomicName>
