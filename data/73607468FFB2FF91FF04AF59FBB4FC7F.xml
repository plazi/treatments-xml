<taxonomicName id="3CC9BEFDFFB1FF91FCE7A916FC66FE8E" authorityName="Pujade-Villar &amp; Lobato-Vila" authorityYear="2017" box="[884,1001,258,285]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="cibriani">
