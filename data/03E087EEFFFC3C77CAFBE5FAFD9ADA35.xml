<subSubSection id="C3536573FFFD3C77CAFAE6FDFD9ADA35" pageId="7" pageNumber="8" type="etymology">
<paragraph id="8BF636F8FFFD3C77CAFAE6FDFE96DB85" blockId="7.[145,276,1700,1724]" box="[145,276,1700,1724]" pageId="7" pageNumber="8">
<heading id="D0BE8194FFFD3C77CAFAE6FDFE96DB85" bold="true" box="[145,276,1700,1724]" fontSize="10" level="3" pageId="7" pageNumber="8" reason="2">
