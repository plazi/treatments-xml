<taxonomicName id="4C424D0DFFE1A70598D2F391F0298BC2" authority="Niedbała, 1994" authorityName="Niedbała" authorityYear="1994" box="[655,803,1680,1704]" class="Arachnida" family="Phthiracaridae" genus="Arphthicarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="529" phylum="Arthropoda" rank="genus">
