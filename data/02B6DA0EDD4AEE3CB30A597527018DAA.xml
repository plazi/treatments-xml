<paragraph id="1C011A6787345530C52535A7BEE80B4D" pageId="30" pageNumber="45">
<taxonomicName id="93E01B097643B190BAACEDC95EC99B50" class="? insecta" family="? curculionidae" genus="Listroderes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="? coleoptera" pageId="30" pageNumber="45" phylum="Arthropoda" rank="species" species="dentipennis">
