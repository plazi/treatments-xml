<elevation id="F283D1A9FFC1FFDCFBA0FEB1FB168D16" box="[1035,1166,283,307]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="2.0" metricValueMin="1.5" pageId="4" pageNumber="636" unit="m" value="17.5" valueMax="20.0" valueMin="15.0">15 to 20 m</elevation>
