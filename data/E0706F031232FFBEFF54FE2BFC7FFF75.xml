<quantity id="AF2173F01232FFBFFBC3F9AAFB9FF9B7" box="[1024,1082,1627,1651]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="72" pageNumber="73" unit="m" value="30.0">30 m</quantity>
anche d'Algier 
<quantity id="AF2173F01232FFBFFB2DF9AAFA8CF9B7" box="[1262,1321,1627,1651]" metricMagnitude="1" metricUnit="m" metricValue="4.0" pageId="72" pageNumber="73" unit="m" value="40.0">40 m</quantity>
Joly” and “ 
<taxonomicName id="AFD9A5961232FFBFFF62F98FFE5BF952" authority=", Jeffr. Palermo" authorityName="Jeffr. Palermo" box="[161,510,1662,1687]" class="Gastropoda" family="Philinidae" genus="Philine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="72" pageNumber="73" phylum="Mollusca" rank="species" species="striatula">Philine striatula, Jeffr. Palermo</taxonomicName>
