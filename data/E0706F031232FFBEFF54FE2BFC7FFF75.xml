<collectionCode id="0EC846D01232FFBFFE8FFB93FE07FBBE" box="[332,418,1122,1147]" country="Italy" httpUri="http://grbio.org/cool/mmbj-pfva" name="Museo Civico di Zoologia" pageId="72" pageNumber="73">MCZR</collectionCode>
