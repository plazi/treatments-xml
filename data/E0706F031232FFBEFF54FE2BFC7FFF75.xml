<collectionCode id="0EC846D01232FFBFFE8CF9C7FE00F98A" box="[335,421,1590,1615]" country="Italy" httpUri="http://grbio.org/cool/mmbj-pfva" name="Museo Civico di Zoologia" pageId="72" pageNumber="73">MCZR</collectionCode>
