<subSubSection id="7AE21058B06B14A21A5FF5DFA609CD09" pageId="1" pageNumber="62" type="nomenclature">
<paragraph id="5DB1E39E1652A65EEF94126C5C17D44B" pageId="1" pageNumber="62">
<taxonomicName id="CFA2307DF822BAFF5C1774EB47E8D00C" LSID="5183A745-67CF-44C2-1438-95C972AC47B6" authority="Byng, V. Florens &amp; Baider" authorityName="Byng, V. Florens &amp; Baider" authorityYear="2015" class="Magnoliopsida" family="Myrtaceae" genus="Syzygium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Syzygium pyneei" order="Myrtales" pageId="1" pageNumber="62" phylum="Tracheophyta" rank="species" species="pyneei" status="sp. nov.">Syzygium pyneei Byng, V. Florens &amp; Baider</taxonomicName>
