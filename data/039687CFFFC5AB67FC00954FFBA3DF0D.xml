<collectionCode id="ED2EAE1CFFC5AB67FB5795E3FAA9DA98" box="[1246,1326,986,1007]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="5" pageNumber="590" type="Museum">USNM</collectionCode>
