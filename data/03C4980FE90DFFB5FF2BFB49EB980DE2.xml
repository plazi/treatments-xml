<number id="3DA53B3EE90FFFB5FACEFD78EA8F0992" box="[1394,1436,692,716]" pageId="3" pageNumber="481" value="5.5" valueMax="8.0" valueMin="3.0">3–8</number>
dorsolateral short spines (some vestigial); with 
<number id="3DA53B3EE90FFFB5FD7CFD1BEDFA09B1" box="[704,745,727,751]" pageId="3" pageNumber="481" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
dorsolateral bristlelike short spines; with 
<number id="3DA53B3EE90FFFB5FB6AFD1BEBF709B1" box="[1238,1252,727,751]" pageId="3" pageNumber="481" value="3.0">3</number>
long bristlelike postspiracular spines; and with 
<number id="3DA53B3EE90FFFB5FE44FD30ED31084A" box="[504,546,764,788]" pageId="3" pageNumber="481" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
