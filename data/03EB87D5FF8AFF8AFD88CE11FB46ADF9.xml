<collectingRegion id="4986F821FF8AFF8AFC78C965FB83AE77" box="[956,1073,1329,1353]" country="United States of America" name="Louisiana" pageId="15" pageNumber="1173">Louisiana</collectingRegion>
. 
<taxonomicName id="4C424D40FF8AFF8AFBFEC965FE2AAE57" authorityName=": Hoffman" authorityYear="1999" class="Diplopoda" family="Pyrgodesmidae" genus="Myrmecodesmus" kingdom="Animalia" order="Polydesmida" pageId="15" pageNumber="1173" phylum="Arthropoda" rank="species" species="digitatus">
<emphasis id="B936EAD1FF8AFF8AFBFEC965FE2AAE57" italics="true" pageId="15" pageNumber="1173">Myrmecodesmus digitatus</emphasis>
