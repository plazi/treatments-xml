<collectingRegion id="49D1F857FF801E6AFE7ADF99A267FD26" box="[472,572,698,720]" country="Spain" name="Catalunya" pageId="17" pageNumber="1758">Catalonia</collectingRegion>
, Tibidabo), 
<collectingCountry id="F3027625FF801E6AFD1FDF99A2A5FD26" box="[701,766,698,720]" name="Serbia" pageId="17" pageNumber="1758">Serbia</collectingCountry>
