<collectingCountry id="F3027625FF801E6AFE38DFBEA1AEFD44" box="[410,501,669,690]" name="Hungary" pageId="17" pageNumber="1758">Hungary</collectingCountry>
, 
<collectingCountry id="F3027625FF801E6AFDA2DFBEA20FFD45" box="[512,596,669,691]" name="Ukraine" pageId="17" pageNumber="1758">Ukraine</collectingCountry>
, 
<collectingCountry id="F3027625FF801E6AFDFDDFBEA28EFD45" box="[607,725,669,691]" name="Kazakhstan" pageId="17" pageNumber="1758">Kazakhstan</collectingCountry>
