<taxonomicName id="FDE0848EFFA9B25BF719FA78FB78A0C7" authority="Fabricius, 1780" authorityName="Fabricius" authorityYear="1780" box="[848,1180,1460,1485]" class="Demospongiae" family="Spongiidae" genus="Spongia" kingdom="Animalia" order="Dictyoceratida" pageId="18" pageNumber="221" phylum="Porifera" rank="species" species="compressa">
