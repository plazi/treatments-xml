<taxonomicName id="C50C584C710E4BE3075F806FBBEDD7A5" authority="Elsen, 1972" authorityName="Elsen" authorityYear="1972" class="Arachnida" family="Ameroseiidae" genus="Afrocypholaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Afrocypholaelaps" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Afrocypholaelaps</taxonomicName>
