<collectingCountry id="F32CD9ACFFF0FFF2AF3A5FA01896FF3A" box="[364,423,152,176]" name="United States of America" pageId="22" pageNumber="23">USA</collectingCountry>
, 
<collectingRegion id="49FF57DEFFF0FFF2AFE45FAF1B35FF3A" box="[434,516,151,176]" country="United States of America" name="Alaska" pageId="22" pageNumber="23">Alaska</collectingRegion>
: 
<collectingMunicipality id="6BE00346FFF0FFF2AC585FAF1BE7FF3A" box="[526,726,151,176]" pageId="22" pageNumber="23">Stephens Passage</collectingMunicipality>
, 
<location id="8EE4CFE7FFF0FFF2ACB75FAF1A4EFF3A" LSID="urn:lsid:plazi:treatment:0392282AFFF3FFF2AEC1592C1819FC2F:8EE4CFE7FFF0FFF2ACB75FAF1A4EFF3A" box="[737,895,151,176]" country="United States of America" municipality="Stephens Passage" name="Thistle Ledge" pageId="22" pageNumber="23" stateProvince="Alaska">Thistle Ledge</location>
, 131 fm (
<quantity id="4CC334D9FFF0FFF2ADBF5FA01D03FF3A" box="[1001,1074,152,176]" metricMagnitude="2" metricUnit="m" metricValue="2.4" pageId="22" pageNumber="23" unit="m" value="240.0">
<elevation id="00167E0FFFF0FFF2ADBF5FA01D03FF3A" box="[1001,1074,152,176]" metricMagnitude="2" metricUnit="m" metricValue="2.4" pageId="22" pageNumber="23" unit="m" value="240.0">240 m</elevation>
