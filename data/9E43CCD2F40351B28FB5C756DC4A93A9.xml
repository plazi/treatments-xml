<taxonomicName id="F9C7A091C4FD6E8AE33E699418D3462E" class="Magnoliopsida" family="Asteraceae" genus="Doronicum" kingdom="Plantae" lsidName="Doronicum austriacum" order="Asterales" pageId="0" pageNumber="69351" phylum="Tracheophyta" rank="species" species="austriacum">
<emphasis id="F24718543EABAD68FF76E33E9F758369" italics="true" pageId="0" pageNumber="69351">Doronicum austriacum</emphasis>
