<taxonomicName id="DC7845EBBA5174AAF8692FA3E36D82E5" authorityName="Krombein" authorityYear="1983" class="Insecta" family="Chrysididae" genus="Imasega" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Imasega rufithorax" order="Hymenoptera" pageId="1" pageNumber="72" phylum="Arthropoda" rank="species" species="rufithorax">Imasega rufithorax</taxonomicName>
