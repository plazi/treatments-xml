<paragraph id="8BB536E0D97E5D0906F4FA0E40DAF9E0" blockId="5.[151,1265,1514,1537]" box="[151,1265,1514,1537]" pageId="5" pageNumber="6">
<emphasis id="B97EEAF2D97E5D0906F4FA0E4539F9E0" bold="true" box="[151,274,1514,1537]" pageId="5" pageNumber="6">FIGURE 3.</emphasis>
