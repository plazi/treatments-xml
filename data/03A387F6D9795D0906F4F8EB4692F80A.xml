<emphasis id="B97EEAF2D9785D0F0222F8E54059F8F8" bold="true" box="[1089,1138,1793,1816]" pageId="3" pageNumber="4">C–D</emphasis>
<taxonomicName id="4C0A4D63D9785D0F0219F8E640F4F8F8" box="[1146,1247,1794,1816]" class="Insecta" family="Miridae" genus="Artchawakomius" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="moteus">
