<taxonomicName id="4C0A4D63D97E5D090708F9A246E1F9BF" authority="Yasunaga" authorityName="Yasunaga" box="[363,714,1606,1631]" class="Insecta" family="Miridae" genus="Archawakomius" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="pius" status="new species">
