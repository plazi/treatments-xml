<taxonomicName id="4C2B4D40FFF2FFBF145CC33D2A563863" box="[1079,1384,821,846]" class="Pycnogonida" family="Phoxichilidiidae" genus="Anoplodactylus" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="angulatus">
