<quantity id="4CF91CE37730FFCBFBC4A24394F9FEDA" box="[1095,1156,296,320]" metricMagnitude="1" metricUnit="m" metricValue="9.8" pageId="103" pageNumber="104" unit="m" value="98.0">98 m</quantity>
