<taxonomicName id="4C7CCBD9FFF1FFF4FDBBCE6C3DD7665A" baseAuthorityName="Harold" baseAuthorityYear="1874" box="[613,756,809,835]" class="Insecta" family="Aphodiidae" genus="Haroldiellus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="lansbergei">
