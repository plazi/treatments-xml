<emphasis id="519CC5531F77FF8CFF53F9ADFED1355A" box="[201,379,1612,1629]" italics="true" pageId="60" pageNumber="339">Mangenotiella stellata</emphasis>
M.Schmid
</taxonomicName>
, 
<taxonomicNameLabel id="4AAF78281F77FF8CFE44F9ADFD8A355A" box="[478,544,1612,1629]" pageId="60" pageNumber="339" rank="species">sp.nov.</taxonomicNameLabel>
typica: 
<emphasis id="519CC5531F77FF8CFDF9F9ADFDDA355A" bold="true" box="[611,624,1612,1629]" pageId="60" pageNumber="339">A</emphasis>
, rameau florifère; 
<emphasis id="519CC5531F77FF8CFC9CF9ADFCB9355A" bold="true" box="[774,787,1612,1629]" pageId="60" pageNumber="339">B</emphasis>
, feuille; 
<emphasis id="519CC5531F77FF8CFCC3F9ADFCCD355A" bold="true" box="[857,871,1612,1629]" pageId="60" pageNumber="339">C</emphasis>
, 
<emphasis id="519CC5531F77FF8CFCEAF9ADFCD4355A" bold="true" box="[880,894,1612,1629]" pageId="60" pageNumber="339">D</emphasis>
, boutons floraux; 
<emphasis id="519CC5531F77FF8CFB8EF9ADFB8A355A" bold="true" box="[1044,1056,1612,1629]" pageId="60" pageNumber="339">E</emphasis>
, calice; 
<emphasis id="519CC5531F77FF8CFBFCF9ADFBDB355A" bold="true" box="[1126,1137,1612,1629]" pageId="60" pageNumber="339">F</emphasis>
, corolle; 
<emphasis id="519CC5531F77FF8CFFEBF982FFD53573" bold="true" box="[113,127,1635,1652]" pageId="60" pageNumber="339">G</emphasis>
, 
<emphasis id="519CC5531F77FF8CFF11F982FF333573" bold="true" box="[139,153,1635,1652]" pageId="60" pageNumber="339">H</emphasis>
, fleurs épanouies; 
<emphasis id="519CC5531F77FF8CFEA1F982FEEB3573" bold="true" box="[315,321,1635,1652]" pageId="60" pageNumber="339">I</emphasis>
