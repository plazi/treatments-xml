<taxonomicName id="4C524D05FFB2FFAC42156783691DFCBB" authority="Obenberger, 1924" authorityName="Obenberger" authorityYear="1924" baseAuthorityName="Obenberger" baseAuthorityYear="1924" class="Insecta" family="Aphididae" genus="Schoutedenia" kingdom="Animalia" order="Aphidomorpha" pageId="8" pageNumber="13" phylum="Arthropoda" rank="species" species="kerremansi">
<emphasis id="B926EA94FFB2FFAC421567836A40FCBB" italics="true" pageId="8" pageNumber="13">Schoutedenia kerremansi</emphasis>
Obenberger, 1924
</taxonomicName>
(= 
<taxonomicName id="4C524D05FFB2FFAC434367A368CBFCBB" authority="Waterhouse, 1875" authorityName="Waterhouse" authorityYear="1875" box="[566,967,761,785]" class="Insecta" family="Buprestidae" genus="Ptosima" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="13" phylum="Arthropoda" rank="species" species="magnifica">
