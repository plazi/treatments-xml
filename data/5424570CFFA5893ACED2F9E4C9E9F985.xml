<taxonomicName id="1B8D9D99FFA5893BCD40F8F9C97EF8E7" box="[773,882,1805,1830]" class="Insecta" family="Scirtidae" genus="Austrocyphon" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="papilio">
