<taxonomicName id="4C374D2367031374FAB0FF27FA6EE480" box="[1328,1429,192,217]" class="Oligohymenophorea" family="Epistylididae" genus="Epistylis" kingdom="Protozoa" order="Peritrichida" pageId="2" pageNumber="37" phylum="Ciliophora" rank="genus">
