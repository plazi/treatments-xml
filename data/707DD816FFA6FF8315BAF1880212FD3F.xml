<emphasis id="CAA0B512FFA6FF8215BAF188010EFC0C" bold="true" box="[151,521,951,977]" pageId="40" pageNumber="41">
<emphasis id="CAA0B512FFA6FF8215BAF188022DFC0C" bold="true" box="[151,298,951,977]" italics="true" pageId="40" pageNumber="41">
<taxonomicName id="3FD41283FFA6FF8215BAF188020EFC0C" authorityName="sensu Franclemont and Todd" authorityYear="1983" box="[151,265,951,977]" class="Insecta" family="Noctuidae" genus="Sympistis" kingdom="Animalia" order="Lepidoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus">Sympistis</taxonomicName>
