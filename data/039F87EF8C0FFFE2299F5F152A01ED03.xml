<subSubSection id="C32C65728C0FFFE229CF5FF42A01ED03" pageId="0" pageNumber="145" type="etymology">
<paragraph id="8B8936F98C0FFFE229CF5FF42BB2ED5E" blockId="0.[151,1436,970,1361]" pageId="0" pageNumber="145">
<emphasis id="B942EAEB8C0FFFE229CF5FF42A13ECBB" bold="true" box="[199,327,1234,1257]" pageId="0" pageNumber="145">Etymology.</emphasis>
