<caption id="47A066540F52FFE46C92FB763F3F11D7" ID-DOI="http://doi.org/10.5281/zenodo.4673176" ID-Zenodo-Dep="4673176" httpUri="https://zenodo.org/record/4673176/files/figure.png" pageId="7" pageNumber="6" startId="7.[192,268,1118,1141]" targetBox="[191,1439,191,1075]" targetPageId="7">
<paragraph id="136036DC0F52FFE46C92FB763F3F11D7" blockId="7.[192,1440,1118,1165]" pageId="7" pageNumber="6">
