<emphasis id="B94291495959FFC6C3C6C57E78C9756E" bold="true" box="[151,708,1616,1643]" pageId="2" pageNumber="21">
Genus 
<taxonomicName id="4C3636D85959FFC6C3BFC57E78C9756E" authority="Muller &amp; Troschel, 1842" authorityName="Muller &amp; Troschel" authorityYear="1842" box="[238,708,1616,1643]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="2" pageNumber="21" phylum="Echinodermata" rank="genus">
