; 
<collectingCountry id="94CD765078433E42FB23F533D7AEFA27" box="[1207,1278,1395,1421]" name="Japan" pageId="5" pageNumber="24">Japan</collectingCountry>
. Separated from 
<taxonomicName id="2BDA4D4378433E42FE13F5DAD1DFFA19" box="[391,655,1434,1459]" class="Insecta" family="Miridae" genus="Cimicicapsus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="24" phylum="Arthropoda" rank="species" species="miyakei">
