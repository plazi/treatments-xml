<taxonomicName id="2BDA4D43784C3E4DFDA8F382D064FC73" box="[572,820,962,985]" class="Insecta" family="Miridae" genus="Cimicicapsus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="29" phylum="Arthropoda" rank="species" species="rubidus" status="sp. n.">
<emphasis id="DEAEEAD2784C3E4DFDA8F382D064FC73" box="[572,820,962,985]" italics="true" pageId="10" pageNumber="29">
Cimicicapsus 
<taxonomicName id="2BDA4D43784C3E4DFD49F382D064FC73" box="[733,820,962,985]" pageId="10" pageNumber="29" rank="species" species="rubidus" status="sp. n.">rubidus</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="DEAEEAD2784C3E4DFCA8F381D02EFC73" bold="true" box="[828,894,961,985]" pageId="10" pageNumber="29">
<taxonomicNameLabel id="C59D57A9784C3E4DFCA8F381D02EFC73" box="[828,894,961,985]" pageId="10" pageNumber="29" rank="species">sp. n.</taxonomicNameLabel>
