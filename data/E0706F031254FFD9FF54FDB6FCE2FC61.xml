<taxonomicName id="AFD9A5961254FFD9FE6FFC7CFCE6FC61" authority="Monterosato, 1884" authorityName="Monterosato" authorityYear="1884" box="[428,835,908,933]" class="Gastropoda" family="Rissoidae" genus="Setia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="46" pageNumber="47" phylum="Mollusca" rank="species" species="turriculata">
