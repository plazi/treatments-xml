<taxonomicName id="4C4D4D2DFFC9CA33FCFFFCFCFC4DFCF2" authorityName="Joicey &amp; Talbot" authorityYear="1916" box="[885,989,860,885]" class="Insecta" family="Lepidoptera (awaiting allocation)" genus="Thyrarctia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="178" phylum="Arthropoda" rank="species" species="semivitrea">
