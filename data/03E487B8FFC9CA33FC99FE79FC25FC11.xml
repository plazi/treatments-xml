</taxonomicName>
with 
<emphasis id="B939EABCFFC9CA33FD51FC9CFC0BFCD1" box="[731,923,828,854]" italics="true" pageId="41" pageNumber="178">
<taxonomicName id="4C4D4D2DFFC9CA33FD51FC9CFC06FCD1" authorityName="Schaus" authorityYear="1905" box="[731,918,828,854]" class="Insecta" family="Arctiidae" genus="Amaxia" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="178" phylum="Arthropoda" rank="species" species="consistens">Amaxia consistens</taxonomicName>
