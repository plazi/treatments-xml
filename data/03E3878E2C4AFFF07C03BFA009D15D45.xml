<quantity id="4CB29B7D2C4AFFF67C5CBA340AAD5E39" box="[343,442,1848,1874]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="2" pageNumber="53" unit="mm" value="1.6">1.6 mm</quantity>
