<taxonomicName id="4C4A4D1B2C4BFFF77C94BB3E09E35F22" authority="Hsiao" authorityName="Hsiao" authorityYear="1963" box="[415,756,1586,1609]" class="Insecta" family="Coreidae" genus="Pseudomictis" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="brevicornis">
<emphasis id="B93EEA8A2C4BFFF77C94BB3E09BC5F22" box="[415,683,1586,1609]" italics="true" pageId="3" pageNumber="54">Pseudomictis brevicornis</emphasis>
