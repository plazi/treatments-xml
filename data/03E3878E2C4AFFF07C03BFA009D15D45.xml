<quantity id="4CB29B7D2C4BFFF77CF1BC4809415835" box="[506,598,324,350]" metricMagnitude="-3" metricUnit="m" metricValue="8.4" pageId="3" pageNumber="54" unit="mm" value="8.4">8.4 mm</quantity>
