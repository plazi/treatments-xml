<quantity id="4CB29B7D2C4BFFF77C71BC600AF258ED" box="[378,485,364,390]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" pageId="3" pageNumber="54" unit="mm" value="11.5">11.5 mm</quantity>
