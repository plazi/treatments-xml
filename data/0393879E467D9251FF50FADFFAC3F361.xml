<taxonomicName id="4C3A4D0B46739253FEE6F85EFE6BF2A0" box="[289,510,1956,1979]" class="Malacostraca" family="Lysianassidae" genus="Bonassa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="53" phylum="Arthropoda" rank="species" species="brasiliensis" status="sp. nov.">
<emphasis id="B94EEA9A46739253FEE6F85EFE6BF2A0" box="[289,510,1956,1979]" italics="true" pageId="8" pageNumber="53">Bonassa brasiliensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27D57E146739253FDC3F859FDCEF2A0" box="[516,603,1955,1979]" pageId="8" pageNumber="53" rank="species">
