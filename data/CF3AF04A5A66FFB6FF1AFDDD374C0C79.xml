<td id="BA722A625A66005DFB4DFAA331600B46" box="[1216,1358,1298,1320]" gridcol="4" gridrow="4" pageId="19" pageNumber="244">No data</td>
</tr>
<tr id="F9A3431E5A66005DFF12FA8D31600B3C" box="[159,1358,1340,1362]" gridrow="5" pageId="19" pageNumber="244">
<th id="BA722A625A66005DFF12FA8D36290B3C" box="[159,519,1340,1362]" gridcol="0" gridrow="5" pageId="19" pageNumber="244">Teeth on coxosternal tooth- plates</th>
<td id="BA722A625A66005DFDA5FA8D36D70B3C" box="[552,761,1340,1362]" gridcol="1" gridrow="5" pageId="19" pageNumber="244">4+4</td>
<td id="BA722A625A66005DFC92FA8D37B10B3C" box="[799,927,1340,1362]" gridcol="2" gridrow="5" pageId="19" pageNumber="244">4+4</td>
<td id="BA722A625A66005DFC50FA8D30510B3C" box="[989,1151,1340,1362]" gridcol="3" gridrow="5" pageId="19" pageNumber="244">4+4</td>
<td id="BA722A625A66005DFB4DFA8D31600B3C" box="[1216,1358,1340,1362]" gridcol="4" gridrow="5" pageId="19" pageNumber="244">4+4</td>
