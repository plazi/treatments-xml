<subSubSection id="BD9545E4EB544041FF36D2FCFD516ED3" box="[197,652,1255,1280]" pageId="13" pageNumber="14" type="etymology">
<paragraph id="F530166FEB544041FF36D2FCFD516ED3" blockId="13.[151,1437,1111,1928]" box="[197,652,1255,1280]" pageId="13" pageNumber="14">
<emphasis id="C7FBCA7DEB544041FF36D2FCFE966F2C" bold="true" box="[197,331,1256,1280]" inLineHeading="true" pageId="13" pageNumber="14" reason="1">Etymology.</emphasis>
