<emphasis id="95FDEA8FFFF2D613A940F87DFE18F82E" box="[321,446,1975,1999]" italics="true" pageId="2" pageNumber="287">Neadeloides</emphasis>
</taxonomicName>
spp.: adults. 1, 
<taxonomicName id="60894D1EFFF2D613AA5EF87DFD46F82E" baseAuthorityName="Moore" baseAuthorityYear="1867" box="[607,736,1975,1999]" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="287" phylum="Arthropoda" rank="species" species="cinerealis">
<emphasis id="95FDEA8FFFF2D613AA5EF87DFD46F82E" box="[607,736,1975,1999]" italics="true" pageId="2" pageNumber="287">N. cinerealis</emphasis>
</taxonomicName>
, male, Golitar, Sikkim; 2, 
<taxonomicName id="60894D1EFFF2D613ABEEF87DFBC9F82E" baseAuthorityName="Moore" baseAuthorityYear="1867" box="[1007,1135,1975,1999]" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="287" phylum="Arthropoda" rank="species" species="cinerealis">
<emphasis id="95FDEA8FFFF2D613ABEEF87DFBC9F82E" box="[1007,1135,1975,1999]" italics="true" pageId="2" pageNumber="287">N. cinerealis</emphasis>
</taxonomicName>
, male, type, Darjeeling; 3, 
<taxonomicName id="60894D1EFFF2D613AD84F87DFF5CF812" baseAuthorityName="Moore" baseAuthorityYear="1867" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="287" phylum="Arthropoda" rank="species" species="cinerealis">
<emphasis id="95FDEA8FFFF2D613AD84F87DFF5CF812" italics="true" pageId="2" pageNumber="287">N. cinerealis</emphasis>
</taxonomicName>
, female, Golitar, Sikkim; 4, 
<taxonomicName id="60894D1EFFF2D613AA1DF811FD3AF812" baseAuthorityName="Moore" baseAuthorityYear="1867" box="[540,668,2011,2035]" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="287" phylum="Arthropoda" rank="species" species="cinerealis">
<emphasis id="95FDEA8FFFF2D613AA1DF811FD3AF812" box="[540,668,2011,2035]" italics="true" pageId="2" pageNumber="287">N. cinerealis</emphasis>
</taxonomicName>
, female, type, Darjeeling; 5, 
<emphasis id="95FDEA8FFFF2D613ABC5F811FBC5F812" box="[964,1123,2011,2035]" italics="true" pageId="2" pageNumber="287">
<taxonomicName id="60894D1EFFF2D613ABC5F811FBF9F812" baseAuthorityName="Hampson" baseAuthorityYear="1896" box="[964,1119,2011,2035]" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="287" phylum="Arthropoda" rank="species" species="glaucoptera">N. glaucoptera</taxonomicName>
,
</emphasis>
male, Golitar, Sikkim.
</paragraph>
</caption>
