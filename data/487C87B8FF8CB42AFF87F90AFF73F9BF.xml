<taxonomicName id="07D54D2DFF8EB42AFCEEF9F6FECDF992" box="[159,346,1597,1621]" class="Magnoliopsida" family="Boraginaceae" genus="Echium" kingdom="Plantae" order="Boraginales" pageId="39" pageNumber="297" phylum="Tracheophyta" rank="species" species="virescens">
