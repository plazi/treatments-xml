</taxonomicName>
(
<taxonomicName id="07D54D2DFF8EB42AFD64F9D1FE1CF9F5" box="[277,395,1562,1586]" class="Magnoliopsida" family="Moraceae" kingdom="Plantae" order="Rosales" pageId="39" pageNumber="297" phylum="Tracheophyta" rank="family">Moraceae</taxonomicName>
), 
<taxonomicName id="07D54D2DFF8EB42AFDEFF9D1FDD3F9F5" box="[414,580,1562,1586]" class="Magnoliopsida" family="Fagaceae" genus="Castanea" kingdom="Plantae" order="Fagales" pageId="39" pageNumber="297" phylum="Tracheophyta" rank="species" species="sativa">
