<taxonomicName id="07D54D2DFF8EB42AFEAAFA33FE9FF9F5" authorityName="C.Linnaeus" authorityYear="1753" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="39" pageNumber="297" phylum="Mollusca" rank="species" species="carica">
