</taxonomicName>
(
<taxonomicName id="07D54D2DFF8EB42AFE23F9D1FD28F9F5" authorityName="Dumortier" authorityYear="1829" box="[594,703,1562,1586]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="39" pageNumber="297" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
