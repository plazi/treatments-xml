<geoCoordinate id="36CD500FFFF6FFC81150F892FE27F8F2" box="[349,487,1879,1903]" degrees="29.05862" direction="north" orientation="latitude" pageId="6" pageNumber="85" precision="1" value="29.05862">29.05862° N</geoCoordinate>
