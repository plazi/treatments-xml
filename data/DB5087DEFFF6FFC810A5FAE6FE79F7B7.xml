<geoCoordinate id="36CD500FFFF6FFC81265F983FCC2F9C0" box="[616,770,1606,1630]" degrees="104.13248" direction="west" orientation="longitude" pageId="6" pageNumber="85" precision="1" value="-104.13248">104.13248° W</geoCoordinate>
