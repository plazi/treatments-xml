<taxonomicName id="94F94D4BFFF6FFC8115AF80BFDF9F87B" authorityName="Taylor" authorityYear="1937" box="[343,569,1998,2022]" class="Reptilia" family="Colubridae" genus="Tantilla" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="85" phylum="Chordata" rank="species" species="hobartsmithi">
