<geoCoordinate id="36CD500FFFF6FFC8115CFA78FE38FA49" box="[337,504,1469,1493]" degrees="103.925014" direction="west" orientation="longitude" pageId="6" pageNumber="85" precision="1" value="-103.92501">103.925014° W</geoCoordinate>
