<taxonomicName id="75A84ECDFFEFE3151BDFEB22FCEBFCCE" authority="Steph. INSULA" authorityName="Steph. INSULA" authorityYear="1910" box="[417,841,770,796]" class="Jungermanniopsida" family="Radulaceae" genus="Radula" kingdom="Animalia" order="Porellales" pageId="1" pageNumber="13" phylum="Bryophyta" rank="species" species="longispica">
