, 
<taxonomicName id="75A84ECDFFEFE315187CEA61FAC5FD89" authority="(Hampe 1853: 715) Jager &amp; Sauerbeck (1877: 267)" authorityName="Jager &amp; Sauerbeck" authorityPageNumber="267" authorityYear="1877" baseAuthorityName="Hampe" baseAuthorityPageNumber="715" baseAuthorityYear="1853" box="[514,1383,577,603]" class="Bryopsida" family="Meteoriaceae" genus="Papillaria" kingdom="Plantae" order="Leucodontales" pageId="1" pageNumber="13" phylum="Bryophyta" rank="species" species="crocea">
