<specimenCode id="DB80476DD305FFE62700F8D2FE51F8A0" box="[336,502,1888,1915]" collectionCode="HKAS" country="China" httpUri="http://grbio.org/cool/di2a-40jb" name="Cryptogamic Herbarium of Kunming Institute of Botany" pageId="4" pageNumber="124">HKAS 115790</specimenCode>
