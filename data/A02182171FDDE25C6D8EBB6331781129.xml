<taxonomicName id="5D8C9E4F9A9EA475ACCC6135E063C272" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:223585" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus maculatus Radamae v. hovoides Forel" order="Hymenoptera" pageNumber="33" phylum="Arthropoda" rank="variety" species="maculatus" status="n. var." variety="hovoides">Var. hovoides</taxonomicName>
, 
<taxonomicNameLabel id="79D280129054A0C11123B1A73170F306" rank="variety">n. var.</taxonomicNameLabel>
</paragraph>
