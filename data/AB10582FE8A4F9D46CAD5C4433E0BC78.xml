<geoCoordinate id="C96B18B83B3DF8C81D407B9D10191980" direction="north" orientation="latitude" precision="925" value="26.383333">26°23'N</geoCoordinate>
; 
<geoCoordinate id="714C453E9237A2C4F936A946A7BF70D4" direction="east" orientation="longitude" precision="925" value="127.48333">127°29'E</geoCoordinate>
