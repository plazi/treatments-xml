<taxonomicName id="C01691334FF59469BD249E8D4E19B4CF" form="wyvernus" genus="Psylloidea" lsidName="Psylloidea wyvernus" pageId="27" pageNumber="28" rank="form" species="wyvernus">wyvernus</taxonomicName>
