<taxonomicName id="79BFEDE6FFB7FFB6AA73FE8CFE550202" box="[362,464,257,282]" class="Actinopterygii" family="Cichlidae" genus="Trichromis" kingdom="Animalia" order="Perciformes" pageId="20" pageNumber="231" phylum="Chordata" rank="species" species="salvini">
