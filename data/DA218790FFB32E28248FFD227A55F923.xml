<taxonomicName id="95884D05FFB32E29248FFB847923FBCC" authorityName="Kraatz" authorityYear="1857" box="[151,315,1111,1136]" class="Insecta" family="Staphylinidae" genus="Amphichroum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="401" phylum="Arthropoda" rank="genus">Amphichroum</taxonomicName>
| schuelkei 
<emphasis id="60FCEA94FFB32E2925A6FB8479EEFBCC" bold="true" box="[446,502,1111,1136]" pageId="14" pageNumber="391">
<taxonomicNameLabel id="7BCF57EFFFB32E2925A6FB8479EEFBCC" box="[446,502,1111,1136]" pageId="14" pageNumber="391" rank="species">sp.n.</taxonomicNameLabel>
