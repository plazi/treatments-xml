<emphasis id="69E64A0AFFB3FFF64FACFE9F7C29FE2D" bold="true" box="[151,304,367,393]" italics="true" pageId="40" pageNumber="241">Heterospilus</emphasis>
(
<emphasis id="69E64A0AFFB3FFF64E7AFE9F7CC3FE2D" bold="true" box="[321,474,367,393]" italics="true" pageId="40" pageNumber="241">Heterospilus</emphasis>
) 
<emphasis id="69E64A0AFFB3FFF64ED0FE9F7F58FE2D" bold="true" box="[491,577,367,393]" italics="true" pageId="40" pageNumber="241">setosus</emphasis>
