<taxonomicName id="ED994D61FE7FFE20F0B1C73977187F8F" authority=": Schmitt, 1924: 96" authorityName=": Schmitt" authorityPageNumber="96" authorityYear="1924" box="[699,1118,376,397]" class="Malacostraca" family="Albuneidae" genus="Lepidopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="393" pageNumber="394" phylum="Arthropoda" rank="species" species="scutellata">
