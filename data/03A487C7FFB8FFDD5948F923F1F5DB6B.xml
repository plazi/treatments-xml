in
</quantity>
<taxonomicName id="4C0D4D52FFBFFFDD5C4CFDA2F1E5DB6B" baseAuthorityName="Berlese" baseAuthorityYear="1903" box="[1067,1193,521,545]" class="Arachnida" family="Scutacaridae" genus="Scutacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="5" pageNumber="381" phylum="Arthropoda" rank="species" species="rotundus">
