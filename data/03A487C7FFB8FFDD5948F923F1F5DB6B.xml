<emphasis id="B979EAC3FFBFFFDD5B8DFDA1F6B7DB6B" box="[1002,1019,521,545]" italics="true" pageId="5" pageNumber="381">φ</emphasis>
<quantity id="4CF59B34FFBFFFDD5B9BFDBBF168DB6B" box="[1020,1060,521,548]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="5" pageNumber="381" unit="in" value="2.0">
