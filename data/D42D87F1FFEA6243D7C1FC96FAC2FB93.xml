14. 
<taxonomicName id="9B844D64FFEA6243D797FC96FDD1FCBB" authority="(Sulzer, 1776)" baseAuthorityName="Sulzer" baseAuthorityYear="1776" box="[193,602,866,891]" class="Insecta" family="Rhaphidophoridae" genus="Dolichopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="palpata">
