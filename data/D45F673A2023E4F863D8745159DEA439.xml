<taxonomicName id="4838A63F01223884BE1384098FE5311F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182439" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus pitjantjatarae McArthur" order="Hymenoptera" pageNumber="331" phylum="Arthropoda" rank="species" species="pitjantjatarae">Camponotus pitjantjatarae</taxonomicName>
