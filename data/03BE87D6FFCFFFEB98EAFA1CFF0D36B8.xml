<quantity id="4CEF9B25FFCFFFEB9A20F951F8C936B8" box="[605,648,1699,1725]" metricMagnitude="0" metricUnit="m" metricValue="6.0" pageId="94" pageNumber="95" unit="m" value="6.0">6 m</quantity>
