<subSubSection id="C30B656A1B5CFFBDFF4460E4EE847591" pageId="8" pageNumber="343" type="description">
<paragraph id="8BAE36E11B5CFFBDFF4460E4E9547262" blockId="8.[151,1437,151,1652]" pageId="8" pageNumber="343">
<typeStatus id="54AA88431B5CFFBDFF4460E4EA2371A5" box="[199,308,440,464]" pageId="8" pageNumber="343">
<emphasis id="B965EAF31B5CFFBDFF4460E4EA2371A5" bold="true" box="[199,308,440,464]" pageId="8" pageNumber="343">Paratype</emphasis>
</typeStatus>
. Ƥ 
<collectingCountry id="F30676711B5CFFBDFEE760EBEAC271BA" box="[356,469,439,463]" name="Colombia" pageId="8" pageNumber="343">Colombia</collectingCountry>
, Risaralda, Mistrato, quebrada el Empalato. 
<quantity id="4CE99B041B5CFFBDFC5960EBEF2F71BA" box="[986,1080,439,463]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="8" pageNumber="343" unit="m" value="1800.0">1800 m</quantity>
.s.n.m. 
<date id="FFAF10211B5CFFBDFB0B60EBEE2A71BA" box="[1160,1341,439,463]" pageId="8" pageNumber="343" value="1992-03-31">
<number id="3DD924C61B5CFFBDFB0B60EBEFB371BA" box="[1160,1188,439,463]" pageId="8" pageNumber="343" value="31.0">31</number>
