<collectingCountry id="F3007650FF46FF6299A7FEF1F8723118" box="[474,563,259,285]" name="Panama" pageId="215" pageNumber="216">Panama</collectingCountry>
(Moser and Sumida in 
<bibRefCitation id="EF864B31FF46FF629B3DFEF1F98A311B" author="Moser" box="[832,971,259,286]" firstAuthor="Moser" pageId="215" pageNumber="216" refId="ref303637" refString="Moser, H. G. (Ed.). (1996) The early stages of fishes in the California Current region. CALCOFI Atlas, No. 33." type="book" year="1996">Moser 1996</bibRefCitation>
). Benthic; depth: 
<quantity id="4CEF9B25FF46FF629CE8FEF1FF463118" box="[1173,1287,259,285]" metricMagnitude="2" metricUnit="m" metricValue="1.17" metricValueMax="1.96" metricValueMin="0.38" pageId="215" pageNumber="216" unit="m" value="117.0" valueMax="196.0" valueMin="38.0">38–196 m</quantity>
(
<quantity id="4CEF9B25FF46FF629D6BFEF0FFD4311B" box="[1302,1429,258,286]" metricMagnitude="2" metricUnit="m" metricValue="1.168908" metricValueMax="1.9568159999999999" metricValueMin="0.381" pageId="215" pageNumber="216" unit="ft" value="383.5" valueMax="642.0" valueMin="125.0">125–642 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FF46FF62996EFED5F8023144" author="Robertson and Allen" box="[275,579,294,321]" firstAuthor="Robertson" pageId="215" pageNumber="216" refId="ref316799" refString="Robertson, D. R. and Allen, G. R. (2015) Shorefishes of the Tropical Eastern Pacific: an Information System. Version 2.0 (2008). Smithsonian Tropical Research Institute, Balboa. http: // biogeodb. stri. si. edu / sftep / en / pages" type="url" year="2015">Robertson and Allen 2015</bibRefCitation>
