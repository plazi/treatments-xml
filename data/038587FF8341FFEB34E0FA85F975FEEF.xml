, 
<taxonomicName id="4C2C4D6A8340FFE4349BF8BDFBACF8E3" authority="Mey &amp; Malicky, 2021" authorityName="Mey &amp; Malicky" authorityYear="2021" box="[236,471,1845,1869]" class="Magnoliopsida" family="Rubiaceae" genus="Lepidostoma" kingdom="Plantae" order="Gentianales" pageId="7" pageNumber="540" phylum="Tracheophyta" rank="species" species="indivisum" status="spec. nov.">
<emphasis id="B958EAFB8340FFE4349BF8BDFBACF8E3" box="[236,471,1845,1869]" italics="true" pageId="7" pageNumber="540">Lepidostoma indivisum</emphasis>
</taxonomicName>
<emphasis id="B958EAFB8340FFE435ABF8BDF839F8E3" bold="true" box="[476,578,1845,1869]" pageId="7" pageNumber="540">
<taxonomicNameLabel id="A26B57808340FFE435ABF8BDF839F8E3" box="[476,578,1845,1869]" pageId="7" pageNumber="540" rank="species">spec. nov.</taxonomicNameLabel>
