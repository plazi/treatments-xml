<taxonomicName id="E8A18DF6FFF5EF38FDD0CA4EFD4F3236" box="[583,741,1555,1578]" class="Insecta" family="Eumenidae" genus="Stenonartonia" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
