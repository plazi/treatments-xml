<taxonomicName id="8FEE4D41FF9CFFB7FF4FFAF864B0CE3E" authorityName="Koch" authorityYear="1844" box="[223,499,1375,1401]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="rotundatum">
