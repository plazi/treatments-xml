<taxonomicName id="4CEACE81DE1F40660373FEBC792052D9" authority="Lin et Yang" authorityName="Lin et Yang" box="[515,854,1233,1256]" class="Insecta" family="Cerambycidae" genus="Glenea" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="nigrorubricollis" status="sp. nov.">
