<taxonomicName id="3DEA4D22D26A505BBED928938161FA3F" authority="Hope, 1838" authorityName="Hope" authorityYear="1838" box="[459,805,1449,1475]" class="Insecta" family="Carabidae" genus="Craspedophorus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="230" phylum="Arthropoda" rank="genus">
