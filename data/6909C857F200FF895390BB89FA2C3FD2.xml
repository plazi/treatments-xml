<heading id="BA57CE2DF200FF8A5390BB89FE353CBE" bold="true" box="[151,397,674,700]" fontSize="11" level="1" pageId="5" pageNumber="335" reason="1">
<emphasis id="D3D4A553F200FF8A5390BB89FE353CBE" bold="true" box="[151,397,674,700]" pageId="5" pageNumber="335">
<taxonomicName id="26A002C2F200FF8A5390BB89FE913CBE" authority="Grisales &amp; De Carvalho, 2019" authorityName="Grisales &amp; De Carvalho" authorityYear="2019" box="[151,297,674,700]" class="Insecta" family="Fanniidae" genus="Fannia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="335" phylum="Arthropoda" rank="species" species="awa" status="sp. nov.">
<emphasis id="D3D4A553F200FF8A5390BB89FE913CBE" bold="true" box="[151,297,674,700]" italics="true" pageId="5" pageNumber="335">Fannia awa</emphasis>
