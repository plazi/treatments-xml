<quantity id="CA02C9DE2D50FFE5FCA6FE5EAD3EFE26" box="[791,881,405,432]" metricMagnitude="-3" metricUnit="m" metricValue="9.6" pageId="11" pageNumber="58" unit="mm" value="9.6">9.6 mm</quantity>
