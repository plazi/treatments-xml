<collectingRegion id="8F0804A6FF81B30E4076F8FACC820C67" box="[1072,1156,1843,1867]" country="United States of America" name="Oregon" pageId="29" pageNumber="30">Oregon</collectingRegion>
at 
<quantity id="8A3467A1FF81B30E40EFF8FACD3D0C66" box="[1193,1339,1843,1867]" metricMagnitude="2" metricUnit="m" metricValue="8.0" metricValueMax="10.0" metricValueMin="6.0" pageId="29" pageNumber="30" unit="m" value="800.0" valueMax="1000.0" valueMin="600.0">
<elevation id="C6E12D77FF81B30E40EFF8FACD3D0C66" box="[1193,1339,1843,1867]" metricMagnitude="2" metricUnit="m" metricValue="8.0" metricValueMax="10.0" metricValueMin="6.0" pageId="29" pageNumber="30" unit="m" value="800.0" valueMax="1000.0" valueMin="600.0">600–1000 m</elevation>
</quantity>
</materialsCitation>
. 
<bibRefCitation id="295DB7B5FF81B30E4101F8FAC8E70C43" author="Walters, J. F." pageId="29" pageNumber="30" pagination="799 - 836" refId="ref238019" refString="Walters, J. F. (1976) Ecology of Hawaiian sergestid shrimps (Penaeidea: Sergestidae). United States Fishery Bulletin, 74, 799 - 836." type="journal article" year="1976">Walters (1976)</bibRefCitation>
