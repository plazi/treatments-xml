<taxonomicName id="4C7522ACFFB9CC7EA3DCBE0DFEA8F831" box="[151,366,1914,1940]" class="Insecta" family="Ichneumonidae" genus="Eremotylus" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="198" phylum="Arthropoda" rank="species" species="undetermined">
