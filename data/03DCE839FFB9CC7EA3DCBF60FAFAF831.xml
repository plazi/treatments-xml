<taxonomicName id="4C7522ACFFB9CC7EA749BE45FB2DF8E9" authorityName="Szepligeti" authorityYear="1905" box="[1026,1259,1842,1868]" class="Insecta" family="Ichneumonidae" genus="Eremotylus" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="198" phylum="Arthropoda" rank="species" species="sibiricus">
