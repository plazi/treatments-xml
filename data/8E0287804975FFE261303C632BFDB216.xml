<taxonomicName id="C1AB4D154975FFE261F23BE22DECB3A7" box="[458,748,1693,1718]" class="Insecta" family="Chrysomelidae" genus="Clavicornaltica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="57" phylum="Arthropoda" rank="species" species="tamdao">
