</taxonomicName>
, 
<taxonomicName id="4C3C6A9E7001EB6F28F2FA8366266247" box="[764,887,1375,1401]" class="Magnoliopsida" family="Amaranthaceae" genus="Beta" kingdom="Plantae" order="Caryophyllales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="species" species="vulgaris">
