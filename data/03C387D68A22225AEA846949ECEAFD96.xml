<paragraph id="8BD536C08A22225BEA846924E8A6FEEE" blockId="26.[151,506,295,355]" box="[151,465,330,355]" pageId="26" pageNumber="27">
Figures 
<number id="3DA224E78A22225BEAE16924E84DFEEE" box="[242,314,330,355]" pageId="26" pageNumber="27" value="45.46">45, 46</number>
, 
<number id="3DA224E78A22225BEB556925E8A6FEEE" box="[326,465,331,355]" pageId="26" pageNumber="27" value="91.5535" valueMax="113.0" valueMin="70.107">70, 107–113</number>
</paragraph>
