<collectingCountry id="F334D8B12C4DFFA1FEACB0F0FE3AFD73" box="[304,408,706,730]" name="Myanmar" pageId="12" pageNumber="639">Myanmar</collectingCountry>
(KREKICH- STRASSOLDO 1914), 
<collectingCountry id="F334D8B12C4DFFA1FD4BB0F3FC9BFD70" box="[727,825,705,729]" name="Thailand" pageId="12" pageNumber="639">Thailand</collectingCountry>
