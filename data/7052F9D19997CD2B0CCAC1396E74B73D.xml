<taxonomicName id="EF629161391F93FA7FF67B117C24C54A" authority="" authorityName="" class="Insecta" family="Braconidae" genus="Canalopius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Canalopius periscopus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="periscopus">Canalopius periscopus</taxonomicName>
