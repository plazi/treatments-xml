<taxonomicName id="4C0D4D7CFFB2FFF8E0F92EFB784949AC" box="[672,823,465,488]" class="Insecta" family="Coccidae" genus="Kilifia" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="acuminata">
<emphasis id="B979EAEDFFB2FFF8E0F92EFB784949AC" box="[672,823,465,488]" italics="true" pageId="11" pageNumber="12">K. acuminata</emphasis>
</taxonomicName>
