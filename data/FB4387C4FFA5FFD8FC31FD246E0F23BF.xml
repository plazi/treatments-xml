<quantity id="B4129B37FFA6FFD9FE4BF82F6E3822A9" box="[430,562,1951,1975]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" metricValueMax="3.1" metricValueMin="2.2" pageId="6" pageNumber="252" unit="mm" value="2.65" valueMax="3.1" valueMin="2.2">2.2–3.1 mm</quantity>
; length of thorax, 
<quantity id="B4129B37FFA6FFD9FF9CF80C6CF622CB" box="[121,252,1980,2004]" metricMagnitude="-3" metricUnit="m" metricValue="1.45" metricValueMax="1.6" metricValueMin="1.3" pageId="6" pageNumber="252" unit="mm" value="1.45" valueMax="1.6" valueMin="1.3">1.3–1.6 mm</quantity>
; length of wing, 3.0– 
<quantity id="B4129B37FFA6FFD9FE3AF80C6E3922CB" box="[479,563,1980,2004]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="6" pageNumber="252" unit="mm" value="3.6">3.6 mm</quantity>
