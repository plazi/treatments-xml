<number id="3D80ED51B409FFB2419CFF38D8349DFF" box="[1275,1319,188,213]" pageId="6" pageNumber="291" value="876.0">876</number>
A), RAG 
<number id="3D80ED51B409FFB240F4FF38D88F9DFE" box="[1427,1436,188,212]" pageId="6" pageNumber="291" value="1.0">1</number>
(C 
<number id="3D80ED51B409FFB245D5FF5BDDCD9DD2" box="[178,222,223,248]" pageId="6" pageNumber="291" value="659.0">659</number>
A, T 
<number id="3D80ED51B409FFB24476FF64DC5E9DD2" box="[273,333,224,248]" pageId="6" pageNumber="291" value="2482.0">2482</number>
C), FGB intron 
<number id="3D80ED51B409FFB2449DFF5BDF1B9DD2" box="[506,520,223,248]" pageId="6" pageNumber="291" value="5.0">5</number>
