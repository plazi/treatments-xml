<taxonomicName id="4C0A4D20FF87FFDAFEE6FE5E39E9FE1B" authority="Yakhontov, 1961: 1473 - 1474" authorityName="Yakhontov" authorityPageNumber="1473 - 1474" authorityYear="1961" box="[264,920,398,422]" class="Insecta" family="Thripidae" genus="Rhopalandrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="tschirkunae">
