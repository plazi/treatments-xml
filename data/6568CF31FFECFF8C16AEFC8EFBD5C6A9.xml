<taxonomicName id="2AC105A4FFE9FF8C1482FC36FC44C768" box="[699,814,1012,1034]" class="Insecta" family="Ichneumonidae" genus="Leptophion" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="117" phylum="Arthropoda" rank="genus">
