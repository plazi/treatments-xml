(
<figureCitation id="79B7ACB4E960500BFC2735ADFBE4EF5B" box="[953,1064,236,257]" captionStart="Figure 30" captionStartId="55.[164,241,1815,1834]" captionTargetBox="[165,1444,193,1786]" captionTargetId="figure-0@55.[164,1444,193,1786]" captionTargetPageId="55" captionText="Figure 30. Aedeagus. A, Heraeus bahiensis sp. nov.; B, Heraeus bolivianus sp. nov.; C, Heraeus brevirostris sp. nov.; D, Heraeus concolor Slater &amp; Baranowski, 1994; E, Heraeus ecuatorianus sp. nov.; F, Heraeus mesoamericanus sp. nov.; G, Heraeus mexicanus sp. nov.; H, Heraeus penai sp. nov." pageId="67" pageNumber="96">FIGS 30G</figureCitation>
, 
<figureCitation id="79B7ACB4E960500BFBAB35ADFBA9EF5B" box="[1077,1125,236,257]" captionStart="Figure 33" captionStartId="60.[144,223,1062,1081]" captionTargetBox="[303,1263,195,1033]" captionTargetId="figure-304@60.[303,1263,195,1033]" captionTargetPageId="60" captionText="Figure 33. Legs: A, Heraeus concolor Slater &amp; Baranowski, 1994; B, Heraeus ecuatorianus sp. nov.; C, Heraeus loja sp. nov.; D, Heraeus mesoamericanus sp. nov.; E, Heraeus mexicanus sp. nov.; F, Heraeus morganae sp. nov." pageId="67" pageNumber="96">33E</figureCitation>
