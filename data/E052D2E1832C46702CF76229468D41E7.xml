<taxonomicName id="B837567EC2890180E6A0A94117AB1441" class="Entognatha" family="Acerentomidae" genus="Yichunentulus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Yichunentulus" order="Protura" pageId="11" pageNumber="30" phylum="Arthropoda" rank="genus">Yichunentulus</taxonomicName>
