<taxonomicName id="DD4F812989E2B332C39749ACA7F1420F" authority="L. (1753)" authorityName="Linnaeus" authorityYear="1771" class="Polypodiopsida" family="Pteridaceae" genus="Polypodium" kingdom="Plantae" order="Polypodiales" phylum="Tracheophyta" rank="species" species="fragrans">
