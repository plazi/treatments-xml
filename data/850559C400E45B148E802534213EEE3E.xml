<taxonomicName id="BFFC12B4CEFB3B623ECF09884EDAE35D" authorityName="Wu &amp; Wu" authorityYear="2021" class="Bivalvia" family="Unionidae" genus="Inversidens" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Inversidens rentianensis" order="Unionida" pageId="0" pageNumber="85" phylum="Mollusca" rank="species" species="rentianensis">
<emphasis id="A43F568A4378036F8AC57BE1C8079CF9" italics="true" pageId="0" pageNumber="85">Inversidens rentianensis</emphasis>
</taxonomicName>
sp. nov., 
<taxonomicName id="0B729D5D50977D82C4FB518909550176" baseAuthorityName="Kobelt" baseAuthorityYear="1879" class="Bivalvia" family="Unionidae" genus="Inversidens" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Inversidens brandtii" order="Unionida" pageId="0" pageNumber="85" phylum="Mollusca" rank="species" species="brandtii">
<emphasis id="F0CE04EDBBFC5944233ACD89CB16929C" italics="true" pageId="0" pageNumber="85">Inversidens brandtii</emphasis>
</taxonomicName>
, 
<taxonomicName id="F4CE63FC755F3B04639199751C336020" baseAuthorityName="Neumayr" baseAuthorityYear="1899" class="Bivalvia" family="Unionidae" genus="Inversidens" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Inversidens pantoensis" order="Unionida" pageId="0" pageNumber="85" phylum="Mollusca" rank="species" species="pantoensis">
<emphasis id="B17834E2153EA2D4609744A9AE30547E" italics="true" pageId="0" pageNumber="85">Inversidens pantoensis</emphasis>
</taxonomicName>
. Characteristic descriptions of 
