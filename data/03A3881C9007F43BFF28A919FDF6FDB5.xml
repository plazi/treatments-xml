<paragraph id="8BB5390A9007F439FF28A984FA02F971" blockId="37.[151,1437,1700,2013]" box="[151,1431,1700,1725]" pageId="37" pageNumber="338">
<typeStatus id="54B187A89007F439FF28A984FF68F970" box="[151,253,1700,1725]" pageId="37" pageNumber="338">Holotype</typeStatus>
male, 
<collectingCountry id="F31D799A9007F439FEFBA984FE3CF970" box="[324,425,1700,1725]" name="Tanzania" pageId="37" pageNumber="338">Tanzania</collectingCountry>
, East Usambara Mountains, MacKay &amp; F Watson, Brit. Mus. 
<number id="3DC22B2D9007F439FBF8A985FBEBF970" box="[1095,1150,1701,1725]" pageId="37" pageNumber="338" value="1983.0">1983</number>
– 
<number id="3DC22B2D9007F439FB31A985FB20F970" box="[1166,1205,1701,1725]" pageId="37" pageNumber="338" value="101.0">101</number>
