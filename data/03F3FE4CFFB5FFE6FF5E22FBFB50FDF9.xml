<number id="3D925D7DFFB5FFE0FEE62061FE91F8EC" box="[303,378,1869,1895]" pageId="2" pageNumber="28" value="43.0" valueMax="48.0" valueMin="38.0">38–48</number>
/XX, 
<number id="3D925D7DFFB5FFE0FE772061FDE2F8EC" box="[446,521,1869,1895]" pageId="2" pageNumber="28" value="45.0" valueMax="50.0" valueMin="40.0">40–50</number>
/XXV; 
<number id="3D925D7DFFB5FFE0FDA82061FD9BF8EC" box="[609,624,1869,1895]" pageId="2" pageNumber="28" value="5.0">5</number>
