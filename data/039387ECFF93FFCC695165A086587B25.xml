<emphasis id="B94EEAE8FF93FFC369C5602B82E4796B" box="[412,438,1559,1584]" italics="true" pageId="6" pageNumber="7">
c 
<number id="3DF224DDFF93FFC369F1602B82E4796B" box="[424,438,1559,1584]" pageId="6" pageNumber="7" value="2.0">2</number>
