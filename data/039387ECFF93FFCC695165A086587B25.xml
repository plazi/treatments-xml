<number id="3DF224DDFF93FFC3696A637382167A32" box="[307,324,1359,1385]" pageId="6" pageNumber="7" value="5.0">5</number>
, setae located 
<number id="3DF224DDFF93FFC369A8637381527A32" box="[497,512,1359,1385]" pageId="6" pageNumber="7" value="2.0">2</number>
/ 
<number id="3DF224DDFF93FFC36A53637381457A32" box="[522,535,1359,1385]" pageId="6" pageNumber="7" value="3.0">3</number>
