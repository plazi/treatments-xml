<number id="3DF224DDFF93FFC36C06602B872D796A" box="[1119,1151,1559,1585]" pageId="6" pageNumber="7" value="37.0">37</number>
; setae 
<emphasis id="B94EEAE8FF93FFC36C88602B878C796B" box="[1233,1246,1559,1584]" italics="true" pageId="6" pageNumber="7">e</emphasis>
<number id="3DF224DDFF93FFC36CBE602B8656796A" box="[1255,1284,1559,1585]" pageId="6" pageNumber="7" value="25.0">25</number>
on ventral annulus 
<number id="3DF224DDFF93FFC3699E600382B57902" box="[455,487,1599,1625]" pageId="6" pageNumber="7" value="58.0">58</number>
; setae 
