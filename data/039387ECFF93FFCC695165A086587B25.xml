<number id="3DF224DDFF93FFC36C9D634B87B67ACA" box="[1220,1252,1399,1425]" pageId="6" pageNumber="7" value="10.0">10</number>
; genu 
<number id="3DF224DDFF93FFC3695163A382447AE2" box="[264,278,1439,1465]" pageId="6" pageNumber="7" value="4.0">4</number>
