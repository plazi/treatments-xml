<typeStatus id="A2258838C4753761FD5DBDF5FD31FC4E" box="[677,725,939,962]" pageId="4" pageNumber="751">TYPE</typeStatus>
SPECIES. — 
<taxonomicName id="BA9E4D19C4753761FCB4BDF4FB79FC4C" authority="Beauvais, 1982" authorityName="Beauvais" authorityYear="1982" box="[844,1181,938,962]" class="Anthozoa" family="Felixaraeidae" genus="Felixaraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="4" pageNumber="751" phylum="Cnidaria" rank="species" species="rennensis">
<emphasis id="4FEAEA88C4753761FCB4BDF4FBE1FC4C" box="[844,1029,938,962]" italics="true" pageId="4" pageNumber="751">Felixaraea rennensis</emphasis>
<bibRefCitation id="190F4B6BC4753761FBF2BDF4FB79FC4C" author="BEAUVAIS M." box="[1034,1181,938,962]" pageId="4" pageNumber="751" refId="ref9982" refString="BEAUVAIS M. 1982. - Revision systematique des Madreporaires des couches de Gosau. Comptoir geologique, Paris, 5 vols." type="book" year="1982">Beauvais, 1982</bibRefCitation>
