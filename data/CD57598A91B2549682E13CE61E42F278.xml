<paragraph id="5B57D754BB9EEA4234C6455241B8EDD4" pageId="0" pageNumber="43">
<taxonomicName id="75B116B8C5E6C375F9689ED346FCEBFC" class="Insecta" family="Thripidae" genus="Tryphactothrips" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tryphactothrips" order="Thysanoptera" pageId="0" pageNumber="43" phylum="Arthropoda" rank="genus">Tryphactothrips</taxonomicName>
