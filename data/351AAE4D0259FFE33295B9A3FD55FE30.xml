<taxonomicName id="7AB364D80259FFE2302DBC4CFD55FAF3" authorityName="Ahmad &amp; Abbas" authorityYear="1987" box="[559,725,1328,1354]" class="Insecta" family="Largidae" genus="Physopelta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="478" phylum="Arthropoda" rank="subGenus" subGenus="Neophysopelta">
<emphasis id="8FC7C3490259FFE2302DBC4CFD55FAF3" box="[559,725,1328,1354]" italics="true" pageId="2" pageNumber="463">Neophysopelta</emphasis>
</taxonomicName>
, from 
<collectingCountry id="C5A45FCB0259FFE23120BC4DFCE5FAF2" box="[802,869,1329,1355]" name="Japan" pageId="2" pageNumber="463">Japan</collectingCountry>
, 
<collectingCountry id="C5A45FCB0259FFE23173BC4DFC38FAF2" box="[881,952,1329,1355]" name="South Korea" pageId="2" pageNumber="463">Korea</collectingCountry>
, and 
<collectingCountry id="C5A45FCB0259FFE231F6BC4CFBCAFAF3" box="[1012,1098,1328,1354]" name="Taiwan" pageId="2" pageNumber="463">Taiwan</collectingCountry>
: 
<emphasis id="8FC7C3490259FFE23654BC4CFBF5FAF3" box="[1110,1141,1328,1354]" italics="true" pageId="2" pageNumber="463">Ph</emphasis>
. (
<emphasis id="8FC7C3490259FFE2368FBC4DFB20FAF3" box="[1165,1184,1329,1354]" italics="true" pageId="2" pageNumber="463">N</emphasis>
.) 
<emphasis id="8FC7C3490259FFE236BABC4DFAB3FAF3" box="[1208,1331,1329,1354]" italics="true" pageId="2" pageNumber="463">gutta gutta</emphasis>
, 
<taxonomicName id="7AB364D80259FFE23740BC4CFEB9FAD7" authority="Souma &amp; Ishikawa, 2021" authorityName="Souma &amp; Ishikawa" authorityYear="2021" class="Insecta" family="Largidae" genus="Physopelta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="463" phylum="Arthropoda" rank="species" species="fusciscutellata" status="sp. nov." subGenus="Neophysopelta">
<emphasis id="8FC7C3490259FFE23740BC4CFAE1FAF3" box="[1346,1377,1328,1354]" italics="true" pageId="2" pageNumber="463">Ph</emphasis>
. (
<emphasis id="8FC7C3490259FFE2377BBC4DFA14FAF3" box="[1401,1428,1329,1354]" italics="true" pageId="2" pageNumber="463">N.</emphasis>
) 
<emphasis id="8FC7C3490259FFE23295BC28FEB9FAD7" box="[151,313,1364,1390]" italics="true" pageId="2" pageNumber="463">fusciscutellata</emphasis>
</taxonomicName>
<emphasis id="8FC7C3490259FFE23342BC29FE19FAD6" bold="true" box="[320,409,1365,1391]" pageId="2" pageNumber="463">
<taxonomicNameLabel id="94F47E320259FFE23342BC29FE19FAD6" box="[320,409,1365,1391]" pageId="2" pageNumber="463" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="7AB364D80259FFE233A5BC28FD06FAD7" authority="Souma &amp; Ishikawa, 2021" authorityName="Souma &amp; Ishikawa" authorityYear="2021" box="[423,646,1364,1391]" class="Insecta" family="Largidae" genus="Physopelta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="463" phylum="Arthropoda" rank="species" species="lutaspidata" status="sp. nov." subGenus="Neophysopelta">
