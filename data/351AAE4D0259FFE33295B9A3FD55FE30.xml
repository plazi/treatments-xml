<taxonomicName id="7AB364D80259FFE236A1B858FE63FEE3" authority="Amyot &amp; Serville, 1843" authorityName="Amyot &amp; Serville" authorityYear="1843" class="Insecta" family="Largidae" genus="Physopelta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="463" phylum="Arthropoda" rank="species" species="erythrocephala">
<emphasis id="8FC7C3490259FFE236A1B858FF67FEE3" italics="true" pageId="2" pageNumber="463">Physopelta erythrocephala</emphasis>
