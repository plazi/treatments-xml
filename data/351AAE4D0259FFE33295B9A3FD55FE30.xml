</taxonomicName>
<emphasis id="8FC7C3490259FFE2308FBC29FD66FAD6" bold="true" box="[653,742,1365,1391]" pageId="2" pageNumber="463">
<taxonomicNameLabel id="94F47E320259FFE2308FBC29FD66FAD6" box="[653,742,1365,1391]" pageId="2" pageNumber="463" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="7AB364D80259FFE230F6BC28FC42FAD7" authorityName="Blote" authorityYear="1931" box="[756,962,1364,1391]" class="Insecta" family="Largidae" genus="Physopelta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="462" phylum="Arthropoda" rank="species" species="parviceps" subGenus="Neophysopelta">
<emphasis id="8FC7C3490259FFE230F6BC28FC93FAD7" box="[756,787,1364,1390]" italics="true" pageId="2" pageNumber="463">Ph</emphasis>
. (
<emphasis id="8FC7C3490259FFE23129BC29FCBEFAD7" box="[811,830,1365,1390]" italics="true" pageId="2" pageNumber="463">N</emphasis>
.) 
<emphasis id="8FC7C3490259FFE23157BC29FC42FAD7" box="[853,962,1365,1390]" italics="true" pageId="2" pageNumber="463">parviceps</emphasis>
</taxonomicName>
, and 
<taxonomicName id="7AB364D80259FFE231FDBC28FB7AFAD7" authorityName="Bergroth" authorityYear="1894" box="[1023,1274,1364,1391]" class="Insecta" family="Largidae" genus="Physopelta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="461" phylum="Arthropoda" rank="species" species="quadriguttata" subGenus="Neophysopelta">
