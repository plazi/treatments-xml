<taxonomicName id="4E2CCC142223A049BC60B72DD30F7B84" authorityName="Sánchez &amp; Vázquez-Benítez &amp; Vázquez-Sánchez &amp; Aquino &amp; Arias" authorityYear="2022" baseAuthorityName="Sánchez &amp; Vázquez-Benítez &amp; Vázquez-Sánchez &amp; Aquino &amp; Arias" baseAuthorityYear="2022" class="Magnoliopsida" family="Cactaceae" genus="Mammillaria" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Mammillaria occidentalis subsp. var. var. sinalensis" order="Caryophyllales" pageId="0" pageNumber="115" phylum="Tracheophyta" rank="variety" species="occidentalis" status="comb. nov." subSpecies="var." variety="sinalensis">Mammillaria occidentalis var. sinalensis</taxonomicName>
