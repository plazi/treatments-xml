<taxonomicName id="4C6D70A3FFFAFF9AFE190364EBA5B238" box="[452,685,1052,1076]" class="Arachnida" family="Ascidae" genus="Cheiroseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="508" phylum="Arthropoda" rank="species" species="neophalangioides">
<emphasis id="B919D732FFFAFF9AFE190364E8DFB23F" box="[452,471,1052,1075]" italics="true" pageId="15" pageNumber="508">C</emphasis>
