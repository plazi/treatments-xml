<number id="3DA51907FFFAFF9AFB7A0364EC01B239" box="[1191,1289,1052,1077]" pageId="15" pageNumber="508" value="252.5" valueMax="255.0" valueMin="250.0">250–255</number>
wide) and it has shorter 
<emphasis id="B919D732FFFAFF9AFEC10339E82FB254" box="[284,295,1089,1112]" italics="true" pageId="15" pageNumber="508">z</emphasis>
<number id="3DA51907FFFAFF9AFEFB0338E83CB254" box="[294,308,1088,1112]" pageId="15" pageNumber="508" value="1.0">1</number>
(
<number id="3DA51907FFFAFF9AFE980338E898B254" box="[325,400,1088,1112]" pageId="15" pageNumber="508" value="19.0" valueMax="20.0" valueMin="18.0">18–20</number>
), 
<emphasis id="B919D732FFFAFF9AFE780339E8B0B254" box="[421,440,1089,1112]" italics="true" pageId="15" pageNumber="508">st</emphasis>
<number id="3DA51907FFFAFF9AFE650338E8CEB254" box="[440,454,1088,1112]" pageId="15" pageNumber="508" value="3.0">3</number>
(
<number id="3DA51907FFFAFF9AFE050338EB29B254" box="[472,545,1088,1112]" pageId="15" pageNumber="508" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
), 
<emphasis id="B919D732FFFAFF9AFDEA0339EB42B254" box="[567,586,1089,1112]" italics="true" pageId="15" pageNumber="508">st</emphasis>
<number id="3DA51907FFFAFF9AFD970338EB50B254" box="[586,600,1088,1112]" pageId="15" pageNumber="508" value="4.0">4</number>
(
<number id="3DA51907FFFAFF9AFDB70347EBBBB254" box="[618,691,1087,1112]" pageId="15" pageNumber="508" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
), 
<emphasis id="B919D732FFFAFF9AFD140338EBEDB254" box="[713,741,1088,1112]" italics="true" pageId="15" pageNumber="508">ad</emphasis>
<number id="3DA51907FFFAFF9AFD380338EBFBB254" box="[741,755,1088,1112]" pageId="15" pageNumber="508" value="3.0">3</number>
