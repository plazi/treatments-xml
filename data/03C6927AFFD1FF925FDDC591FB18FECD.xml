<emphasis id="B91BFF7EFFD1FF945E15C2EEFDFAF888" box="[351,548,1772,1797]" italics="true" pageId="18" pageNumber="281">
Female 
<typeStatus id="54D49DCEFFD1FF945EF1C2EEFDC0F888" box="[443,542,1772,1797]" pageId="18" pageNumber="281">holotype</typeStatus>
