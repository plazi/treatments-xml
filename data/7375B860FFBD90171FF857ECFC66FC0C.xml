<location id="FE035FADFFBD90171E8656C8FDA1FE7F" LSID="urn:lsid:plazi:treatment:7375B860FFBD90171FF857ECFC66FC0C:FE035FADFFBD90171E8656C8FDA1FE7F" box="[502,551,464,485]" country="China" name="Abbe" pageId="17" pageNumber="196" stateProvince="Gansu">Abbe</location>
<collectionCode id="9DCD91B3FFBD90171D5D56C8FDB9FE7F" box="[557,575,464,485]" country="United Kingdom" lsid="urn:lsid:biocol.org:col:15670" name="Royal Botanic Garden Edinburgh" pageId="17" pageNumber="196" type="Herbarium">E</collectionCode>
