Africa (
<collectingCountry id="ACD3AAC51867E5171F8FFCA7FD9DFCD5" box="[445,567,800,824]" name="Cameroon" pageId="2" pageNumber="433">Cameroon</collectingCountry>
; 
<collectingCountry id="ACD3AAC51867E5171C70FC98FD30FCD5" box="[578,666,799,824]" name="Nigeria" pageId="2" pageNumber="433">Nigeria</collectingCountry>
; 
<collectingCountry id="ACD3AAC51867E5171C97FC98FBA3FCD5" box="[677,1033,799,824]" name="Democratic Republic of the Congo" pageId="2" pageNumber="433">
Democratic Republic of 
<collectingRegion id="160024B71867E5171D8BFCA7FBA3FCD5" box="[953,1033,800,824]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="2" pageNumber="433">Congo</collectingRegion>
</collectingCountry>
).
</paragraph>
