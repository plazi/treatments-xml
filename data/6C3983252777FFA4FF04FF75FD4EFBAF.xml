<collectingCountry id="9C8772A32776FFA4FB99FD1EFAD3FCE2" box="[1034,1324,764,789]" name="Mozambique" pageId="9" pageNumber="247">Republic of Mozambique</collectingCountry>
, 
<collectingCountry id="9C8772A32776FFA4FAA4FD1EFEB8FCCF" name="South Africa" pageId="9" pageNumber="247">Republic of South Africa</collectingCountry>
, 
<collectingCountry id="9C8772A32776FFA4FEC0FCFDFE47FCCF" box="[339,440,799,824]" name="Tanzania" pageId="9" pageNumber="247">Tanzania</collectingCountry>
