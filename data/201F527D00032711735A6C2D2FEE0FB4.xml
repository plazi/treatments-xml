<taxonomicName id="B6F76F1FD3B093314653B91F07574E41" LSID="http://zoobank.org/894CEC06-624C-4F74-9A3E-B2E0D09DFA2D" authority="Aguirre, Almeida &amp; Shaw" class="Insecta" family="Braconidae" genus="Meteorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Meteorus flavistigma" order="Hymenoptera" pageId="23" pageNumber="56" phylum="Arthropoda" rank="species" species="flavistigma">Meteorus flavistigma Aguirre, Almeida &amp; Shaw</taxonomicName>
<taxonomicNameLabel id="2F961E08F21841935981DD20BCC9D8DB" pageId="23" pageNumber="56">sp. n.</taxonomicNameLabel>
