<typeStatus id="54CD8866FFB8AB219DAB113542CE3909" box="[1012,1171,272,298]" pageId="84" pageNumber="85">HOLOTYPE</typeStatus>
<taxonomicName id="4C764D47FFB8AB219AFD113546A63971" authority="Borkent" authorityName="Borkent" authorityYear="2008" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="truncata">
