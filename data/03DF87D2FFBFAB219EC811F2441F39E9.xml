col.”, “1.077”, “1.077”, “1.137”, “S100 T990” (
<collectionCode id="ED67AE01FFB8AB219DD8114545883959" box="[903,981,352,378]" country="Brazil" httpUri="http://grbio.org/cool/tb11-8j4v" name="Universidade de Sao Paulo" pageId="84" pageNumber="85">DEFS</collectionCode>
