<taxonomicName id="4C504D4FFFE3FFE0FDD6FE4C7A8EFE2A" box="[559,689,414,440]" class="Magnoliopsida" family="Gesneriaceae" genus="Lenbrassia" higherTaxonomySource="CoL" kingdom="Plantae" order="Lamiales" pageId="6" pageNumber="88" phylum="Tracheophyta" rank="genus">
