<number id="D95AAA5BFFBEFFA4FAF5FA8D1061FA36" box="[1330,1346,1384,1408]" pageId="33" pageNumber="234" value="2.0">2</number>
, with 
<number id="D95AAA5BFFBEFFA4FA49FA8D10BFFA36" box="[1422,1436,1384,1408]" pageId="33" pageNumber="234" value="3.0">3</number>
