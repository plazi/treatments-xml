<taxonomicName id="16AECEBCC379BA39FE48FC9D8303FCF4" authorityName="Linnaeus" authorityYear="1758" box="[455,543,800,826]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
