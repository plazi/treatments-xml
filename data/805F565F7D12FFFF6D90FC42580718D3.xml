<taxonomicName id="CFF69CCA7D12FFFB6880FAA15BBC1D6A" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="340" phylum="Arthropoda" rank="species" species="arayat">
