<taxonomicName id="CFF69CCA7D12FFFB6E3CFAC658E01D6B" box="[827,956,1365,1389]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="340" phylum="Arthropoda" rank="species" species="pagbilao">
