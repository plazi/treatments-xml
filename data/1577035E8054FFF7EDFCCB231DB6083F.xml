<quantity id="5A261FAD8054FFF7EE81C8031D62083E" box="[1002,1087,1578,1603]" metricMagnitude="1" metricUnit="m" metricValue="2.55" metricValueMax="5.0" metricValueMin="0.1" pageId="80" pageNumber="81" unit="m" value="25.5" valueMax="50.0" valueMin="1.0">1–50 m</quantity>
