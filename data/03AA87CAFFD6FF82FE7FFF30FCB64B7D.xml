<taxonomicName id="4C034D5FFFD6FF82FCAFFECDFC2E497F" authorityName="Dana" authorityYear="1846" box="[880,973,334,355]" class="Anthozoa" family="Oculinidae" genus="Madrepora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="61" pageNumber="60" phylum="Cnidaria" rank="species" species="echinata">echinata</taxonomicName>
