94. 
<taxonomicName id="4C4A9AB9992B8559E8D1FE47D7DBF1BD" authority="Latreille, 1809" authorityName="Latreille" authorityYear="1809" box="[193,650,440,467]" class="Insecta" family="Dolichopodidae" genus="Ortochile" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="nigrocoerulea">
