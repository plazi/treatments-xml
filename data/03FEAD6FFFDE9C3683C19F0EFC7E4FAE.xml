with 
<number id="3D9F0E5EFFDE9C3687E79E76FB2348CA" box="[1201,1215,874,900]" pageId="6" pageNumber="61" value="2.0">2</number>
robust setae and 
<number id="3D9F0E5EFFDE9C3686DB9E76FA0048CA" box="[1421,1436,874,900]" pageId="6" pageNumber="61" value="1.0">1</number>
