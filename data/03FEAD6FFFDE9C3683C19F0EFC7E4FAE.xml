<number id="3D9F0E5EFFDE9C3681369E01FDF34879" box="[608,623,797,823]" pageId="6" pageNumber="61" value="2.0">2</number>
article 
<number id="3D9F0E5EFFDE9C3681919E01FD4A4879" box="[711,726,797,823]" pageId="6" pageNumber="61" value="4.0">4</number>
ventrodistal margin with 
<number id="3D9F0E5EFFDE9C3687509E01FB894879" box="[1030,1045,797,823]" pageId="6" pageNumber="61" value="2.0">2</number>
strong acute process, peduncular article 
<number id="3D9F0E5EFFDE9C3683B19E58FF694810" box="[231,245,836,862]" pageId="6" pageNumber="61" value="5.0">5</number>
with 
<number id="3D9F0E5EFFDE9C36826E9E58FEDA4810" box="[312,326,836,862]" pageId="6" pageNumber="61" value="1.0">1</number>
acute process on distal margin and 
<number id="3D9F0E5EFFDE9C3681B09E58FD694810" box="[742,757,836,862]" pageId="6" pageNumber="61" value="1.0">1</number>
