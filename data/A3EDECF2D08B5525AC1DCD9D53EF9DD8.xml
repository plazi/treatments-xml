* 
<taxonomicName id="B983D79F21EF76CF2399B088353A383F" LSID="A3EDECF2-D08B-5525-AC1D-CD9D53EF9DD8" authority="Valdes, Mollo &amp; Ortea, 1999" authorityName="Valdes, Mollo &amp; Ortea" authorityYear="1999" class="Gastropoda" family="Chromodorididae" genus="Chromodoris" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Chromodoris mandapamensis" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="species" species="mandapamensis">
