<caption id="DF55666DFFD6DE193951FC10B325FC3D" ID-DOI="http://doi.org/10.5281/zenodo.5372490" ID-Zenodo-Dep="5372490" httpUri="https://zenodo.org/record/5372490/files/figure.png" pageId="10" pageNumber="367" startId="10.[113,124,922,939]" targetBox="[132,1210,216,880]" targetPageId="10">
<paragraph id="8B9536E5FFD6DE193951FC10B325FC3D" blockId="10.[113,1211,922,984]" pageId="10" pageNumber="367">
