<taxonomicName id="4C571393FFD8385AFF0EC390FDC7FE52" authority="(Malloch)" baseAuthorityName="Malloch" baseAuthorityYear="1919" box="[151,515,403,430]" class="Insecta" family="Chironomidae" genus="Micropsectra" kingdom="Animalia" order="Diptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="polita">
