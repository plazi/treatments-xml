<quantity id="A410B4A41F40FFBBFEBCFD16FEF73008" box="[294,349,759,783]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="11" pageNumber="290" unit="mm" value="8.0">8 mm</quantity>
