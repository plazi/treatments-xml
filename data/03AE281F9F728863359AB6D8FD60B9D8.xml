<collectingRegion id="49C357EB9F728862348BB780FE44BDC4" box="[390,480,2009,2035]" country="Germany" name="Bayern" pageId="4" pageNumber="21">Bavaria</collectingRegion>
, 
<collectingCountry id="F310D9999F72886234E6B780FDF7BDC4" box="[491,595,2009,2035]" name="Germany" pageId="4" pageNumber="21">Germany</collectingCountry>
as 
<taxonomicName id="4C07E28A9F7288623775B780FB92BDC4" authority="Abonyi, 1928" authorityName="Abonyi" authorityYear="1928" box="[632,1078,2009,2035]" class="Oligohymenophorea" family="Lagenophryidae" genus="Lagenophrys" higherTaxonomySource="GBIF" kingdom="Chromista" order="Peritrichida" pageId="4" pageNumber="21" phylum="Ciliophora" rank="species" species="wallengreni">
