(Jurine, 1820)
</taxonomicName>
and on 
<taxonomicName id="4C07E28A9F728862368DB7EFFBC7BDF8" baseAuthorityName="Jurine" baseAuthorityYear="1820" box="[896,1123,1973,2000]" class="Ostracoda" family="Cyprididae" genus="Cypria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="4" pageNumber="21" phylum="Arthropoda" rank="species" species="ophthalmica">
