<taxonomicName id="8F0E53F8A49FFF0692CC21301057792F" authority="Cerretti &amp; Pape, 2020" authorityName="Cerretti &amp; Pape" authorityYear="2020" class="Insecta" family="Rhinophoridae" genus="Marshallicona" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Marshallicona quitu" order="Diptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="quitu">
