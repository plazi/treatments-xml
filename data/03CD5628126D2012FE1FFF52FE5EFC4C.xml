<taxonomicName id="4C649CBD126E2012FDF9FC8DFCBCFCCC" authority="Wittmer, 1997" authorityName="Wittmer" authorityYear="1997" box="[588,861,767,791]" class="Insecta" family="Cantharidae" genus="Micropodabrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="85" phylum="Arthropoda" rank="species" species="jendeki">
