<taxonomicName id="4C649CBD126E2012FE23FD52FD6EFD2C" authority="Švihla, 2005" authorityName="Švihla" authorityYear="2005" box="[406,655,735,759]" class="Insecta" family="Cantharidae" genus="Mimopodabrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="85" phylum="Arthropoda" rank="species" species="eduardi" status="sp. nov.">
