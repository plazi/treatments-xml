<taxonomicName id="EAEA4D0BFFB1FFB8FD26519C8765FE1F" authority="White, 1855" authorityName="White" authorityYear="1855" box="[661,1049,478,503]" class="Insecta" family="Cerambycidae" genus="Alcidion" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="257" phylum="Arthropoda" rank="species" species="carinatum">
