<emphasis id="FE8DEABCFFC9FFD6FF434DCDFED61DA4" bold="true" box="[151,276,1208,1230]" pageId="3" pageNumber="234">FIGURE 2.</emphasis>
<taxonomicName id="0BF94D2DFFC9FFD6FEC84DCDFE381DA7" box="[284,506,1208,1230]" class="Insecta" family="Cicadellidae" genus="Mendozellus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="234" phylum="Arthropoda" rank="species" species="asunctia">
