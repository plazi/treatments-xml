<taxonomicName id="4C674D31D303FFCCEB419C55FB07228B" box="[1094,1194,369,392]" class="Mammalia" family="Muridae" genus="Rattus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="9" pageNumber="504" phylum="Chordata" rank="species" species="rattus">
