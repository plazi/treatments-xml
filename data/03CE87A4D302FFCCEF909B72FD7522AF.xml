<taxonomicName id="4C674D31D303FFCCEC7E9C55FBA7228B" box="[889,1034,367,392]" class="Mammalia" family="Muridae" genus="Mus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="9" pageNumber="504" phylum="Chordata" rank="species" species="musculus">
