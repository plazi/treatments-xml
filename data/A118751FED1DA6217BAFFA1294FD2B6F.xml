<taxonomicName id="EEB1BF8AED1DA6217861FA7997D32B1A" box="[857,993,1519,1542]" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="6" pageNumber="178" phylum="Chordata" rank="species" species="auritus">
