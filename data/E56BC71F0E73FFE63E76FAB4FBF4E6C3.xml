<quantity id="AA3ADBEC0E71FFE63BB7FE29FAF6E3CD" box="[1275,1336,385,408]" metricMagnitude="0" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="70" pageNumber="197" unit="m" value="3.5" valueMax="4.0" valueMin="3.0">
