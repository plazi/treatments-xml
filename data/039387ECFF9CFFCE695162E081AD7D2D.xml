<taxonomicName id="4C3A4D79FF9EFFCE690867D882B67EA6" box="[337,484,484,509]" class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="genus">
