<collectingCountry id="F328763BFFEA043C7BAD4111FDA0C44C" box="[448,552,1895,1919]" name="Germany" pageId="5" pageNumber="88">Germany</collectingCountry>
(
<number id="3DF7248CFFEA043C78554111FDDDC44C" box="[568,597,1895,1919]" pageId="5" pageNumber="88" value="18.0">18</number>
&amp; 
<date id="FF81106BFFEA043C78154111FC85C44C" box="[632,781,1895,1920]" pageId="5" pageNumber="88" value="1973-05-23">
<number id="3DF7248CFFEA043C78154111FD1CC44C" box="[632,660,1895,1919]" pageId="5" pageNumber="88" value="23.0">23</number>
