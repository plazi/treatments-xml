<subSubSection pageId="null" pageNumber="316" type="vernacular_name">
<paragraph pageId="null" pageNumber="316">
<normalizedToken originalValue="Glänzende" pageId="null" pageNumber="316">Glaenzende</normalizedToken>
Kammschmiele
</paragraph>
