(
<taxonomicName box="[1267,1423,1344,1370]" class="Magnoliopsida" family="Myrsinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ericales" pageId="4" pageNumber="57" phylum="Tracheophyta" rank="family">Myrsinaceae</taxonomicName>
