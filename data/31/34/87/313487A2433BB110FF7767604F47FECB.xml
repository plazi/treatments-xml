<number box="[707,736,1821,1847]" pageId="1" pageNumber="54" value="21.0">21</number>
(
<number box="[753,829,1821,1847]" pageId="1" pageNumber="54" value="21.5" valueMax="24.0" valueMin="19.0">19–24</number>
