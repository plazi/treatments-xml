<subSubSection box="[199,1009,1123,1149]" pageId="6" pageNumber="431" type="etymology">
<paragraph blockId="6.[151,1437,151,1437]" box="[199,1009,1123,1149]" pageId="6" pageNumber="431">
<emphasis bold="true" box="[199,334,1123,1149]" pageId="6" pageNumber="431">Etymology.</emphasis>
