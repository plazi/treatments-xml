<taxonomicName class="Dicotyledoneae" family="Caricaceae" genus="P." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="P. polymitus" order="Violales" pageId="41" pageNumber="92" phylum="Angiospermae" rank="species" species="polymitus">
