(
<bibRefCitation author="Bhalerao" box="[815,986,1013,1038]" pageId="74" pageNumber="75" refString="Bhalerao, G. D. (1935) On two new monostomes from avian hosts in British India. Indian Journal of Veterinary Sciences and Animal Husbandry, 5, 49 - 63." type="journal article" year="1935">
Bhalerao, 
<number box="[932,986,1013,1038]" pageId="74" pageNumber="75" value="1935.0">1935</number>
</bibRefCitation>
)
</taxonomicName>
