<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[1076,1428,222,247]" class="Aves" family="Charadriidae" genus="Lobivanellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Charadriiformes" pageId="53" pageNumber="54" phylum="Chordata" rank="species" species="lobatus">
