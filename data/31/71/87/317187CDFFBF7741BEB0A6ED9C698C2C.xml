<subSubSection pageId="83" pageNumber="84" type="nomenclature">
<paragraph blockId="83.[151,1436,1229,1325]" box="[151,550,1229,1255]" pageId="83" pageNumber="84">
<heading bold="true" box="[151,550,1229,1255]" fontSize="11" level="1" pageId="83" pageNumber="84" reason="1">
<taxonomicName authority="Witenberg, 1923" authorityName="Witenberg" authorityYear="1923" box="[151,550,1229,1255]" class="Trematoda" family="Cyclocoelidae" genus="Promptenovum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Echinostomida" pageId="83" pageNumber="84" phylum="Platyhelminthes" rank="genus">
<emphasis bold="true" box="[151,550,1229,1255]" pageId="83" pageNumber="84">
<emphasis bold="true" box="[151,337,1229,1255]" italics="true" pageId="83" pageNumber="84">Promptenovum</emphasis>
<bibRefCitation author="Witenberg" box="[344,550,1229,1255]" pageId="83" pageNumber="84" refString="Witenberg, G. G. (1923) The trematodes of the family Cyclocoelidae and a new principle of their systematics. Trudy Gosudarstvennogo Institute Ҫeksperimental'noaei Veterinarii, 1, 84 - 141. [in Russian]" type="journal article" year="1923">
