<taxonomicName authority="Stossich, 1902" authorityName="Stossich" authorityYear="1902" box="[369,825,762,787]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="56" pageNumber="57" phylum="Platyhelminthes" rank="species" species="brasilianum">
