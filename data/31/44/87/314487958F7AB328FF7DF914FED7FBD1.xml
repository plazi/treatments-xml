, 
<taxonomicName box="[1104,1218,439,464]" class="Gastropoda" family="Pupinidae" genus="Pseudopomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="sophiae" status="n. sp.">
<emphasis box="[1104,1218,439,464]" italics="true" pageId="21" pageNumber="22">P. sophiae</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1224,1287,439,464]" pageId="21" pageNumber="22">
