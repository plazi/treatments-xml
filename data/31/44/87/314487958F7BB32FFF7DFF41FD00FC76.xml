(
<bibRefCitation author="Godwin-Austen" box="[395,643,818,843]" pageId="18" pageNumber="19" refString="Godwin-Austen, H. H. (1917) Zoological results of the Abor Expedition, 1911 - 12. XLVII Mollusca, VII. Cyclophoridae (in part) Records of the Indian Museum, 8, 493 - 580." type="journal article" year="1917">
Godwin-Austen 
<number box="[583,643,819,843]" pageId="18" pageNumber="19" value="1917.0">1917</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="Godwin-Austen 1917" authorityName="Godwin-Austen" authorityYear="1917" box="[662,1070,818,843]" class="Mollusca" family="Mollusca" genus="Eupomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="sibbumensis">
<emphasis box="[662,799,818,843]" italics="true" pageId="18" pageNumber="19">sibbumensis</emphasis>
(
<bibRefCitation author="Godwin-Austen" box="[813,1061,818,843]" pageId="18" pageNumber="19" refString="Godwin-Austen, H. H. (1917) Zoological results of the Abor Expedition, 1911 - 12. XLVII Mollusca, VII. Cyclophoridae (in part) Records of the Indian Museum, 8, 493 - 580." type="journal article" year="1917">
Godwin-Austen 
<number box="[1001,1061,819,843]" pageId="18" pageNumber="19" value="1917.0">1917</number>
</bibRefCitation>
)
</taxonomicName>
.
</paragraph>
</subSubSection>
<subSubSection pageId="18" pageNumber="19" type="distribution">
