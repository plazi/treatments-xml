(
<figureCitation box="[159,256,1473,1498]" captionStart="FIGURE 5" captionStartId="12.[151,249,1921,1943]" captionTargetBox="[224,1351,211,1896]" captionTargetId="figure-64@12.[209,1365,193,1898]" captionTargetPageId="12" captionText="FIGURE 5. Head of 3rd instar larvae of Myrmeleontini, ventral view; A, Myrmeleon formicarius; B, Myrmeleon gerlindae; C, Myrmeleon punicanus; D, Myrmeleon bore; E, Myrmeleon inconspicuus; F, Myrmeleon mariaemathildae; G, Myrmeleon hyalinus; H, Myrmeleon fasciatus; I, Euroleon nostras." figureDoi="http://doi.org/10.5281/zenodo.4909377" httpUri="https://zenodo.org/record/4909377/files/figure.png" pageId="56" pageNumber="57">Figs. 5E</figureCitation>
, 
<figureCitation box="[268,301,1473,1498]" captionStart="FIGURE 6" captionStartId="13.[151,249,1926,1948]" captionTargetBox="[245,1291,199,1896]" captionTargetId="figure-69@13.[229,1336,193,1898]" captionTargetPageId="13" captionText="FIGURE 6. IX abdominal sternite of 3rd instar larvae of Myrmeleontini, ventral view; A, Myrmeleon gerlindae; B, Myrmeleon punicanus; C, Myrmeleon formicarius; D, Myrmeleon bore; E, Myrmeleon inconspicuus; F, Myrmeleon mariaemathildae; G, Myrmeleon hyalinus; H, Myrmeleon fasciatus; I, Euroleon nostras. Scale bar: 1 mm." figureDoi="http://doi.org/10.5281/zenodo.4909381" httpUri="https://zenodo.org/record/4909381/files/figure.png" pageId="56" pageNumber="57">6E</figureCitation>
, 
<figureCitation box="[313,342,1473,1498]" captionStart="FIGURE 30" captionStartId="58.[151,249,1932,1954]" captionTargetBox="[238,1353,206,1903]" captionTargetId="figure-46@58.[229,1357,193,1908]" captionTargetPageId="58" captionText="FIGURE 30. Myrmeleon inconspicuus Rambur, 1842, 3rd instar larva (Italy: Veneto, Venezia, Punta Sabbioni). Dorsal (above), ventral (middle) and lateral (below) view; a–c: diagnostic characters, see species description." figureDoi="http://doi.org/10.5281/zenodo.4909475" httpUri="https://zenodo.org/record/4909475/files/figure.png" pageId="56" pageNumber="57">30</figureCitation>
