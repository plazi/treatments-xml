</taxonomicName>
and 
<taxonomicName authority="Larsen and Patterson, 1990" authorityName="Larsen and Patterson" authorityYear="1990" box="[481,968,137,161]" class="Imbricatea" family="Thaumatomastigidae" genus="Protaspis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Thaumatomonadida" pageId="64" pageNumber="545" phylum="Cercozoa" rank="species" species="verrucosa">
