, 
<taxonomicName authority="Skuja, 1948" authorityName="Skuja" authorityYear="1948" class="Imbricatea" family="Thaumatomastigidae" genus="Protaspis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Thaumatomonadida" pageId="63" pageNumber="544" phylum="Cercozoa" rank="species" species="obovata">
