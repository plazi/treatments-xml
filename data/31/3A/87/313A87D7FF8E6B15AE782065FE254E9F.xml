<taxonomicName authorityName="S.M.Tong" authorityYear="1997" box="[221,379,455,479]" class="Bicoecea" family="Placidiaceae" genus="Pendulomonas" higherTaxonomySource="GBIF" kingdom="Chromista" order="Placidiida" pageId="59" pageNumber="540" phylum="Bigyra" rank="genus">
