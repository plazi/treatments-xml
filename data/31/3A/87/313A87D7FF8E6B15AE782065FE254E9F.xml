<taxonomicName authority="Tong, 1997" authorityName="Tong" authorityYear="1997" class="Bicoecea" family="Placidiaceae" genus="Pendulomonas" higherTaxonomySource="GBIF" kingdom="Chromista" order="Placidiida" pageId="59" pageNumber="540" phylum="Bigyra" rank="species" species="adriperis">
