<taxonomicName authorityName="Stein" authorityYear="1878" baseAuthorityName="Ehrenberg" baseAuthorityYear="1838" box="[851,1027,917,941]" class="Euglenoidea" family="Peranemaceae" genus="Heteronema" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Peranemida" pageId="20" pageNumber="501" phylum="Euglenozoa" rank="species" species="globuliferum">
<emphasis box="[851,1027,917,941]" italics="true" pageId="20" pageNumber="501">H. globuliferum</emphasis>
