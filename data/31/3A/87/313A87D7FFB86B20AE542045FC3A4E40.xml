, 
<collectingCountry box="[792,832,1572,1596]" name="Fiji" pageId="13" pageNumber="494">Fiji</collectingCountry>
, Gulf of 
<collectingCountry box="[938,1027,1572,1596]" name="Finland" pageId="13" pageNumber="494">Finland</collectingCountry>
, 
<collectingCountry box="[79,197,1604,1628]" name="Greenland" pageId="13" pageNumber="494">Greenland</collectingCountry>
