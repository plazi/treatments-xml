<potCollectionCode box="[189,277,485,511]" pageId="14" pageNumber="15">LENGTH</potCollectionCode>
. Body 
<quantity box="[362,455,485,511]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="14" pageNumber="15" unit="mm" value="1.8">1.8 mm</quantity>
, fore wing 
<quantity box="[587,680,485,511]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="14" pageNumber="15" unit="mm" value="2.4">2.4 mm</quantity>
, hind wing 
