<paragraph blockId="32.[189,363,1818,1844]" box="[189,363,1818,1844]" pageId="32" pageNumber="33">
<heading bold="true" box="[189,363,1818,1844]" fontSize="11" level="3" pageId="32" pageNumber="33" reason="3">
<emphasis bold="true" box="[189,363,1818,1844]" pageId="32" pageNumber="33">Redescription</emphasis>
