<subSubSection box="[189,342,1745,1819]" pageId="21" pageNumber="22" type="distribution">
<paragraph blockId="21.[189,342,1745,1771]" box="[189,342,1745,1771]" pageId="21" pageNumber="22">
<heading bold="true" box="[189,342,1745,1771]" fontSize="11" level="3" pageId="21" pageNumber="22" reason="3">
<emphasis bold="true" box="[189,342,1745,1771]" pageId="21" pageNumber="22">Distribution</emphasis>
</heading>
