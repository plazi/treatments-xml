<taxonomicName authorityName="Gibson" authorityYear="1976" baseAuthorityName="Yamaguti" baseAuthorityYear="1968" box="[650,789,132,156]" class="Monogenea" family="Capsalidae" genus="Menziesia" kingdom="Animalia" order="Capsalidea" pageId="89" pageNumber="750" phylum="Platyhelminthes" rank="species" species="merinthe">
