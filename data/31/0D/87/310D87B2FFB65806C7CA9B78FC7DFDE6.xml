<collectingRegion box="[230,358,1154,1173]" country="Indonesia" name="Sumatera Selatan" pageId="11" pageNumber="830">South Sumatra</collectingRegion>
, 
<collectingCountry box="[367,455,1154,1173]" name="Indonesia" pageId="11" pageNumber="830">Indonesia</collectingCountry>
].
</paragraph>
