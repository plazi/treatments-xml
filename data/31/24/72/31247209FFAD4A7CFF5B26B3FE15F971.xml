<emphasis italics="true" pageId="17" pageNumber="138">
<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Leptactina" kingdom="Plantae" order="Gentianales" pageId="17" pageNumber="138" phylum="Tracheophyta" rank="species" species="delagoensis">Leptactina delagoensis</taxonomicName>
