<subSubSection pageId="3" pageNumber="64" type="reference_group">
<paragraph pageId="3" pageNumber="64">
<taxonomicName class="Insecta" family="Heptageniidae" genus="Epeorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Epeorus (Belovius) khayengensis" order="Ephemeroptera" pageId="3" pageNumber="64" phylum="Arthropoda" rank="species" species="khayengensis" subGenus="Belovius">Epeorus (Belovius) khayengensis</taxonomicName>
