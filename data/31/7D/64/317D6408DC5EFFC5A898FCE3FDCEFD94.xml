<typeStatus box="[557,598,380,401]" pageId="16" pageNumber="17">type</typeStatus>
locality, 
<emphasis bold="true" box="[210,303,407,428]" pageId="16" pageNumber="17">
<collectingCountry box="[210,303,407,428]" name="Brazil" pageId="16" pageNumber="17">BRAZIL</collectingCountry>
