, 
<figureCitation box="[405,443,816,837]" captionStart="Figs" captionStartId="57.[175,215,1345,1364]" captionTargetBox="[211,1107,228,1255]" captionTargetId="figure-156@57.[151,1173,183,1322]" captionTargetPageId="57" captionText="Figs. 169–175. Antennae of Ichnea spp. 169) I. mimica; 170) I. lycoides; 171) I. frenata; 172) I. opaca; 173) I. praeusta; 174) I. procera; 175) I. callanga." figureDoi="http://doi.org/10.5281/zenodo.4908685" httpUri="https://zenodo.org/record/4908685/files/figure.png" pageId="40" pageNumber="41">172</figureCitation>
, 
<figureCitation box="[455,493,816,837]" captionStart="Figs" captionStartId="58.[197,238,1661,1680]" captionTargetBox="[271,1086,196,1603]" captionTargetId="figure-0@58.[173,1193,121,1660]" captionTargetPageId="58" captionText="Figs. 176–190. Aedeagi of Ichnea spp. 176) I. plumbea; 177) I. gremia; 178) I. digna; 179) I. callanga; 180) I. frenata; 181) I. procera; 182) I. opaca; 183) I. divisa; 184) I. incerta; 185) I. aequinoctialis; 186) I. gregata; 187) I. acanthomelina; 188) I. marginella; 189) I. aterrima; 190) I. lycoides." figureDoi="http://doi.org/10.5281/zenodo.4908687" httpUri="https://zenodo.org/record/4908687/files/figure.png" pageId="40" pageNumber="41">182</figureCitation>
; 
<figureCitation box="[505,569,816,837]" captionStart="Map 5" captionStartId="38.[197,239,1427,1446]" captionTargetBox="[180,1188,193,1392]" captionTargetId="figure-121@38.[173,1195,183,1404]" captionTargetPageId="38" captionText="Map 5. Geographic distribution of Ichnea spp." figureDoi="http://doi.org/10.5281/zenodo.4908641" httpUri="https://zenodo.org/record/4908641/files/figure.png" pageId="40" pageNumber="41">Map 5</figureCitation>
