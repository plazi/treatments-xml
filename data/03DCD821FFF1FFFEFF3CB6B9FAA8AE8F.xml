<emphasis id="B901B525FFF1FFFEFE09B70BFDDBAE5E" bold="true" box="[418,516,773,798]" pageId="9" pageNumber="519">1090458</emphasis>
, 11/930, [
<geoCoordinate id="EE410FF0FFF1FFFEFDDEB708FD00AE5E" box="[629,735,774,798]" direction="south" orientation="latitude" pageId="9" pageNumber="519" precision="55" value="-70.142">70.142°S</geoCoordinate>
<geoCoordinate id="EE410FF0FFF1FFFEFD4DB708FCB3AE5E" box="[742,876,774,798]" direction="west" orientation="longitude" pageId="9" pageNumber="519" precision="55" value="-109.808">109.808°W</geoCoordinate>
], 
<quantity id="4C8DC4D2FFF1FFFEFCD5B708FC09AE5D" box="[894,982,774,798]" metricMagnitude="3" metricUnit="m" metricValue="1.391" pageId="9" pageNumber="519" unit="m" value="1391.0">1391 m</quantity>
