<taxonomicName id="8ED58B3DF1F866B7D94AE710D05E3B94" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25288" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus myops Emery" order="Hymenoptera" pageId="15" pageNumber="564" phylum="Arthropoda" rank="species" species="myops">myops</taxonomicName>
, 
<taxonomicName id="89FA379F575631298AA6BD5AACFA44AF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25304" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus pupulatus Brown" order="Hymenoptera" pageId="15" pageNumber="564" phylum="Arthropoda" rank="species" species="pupulatus">pupulatus</taxonomicName>
