<taxonomicName id="4797AD8EAA91C521D642DDBD916B87D1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25317" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus subcoecus Forel" order="Hymenoptera" pageId="15" pageNumber="564" phylum="Arthropoda" rank="species" species="subcoecus">subcoecus</taxonomicName>
