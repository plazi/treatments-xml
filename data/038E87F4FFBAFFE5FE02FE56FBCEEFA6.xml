<collectingRegion id="49E3F800FFBAFFE5FD61FCE9FCC6EFA6" box="[743,830,888,912]" country="Saudi Arabia" name="Ash Sharqiyah" pageId="11" pageNumber="12">Oriental</collectingRegion>
