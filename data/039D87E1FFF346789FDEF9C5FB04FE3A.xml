<number id="3DFC24D0FFF0467E9A14F960FADBF904" box="[1373,1402,1750,1774]" pageId="2" pageNumber="293" value="29.0">29</number>
/ 
<number id="3DFC24D0FFF0467E9ACBF960FA3DF904" box="[1410,1436,1750,1774]" pageId="2" pageNumber="293" value="31.0">31</number>
