<caption id="DF4B667FFFF1467F9FDEF8A7FD14F88E" httpUri="https://zenodo.org/record/228809/files/figure.png" pageId="3" pageNumber="294" targetBox="[151,1436,481,1787]" targetPageId="3">
<paragraph id="8B8B36F7FFF1467F9FDEF8A7FD14F88E" blockId="3.[151,1435,1809,1892]" pageId="3" pageNumber="294">
