<taxonomicName id="A2AB4D0EA0657A596421FA7E6ADBF98F" authorityName="Chen and Toda, 2001:" authorityYear="1542" box="[562,698,1532,1556]" class="Insecta" family="Drosophilidae" genus="Amiota" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="294" phylum="Arthropoda" rank="species" species="cuii" subGenus="Amiota">
