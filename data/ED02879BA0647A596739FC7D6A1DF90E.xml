<taxonomicName id="A2AB4D0EA0657A596524F9BE6B3FF9CF" authorityName="Chen and Toda, 2001:" authorityYear="1542" box="[823,862,1596,1620]" class="Insecta" family="Drosophilidae" genus="Amiota" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="288" phylum="Arthropoda" rank="species" species="cuii">
