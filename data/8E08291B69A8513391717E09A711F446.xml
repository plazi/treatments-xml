<taxonomicName id="F4104807296B114D6AB94116D9A5E6E9" authorityName="Li &amp; Wang &amp; Hua" authorityYear="2021" class="Insecta" family="Panorpidae" genus="Panorpa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Panorpa uncinata" order="Mecoptera" pageId="0" pageNumber="309" phylum="Arthropoda" rank="species" species="uncinata">
