<collectingCountry box="[692,792,1887,1911]" name="Thailand" pageId="21" pageNumber="22">Thailand</collectingCountry>
(
<taxonomicName box="[811,1019,1888,1912]" class="Arachnida" family="Stylocellidae" genus="Fangensis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="leclerci">
<emphasis box="[811,1019,1888,1912]" italics="true" pageId="21" pageNumber="22">Fangensis leclerci</emphasis>
</taxonomicName>
) and Kaeng Lawa Cave (
<taxonomicName class="Arachnida" family="Stylocellidae" genus="Fangensis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="spelaeus">
<emphasis italics="true" pageId="21" pageNumber="22">Fangensis spelaeus</emphasis>
</taxonomicName>
) and Tham Nam Cave (
<taxonomicName box="[518,767,1925,1948]" class="Arachnida" family="Stylocellidae" genus="Fangensis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="cavernarus">
<emphasis box="[518,767,1925,1948]" italics="true" pageId="21" pageNumber="22">Fangensis cavernarus</emphasis>
