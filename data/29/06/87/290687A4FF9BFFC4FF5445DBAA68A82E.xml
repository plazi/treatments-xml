c 
<number box="[766,780,1498,1522]" pageId="4" pageNumber="239" value="3.0">3</number>
</emphasis>
, 
<emphasis box="[795,824,1497,1522]" italics="true" pageId="4" pageNumber="239">
d 
<number box="[812,824,1497,1522]" pageId="4" pageNumber="239" value="1.0">1</number>
</emphasis>
, 
<emphasis box="[838,950,1497,1522]" italics="true" pageId="4" pageNumber="239">
e 
<number box="[852,866,1498,1522]" pageId="4" pageNumber="239" value="1.0">1</number>
, h 
