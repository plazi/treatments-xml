<number box="[314,326,1570,1594]" pageId="4" pageNumber="239" value="1.0">1</number>
a
</emphasis>
, 
<emphasis box="[361,389,1570,1594]" italics="true" pageId="4" pageNumber="239">
<number box="[361,375,1570,1594]" pageId="4" pageNumber="239" value="3.0">3</number>
a
</emphasis>
, 
<emphasis box="[409,437,1570,1594]" italics="true" pageId="4" pageNumber="239">
<number box="[409,423,1570,1594]" pageId="4" pageNumber="239" value="4.0">4</number>
a
