, 
<emphasis box="[351,365,1643,1666]" italics="true" pageId="4" pageNumber="239">σ</emphasis>
, 
<emphasis box="[378,404,1641,1666]" italics="true" pageId="4" pageNumber="239">kT</emphasis>
, 
<emphasis box="[418,433,1643,1666]" italics="true" pageId="4" pageNumber="239">φ</emphasis>
, 
