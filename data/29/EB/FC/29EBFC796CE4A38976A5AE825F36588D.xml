<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2414" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus Mayr" order="Hymenoptera" pageId="1" pageNumber="281" phylum="Arthropoda" rank="genus">Camponotus</taxonomicName>
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:193318" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus Edmondi Andr?©" order="Hymenoptera" pageId="1" pageNumber="281" phylum="Arthropoda" rank="species" species="Edmondi">Edmondi</taxonomicName>
