<taxonomicName authorityName="Fabricius" authorityYear="1798" box="[827,966,1098,1122]" class="Insecta" family="Calopterygidae" genus="Hetaerina" kingdom="Animalia" order="Odonata" pageId="5" pageNumber="490" phylum="Arthropoda" rank="species" species="americana">
