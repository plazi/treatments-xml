<emphasis bold="true" box="[132,143,1985,2002]" pageId="23" pageNumber="46">b</emphasis>
, antennal groove; 
<emphasis bold="true" box="[296,314,1985,2005]" pageId="23" pageNumber="46">
b 
<subScript attach="left" box="[307,314,1993,2005]" fontSize="5" pageId="23" pageNumber="46">1</subScript>
</emphasis>
, hepatic groove; 
<emphasis bold="true" box="[457,468,1985,2002]" pageId="23" pageNumber="46">c</emphasis>
, postcervical groove; 
<emphasis bold="true" box="[650,661,1985,2002]" pageId="23" pageNumber="46">d</emphasis>
, gastro-orbital groove; 
<emphasis bold="true" box="[855,871,1985,2002]" pageId="23" pageNumber="46">di</emphasis>
, diaeresis; 
<emphasis bold="true" box="[964,993,1985,2005]" pageId="23" pageNumber="46">
e 
<subScript attach="both" box="[975,982,1993,2005]" fontSize="5" pageId="23" pageNumber="46">1</subScript>
e
</emphasis>
, cervical groove; 
<emphasis bold="true" box="[1138,1143,1985,2002]" pageId="23" pageNumber="46">i</emphasis>
, inferior groove; 
<emphasis bold="true" box="[1281,1297,1985,2002]" pageId="23" pageNumber="46">ip</emphasis>
, intercalated plate; 
<emphasis bold="true" box="[132,181,2008,2025]" pageId="23" pageNumber="46">Mxp3</emphasis>
