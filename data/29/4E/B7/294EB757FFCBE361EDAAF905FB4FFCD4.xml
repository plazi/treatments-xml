and 
<taxonomicName authority="VIEIRA &amp; JONES &amp; WINSTON, 2013" authorityName="VIEIRA &amp; JONES &amp; WINSTON" authorityYear="2013" box="[516,644,656,679]" class="Gymnolaemata" family="Candidae" genus="Cradoscrupocellaria" kingdom="Animalia" order="Cheilostomatida" pageId="21" pageNumber="22" phylum="Bryozoa" rank="species" species="normani" status="n. sp.">
