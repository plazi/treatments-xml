<taxonomicName authority="VIEIRA &amp; JONES &amp; WINSTON, 2013" authorityName="VIEIRA &amp; JONES &amp; WINSTON" authorityYear="2013" box="[228,392,656,679]" class="Gymnolaemata" family="Candidae" genus="Cradoscrupocellaria" kingdom="Animalia" order="Cheilostomatida" pageId="21" pageNumber="22" phylum="Bryozoa" rank="species" species="jamaicensis" status="n. sp.">
<emphasis box="[228,247,656,679]" italics="true" pageId="21" pageNumber="22">C</emphasis>
. 
<emphasis box="[260,392,656,679]" italics="true" pageId="21" pageNumber="22">jamaicensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[399,462,655,680]" pageId="21" pageNumber="22">
<taxonomicNameLabel box="[399,462,655,680]" pageId="21" pageNumber="22" rank="species">n. sp.</taxonomicNameLabel>
