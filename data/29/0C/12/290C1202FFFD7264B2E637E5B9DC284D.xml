<subSubSection box="[199,1193,1231,1256]" pageId="3" pageNumber="299" type="etymology">
<paragraph blockId="3.[151,1437,151,1365]" box="[199,1193,1231,1256]" pageId="3" pageNumber="299">
<emphasis bold="true" box="[199,334,1231,1256]" pageId="3" pageNumber="299">Etymology.</emphasis>
