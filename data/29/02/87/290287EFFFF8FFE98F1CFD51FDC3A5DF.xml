<taxonomicName baseAuthorityName="Daudin" baseAuthorityYear="1802" box="[220,433,874,897]" class="Amphibia" family="Bufonidae" genus="Caiman" kingdom="Animalia" order="Anura" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="latirostris">
