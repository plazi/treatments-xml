<taxonomicName authorityName="Barbour &amp; Loveridge" authorityYear="1929" box="[351,621,1382,1405]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="mabouia">
