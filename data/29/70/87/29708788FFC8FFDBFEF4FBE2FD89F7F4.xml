<taxonomicName authority="Retowski, 1889" authorityName="Retowski" authorityYear="1889" box="[250,688,1199,1219]" class="Gastropoda" family="Enidae" genus="Buliminus" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="14" pageNumber="17" phylum="Mollusca" rank="species" species="biplicatus" subGenus="Brephulus">
