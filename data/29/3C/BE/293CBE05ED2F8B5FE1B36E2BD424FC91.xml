<date pageId="5" pageNumber="66" value="2006-10-06">
<number box="[1315,1330,1925,1951]" pageId="5" pageNumber="66" value="6.0">6</number>
October 
<number box="[151,213,1965,1991]" pageId="5" pageNumber="66" value="2006.0">2006</number>
