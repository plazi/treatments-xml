<collectingCountry box="[364,498,1585,1611]" name="Bangladesh" pageId="6" pageNumber="457">Bangladesh</collectingCountry>
, 
<collectingCountry box="[512,572,1585,1611]" name="India" pageId="6" pageNumber="457">India</collectingCountry>
, 
<collectingCountry box="[588,697,1586,1612]" name="Myanmar" pageId="6" pageNumber="457">Myanmar</collectingCountry>
(Anichtchenko 2017; 
<bibRefCitation author="Lorenz, W." box="[961,1109,1585,1612]" pageId="6" pageNumber="457" pagination="2000" refId="ref27320" refString="Lorenz, W. (2018) CarabCat: Global database of ground beetles. Version October 2017. In: Roskov, Y., Abucay, L., Orrell, T., Nicolson, D., Bailly, N., Kirk, P. M., Bourgoin, T., DeWalt, R. E., Decock, W., De Wever, A., Nieukerken, E. van, Zarucchi, J. &amp; Penev, L. (Eds.), Species 2000 &amp; ITIS Catalogue of Life, 30 th June 2018. Species 2000: Naturalis, Leiden, Available from: https: // www. catalogueoflife. org / col (accessed 20 July 2020)" type="book chapter" year="2018">Lorenz 2018</bibRefCitation>
) and 
<collectingCountry box="[1179,1274,1585,1611]" name="Pakistan" pageId="6" pageNumber="457">Pakistan</collectingCountry>
