(mean value: 5.5 ± 
<quantity box="[813,905,1159,1185]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="19" pageNumber="270" unit="mm" value="0.2">0.2 mm</quantity>
); 
<typeStatus box="[927,1029,1159,1185]" pageId="19" pageNumber="270">holotype</typeStatus>
: 
<quantity box="[1042,1133,1158,1185]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" pageId="19" pageNumber="270" unit="mm" value="5.6">5.6 mm</quantity>
. TL-h: 
<quantity box="[1220,1357,1158,1185]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" metricValueMax="5.1" metricValueMin="4.7" pageId="19" pageNumber="270" unit="mm" value="4.9" valueMax="5.1" valueMin="4.7">4.7–5.1 mm</quantity>
(mean value: 4.9 ± 
<quantity box="[291,381,1195,1221]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="19" pageNumber="270" unit="mm" value="0.1">0.1 mm</quantity>
); 
<typeStatus box="[401,503,1195,1221]" pageId="19" pageNumber="270">holotype</typeStatus>
: 
<quantity box="[513,602,1194,1221]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" pageId="19" pageNumber="270" unit="mm" value="5.1">5.1 mm</quantity>
. MW: 
<quantity box="[678,813,1194,1221]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" metricValueMax="2.8" metricValueMin="2.5" pageId="19" pageNumber="270" unit="mm" value="2.65" valueMax="2.8" valueMin="2.5">2.5–2.8 mm</quantity>
(mean value: 2.7 ± 
<quantity box="[1036,1126,1195,1221]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="19" pageNumber="270" unit="mm" value="0.1">0.1 mm</quantity>
