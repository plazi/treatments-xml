<taxonomicName id="4C55D09CFFC2F77AFE7248460BE8F8C8" authorityName="Prell" authorityYear="1914" box="[432,673,1828,1851]" class="Insecta" family="Dynastidae" genus="Megaceras" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="28" phylum="Arthropoda" rank="species" species="crassum">
<emphasis id="B921770DFFC2F77AFE7248460BE8F8C8" box="[432,673,1828,1851]" italics="true" pageId="29" pageNumber="28">Megaceras crassum</emphasis>
</taxonomicName>
is widely distributed in 
<collectingCountry id="F342EB8FFFC2F77AFC0C48410D70F8C8" box="[974,1081,1827,1851]" name="Ecuador" pageId="29" pageNumber="28">Ecuador</collectingCountry>
, 
<collectingCountry id="F342EB8FFFC2F77AFB8648460DCBF8C8" box="[1092,1154,1828,1851]" name="Peru" pageId="29" pageNumber="28">Peru</collectingCountry>
, 
<collectingCountry id="F342EB8FFFC2F77AFB4D48410C4EF8C8" box="[1167,1287,1827,1851]" name="Colombia" pageId="29" pageNumber="28">Colombia</collectingCountry>
, 
<collectingCountry id="F342EB8FFFC2F77AFAD048410C23F8C8" box="[1298,1386,1827,1851]" name="Bolivia" pageId="29" pageNumber="28">Bolivia</collectingCountry>
