<taxonomicName id="3EA04D828B7BFFCF6088FBE8FCE77756" box="[728,810,1125,1151]" class="Aves" family="Picidae" kingdom="Animalia" order="Piciformes" pageId="15" pageNumber="16" phylum="Chordata" rank="family">Picidae</taxonomicName>
(
<taxonomicName id="3EA04D828B7BFFCF6169FBE8FC7477A9" box="[825,953,1125,1152]" class="Aves" kingdom="Animalia" order="Piciformes" pageId="15" pageNumber="16" phylum="Chordata" rank="order">Piciformes</taxonomicName>
