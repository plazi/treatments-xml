</taxonomicName>
(
<taxonomicName id="3EA04D828B7BFFCF61E6FB90FBFA7711" box="[950,1079,1053,1080]" class="Aves" family="Cotingidae" kingdom="Animalia" order="Passeriformes" pageId="15" pageNumber="16" phylum="Chordata" rank="family">Cotingidae</taxonomicName>
