<taxonomicName id="A29C4D54FFC1FF96A4D7CB5DFF68FDB4" authorityName="Linnaeus" authorityYear="1753" box="[164,254,590,617]" class="Magnoliopsida" family="Chrysobalanaceae" genus="Hirtella" kingdom="Plantae" order="Malpighiales" pageId="16" pageNumber="192" phylum="Tracheophyta" rank="genus">
