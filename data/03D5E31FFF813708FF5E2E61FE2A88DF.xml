<elevation id="0051B53AFF873708FDA02C2FFD4188BF" box="[588,664,373,395]" metricMagnitude="2" metricUnit="m" metricValue="1.6" pageId="4" pageNumber="1325" unit="m" value="160.0">160 m</elevation>
