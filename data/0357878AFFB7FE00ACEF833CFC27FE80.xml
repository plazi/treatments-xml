<taxonomicName id="4CFE4D1FFFB0FE00ACEF875CFE92FEE3" box="[178,334,277,299]" class="Magnoliopsida" family="Campanulaceae" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="1033" phylum="Tracheophyta" rank="family">Campanulaceae</taxonomicName>
