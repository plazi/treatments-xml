<taxonomicName id="4CFE4D1FFFB0FE00AE6886B1FCC3FEC6" authority=", Compositae" authorityName="Compositae" box="[565,799,248,270]" class="Magnoliopsida" family="Araliaceae" kingdom="Plantae" order="Apiales" pageId="4" pageNumber="1033" phylum="Tracheophyta" rank="family">Araliaceae, Compositae</taxonomicName>
