<collectingCountry id="F3E9760CFFB0FE00ADF7877BFDFAFE80" box="[426,550,306,328]" name="Germany" pageId="4" pageNumber="1033">Deutschland</collectingCountry>
, 
<collectingCountry id="F3E9760CFFB0FE00AE72877BFDBBFE80" box="[559,615,306,328]" name="Poland" pageId="4" pageNumber="1033">Polen</collectingCountry>
