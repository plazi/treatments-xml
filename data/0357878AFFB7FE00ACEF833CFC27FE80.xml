<taxonomicName id="4CFE4D1FFFB0FE00ACEF86B1FE44FEC6" authority=", Labiatae" authorityName="Labiatae" box="[178,408,248,270]" class="Magnoliopsida" family="Boraginaceae" kingdom="Plantae" order="Boraginales" pageId="4" pageNumber="1033" phylum="Tracheophyta" rank="family">Boraginaceae, Labiatae</taxonomicName>
