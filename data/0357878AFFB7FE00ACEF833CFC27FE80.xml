<taxonomicName id="4CFE4D1FFFB0FE00AEC2875CFD24FEE3" authorityName="de Jussieu" authorityYear="1789" box="[671,760,277,299]" class="Liliopsida" family="Liliaceae" kingdom="Plantae" order="Liliales" pageId="4" pageNumber="1033" phylum="Tracheophyta" rank="family">Liliaceae</taxonomicName>
