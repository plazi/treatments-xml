<taxonomicName id="4C0B4D2BFFB2A512CC58C695FB90A0FD" box="[885,1033,1733,1758]" class="Gastropoda" family="Conidae" genus="Conus" kingdom="Animalia" order="Neogastropoda" pageId="11" pageNumber="12" phylum="Mollusca" rank="species" species="achatinus">
<emphasis id="B97FEABAFFB2A512CC58C695FB90A0FD" box="[885,1033,1733,1758]" italics="true" pageId="11" pageNumber="12">C. achatinus</emphasis>
</taxonomicName>
from 
<collectingCountry id="F31C7638FFB2A512CB62C695FB15A0FC" box="[1103,1164,1733,1759]" name="India" pageId="11" pageNumber="12">India</collectingCountry>
