<taxonomicName id="97CC26F3202C405E09DED0D95F801B81" box="[422,646,942,967]" class="Insecta" family="Cleridae" genus="Hydnocera" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="317" phylum="Arthropoda" rank="species" species="testacea">
