<subSubSection id="14C737B8FF867C79D7619E14FEE3FD73" pageId="29" pageNumber="30" type="distribution">
<paragraph id="5C626433FF867C79D7619E14FEE3FD73" blockId="29.[98,1066,591,687]" pageId="29" pageNumber="30">
<emphasis id="6EA9B821FF867C79D7619E14FF7BFDB5" bold="true" box="[98,249,591,617]" pageId="29" pageNumber="30">Distribution</emphasis>
: 
<taxonomicName id="9BDD1FB0FF867C79D60F9E0BFEFFFDB6" box="[268,381,592,618]" class="Insecta" family="Scarabaeidae" genus="Ateuchus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="costatus">
<emphasis id="6EA9B821FF867C79D60F9E0BFEFFFDB6" box="[268,381,592,618]" italics="true" pageId="29" pageNumber="30">S. costatus</emphasis>
</taxonomicName>
