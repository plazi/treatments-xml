<taxonomicName id="B5E14D6F246ABD33FF64DA9019C8A7EC" authority="Willdenow (1805: 593)" authorityName="Willdenow" authorityPageNumber="593" authorityYear="1805" box="[196,759,1233,1261]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="116" pageNumber="117" phylum="Tracheophyta" rank="species" species="pinnatifrons">
<emphasis id="4095EAFE246ABD33FF64DA901AE8A7EA" bold="true" box="[196,471,1233,1260]" italics="true" pageId="116" pageNumber="117">Geonoma pinnatifrons</emphasis>
<bibRefCitation id="16704B1D246ABD33FE7FDA9119C8A7EC" author="Willdenow, C." box="[479,759,1235,1261]" journalOrPublisher="G. C. Nauk, Berlin, Germany" pageId="116" pageNumber="117" pagination="1 - 629" refId="ref124951" refString="Willdenow, C. (1805) Species Plantarum. Vol. 4, part 1. G. C. Nauk, Berlin, Germany, pp. 1 - 629." title="Species Plantarum" type="book chapter" year="1805">Willdenow (1805: 593)</bibRefCitation>
