<taxonomicName id="0AA04D6ECA29FFB6FAEBFA51FA1CFA18" authorityName="Zwick" authorityYear="1980" box="[1350,1437,1491,1513]" class="Insecta" family="Perlodidae" genus="Neofilchneria" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="569" phylum="Arthropoda" rank="species" species="erberi">
