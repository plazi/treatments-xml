<subSubSection id="C346656546D0C7B1DAC6F9D0F72AC405" lastPageId="198" lastPageNumber="1515" pageId="196" pageNumber="1513" type="materials_examined">
<paragraph id="8BE336EE46D0C7B3DAC6F9D0F408C035" blockId="196.[178,868,1475,1600]" box="[178,600,1580,1600]" pageId="196" pageNumber="1513">
<typeStatus id="54E7884C46D0C7B3DAC6F9D0F6B1C035" box="[178,225,1580,1600]" pageId="196" pageNumber="1513">Type</typeStatus>
species: 
<taxonomicName id="4C5C4D6D46D0C7B3DB40F9D0F408C035" authority="CAMERON." box="[308,600,1580,1600]" class="Insecta" family="Ichneumonidae" genus="Pachyjoppa" kingdom="Animalia" order="Hymenoptera" pageId="196" pageNumber="1513" phylum="Arthropoda" rank="species" species="tibialis">
