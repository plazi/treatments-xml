<number id="3DC224FDDB1F2712FD95ACA4B732E0D6" box="[534,550,444,470]" pageId="10" pageNumber="11" value="4.0">4</number>
): Body length, 
<quantity id="4CF29B3FDB1F2712FD59ACA4B670E0D6" box="[730,868,444,470]" metricMagnitude="-3" metricUnit="m" metricValue="9.25" metricValueMax="10.0" metricValueMin="8.5" pageId="10" pageNumber="11" unit="mm" value="9.25" valueMax="10.0" valueMin="8.5">8.5–10 mm</quantity>
. Wing length, 
<quantity id="4CF29B3FDB1F2712FB8DACA4B1B9E0D6" box="[1038,1197,444,470]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.15" metricValueMin="0.95" pageId="10" pageNumber="11" unit="mm" value="10.5" valueMax="11.5" valueMin="9.5">9.5–11.5 mm</quantity>
