<paragraph id="07E45FE3104DB2A4EE280CB105E02DE2" pageNumber="130">
<taxonomicName id="D0710F3574809786527A26738D0B179B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138393" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica foreli Bondroit" order="Hymenoptera" pageNumber="130" phylum="Arthropoda" rank="species" species="foreli">Formica foreli Emery</taxonomicName>
