<taxonomicName id="4C2901BE04011552FED448EEFCCDF915" authority="Bouchet, 1985" authorityName="Bouchet" authorityYear="1985" box="[503,806,1734,1761]" class="Gastropoda" family="Triphoridae" genus="Pogonodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="8" pageNumber="9" phylum="Mollusca" rank="genus">
