<taxonomicName id="4C2901BE04011552FB6E48EEFAB2F915" authority="Cope, 1880" authorityName="Cope" authorityYear="1880" box="[1101,1369,1735,1761]" class="Gastropoda" family="Triphoridae" genus="Pogonodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="8" pageNumber="9" phylum="Mollusca" rank="genus">
<emphasis id="B95DA62F04011552FB6E48EEFB25F915" box="[1101,1230,1735,1761]" italics="true" pageId="8" pageNumber="9">Pogonodon</emphasis>
Cope, 1880
</taxonomicName>
(
<taxonomicName id="4C2901BE04011552FA4A48EEFF08F8F1" authorityName="Bowdich" authorityYear="1821" class="Gastropoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="9" phylum="Mollusca" rank="order">Carnivora</taxonomicName>
