<paragraph id="C41136CBFFBCFFCDA49AFA640E3EFA4D" blockId="26.[189,324,1453,1516]" pageId="26" pageNumber="27">
<emphasis id="F6DAEAD9FFBCFFCDA49AFA640E64FA66" bold="true" box="[234,324,1453,1479]" pageId="26" pageNumber="27">Length</emphasis>
<quantity id="03569B2EFFBCFFCDA4CDFA180E3AFA4D" box="[189,282,1489,1516]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="26" pageNumber="27" unit="mm" value="2.5">2.5 mm</quantity>
