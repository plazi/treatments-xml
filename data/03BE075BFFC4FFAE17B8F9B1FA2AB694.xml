Dorsal cephalic and body chaetotaxy: 54 &amp; 57, 
<taxonomicName id="4C17CDCEFFC1FFAF146EF8ADFC17B726" box="[833,936,1812,1834]" class="Entognatha" family="Tomoceridae" genus="Monodontocerus" kingdom="Animalia" order="Collembola" pageId="14" pageNumber="571" phylum="Arthropoda" rank="species" species="absens" status="sp. nov.">
<emphasis id="B9636A5FFFC1FFAF146EF8ADFC17B726" box="[833,936,1812,1834]" italics="true" pageId="14" pageNumber="571">M. absens</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A250D724FFC1FFAF1481F8ADFC40B726" box="[942,1023,1813,1834]" pageId="14" pageNumber="571" rank="species">
<emphasis id="B9636A5FFFC1FFAF1481F8ADFC40B726" bold="true" box="[942,1023,1813,1834]" pageId="14" pageNumber="571">sp. nov.</emphasis>
</taxonomicNameLabel>
; 55 &amp; 58, 
<taxonomicName id="4C17CDCEFFC1FFAF1341F8ADFABDB726" box="[1134,1282,1812,1834]" class="Entognatha" family="Tomoceridae" genus="Monodontocerus" kingdom="Animalia" order="Collembola" pageId="14" pageNumber="571" phylum="Arthropoda" rank="species" species="mulunensis" status="sp. nov.">
<emphasis id="B9636A5FFFC1FFAF1341F8ADFABDB726" box="[1134,1282,1812,1834]" italics="true" pageId="14" pageNumber="571">M. mulunensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A250D724FFC1FFAF1226F8ADFAE5B726" box="[1289,1370,1813,1834]" pageId="14" pageNumber="571" rank="species">
<emphasis id="B9636A5FFFC1FFAF1226F8ADFAE5B726" bold="true" box="[1289,1370,1813,1834]" pageId="14" pageNumber="571">sp. nov.</emphasis>
</taxonomicNameLabel>
; 56 &amp; 59, 
<taxonomicName id="4C17CDCEFFC1FFAF1793F88CFEFDB745" box="[188,322,1843,1865]" class="Entognatha" family="Tomoceridae" genus="Monodontocerus" kingdom="Animalia" order="Collembola" pageId="14" pageNumber="571" phylum="Arthropoda" rank="species" species="trigrandis" status="sp. nov.">
<emphasis id="B9636A5FFFC1FFAF1793F88CFEFDB745" box="[188,322,1843,1865]" italics="true" pageId="14" pageNumber="571">M. trigrandis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A250D724FFC1FFAF1667F88BFE27B744" box="[328,408,1843,1864]" pageId="14" pageNumber="571" rank="species">
