<typeStatus id="8415BB9533540A146178FFA4FE3944E3" box="[367,423,672,698]" pageId="1" pageNumber="60">type</typeStatus>
.” [blue square, handwritten]; “Pleias 
<taxonomicName id="9CAE7EB433540A146375FFA4FC5B44E3" box="[866,965,672,698]" class="Insecta" family="Reduviidae" genus="Ploiaria" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="ritsemae">ritsemae</taxonomicName>
