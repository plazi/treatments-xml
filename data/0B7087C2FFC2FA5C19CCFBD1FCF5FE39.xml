: Few records from 
<collectingCountry id="FBCE7644FFC3FA5C1B0FFED6FCC4FEC7" box="[596,864,338,364]" name="Papua New Guinea" pageId="15" pageNumber="16">Bismarck Archipelago</collectingCountry>
(
<collectingRegion id="411DF836FFC3FA5C1A28FED6FBA8FEC7" box="[883,1036,338,364]" country="Papua New Guinea" name="West New Britain" pageId="15" pageNumber="16">New Britain</collectingRegion>
, 
<collectingRegion id="411DF836FFC3FA5C1D40FED6FB16FEC7" box="[1051,1202,338,364]" country="Papua New Guinea" name="New Ireland" pageId="15" pageNumber="16">New Ireland</collectingRegion>
