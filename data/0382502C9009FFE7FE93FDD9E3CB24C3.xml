<taxonomicName id="4C2B9AB99004FFE7FCFFFADBE36E2451" authority="Zacharda &amp; Grafitti &amp; Piva, 2011" authorityName="Zacharda &amp; Grafitti &amp; Piva" authorityYear="2011" box="[844,1079,1324,1348]" class="Arachnida" family="Rhagidiidae" genus="Rhagidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="10" pageNumber="675" phylum="Arthropoda" rank="species" species="serpentiforma" status="sp. nov." subGenus="Deharvengiella">
