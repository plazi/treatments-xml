<taxonomicName id="4C00C8E1FFC801286C3AF917A1C8F978" baseAuthorityName="Casey" baseAuthorityYear="1893" box="[283,395,1710,1729]" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbrosus">
<emphasis id="B9746F70FFC801286C3AF917A1C8F978" box="[283,395,1710,1729]" italics="true" pageId="6" pageNumber="7">M. umbrosus</emphasis>
