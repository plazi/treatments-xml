<caption id="DF7FE3EAFFC801286DE4F9A6A243F978" ID-DOI="http://doi.org/10.5281/zenodo.4892946" ID-Zenodo-Dep="4892946" httpUri="https://zenodo.org/record/4892946/files/figure.png" pageId="6" pageNumber="7" startId="6.[197,229,1567,1586]" targetBox="[175,657,1158,1542]" targetPageId="6">
<paragraph id="8BBFB362FFC801286DE4F9A6A243F978" blockId="6.[173,660,1567,1729]" pageId="6" pageNumber="7">
<emphasis id="B9746F70FFC801286DE4F9A6A143F98B" bold="true" box="[197,256,1567,1586]" pageId="6" pageNumber="7">Fig. 5.</emphasis>
<taxonomicName id="4C00C8E1FFC801286C37F9A6A1B0F98B" baseAuthorityName="Casey" baseAuthorityYear="1893" box="[278,499,1567,1586]" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbilicatus">
<emphasis id="B9746F70FFC801286C37F9A6A1B0F98B" box="[278,499,1567,1586]" italics="true" pageId="6" pageNumber="7">Margarinotus umbilicatus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C00C8E1FFC801286F3FF9A6A0BCF9F0" baseAuthorityName="Casey" baseAuthorityYear="1893" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbrosus">
<emphasis id="B9746F70FFC801286F3FF9A6A0BCF9F0" italics="true" pageId="6" pageNumber="7">Margarinotus umbrosus</emphasis>
</taxonomicName>
, median lobe of aedeagus. A. 
<taxonomicName id="4C00C8E1FFC801286F2CF98FA2CEF9F0" baseAuthorityName="Casey" baseAuthorityYear="1893" box="[525,653,1590,1609]" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbilicatus">
<emphasis id="B9746F70FFC801286F2CF98FA2CEF9F0" box="[525,653,1590,1609]" italics="true" pageId="6" pageNumber="7">M. umbilicatus</emphasis>
</taxonomicName>
, dorsal view. B. 
<taxonomicName id="4C00C8E1FFC801286C64F9F7A191F9D8" baseAuthorityName="Casey" baseAuthorityYear="1893" box="[325,466,1614,1633]" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbilicatus">
<emphasis id="B9746F70FFC801286C64F9F7A191F9D8" box="[325,466,1614,1633]" italics="true" pageId="6" pageNumber="7">M. umbilicatus</emphasis>
</taxonomicName>
