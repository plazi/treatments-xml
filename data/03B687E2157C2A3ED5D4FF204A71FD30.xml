<date id="FFA11034157C2A3ED6E4FEF04ACEFE8E" box="[898,1019,264,284]" pageId="25" pageNumber="752" value="2007-02-14" valueMax="2007-02-16" valueMin="2007-02-14">14-16.II.2007</date>
