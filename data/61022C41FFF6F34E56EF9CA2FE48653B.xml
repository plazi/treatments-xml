<taxonomicName id="2EABE6D4FFF6F34E57439F37FC886316" authority="Weirauch, 2006" authorityName="Weirauch" authorityYear="2006" box="[315,788,575,600]" class="Insecta" family="Reduviidae" genus="Leptophysoderes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="orellana">
