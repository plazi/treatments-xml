<paragraph id="1B3D2A4C2C50FFAB1EF85878FE0FF882" blockId="4.[151,386,1862,1888]" box="[151,386,1862,1888]" pageId="4" pageNumber="5">
<heading id="40759D202C50FFAB1EF85878FE0FF882" bold="true" box="[151,386,1862,1888]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<taxonomicName id="DC8251CF2C50FFAB1EF85878FE0FF882" authority="Dana, 1850" authorityName="Dana" authorityYear="1850" box="[151,386,1862,1888]" class="Malacostraca" family="Mysidae" genus="Siriella" kingdom="Animalia" order="Mysida" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
<emphasis id="29F6F65E2C50FFAB1EF85878FE0FF882" bold="true" box="[151,386,1862,1888]" pageId="4" pageNumber="5">
<emphasis id="29F6F65E2C50FFAB1EF85878FF62F882" bold="true" box="[151,239,1862,1888]" italics="true" pageId="4" pageNumber="5">Siriella</emphasis>
<bibRefCitation id="7F1357BD2C50FFAB1E995878FE0FF882" author="Dana" box="[246,386,1862,1888]" pageId="4" pageNumber="47" refString="Dana, J. D. (1850) Synopsis generum crustaceorum ordinis &quot; Schizopoda &quot;. American Journal of Science and Arts, 2 (9), 129 - 133." type="journal article" year="1850">
Dana, 
<number id="AD4A386B2C50FFAB1F285878FE0FF882" box="[327,386,1862,1888]" pageId="4" pageNumber="5" value="1850.0">1850</number>
</bibRefCitation>
</emphasis>
