</taxonomicName>
(
<taxonomicName id="BA94A089A326654659C1FF12687B5989" box="[972,1141,151,178]" class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="23" pageNumber="518" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
