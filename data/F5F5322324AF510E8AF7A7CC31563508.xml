<paragraph id="3FFC4103E1119FAEC0814FB3CB8040D0" pageId="0" pageNumber="1">
<taxonomicName id="0FE71F5AC4609DE30B2CA9096243453B" authority="Blume, 1823" authorityName="Blume" authorityYear="1829" baseAuthorityName="Wall." class="Magnoliopsida" family="Magnoliaceae" genus="Manglietia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Manglietia insignis" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="insignis">
