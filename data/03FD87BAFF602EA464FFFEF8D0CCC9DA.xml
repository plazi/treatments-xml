<emphasis id="B920EABEFF632EA464AFFCFDD316C96A" bold="true" box="[199,345,836,861]" pageId="143" pageNumber="144">Distribution</emphasis>
. Chatham Rise, 
<collectingCountry id="F343763CFF632EA4667BFCFCD0E3C96A" box="[531,684,836,861]" name="New Zealand" pageId="143" pageNumber="144">New Zealand</collectingCountry>
, 
<geoCoordinate id="EE60506BFF632EA466DFFCFDD15AC96B" box="[695,789,836,860]" degrees="44" direction="south" minutes="30" orientation="latitude" pageId="143" pageNumber="144" precision="925" value="-44.5">44°30’S</geoCoordinate>
, 
<geoCoordinate id="EE60506BFF632EA46749FCFDD1EAC96A" box="[801,933,836,861]" degrees="174" direction="east" minutes="18.6" orientation="longitude" pageId="143" pageNumber="144" precision="92" value="174.31">174°18.6’E</geoCoordinate>
