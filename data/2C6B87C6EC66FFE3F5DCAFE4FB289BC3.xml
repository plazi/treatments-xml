<taxonomicName id="63C24D53EC66FFE3F617A889FC4F9BF8" authorityName="Gaertner" authorityYear="1788" box="[835,977,1457,1483]" class="Magnoliopsida" family="Apiaceae" genus="Visnaga" kingdom="Plantae" order="Apiales" pageId="0" pageNumber="50" phylum="Tracheophyta" rank="species" species="daucoides">
