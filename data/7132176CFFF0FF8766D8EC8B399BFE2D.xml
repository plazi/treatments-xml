<date id="8D2580BAFFF0FF81650BECFB3A9AF8BA" box="[859,968,1778,1799]" pageId="3" pageNumber="290" value="2020-10-08">
<collectingDate id="9D617952FFF0FF81650BECFB3A9AF8BA" box="[859,968,1778,1799]" pageId="3" pageNumber="290" value="2020-10-08">8 Oct 2020</collectingDate>
</date>
, 
<emphasis id="CBEF7A68FFF0FF816582ECFA3DDCF8BA" box="[978,1166,1778,1800]" italics="true" pageId="3" pageNumber="290">Qun Liu Qun gl001</emphasis>
(
<collectionCode id="9F8A3EBFFFF0FF8162C4ECFB3D99F8B5" box="[1172,1227,1778,1800]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15842" lsid="urn:lsid:biocol.org:col:15842" name="Kunming Institute of Botany, Chinese Academy of Sciences" pageId="3" pageNumber="290" type="Herbarium">KUN</collectionCode>
), 
<emphasis id="CBEF7A68FFF0FF8162B3ECFA3C1BF8BA" box="[1251,1353,1778,1800]" italics="true" pageId="3" pageNumber="290">Qun gl002</emphasis>
(
<collectionCode id="9F8A3EBFFFF0FF816300ECFB3CDBF8B5" box="[1360,1417,1778,1800]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15842" lsid="urn:lsid:biocol.org:col:15842" name="Kunming Institute of Botany, Chinese Academy of Sciences" pageId="3" pageNumber="290" type="Herbarium">KUN</collectionCode>
