<quantity id="4C81BACF8B648F74E7159E19A5F97B07" box="[552,641,1828,1852]" metricMagnitude="-3" metricUnit="m" metricValue="9.7" pageId="2" pageNumber="281" unit="mm" value="9.7">9.7 mm</quantity>
; fore wing length 
<quantity id="4C81BACF8B648F74E6719E19A4DD7B07" box="[844,933,1828,1852]" metricMagnitude="-3" metricUnit="m" metricValue="7.9" pageId="2" pageNumber="281" unit="mm" value="7.9">7.9 mm</quantity>
