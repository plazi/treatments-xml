<collectionCode id="ED0078E56646FFA6FE711770E0C2EF20" box="[394,450,577,603]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="3" pageNumber="514">SMF</collectionCode>
uncatalogued, female, 
<quantity id="4CE94DC56646FFA6FD3D1770E20BEF27" box="[710,779,577,604]" metricMagnitude="-1" metricUnit="m" metricValue="6.4" pageId="3" pageNumber="514" unit="cm" value="64.0">64 cm</quantity>
