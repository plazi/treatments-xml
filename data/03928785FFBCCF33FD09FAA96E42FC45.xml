<taxonomicName id="4C3B4D10FF83CF32FBFDFC7068CBFBCF" authority="Mantilleri, 2009" authorityName="Mantilleri" authorityYear="2009" box="[1041,1209,1013,1031]" class="Insecta" family="Brentidae" genus="Higonius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="931" phylum="Arthropoda" rank="species" species="philippinensis" status="n. sp." subGenus="Higonodes">
<emphasis id="B94FEA81FF83CF32FBFDFC706857FBCF" box="[1041,1061,1013,1030]" italics="true" pageId="32" pageNumber="931">H.</emphasis>
(
<emphasis id="B94FEA81FF83CF32FBC1FC706833FBCF" box="[1069,1089,1013,1030]" italics="true" pageId="32" pageNumber="931">H.</emphasis>
) 
<emphasis id="B94FEA81FF83CF32FBA4FC7068CBFBCF" box="[1096,1209,1013,1030]" italics="true" pageId="32" pageNumber="931">philippinensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27C57FAFF83CF32FF9DFB896CD3FBD4" box="[113,161,1036,1053]" pageId="32" pageNumber="931" rank="species">n. sp.</taxonomicNameLabel>
