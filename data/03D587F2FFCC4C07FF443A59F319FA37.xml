<subSubSection id="C366656FFFCC4C07FBBF2534FAC2EF76" pageId="3" pageNumber="352" type="discussion">
<paragraph id="8BC336E4FFCC4C07FBBF2534FAC2EF76" pageId="3" pageNumber="352">
<taxonomicName id="4C7C4D67FFCC4C07FBBF2534FA93EF83" authorityName="Thomas" authorityYear="1920" box="[1050,1387,4163,4202]" class="Mammalia" family="Emballonuridae" genus="Taphozous" kingdom="Animalia" order="Chiroptera" pageId="3" pageNumber="352" phylum="Chordata" rank="species" species="hamiltoni">
