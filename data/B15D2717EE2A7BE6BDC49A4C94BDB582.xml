<taxonomicName id="BB44F3A62A4D37C1924EE5DF7E1E6229" authority="L." class="Magnoliopsida" family="Geraniaceae" genus="Geranium" kingdom="Plantae" order="Geraniales" pageId="null" pageNumber="627" phylum="Tracheophyta" rank="species" species="palustre">
Geranium 
<normalizedToken id="F7DEC58398CE137EB24CE318AB8E1BA7" originalValue="palústre" pageId="null" pageNumber="627">palustre</normalizedToken>
<authorityName id="49F271B9918A0BE7D0566052A4F95058" pageId="null" pageNumber="627">L.</authorityName>
