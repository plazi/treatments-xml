<geoCoordinate id="EE235007FFE4FFC09BE3FAE5FE543534" box="[926,1045,1303,1329]" degrees="114" direction="west" minutes="24" orientation="longitude" pageId="117" pageNumber="118" precision="925" value="-114.4">114°24’W</geoCoordinate>
