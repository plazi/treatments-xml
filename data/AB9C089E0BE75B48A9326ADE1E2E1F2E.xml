<taxonomicName id="6E9ABD6818406CD3D3D0F0386F069B62" authorityName="Endrodi" authorityYear="1963" class="Insecta" family="Scarabaeidae" genus="Cyclocephala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyclocephala striata" order="Coleoptera" pageId="116" pageNumber="217" phylum="Arthropoda" rank="species" species="striata">
