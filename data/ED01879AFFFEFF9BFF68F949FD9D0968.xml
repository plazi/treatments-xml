<quantity id="A2509B69FFF1FF9BFDB7FF57FD650ACC" box="[584,698,159,186]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" metricValueMax="1.5" metricValueMin="1.0" pageId="25" pageNumber="26" unit="mm" value="1.25" valueMax="1.5" valueMin="1.0">1–1.5 mm</quantity>
