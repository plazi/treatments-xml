<taxonomicName id="FB8977C0FFC0FF8C3F4814DAFA906289" authority="Filipjev 1922" authorityName="Filipjev" authorityYear="1922" box="[888,1320,1724,1748]" class="Adenophorea" family="Comesomatidae" genus="Sabatieria" kingdom="Animalia" order="Desmodorida" pageId="2" pageNumber="265" phylum="Nematoda" rank="species" species="longicaudata">
<emphasis id="0EFDD051FFC0FF8C3F4814DAFB3F6289" box="[888,1159,1724,1748]" italics="true" pageId="2" pageNumber="265">Sabatieria longicaudata</emphasis>
<bibRefCitation id="581871B2FFC0FF8C38BF14DAFA906289" author="Filipjev, I." box="[1167,1320,1724,1748]" pageId="2" pageNumber="265" pagination="83 - 184" refId="ref12163" refString="Filipjev, I. (1922) Encore sur les Nematodes libre de la Mer Noire. Acta Instituti Agronomici Stauropolitani, 1 (16), 83 - 184." type="journal article" year="1922">Filipjev 1922</bibRefCitation>
