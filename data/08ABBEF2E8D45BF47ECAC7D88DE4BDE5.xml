<paragraph id="589494DE9B7C1BA56DA803813EED631B" pageId="null" pageNumber="557">
<taxonomicName id="42EA33F7A8F20BAEACA02B04DDBB72E2" authority="L." class="Liliopsida" family="Asparagaceae" genus="Ornithogalum" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="557" phylum="Tracheophyta" rank="species" species="nutans">
Ornithogalum 
<normalizedToken id="A7F1D0602DC22DAD01D72C444C2BFF19" originalValue="nútans" pageId="null" pageNumber="557">nutans</normalizedToken>
<authorityName id="E37558D28EF52815555AB3BCC157C357" pageId="null" pageNumber="557">L.</authorityName>
