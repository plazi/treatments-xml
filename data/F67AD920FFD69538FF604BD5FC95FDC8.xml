<taxonomicName id="B9D313B5FFD4953FFEBA4E79FC41F9B2" authority="Yang and Yang, 1993" authorityName="Yang and Yang" authorityYear="1993" box="[333,996,1633,1656]" class="Insecta" family="Bibionidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="35" phylum="Arthropoda" rank="species" species="pseudacanthophora" subGenus="Acutipula">
