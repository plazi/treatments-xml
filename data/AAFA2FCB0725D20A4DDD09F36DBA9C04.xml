<taxonomicName id="69E2D212278433273B3C5CD94122540F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:193260" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus christoides Forel" order="Hymenoptera" pageNumber="61" phylum="Arthropoda" rank="species" species="christoides" status="n. st.">CAMPONOTUS CHRISTOIDES</taxonomicName>
