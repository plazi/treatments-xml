<collectorName id="9E205334FFADE65E1045FD66FE3AFD16" box="[363,484,717,742]" pageId="18" pageNumber="209">La Esfinge</collectorName>
(
<location id="360A6039FFADE65E10DDFD66FD50FD16" LSID="urn:lsid:plazi:treatment:BB7C87F4FFADE65E11B9FE7AFC72F806:360A6039FFADE65E10DDFD66FD50FD16" box="[499,654,717,742]" name="Gran Canaria" pageId="18" pageNumber="209" stateProvince="Canary Islands">Gran Canaria</location>
