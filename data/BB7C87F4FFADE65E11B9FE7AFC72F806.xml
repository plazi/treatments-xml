<collectingRegion id="F111F800FFADE65E13B6FD66FC9DFD16" box="[664,835,717,742]" country="Spain" name="Canarias" pageId="18" pageNumber="209">Canary Islands</collectingRegion>
), UTM 28 R 
<elevation id="B8F8D1D1FFADE65E12F9FD65FB9EFD16" box="[983,1088,717,742]" metricMagnitude="5" metricUnit="m" metricValue="4.59801" pageId="18" pageNumber="209" unit="m" value="459801.0">
<quantity id="F42D9B07FFADE65E12F9FD65FB9EFD16" box="[983,1088,717,742]" metricMagnitude="5" metricUnit="m" metricValue="4.59801" pageId="18" pageNumber="209" unit="m" value="459801.0">459801m</quantity>
</elevation>
E 
<elevation id="B8F8D1D1FFADE65E1571FD65FB03FD15" box="[1119,1245,717,742]" metricMagnitude="6" metricUnit="m" metricValue="3.115759" pageId="18" pageNumber="209" unit="m" value="3115759.0">
<quantity id="F42D9B07FFADE65E1571FD65FB03FD15" box="[1119,1245,717,742]" metricMagnitude="6" metricUnit="m" metricValue="3.115759" pageId="18" pageNumber="209" unit="m" value="3115759.0">3115759 m</quantity>
</elevation>
N
</materialsCitation>
, 
