<taxonomicName id="AFF1C1AA9F391C22FE3463777E0C3DFE" authority="(Ylla et al. 2010)" authorityName="Ylla" authorityYear="2010" baseAuthorityName="Ylla et al." baseAuthorityYear="2010" box="[499,836,873,899]" class="Magnoliopsida" family="Lamiaceae" genus="Rosmarinus" kingdom="Plantae" order="Lamiales" pageId="23" pageNumber="424" phylum="Tracheophyta" rank="genus">
