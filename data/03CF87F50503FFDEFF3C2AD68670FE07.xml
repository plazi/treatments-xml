<collectingCountry id="F37176730503FFDEFE1E2B048160FEDB" box="[437,590,325,349]" name="Portugal" pageId="6" pageNumber="25">PORTUGAL</collectingCountry>
. Bragança district: 
<collectingCountry id="F37176730503FFDEFC8D2B048049FEDB" box="[806,871,325,349]" name="Palau" pageId="6" pageNumber="25">Palão</collectingCountry>
, 
<date id="FFD810230503FFDEFCD92B04870DFEDB" box="[882,1059,325,349]" pageId="6" pageNumber="25" value="2007-10-03">
<number id="3DAE24C40503FFDEFCD92B0480AEFEDB" box="[882,896,325,349]" pageId="6" pageNumber="25" value="3.0">3</number>
