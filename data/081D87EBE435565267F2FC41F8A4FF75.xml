<paragraph id="800B36FDE435565267A2F856F8A4FF75" blockId="6.[151,1437,1045,2006]" lastBlockId="7.[151,1436,151,213]" lastPageId="7" lastPageNumber="508" pageId="6" pageNumber="507">
<emphasis id="B2C0EAEFE435565367A2F856F848F876" bold="true" box="[199,315,1981,2006]" pageId="6" pageNumber="507">Remarks.</emphasis>
<taxonomicName id="47B44D7EE43556536626F855F8C8F876" authority="Kocian &amp; Schülke, 2018" authorityName="Kocian &amp; Schülke" authorityYear="2018" box="[323,443,1981,2006]" class="Insecta" family="Staphylinidae" genus="Ischnosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="507" phylum="Arthropoda" rank="species" species="schmidti" status="sp. n.">
<emphasis id="B2C0EAEFE43556536626F855F8C8F876" box="[323,443,1981,2006]" italics="true" pageId="6" pageNumber="507">I. schmidti</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A9F35794E435565366A6F856FB77F876" box="[451,516,1981,2006]" pageId="6" pageNumber="507" rank="species">
<emphasis id="B2C0EAEFE435565366A6F856FB77F876" bold="true" box="[451,516,1981,2006]" pageId="6" pageNumber="507">sp. n.</emphasis>
</taxonomicNameLabel>
belongs to the 
<taxonomicName id="47B44D7EE435565365D6F855FA6BF875" baseAuthorityName="Horn" baseAuthorityYear="1877" box="[691,792,1982,2005]" class="Insecta" family="Staphylinidae" genus="Ischnosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="507" phylum="Arthropoda" rank="species" species="pictum">
