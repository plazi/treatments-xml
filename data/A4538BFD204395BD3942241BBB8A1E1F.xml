<subSubSection id="48DAC0A6DB3A8909EB914C2425CC8FC3" pageId="4" pageNumber="81" type="reference_group">
<paragraph id="6CA50F66A26A61AE1CD7D58A8372A62F" pageId="4" pageNumber="81">
<taxonomicName id="AF7594DD5A8D92682D174A4AF0BFCA75" class="Malacostraca" family="Bopyridae" genus="Tylokepon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tylokepon bonnieri" order="Isopoda" pageId="4" pageNumber="81" phylum="Arthropoda" rank="species" species="bonnieri">
<pageBreakToken id="381A6D154B08D38E09A9C5EB69B06D7D" pageId="4" pageNumber="81" start="start">Tylokepon</pageBreakToken>
