<quantity id="4C98989DA6695854982FF8B9FC77455B" box="[861,969,1892,1919]" metricMagnitude="-2" metricUnit="m" metricValue="1.51" pageId="9" pageNumber="117" unit="mm" value="15.1">15.1 mm</quantity>
, length of tegmen: 
<quantity id="4C98989DA66958549FD8F8B8FAA8455B" box="[1194,1302,1893,1919]" metricMagnitude="-2" metricUnit="m" metricValue="1.31" pageId="9" pageNumber="117" unit="mm" value="13.1">13.1 mm</quantity>
