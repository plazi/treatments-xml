<collectionCode id="ED5EAE4AFF75FF03F7D8FEF92AE97523" box="[1165,1249,260,284]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="150" pageNumber="151" type="Museum">BPBM</collectionCode>
