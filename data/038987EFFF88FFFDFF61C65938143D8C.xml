0° 
<number id="3DE824DEFF88FFFDFBDDC3E7389B3D1C" box="[1067,1153,1266,1290]" pageId="18" pageNumber="19" value="32.517">32.517</number>
’S
</geoCoordinate>
, 
<geoCoordinate id="EE14503EFF88FFFDFB52C3E7392C3D1C" box="[1188,1334,1266,1290]" direction="east" orientation="longitude" pageId="18" pageNumber="19" precision="1" value="37.701683">
<number id="3DE824DEFF88FFFDFB52C3E738DB3D1C" box="[1188,1217,1266,1290]" pageId="18" pageNumber="19" value="37.0">37</number>
° 
<number id="3DE824DEFF88FFFDFB3CC3E739043D1C" box="[1226,1310,1266,1290]" pageId="18" pageNumber="19" value="42.101">42.101</number>
’E
</geoCoordinate>
, 
