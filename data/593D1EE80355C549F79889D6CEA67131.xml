<taxonomicName id="F5D67FB77F790963924D927C8D1BE1BC" class="Polychaeta" family="Paraonidae" genus="Aricidea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aricidea quadrilobata" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="quadrilobata">Aricidea quadrilobata</taxonomicName>
