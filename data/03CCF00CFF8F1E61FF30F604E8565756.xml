<taxonomicName id="4C653A99FF8E1E61FE7CF12FEE355751" box="[475,559,562,587]" class="Insecta" family="Psychomyiidae" genus="Paduniella" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="57" phylum="Arthropoda" rank="species" species="nandra">
