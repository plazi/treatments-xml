<number id="3D98C625FFF2FFFE4B2087C847744C55" box="[274,288,1518,1542]" pageId="10" pageNumber="193" value="2.0">2</number>
anterior ends male, 
<specimenCount id="9D561F8BFFF2FFFE483487CB44264C55" box="[518,626,1517,1542]" pageId="10" pageNumber="193" type="female">
<number id="3D98C625FFF2FFFE483487CB44404C55" box="[518,532,1517,1542]" pageId="10" pageNumber="193" value="5.0">5</number>
