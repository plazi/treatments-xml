<taxonomicName id="AFBF4D0F6642291630B1A90AFE181A6F" authority="Jadin &amp; Vercammen-Grandjean, 1954" authorityName="Jadin &amp; Vercammen-Grandjean" authorityYear="1954" class="Arachnida" family="Trombiculidae" genus="Schoutedenichia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="fulleri">
