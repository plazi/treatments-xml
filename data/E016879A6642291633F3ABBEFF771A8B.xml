<taxonomicName id="AFBF4D0F66422916313BA96EFC3E1A6F" box="[607,937,583,609]" class="Arachnida" family="Trombiculidae" genus="Walchiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="66" pageNumber="67" phylum="Arthropoda" rank="subSpecies" species="oudemansi" subSpecies="fulleri">
