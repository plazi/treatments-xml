<taxonomicName id="AFBF4D0F66422916377BAA9EFF4B19FB" authority="(Vercammen-Grandjean 1971)" baseAuthorityName="Vercammen-Grandjean" baseAuthorityYear="1971" class="Arachnida" family="Trombiculidae" genus="Gahrliepia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="fulleri">
