<typeStatus id="B704882E6642291632B0AAD6FDA11A17" box="[468,566,511,537]" pageId="66" pageNumber="67">holotype</typeStatus>
of 
<taxonomicName id="AFBF4D0F66422916313AAAD6FCD31A17" authority="in MHNG" authorityName="in MHNG" box="[606,836,511,537]" class="Arachnida" family="Trombiculidae" genus="Gahrliepia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="fulleri">
