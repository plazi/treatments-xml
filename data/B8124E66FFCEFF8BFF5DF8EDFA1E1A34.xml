<quantity id="F7435295FFCEFF88FDB3F840FD121019" box="[614,679,2032,2054]" metricMagnitude="-2" metricUnit="m" metricValue="2.5" pageId="5" pageNumber="155" unit="cm" value="2.5">2.5 cm</quantity>
