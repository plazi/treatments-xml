<taxonomicName id="4C4E4D18FFD8E06EDEFCFE8EFD6A7815" authority="Puthz, 2003" authorityName="Puthz" authorityYear="2003" box="[486,766,260,285]" class="Insecta" family="Staphylinidae" genus="Stenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="111" phylum="Arthropoda" rank="species" species="wuyiensis">
