<quantity id="F1759B46FFBCFFFB6E767CA14CD6FDA7" box="[378,486,499,525]" metricMagnitude="-4" metricUnit="m" metricValue="4.9" pageId="47" pageNumber="48" unit="mm" value="0.49">0.49 mm</quantity>
