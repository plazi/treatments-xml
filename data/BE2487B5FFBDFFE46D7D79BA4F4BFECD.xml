<quantity id="F1759B46FFBCFFFB6E927CE04F3AFE66" box="[414,522,434,460]" metricMagnitude="-4" metricUnit="m" metricValue="2.1" pageId="47" pageNumber="48" unit="mm" value="0.21">0.21 mm</quantity>
, 
<quantity id="F1759B46FFBCFFFB6D197CE34F4EFE66" box="[533,638,433,460]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="47" pageNumber="48" unit="mm" value="0.45">0.45 mm</quantity>
