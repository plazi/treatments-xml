<quantity id="F1759B46FFBCFFFB6D057C1F4F45FECD" box="[521,629,333,359]" metricMagnitude="-4" metricUnit="m" metricValue="2.9" pageId="47" pageNumber="48" unit="mm" value="0.29">0.29 mm</quantity>
