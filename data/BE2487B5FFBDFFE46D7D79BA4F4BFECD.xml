<quantity id="F1759B46FFBCFFFB6D377CA14F94FDA7" box="[571,676,499,525]" metricMagnitude="-4" metricUnit="m" metricValue="4.8" pageId="47" pageNumber="48" unit="mm" value="0.48">0.48 mm</quantity>
