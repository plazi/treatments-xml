<quantity id="F1759B46FFBCFFFB6CD57C1F4976FECD" box="[985,1094,333,359]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="47" pageNumber="48" unit="mm" value="0.34">0.34 mm</quantity>
