<taxonomicName id="E3A3581F687EC303A288B6349A6AABF1" class="Magnoliopsida" family="Rosaceae" genus="Rubus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rubus" order="Rosales" pageId="4" pageNumber="59" phylum="Tracheophyta" rank="genus">Rubus</taxonomicName>
