<taxonomicName id="F0E75C8BFFDCFFE1FE24DA25D692FD81" box="[482,645,585,611]" class="Magnoliopsida" family="Ranunculaceae" genus="Clematis" kingdom="Plantae" order="Ranunculales" pageId="8" pageNumber="519" phylum="Tracheophyta" rank="species" species="recta">
