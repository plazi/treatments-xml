<collectingCountry id="8FC1763AFFB7FFEAFC7BFA02FC696EFC" box="[939,1010,1510,1535]" name="China" pageId="3" pageNumber="311">China</collectingCountry>
: Turkestan [= 
<collectingRegion id="3512F848FFB7FFEAFB70FA02FA9E6EFC" box="[1184,1285,1510,1535]" country="China" name="Xinjiang Uygur" pageId="3" pageNumber="311">Xinjiang</collectingRegion>
] (
<bibRefCitation id="93474B5BFFB7FFEAFACFFA03FA146EFC" author="Wu, C. F." box="[1311,1423,1510,1535]" pageId="3" pageNumber="311" refId="ref20308" refString="Wu, C. F. (1935) Catalogus insectorum sinensium. Vol. II. Fan Memorial Institute of Biology, Peiping, 634 pp." type="book" year="1935">Wu 1935</bibRefCitation>
