<paragraph id="8BF236ADFFC02B1CFF3CFAC43B4D1A38" blockId="19.[151,538,1375,1436]" box="[151,538,1375,1401]" pageId="19" pageNumber="20">
<heading id="D0BA81C1FFC02B1CFF3CFAC43B4D1A38" bold="true" box="[151,538,1375,1401]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis id="B939EABFFFC02B1CFF3CFAC43B4D1A38" bold="true" box="[151,538,1375,1401]" pageId="19" pageNumber="20">
<taxonomicName id="4C4D4D2EFFC02B1CFF3CFAC4382C1A38" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[151,379,1375,1401]" class="Insecta" family="Hydropsychidae" genus="Diplectrona" kingdom="Animalia" order="Trichoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="tiguna" status="new species">
<emphasis id="B939EABFFFC02B1CFF3CFAC4382C1A38" bold="true" box="[151,379,1375,1401]" italics="true" pageId="19" pageNumber="20">Diplectrona tiguna</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A20A57C4FFC02B1CFE21FAC43B4D1A38" box="[394,538,1375,1401]" pageId="19" pageNumber="20" rank="species">new species</taxonomicNameLabel>
