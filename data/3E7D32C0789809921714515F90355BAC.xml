<taxonomicName id="A195F9A37DFC986872C2C3A96DFB6F8C" LSID="urn:lsid:zoobank.org:act:225E19E9-CF6B-46A1-A16F-7EFAD3A91A58" class="Diplopoda" family="Polydesmidae" genus="Nipponesmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nipponesmus minor" order="Polydesmida" pageId="3" pageNumber="12" phylum="Arthropoda" rank="species" species="minor">Nipponesmus minor</taxonomicName>
<taxonomicNameLabel id="BEB5206F756F4CEBED1A90976F295B03" pageId="3" pageNumber="12">sp. n.</taxonomicNameLabel>
