<taxonomicName id="11D23BA80B7DA54B3877E57F85339D72" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Castanopsis" order="Fagales" pageId="12" pageNumber="115" phylum="Tracheophyta" rank="genus">Castanopsis</taxonomicName>
