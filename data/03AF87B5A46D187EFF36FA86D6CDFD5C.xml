<emphasis id="B972EAB1A46F187EFF64FE23D75EFE17" bold="true" box="[197,380,471,497]" pageId="11" pageNumber="12">
<typeStatus id="54BD8801A46F187EFF64FE23D722FE17" box="[197,256,471,497]" pageId="11" pageNumber="12">Type</typeStatus>
