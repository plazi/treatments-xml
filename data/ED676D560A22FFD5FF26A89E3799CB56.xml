<taxonomicName id="A2CEA7C30A22FFD5FE49AB5930A4CB56" box="[504,673,1605,1631]" class="Anthozoa" family="Cladopathidae" kingdom="Animalia" order="Antipatharia" pageId="1" pageNumber="50" phylum="Cnidaria" rank="subFamily" subFamily="Cladopathinae">Cladopathinae</taxonomicName>
