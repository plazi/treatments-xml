<taxonomicName id="66178B46097E08D3C47344D2FDC084E3" class="Gymnolaemata" family="Candidae" genus="Emma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Emma" order="Cheilostomatida" pageId="137" pageNumber="138" phylum="Bryozoa" rank="genus">Emma</taxonomicName>
