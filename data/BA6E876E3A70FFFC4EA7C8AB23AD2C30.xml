<subSubSection id="7ADD65F33A70FFFC4EF6CC9E23AD2C30" pageId="14" pageNumber="67" type="etymology">
<paragraph id="327836783A70FFFC4EF6CC9E26E42DD0" blockId="14.[151,1436,266,1372]" box="[198,1234,1186,1212]" pageId="14" pageNumber="67">
<emphasis id="00B3EA6A3A70FFFC4EF6CC9E23652DD0" bold="true" box="[198,339,1186,1212]" pageId="14" pageNumber="67">Etymology.</emphasis>
