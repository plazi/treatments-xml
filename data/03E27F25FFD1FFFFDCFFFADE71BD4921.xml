<quantity id="4CB363D6FFD1FFE0DF0FF9F373844951" box="[871,918,1624,1650]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="47" pageNumber="48" unit="m" value="3.0">3 m</quantity>
