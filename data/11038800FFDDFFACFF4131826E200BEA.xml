<taxonomicName id="5EAA4295FFDDFFACFBAD30FD6EB90B12" baseAuthorityName="Hulcr &amp; Cognato" baseAuthorityYear="2010" box="[1147,1268,1534,1559]" class="Insecta" family="Curculionidae" genus="Fortiborus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="genus">
