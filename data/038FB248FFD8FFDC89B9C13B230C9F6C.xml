<heading id="D0D1B432FFD8FFDF89B9C13B21A19932" bold="true" box="[151,491,1832,1858]" fontSize="11" level="1" pageId="105" pageNumber="106" reason="1">
<taxonomicName id="4C2678DDFFD8FFDF89B9C13B21A19932" authority="Fourment, 1883" authorityName="Fourment" authorityYear="1883" box="[151,491,1832,1858]" class="Chromadorea" family="Cystidicolidae" genus="Spinitectus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="105" pageNumber="106" phylum="Nematoda" rank="genus">
