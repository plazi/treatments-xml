<taxonomicName id="5F53F2CF98706AB521FB46EEC03DD117" class="Insecta" family="Arctiidae" genus="Paracles" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracles azollae" order="Lepidoptera" pageId="11" pageNumber="76" phylum="Arthropoda" rank="species" species="azollae">Paracles azollae</taxonomicName>
