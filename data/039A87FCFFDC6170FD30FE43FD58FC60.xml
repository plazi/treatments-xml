<taxonomicName id="4C334D69FFDC6170FCC6FCC4FBB9FC80" authorityName="J.N.F.Wille" authorityYear="1909" box="[847,1025,849,876]" class="Trebouxiophyceae" family="Botryococcaceae" higherTaxonomySource="IPNI" kingdom="Plantae" order="Trebouxiales" pageId="29" pageNumber="116" phylum="Chlorophyta" rank="family">Botryococcaceae</taxonomicName>
