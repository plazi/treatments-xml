<number id="8071248EFF90FFEFFD9DF9B32724F911" box="[573,587,1700,1726]" pageId="2" pageNumber="41" value="1.0">1</number>
st row comprising 
<number id="8071248EFF90FFEFFCBEF9B32654F911" box="[798,827,1700,1726]" pageId="2" pageNumber="41" value="10.0">10</number>
