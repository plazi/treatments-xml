<number id="8071248EFF90FFEFFC3AF9B326C6F911" box="[922,937,1700,1726]" pageId="2" pageNumber="41" value="2.0">2</number>
nd row 
<number id="8071248EFF90FFEFFC56F9B3216BF911" box="[1014,1028,1700,1726]" pageId="2" pageNumber="41" value="7.0">7</number>
