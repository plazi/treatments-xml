<collectingCountry id="4EAE7639FF91FFEEFDD5FEF027DFFDAE" box="[629,688,487,513]" name="India" pageId="3" pageNumber="42">Hind</collectingCountry>
tibia with 
<number id="8071248EFF91FFEEFC8EFEF02653FDAE" box="[814,828,487,513]" pageId="3" pageNumber="42" value="1.0">1</number>
