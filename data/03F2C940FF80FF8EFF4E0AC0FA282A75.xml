<geoCoordinate id="EE6F1E91FF80FF8EFF20091CFEE32D75" box="[249,343,513,539]" degrees="90" direction="east" orientation="longitude" pageId="9" pageNumber="255" precision="55555" value="90.0">90Â° East</geoCoordinate>
