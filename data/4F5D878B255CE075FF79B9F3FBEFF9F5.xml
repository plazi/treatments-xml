<taxonomicName id="00F44D1E255CE075FBE9BF58FB58FAB1" box="[1031,1205,1305,1331]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="mandshurica">
