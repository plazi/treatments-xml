: RFE—AO, KK, PK (Baryshnikova 2006); 
<collectingCountry id="BFE3760D255CE075FC49BF7FFC0AFAD5" box="[935,999,1341,1367]" name="Japan" pageId="29" pageNumber="30">Japan</collectingCountry>
(
<bibRefCitation id="A3654B6C255CE075FC18BF7FFB78FADA" author="Kumata, T." box="[1014,1173,1341,1368]" pageId="29" pageNumber="30" pagination="53 - 90" refId="ref34019" refString="Kumata, T. (1963) Taxonomic studies on the Lithocolletinae of Japan (Lepidoptera: Gracillariidae) Part I. Insecta Matsumurana, 25, 53 - 90." type="journal article" year="1963">Kumata 1963</bibRefCitation>
), 
<collectingCountry id="BFE3760D255CE075FB45BF7FFB02FAD5" box="[1195,1263,1341,1367]" name="South Korea" pageId="29" pageNumber="30">Korea</collectingCountry>
