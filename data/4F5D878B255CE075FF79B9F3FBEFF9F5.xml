, 
<taxonomicName id="00F44D1E255CE075FAACBF5BFA98FAB1" authorityName="C.Linnaeus" authorityYear="1753" box="[1346,1397,1305,1331]" class="Magnoliopsida" family="Malvaceae" genus="Tilia" kingdom="Plantae" order="Malvales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="genus">
