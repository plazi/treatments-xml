<typeStatus id="E8A070309B91BE4D958E648760C8037B">paratype</typeStatus>
, dorsal view 
<emphasis id="1A2FA126457E9A9751222229FF831DDE" bold="true" pageId="0" pageNumber="127">B</emphasis>
<typeStatus id="11553FE715C28EC90AAC65D4A02139DC">paratype</typeStatus>
, ventral view 
<emphasis id="12729517AAACA9785B80AA2C9BBC47E9" bold="true" pageId="0" pageNumber="127">C</emphasis>
<typeStatus id="F0B734629638BA86BCE358BAA7D9E557">paratype</typeStatus>
, dorsal view 
<emphasis id="FD252FFA75FDB1625150A5A1D7A458B7" bold="true" pageId="0" pageNumber="127">D</emphasis>
