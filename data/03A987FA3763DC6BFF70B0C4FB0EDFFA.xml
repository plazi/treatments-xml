females; N = 
<specimenCount id="9D06FD653763DC69FD5EB12BFCFEDC4D" box="[697,781,1852,1875]" pageId="0" pageNumber="57" type="male">
<number id="3DC824CB3763DC69FD5EB12BFD35DC4D" box="[697,710,1852,1875]" pageId="0" pageNumber="57" value="4.0">4</number>
