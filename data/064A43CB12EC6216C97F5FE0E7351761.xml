<taxonomicName id="7FAF6D00A3A2AB5EF84FB2180F2E9E04" genus="Hypostena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypostena" pageId="0" pageNumber="11223" rank="genus">Hypostena</taxonomicName>
