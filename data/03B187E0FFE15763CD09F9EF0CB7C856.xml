<taxonomicName id="4C184D75FFE25763CF16FE1E0DF3CB8F" box="[791,941,479,504]" class="Arachnida" family="Aturidae" genus="Kongsbergia" kingdom="Animalia" order="Trombidiformes" pageId="34" pageNumber="35" phylum="Arthropoda" rank="genus">
