<taxonomicName id="4C184D75FFE25763CED4FE760DA6CBA6" authority="Thor, 1899" authorityName="Thor" authorityYear="1899" box="[725,1016,439,465]" class="Arachnida" family="Aturidae" genus="Kongsbergia" kingdom="Animalia" order="Trombidiformes" pageId="34" pageNumber="35" phylum="Arthropoda" rank="genus">
