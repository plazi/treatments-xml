<quantity id="4CD6E6D71B592203FDA1E8D344ADFD38" box="[519,684,739,763]" metricMagnitude="3" metricUnit="m" metricValue="1.251" metricValueMax="2.322" metricValueMin="0.18" pageId="48" pageNumber="499" unit="m" value="1251.0" valueMax="2322.0" valueMin="180.0">180 to 2322 m</quantity>
