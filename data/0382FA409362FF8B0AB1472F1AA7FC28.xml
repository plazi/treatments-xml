<number id="3DE359719362FF8B084F47C719DAFC0D" box="[617,672,938,961]" pageId="1" pageNumber="94" value="1860.0">1860</number>
</bibRefCitation>
) (
<bibRefCitation id="EFBA36A79362FF8B089147C7184FFC0D" author="Stein" box="[695,821,938,961]" pageId="1" pageNumber="94" refString="Stein, J. P. E. F. (1860) Zwei neue Prostemma - Arten. Berliner Entomologische Zeitschrift, 4, 76 - 78. http: // dx. doi. org / 10.1002 / mmnd. 18600040107" type="journal article" year="1860">
Stein, 
<number id="3DE359719362FF8B08DB47C7184FFC0D" box="[765,821,938,961]" pageId="1" pageNumber="94" value="1860.0">1860</number>
</bibRefCitation>
, 
<bibRefCitation id="EFBA36A79362FF8B096647C71890FC0D" author="Kerzhner" box="[832,1002,938,961]" pageId="1" pageNumber="94" refString="Kerzhner, I. M. (2007) Nabidae of the West Indies (Heteroptera). Zoosystematica Rossica, 16 (2), 225 - 234." type="journal article" year="2007">
