<quantity id="4C839B74FFC6B336FE104984FD95FF18" box="[444,580,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="3.45" metricValueMax="3.5" metricValueMin="3.4" pageId="36" pageNumber="37" unit="mm" value="3.45" valueMax="3.5" valueMin="3.4">3.4–3.5 mm</quantity>
long, 
<quantity id="4C839B74FFC6B336FD224984FCE3FF18" box="[654,818,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="1.06" metricValueMax="1.07" metricValueMin="1.05" pageId="36" pageNumber="37" unit="mm" value="1.06" valueMax="1.07" valueMin="1.05">1.05–1.07 mm</quantity>
wide across eyes. Female 3.8–4.0 mm long, 
<quantity id="4C839B74FFC6B336FA964984FF13FF3C" metricMagnitude="-3" metricUnit="m" metricValue="1.225" metricValueMax="1.25" metricValueMin="1.2" pageId="36" pageNumber="37" unit="mm" value="1.225" valueMax="1.25" valueMin="1.2">1.2–1.25 mm</quantity>
