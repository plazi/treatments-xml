<taxonomicName id="26528AEAE5F89D3027AC745C4BD01E58" LSID="urn:lsid:zoobank.org:act:741943E9-A379-42E0-807D-811A596FCE88" class="Insecta" family="Staphylinidae" genus="Lathrobium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lathrobium lingae" order="Coleoptera" pageId="0" pageNumber="21" phylum="Arthropoda" rank="species" species="lingae">Lathrobium lingae</taxonomicName>
