<typeStatus id="AB368817FFC0C375FE9AE2F3FE66FBA3" box="[346,440,1014,1036]" pageId="37" pageNumber="1366">Holotypus</typeStatus>
(
<specimenCount id="628BFD3CFFC0C375FE0BE2F2FE05FBA2" box="[459,475,1015,1037]" pageId="37" pageNumber="1366" type="female">♀</specimenCount>
): 
<collectingCountry id="0C9A7625FFC0C375FE30E2F2FDE5FBA3" box="[496,571,1015,1036]" name="China" pageId="37" pageNumber="1366">CHINA</collectingCountry>
