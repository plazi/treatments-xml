<taxonomicName id="A5E4A9E1B949FF86135A5C1634E3F99B" authority="Saussure, 1877" authorityName="Saussure" authorityYear="1877" box="[287,650,1585,1611]" class="Insecta" family="Phalangopsidae" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="subFamily" subFamily="Cacoplistinae">Cacoplistinae Saussure, 1877</taxonomicName>
