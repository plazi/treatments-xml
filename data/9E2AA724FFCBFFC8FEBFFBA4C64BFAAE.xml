<taxonomicName id="D1836DB1FFCBFFC8FDEDFB53C777FB7E" authority="Emery" authorityName="Emery" box="[602,815,1193,1219]" class="Insecta" family="Formicidae" genus="Plagiolepis" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="alluaudi">
