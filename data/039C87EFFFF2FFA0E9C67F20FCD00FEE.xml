<taxonomicName id="4C354D7AFFF2FFA6E8A078E0FD8108C0" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[197,563,1243,1267]" class="Insecta" family="Blattidae" genus="Blatta" kingdom="Animalia" order="Blattodea" pageId="3" pageNumber="598" phylum="Arthropoda" rank="species" species="orientalis">
