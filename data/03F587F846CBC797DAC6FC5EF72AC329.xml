<paragraph id="8BE336EE46CBC7A8DAC6FBCCF4FBC231" blockId="223.[178,1085,969,1092]" box="[178,683,1072,1092]" pageId="223" pageNumber="1540">
<taxonomicName id="4C5C4D6D46CBC7A8DAC6FBCCF7B5C231" authority=": HEINRICH 1978" authorityName=": HEINRICH" authorityYear="1978" box="[178,485,1072,1092]" class="Insecta" family="Ichneumonidae" genus="Pyramidophorus" kingdom="Animalia" order="Hymenoptera" pageId="223" pageNumber="1540" phylum="Arthropoda" rank="genus">
