<emphasis id="B9191C72FF888D3F7AD20388FE8296E3" bold="true" box="[151,290,1224,1246]" pageId="11" pageNumber="12">
<typeStatus id="54D67EC2FF888D3F7AD20388FF6C96E3" box="[151,204,1224,1246]" pageId="11" pageNumber="12">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C6DBBE3FF888D3F7B6C0388FD0996E3" authority="Riek, 1955: 678" authorityName="Riek" authorityPageNumber="678" authorityYear="1955" box="[297,681,1224,1246]" class="Insecta" family="Neuroptera (awaiting allocation)" genus="Lithosmylidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="lineata">
