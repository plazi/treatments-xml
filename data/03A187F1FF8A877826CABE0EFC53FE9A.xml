<taxonomicName id="4C084D64FF8A87792774BEC0FD6CFB47" authority="Steenstrup, 1881" authorityName="Steenstrup" authorityYear="1881" box="[297,685,1194,1216]" class="Cephalopoda" family="Idiosepiidae" genus="Idiosepius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sepiolida" pageId="6" pageNumber="457" phylum="Mollusca" rank="species" species="pygmaeus">
