<taxonomicName id="4C094D7AF662A4342CFC446BFAA6F333" baseAuthorityName="Galiano" baseAuthorityYear="1986" box="[1277,1401,1611,1637]" class="Arachnida" family="Salticidae" genus="Peckhamia" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="155" phylum="Arthropoda" rank="genus">
