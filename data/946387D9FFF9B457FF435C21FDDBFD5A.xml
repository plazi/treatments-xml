(
<figureCitation id="84F12A4AFFF9B455FF4B5CFCFE97FA2C" box="[159,266,1442,1467]" captionStart-0="FIGURES 1–3" captionStart-1="FIGURES 4–6" captionStartId-0="2.[151,264,969,992]" captionStartId-1="3.[151,264,1970,1993]" captionTargetBox-0="[239,1345,230,914]" captionTargetBox-1="[290,1322,839,1919]" captionTargetId-0="figure@2.[226,1360,218,921]" captionTargetId-1="figure@3.[226,1360,799,1956]" captionTargetPageId-0="2" captionTargetPageId-1="3" captionText-0="FIGURES 1–3. Araucariocladus amfractus sp. nov., male holotype, habitus dorsal. 1, dorsal; 2, lateral; 3, habitus ventral; Scale bar: 2.0 mm." captionText-1="FIGURES 4–6. Araucariocladus spp., antenna. 4–5 A. amfractus sp. nov., 4, male holotype, 18 antennomeres; 5, male paratype, 17 antennomeres. 6, A. hiems, male, 17 antennomeres; Scale bar: 2.0 mm." httpUri-0="https://zenodo.org/record/2614284/files/figure.png" httpUri-1="https://zenodo.org/record/2614290/files/figure.png" pageId="1" pageNumber="563">Figs. 1–5</figureCitation>
, 
<figureCitation id="84F12A4AFFF9B455FEC25CFCFED2FA2C" box="[278,335,1442,1467]" captionStart-0="FIGURES 7–15" captionStart-1="FIGURES 16–17" captionStartId-0="4.[151,264,1937,1960]" captionStartId-1="5.[151,264,857,880]" captionTargetBox-0="[231,1357,340,1879]" captionTargetBox-1="[331,1248,240,793]" captionTargetId-0="figure@4.[226,1360,238,1879]" captionTargetId-1="figure@5.[151,1435,193,832]" captionTargetPageId-0="4" captionTargetPageId-1="5" captionText-0="FIGURES 7–15. Araucariocladus amfractus sp. nov., 7–9, male holotype 7, male head, ventral; 8, male head, lateral; 9, pronotum, dorsal; 8–15, male paratype, terminalia: 10, sternum VIII and pygidium, ventral; 11, syntergite, dorsal; 12, sternum IX, dorsal; 13–15, aedeagus, 13, dorsal, 14, lateral, 15, ventral. Scale bar: 0.5 mm (7–9); 0.5 mm (10–15)." captionText-1="FIGURES 16–17. Araucariocladus spp., labial palp, dorsal view. 16, A. amfractus, male paratype. 17, A. hiems, male paratype. Scale bar: 0.125 mm." httpUri-0="https://zenodo.org/record/2614292/files/figure.png" httpUri-1="https://zenodo.org/record/2614294/files/figure.png" pageId="1" pageNumber="563">7–16</figureCitation>
