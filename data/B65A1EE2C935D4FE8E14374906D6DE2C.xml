, 
<taxonomicName id="028F78C05218192FCB260D91F4D1B9E5" genus="A." lsidName="A. colossus" pageId="3" pageNumber="74" rank="species" species="colossus">A. colossus</taxonomicName>
and 
<taxonomicName id="D21DB89348CAE4520C118F0518CD9139" genus="A." lsidName="A. guatemalensis" pageId="3" pageNumber="74" rank="species" species="guatemalensis">A. guatemalensis</taxonomicName>
