<taxonomicName id="5756620A51E21F170B8DDEC7A7E3A6D8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:231929" authority="Forel (Atopogyne)" authorityName="Forel (Atopogyne)" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Uploaded - Cremastogaster africana subsp. laurenti" lsidName-HNS="Cremastogaster africana subsp. laurenti Forel" order="Hymenoptera" pageNumber="70" phylum="Arthropoda" rank="subSpecies" species="africana" subSpecies="laurenti">Laurenti</taxonomicName>
