<paragraph id="52F74D75B33F634CB95B5DE423538F5B" pageId="9" pageNumber="136">
<taxonomicName id="2B004323EF9328E7691988FEA90044AC" class="Magnoliopsida" family="Betulaceae" genus="Betula" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Betula ermanii" order="Fagales" pageId="9" pageNumber="136" phylum="Tracheophyta" rank="species" species="ermanii">Betula ermanii</taxonomicName>
