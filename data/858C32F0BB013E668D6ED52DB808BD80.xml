<taxonomicName id="15554219434B24B2179E1CD5F579E4D4" authorityName="Eggers" authorityYear="1923" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus polyodon" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="polyodon">
