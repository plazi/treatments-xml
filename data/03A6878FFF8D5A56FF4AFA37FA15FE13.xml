. Mediterranean, 
<collectingCountry id="F3187609FF8C5A56FDFAFE40FDECFE13" box="[551,620,392,418]" name="Japan" pageId="13" pageNumber="14">Japan</collectingCountry>
, 
<collectingCountry id="F3187609FF8C5A56FDA5FE40FD43FE13" box="[632,707,392,418]" name="South Korea" pageId="13" pageNumber="14">Korea</collectingCountry>
, 
<collectingCountry id="F3187609FF8C5A56FD13FE40FCB9FE13" box="[718,825,392,418]" name="Bermuda" pageId="13" pageNumber="14">Bermuda</collectingCountry>
(cave), Gulf of 
<collectingCountry id="F3187609FF8C5A56FC2BFE40FBD2FE13" box="[1014,1106,392,418]" name="Mexico" pageId="13" pageNumber="14">Mexico</collectingCountry>
, Society Islands. 
<quantity id="4CF79B7CFF8C5A56FAC2FE40FA11FE13" box="[1311,1425,392,418]" metricMagnitude="1" metricUnit="m" metricValue="1.525" metricValueMax="2.55" metricValueMin="0.5" pageId="13" pageNumber="14" unit="m" value="15.25" valueMax="25.5" valueMin="5.0">5–25.5 m</quantity>
.
