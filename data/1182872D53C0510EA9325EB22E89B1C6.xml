<taxonomicName id="5D859954B159A713E2AD3E23BB6D6361" authority="var. flaviclavus" authorityName="Dodd" authorityYear="1920" class="Insecta" family="Platygastridae" genus="Gryonoides" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gryonoides flaviclavus subsp. var. var. flaviclavus" order="Hymenoptera" pageId="0" pageNumber="523" phylum="Arthropoda" rank="variety" species="flaviclavus" subSpecies="var." variety="flaviclavus">Gryonoides flaviclavus var. flaviclavus</taxonomicName>
