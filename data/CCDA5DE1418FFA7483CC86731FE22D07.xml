<pageBreakToken id="990A9F9253741CB8AA2BCCF23A0FB9F1" pageId="132" pageNumber="132" start="start">71</pageBreakToken>
. 
<taxonomicName id="CC16290A03BC641875574D1966D609E5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:144429" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica virulens Smith" order="Hymenoptera" pageId="132" pageNumber="132" phylum="Arthropoda" rank="species" species="virulens">Myrmica virulens</taxonomicName>
