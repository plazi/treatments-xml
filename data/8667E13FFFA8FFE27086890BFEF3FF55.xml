<taxonomicName id="C9CE2BAAFFABFFE4710D8A28FDC3F8F8" authority="Huang &amp; Huang &amp; Xu, 2019" authorityName="Huang &amp; Huang &amp; Xu" authorityYear="2019" box="[284,527,1890,1914]" class="Adenophorea" family="Ironidae" genus="Thalassironus" kingdom="Animalia" order="Enoplida" pageId="2" pageNumber="172" phylum="Nematoda" rank="species" species="filiformis" status="sp. nov.">
<emphasis id="3CBA8C3BFFABFFE4710D8A28FDC3F8F8" box="[284,527,1890,1914]" italics="true" pageId="2" pageNumber="172">Thalassironus filiformis</emphasis>
