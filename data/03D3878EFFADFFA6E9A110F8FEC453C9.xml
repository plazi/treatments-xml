<taxonomicName id="4C7A4D1BFFAEFFA6EA7211A9FDBD53F7" box="[287,567,1463,1487]" class="Arachnida" family="Laelapidae" genus="Gigantolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="46" pageNumber="46" phylum="Arthropoda" rank="species" species="oudemansi">
<emphasis id="B90EEA8AFFAEFFA6EA7211A9FDBD53F7" box="[287,567,1463,1487]" italics="true" pageId="46" pageNumber="46">Gigantolaelaps oudemansi</emphasis>
