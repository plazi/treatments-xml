<taxonomicName id="B4192BA8EBB0C3631B45E20117FB4528" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33918" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole partita Mayr" order="Hymenoptera" pageNumber="79" phylum="Arthropoda" rank="species" species="partita">Pheidole partita Mayr</taxonomicName>
