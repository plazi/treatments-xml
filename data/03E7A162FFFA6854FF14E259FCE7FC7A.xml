<subSubSection id="C35443FFFFFA6854FF14E364FD40FCEE" pageId="9" pageNumber="341" type="etymology">
<paragraph id="8BF11074FFFA6854FF14E364FD40FCEE" blockId="9.[151,1436,546,969]" pageId="9" pageNumber="341">
<emphasis id="B93ACC66FFFA6854FF14E364FED9FC8A" bold="true" box="[151,290,799,825]" pageId="9" pageNumber="341">Etymology:</emphasis>
