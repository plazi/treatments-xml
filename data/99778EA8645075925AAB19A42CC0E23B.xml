<taxonomicName id="6C7A8753DEEC5E27710033E0545A1F9B" authority="L." class="Magnoliopsida" family="Boraginaceae" genus="Onosma" kingdom="Plantae" order="Boraginales" pageId="null" pageNumber="64" phylum="Tracheophyta" rank="genus">
