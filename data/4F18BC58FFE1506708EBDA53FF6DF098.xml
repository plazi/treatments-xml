<number id="71791F69FFE350670C30D8F9FBFDF0BB" box="[1100,1127,1973,1996]" pageId="7" pageNumber="42" value="20.0">20</number>
female
</specimenCount>
<number id="71791F69FFE350670CC2D8F9FB51F0BB" box="[1214,1227,1973,1996]" pageId="7" pageNumber="42" value="8.0">8</number>
th sternite; 
