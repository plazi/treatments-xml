<number id="71791F69FFE350670A9DD8DCFD65F0D0" box="[737,767,1936,1959]" pageId="7" pageNumber="42" value="15.0">15</number>
: male 
<number id="71791F69FFE350670B38D8DCFCCBF0D0" box="[836,849,1936,1959]" pageId="7" pageNumber="42" value="8.0">8</number>
th tergite; 
<number id="71791F69FFE350670BC9D8DCFC49F0D0" box="[949,979,1936,1959]" pageId="7" pageNumber="42" value="16.0">16</number>
: male 
<number id="71791F69FFE350670C64D8DCFBBFF0D0" box="[1048,1061,1936,1959]" pageId="7" pageNumber="42" value="8.0">8</number>
th sternite; 
<number id="71791F69FFE350670CE8D8DCFB28F0D0" box="[1172,1202,1936,1959]" pageId="7" pageNumber="42" value="17.0">17</number>
