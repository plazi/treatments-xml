<quantity id="617B66EEFFF1E06EFB3B6485FEAEFA2E" metricMagnitude="3" metricUnit="m" metricValue="1.175" metricValueMax="1.35" metricValueMin="1.0" pageId="33" pageNumber="34" unit="m" value="1175.0" valueMax="1350.0" valueMin="1000.0">1000­ 1350 m</quantity>
, 
<date id="D23DEDCBFFF1E06EFEAB64EDFE2FFA2E" box="[374,486,1487,1513]" pageId="33" pageNumber="34" value="1994-07">
