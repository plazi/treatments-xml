<taxonomicName id="4C534D1EFFDAD11DFABAFB34FEF002F9" authority="Illiger" authorityName="Illiger" class="Insecta" family="Formicidae" genus="Dorylus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="69" phylum="Arthropoda" rank="species" species="nigricans">
<emphasis id="B927EA8FFFDAD11DFABAFB34FEBC02FA" italics="true" pageId="8" pageNumber="69">Dorylus nigricans</emphasis>
Illiger
</taxonomicName>
and 
<taxonomicName id="4C534D1EFFDAD11DFE7DFB5BFD3502F9" authority="Linnaeus" authorityName="Linnaeus" box="[389,648,1225,1250]" class="Insecta" family="Formicidae" genus="Dorylus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="69" phylum="Arthropoda" rank="species" species="helvolus">
<emphasis id="B927EA8FFFDAD11DFE7DFB5BFDBB02F9" box="[389,518,1225,1250]" italics="true" pageId="8" pageNumber="69">D. helvolus</emphasis>
(Linnaeus)
</taxonomicName>
