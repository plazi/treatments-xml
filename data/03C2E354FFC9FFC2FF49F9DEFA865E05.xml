<paragraph id="8BD45242FFC9FFCDFF49F9DEFD6B5BDC" blockId="7.[151,665,1643,1669]" box="[151,665,1643,1669]" pageId="7" pageNumber="262">
<heading id="D09CE52EFFC9FFCDFF49F9DEFD6B5BDC" bold="true" box="[151,665,1643,1669]" fontSize="11" level="1" pageId="7" pageNumber="262" reason="1">
<taxonomicName id="4C6B29C1FFC9FFCDFF49F9DEFD6B5BDC" authority="Muller &amp; Henle, 1841" authorityName="Muller &amp; Henle" authorityYear="1841" box="[151,665,1643,1669]" class="Elasmobranchii" family="Rajidae" genus="Leucoraja" kingdom="Animalia" order="Rajiformes" pageId="7" pageNumber="262" phylum="Chordata" rank="species" species="naevus">
<emphasis id="B91F8E50FFC9FFCDFF49F9DEFD6B5BDC" bold="true" box="[151,665,1643,1669]" pageId="7" pageNumber="262">
<emphasis id="B91F8E50FFC9FFCDFF49F9DEFE9D5BDC" bold="true" box="[151,367,1643,1669]" italics="true" pageId="7" pageNumber="262">Leucoraja naevus</emphasis>
(Müller &amp; Henle, 1841)
</emphasis>
</taxonomicName>
