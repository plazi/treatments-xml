<paragraph id="8BED0424FFD21754FF3A0BC03B1CFECB" blockId="87.[151,1436,151,319]" pageId="87" pageNumber="88">
<materialsCitation id="3B3A0E79FFD21754FF3A0BC03B18FECB" collectionCode="SMF" country="Angola" county="Namib Province" location="Epigynum" municipality="Material" pageId="87" pageNumber="88" specimenCount="9" specimenCount-female="5" specimenCount-male="4">
