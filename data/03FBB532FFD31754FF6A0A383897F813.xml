<collectionCode id="ED439CE1FFD21754FF040A703B18FECB" box="[249,306,295,319]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="87" pageNumber="88">SMF</collectionCode>
