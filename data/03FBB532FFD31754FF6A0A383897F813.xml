<collectingCountry id="F34544B4FFD21754FE570A53382BFEE8" box="[426,513,260,284]" name="Angola" pageId="87" pageNumber="88">Angola</collectingCountry>
: 
<emphasis id="B926D836FFD21754FDF10A5338E9FEE8" box="[524,707,260,284]" italics="true" pageId="87" pageNumber="88">
<collectingCounty id="628C7CA8FFD21754FDF10A5338E9FEE8" box="[524,707,260,284]" pageId="87" pageNumber="88">Namib Province</collectingCounty>
