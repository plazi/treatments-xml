<specimenCount id="9D54CFADFFD21754FACA0A533F73FEEF" box="[1335,1369,260,284]" pageId="87" pageNumber="88" type="male">4♂</specimenCount>
<specimenCount id="9D54CFADFFD21754FAA40A533F56FEEF" box="[1369,1404,260,284]" pageId="87" pageNumber="88" type="female">3♀</specimenCount>
