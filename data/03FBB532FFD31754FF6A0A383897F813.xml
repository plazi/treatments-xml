<taxonomicName id="4C527FA7FFD21754FE670BEB3802FF20" authorityName="Simon" authorityYear="1910" box="[410,552,188,212]" class="Arachnida" family="Pholcidae" genus="Smeringopus" kingdom="Animalia" order="Araneae" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="atomarius">
