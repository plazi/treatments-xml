<subSubSection id="DDCA6555FF99670EFF20F82FFB33AAF4" box="[199,1202,1939,1964]" pageId="2" pageNumber="60" type="etymology">
<paragraph id="956F36DEFF99670EFF20F82FFB33AAF4" blockId="2.[151,1437,1615,2036]" box="[199,1202,1939,1964]" pageId="2" pageNumber="60">
<emphasis id="A7A4EACCFF99670EFF20F82FFEC7AAF3" bold="true" box="[199,326,1940,1964]" pageId="2" pageNumber="60">Etymology</emphasis>
