<taxonomicName id="52D04D5DFF99670EFBE1F82FFBB8AAF3" box="[1030,1081,1940,1964]" class="Liliopsida" family="Orchidaceae" genus="Inti" kingdom="Plantae" order="Asparagales" pageId="2" pageNumber="60" phylum="Tracheophyta" rank="species" species="levis">
