<collectingRegion id="49F52BDE197EFF92697194281B22FBBA" box="[781,1027,1062,1084]" country="Australia" name="Northern Territory" pageId="65" pageNumber="65">Northern Territory</collectingRegion>
, 
<collectingCountry id="F326A5AC197EFF926E5D94281BB1FBBA" box="[1057,1168,1062,1084]" name="Australia" pageId="65" pageNumber="65">Australia</collectingCountry>
