<taxonomicName id="69E64D628B6E87614C610DD7FC09FCB2" box="[816,910,746,771]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="theresae">
