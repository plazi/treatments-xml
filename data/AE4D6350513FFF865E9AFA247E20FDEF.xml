<taxonomicName id="E1E4A9C5513DFF875F14F9727FADF935" authority="Tosquinet, 1896" authorityName="Tosquinet" authorityYear="1896" box="[281,656,1700,1722]" class="Insecta" family="Ichneumonidae" genus="Belesica" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="167" phylum="Arthropoda" rank="species" species="pictipennis">
