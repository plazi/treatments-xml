specimen:
</emphasis>
<collectionCode id="B09F05ACFFB1FFE31B4FFC2B75FAFC29" box="[358,447,955,976]" country="Switzerland" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="21" type="Museum">MHNG</collectionCode>
<date id="A230BBA9FFB1FFE31BE2FC2B7663FC29" box="[459,550,955,976]" pageId="21" value="2232-05">
<collectingDate id="B2744241FFB1FFE31BE2FC2B7663FC29" box="[459,550,955,976]" pageId="21" value="2232-05">V-2232</collectingDate>
</date>
