<paragraph id="D6319D69FFB1FFE31AB8FC2B74B1FC16" blockId="21.[145,762,954,1743]" pageId="21">
<materialsCitation id="66E69734FFB1FFE31AB8FC2B74B4FC16" collectingDate="2232-05" collectionCode="MHNG" pageId="21" specimenCount="1" typeStatus="holotype">
