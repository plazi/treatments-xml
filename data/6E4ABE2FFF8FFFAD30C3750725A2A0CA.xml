<taxonomicName id="21E374BAFF8FFFAD323172D2258EA396" box="[613,732,1470,1494]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="234" phylum="Arthropoda" rank="species" species="atlantis">
