<taxonomicName id="4C562CA70A3FF54AFC73CDBAAEABFE3D" authority="Leme" authorityName="Leme" box="[908,1183,404,429]" class="Gastropoda" family="Strophocheilidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="58" pageNumber="59" phylum="Mollusca" rank="family">Megalobulimidae Leme</taxonomicName>
