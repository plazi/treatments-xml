<taxonomicName id="8BF8FC93FFCCFFD6D34D2A6BB7C64FC9" baseAuthorityName="REY" baseAuthorityYear="1882" class="Insecta" family="Staphylinidae" genus="Sepedophilus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="422" phylum="Arthropoda" rank="species" species="aestivus">
