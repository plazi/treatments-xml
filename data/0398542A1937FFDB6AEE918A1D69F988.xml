<emphasis id="B945392E1937FFDB6B6B92951EF1FD36" bold="true" box="[279,464,667,688]" pageId="8" pageNumber="8">
<specimenCode id="DB974D471937FFDB6B6B92951EF1FD36" box="[279,464,667,688]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="8" pageNumber="8">AMNH 719010</specimenCode>
