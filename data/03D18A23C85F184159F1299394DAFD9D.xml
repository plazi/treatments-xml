<taxonomicName id="4C7840B6C85C18405A362FDD94B9FC24" authorityName="Rovereto" authorityYear="1914" box="[852,1011,910,929]" class="Aves" family="Psilopteridae" genus="Procariama" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gruiformes" pageId="30" pageNumber="85" phylum="Chordata" rank="species" species="simplex">
