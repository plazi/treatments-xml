<taxonomicName id="4C7840B6C85C1840586B28FC96E2FB44" authorityName="Rovereto" authorityYear="1914" box="[265,424,1198,1217]" class="Aves" family="Psilopteridae" genus="Procariama" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gruiformes" pageId="30" pageNumber="85" phylum="Chordata" rank="species" species="simplex">
