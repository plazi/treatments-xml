</taxonomicName>
</collectedFrom>
(
<taxonomicName id="33F37B88F32916499DC6CA939116F97E" box="[420,591,1749,1770]" class="Magnoliopsida" family="Hamamelidaceae" kingdom="Plantae" order="Saxifragales" pageId="7" pageNumber="382" phylum="Tracheophyta" rank="family">Hamamelidaceae</taxonomicName>
), 
<taxonomicName id="33F37B88F32916499E3DCA93919DF97E" box="[607,708,1749,1770]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="7" pageNumber="382" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
, and 
<taxonomicName id="33F37B88F32916499E94CA93900BF97E" box="[758,850,1749,1770]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="382" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
; 
<collectingCountry id="8CE4409BF32916499F3ECA939673F97E" box="[860,1322,1749,1770]" isEnumeration="true" pageId="7" pageNumber="382">China, India, Indonesia, Japan, Malaysia, Nepal</collectingCountry>
, 
<collectingCountry id="8CE4409BF32916499951CA9396D1F97E" box="[1331,1416,1749,1770]" name="Thailand" pageId="7" pageNumber="382">Thailand</collectingCountry>
......................................................................... 
