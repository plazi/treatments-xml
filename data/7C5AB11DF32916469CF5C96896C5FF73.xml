</taxonomicName>
</collectedFrom>
(
<taxonomicName id="33F37B88F32916499D64CB0C923CF8CB" box="[262,357,1866,1887]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="382" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
); 
<collectingCountry id="8CE4409BF32916499D17CB0C92E8F8CB" box="[373,433,1866,1887]" name="China" pageId="7" pageNumber="382">China</collectingCountry>
, 
<collectingCountry id="8CE4409BF32916499DD9CB0C92ABF8CB" box="[443,498,1866,1887]" name="Japan" pageId="7" pageNumber="382">Japan</collectingCountry>
................................................... 
