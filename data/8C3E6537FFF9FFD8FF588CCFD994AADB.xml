<number id="B25FC606FFF9FFD8FCD58F63D93BAC1E" box="[794,853,844,866]" pageId="4" pageNumber="28" value="21.0" valueMax="26.0" valueMin="16.0">16-26</number>
<taxonomicName id="C397AFA2FFF9FFD8FF588F44D86EACFD" authority="Fieber" authorityName="Fieber" box="[151,512,875,897]" class="Insecta" family="Issidae" genus="Agalmatium" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="28" phylum="Arthropoda" rank="species" species="melanophleps">
