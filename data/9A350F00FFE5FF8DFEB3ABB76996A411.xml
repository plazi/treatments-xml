<taxonomicName id="D59CC595FFE4FF8BFE28AD556AE4A71B" authority="Csuzdi, 2005" authorityName="Csuzdi" authorityYear="2005" box="[435,640,536,555]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" kingdom="Animalia" order="Haplotaxida" pageId="11" pageNumber="3049" phylum="Annelida" rank="species" species="coeruleoviridis" status="sp. nov." subGenus="Diplothecodrilus">
