<heading id="9A4B2D2CFFC4BA04FC2CFE2EFB6AFE04" bold="true" box="[1009,1255,413,437]" centered="true" fontSize="10" level="2" pageId="37" pageNumber="928" reason="2">
<emphasis id="F3C84652FFC4BA04FC2CFE2EFB6AFE04" bold="true" box="[1009,1255,413,437]" pageId="37" pageNumber="928">
<taxonomicName id="06BCE1C3FFC4BA04FC2CFE2EFBEDFE04" authority="Schubart &amp; Ng, 2020" authorityName="Schubart &amp; Ng" authorityYear="2020" box="[1009,1120,413,437]" class="Malacostraca" family="Sesarmidae" genus="Cristarma" kingdom="Animalia" order="Decapoda" pageId="37" pageNumber="928" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="F3C84652FFC4BA04FC2CFE2EFBEDFE04" bold="true" box="[1009,1120,413,437]" italics="true" pageId="37" pageNumber="928">Cristarma</emphasis>
