<taxonomicName id="4C71E0ABBD2011053F0FFCC1FC5DFCDD" authorityName="Broun" authorityYear="1921" box="[842,907,818,839]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="strialis">
