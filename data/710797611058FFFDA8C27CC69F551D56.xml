<quantity id="3E568B92105AFFFDAA587A5D9B2E1D4C" box="[287,324,841,862]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="50" pageNumber="122" unit="m" value="1.0">1m</quantity>
