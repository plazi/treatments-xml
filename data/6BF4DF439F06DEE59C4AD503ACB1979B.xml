<taxonomicName id="0EBCACF49C106129E4B570ABAF115B5F" class="Insecta" family="Staphylinidae" genus="Platandria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platandria carolinae" order="Coleoptera" pageId="5" pageNumber="124" phylum="Arthropoda" rank="species" species="carolinae">Platandria carolinae</taxonomicName>
