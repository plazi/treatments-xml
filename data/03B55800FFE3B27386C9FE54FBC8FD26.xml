<subSubSection id="C306BA9DFFE3B27386C9FD81FBC8FD26" pageId="2" pageNumber="27" type="description">
<paragraph id="8BA3E916FFE3B27386C9FD81FBC8FD26" blockId="2.[151,1436,512,719]" pageId="2" pageNumber="27">
<taxonomicName id="4C1C9295FFE3B27386C9FD81FCB8FD6E" authority="Belk &amp; Pereira, 1982" authorityName="Belk &amp; Pereira" authorityYear="1982" box="[151,772,620,647]" class="Branchiopoda" family="Thamnocephalidae" genus="Thamnocephalus" kingdom="Animalia" order="Anostraca" pageId="2" pageNumber="27" phylum="Arthropoda" rank="species" species="venezuelensis">
