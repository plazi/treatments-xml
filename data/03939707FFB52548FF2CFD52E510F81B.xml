</geoCoordinate>
<date id="FF8400D1FFB52548FE8AFC67E18EFC63" box="[305,425,986,1012]" pageId="9" pageNumber="46" value="2001-05-17">
<number id="3DF23436FFB52548FE8AFC67E141FC63" box="[305,358,986,1012]" pageId="9" pageNumber="46" value="17.5">17.5</number>
