<taxonomicName id="8803B5FAEF16C3370C93ABC6704DF903" class="Liliopsida" family="Marantaceae" genus="Calathea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Calathea lanata" order="Zingiberales" pageId="262" pageNumber="263" phylum="Tracheophyta" rank="species" species="lanata">Calathea lanata</taxonomicName>
Peterson (
<taxonomicName id="F40F7BCD2B3A70450537B3071D84419C" genus="Marantaceae" lsidName="Marantaceae" pageId="262" pageNumber="263" rank="genus">Marantaceae</taxonomicName>
