; intertentorial pit distance = 
<quantity id="4CEE2682F6469E2CFA9F17F547BF5B6A" metricMagnitude="-4" metricUnit="m" metricValue="1.675" metricValueMax="1.7" metricValueMin="1.65" pageId="1" pageNumber="55" unit="mm" value="0.1675" valueMax="0.17" valueMin="0.165">0.165–0.17 mm</quantity>
