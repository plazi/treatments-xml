<caption id="3F80688F8F25EF0F12FFF8458F1BF853" httpUri="https://zenodo.org/record/253718/files/figure.png" pageId="13" pageNumber="14" targetBox="[179,1400,385,1920]" targetPageId="13">
<paragraph id="6B4038078F25EF0F12FFF8458F1BF853" blockId="13.[151,1436,1948,2001]" pageId="13" pageNumber="14">
<emphasis id="598BE4158F25EF0F12FFF8458B4CF833" bold="true" box="[151,277,1948,1970]" pageId="13" pageNumber="14">FIGURE 9.</emphasis>
<taxonomicName id="ACFF43848F25EF0F1377F8448847F830" box="[287,542,1949,1970]" class="Insecta" family="Cicadellidae" genus="Coriojassus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="yunnanensis" status="sp. nov.">
<emphasis id="598BE4158F25EF0F1377F8448847F830" box="[287,542,1949,1970]" italics="true" pageId="13" pageNumber="14">Coriojassus yunnanensis</emphasis>
</taxonomicName>
<emphasis id="598BE4158F25EF0F1040F8458822F833" bold="true" box="[552,635,1948,1969]" pageId="13" pageNumber="14">
<taxonomicNameLabel id="42B8596E8F25EF0F1040F8458822F833" box="[552,635,1948,1969]" pageId="13" pageNumber="14" rank="species">sp. nov.</taxonomicNameLabel>
