<quantity id="6978DCE6E80CFFF0FB09A9ACFAC5A780" box="[1230,1350,1950,1975]" metricMagnitude="-2" metricUnit="m" metricValue="1.052" pageId="3" pageNumber="404" unit="mm" value="10.52">10.52 mm</quantity>
