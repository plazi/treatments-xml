<subSubSection id="50F80EB85745FF923E58F83F0272F7A0" pageId="25" pageNumber="346" type="etymology">
<paragraph id="185D5D335745FF923E58F83F0272F7A0" blockId="25.[794,1399,2010,2098]" pageId="25" pageNumber="346">
<emphasis id="2A9681215745FF923E58F83F04A0F860" bold="true" box="[794,917,2010,2034]" pageId="25" pageNumber="346">Etymology</emphasis>
