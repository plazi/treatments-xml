<caption id="9A93664F8B5CFFCEFF2EFB20FBF9FAF2" ID-DOI="http://doi.org/10.5281/zenodo.4669742" ID-Zenodo-Dep="4669742" box="[188,1135,1267,1286]" httpUri="https://zenodo.org/record/4669742/files/figure.png" pageId="17" pageNumber="2367" startId="17.[188,257,1267,1286]" targetBox="[153,1173,133,1224]" targetPageId="17">
<paragraph id="CE5336C78B5CFFCEFF2EFB20FBF9FAF2" blockId="17.[188,1135,1267,1286]" box="[188,1135,1267,1286]" pageId="17" pageNumber="2367">
