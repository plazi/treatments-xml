<taxonomicName id="09EC4D448B5CFFCEFEC0FB20FD98FAF2" authority="Ueckermann &amp; Grout, 2007" authorityName="Ueckermann &amp; Grout" authorityYear="2007" box="[338,526,1267,1286]" class="Arachnida" family="Iolinidae" genus="Kakamasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="17" pageNumber="2367" phylum="Arthropoda" rank="species" species="cataracta" status="sp. n.">
