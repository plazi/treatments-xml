<taxonomicName id="4C404D2BFF8CBA65BE42F923AA761EA0" box="[783,879,1676,1701]" class="Actinopterygii" family="Anguillidae" genus="Anguilla" kingdom="Animalia" order="Anguilliformes" pageId="2" pageNumber="142" phylum="Chordata" rank="genus">
