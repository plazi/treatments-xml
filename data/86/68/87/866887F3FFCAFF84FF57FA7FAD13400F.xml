<subSubSection box="[151,566,1413,1439]" pageId="1" pageNumber="414" type="nomenclature">
<paragraph blockId="1.[151,566,1413,1439]" box="[151,566,1413,1439]" pageId="1" pageNumber="414">
<heading bold="true" box="[151,566,1413,1439]" fontSize="11" level="1" pageId="1" pageNumber="414" reason="1">
<emphasis bold="true" box="[151,566,1413,1439]" pageId="1" pageNumber="414">
Subfamily 
<taxonomicName authority="Sieg, 1980" authorityName="Sieg" authorityYear="1980" box="[287,566,1413,1439]" class="Malacostraca" family="Tanaididae" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="414" phylum="Arthropoda" rank="subFamily" subFamily="Sinelobinae">
Sinelobinae 
<bibRefCitation author="Sieg, J." box="[440,566,1413,1439]" pageId="1" pageNumber="414" pagination="1 - 267" refId="ref8678" refString="Sieg, J. (1980) Taxonomische monographie der Tanaidae Dana 1849 (Crustacea: Tanaidacea). Abhandlungen der Senckenbergischen Naturforschenden Gesellschaft, 537, 1 - 267." type="journal article" year="1980">Sieg, 1980</bibRefCitation>
