<taxonomicName class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis nobilis" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="nobilis">Chrysis nobilis</taxonomicName>
, Kl.; 
<taxonomicName authority="Lepeletier &amp; Serville, 1828" authorityName="Lepeletier &amp; Serville" authorityYear="1828" class="Insecta" family="Chrysididae" genus="Pyria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pyria producta" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="producta">Pyria producta</taxonomicName>
