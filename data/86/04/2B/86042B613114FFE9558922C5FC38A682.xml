<materialsCitation collectingDate="1981-09-12" country="United Kingdom" location="Little Lever" municipality="Smiths Reservoirs" pageId="10" pageNumber="696" specimenCount="1" stateProvince="Lancashire" typeStatus="holotype">
<typeStatus box="[146,253,209,233]" pageId="10" pageNumber="696">Holotype</typeStatus>
female: 
<collectingCountry box="[371,470,209,233]" name="United Kingdom" pageId="10" pageNumber="696">England</collectingCountry>
, 
<collectingRegion box="[492,620,209,233]" country="United Kingdom" name="Lancashire" pageId="10" pageNumber="696">Lancashire</collectingRegion>
, near 
<collectingMunicipality box="[710,929,209,233]" pageId="10" pageNumber="696">Smiths Reservoirs</collectingMunicipality>
