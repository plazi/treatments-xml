, 
<date box="[146,265,242,266]" pageId="10" pageNumber="696" value="1981-09-12">
<collectingDate box="[146,265,242,266]" pageId="10" pageNumber="696" value="1981-09-12">12.ix.1981</collectingDate>
</date>
