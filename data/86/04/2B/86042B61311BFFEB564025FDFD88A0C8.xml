female: 
<collectingCountry box="[352,451,776,800]" name="United Kingdom" pageId="8" pageNumber="694">England</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:86042B61311BFFEB564025FDFD88A0C8:0B72CCAC3114FFEB55992530FD31A73D" box="[463,692,776,800]" country="United Kingdom" municipality="Norwood" name="South East London" pageId="8" pageNumber="694">South East London</location>
