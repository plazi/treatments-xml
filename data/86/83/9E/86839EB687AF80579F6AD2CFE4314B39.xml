<paragraph pageId="3" pageNumber="176">
<emphasis bold="true" pageId="3" pageNumber="176">Figures 7-10.</emphasis>
<taxonomicName authority="Konow, 1902" authorityName="Konow" authorityYear="1902" class="Insecta" family="Magnoliaceae" genus="Cladiucha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cladiucha insolita" order="Hymenoptera" pageId="3" pageNumber="176" phylum="Arthropoda" rank="species" species="insolita">
<emphasis italics="true" pageId="3" pageNumber="176">Cladiucha insolita</emphasis>
</taxonomicName>
