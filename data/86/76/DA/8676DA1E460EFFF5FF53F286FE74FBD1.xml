<emphasis box="[573,697,982,1007]" italics="true" pageId="117" pageNumber="118">
<taxonomicName box="[573,692,982,1007]" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" kingdom="Plantae" order="Ericales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="genus">Vaccinium</taxonomicName>
