<taxonomicName class="Insecta" family="Tephritidae" genus="Dacus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dacus longistylus" order="Diptera" pageId="1" pageNumber="138" phylum="Arthropoda" rank="species" species="longistylus">Dacus longistylus</taxonomicName>
