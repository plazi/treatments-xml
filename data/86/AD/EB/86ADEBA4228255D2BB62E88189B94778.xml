<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Dolichos" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dolichos scaber" order="Fabales" pageId="0" pageNumber="67" phylum="Tracheophyta" rank="species" species="scaber">
<emphasis italics="true" pageId="0" pageNumber="67">Dolichos scaber</emphasis>
</taxonomicName>
