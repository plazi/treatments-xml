<geoCoordinate box="[895,973,293,315]" degrees="40.46" direction="north" orientation="latitude" pageId="41" pageNumber="46" precision="555" value="40.46">N40.46</geoCoordinate>
<geoCoordinate box="[989,1075,293,315]" degrees="81.57" direction="west" orientation="longitude" pageId="41" pageNumber="46" precision="555" value="-81.57">W81.57</geoCoordinate>
], 
<date box="[178,282,322,344]" pageId="41" pageNumber="46" value="2007-06-16">16.6.2007</date>
, 
<specimenCount box="[292,326,322,349]" pageId="41" pageNumber="46" type="female">1♀</specimenCount>
