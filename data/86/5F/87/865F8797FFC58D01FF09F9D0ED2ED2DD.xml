<emphasis bold="true" box="[178,269,1542,1564]" italics="true" pageId="10" pageNumber="15">Andrena</emphasis>
(
<emphasis bold="true" box="[284,375,1542,1564]" italics="true" pageId="10" pageNumber="15">Andrena</emphasis>
) 
<emphasis bold="true" box="[390,481,1542,1564]" italics="true" pageId="10" pageNumber="15">clarkella</emphasis>
