<subSubSection box="[197,1107,711,738]" pageId="29" pageNumber="30" type="etymology">
<paragraph blockId="29.[151,1437,152,738]" box="[197,1107,711,738]" pageId="29" pageNumber="30">
<emphasis bold="true" box="[197,338,711,737]" pageId="29" pageNumber="30">Etymology.</emphasis>
