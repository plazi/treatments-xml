<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181737" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole humeridens Wilson" lsidName-HNS="Pheidole humeridens Wilson" order="Hymenoptera" pageNumber="282" phylum="Arthropoda" rank="species" species="humeridens">humeridens</taxonomicName>
