(
<bibRefCitation id="EF964B678E10FF91FB16EA7CFA65BF5C" box="[1259,1431,1833,1858]" pageId="22" pageNumber="223" refId="ref23038" refString="Holthuis, L. B. (1952) Reports of the Lund University Chile Expedition 1948 - 1949. 5. The Crustacea Decapoda Macrura of Chile. Lunds Universitets Arsskrift, N. F., Avd. 2, 47 (10), 1 - 110." type="journal article">Holthuis 1952</bibRefCitation>
; 
<bibRefCitation id="EF964B678E10FF91FF6AEA18FE4ABF78" box="[151,440,1869,1894]" pageId="22" pageNumber="223" refId="ref27774" refString="Thatje, S. &amp; Bacardit, R. (2000 b) Morphological variability in larval stages of Nauticaris magellanica (A. Milne Edwards, 1891) (Decapoda: Caridea: Hippolytidae) from South American waters. Bulletin of Marine Science, 66 (2), 375 - 398." type="journal article">Thatje &amp; Bacardit 2000b</bibRefCitation>
