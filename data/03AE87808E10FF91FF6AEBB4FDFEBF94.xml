<taxonomicName id="4C074D158E10FF91FDACEA18FB88BF78" authority="A. Milne-Edwards, 1891" authorityName="A. Milne-Edwards" authorityYear="1891" box="[593,1146,1869,1894]" class="Malacostraca" family="Hippolytidae" genus="Hippolyte" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="223" phylum="Arthropoda" rank="species" species="consobrinus">
