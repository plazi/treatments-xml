<taxonomicName id="B4971FD7FFABFFFCFEC76FFAFD33FE10" authority="Schouteden, 1932" authorityName="Schouteden" authorityYear="1932" box="[381,723,440,467]" class="Insecta" family="Reduviidae" genus="Peprius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="241" phylum="Arthropoda" rank="species" species="bequaerti">
