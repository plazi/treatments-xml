<taxonomicName id="B4971FD7FFABFFFCFC656FFAFB87FE11" baseAuthorityName="Signoret" baseAuthorityYear="1858" box="[991,1127,440,466]" class="Insecta" family="Reduviidae" genus="Peprius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="241" phylum="Arthropoda" rank="species" species="nodulipes">
