<paragraph id="49390905FFAFFFF1FF2EF9F0FE56F962" blockId="8.[160,1155,1639,1698]" pageId="8" pageNumber="1919">
As 
<taxonomicName id="8E867286FFAFFFF1FF48F9F0FD4BF9BF" authority="KrOyer 1845, p. 304" authorityName="KrOyer" authorityPageNumber="304" authorityYear="1845" box="[198,649,1639,1663]" class="Malacostraca" family="Talitridae" genus="Orchestia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="1919" phylum="Arthropoda" rank="species" species="platensis">
