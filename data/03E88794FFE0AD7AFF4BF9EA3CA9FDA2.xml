<collectingCountry id="F3567612FFE3AD7AFEDAFED53FC1FE16" box="[262,389,367,393]" name="Costa Rica" pageId="2" pageNumber="393">Costa Rica</collectingCountry>
, 
<collectingCountry id="F3567612FFE3AD7AFE4CFED53FA9FE16" box="[400,493,367,393]" name="Panama" pageId="2" pageNumber="393">Panama</collectingCountry>
