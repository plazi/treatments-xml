[eastern, 
<collectingRegion id="4985F860FFE3AD7AFD08FEF13D6BFEFA" box="[724,815,331,357]" country="United States of America" name="Virginia" pageId="2" pageNumber="393">Virginia</collectingRegion>
south to 
<collectingRegion id="4985F860FFE3AD7AFC7DFEF03DBDFEFA" box="[929,1017,330,357]" country="United States of America" name="Florida" pageId="2" pageNumber="393">Florida</collectingRegion>
]), West Indies (
<collectingCountry id="F3567612FFE3AD7AFB73FEF13B70FEF9" box="[1199,1332,331,358]" name="Saint Vincent and the Grenadines" pageId="2" pageNumber="393">St. Vincent</collectingCountry>
