<collectingCountry id="F3567612FFE3AD7AFD6AFED53D6DFE16" box="[694,809,367,393]" name="Colombia" pageId="2" pageNumber="393">Colombia</collectingCountry>
, 
<collectingCountry id="F3567612FFE3AD7AFCE8FED53DD7FE16" box="[820,915,367,393]" name="Ecuador" pageId="2" pageNumber="393">Ecuador</collectingCountry>
, 
<collectingCountry id="F3567612FFE3AD7AFC42FED53D95FE16" box="[926,977,367,393]" name="Peru" pageId="2" pageNumber="393">Peru</collectingCountry>
