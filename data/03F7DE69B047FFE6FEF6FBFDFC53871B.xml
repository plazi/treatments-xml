<taxonomicName id="4C5E14FCB047FFE6FEF6FBFDFD448405" authority="Piersig, 1895" authorityName="Piersig" authorityYear="1895" box="[264,667,1110,1136]" class="Arachnida" family="Hydrachnidae" genus="Hydrachna" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="47" phylum="Arthropoda" rank="species" species="inermis">
<emphasis id="B92AB36DB047FFE6FEF6FBFDFD448405" bold="true" box="[264,667,1110,1136]" pageId="12" pageNumber="47">
<emphasis id="B92AB36DB047FFE6FEF6FBFDFE2C8405" bold="true" box="[264,499,1110,1136]" italics="true" pageId="12" pageNumber="47">Hydrachna inermis</emphasis>
<bibRefCitation id="EFCF128EB047FFE6FE05FBFDFD448405" author="Piersig" box="[507,667,1110,1136]" pageId="12" pageNumber="63" refString="Piersig, R. (1895) Beitrage zur Systematik und Entwicklungsgeschichte der Wassermilben. Zoologischer Anzeiger, 18 (466), 19 - 25." type="journal article" year="1895">
Piersig, 
<number id="3D967D58B047FFE6FD9EFBFDFD448405" box="[608,667,1110,1136]" pageId="12" pageNumber="47" value="1895.0">1895</number>
</bibRefCitation>
</emphasis>
