<quantity id="4CEF9B25FFEAFFCE9C69FB55FE1634C4" box="[1044,1111,1191,1217]" metricMagnitude="-1" metricUnit="m" metricValue="1.1" pageId="123" pageNumber="124" unit="cm" value="11.0">11 cm</quantity>
