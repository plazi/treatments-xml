<taxonomicName id="0695C1037BA5F6EFB904DF7BB7CCFA7F" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. paracolhounii" order="Lepidoptera" pageId="7" pageNumber="32" phylum="Arthropoda" rank="species" species="paracolhounii">Ca. paracolhounii</taxonomicName>
and 
<taxonomicName id="CC674EE9CCE830BA7993F20BD7D74E5B" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. pseudocolhounii" order="Lepidoptera" pageId="7" pageNumber="32" phylum="Arthropoda" rank="species" species="pseudocolhounii">Ca. pseudocolhounii</taxonomicName>
