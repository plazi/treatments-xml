<taxonomicName id="7FCDC906FC946631F1BC6472F2DEA9EF" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. fujianensis" order="Lepidoptera" pageId="7" pageNumber="32" phylum="Arthropoda" rank="species" species="fujianensis">Ca. fujianensis</taxonomicName>
