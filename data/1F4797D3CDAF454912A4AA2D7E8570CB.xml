<taxonomicName id="9A8AA3C8AD10673DBE8A84A3548064B0" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. colhounii" order="Lepidoptera" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="colhounii">Ca. colhounii</taxonomicName>
, 
<taxonomicName id="50676F4A2BEE8714D9FF5F1DFBBDEB49" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. eucalypti" order="Lepidoptera" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="eucalypti">Ca. eucalypti</taxonomicName>
, 
<taxonomicName id="6E6E7447FF2AB9A0F705DC1D279B7AE2" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. fujianensis" order="Lepidoptera" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="fujianensis">Ca. fujianensis</taxonomicName>
, 
<taxonomicName id="973E3855900DAFF95D935D85442893B3" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. nymphaeae" order="Lepidoptera" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="nymphaeae">Ca. nymphaeae</taxonomicName>
, 
<taxonomicName id="62799B6FA13FA49CC2431645A4074825" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. paracolhounii" order="Lepidoptera" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="paracolhounii">Ca. paracolhounii</taxonomicName>
and 
<taxonomicName id="AE1FAECAF6BDEF85AFDF981AD57757A2" class="Insecta" family="Dalceridae" genus="Ca." higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ca. pseudocolhounii" order="Lepidoptera" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="pseudocolhounii">Ca. pseudocolhounii</taxonomicName>
(
