<taxonomicName id="41ED4D0B1F3FFFB78DA57B6D470E516B" authority="Graham, C." authorityName="Graham, C." class="Insecta" family="Tephritidae" genus="Ceratitis" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="species" species="anonae">
<emphasis id="B499EA9A1F3FFFB78DA57B6D43895100" box="[1277,1436,1298,1319]" italics="true" pageId="7" pageNumber="595">Ceratitis anonae</emphasis>
Graham, C.
</taxonomicName>
<emphasis id="B499EA9A1F3FFFB7887F7B494783516C" box="[295,406,1334,1355]" italics="true" pageId="7" pageNumber="595">fascioentris</emphasis>
(Bezzi) of genus 
<taxonomicName id="41ED4D0B1F3FFFB78B287B494595516B" authority="MacLeay, 1829" authorityName="MacLeay" authorityYear="1829" box="[624,896,1330,1357]" class="Insecta" family="Tephritidae" genus="Ceratitis" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="genus">
<emphasis id="B499EA9A1F3FFFB78B287B4944D1516C" box="[624,708,1334,1355]" italics="true" pageId="7" pageNumber="595">Ceratitis</emphasis>
MacLeay, 1829
</taxonomicName>
and 
<taxonomicName id="41ED4D0B1F3FFFB78A987B4A4318516A" authority="Munro" authorityName="Munro" authorityYear="1938" box="[960,1293,1331,1357]" class="Insecta" family="Tephritidae" genus="Trirhithrum" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="species" species="meladiscum">
