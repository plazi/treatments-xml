<taxonomicName id="41ED4D0B1F3FFFB78A7F7B7045F6510F" authorityName="Fischer" authorityYear="1965" box="[807,995,1294,1321]" class="Insecta" family="Agromyzidae" genus="Sternulopius" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="species" species="bisternaulicus">
