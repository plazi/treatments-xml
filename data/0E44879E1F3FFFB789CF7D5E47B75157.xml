<taxonomicName id="41ED4D0B1F3FFFB789CF7B26478B5157" authority="Bezzi, 1918" authorityName="Bezzi" authorityYear="1918" box="[151,414,1366,1392]" class="Insecta" family="Tephritidae" genus="Trirhithrum" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="genus">
