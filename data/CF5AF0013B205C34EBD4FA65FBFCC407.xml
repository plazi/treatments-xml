<geoCoordinate id="22C727D03B205C34EA1AF965FE0AC0B4" box="[328,432,1675,1699]" degrees="7" direction="north" minutes="15.24" orientation="latitude" pageId="58" pageNumber="857" precision="9" value="7.254">7°15.24’N</geoCoordinate>
