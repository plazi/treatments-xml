<taxonomicName id="DFA74D62FFAF960BFEBEFEB8B71D317E" baseAuthorityName="Muller" baseAuthorityYear="1923" box="[331,560,310,334]" class="Amphibia" family="Leptodactylidae" genus="Adenomera" kingdom="Animalia" order="Anura" pageId="12" pageNumber="785" phylum="Chordata" rank="species" species="andreae">
