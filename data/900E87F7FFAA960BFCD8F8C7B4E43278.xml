<quantity id="DF5F9B04FFAF960BFE1EFEF7B71031A0" box="[491,573,377,401]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="12" pageNumber="785" unit="m" value="1200.0">1200 m</quantity>
