<taxonomicName id="DFA74D62FFAF960BFEE8FE42B4C031D4" box="[285,493,460,484]" class="Amphibia" family="Leptodactylidae" genus="Adenomera" kingdom="Animalia" order="Anura" pageId="12" pageNumber="785" phylum="Chordata" rank="species" species="undetermined">
