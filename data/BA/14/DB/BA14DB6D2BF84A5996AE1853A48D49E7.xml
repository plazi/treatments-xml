<subSubSection pageId="15" pageNumber="295" type="reference_group">
<paragraph pageId="15" pageNumber="295" type="mainText">
Add. corr. Faune 
<normalizedToken originalValue="hymén" pageId="15" pageNumber="295">hymen</normalizedToken>
. province 
<normalizedToken originalValue="Québec" pageId="15" pageNumber="295">Quebec</normalizedToken>
