<location LSID="urn:lsid:plazi:treatment:BA7387ADFF80363EFF125A59FE22FDB4:37056060FF86363EFF125C9EFF67FE7C" box="[151,195,439,464]" county="Kilburn" municipality="This" name="The" pageId="13" pageNumber="314">The</location>
remaining name 
<taxonomicName box="[388,544,439,464]" class="Gastropoda" family="Buccinidae" genus="Glypteuthria" kingdom="Animalia" order="Neogastropoda" pageId="13" pageNumber="314" phylum="Mollusca" rank="species" species="solidissima">
<emphasis box="[388,544,439,464]" italics="true" pageId="13" pageNumber="314">G. solidissima</emphasis>
</taxonomicName>
