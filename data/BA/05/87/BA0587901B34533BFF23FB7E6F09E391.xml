<taxonomicName authority="Hirose &amp; Gordon, 2020" authorityName="Hirose &amp; Gordon" authorityYear="2020" box="[283,505,1960,1984]" class="Gymnolaemata" family="Vesiculariidae" genus="Amathia" kingdom="Animalia" order="Ctenostomatida" pageId="14" pageNumber="325" phylum="Bryozoa" rank="species" species="reptopinnata" status="n. sp.">
<emphasis box="[283,505,1960,1984]" italics="true" pageId="14" pageNumber="325">
<taxonomicName box="[283,369,1960,1984]" class="Gymnolaemata" family="Vesiculariidae" genus="Amathia" higherTaxonomySource="CoL" kingdom="Animalia" order="Ctenostomatida" pageId="14" pageNumber="325" phylum="Bryozoa" rank="genus">Amathia</taxonomicName>
