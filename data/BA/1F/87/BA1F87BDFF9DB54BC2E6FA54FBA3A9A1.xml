<paragraph blockId="3.[151,854,1498,1523]" box="[151,854,1498,1523]" pageId="3" pageNumber="197">
<typeStatus box="[151,207,1499,1523]" pageId="3" pageNumber="197">Type</typeStatus>
species: 
<taxonomicName box="[309,605,1498,1523]" class="Insecta" family="Mantoididae" genus="Paramantoida" kingdom="Animalia" order="Mantodea" pageId="3" pageNumber="197" phylum="Arthropoda" rank="species" species="amazonica" status="n. sp.">
<emphasis bold="true" box="[309,605,1498,1523]" italics="true" pageId="3" pageNumber="197">Paramantoida amazonica</emphasis>
