<collectingRegion box="[1072,1240,1354,1380]" country="Russia" name="Saint Petersburg" pageId="18" pageNumber="65">St. Petersburg</collectingRegion>
, 
<collectingCountry box="[1251,1334,1354,1381]" name="Russia" pageId="18" pageNumber="65">Russia</collectingCountry>
).
</paragraph>
