<subSubSection pageId="8" pageNumber="55" type="diagnosis">
<paragraph blockId="8.[189,310,637,663]" box="[189,310,637,663]" pageId="8" pageNumber="55">
<heading bold="true" box="[189,310,637,663]" fontSize="11" level="3" pageId="8" pageNumber="55" reason="3">
