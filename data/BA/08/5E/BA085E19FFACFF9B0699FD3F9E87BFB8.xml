. 
<taxonomicName box="[284,633,1928,1950]" class="Insecta" family="Osmylopsychopidae" genus="Osmylopsychostoechus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="10" pageNumber="125" phylum="Arthropoda" rank="species" species="sogulensis">
