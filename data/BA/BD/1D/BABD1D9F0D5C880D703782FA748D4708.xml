<taxonomicName authority="Leconte" authorityName="Leconte" class="Insecta" family="Corylophidae" genus="Rhypobius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhypobius" order="Coleoptera" pageId="174" pageNumber="175" phylum="Arthropoda" rank="genus">Rhypobius</taxonomicName>
