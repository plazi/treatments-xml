<keyStep pageId="2" pageNumber="99">
<paragraph blockId="2.[151,1436,832,1734]" pageId="2" pageNumber="99">
<keyLead pageId="2" pageNumber="99">
<number box="[151,166,1302,1324]" pageId="2" pageNumber="99" value="6.0">6</number>
