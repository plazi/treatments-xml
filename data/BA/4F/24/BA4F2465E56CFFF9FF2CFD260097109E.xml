<taxonomicName authority="Cass." authorityName="Cass." box="[637,739,980,1001]" class="Magnoliopsida" family="Asteraceae" genus="Acroptilon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="2" pageNumber="99" phylum="Tracheophyta" rank="genus">Acroptilon</taxonomicName>
</emphasis>
, and 
<taxonomicName box="[791,906,980,1001]" class="Magnoliopsida" family="Asteraceae" genus="Onopordum" kingdom="Plantae" order="Asterales" pageId="2" pageNumber="99" phylum="Tracheophyta" rank="genus">
<emphasis box="[791,906,980,1001]" italics="true" pageId="2" pageNumber="99">Onopordum</emphasis>
</taxonomicName>
spp. (
<taxonomicName box="[965,1160,980,1001]" class="Insecta" family="Tephritidae" genus="Urophora" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="99" phylum="Arthropoda" rank="species" species="xanthippe">
<emphasis box="[965,1160,980,1001]" italics="true" pageId="2" pageNumber="99">Urophora xanthippe</emphasis>
</taxonomicName>
