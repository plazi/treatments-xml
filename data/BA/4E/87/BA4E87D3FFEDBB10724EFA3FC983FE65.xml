<taxonomicName authority="in Martens (1978)" authorityName="in Martens" authorityYear="1978" box="[461,964,408,435]" class="Arachnida" family="Phalangiidae" genus="Leiobunum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="nigripalpe">
