<taxonomicName authorityName="Herbst" authorityYear="1799" box="[756,886,374,399]" class="Arachnida" family="Phalangiidae" genus="Leiobunum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="rupestre">
