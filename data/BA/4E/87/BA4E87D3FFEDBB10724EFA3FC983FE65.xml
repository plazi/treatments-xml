<taxonomicName authority="Roewer" authorityName="Roewer" authorityYear="1910" box="[189,371,373,399]" class="Arachnida" family="Gagrellidae" genus="Nelima" kingdom="Animalia" order="Opiliones" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
