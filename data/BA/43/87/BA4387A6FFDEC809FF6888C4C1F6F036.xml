<taxonomicName authority="Steiner, 1918" authorityName="Steiner" authorityYear="1918" class="Adenophorea" family="Chromadoridae" genus="Odontocricus" kingdom="Animalia" order="Desmodorida" pageId="1" pageNumber="62" phylum="Nematoda" rank="genus">
