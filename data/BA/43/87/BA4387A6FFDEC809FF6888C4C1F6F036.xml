<taxonomicName authority="Cobb, 1933" authorityName="Cobb" authorityYear="1933" box="[243,492,1630,1654]" class="Adenophorea" family="Chromadoridae" genus="Dasylaimus" kingdom="Animalia" order="Desmodorida" pageId="1" pageNumber="62" phylum="Nematoda" rank="genus">
<emphasis box="[243,355,1630,1654]" italics="true" pageId="1" pageNumber="62">Dasylaimus</emphasis>
Cobb, 1933
</taxonomicName>
, 
<taxonomicName authority="Steiner and Hoeppli, 1926" authorityName="Steiner and Hoeppli" authorityYear="1926" class="Adenophorea" family="Chromadoridae" genus="Dicriconema" kingdom="Animalia" order="Desmodorida" pageId="1" pageNumber="62" phylum="Nematoda" rank="genus">
