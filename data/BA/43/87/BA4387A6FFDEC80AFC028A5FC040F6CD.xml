<taxonomicName authorityName="Wieser" authorityYear="1959" box="[653,767,328,352]" class="Adenophorea" family="Chromadoridae" genus="Graphonema" kingdom="Animalia" order="Desmodorida" pageId="2" pageNumber="63" phylum="Nematoda" rank="species" species="biseriale">
