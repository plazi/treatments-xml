<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230744" authority="Emery, 1912" authorityName="Emery" authorityYear="1912" class="Insecta" family="Formicidae" genus="Atopula" genus.completionEvidence="missing" genus.completionRound="1" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Atopula ceylonensis" order="Hymenoptera" pageNumber="104" phylum="Arthropoda" rank="species" species="ceylonensis">ceylonensis Emery</taxonomicName>
