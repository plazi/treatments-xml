<emphasis italics="true" pageId="113" pageNumber="496">
<taxonomicName authorityName="F. SMITH" authorityYear="1856" class="Liliopsida" family="Poaceae" genus="Ammophila" kingdom="Plantae" order="Poales" pageId="113" pageNumber="496" phylum="Tracheophyta" rank="species" species="terminata">Ammophila terminata</taxonomicName>
