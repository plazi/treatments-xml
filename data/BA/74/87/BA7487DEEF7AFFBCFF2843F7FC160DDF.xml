<paragraph blockId="4.[151,1437,1275,1649]" pageId="4" pageNumber="5">
Genus 
<taxonomicName box="[233,381,1275,1300]" class="Entognatha" family="Arrhopalitidae" genus="Arrhopalites" kingdom="Animalia" order="Collembola" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
