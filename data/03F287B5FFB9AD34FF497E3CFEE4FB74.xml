<collectionCode id="ED4AAE66FFB9AD38FF497F1CFF74F8A2" box="[151,196,1830,1854]" country="Australia" name="CSIRO Canberra Rhizobium Collection" pageId="4" pageNumber="35">CC</collectionCode>
