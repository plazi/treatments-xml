<quantity id="4CE307B6356AFFB444F74690342AFE15" box="[424,617,475,501]" metricMagnitude="-5" metricUnit="m" metricValue="1.7" metricValueMax="2.3" metricValueMin="1.0999999999999999" pageId="110" pageNumber="111" unit="mm" value="0.017" valueMax="0.023" valueMin="0.011">
<geoCoordinate id="EE2FCC94356AFFB444F7469037A5FE15" box="[424,486,475,501]" degrees="0.011" direction="north" orientation="latitude" pageId="110" pageNumber="111" precision="55" value="0.011">0.011</geoCoordinate>
<geoCoordinate id="EE2FCC94356AFFB444B946903470FE15" box="[486,563,475,501]" degrees="0.023" direction="west" orientation="longitude" pageId="110" pageNumber="111" precision="55" value="-0.023">–0.023</geoCoordinate>
mm
