<taxonomicName id="4C1BD1D0356AFFB44678469035D4FE14" authorityName="Beier" authorityYear="1966" box="[807,919,475,501]" class="Arachnida" family="Chthoniidae" genus="Chthonius" kingdom="Animalia" order="Pseudoscorpiones" pageId="110" pageNumber="111" phylum="Arthropoda" rank="species" species="agazzii">
