<paragraph id="8B933684FFA0FFDBFE56FB135835C684" blockId="10.[437,796,1220,1277]" box="[437,796,1220,1244]" pageId="10" pageNumber="1716">
<heading id="D0DB81E8FFA0FFDBFE56FB135835C684" box="[437,796,1220,1244]" centered="true" fontSize="10" level="2" pageId="10" pageNumber="1716" reason="3">
<taxonomicName id="4C2C4D07FFA0FFDBFE56FB1359F7C684" authority="Maes, 2002" authorityName="Maes" authorityYear="2002" box="[437,734,1220,1244]" class="Insecta" family="Crambidae" genus="Adelpherupa" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="1716" phylum="Arthropoda" rank="species" species="costipunctalis" status="sp.n.">
