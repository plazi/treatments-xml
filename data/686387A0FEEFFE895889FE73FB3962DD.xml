<taxonomicName id="27CA4D35FEEFFE895B08FCEAFC4962F9" authorityName="Rondani" authorityYear="1861" box="[790,1018,870,896]" class="Magnoliopsida" family="Apiaceae" genus="Bonannia" kingdom="Animalia" order="Apiales" pageId="290" pageNumber="291" phylum="Tracheophyta" rank="species" species="monticola">
