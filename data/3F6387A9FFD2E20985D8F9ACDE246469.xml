</taxonomicName>
, present 
<taxonomicName id="70CA4D3CFFD2E276847BF8DCDE0A6201" authority="Hirohito, 1988" authorityName="Hirohito" authorityYear="1988" box="[308,772,1807,1832]" class="Hydrozoa" family="Hydractiniidae" genus="Schuchertinia" kingdom="Animalia" order="Anthoathecata" pageId="63" pageNumber="64" phylum="Cnidaria" rank="species" species="reticulata">
<emphasis id="85BEEAADFFD2E276847BF8DCDF4A6201" box="[308,580,1807,1832]" italics="true" pageId="63" pageNumber="64">Schuchertinia reticulata</emphasis>
