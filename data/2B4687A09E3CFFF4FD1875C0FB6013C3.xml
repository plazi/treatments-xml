<treatmentCitationGroup id="83FF11989E3CFFF4FD287625FCE414F1" pageId="56" pageNumber="56">
Now 
<taxonomicName id="64EF4D359E3CFFF4FD657624FB50141C" authority="Cassin, 1866" authorityName="Cassin" authorityYear="1866" box="[726,1190,574,594]" class="Aves" family="Icteridae" genus="Molothrus" kingdom="Animalia" order="Passeriformes" pageId="56" pageNumber="56" phylum="Chordata" rank="subSpecies" species="bonariensis" subSpecies="cabanisii">
<emphasis id="919BEAA49E3CFFF4FD657624FBE1141F" box="[726,1047,574,594]" italics="true" pageId="56" pageNumber="56">Molothrus bonariensis cabanisii</emphasis>
<bibRefCitation id="C77E4B479E3CFFF4FBAC7624FB50141C" author="Cassin, J." box="[1055,1190,574,593]" pageId="56" pageNumber="56" pagination="10 - 25" refId="ref113722" refString="Cassin, J. 1866. A study of the Icteridae. Proceedings of the Academy of Natural Sciences of Philadelphia 18: 10 - 25." type="journal article" year="1866">Cassin, 1866</bibRefCitation>
</taxonomicName>
. See 
<treatmentCitation id="224E10A79E3CFFF4FD6D7643FC7C1421" author="Stone, W." box="[734,906,601,620]" page="346" pageId="56" pageNumber="56" year="1891">
<bibRefCitation id="C77E4B479E3CFFF4FD6D7643FC7C1421" author="Stone, W." box="[734,906,601,620]" pageId="56" pageNumber="56" pagination="344 - 347" refId="ref128704" refString="Stone, W. 1891. A revision of the species of Molothrus allied to M. bonariensis (Gm.). Auk 8: 344 - 347." type="journal article" year="1891">Stone, 1891: 346</bibRefCitation>
</treatmentCitation>
