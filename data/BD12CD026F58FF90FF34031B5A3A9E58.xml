<collectingCountry id="4DAC3C846F58FF90FEC4047E5ECF9DA8" box="[359,426,1147,1173]" name="China" pageId="9" pageNumber="410">China</collectingCountry>
(
<collectingRegion id="F77FB2F66F58FF90FE1A047E5D7D9DA8" box="[441,536,1147,1173]" country="China" name="Sichuan" pageId="9" pageNumber="410">Sichuan</collectingRegion>
, 
<collectingRegion id="F77FB2F66F58FF90FD8104795D189DAB" box="[546,637,1148,1174]" country="China" name="Yunnan" pageId="9" pageNumber="410">Yunnan</collectingRegion>
).
