<taxonomicName id="10889CFDFFD5FF94E437F86E83BE5FF6" authorityName="Jeannel" authorityYear="1962" box="[275,420,2023,2045]" class="Insecta" family="Staphylinidae" genus="Achilia" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="139" phylum="Arthropoda" rank="species" species="temporalis">
