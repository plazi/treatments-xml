<caption id="DF126655EF123A76FF4DF9ADFC52F961" ID-DOI="http://doi.org/10.5281/zenodo.4591254" ID-Zenodo-Dep="4591254" httpUri="https://zenodo.org/record/4591254/files/figure.png" pageId="67" pageNumber="68" startId="67.[136,245,1635,1661]" targetBox="[186,1399,209,1594]" targetPageId="67">
