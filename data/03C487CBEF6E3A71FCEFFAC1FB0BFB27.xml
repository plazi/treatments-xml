<taxonomicName id="4C6D4D5EEF123A76FEE3F9ADFD80F9BC" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[294,581,1635,1661]" class="Maxillopoda" family="Notodelphyidae" genus="Paranotodelphys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="bisetata" status="sp. nov.">
<emphasis id="B919EACFEF123A76FEE3F9ADFD80F9BC" box="[294,581,1635,1661]" italics="true" pageId="67" pageNumber="68">Paranotodelphys bisetata</emphasis>
</taxonomicName>
