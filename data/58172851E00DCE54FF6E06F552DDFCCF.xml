, 
<number id="66768B60E00ECE54FA9D06D855DFFE14" box="[1380,1432,439,463]" pageId="2" pageNumber="66" value="1.12">1.12</number>
, 
<number id="66768B60E00ECE54FF6E06B3508CFE2F" box="[151,203,476,500]" pageId="2" pageNumber="66" value="2.65">2.65</number>
, 
<number id="66768B60E00ECE54FF2F06B3514DFE2F" box="[214,266,476,500]" pageId="2" pageNumber="66" value="3.98">3.98</number>
, 
<number id="66768B60E00ECE54FEEC06B3510DFE2F" box="[277,330,476,500]" pageId="2" pageNumber="66" value="1.73">1.73</number>
