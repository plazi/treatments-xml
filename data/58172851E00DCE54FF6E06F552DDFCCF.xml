, 
<number id="66768B60E00ECE54FE2A06D85240FE14" box="[467,519,439,463]" pageId="2" pageNumber="66" value="1.02">1.02</number>
, 
<number id="66768B60E00ECE54FDED06D8520FFE14" box="[532,584,439,463]" pageId="2" pageNumber="66" value="2.35">2.35</number>
, 
<number id="66768B60E00ECE54FDAC06D852CEFE14" box="[597,649,439,463]" pageId="2" pageNumber="66" value="2.65">2.65</number>
, 
<number id="66768B60E00ECE54FD6E06D8528BFE14" box="[663,716,439,463]" pageId="2" pageNumber="66" value="1.84">1.84</number>
