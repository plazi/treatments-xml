<number id="66768B60E00ECE54FF6E06D8508CFE14" box="[151,203,439,463]" pageId="2" pageNumber="66" value="2.75">2.75</number>
, 
<number id="66768B60E00ECE54FF2106D8514BFE14" box="[216,268,439,463]" pageId="2" pageNumber="66" value="1.94">1.94</number>
), II 
<number id="66768B60E00ECE54FEC606D85139FE14" box="[319,382,439,463]" pageId="2" pageNumber="66" value="10.72">10.72</number>
(
