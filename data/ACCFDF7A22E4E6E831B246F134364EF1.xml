<taxonomicName id="E0C3B5A07C1D30918C33EDFF64C36397" authority="Medianero &amp; Nieves-Aldrey, 2013" authorityName="Medianero &amp; Nieves-Aldrey" authorityYear="2013" class="Insecta" family="Cynipidae" genus="Barucynips" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Barucynips panamensis" order="Hymenoptera" pageId="1" pageNumber="26" phylum="Arthropoda" rank="species" species="panamensis">Barucynips panamensis</taxonomicName>
