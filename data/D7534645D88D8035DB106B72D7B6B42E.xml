<taxonomicName id="DEB8659E4E0ADDED906FAC0FF2A9E92C" class="Magnoliopsida" family="Rubiaceae" genus="Paederia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Paederia foetida" order="Gentianales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="foetida">Paederia foetida</taxonomicName>
