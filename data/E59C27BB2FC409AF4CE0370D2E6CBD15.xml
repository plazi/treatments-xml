<subSubSection id="A719DC03C5708279D6E68FBBA7F70D88" pageId="11" pageNumber="12" type="reference_group">
<paragraph id="C8BC70255DB447BE30FBF27E988C2405" pageId="11" pageNumber="12">
<taxonomicName id="1E047E4AF8F9C4599C05005B9F5C9EF8" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Trametes ribicola" order="Polyporales" pageId="11" pageNumber="12" phylum="Basidiomycota" rank="species" species="ribicola">Trametes ribicola</taxonomicName>
