<subSubSection id="C369656D1036FFF0FF42FAE5B798F9AA" pageId="89" pageNumber="90" type="diagnosis">
<paragraph id="8BCC36E61036FFF0FF42FAE5B77CFAC0" blockId="89.[189,310,1336,1362]" box="[189,310,1336,1362]" pageId="89" pageNumber="90">
<heading id="D084818A1036FFF0FF42FAE5B77CFAC0" bold="true" box="[189,310,1336,1362]" fontSize="11" level="3" pageId="89" pageNumber="90" reason="3">
