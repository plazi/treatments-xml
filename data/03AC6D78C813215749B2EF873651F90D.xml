<number id="3DCDCE49C815215748DEECF23762FA09" box="[507,524,1436,1460]" pageId="20" pageNumber="471" value="2.0">2</number>
): TL 
<number id="3DCDCE49C81521574B6EECF53712FA09" box="[587,636,1435,1460]" pageId="20" pageNumber="471" value="1.45">1.45</number>
&amp; 
<quantity id="4CFD718BC81521574B85ECF23669FA0E" box="[672,775,1436,1460]" metricMagnitude="-3" metricUnit="m" metricValue="1.47" pageId="20" pageNumber="471" unit="mm" value="1.47">1.47 mm</quantity>
; PW 
<number id="3DCDCE49C81521574A61ECF5361BFA09" box="[836,885,1435,1460]" pageId="20" pageNumber="471" value="0.58">0.58</number>
&amp; 
<quantity id="4CFD718BC81521574ABFECF5316FFA0E" box="[922,1025,1435,1460]" metricMagnitude="-4" metricUnit="m" metricValue="6.3" pageId="20" pageNumber="471" unit="mm" value="0.63">0.63 mm</quantity>
