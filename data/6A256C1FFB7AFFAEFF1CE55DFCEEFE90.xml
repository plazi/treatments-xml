<quantity id="257470ECFB7AFFA2FE0FE432FDF6F8D5" box="[388,537,1877,1903]" metricMagnitude="3" metricUnit="m" metricValue="1.175" metricValueMax="1.65" metricValueMin="0.7" pageId="22" pageNumber="23" unit="m" value="1175.0" valueMax="1650.0" valueMin="700.0">700-1650 m</quantity>
