<taxonomicName id="7682D3B4FFA1BF25FC1D6BB4DFE4DD2E" box="[997,1174,764,790]" class="Insecta" family="Cerambycidae" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="37" phylum="Arthropoda" rank="family">Cerambycidae</taxonomicName>
); black oak (
<taxonomicName id="7682D3B4FFA1BF25FEEA6A6CD957DD06" authority="Lam." authorityName="Lam." box="[274,549,804,830]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="37" phylum="Tracheophyta" rank="species" species="velutina">
