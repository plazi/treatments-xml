(Say)
</taxonomicName>
; and on hickory (
<emphasis id="83F67425FFA1BF25FB2F6A6CDA60DD5D" italics="true" pageId="6" pageNumber="37">
<taxonomicName id="7682D3B4FFA1BF25FB2F6A6CDE50DD05" box="[1239,1314,804,829]" class="Magnoliopsida" family="Juglandaceae" genus="Carya" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="37" phylum="Tracheophyta" rank="genus">Carya</taxonomicName>
) (
</emphasis>
<taxonomicName id="7682D3B4FFA1BF25FEEA6A04DAC2DD5E" box="[274,432,844,870]" class="Magnoliopsida" family="Juglandaceae" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="37" phylum="Tracheophyta" rank="family">Juglandaceae</taxonomicName>
