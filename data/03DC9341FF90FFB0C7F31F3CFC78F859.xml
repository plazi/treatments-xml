<taxonomicName id="4C7559D4FF91FFB0C5DB1A6DFB9FFDDD" authority="Shiino, 1963" authorityName="Shiino" authorityYear="1963" box="[703,1072,548,573]" class="Malacostraca" family="Apseudidae" genus="Apseudes" kingdom="Animalia" order="Tanaidacea" pageId="7" pageNumber="408" phylum="Arthropoda" rank="species" species="tenuicorporeus" status="comb. nov.">
