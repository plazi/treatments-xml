<taxonomicName id="4C334D143E4AF0529988FE23C7B94A68" authority="Paulsen" authorityName="Paulsen" authorityYear="2018" box="[653,945,1848,1870]" class="Insecta" family="Lucanidae" genus="Trogellus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="species" species="trajectus" status="new species">
