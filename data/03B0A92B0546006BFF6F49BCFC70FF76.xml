<geoCoordinate id="EE2D7EFA0558006AFF6F4A8BFED9F852" box="[151,287,1987,2012]" direction="east" orientation="longitude" pageId="48" pageNumber="49" precision="1" value="37.51515">E37.51515°</geoCoordinate>
