<quantity id="4CE1B5D80558006AFED24A8BFEBDF852" box="[298,379,1987,2012]" metricMagnitude="3" metricUnit="m" metricValue="2.59" pageId="48" pageNumber="49" unit="m" value="2590.0">2590m</quantity>
