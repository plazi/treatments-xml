<geoCoordinate id="EE2D7EFA0558006AFAE74AE8FA5EF836" box="[1311,1432,1952,1976]" direction="south" orientation="latitude" pageId="48" pageNumber="49" precision="1" value="-3.19304">S3.19304°</geoCoordinate>
