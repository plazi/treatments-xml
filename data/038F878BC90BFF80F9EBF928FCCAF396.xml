<emphasis id="B952EA8FC90BFF81F9EBF8ECFEBCF554" bold="true" box="[151,293,1845,1870]" pageId="24" pageNumber="25">Type-species.</emphasis>
<taxonomicName id="4C264D1EC90BFF81F850F8EFFD35F554" authority="Pourtales, 1880" authorityName="Pourtales" authorityYear="1880" box="[300,684,1845,1870]" class="Anthozoa" family="Antipathidae" genus="Antipathes" kingdom="Animalia" order="Antipatharia" pageId="24" pageNumber="25" phylum="Cnidaria" rank="species" species="tanacetum">
