<taxonomicName id="66C34D0A56BE75F65E9DFD91FE910F71" authorityName="Distant" authorityYear="1904" box="[195,292,589,613]" class="Insecta" family="Miridae" genus="Felisacus" kingdom="Animalia" order="Hemiptera" pageId="161" pageNumber="161" phylum="Arthropoda" rank="genus">
