<paragraph id="636A36C45B05FFE9FF57FE9A3026FE03" blockId="8.[151,1436,294,391]" pageId="8" pageNumber="107">
<materialsCitation id="D3BD3C995B05FFE9FF57FE9A3026FE03" ID-GBIF-Occurrence="1585880167" collectingDate="2016-10-30" collectorName="This &amp; Heavily &amp; Puerto de la Cruz &amp; Tenerife" county="In" location="Franco" municipality="Macaronesian area" pageId="8" pageNumber="107" specimenCount="1" stateProvince="Madeira">
