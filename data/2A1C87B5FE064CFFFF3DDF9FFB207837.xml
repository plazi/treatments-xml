<quantity id="654D9B46FE044CFFFD78DE25FD147A68" box="[649,731,342,366]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="402" pageNumber="403" unit="mm" value="1.7">1.7 mm</quantity>
