). 
<materialsCitation id="6ED29043102BFFEFFDE1DEA841CD95F5" ID-GBIF-Occurrence="3014908423" country="Mexico" elevation="1000" location="Guerrero" pageId="8" pageNumber="565" specimenCount="1" stateProvince="Guerrero">
