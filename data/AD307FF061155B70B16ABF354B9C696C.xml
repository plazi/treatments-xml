<geoCoordinate id="AAB2E3C4E1C4B7A507E0E5656634D8F8" degrees="21" direction="south" minutes="39" orientation="latitude" precision="15" seconds="36" value="-21.66">21°39'36&quot;S</geoCoordinate>
