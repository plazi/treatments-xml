<collectionCode id="ED2A20C7FFC0D35D6A07FD74FDDE2BC7" LSID="urn:lsid:biocol.org:col:14382" box="[563,632,655,679]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="5" pageNumber="596">MOD</collectionCode>
