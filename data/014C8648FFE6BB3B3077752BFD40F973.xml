<collectionCode id="EFF4AF9BFFE7BB3B30D27348FDC5FA37" box="[492,574,1392,1415]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="13" pageNumber="369">MNHN</collectionCode>
