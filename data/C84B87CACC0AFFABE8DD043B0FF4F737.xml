<quantity id="871A9B39CC0AFFA9ED6207650A63F26A" box="[1320,1404,1826,1850]" metricMagnitude="-2" metricUnit="m" metricValue="7.0" pageId="28" pageNumber="29" unit="mm" value="70.0">70 mm</quantity>
