<taxonomicName id="87E24D5FCC0AFFA9E96107660E87F269" box="[299,408,1825,1850]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="28" pageNumber="29" phylum="Cnidaria" rank="genus">
