</taxonomicName>
, 
<taxonomicName id="87E24D5FCC0AFFA9EB8F069E0B8EF3A1" box="[965,1169,1753,1778]" class="Hydrozoa" family="Sertulariidae" genus="Symplectoscyphus" kingdom="Animalia" order="Leptothecata" pageId="28" pageNumber="29" phylum="Cnidaria" rank="genus">
