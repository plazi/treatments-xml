<quantity id="871A9B39CC0AFFA9EB3206B90CC9F246" box="[888,982,1790,1814]" metricMagnitude="-1" metricUnit="m" metricValue="1.9" pageId="28" pageNumber="29" unit="mm" value="190.0">190 mm</quantity>
