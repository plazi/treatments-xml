<taxonomicName id="87E24D5FCC0AFFA9ECB4069E0E02F246" class="Hydrozoa" family="Bougainvilliidae" genus="Rhizorhagium" kingdom="Animalia" order="Anthoathecata" pageId="28" pageNumber="29" phylum="Cnidaria" rank="species" species="antarcticum">
