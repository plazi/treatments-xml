<collectingRegion id="BC5233EFFFD9FFBCFE68F8B5FD18FF69" box="[509,745,188,213]" country="Brazil" name="Rio Grande do Norte" pageId="4" pageNumber="410">Rio Grande do Norte</collectingRegion>
