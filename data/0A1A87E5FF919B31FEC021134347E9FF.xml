<paragraph id="820C36F3FF9E9B31FF3E217E432EEAD5" blockId="40.[138,1185,337,489]" pageId="40" pageNumber="695">
<taxonomicName id="45B34D70FF9E9B31FF3E217E4309EA15" authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Ii" baseAuthorityYear="1964" box="[138,374,337,361]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="40" pageNumber="695" phylum="Arthropoda" rank="species" species="tamurai">
