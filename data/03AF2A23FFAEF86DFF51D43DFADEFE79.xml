<taxonomicName id="4C06E0B6FFAFF86DFD07D055FCDAFECD" authorityName="Jack" authorityYear="1962" box="[705,827,333,359]" class="Arachnida" family="Pterygosomatidae" genus="Pterygosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="phillipsi">
