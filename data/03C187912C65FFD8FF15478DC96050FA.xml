FEMALE: (n = 
<number id="3DA024A02C65FFDAFE76461AC801573C" box="[500,515,1290,1316]" pageId="4" pageNumber="61" value="8.0">8</number>
) Body fusiform, 
<number id="3DA024A02C65FFDAFD4D461AC8F9573C" box="[719,763,1290,1316]" pageId="4" pageNumber="61" value="266.0">266</number>
(
<number id="3DA024A02C65FFDAFC89461AC975573C" box="[779,887,1290,1316]" pageId="4" pageNumber="61" value="271.0" valueMax="289.0" valueMin="253.0">253–289</number>
), 
<number id="3DA024A02C65FFDAFC0E461AC9AB573C" box="[908,937,1290,1316]" pageId="4" pageNumber="61" value="67.0">67</number>
(
<number id="3DA024A02C65FFDAFC3B461ACE07573C" box="[953,1029,1290,1316]" pageId="4" pageNumber="61" value="67.5" valueMax="70.0" valueMin="65.0">65–70</number>
) wide, 
<number id="3DA024A02C65FFDAFBDF461ACE78573C" box="[1117,1146,1290,1316]" pageId="4" pageNumber="61" value="70.0">70</number>
(
<number id="3DA024A02C65FFDAFB08461ACED5573C" box="[1162,1239,1290,1316]" pageId="4" pageNumber="61" value="70.0" valueMax="72.0" valueMin="68.0">68–72</number>
) thick; light yellow. Gnathosoma 
<number id="3DA024A02C65FFDAFEEF4622CB885754" box="[365,394,1330,1356]" pageId="4" pageNumber="61" value="56.0">56</number>
(
<number id="3DA024A02C65FFDAFE194622CBE85754" box="[411,490,1330,1356]" pageId="4" pageNumber="61" value="57.0" valueMax="61.0" valueMin="53.0">53–61</number>
), projecting downwards, pedipalp coxal setae (
<emphasis id="B91CEA952C65FFDAFB954623CE315754" box="[1047,1075,1331,1356]" italics="true" pageId="4" pageNumber="61">ep</emphasis>
) 
<number id="3DA024A02C65FFDAFBC64622CE515754" box="[1092,1107,1330,1356]" pageId="4" pageNumber="61" value="3.0">3</number>
(
<number id="3DA024A02C65FFDAFBE74622CE965754" box="[1125,1172,1330,1356]" pageId="4" pageNumber="61" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis id="B91CEA952C65FFDAFE8C464BCB1F576C" box="[270,285,1371,1396]" italics="true" pageId="4" pageNumber="61">d</emphasis>
) 
<number id="3DA024A02C65FFDAFEAC464ACB49576C" box="[302,331,1370,1396]" pageId="4" pageNumber="61" value="10.0">10</number>
(
<number id="3DA024A02C65FFDAFEDE464ACB98576C" box="[348,410,1370,1396]" pageId="4" pageNumber="61" value="10.0" valueMax="11.0" valueMin="9.0">9–11</number>
). Chelicerae 
<number id="3DA024A02C65FFDAFDB6464AC853576C" box="[564,593,1370,1396]" pageId="4" pageNumber="61" value="50.0">50</number>
(
<number id="3DA024A02C65FFDAFDE3464AC8B2576C" box="[609,688,1370,1396]" pageId="4" pageNumber="61" value="50.0" valueMax="52.0" valueMin="48.0">48–52</number>
), oral stylets 
<number id="3DA024A02C65FFDAFCCE464AC96B576C" box="[844,873,1370,1396]" pageId="4" pageNumber="61" value="56.0">56</number>
(
<number id="3DA024A02C65FFDAFCF8464AC9CB576C" box="[890,969,1370,1396]" pageId="4" pageNumber="61" value="57.0" valueMax="61.0" valueMin="53.0">53–61</number>
). 
<emphasis id="B91CEA952C65FFDAFC5F464ACEAB576C" bold="true" box="[989,1193,1370,1396]" pageId="4" pageNumber="61">Prodorsal shield</emphasis>
<number id="3DA024A02C65FFDAFB33464ACECC576C" box="[1201,1230,1370,1396]" pageId="4" pageNumber="61" value="34.0">34</number>
(
<number id="3DA024A02C65FFDAFB5C464ACF2E576C" box="[1246,1324,1370,1396]" pageId="4" pageNumber="61" value="34.5" valueMax="35.0" valueMin="34.0">34–35</number>
