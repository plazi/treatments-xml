), basiventral femoral setae (
<emphasis id="B91CEA952C65FFDAFAD3455BCF6E547C" box="[1361,1388,1611,1636]" italics="true" pageId="4" pageNumber="61">bv</emphasis>
) 
<number id="3DA024A02C65FFDAFAFD455ACF9E547C" box="[1407,1436,1610,1636]" pageId="4" pageNumber="61" value="12.0">12</number>
(
<number id="3DA024A02C65FFDAFF224562CAEF5494" box="[160,237,1650,1676]" pageId="4" pageNumber="61" value="12.5" valueMax="13.0" valueMin="12.0">12–13</number>
