</taxonomicName>
(= 
<taxonomicName id="4C3F4D44FF91FFD4FB7DF979D8E5F920" authority="Zett." authorityName="Zett." class="Insecta" family="Agromyzidae" genus="Phytobia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="173" phylum="Arthropoda" rank="species" species="carbonaria">
