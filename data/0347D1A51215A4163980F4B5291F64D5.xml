<subSubSection id="4F4FC1E13A744939D143F3E57CCCE43B" pageId="9" pageNumber="10" type="nomenclature">
<paragraph id="6B97282B22316DA072E6CBC2BE7B81FE" pageId="9" pageNumber="10">
<taxonomicName id="C82573070254577DBCAD84D287165B44" LSID="http://species-id.net/wiki/Dinothrips" authority="Bagnall" class="Insecta" family="Phlaeothripidae" genus="Dinothrips" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dinothrips" order="Thysanoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">Dinothrips Bagnall</taxonomicName>
