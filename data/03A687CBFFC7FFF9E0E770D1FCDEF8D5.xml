<collectingCountry id="F318764DFFC7FFF9E19E7503FD8EF8D4" box="[494,589,1796,1822]" name="Vietnam" pageId="7" pageNumber="8">Vietnam</collectingCountry>
(
<collectingRegion id="49CBF83FFFC7FFF9E22B7503FCD5F8D5" box="[603,790,1796,1823]" country="Vietnam" name="Vinh Phuc" pageId="7" pageNumber="8">Vinh Phuc Prov.</collectingRegion>
