</taxonomicName>
(
<taxonomicName id="4C5B35DD81B40DA120DFAF08FA01FB01" box="[1278,1423,1142,1167]" class="Magnoliopsida" family="Sapindaceae" kingdom="Plantae" order="Sapindales" pageId="159" pageNumber="160" phylum="Tracheophyta" rank="family">Sapindaceae</taxonomicName>
