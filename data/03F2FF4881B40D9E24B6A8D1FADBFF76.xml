<date id="FFE5689E81B40DA124B6AFE4FF70FB3D" box="[151,254,1178,1203]" pageId="159" pageNumber="160" value="1899-02-09">
<number id="3D935C7981B40DA124B6AFE4FF2BFB3D" box="[151,165,1178,1203]" pageId="159" pageNumber="160" value="9.0">9</number>
