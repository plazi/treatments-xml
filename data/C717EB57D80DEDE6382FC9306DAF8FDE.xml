<taxonomicName id="DEA71B9939DC79523AD23D22ADA6B764" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Dipsacaceae" genus="Scabiosa" kingdom="Plantae" order="Dipsacales" phylum="Tracheophyta" rank="species" species="columbaria">Scabiosa columbaria</taxonomicName>
