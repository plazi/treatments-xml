<taxonomicName id="4C684D0EB03E726CCD0DFF2BFEEEFF18" authority="Benjamin, 2017" authorityName="Benjamin" authorityYear="2017" class="Arachnida" family="Thomisidae" genus="Epidius" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="481" phylum="Arthropoda" rank="species" species="mahavira" status="sp. nov.">
<emphasis id="B91CEA9FB03E726CCD0DFF2BFEEEFF18" italics="true" pageId="13" pageNumber="481">Epidius mahavira</emphasis>
