D, 
<taxonomicName id="4C0A4F9F9F59FFA6FF71FB0C0EB97D8A" authorityName="Barden &amp; Grimaldi" authorityYear="2017" box="[135,246,1237,1252]" class="Hexapoda" family="Formicidae" genus="Linguamyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
