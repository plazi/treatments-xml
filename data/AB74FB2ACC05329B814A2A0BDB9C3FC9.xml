<paragraph id="AAB21B3C20A53D6AA311F266B9F9CC63" pageId="1" pageNumber="48">
<taxonomicName id="D56F45FEE46DB3755E02B7F8DE7DAF3D" class="Insecta" family="Eumenidae" genus="Subancistrocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Subancistrocerus" order="Hymenoptera" pageId="1" pageNumber="48" phylum="Arthropoda" rank="genus">Subancistrocerus</taxonomicName>
