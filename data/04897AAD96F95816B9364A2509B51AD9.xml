<geoCoordinate id="7FBD5F6F213B2CE9EA0975C94F68EEC6" degrees="42" direction="north" minutes="29" orientation="latitude" precision="15" seconds="0" value="42.483334">42°29'0&quot;N</geoCoordinate>
