<emphasis id="B971EAE6FF38FF4EFB8EF9093A68F965" bold="true" pageId="131" pageNumber="131">
<collectionCode id="ED14AE31FF38FF4EFB8EF9093DACF907" box="[1137,1227,1678,1699]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="131" pageNumber="131">AMNH</collectionCode>
