= 
<taxonomicName id="4C556C8E9A755211FF69FD5CFE938A91" baseAuthorityName="Nabozhenko" baseAuthorityYear="2008" box="[172,285,723,747]" class="Insecta" family="Tenebrionidae" genus="Stenohelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="297" phylum="Arthropoda" rank="subGenus" subGenus="Gunarellus">
