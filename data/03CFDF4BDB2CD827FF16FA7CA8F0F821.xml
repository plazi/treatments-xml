<taxonomicName id="4C6615DEDB2CD827FF16F9FBA84BF9FC" authority="Bergroth, 1894: 165" authorityName="Bergroth" authorityPageNumber="165" authorityYear="1894" box="[151,610,1557,1579]" class="Insecta" family="Coreidae" genus="Leptoglossus" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="151" phylum="Arthropoda" rank="species" species="argentinus">
<emphasis id="B912B24FDB2CD827FF16F9FBABA4F9FC" box="[151,397,1557,1579]" italics="true" pageId="8" pageNumber="151">Leptoglossus argentinus</emphasis>
<treatmentCitation id="0AC7484CDB2CD827FE12F9FBA84BF9FC" author="Bergroth" box="[403,610,1557,1579]" page="165" pageId="8" pageNumber="151" year="1894">
<bibRefCitation id="EFF713ACDB2CD827FE12F9FBA806F9FC" author="Bergroth" box="[403,559,1557,1579]" pageId="8" pageNumber="151" refString="Bergroth, E. (1894) Liste de quelques Hemipteres de la Plata. Revue d'Entomologie, 18, 164 - 167." type="journal article" year="1894">
Bergroth, 
<number id="3DAE7C7ADB2CD827FE78F9FBA806F9FC" box="[505,559,1557,1579]" pageId="8" pageNumber="151" value="1894.0">1894</number>
