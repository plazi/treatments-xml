<taxonomicName id="D55F3CC62AED8DB9E30B31F8B0F02714" genus="D." lsidName="D. complanatis" pageId="0" pageNumber="35" rank="species" species="complanatis">
<emphasis id="3E850BBF3309A0FA89C8D9497C4F5C54" italics="true" pageId="0" pageNumber="35">D. complanatis</emphasis>
</taxonomicName>
sp. nov. (white circle), 
<taxonomicName id="1C78FDCD39D7255C307EA3655F892A8C" genus="D." lsidName="D. xalapensis" pageId="0" pageNumber="35" rank="species" species="xalapensis">
<emphasis id="0CDD4A17886FEFBEB5BC20837E88A890" italics="true" pageId="0" pageNumber="35">D. xalapensis</emphasis>
</taxonomicName>
Delgado and 
<normalizedToken id="2EE2A110AC0114BABB52052666E1263A" originalValue="Capistrán">Capistran</normalizedToken>
