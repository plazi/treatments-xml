<taxonomicName id="B2C8C97F3C70B736A3CDD2C8422C9EF8" class="Insecta" family="Chrysomelidae" genus="Eumolpus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eumolpus" order="Coleoptera" pageId="237" pageNumber="238" phylum="Arthropoda" rank="genus">Eumolpus</taxonomicName>
