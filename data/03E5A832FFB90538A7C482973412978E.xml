<subSubSection id="C3564AAFFFBC0538A68887BA342F9719" pageId="20" pageNumber="21" type="distribution">
<paragraph id="8BF31924FFBC0538A68887BA342F9719" blockId="20.[120,1224,225,439]" pageId="20" pageNumber="21">
<materialsCitation id="3B241379FFBC0538A68887BA342A9719" country="Democratic Republic of the Congo" location="Katanga Province" pageId="20" pageNumber="21" specimenCount="1" stateProvince="Katanga">
