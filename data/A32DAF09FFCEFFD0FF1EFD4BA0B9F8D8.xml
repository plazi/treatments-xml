<taxonomicName id="EC84659CFFCEFFD0FB2BF9FCA66EF949" box="[1186,1286,1647,1670]" class="Arachnida" family="Linyphiidae" genus="Nasoona" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="78" phylum="Arthropoda" rank="genus">
<emphasis id="19F0C20DFFCEFFD0FB2BF9FCA66EF949" box="[1186,1286,1647,1670]" italics="true" pageId="11" pageNumber="78">Nasoona</emphasis>
</taxonomicName>
(see 
<bibRefCitation id="4F1563EEFFCEFFD0FAC9F9FDA3B9F964" accessDate="2014-07-07" author="Platnick, N. I." firstAuthor="Platnick" journalOrPublisher="American Museum of Natural History" pageId="11" pageNumber="78" publicationUrl="http://research.amnh.org/entomology/spiders/catalog/index.html" refId="ref15682" refString="Platnick, N. I. (2014) The world spider catalog, version 15.0. American Museum of Natural History. Available from: at http: // research. amnh. org / entomology / spiders / catalog / index. html (accessed 7 July 2014)" title="The world spider catalog, version 15.0" type="journal article" year="2014">
Platnick 
<number id="9D4C0C38FFCEFFD0FF1EF900A3B9F964" box="[151,209,1683,1707]" pageId="11" pageNumber="78" value="2014.0">2014</number>
</bibRefCitation>
) and further five newly transferred from 
<taxonomicName id="EC84659CFFCEFFD0FD28F901A05BF964" box="[673,819,1682,1707]" class="Arachnida" family="Linyphiidae" genus="Gorbothorax" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="78" phylum="Arthropoda" rank="genus">
<emphasis id="19F0C20DFFCEFFD0FD28F901A05BF964" box="[673,819,1682,1707]" italics="true" pageId="11" pageNumber="78">Gorbothorax</emphasis>
