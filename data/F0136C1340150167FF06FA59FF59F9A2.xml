<collectionCode id="1EAB45C040120166FF54F9E1FE96F9D8" box="[222,309,1540,1566]" country="Ecuador" httpUri="http://grbio.org/cool/kwpg-nhbg" name="Museo de Zoologia, Pontificia Universidad Catolica del Ecuador" pageId="60" pageNumber="61" type="Museum">QCAZ</collectionCode>
