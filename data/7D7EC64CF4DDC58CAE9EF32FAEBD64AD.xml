<subSubSection id="302E0EDAB3AA791E99EACE347DD2EB71" type="reference_group">
<paragraph id="89E3447CBF1431EB2C70D16E6742BE86" pageNumber="124">
<taxonomicName id="EE0C3154EE6139F66A4AF00B2C70AFA4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138509" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica cinerea Mayr" order="Hymenoptera" pageNumber="124" phylum="Arthropoda" rank="species" species="cinerea">Formica cinerea Mayr</taxonomicName>
