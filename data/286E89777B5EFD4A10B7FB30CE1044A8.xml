<paragraph id="A07838617B5EFD4A10A9F86CCE1044A8" blockId="16.[136,1452,1418,2093]" lastBlockId="17.[136,1452,158,941]" lastPageId="17" lastPageNumber="18" pageId="16" pageNumber="17">
<emphasis id="92B3E4737B5EFD4B10A9F86CCEB64F0D" bold="true" box="[136,667,2030,2056]" pageId="16" pageNumber="17">­­­ Specimens­­­examined:—Bocas­­­del­­­Toro:­­­</emphasis>
Changuinola, PILA, región Caribe, Punto 24, 1005 m, 
<emphasis id="92B3E4737B5EFD4B1522F86DCC9F4F29" italics="true" pageId="16" pageNumber="17">De Sedas et al. 909</emphasis>
(PMA). 
<emphasis id="92B3E4737B5EFD4B113BF793CDAC4F29" bold="true" box="[282,385,2065,2092]" pageId="16" pageNumber="17">
<collectingRegion id="6203F6837B5EFD4B113BF793CDAC4F29" box="[282,385,2065,2092]" country="Panama" name="Chiriqui" pageId="16" pageNumber="17">Chiriquí</collectingRegion>
