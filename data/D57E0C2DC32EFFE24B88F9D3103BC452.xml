<taxonomicName id="9AD7C6B8C32EFFE34801F81712E8C1E0" box="[798,928,1940,1964]" class="Liliopsida" family="Arecaceae" genus="Parastethynium" kingdom="Plantae" order="Arecales" pageId="2" pageNumber="51" phylum="Tracheophyta" rank="species" species="maxwelli">
