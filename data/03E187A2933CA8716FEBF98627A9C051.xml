<taxonomicName id="4C484D37933CA8706ECDF88321B8C5F9" authorityName="Alexander" authorityYear="1929" box="[433,689,1915,1941]" class="Insecta" family="Bibionidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="aldrichiana" subGenus="Vestiplex">
