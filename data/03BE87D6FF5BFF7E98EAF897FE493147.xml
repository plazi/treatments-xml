<collectingCountry id="F3007650FF5BFF7F99EFF85CFB9437CD" box="[402,469,1966,1992]" name="Japan" pageId="202" pageNumber="203">Japan</collectingCountry>
, 
<collectingCountry id="F3007650FF5BFF7F999CF85FF86637C2" box="[481,551,1965,1991]" name="China" pageId="202" pageNumber="203">China</collectingCountry>
