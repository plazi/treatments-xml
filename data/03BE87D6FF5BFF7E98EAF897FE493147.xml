<quantity id="4CEF9B25FF5AFF7E9AC3FEF1F8BB3118" box="[702,762,259,285]" metricMagnitude="1" metricUnit="m" metricValue="1.4" pageId="203" pageNumber="204" unit="m" value="14.0">14 m</quantity>
(
<quantity id="4CEF9B25FF5AFF7E9B76FEF0F904311B" box="[779,837,258,286]" metricMagnitude="1" metricUnit="m" metricValue="1.3716" pageId="203" pageNumber="204" unit="ft" value="45.0">45 ft</quantity>
