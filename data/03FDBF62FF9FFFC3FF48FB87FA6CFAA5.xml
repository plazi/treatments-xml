<taxonomicName id="4C5475F7FF9FFFC3FAEAFB69FA6CFA8A" box="[1333,1436,1268,1289]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="29" phylum="Arthropoda" rank="species" species="galbina">
