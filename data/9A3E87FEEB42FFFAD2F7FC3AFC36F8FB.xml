<quantity id="D56F9B0DEB42FFFCD066FB29FD9AFB07" box="[518,602,1183,1208]" metricMagnitude="1" metricUnit="m" metricValue="2.95" pageId="35" pageNumber="536" unit="m" value="29.5">29.5 m</quantity>
