<emphasis id="20E3EAFAEB44FFFAD34AFA2EFE23FA1E" box="[298,483,1432,1454]" italics="true" pageId="37" pageNumber="538">
<taxonomicName id="D5974D6BEB44FFFAD34AFA2EFEB2FA1E" box="[298,370,1432,1454]" class="Polychaeta" family="Cirratulidae" genus="Tharyx" higherTaxonomySource="CoL" kingdom="Animalia" order="Terebellida" pageId="37" pageNumber="538" phylum="Annelida" rank="genus">Tharyx</taxonomicName>
<taxonomicName id="D5974D6BEB44FFFAD31AFA2FFE23FA1E" box="[378,483,1433,1454]" pageId="37" pageNumber="538" rank="species" species="circacutus" status="n. sp.">circacutus</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="3BD05781EB44FFFAD38AFA2EFDE5FA1E" box="[490,549,1432,1454]" pageId="37" pageNumber="538" rank="species">
