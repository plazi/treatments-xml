<collectingCountry id="F360760AFFE4FFCCFE387345D1CBFCE1" box="[404,475,789,815]" name="Brazil" pageId="28" pageNumber="29">Brazil</collectingCountry>
, 
<collectingCountry id="F360760AFFE4FFCCFE4B7345D248FCE1" box="[487,600,789,815]" name="Colombia" pageId="28" pageNumber="29">Colombia</collectingCountry>
