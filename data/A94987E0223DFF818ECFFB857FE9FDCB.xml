<treatmentCitationGroup id="01F011D8223DFF808ECFFB197F01FB05" box="[151,594,1193,1216]" pageId="10" pageNumber="53">
<taxonomicName id="E6E04D75223DFF808ECFFB197F1DFB05" authority="Loew, 1847: 505" authorityName="Loew" authorityPageNumber="505" authorityYear="1847" box="[151,590,1193,1216]" class="Insecta" family="Asilidae" genus="Stichopogon" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="53" phylum="Arthropoda" rank="species" species="inaequalis">
<emphasis id="1394EAE4223DFF808ECFFB197CC2FB05" box="[151,401,1193,1216]" italics="true" pageId="10" pageNumber="53">Stichopogon inaequalis</emphasis>
<treatmentCitation id="A04110E7223DFF808FCFFB197F1DFB05" author="Loew" box="[407,590,1193,1216]" page="505" pageId="10" pageNumber="53" year="1847">
<bibRefCitation id="45714B07223DFF808FCFFB197F4BFB05" author="Loew" box="[407,536,1193,1216]" pageId="10" pageNumber="58" refString="Loew, H. (1847) Uber die europaischen Raubfliegen (Diptera, Asilica). Linnaea Entomologica, 2, 384 - 568, 585 - 591." type="journal article" year="1847">
Loew, 
<number id="972824D1223DFF808F87FB197F4BFB05" box="[479,536,1193,1216]" pageId="10" pageNumber="53" value="1847.0">1847</number>
</bibRefCitation>
