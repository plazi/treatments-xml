<taxonomicName id="4C2E09CCFFBB2737FEED8E65FC17BE47" box="[338,562,1544,1567]" class="Magnoliopsida" family="Lamiaceae" genus="Teucrium" kingdom="Plantae" order="Lamiales" pageId="4" pageNumber="467" phylum="Tracheophyta" rank="species" species="capitatum">
