<taxonomicName id="4C2E09CCFFBB2737FDE08E6AFCFABE78" box="[607,735,1543,1568]" class="Magnoliopsida" family="Lamiaceae" kingdom="Plantae" order="Lamiales" pageId="4" pageNumber="467" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
