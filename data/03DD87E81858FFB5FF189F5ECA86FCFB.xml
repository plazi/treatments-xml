<taxonomicName id="4C744D7D1859FFB5FB369BDCCA04FC82" authority="Pollock, 2018" authorityName="Pollock" authorityYear="2018" box="[1209,1305,767,789]" class="Insecta" family="Salpingidae" genus="Elacatis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="304" phylum="Arthropoda" rank="species" species="larsoni" status="new species">
