<materialsCitation id="3B043CEEFFFCF94DFF6BFB5C646EFAAD" ID-GBIF-Occurrence="1324948223" collectorName="Di Geronimo &amp; La Perna" country="Italy" location="Sicily" pageId="56" pageNumber="57" specimenCount="1" stateProvince="Sicily">
