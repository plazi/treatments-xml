of 
<emphasis id="1E8D8F69FFC7B216CF32FE9FBF51FEF2" box="[1292,1352,296,319]" italics="true" pageId="13" pageNumber="214">nigra</emphasis>
, male, 
<emphasis id="1E8D8F69FFC7B216CAA9FEFBBB1BFEA8" bold="true" box="[151,258,332,357]" pageId="13" pageNumber="214">
<collectingCountry id="54EE13EBFFC7B216CAA9FEFBBB1BFEA8" box="[151,258,332,357]" name="Brazil" pageId="13" pageNumber="214">BRAZIL</collectingCountry>
