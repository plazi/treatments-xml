<collectingRegion id="B9482EADD32FC16CDBB3FE80304FFEA8" box="[461,584,332,357]" country="Iran" name="Azarbayjan-e Sharqi" pageId="10" pageNumber="539">
<collectingCountry id="039BA0DFD32FC16CDBB3FE80304FFEA8" box="[461,584,332,357]" name="Azerbaijan" pageId="10" pageNumber="539">Azerbaijan</collectingCountry>
</collectingRegion>
