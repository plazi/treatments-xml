<quantity id="4CAC9B05FFCEFFDFD45B4EE8C0A254BE" box="[1103,1184,724,750]" metricMagnitude="2" metricUnit="m" metricValue="3.65" pageId="28" pageNumber="29" unit="m" value="365.0">365 m</quantity>
], SA; 
<collectingRegion id="4990F802FFCEFFDFD4FA4EE8C12854BE" box="[1262,1322,724,750]" country="South Africa" name="Western Cape" pageId="28" pageNumber="29">Cape</collectingRegion>
Jaffa, 100, 130, 300 fms. [= 
<quantity id="4CAC9B05FFCEFFDFD24A4EC0C7245546" box="[606,806,764,790]" metricMagnitude="8" metricUnit="m" metricValue="1.82237548" pageId="28" pageNumber="29" unit="m" value="1.82237548E8">182, 237, 548 m</quantity>
], SA; 
<collectingRegion id="4990F802FFCEFFDFD3664EC0C7AC5546" box="[882,942,764,790]" country="South Africa" name="Western Cape" pageId="28" pageNumber="29">Cape</collectingRegion>
Wiles, 100 fms. [= 
<quantity id="4CAC9B05FFCEFFDFD4B44EC0C0ED5546" box="[1184,1263,764,790]" metricMagnitude="2" metricUnit="m" metricValue="1.8199999999999998" pageId="28" pageNumber="29" unit="m" value="182.0">182 m</quantity>
], SA (
<bibRefCitation id="EFC54B11FFCEFFDFD1044F18C5AD556E" author="Cotton, B. C." box="[272,431,804,830]" pageId="28" pageNumber="29" pagination="150 - 171" refId="ref12420" refString="Cotton, B. C. (1945) Southern Australian Gastropoda Part I. Streptoneura. Transactions of the Royal Society of South Australia, 69, 150 - 171, pls. 12 - 13." type="journal article" year="1945">Cotton, 1945</bibRefCitation>
