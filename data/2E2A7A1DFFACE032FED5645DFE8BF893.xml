<taxonomicName id="6183B088FFADE032FD9667B8FC84F974" box="[587,845,1690,1715]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="rubetae">
