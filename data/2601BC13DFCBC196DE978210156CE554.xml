<taxonomicName id="7AFE4B17CF477455DE84D360AD3CAFA1" class="Insecta" family="Cerambycidae" genus="Mythodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mythodes" order="Coleoptera" pageId="207" pageNumber="208" phylum="Arthropoda" rank="genus">Mythodes</taxonomicName>
