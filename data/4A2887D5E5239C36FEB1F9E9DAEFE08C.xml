<td id="3F605DFDE52563D7FD42F9CBDA72E255" box="[763,792,1637,1663]" gridcol="1" gridrow="1" pageId="29" pageNumber="30">all</td>
<td id="3F605DFDE52563D7FC9BF9CBDA34E255" box="[802,862,1637,1663]" gridcol="2" gridrow="1" pageId="29" pageNumber="30">other</td>
<td id="3F605DFDE52563D7FCDEF9CBDA84E255" box="[871,1006,1637,1663]" gridcol="3" gridrow="1" pageId="29" pageNumber="30">
<taxonomicName id="05814D40E5259C35FCDEF9C8DA84E255" box="[871,1006,1638,1663]" class="Arachnida" family="Theridiidae" genus="Anelosimus" kingdom="Animalia" order="Araneae" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis id="F0F5EAD1E5259C35FCDEF9C8DA84E255" box="[871,1006,1638,1663]" italics="true" pageId="29" pageNumber="30">Anelosimus</emphasis>
</taxonomicName>
</td>
<td id="3F605DFDE52563D7FC41F9CBDD39E255" box="[1016,1107,1637,1663]" gridcol="4" gridrow="1" pageId="29" pageNumber="30">females</td>
<td id="3F605DFDE52563D7FBE4F9CBDD10E255" box="[1117,1146,1637,1663]" gridcol="5" gridrow="1" pageId="29" pageNumber="30">by</td>
<td id="3F605DFDE52563D7FB3DF9CBDDC2E255" box="[1156,1192,1637,1663]" gridcol="6" gridrow="1" pageId="29" pageNumber="30">the</td>
