</taxonomicName>
(
<taxonomicName id="4C304D65F74D805B5191FAEEDD9ECA65" box="[1014,1159,1326,1351]" class="Agaricomycetes" family="Russulaceae" higherTaxonomySource="GBIF" kingdom="Fungi" order="Agaricales" pageId="27" pageNumber="328" phylum="Basidiomycota" rank="family">Russulaceae</taxonomicName>
), 
