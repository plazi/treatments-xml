<taxonomicName id="4C304D65F74D805B5039FC81DAA1CC75" authority="Erichson, 1843" authorityName="Erichson" authorityYear="1843" box="[606,952,833,855]" class="Insecta" family="Nitidulidae" genus="Pallodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="328" phylum="Arthropoda" rank="species" species="silaceus">
