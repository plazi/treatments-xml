<collectingCountry id="F3007632A45C594923BBFDEFFDA6D825" box="[452,544,554,580]" name="China" pageId="9" pageNumber="120">CHINA</collectingCountry>
. 
<collectingRegion id="49D3F840A45C59492056FDEEFD6BD825" box="[553,749,554,581]" country="China" name="Yunnan" pageId="9" pageNumber="120">Yunnan Province</collectingRegion>
: 
<location id="8EC86079A45C59492088FDEFFCB3D825" LSID="urn:lsid:plazi:treatment:03BE87B4A452594922F7F871FED9DE5D:8EC86079A45C59492088FDEFFCB3D825" box="[759,821,554,580]" country="China" name="Pu'er" pageId="9" pageNumber="120" stateProvince="Yunnan">Pu’er</location>
, on dead wood of coffee, 
<date id="FFA91062A45C5949262DFDEFFAAED825" box="[1106,1320,554,580]" pageId="9" pageNumber="120" value="2020-12-23">
