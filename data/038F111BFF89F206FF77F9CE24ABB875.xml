<taxonomicName id="4C26DB8EFF89F206FC26FDB72500B875" box="[985,1040,542,566]" class="Insecta" family="Lampyridae" genus="Auge" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="187" phylum="Arthropoda" rank="genus">Auge</taxonomicName>
