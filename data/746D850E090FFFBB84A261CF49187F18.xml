<treatmentCitationGroup id="DCD41336090FFFBB84A2607449207801" pageId="1" pageNumber="292">
<taxonomicName id="3BC44F9B090FFFBB84A2607449F27820" authority="Parker, 1929:20" authorityName="Parker" authorityPageNumber="20" authorityYear="1929" box="[151,401,833,855]" class="Insecta" family="Crabronidae" genus="Selman" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="292" phylum="Arthropoda" rank="genus">
<emphasis id="CEB0E80A090FFFBB84A2607448817820" box="[151,226,833,855]" italics="true" pageId="1" pageNumber="292">Selman</emphasis>
<treatmentCitation id="7D651209090FFFBB84DE607449F27820" author="Parker" box="[235,401,833,855]" page="20" pageId="1" pageNumber="292" year="1929">
<bibRefCitation id="985549E9090FFFBB84DE607449117820" author="Parker" box="[235,370,833,855]" pageId="1" pageNumber="292" refString="Parker, J. B. (1929) A generic revision of the fossorial wasps of the tribes Stizini and Bembicini, with notes and descriptions of new species. Proceedings of the United States National Museum, 75 (5), 1 - 203. http: // dx. doi. org / 10.5479 / si. 00963801.75 - 2776.1" type="journal article" year="1929">
Parker, 
<number id="4A0C263F090FFFBB850E607449117820" box="[315,370,833,855]" pageId="1" pageNumber="292" value="1929.0">1929</number>
</bibRefCitation>
: 
<number id="4A0C263F090FFFBB8540607449F27820" box="[373,401,833,855]" pageId="1" pageNumber="292" value="20.0">20</number>
</treatmentCitation>
</taxonomicName>
. 
<typeStatus id="237F8ABA090FFFBB85AB607749B37820" box="[414,464,834,855]" pageId="1" pageNumber="292">Type</typeStatus>
species: 
<taxonomicName id="3BC44F9B090FFFBB860660744B0D7820" authority="Parker, 1929" authorityName="Parker" authorityYear="1929" box="[563,878,833,855]" class="Insecta" family="Crabronidae" genus="Selman" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="292" phylum="Arthropoda" rank="species" species="angustus">
<emphasis id="CEB0E80A090FFFBB860660744A827820" box="[563,737,833,855]" italics="true" pageId="1" pageNumber="292">Selman angustus</emphasis>
<bibRefCitation id="985549E9090FFFBB86DF60744B0D7820" author="Parker" box="[746,878,833,855]" pageId="1" pageNumber="292" refString="Parker, J. B. (1929) A generic revision of the fossorial wasps of the tribes Stizini and Bembicini, with notes and descriptions of new species. Proceedings of the United States National Museum, 75 (5), 1 - 203. http: // dx. doi. org / 10.5479 / si. 00963801.75 - 2776.1" type="journal article" year="1929">
Parker, 
<number id="4A0C263F090FFFBB870E60744B0D7820" box="[827,878,833,855]" pageId="1" pageNumber="292" value="1929.0">1929</number>
</bibRefCitation>
</taxonomicName>
(= 
<taxonomicName id="3BC44F9B090FFFBB87A260744D757820" authority="Taschenberg, 1870" authorityName="Taschenberg" authorityYear="1870" box="[919,1302,833,855]" genus="Monedula" kingdom="Animalia" pageId="1" pageNumber="292" rank="species" species="notata">
<emphasis id="CEB0E80A090FFFBB87A260744C2B7820" box="[919,1096,833,855]" italics="true" pageId="1" pageNumber="292">Monedula notata</emphasis>
<bibRefCitation id="985549E9090FFFBB806460744D757820" author="Taschenberg" box="[1105,1302,833,855]" pageId="1" pageNumber="292" refString="Taschenberg, E. L. (1870) Die Larridae and Bembecidae des zoologischen Museums der hiesigen Universitat. Zeitschrift fur die Gesammten Naturwissenschaften, 36, 1 - 27." type="journal article" year="1870">
Taschenberg, 
<number id="4A0C263F090FFFBB80D560744D757820" box="[1248,1302,833,855]" pageId="1" pageNumber="292" value="1870.0">1870</number>
</bibRefCitation>
