<paragraph id="6E869FD534F1CB19E96EECC84CBA04F6" pageId="400" pageNumber="401">
<taxonomicName id="1C7A22F669E14DE7B4C36E60DE08F3CA" authorityName="Casey" authorityYear="1918" class="Hexapoda" family="Carabidae" genus="Bembidion" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="400" pageNumber="401" phylum="Arthropoda" rank="species" species="placabile">Bembidion placabile</taxonomicName>
