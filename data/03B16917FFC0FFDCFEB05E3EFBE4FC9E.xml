<quantity id="4CE075E4FFC5FFDCFEE05D76FE34FE0E" box="[344,437,364,390]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="4" pageNumber="5" unit="mm" value="3.6">3.6 mm</quantity>
