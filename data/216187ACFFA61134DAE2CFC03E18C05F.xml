<taxonomicName id="6EC84D39FFA61134DBB8C8643FA1C03B" authorityName="Stejneger" authorityYear="1904" box="[461,657,1302,1328]" class="Reptilia" family="Dactyloidae" genus="Anolis" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="73" phylum="Chordata" rank="species" species="evermanni">
<emphasis id="9BBCEAA8FFA61134DBB8C8643FA1C03B" box="[461,657,1302,1328]" italics="true" pageId="20" pageNumber="73">Anolis evermanni</emphasis>
</taxonomicName>
and 
<taxonomicName id="6EC84D39FFA61134D8B2C8653E18C03B" box="[711,808,1302,1329]" class="Reptilia" family="Dactyloidae" genus="Anolis" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="73" phylum="Chordata" rank="species" species="krugii">
<emphasis id="9BBCEAA8FFA61134D8B2C8653FE8C03A" box="[711,728,1303,1329]" italics="true" pageId="20" pageNumber="73">A</emphasis>
