, 
<taxonomicName id="4DA14D12FFEB856EB3906767265AFE41" box="[1023,1290,366,390]" class="Insecta" family="Hydrometridae" genus="Hydrometra" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="234" phylum="Arthropoda" rank="species" species="gracilenta">
