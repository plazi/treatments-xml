<paragraph id="EB12E860EF65FF993799FB318640FB26" blockId="3.[151,1436,1161,1258]" pageId="3" pageNumber="399">
<emphasis id="D9D93472EF65FF993799FB3184F5FB6D" bold="true" box="[151,214,1161,1185]" pageId="3" pageNumber="399">Note.</emphasis>
Perhaps 
<taxonomicName id="2CAD93E3EF65FF993645FB3186CFFB6E" authority="Chen" authorityName="Chen" authorityYear="2002" box="[331,748,1161,1186]" class="Insecta" family="Delphacidae" genus="Elachodelphax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="399" phylum="Arthropoda" rank="species" species="xingjiangensis">
