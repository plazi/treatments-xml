<taxonomicName id="4C6F3D86FFAA0A4DFF71F8AFFEDFF8C8" box="[151,350,1855,1880]" class="Insecta" family="Lonchaeidae" genus="Lamprolonchaea" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="47" phylum="Arthropoda" rank="genus">
