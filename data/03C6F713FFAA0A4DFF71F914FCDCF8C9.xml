<taxonomicName id="4C6F3D86FFAA0A4DFE9BF88AFD97F8A3" box="[381,534,1818,1843]" class="Insecta" family="Lonchaeidae" genus="Lonchaea" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="47" phylum="Arthropoda" rank="species" species="brouniana">
