</date>
, 
<collectorName id="26985321FFD2DD2DAE70F83D4932FF31" lastPageId="12" lastPageNumber="49" pageId="11" pageNumber="48">M. Chevreux</collectorName>
&quot; and 
<specimenCode id="DBCB9E8CFFD5DD2DAA47FF604981FF31" box="[333,447,152,178]" collectionCode="DGF" pageId="12" pageNumber="49">DGF 716</specimenCode>
, &quot; 
<location id="8EB2602CFFD5DD2DAAD8FF604A97FF31" LSID="urn:lsid:plazi:treatment:03C487E1FFD2DD2DAB9DF9924D20FF57:8EB2602CFFD5DD2DAAD8FF604A97FF31" box="[466,681,152,178]" country="Senegal" latitude="145.8" name="Fontaine de Hann" pageId="12" pageNumber="49">Fontaine de Hann</location>
, 
<collectingCountry id="F37A7667FFD5DD2DA9BFFF604B2AFF31" box="[693,788,152,178]" name="Senegal" pageId="12" pageNumber="49">Senegal</collectingCountry>
, 
<geoCoordinate id="EE595030FFD5DD2DA82AFF604B43FF31" box="[800,893,152,178]" degrees="145, 8" direction="north" orientation="latitude" pageId="12" pageNumber="49" precision="5555" value="145.8">
N145, 
<date id="FFD31037FFD5DD2DA87AFF604B43FF31" box="[880,893,152,178]" pageId="12" pageNumber="49" value="1890-05-08">8</date>
</geoCoordinate>
.V.1890, 
<collectorName id="26985321FFD5DD2DA8E2FF604CB4FF31" box="[1000,1162,152,178]" pageId="12" pageNumber="49">M. Chevreux</collectorName>
