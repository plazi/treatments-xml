<taxonomicName id="4C3B4D0E731DAB4F47A8FCB5FAF3FCE6" baseAuthorityName="Fye" baseAuthorityYear="1965" box="[1258,1383,868,893]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="relativa">
