<taxonomicName id="4C3B4D0E731DAB4F400DFC59FC5EFC3B" authorityName="Provancher" authorityYear="1888" box="[847,970,904,928]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="inermis">
