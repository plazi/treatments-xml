<taxonomicName id="4C3B4D0E731DAB4F413EFCB5FC2CFCE6" authority="(Hobbs 1968)" authorityName="Hobbs" authorityYear="1968" baseAuthorityName="Hobbs" baseAuthorityYear="1968" box="[636,952,868,893]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="rotundata">
