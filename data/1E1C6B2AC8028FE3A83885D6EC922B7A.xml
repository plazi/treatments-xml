<taxonomicName id="EADB8751BB206F24A74C4B72529E11F1" class="Magnoliopsida" family="Boraginaceae" genus="Cynoglossum" kingdom="Plantae" order="Boraginales" pageId="null" pageNumber="55" phylum="Tracheophyta" rank="genus">Cynoglossum</taxonomicName>
