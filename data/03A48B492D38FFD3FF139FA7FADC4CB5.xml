</taxonomicName>
sp. B, 
<taxonomicName id="4C0D41DC2D38FFD3FD9D99FCFD244CB5" box="[537,754,1721,1748]" class="Insecta" family="Tortricidae" genus="Sycacantha" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="202" phylum="Arthropoda" rank="species" species="sphaerocosmana">
<emphasis id="B979E64D2D38FFD3FD9D99FCFD244CB5" box="[537,754,1721,1748]" italics="true" pageId="1" pageNumber="202">S. sphaerocosmana</emphasis>
</taxonomicName>
, and 
<taxonomicName id="4C0D41DC2D38FFD3FCAB99FCFC014CB5" box="[815,983,1721,1748]" class="Insecta" family="Tortricidae" genus="Sycacantha" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="202" phylum="Arthropoda" rank="species" species="castanicolor">
<emphasis id="B979E64D2D38FFD3FCAB99FCFC014CB5" box="[815,983,1721,1748]" italics="true" pageId="1" pageNumber="202">S. castanicolor</emphasis>
