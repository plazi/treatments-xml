<taxonomicName id="4C7DF5CC3677FFEEFEF8F80F37183CD1" authorityName="in Mohrig &amp; Jaschhof" authorityYear="1999" box="[349,604,1930,1954]" class="Insecta" family="Sciaridae" genus="Pseudolycoriella" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="bispina">
