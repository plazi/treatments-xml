<taxonomicName id="07FE4D2AFFBBC324FF0BFB05FD4EFB41" authority="Annandale, 1905" authorityName="Annandale" authorityYear="1905" box="[151,713,1230,1256]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="43" phylum="Chordata" rank="species" species="subtriedroides">
<emphasis id="F28AEABBFFBBC324FF0BFB05FD4EFB41" bold="true" box="[151,713,1230,1256]" pageId="6" pageNumber="43">
<emphasis id="F28AEABBFFBBC324FF0BFB05FE68FB41" bold="true" box="[151,495,1230,1256]" italics="true" pageId="6" pageNumber="43">Hemidactylus subtriedroides</emphasis>
Annandale, 
<number id="7636248EFFBBC324FD12FB05FD4EFB41" box="[654,713,1230,1256]" pageId="6" pageNumber="43" value="1905.0">1905</number>
</emphasis>
</taxonomicName>
