<taxonomicName id="25C836DBFFAF014814C3F9B3644CDF7D" authorityName="J. A. Allen" authorityYear="1876" box="[2216,2365,1592,1621]" class="Mammalia" family="Procyonidae" genus="Bassaricyon" kingdom="Animalia" order="Carnivora" pageId="2" pageNumber="525" phylum="Chordata" rank="genus" status="n. sp.">Bassaricyon</taxonomicName>
