<taxonomicName id="4C3D628D660D8210FBB7D25AFA319ABE" box="[1145,1435,380,405]" class="Rhynchonellata" family="Frenulinidae" genus="Frenulina" kingdom="Animalia" order="Terebratulida" pageId="15" pageNumber="16" phylum="Brachiopoda" rank="species" species="sanguinolenta">
