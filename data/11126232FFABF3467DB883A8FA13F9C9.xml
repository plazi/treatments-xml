<taxonomicName id="5EBBA8A7FFABF346786D8289FA13FC09" authorityName="DI Cartwright" authorityYear="2009" box="[1346,1436,913,936]" class="Insecta" family="Ecnomidae" genus="Austrotinodes" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="doota">
