<taxonomicName id="5EBBA8A7FFABF346786C82A9FA14FC69" authorityName="DI Cartwright" authorityYear="2009" box="[1347,1435,945,968]" class="Insecta" family="Ecnomidae" genus="Austrotinodes" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="varus">
