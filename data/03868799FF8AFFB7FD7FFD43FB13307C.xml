<taxonomicName id="4C2F4D0CFF8AFFB7FBA0FB20FB563026" box="[1052,1139,1267,1288]" class="Magnoliopsida" family="Asteraceae" genus="Aster" kingdom="Plantae" order="Asterales" pageId="3" pageNumber="353" phylum="Tracheophyta" rank="species" species="undetermined">
