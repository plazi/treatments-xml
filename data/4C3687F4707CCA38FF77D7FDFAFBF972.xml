<taxonomicName id="039F4D61707FCA38FE05D27DFDA0FCA3" box="[485,599,833,855]" class="Insecta" family="Coreidae" genus="Anisoscelis" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="205" phylum="Arthropoda" rank="genus">
<emphasis id="F6EBEAF0707FCA38FE05D27DFDA0FCA3" box="[485,599,833,855]" italics="true" pageId="10" pageNumber="205">Anisoscelis</emphasis>
</taxonomicName>
(
<taxonomicName id="039F4D61707FCA38FD86D27EFD60FCA3" authority="Osuna, 1977" authorityName="Osuna" authorityYear="1977" box="[614,663,834,855]" class="Insecta" family="Coreidae" genus="Bitta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="205" phylum="Arthropoda" rank="genus">
<emphasis id="F6EBEAF0707FCA38FD86D27EFD60FCA3" box="[614,663,834,855]" italics="true" pageId="10" pageNumber="205">Bitta</emphasis>
</taxonomicName>
) spp., in two views. 13–14, 
<taxonomicName id="039F4D61707FCA38FC57D27EFAF0FCA3" authority="Westwood." authorityName="Westwood." box="[951,1287,833,855]" class="Insecta" family="Coreidae" genus="Anisoscelis" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="205" phylum="Arthropoda" rank="species" species="hymenipherus" subGenus="Bitta">
<emphasis id="F6EBEAF0707FCA38FC57D27EFC31FCA3" box="[951,966,834,855]" italics="true" pageId="10" pageNumber="205">A</emphasis>
. (
<emphasis id="F6EBEAF0707FCA38FC3CD27EFC1CFCA3" box="[988,1003,834,855]" italics="true" pageId="10" pageNumber="205">B</emphasis>
.) 
<emphasis id="F6EBEAF0707FCA38FBE1D27DFB78FCA3" box="[1025,1167,833,855]" italics="true" pageId="10" pageNumber="205">hymenipherus</emphasis>
