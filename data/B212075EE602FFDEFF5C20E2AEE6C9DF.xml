<taxonomicName id="FDBBCDCBE602FFDDFF5C20E2AFEFCC74" authority="Thomson, 1868" authorityName="Thomson" authorityYear="1868" box="[151,489,1769,1795]" class="Insecta" family="Cerambycidae" genus="Drycothaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="320" phylum="Arthropoda" rank="genus">
