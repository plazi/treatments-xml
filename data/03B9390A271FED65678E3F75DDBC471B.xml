<taxonomicName id="4C10F39F271DED65650D3A93DB7E40C3" authorityName="Gibon" authorityYear="2017" box="[532,623,944,968]" class="Insecta" family="Psychomyiidae" genus="Tinodes" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="375" phylum="Arthropoda" rank="species" species="furcatus">
