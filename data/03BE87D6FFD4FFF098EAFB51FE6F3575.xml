<quantity id="4CEF9B25FFD4FFF09CC3FB19FF5A3503" box="[1214,1307,1259,1286]" metricMagnitude="3" metricUnit="m" metricValue="4.939" pageId="69" pageNumber="70" unit="m" value="4939.0">4,939 m</quantity>
