<quantity id="BC54E1DFC213E821FC9418A0BCC3FE0A" box="[835,910,485,509]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" pageId="29" pageNumber="554" unit="mm" value="30.0">30 mm</quantity>
