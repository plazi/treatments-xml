<quantity id="BC54E1DFC213E821FC23188CBB0DFE17" box="[1012,1088,457,481]" metricMagnitude="-2" metricUnit="m" metricValue="2.1" pageId="29" pageNumber="554" unit="mm" value="21.0">21 mm</quantity>
