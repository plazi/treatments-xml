<quantity id="BC54E1DFC213E821FD6C1B47BC4BFDED" box="[699,774,514,538]" metricMagnitude="-2" metricUnit="m" metricValue="2.6" pageId="29" pageNumber="554" unit="mm" value="26.0">26 mm</quantity>
................................................. 3
