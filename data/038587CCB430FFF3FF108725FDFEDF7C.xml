<collectingCountry id="F33B764AB430FFF3FEE086CAFE3CD8C8" box="[359,426,937,963]" name="China" pageId="4" pageNumber="214">China</collectingCountry>
(
<collectingRegion id="49E8F838B430FFF3FE3E86CAFD83D8C8" box="[441,533,937,963]" country="China" name="Yunnan" pageId="4" pageNumber="214">Yunnan</collectingRegion>
