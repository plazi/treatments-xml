<taxonomicName id="FBE87275523A776E2261EFB4054FF47C" LSID-ZBK="urn:lsid:zoobank.org:act:4781B086-6A2E-487B-8D97-1D1A856A235A" class="Actinopterygii" family="Ariidae" genus="Galeichthys" kingdom="Animalia" lsidName-ZBK="Galeichthys Valenciennes 1840:28" order="Siluriformes" pageNumber="52" phylum="Chordata" rank="genus">Galeichthys</taxonomicName>
