<taxonomicName id="8F4EA99A2DA91BF408DDCFF941F4BFA6" class="Actinopterygii" family="Ariidae" genus="Plicofollis" kingdom="Animalia" order="Siluriformes" pageNumber="52" phylum="Chordata" rank="species" species="platystomus">Plicofollis platystomus</taxonomicName>
, 
<taxonomicName id="038BAA4919F79E678E6ECD48CB245E69" LSID-ZBK="urn:lsid:zoobank.org:act:AAD7809B-5174-4F77-B9AC-2359BB91181F" class="Actinopterygii" family="Ariidae" genus="Potamarius" kingdom="Animalia" lsidName-ZBK="Potamarius Hubbs &amp; Miller 1960:101" order="Siluriformes" pageNumber="52" phylum="Chordata" rank="genus">Potamarius</taxonomicName>
