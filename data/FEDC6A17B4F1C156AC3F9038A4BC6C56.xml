), 
<taxonomicName id="369B6B4F83550ADE48271262458663F9" LSID-ZBK="urn:lsid:zoobank.org:act:3F1F6AF2-6F38-42C0-9C82-C1468F19FAF0" class="Actinopterygii" family="Ariidae" genus="Hemiarius" kingdom="Animalia" lsidName-ZBK="Hemiarius Bleeker 1862:7, 29" order="Siluriformes" pageNumber="52" phylum="Chordata" rank="genus">Hemiarius</taxonomicName>
, 
<taxonomicName id="AC1D8AB9AD1E1ADF64C606232F484BA2" LSID-ZBK="urn:lsid:zoobank.org:act:086E7B0F-D030-44BD-9DCC-8CAEB178CBAA" class="Actinopterygii" family="Ariidae" genus="Notarius" kingdom="Animalia" lsidName-ZBK="Notarius Gill 1863:171" order="Siluriformes" pageNumber="52" phylum="Chordata" rank="genus">Notarius</taxonomicName>
