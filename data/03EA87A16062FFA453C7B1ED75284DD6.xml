<caption id="DF3C663F6060FFA253C7B26771B6486D" httpUri="https://zenodo.org/record/243786/files/figure.png" pageId="3" pageNumber="275" targetBox="[160,1419,208,1797]" targetPageId="3">
<paragraph id="8BFC36B76060FFA253C7B26771B6486D" blockId="3.[151,1436,1826,1910]" pageId="3" pageNumber="275">
