<heading id="D08E819BCC50D74D43C2A5DEFC7D9FA9" bold="true" box="[151,981,151,178]" fontSize="11" level="1" pageId="1" pageNumber="360" reason="1">
<emphasis id="B90DEAE5CC50D74D43C2A5DEFC7D9FA9" bold="true" box="[151,981,151,178]" pageId="1" pageNumber="360">
<taxonomicName id="4C794D74CC50D74D43C2A5DEFD359FA9" authority="Marinho" authorityName="Marinho &amp; Costa &amp; Vivallo" authorityYear="2019" box="[151,669,151,178]" class="Insecta" family="Eulophidae" genus="Chaenotetrastichus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="360" phylum="Arthropoda" rank="species" species="neotropicalis">
<emphasis id="B90DEAE5CC50D74D43C2A5DEFD8D9FAA" bold="true" box="[151,549,151,177]" italics="true" pageId="1" pageNumber="360">Chaenotetrastichus neotropicalis</emphasis>
