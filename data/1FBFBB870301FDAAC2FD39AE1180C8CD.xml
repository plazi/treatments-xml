<pageBreakToken id="F08B6A64A4CA445B9B916560C6D7028E" pageId="0" pageNumber="168" start="start">22</pageBreakToken>
. 
<taxonomicName id="69856CA60DE7BAEC48D40289A84FC636" class="Chilopoda" family="Lithobiidae" genus="Lithobius" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="168" phylum="Arthropoda" rank="species" species="Apfelbecki">L. Apfelbecki</taxonomicName>
<taxonomicNameLabel id="672C21CF2A751AD1780A7265C3232D6A" pageId="0" pageNumber="168" rank="species">n. sp.</taxonomicNameLabel>
