<typeStatus id="54DA8851FFE4FF9DFF12A013FF3AF8A3" box="[151,210,1804,1829]" pageId="15" pageNumber="216">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C614D70FFE4FF9DFEBCA013FCFFF8A2" authority="Viets, 1938" authorityName="Viets" authorityYear="1938" box="[313,791,1804,1829]" class="Arachnida" family="Halacaridae" genus="Enterohalacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="15" pageNumber="216" phylum="Arthropoda" rank="species" species="minutipalpus">
