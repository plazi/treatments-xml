<paragraph id="8B877956FFFA996A8377FE43FAF5CCBE" blockId="19.[264,1324,284,1310]" pageId="19" pageNumber="20">
<emphasis id="B94CA544FFFA996A8377FE43FDF3CCED" bold="true" box="[312,529,403,429]" pageId="19" pageNumber="20">
<collectingCountry id="F32F39C6FFFA996A8377FE43FE95CCED" box="[312,375,403,429]" name="Malta" pageId="19" pageNumber="20">Male</collectingCountry>
