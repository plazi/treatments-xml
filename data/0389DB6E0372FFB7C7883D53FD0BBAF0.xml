<emphasis id="B954B66A0372FFB6C7883D53FD7EBB57" bold="true" box="[151,720,1619,1645]" pageId="10" pageNumber="503">
<taxonomicName id="4C2011FB0372FFB6C7883D53FDC8BB57" authority="Chen" authorityName="Chen" box="[151,614,1619,1645]" class="Insecta" family="Drosophilidae" genus="Phortica" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="503" phylum="Arthropoda" rank="species" species="ryukyuensis" status="sp. nov." subGenus="Phortica">
