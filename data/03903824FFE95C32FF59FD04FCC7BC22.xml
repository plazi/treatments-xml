<collectionCode id="ED2811F7FFE95C32FBB9FC3DFB31BBFE" box="[1143,1232,963,989]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="2" pageNumber="569">AMNH</collectionCode>
