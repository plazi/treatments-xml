<taxonomicName id="8089724D405E1D830BEBA1B4B042E3FF" authorityName="Handmann" authorityYear="1882" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis fuchsi" order="Pteropoda" pageId="124" pageNumber="125" phylum="Mollusca" rank="species" species="fuchsi">
