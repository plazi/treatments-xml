<taxonomicName id="F3EC9501F87D7EC04D8A7E320586BDAE" authorityName="Brusina" authorityYear="1892" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis handmanni" order="Pteropoda" pageId="124" pageNumber="125" phylum="Mollusca" rank="species" species="handmanni">
