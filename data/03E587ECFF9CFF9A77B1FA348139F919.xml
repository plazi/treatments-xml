<taxonomicName id="4C4C4D79FF9DFF9A7613FB9D8210FB4B" authorityName="Lund" authorityYear="1839" box="[554,639,1151,1175]" class="Mammalia" family="Canidae" genus="Speothos" kingdom="Animalia" order="Carnivora" pageId="41" phylum="Chordata" rank="genus">
