AMNH 98639) and 
<taxonomicName id="4C4C4D79FF9DFF9A77E3FB8084FAFBA6" baseAuthorityName="Lund" baseAuthorityYear="1842" box="[986,1173,1122,1146]" class="Mammalia" family="Canidae" genus="Speothos" kingdom="Animalia" order="Carnivora" pageId="41" phylum="Chordata" rank="species" species="venaticus">
