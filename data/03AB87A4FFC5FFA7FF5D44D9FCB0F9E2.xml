<number id="3DCA2495FFC5FFA7FEE844E0FEEDFDB0" box="[317,331,509,533]" pageId="2" pageNumber="160" value="7.0">7</number>
). Body fusiform, 
<number id="3DCA2495FFC5FFA7FDC444E1FD9FFDB1" box="[529,569,508,532]" pageId="2" pageNumber="160" value="163.0">163</number>
(
<number id="3DCA2495FFC5FFA7FD9F44E1FD0CFDB1" box="[586,682,508,532]" pageId="2" pageNumber="160" value="190.5" valueMax="218.0" valueMin="163.0">163–218</number>
) long, 
<number id="3DCA2495FFC5FFA7FD2E44E1FCB0FDB1" box="[763,790,508,532]" pageId="2" pageNumber="160" value="65.0">65</number>
(
<number id="3DCA2495FFC5FFA7FCF344E1FCCAFDB1" box="[806,876,508,532]" pageId="2" pageNumber="160" value="69.5" valueMax="75.0" valueMin="64.0">64–75</number>
) wide, 
<number id="3DCA2495FFC5FFA7FC1444E1FC7AFDB1" box="[961,988,508,532]" pageId="2" pageNumber="160" value="60.0">60</number>
(
<number id="3DCA2495FFC5FFA7FC3944E0FB94FDB0" box="[1004,1074,509,533]" pageId="2" pageNumber="160" value="64.5" valueMax="80.0" valueMin="49.0">49–80</number>
) thick, yellowish. Gnathosoma 
<number id="3DCA2495FFC5FFA7FA4444E1FA0AFDB1" box="[1425,1452,508,532]" pageId="2" pageNumber="160" value="25.0">25</number>
(
<number id="3DCA2495FFC5FFA7FF454702FF73FD92" box="[144,213,543,567]" pageId="2" pageNumber="160" value="26.5" valueMax="28.0" valueMin="25.0">25–28</number>
) long, projecting down; dorsal pedipalp genual setae (
<emphasis id="B976EAA0FFC5FFA7FCF84702FC9CFD92" box="[813,826,543,567]" italics="true" pageId="2" pageNumber="160">d</emphasis>
) 
<number id="3DCA2495FFC5FFA7FC984702FCCEFD92" box="[845,872,543,567]" pageId="2" pageNumber="160" value="13.0">13</number>
(
<number id="3DCA2495FFC5FFA7FCAC4702FC19FD92" box="[889,959,543,567]" pageId="2" pageNumber="160" value="14.0" valueMax="15.0" valueMin="13.0">13–15</number>
), bifurcate at about 
<number id="3DCA2495FFC5FFA7FB4B4702FB01FD92" box="[1182,1191,543,567]" pageId="2" pageNumber="160" value="1.0">1</number>
/ 
<number id="3DCA2495FFC5FFA7FB644702FB66FD92" box="[1201,1216,543,567]" pageId="2" pageNumber="160" value="4.0">4</number>
from base. Prodorsal shield 
<number id="3DCA2495FFC5FFA7FF04475CFF4AFDFC" box="[209,236,577,601]" pageId="2" pageNumber="160" value="56.0">56</number>
(
<number id="3DCA2495FFC5FFA7FF29475CFEE7FDFC" box="[252,321,577,601]" pageId="2" pageNumber="160" value="55.5" valueMax="57.0" valueMin="54.0">54–57</number>
