and 
<taxonomicName id="4C604D5A1A414F11714CF96BECC4F96C" baseAuthorityName="Olivares &amp; Burckhardt" baseAuthorityYear="1997" box="[526,649,1738,1762]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="13" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
(
<bibRefCitation id="EFF14B281A414F1171DAF968ED1BF96C" author="Hodkinson" box="[664,854,1737,1762]" pageId="13" refString="Hodkinson, I. D. (1991) A review of Katacephala Crawford with description of an allied genus from South America (Insecta, Homoptera, Psylloidea). Zoologica Scripta, 20 (1), 77 - 87. https: // doi. org / 10.1111 / j. 1463 - 6409.1991. tb 00275. x" type="journal article" year="1991">Hodkinson 1991</bibRefCitation>
; 
<bibRefCitation id="EFF14B281A414F117020F968EA7DF96C" author="Burckhardt" box="[866,1072,1737,1762]" pageId="13" refString="Burckhardt, D. (1994 a) Generic key to Chilean jumping plant-lice (Homoptera: Psylloidea) with inclusion of potential pests. Revista Chilena de Entomologia, 21, 57 - 67." type="journal article" year="1994" yearSuffix="a">Burckhardt 1994a</bibRefCitation>
).
</paragraph>
</subSubSection>
