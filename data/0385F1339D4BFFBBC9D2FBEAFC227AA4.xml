<taxonomicName id="4C2C3BA69D4BFFBBC9F4FBEAFCCF7B28" authorityName="in The Plant List" authorityYear="2013" box="[690,895,1050,1077]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="avellana">
