<emphasis id="B957A82AFFC6A47DFF3DF99FFDAB797C" bold="true" box="[151,547,1553,1579]" pageId="6" pageNumber="7">
Genus 
<taxonomicName id="4C230FBBFFC6A47DFF44F99FFDAB797C" authority="Mills, 1938" authorityName="Mills" authorityYear="1938" box="[238,547,1553,1579]" class="Entognatha" family="Paronellidae" genus="Trogolaphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis id="B957A82AFFC6A47DFF44F99FFE1B797C" bold="true" box="[238,403,1553,1579]" italics="true" pageId="6" pageNumber="7">Trogolaphysa</emphasis>
<bibRefCitation id="EFB209C9FFC6A47DFE30F99FFDAB797C" author="Mills" box="[410,547,1553,1579]" pageId="6" pageNumber="7" refString="Mills, H. B. (1938) Collembola from Yucatan Caves. Carnegie Institution of Washington Publication, 491, 183 - 190." type="journal article" year="1938">
Mills, 
<number id="3DEB661FFFC6A47DFE42F99FFDAB797C" box="[488,547,1553,1579]" pageId="6" pageNumber="7" value="1938.0">1938</number>
</bibRefCitation>
