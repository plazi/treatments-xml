, 
<taxonomicName id="8AAE4D3259417A04FD826709FD1FF9DC" authorityName="Seidlitz" authorityYear="1893" box="[530,657,1564,1590]" class="Insecta" family="Tenebrionidae" genus="Blaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="rhumeri">
