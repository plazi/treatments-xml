</taxonomicName>
and 
<taxonomicName id="8AAE4D3259417A04FD5B6709FCDDF9DC" authorityName="Seidlitz" authorityYear="1893" box="[715,851,1564,1590]" class="Insecta" family="Tenebrionidae" genus="Blaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="sulcifera">
