<subSubSection id="D5C4E1C380BAFF19EDACCEF618610F3C" pageId="190" pageNumber="191" type="distribution">
<paragraph id="9D61B24880BAFF19EDACCEF618610F3C" blockId="190.[151,1436,151,320]" pageId="190" pageNumber="191">
<emphasis id="AFAA6E5A80BAFF19EDACCEF618A50E84" bold="true" box="[199,504,223,248]" pageId="190" pageNumber="191">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA80BAFF19EF69CEC91BCB0E84" box="[514,662,223,248]" country="Papua New Guinea" name="New Ireland" pageId="190" pageNumber="191">New Ireland</collectingRegion>
