<subSubSection id="C32765173C2FFFC42B9CFDF2FEEE4CD9" pageId="7" pageNumber="198" type="etymology">
<paragraph id="8B82369C3C2FFFC42B9CFDF2FEEE4CD9" blockId="7.[151,1437,152,755]" pageId="7" pageNumber="198">
<emphasis id="B949EA8E3C2FFFC42B9CFDF2FECF4C91" bold="true" box="[199,334,621,647]" pageId="7" pageNumber="198">Etymology.</emphasis>
