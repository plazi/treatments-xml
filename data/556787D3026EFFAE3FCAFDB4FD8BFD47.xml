<collectionCode id="BBDFAE00026EFFAD3DE3F934FCC0F947" box="[745,831,1730,1754]" country="New Zealand" httpUri="http://grbio.org/cool/n6z3-qvv0" name="Canterbury Museum" pageId="49" pageNumber="48">CMNZ</collectionCode>
