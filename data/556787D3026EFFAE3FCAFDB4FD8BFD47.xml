<paragraph id="DD7136C5026EFFAD3FCAFD54FCADFC87" blockId="49.[192,1440,674,794]" pageId="49" pageNumber="48">
<materialsCitation id="6DA63C98026EFFAD3FCAFD54FCFAFD67" collectionCode="NZAC" collectorName="G. Kuschel" country="New Zealand" location="Stewart Island" pageId="49" pageNumber="48" specimenCount="2" typeStatus="holotype">
