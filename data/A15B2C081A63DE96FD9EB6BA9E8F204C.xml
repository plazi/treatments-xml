<taxonomicName id="B9D3986689D1ED368229B5D1A4F9E726" authorityName="Seeley" authorityYear="1869" class="Reptilia" family="Anhangueridae" genus="Ornithocheirus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Pterosauria" pageId="20" pageNumber="21" phylum="Chordata" rank="genus">
