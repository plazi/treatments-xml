<taxonomicName id="2BBF2BF634748EEA857D6AAEB4C0ABE6" class="Arachnida" family="Damaeidae" genus="Damaeus" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tecticola">Damaeus tecticola Michael</taxonomicName>
