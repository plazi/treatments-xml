<taxonomicName id="EF088ED6B5387450CB39E5278A8AC22E" class="Arachnida" family="Damaeidae" genus="Damaeus" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tenuipes">Damaeus tenuipes Michael</taxonomicName>
