<taxonomicName id="4C624D1CFFC4FF87FD4FFF4D9AD1E18A" authority="Fabricius, 1805" authorityName="Fabricius" authorityYear="1805" box="[708,984,189,215]" class="Insecta" family="Neriidae" genus="Nerius" kingdom="Animalia" order="Diptera" pageId="117" pageNumber="118" phylum="Arthropoda" rank="genus">
<emphasis id="B916EA8DFFC4FF87FD4FFF4D9A1BE18B" box="[708,786,189,214]" italics="true" pageId="117" pageNumber="118">Nerius</emphasis>
Fabricius, 1805
</taxonomicName>
; junior synonym of 
<taxonomicName id="4C624D1CFFC4FF87FB44FF4D980EE1A1" authority="Fabricius, 1805" authorityName="Fabricius" authorityYear="1805" class="Insecta" family="Neriidae" genus="Nerius" kingdom="Animalia" order="Diptera" pageId="117" pageNumber="118" phylum="Arthropoda" rank="genus">
<emphasis id="B916EA8DFFC4FF87FB44FF4D9C14E18B" box="[1231,1309,189,214]" italics="true" pageId="117" pageNumber="118">Nerius</emphasis>
Fabricius, 1805
</taxonomicName>
[
<emphasis id="B916EA8DFFC4FF87FE93FF129847E1A6" box="[280,334,226,251]" italics="true" pageId="117" pageNumber="118">teste</emphasis>
<bibRefCitation id="EFF34B6EFFC4FF87FEDEFF129B22E1A1" author="Neave, S. A." box="[341,555,226,252]" pageId="117" pageNumber="118" refId="ref132537" refString="Neave, S. A. (1940) Nomenclator zoologicus. A list of the names of genera and subgenera in zoology from the tenth edition of Linnaeus 1758 to the end of 1935. In four volumes. Vol. III. M - P. Zoological Society of London. xiv + 957 pp. [19 June] [Date of receipt at BMNH.]" type="book" year="1940">Neave, 1940: 318</bibRefCitation>
