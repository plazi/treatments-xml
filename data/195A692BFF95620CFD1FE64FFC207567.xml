<quantity id="560B75D8FF96620CFC5FE0F4FC2E71AF" box="[901,989,725,746]" metricMagnitude="-2" metricUnit="m" metricValue="1.75" pageId="6" unit="mm" value="17.5">17.5 mm</quantity>
; prothoracic length 
<quantity id="560B75D8FF96620CFD77E0D1FD087040" box="[685,763,752,773]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="6" unit="mm" value="2.6">2.6 mm</quantity>
; anterior prothoracic width 
<quantity id="560B75D8FF96620CFBD0E0D1FBAB7040" box="[1034,1112,752,773]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="6" unit="mm" value="2.6">2.6 mm</quantity>
; basal prothoracic width 
<quantity id="560B75D8FF96620CFC86E12BFC5A705A" box="[860,937,778,799]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="6" unit="mm" value="2.4">2.4 mm</quantity>
; humeral width 
<quantity id="560B75D8FF96620CFB9EE12BFB63705A" box="[1092,1168,778,799]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="6" unit="mm" value="3.5">3.5 mm</quantity>
; elytral length 
<quantity id="560B75D8FF96620CFCEFE104FC7D707F" box="[821,910,805,826]" metricMagnitude="-2" metricUnit="m" metricValue="1.23" pageId="6" unit="mm" value="12.3">12.3 mm</quantity>
.
</paragraph>
</subSubSection>
