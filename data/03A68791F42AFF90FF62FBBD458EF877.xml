(
<taxonomicName id="4C0F4D04F42AFF90FC0AF86544D5F877" box="[1023,1223,2011,2035]" class="Magnoliopsida" family="Dipterocarpaceae" kingdom="Plantae" order="Malvales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Dipterocarpaceae</taxonomicName>
) (
<bibRefCitation id="EF9E4B76F42AFF90FB28F8654582F877" author="Holloway" box="[1245,1424,2011,2035]" pageId="12" pageNumber="13" refString="Holloway, J. D. (1985) Moths of Borneo, part 14: Family Noctuidae: Subfamilies Euteliinae, Stictopterinae, Plusiinae, Pantheinae. Malayan Nature Journal, 38, 157 - 317." type="journal article" year="1985">
Holloway 
