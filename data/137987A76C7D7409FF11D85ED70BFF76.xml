<quantity id="5C289B546C7F7408FDD1DB54D4FEF92B" box="[599,686,1649,1673]" metricMagnitude="-3" metricUnit="m" metricValue="6.9" pageId="2" pageNumber="45" unit="mm" value="6.9">6.9 mm</quantity>
