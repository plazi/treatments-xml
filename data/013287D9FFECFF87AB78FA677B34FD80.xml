<collectorName id="246E5319FFECFF80A896F9D078D2F9FD" box="[647,726,1537,1561]" pageId="35" pageNumber="2794">Viçosa</collectorName>
, 
<collectingRegion id="4B5FF82DFFECFF80A8F4F9D07980F9FD" box="[741,900,1537,1561]" country="Brazil" name="Minas Gerais" pageId="35" pageNumber="2794">Minas Gerais</collectingRegion>
</materialsCitation>
