<materialsCitation id="39F33C92FFECFF80AB8BF9D37980F9FD" box="[410,900,1537,1561]" collectorName="Vicosa" location="Minas Gerais" pageId="35" pageNumber="2794" specimenCount="1" stateProvince="Minas Gerais">
