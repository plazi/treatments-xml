<taxonomicName id="4C284D0DFF99B566FD330565F8473B28" authority="Percy Csiro, 2003" authorityName="Percy Csiro" authorityYear="2003" box="[641,847,853,874]" class="Insecta" family="Psyllidae" genus="Arytinnis" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="424" phylum="Arthropoda" rank="species" species="menceyata" status="sp. n.">
