<taxonomicName id="9EDF6AC1FFC5BD7EFEA028FEFD2AFC0B" authority="Fabricius, 1781: 331" authorityName="Fabricius" authorityPageNumber="331" authorityYear="1781" box="[297,720,960,982]" class="Insecta" family="Meloidae" genus="Cerocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="schreberi">
