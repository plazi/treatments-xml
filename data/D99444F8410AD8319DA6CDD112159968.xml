<taxonomicName id="9E7052B3257A4ECC9352F8733E26D0B7" authorityName="Linnaeus" authorityYear="1753" baseAuthorityName="Linnaeus" class="Magnoliopsida" family="Asteraceae" genus="Bidens" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="fruticosa">
