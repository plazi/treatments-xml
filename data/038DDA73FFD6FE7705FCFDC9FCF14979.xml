<taxonomicName id="4C2410E6FFD6FE77078EFCC9FEFD4C27" authorityName="Riedel" authorityYear="1967" class="Polycystina" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="66" pageNumber="570" phylum="Radiozoa" rank="superFamily" superFamily="Amphipyndacoidea">Amphipyndacoidea</taxonomicName>
