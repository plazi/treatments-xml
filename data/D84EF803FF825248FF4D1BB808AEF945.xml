<taxonomicName id="97E73296FF825248FD3719D408A7F944" box="[749,897,1689,1714]" class="Insecta" family="Lucanidae" genus="Sclerostomus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="sulcicollis">
