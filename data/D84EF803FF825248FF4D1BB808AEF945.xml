<emphasis id="62939507FF825248FDA1193E0830F97A" box="[635,790,1651,1676]" italics="true" pageId="4" pageNumber="27">
<taxonomicName id="97E73296FF825248FDA1193E0837F97A" box="[635,785,1651,1676]" class="Insecta" family="Lucanidae" genus="Sclerostomus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="sulcicollis" status="new synonymy">S. sulcicollis</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="79A0287CFF825248FCC6193F08F6F97A" box="[796,976,1650,1676]" pageId="4" pageNumber="27">new synonymy</taxonomicNameLabel>
. One 
<typeStatus id="8F5CF7B7FF825248FBCB193F0F89F97A" box="[1041,1199,1650,1676]" pageId="4" pageNumber="27">paralectotype</typeStatus>
from the 
<typeStatus id="8F5CF7B7FF825248FAFA193F0E75F97A" box="[1312,1363,1650,1676]" pageId="4" pageNumber="27">type</typeStatus>
series of 
<taxonomicName id="97E73296FF825248FF6C19D40AECF945" authority="Westwood" authorityName="Westwood" box="[182,458,1689,1715]" class="Insecta" family="Lucanidae" genus="Sclerostomus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="costatus">
<emphasis id="62939507FF825248FF6C19D40A12F944" box="[182,308,1689,1714]" italics="true" pageId="4" pageNumber="27">S. costatus</emphasis>
