<quantity id="98099B14FFD607491582FD16F857FD1E" box="[452,533,659,686]" metricMagnitude="-2" metricUnit="m" metricValue="2.2" pageId="11" pageNumber="344" unit="mm" value="22.0">22 mm</quantity>
