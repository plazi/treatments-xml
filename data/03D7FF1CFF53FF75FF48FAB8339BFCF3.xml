<taxonomicName id="4C7E3589FF53FF75FCA2F9C53568FD32" authorityName="Burmeister" authorityYear="1844" box="[810,1044,683,707]" class="Insecta" family="Rutelidae" genus="Pelidnota" kingdom="Animalia" order="Coleoptera" pageId="152" pageNumber="151" phylum="Arthropoda" rank="species" species="virescens">
