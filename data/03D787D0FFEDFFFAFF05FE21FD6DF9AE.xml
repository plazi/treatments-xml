</emphasis>
<taxonomicName id="4C7E4D45FFECFFFAFB69FA82FAE8FAC6" authority="PLANT, 2013" authorityName="PLANT" authorityYear="2013" box="[1275,1393,1334,1358]" class="Insecta" family="Hybotidae" genus="Hybos" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="paknok" status="sp. nov.">
