<taxonomicName id="4C7E4D45FFECFFFAFEDFFA83FD41FAC6" authority="Yang &amp; Yang, 2004" authorityName="Yang &amp; Yang" authorityYear="2004" box="[333,728,1334,1359]" class="Insecta" family="Hybotidae" genus="Hybos" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="longus">
