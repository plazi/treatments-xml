<taxonomicName id="4C754D32FF842F35D98CFBB8FBDF2B20" authority="Kim &amp; Chan, 2005" authorityName="Kim &amp; Chan" authorityYear="2005" box="[967,1084,1099,1123]" class="Malacostraca" family="Crangonidae" genus="Prionocrangon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="1601" phylum="Arthropoda" rank="species" species="formosa" status="sp. nov.">
