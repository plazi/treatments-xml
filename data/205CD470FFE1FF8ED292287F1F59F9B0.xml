<quantity id="6F0DC883FFE1FF8ED10C28C71F62FB2F" box="[812,879,1180,1200]" metricMagnitude="2" metricUnit="m" metricValue="4.7" pageId="17" pageNumber="1694" unit="m" value="470.0">470 m</quantity>
