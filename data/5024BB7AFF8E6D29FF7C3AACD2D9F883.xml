<emphasis id="EAF9D67EFF8E6D29FB8F38ECD5F9F95F" box="[1124,1165,1765,1788]" italics="true" pageId="19" pageNumber="320">e.g.</emphasis>
, 
<taxonomicName id="1F8D71EFFF8E6D29FB7438EDD457F95E" box="[1183,1315,1764,1789]" class="Aves" family="Furnariidae" kingdom="Animalia" order="Passeriformes" pageId="19" pageNumber="320" phylum="Chordata" rank="family">Furnariidae</taxonomicName>
, 
<taxonomicName id="1F8D71EFFF8E6D29FAD838EDD4ECF95E" box="[1331,1432,1764,1789]" class="Aves" family="Icteridae" kingdom="Animalia" order="Passeriformes" pageId="19" pageNumber="320" phylum="Chordata" rank="family">Icteridae</taxonomicName>
, 
<taxonomicName id="1F8D71EFFF8E6D29FF7C390ED18BF883" box="[151,255,1799,1824]" class="Aves" family="Mimidae" kingdom="Animalia" order="Passeriformes" pageId="19" pageNumber="320" phylum="Chordata" rank="family">Mimidae</taxonomicName>
, 
<taxonomicName id="1F8D71EFFF8E6D29FEE1390ED0DEF883" box="[266,426,1799,1824]" class="Aves" family="Troglodytidae" kingdom="Animalia" order="Passeriformes" pageId="19" pageNumber="320" phylum="Chordata" rank="family">Troglodytidae</taxonomicName>
, and 
<taxonomicName id="1F8D71EFFF8E6D29FE0E390ED311F883" box="[485,613,1799,1824]" class="Aves" family="Tyrannidae" kingdom="Animalia" order="Passeriformes" pageId="19" pageNumber="320" phylum="Chordata" rank="family">Tyrannidae</taxonomicName>
) (
<bibRefCitation id="BC1C779DFF8E6D29FD91390ED2D5F883" author="Turienzo" box="[634,929,1799,1824]" pageId="19" pageNumber="320" refString="Turienzo, P. &amp; Di Iorio, O. (2007) Insects found in birds'nests from Argentina. Part I: a bibliographical review, with taxonomical corrections, comments and a hypothetical mechanism of transmission of cimicid bugs. Zootaxa, 1561, 1 - 52." type="journal article" year="2007">
