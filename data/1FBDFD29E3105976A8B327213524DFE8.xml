<taxonomicName id="BB128343A8C1432F8BD9DD28BDADC0E6" authority="Huth, 1895" authorityName="DuPasquier &amp; Andro-Durand &amp; Batory &amp; Wang &amp; Jabbour" authorityYear="2021" class="Magnoliopsida" family="Ranunculaceae" genus="Consolida" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Consolida orientalis subsp. var. var. thessalonica" order="Ranunculales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="variety" species="orientalis" status="comb. nov." subSpecies="var." variety="thessalonica">
