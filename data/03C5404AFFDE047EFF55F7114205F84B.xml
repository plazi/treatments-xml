<paragraph id="8BD3F15CFFDE047EFF0BF51F443FF8DF" blockId="9.[151,1436,1362,1963]" pageId="9" pageNumber="31">
<emphasis id="B9182D4EFFDE047EFF0BF51F46CAF8FA" bold="true" box="[201,460,1794,1819]" inLineHeading="true" pageId="9" pageNumber="31" reason="1">General distribution.</emphasis>
East 
<collectingCountry id="F37BB1CCFFDE047EFDDAF51F456FF8FB" box="[536,617,1794,1818]" name="Turkey" pageId="9" pageNumber="31">Turkey</collectingCountry>
, 
<collectingCountry id="F37BB1CCFFDE047EFDBEF51F45DEF8FB" box="[636,728,1794,1818]" name="Georgia" pageId="9" pageNumber="31">Georgia</collectingCountry>
, 
<collectingCountry id="F37BB1CCFFDE047EFD28F51F4449F8FB" box="[746,847,1794,1818]" name="Armenia" pageId="9" pageNumber="31">Armenia</collectingCountry>
, 
<collectingCountry id="F37BB1CCFFDE047EFCA0F51F44E6F8FB" box="[866,992,1794,1818]" name="Azerbaijan" pageId="9" pageNumber="31">Azerbaijan</collectingCountry>
, 
<collectingCountry id="F37BB1CCFFDE047EFC30F51F4335F8FB" box="[1010,1075,1794,1818]" name="Israel" pageId="9" pageNumber="31">Israel</collectingCountry>
, 
<collectingCountry id="F37BB1CCFFDE047EFB87F51E4373F8FB" box="[1093,1141,1795,1818]" name="Iraq" pageId="9" pageNumber="31">Iraq</collectingCountry>
, 
<collectingCountry id="F37BB1CCFFDE047EFB4AF51E43BEF8FB" box="[1160,1208,1795,1818]" name="Iran" pageId="9" pageNumber="31">Iran</collectingCountry>
