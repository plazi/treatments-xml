<typeStatus id="54CB1DC19322461C86C3FD3934B4FD59" box="[151,204,682,705]" pageId="1" pageNumber="42">Type</typeStatus>
species: 
<taxonomicName id="4C70D8E09322461C8779FD393696FD59" authority="Schedl, 1965" authorityName="Schedl" authorityYear="1965" box="[301,750,682,705]" class="Insecta" family="Curculionidae" genus="Dolurgocleptes" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="42" phylum="Arthropoda" rank="species" species="malgassicus">
