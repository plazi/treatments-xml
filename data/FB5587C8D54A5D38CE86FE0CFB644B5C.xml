<quantity id="B4049B3BD54D5D38CC62FDCCFDF84A5C" box="[548,626,577,602]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="28" pageNumber="27" unit="km" value="15.0">15 km</quantity>
