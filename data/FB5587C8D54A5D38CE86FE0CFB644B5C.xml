<quantity id="B4049B3BD54D5D38CB55FDCFFAB74A5C" box="[1299,1341,578,602]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="28" pageNumber="27" unit="m" value="2.0">2m</quantity>
, 
<collectionCode id="15EDAE1BD54D5D38CB01FDCFFA1B4A5C" box="[1351,1425,578,602]" country="USA" httpUri="http://grbio.org/cool/jekt-x82m" name="Florida State Collection of Arthropods, The Museum of Entomology" pageId="28" pageNumber="27" type="Museum">FSCA</collectionCode>
