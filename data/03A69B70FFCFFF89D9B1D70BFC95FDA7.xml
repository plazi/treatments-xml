(
<collectionCode id="ED1EB2A3FFC9FF89D817D21FFE5FFE13" box="[305,392,369,395]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="116" pageNumber="117">AMNH</collectionCode>
<collectionCode id="ED1EB2A3FFC9FF89D8B5D21FFE69FE13" box="[403,446,369,395]" pageId="116" pageNumber="117">PBI</collectionCode>
