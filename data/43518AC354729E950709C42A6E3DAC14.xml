<taxonomicName id="DA00AE7D0D7C21B7DAB0585F5E6CAFE7" class="Insecta" family="Delphacidae" genus="Sogatella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sogatella furcifera" order="Hemiptera" pageId="32" pageNumber="77" phylum="Arthropoda" rank="species" species="furcifera">Sogatella furcifera</taxonomicName>
