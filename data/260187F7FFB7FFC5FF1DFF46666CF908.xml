<caption id="FAD76669FFACFFC4FF1DF80064C0FF93" ID-DOI="http://doi.org/10.5281/zenodo.5363474" ID-Zenodo-Dep="5363474" box="[151,979,2001,2025]" httpUri="https://zenodo.org/record/5363474/files/figure.png" pageId="22" pageNumber="279" startId="22.[151,255,2001,2025]" targetBox="[280,1305,190,1983]" targetPageId="22">
