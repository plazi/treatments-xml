<taxonomicName id="4C6844F76E03D205FE95FDC4FDACE178" authority="Vokes, 1970" authorityName="Vokes" authorityYear="1970" box="[321,595,613,637]" class="Gastropoda" family="Muricidae" genus="Pazinotus" kingdom="Animalia" order="Neogastropoda" pageId="20" pageNumber="457" phylum="Mollusca" rank="genus">
