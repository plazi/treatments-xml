<taxonomicName id="7A74715B1895F2983D8973571527B381" class="Insecta" family="Tortricidae" genus="Clepsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Clepsis semiana" order="Lepidoptera" pageId="0" pageNumber="51" phylum="Arthropoda" rank="species" species="semiana">
