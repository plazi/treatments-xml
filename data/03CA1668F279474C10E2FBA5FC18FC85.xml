<quantity id="4C9B0A9BF279474C1700FA8FFB69FE65" box="[1102,1264,406,430]" metricMagnitude="-3" metricUnit="m" metricValue="2.675" metricValueMax="3.4" metricValueMin="1.95" pageId="5" pageNumber="298" unit="mm" value="2.675" valueMax="3.4" valueMin="1.95">1.95–3.40 mm</quantity>
