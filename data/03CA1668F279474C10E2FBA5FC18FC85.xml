<quantity id="4C9B0A9BF279474C164AFA6FFA3EFE45" box="[1284,1447,374,398]" metricMagnitude="-3" metricUnit="m" metricValue="4.875" metricValueMax="5.72" metricValueMin="4.03" pageId="5" pageNumber="298" unit="mm" value="4.875" valueMax="5.72" valueMin="4.03">4.03–5.72 mm</quantity>
