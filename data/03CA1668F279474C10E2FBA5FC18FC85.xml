<quantity id="4C9B0A9BF279474C1011FA8FFB98FE65" box="[863,1025,406,430]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" metricValueMax="2.06" metricValueMin="1.34" pageId="5" pageNumber="298" unit="mm" value="1.7" valueMax="2.06" valueMin="1.34">1.34–2.06 mm</quantity>
