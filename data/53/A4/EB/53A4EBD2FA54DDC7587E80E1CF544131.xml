<subSubSection pageId="9" pageNumber="10" type="nomenclature">
<paragraph pageId="9" pageNumber="10">
8. 
<taxonomicName LSID="urn:lsid:zoobank.org:act:0235E0EC-2D2D-4857-9720-33CA745EFB42" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus angustus" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="angustus">Trigonopterus angustus Riedel</taxonomicName>
