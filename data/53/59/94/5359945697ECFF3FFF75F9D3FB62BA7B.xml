, 
<taxonomicName authority="(Brunner, 1888)" authorityName="Brunner" authorityYear="1888" baseAuthorityName="Brunner von Wattenwyl" baseAuthorityYear="1888" box="[866,1155,1859,1881]" class="Insecta" family="Gryllacrididae" genus="Haplogryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="189" pageNumber="190" phylum="Arthropoda" rank="species" species="castanea">
