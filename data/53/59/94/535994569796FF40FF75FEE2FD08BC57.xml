<taxonomicName baseAuthorityName="Stoll" baseAuthorityYear="1813" box="[269,359,437,459]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="193" pageNumber="194" phylum="Arthropoda" rank="species" species="signifera">
