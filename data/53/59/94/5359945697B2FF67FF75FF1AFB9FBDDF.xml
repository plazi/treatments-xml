<taxonomicName authorityName="Karny" authorityYear="1937" box="[368,538,1817,1839]" class="Insecta" family="Gryllacrididae" genus="Phlebogryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="genus">
<emphasis box="[368,538,1817,1839]" italics="true" pageId="229" pageNumber="230">Phlebogryllacris</emphasis>
</taxonomicName>
: 
<emphasis bold="true" box="[553,602,1817,1838]" pageId="229" pageNumber="230">A–D</emphasis>
, 
<taxonomicName authority="(Karny, 1926)" authorityName="Karny" authorityYear="1926" baseAuthorityName="Karny" baseAuthorityYear="1926" box="[616,881,1817,1839]" class="Insecta" family="Gryllacrididae" genus="Phlebogryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="cyanipes">
<emphasis box="[616,636,1818,1839]" italics="true" pageId="229" pageNumber="230">P.</emphasis>
<emphasis box="[642,731,1818,1839]" italics="true" pageId="229" pageNumber="230">cyanipes</emphasis>
(Karny, 1926)
</taxonomicName>
female holotype; 
<emphasis bold="true" box="[1069,1108,1817,1838]" pageId="229" pageNumber="230">E–I</emphasis>
, 
<taxonomicName authority="(Walker, 1869)" authorityName="Walker" authorityYear="1869" baseAuthorityName="Walker" baseAuthorityYear="1869" box="[1122,1379,1817,1839]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="venosa">
<emphasis box="[1122,1142,1818,1839]" italics="true" pageId="229" pageNumber="230">P.</emphasis>
