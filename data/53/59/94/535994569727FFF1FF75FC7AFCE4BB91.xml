<taxonomicName authorityName="Karny" authorityYear="1937" box="[808,981,1154,1177]" class="Insecta" family="Gryllacrididae" genus="Anancistrogera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="genus">
<emphasis box="[808,981,1154,1177]" italics="true" pageId="112" pageNumber="113">Anancistrogera</emphasis>
</taxonomicName>
. 
<taxonomicName authorityName="Karny" authorityYear="1937" box="[999,1195,1153,1177]" class="Insecta" family="Gryllacrididae" genus="Aphanogryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="genus">
<emphasis box="[999,1195,1153,1177]" italics="true" pageId="112" pageNumber="113">Aphanogryllacris</emphasis>
</taxonomicName>
