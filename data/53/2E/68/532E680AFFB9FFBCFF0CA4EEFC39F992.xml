<quantity box="[957,1021,1013,1038]" metricMagnitude="4" metricUnit="m" metricValue="1.0" pageId="21" pageNumber="374" unit="km" value="10.0">10km</quantity>
E Gordonvale, 
<quantity box="[1207,1344,1014,1038]" metricMagnitude="2" metricUnit="m" metricValue="9.5" metricValueMax="10.0" metricValueMin="9.0" pageId="21" pageNumber="374" unit="m" value="950.0" valueMax="1000.0" valueMin="900.0">900–1000m</quantity>
, 
