<paragraph blockId="21.[813,1457,215,292]" pageId="21" pageNumber="526">
TYPE LOCALITY. — Lar valley (
<quantity box="[1129,1272,215,239]" metricMagnitude="3" metricUnit="m" metricValue="2.54" metricValueMax="2.9" metricValueMin="2.18" pageId="21" pageNumber="526" unit="m" value="2540.0" valueMax="2900.0" valueMin="2180.0">2180-2900 m</quantity>
