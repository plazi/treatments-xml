<typeStatus box="[226,312,816,840]" pageId="7" pageNumber="512">holotype</typeStatus>
and five 
<typeStatus box="[402,494,816,840]" pageId="7" pageNumber="512">paratypes</typeStatus>
from the coastal plain of the 
<collectingCounty box="[132,254,842,866]" pageId="7" pageNumber="512">Persian Gulf</collectingCounty>
in 
<collectingRegion box="[290,425,842,866]" country="Iran" name="Fars" pageId="7" pageNumber="512">Fars Province</collectingRegion>
. 
<collectingMunicipality box="[436,536,842,866]" pageId="7" pageNumber="512">According</collectingMunicipality>
