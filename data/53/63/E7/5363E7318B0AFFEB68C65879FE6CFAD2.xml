<location LSID="urn:lsid:plazi:treatment:5363E7318B0AFFEB68C65879FE6CFAD2:DE1500FC8B0AFFEB6A155925FDCCFCBE" box="[571,622,842,866]" county="Persian Gulf" municipality="According" name="Smid" pageId="7" pageNumber="512" stateProvince="Fars">Smid</location>
<emphasis box="[630,679,842,866]" italics="true" pageId="7" pageNumber="512">et al.</emphasis>
