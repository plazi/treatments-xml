<number box="[1175,1191,1199,1223]" pageId="14" pageNumber="488" value="2.0">2</number>
, -, 
<number box="[1228,1242,1199,1223]" pageId="14" pageNumber="488" value="3.0">3</number>
sens, 
<number box="[1314,1328,1199,1223]" pageId="14" pageNumber="488" value="1.0">1</number>
, 0/ 
<number box="[1370,1380,1199,1223]" pageId="14" pageNumber="488" value="1.0">1</number>
, 0, 0 ms. Mac m 
