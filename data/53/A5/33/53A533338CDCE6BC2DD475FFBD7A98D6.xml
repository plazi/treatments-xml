<taxonomicName authority="R端bsaamen, 1899" authorityName="R端bsaamen" authorityYear="1899" class="Insecta" family="Fagaceae" genus="Coccomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Coccomorpha" order="Diptera" pageId="7" pageNumber="64" phylum="Arthropoda" rank="family">Fagaceae</taxonomicName>
