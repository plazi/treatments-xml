<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:B824B602-4F87-4771-B334-61385645D6C4" class="Actinopterygii" family="Ariidae" genus="Ketengus" kingdom="Animalia" lsidName-ZBK="Ketengus Bleeker 1847:167" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Ketengus</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:F589893C-76A9-4820-A8B9-B794837B3FB9" class="Actinopterygii" family="Ariidae" genus="Nedystoma" kingdom="Animalia" lsidName-ZBK="Nedystoma Ogilby 1898:32" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Nedystoma</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:086E7B0F-D030-44BD-9DCC-8CAEB178CBAA" class="Actinopterygii" family="Ariidae" genus="Notarius" kingdom="Animalia" lsidName-ZBK="Notarius Gill 1863:171" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Notarius</taxonomicName>
, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:7EFF0E2C-627F-4985-9DCA-5178DA9034F3" class="Actinopterygii" family="Ariidae" genus="Osteogeneiosus" kingdom="Animalia" lsidName-ZBK="Osteogeneiosus Bleeker 1846:173" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Osteogeneiosus</taxonomicName>
