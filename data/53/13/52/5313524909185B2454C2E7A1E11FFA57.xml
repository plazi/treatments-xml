<subSubSection pageId="5" pageNumber="40" type="reference_group">
<paragraph blockId="5.[151,495,461,516]" box="[151,495,461,484]" pageId="5" pageNumber="40">
<taxonomicName authority="Distant, 1908: 224" authorityName="Distant" authorityPageNumber="224" authorityYear="1908" box="[151,495,461,484]" class="Insecta" family="Cicadellidae" genus="Kolla" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="40" phylum="Arthropoda" rank="species" species="lineata">
