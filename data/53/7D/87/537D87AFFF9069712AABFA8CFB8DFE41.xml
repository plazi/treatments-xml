<typeStatus box="[840,938,1622,1646]" pageId="10" pageNumber="55">syntypes</typeStatus>
from 
<collectionCode LSID="urn:lsid:biocol.org:col:34975" box="[1006,1092,1622,1646]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34975" name="Instituut voor Taxonomische Zoologie, Zoologisch Museum" pageId="10" pageNumber="55">ZMAN</collectionCode>
