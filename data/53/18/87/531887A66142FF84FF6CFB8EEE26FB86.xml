<paragraph blockId="1.[151,670,1218,1244]" box="[151,670,1218,1244]" pageId="1" pageNumber="52">
<typeStatus box="[151,209,1218,1244]" pageId="1" pageNumber="52">Type</typeStatus>
species: 
<taxonomicName box="[316,599,1219,1244]" class="Arachnida" family="Proctophyllodidae" genus="Atelophyllodes" kingdom="Animalia" order="Astigmata" pageId="1" pageNumber="52" phylum="Arthropoda" rank="species" species="menurae" status="sp. n." subClass="Acari" superFamily="Analgoidea">
<emphasis box="[316,599,1219,1244]" italics="true" pageId="1" pageNumber="52">Atelophyllodes menurae</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[608,670,1218,1244]" pageId="1" pageNumber="52" rank="species">sp. n.</taxonomicNameLabel>
