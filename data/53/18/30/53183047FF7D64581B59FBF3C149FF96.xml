<taxonomicName authorityName="Mata-Casanova" authorityYear="2013" box="[670,749,1705,1729]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="pilosa">
<emphasis box="[670,749,1705,1729]" italics="true" pageId="2" pageNumber="33">X. pilosa</emphasis>
