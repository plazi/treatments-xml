</taxonomicName>
and 
<taxonomicName authority="Mata-Casanova, 2013" authorityName="Mata-Casanova" authorityYear="2013" box="[464,760,1652,1677]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="pilosa">
