<emphasis box="[886,1109,1527,1551]" italics="true" pageId="9" pageNumber="40">
<taxonomicName authorityName="Mata-Casanova &amp; Pujade-Villar" authorityYear="2014" box="[886,999,1527,1551]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="arapahoe">X. arapahoe</taxonomicName>
, 
<taxonomicName authorityName="Ashmead" authorityYear="1896" box="[1008,1105,1527,1551]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="flavipes">X. flavipes</taxonomicName>
