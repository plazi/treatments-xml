<subSubSection pageId="27" pageNumber="220" type="distribution">
<paragraph blockId="27.[151,1437,151,971]" box="[199,1222,838,863]" pageId="27" pageNumber="220">
<emphasis bold="true" box="[199,352,838,863]" pageId="27" pageNumber="220">Distribution.</emphasis>
