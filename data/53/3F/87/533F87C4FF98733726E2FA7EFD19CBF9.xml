<collectionCodeEnum box="[989,1006,764,788]" pageId="21" pageNumber="214">
<collectionCode box="[989,1006,764,788]" country="Sweden" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15668" name="Department of Botany, Swedish Museum of Natural History" pageId="21" pageNumber="214">S</collectionCode>
