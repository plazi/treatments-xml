<taxonomicName id="4C574D65FF9BFFE0FE11FDC7FD83FD94" authorityName=": Doderlein" authorityYear="1910" box="[465,586,592,615]" class="Echinoidea" family="Loveniidae" genus="Echinocardium" kingdom="Animalia" order="Spatangoida" pageId="60" pageNumber="61" phylum="Echinodermata" rank="species" species="capense">
