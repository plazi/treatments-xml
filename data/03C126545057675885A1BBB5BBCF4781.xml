<taxonomicNameLabel id="A22FF62B505767588689BF88BF3945F8" box="[959,1048,1194,1219]" pageId="1" pageNumber="108" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C68ECC1505767588178BF89BFE345F8" authority="Jendek &amp; Grebennikov, 2018" authorityName="Jendek &amp; Grebennikov" authorityYear="2018" box="[1102,1218,1195,1219]" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="108" phylum="Arthropoda" rank="species" species="porthos" status="sp. nov.">
<emphasis id="B91C4B50505767588178BF89BFE345F8" box="[1102,1218,1195,1219]" italics="true" pageId="1" pageNumber="108">A. porthos</emphasis>
</taxonomicName>
<emphasis id="B91C4B505057675881FCBF88BE0545F8" bold="true" box="[1226,1316,1194,1219]" pageId="1" pageNumber="108">
