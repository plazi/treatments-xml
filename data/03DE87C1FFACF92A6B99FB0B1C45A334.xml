; ratio of pedipalpal femur/movable finger 
<number id="3DBF24F0FFA2F92A6FC2FDB21D60A22E" box="[1228,1329,574,598]" pageId="73" pageNumber="74" value="1.1749999999999998" valueMax="1.2" valueMin="1.15">1.15–1.2</number>
; ratio of pedipalpal femur/carapace 
<number id="3DBF24F0FFA2F92A6AC8FDED19BDA201" box="[454,492,609,633]" pageId="73" pageNumber="74" value="1.5">1.5</number>
