<number id="3DBF24F0FFA2F92A69C0FE8D1B60A161" box="[718,817,257,281]" pageId="73" pageNumber="74" value="149.5" valueMax="150.0" valueMin="149.0">149–150</number>
) 
<number id="3DBF24F0FFA2F92A684FFE8D1B35A161" box="[833,868,257,281]" pageId="73" pageNumber="74" value="6.8">6.8</number>
