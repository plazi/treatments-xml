<quantity id="4C8F9B32FFA2F92A6A5FFE0219F9A1DE" box="[337,424,398,422]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="73" pageNumber="74" unit="mm" value="0.1">0.1 mm</quantity>
