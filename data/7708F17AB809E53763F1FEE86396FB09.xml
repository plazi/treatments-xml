<taxonomicName id="38A13BEFB809E537624EFB80628BFB6D" authorityName=", Unidentified" box="[296,487,1048,1074]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="order">LEPIDOPTERA</taxonomicName>
