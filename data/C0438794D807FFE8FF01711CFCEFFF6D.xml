<taxonomicName id="8FEA4D01D804FFE9FDB47326FCA5FB4C" authority="Shen &amp; Leschen, 2020" authorityName="Shen &amp; Leschen" authorityYear="2020" box="[546,769,1326,1350]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="70" pageNumber="10" phylum="Arthropoda" rank="species" species="waikaremoana" status="sp. n." subGenus="Byraxis">
