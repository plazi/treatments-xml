<paragraph id="76B152432E0E987DD1E23570F134D9F0" pageId="0" pageNumber="89">
<taxonomicName id="88A10C8EE537EA323292459C967C3A5E" LSID="7D46C9F1-F382-5544-AF9D-5E4A58DBD84C" authority="F. Wen, Y. G. Wei &amp; T. V. Do" authorityName="F. Wen, Y. G. Wei &amp; T. V. Do" authorityYear="2020" class="Magnoliopsida" family="Gesneriaceae" genus="Michaelmoelleria" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Michaelmoelleria" order="Lamiales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="genus" status="gen. nov.">Michaelmoelleria F. Wen, Y.G. Wei &amp; T.V. Do</taxonomicName>
