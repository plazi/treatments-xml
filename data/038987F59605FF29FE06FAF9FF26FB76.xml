<paragraph id="8B9F36E39605FF26FE06FAF9FCD4FADC" blockId="39.[484,826,1298,1322]" box="[484,826,1298,1322]" pageId="39" pageNumber="2957">
<heading id="D0D7818F9605FF26FE06FAF9FCD4FADC" box="[484,826,1298,1322]" centered="true" fontSize="10" level="2" pageId="39" pageNumber="2957" reason="2">
<taxonomicName id="4C204D609605FF26FE06FAF9FD3BFADC" authority="Mound &amp; Minaei, 2007" authorityName="Mound &amp; Minaei" authorityYear="2007" box="[484,725,1298,1322]" class="Insecta" family="Phlaeothripidae" genus="Haplothrips" kingdom="Animalia" order="Thysanoptera" pageId="39" pageNumber="2957" phylum="Arthropoda" rank="species" species="howei" status="sp. nov.">
<emphasis id="B954EAF19605FF26FE06FAF9FD3BFADC" bold="true" box="[484,725,1298,1322]" italics="true" pageId="39" pageNumber="2957">Haplothrips howei</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A267578A9605FF26FD3CFAF9FCD4FADC" box="[734,826,1298,1322]" pageId="39" pageNumber="2957" rank="species">sp. nov.</taxonomicNameLabel>
