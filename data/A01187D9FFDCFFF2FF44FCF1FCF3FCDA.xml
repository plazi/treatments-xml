<subSubSection id="60A26544FFDCFFF2FF44FCAAFC4CFBFA" box="[151,937,885,910]" pageId="12" pageNumber="55" type="materials_examined">
<paragraph id="280736CFFFDCFFF2FF44FCAAFC4CFBFA" blockId="12.[151,1437,885,1198]" box="[151,937,885,910]" pageId="12" pageNumber="55">
<emphasis id="1ACCEADDFFDCFFF2FF44FCAAFE67FBFA" bold="true" box="[151,386,885,910]" pageId="12" pageNumber="55">Material examined:</emphasis>
