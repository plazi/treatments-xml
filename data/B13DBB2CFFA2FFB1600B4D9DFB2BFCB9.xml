<figureCitation id="A1AF16BFFFA2FFB060764DBEFE18FD46" box="[373,405,679,705]" captionStart="FIGURES 14 – 19" captionStartId="6.[264,383,1331,1355]" captionTargetBox="[282,1304,569,1298]" captionTargetId="figure@6.[282,1305,569,1304]" captionTargetPageId="6" captionText="FIGURES 14 – 19. Diplonevra spp. Left wings." httpUri="https://zenodo.org/record/169965/files/figure.png" pageId="14" pageNumber="35">19</figureCitation>
, 
<figureCitation id="A1AF16BFFFA2FFB060A24DBEFE4FFD46" box="[417,450,679,705]" captionStart="FIGURES 20 – 25" captionStartId="7.[264,383,1867,1891]" captionTargetBox="[306,1283,727,1834]" captionTargetId="figure@7.[273,1315,711,1840]" captionTargetPageId="7" captionText="FIGURES 20 – 25. Diplonevra spp. Male terminalia, right lateral." httpUri="https://zenodo.org/record/169966/files/figure.png" pageId="14" pageNumber="35">
