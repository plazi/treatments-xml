<quantity id="4C929B44FF955B64FE6EA062FDEEF8A7" box="[486,583,1793,1817]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="15" pageNumber="404" unit="mm" value="2.1">2.10 mm</quantity>
