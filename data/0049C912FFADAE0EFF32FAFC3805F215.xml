<taxonomicName id="4FE00387FFADAE0EFE9AF8D73DB4F434" authorityName="Erichson" authorityYear="1836" box="[314,409,1838,1857]" class="Insecta" family="Curculionidae" genus="Cryphalus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
