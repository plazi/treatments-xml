<taxonomicName id="60894D1EFFF3D612A8B7F885FEFBF889" baseAuthorityName="Hampson" baseAuthorityYear="1896" box="[182,349,1870,1896]" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="288" phylum="Arthropoda" rank="species" species="glaucoptera">
