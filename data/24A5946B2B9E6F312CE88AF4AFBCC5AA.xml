<taxonomicName id="E5DE55A6D69D21FCFD020EB634ABE13C" LSID="MB824077" authority="Sheng H. Wu &amp; C. C. Chen" class="Agaricomycetes" family="FAMILIA" genus="Hydnophanerochaete" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hydnophanerochaete" order="ORDO" pageId="0" pageNumber="75" phylum="Basidiomycota" rank="genus">Hydnophanerochaete Sheng H. Wu &amp; C.C. Chen</taxonomicName>
<taxonomicNameLabel id="60A627F2614ECB8CE3E2CAB8D7BB7D93" pageId="0" pageNumber="75">gen. nov.</taxonomicNameLabel>
