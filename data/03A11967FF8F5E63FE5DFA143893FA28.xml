<caption id="DF77F8F9FF805E63FF00FD71399EFD35" ID-DOI="http://doi.org/10.5281/zenodo.5213593" ID-Zenodo-Dep="5213593" httpUri="https://zenodo.org/record/5213593/files/figure.png" pageId="8" pageNumber="77" startId="8.[138,207,691,710]" targetBox="[184,1138,136,646]" targetPageId="8">
