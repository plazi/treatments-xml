<number id="11753E1AFF80FFD5FA00F8EE1ACCF8E1" box="[1415,1431,1887,1911]" pageId="9" pageNumber="10" value="2.0">2</number>
, tubular; 
<number id="11753E1AFF80FFD5FF72F8321E58F80D" box="[245,259,1923,1947]" pageId="9" pageNumber="10" value="3.0">3</number>
and 
<number id="11753E1AFF80FFD5FEBCF8321E12F80D" box="[315,329,1923,1947]" pageId="9" pageNumber="10" value="4.0">4</number>
of equal length, combined approximately as long as 
<number id="11753E1AFF80FFD5FC27F8321CF5F80D" box="[928,942,1923,1947]" pageId="9" pageNumber="10" value="5.0">5</number>
(
<number id="11753E1AFF80FFD5FC38F8321C96F80D" box="[959,973,1923,1947]" pageId="9" pageNumber="10" value="3.0">3</number>
and 
<number id="11753E1AFF80FFD5FB82F8321B48F80D" box="[1029,1043,1923,1947]" pageId="9" pageNumber="10" value="4.0">4</number>
