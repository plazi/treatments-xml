J 
<number id="11753E1AFF80FFD5FF6DFB4A1E6BFA85" box="[234,304,1275,1299]" pageId="9" pageNumber="10" value="20068.0">20068</number>
</accessionNumber>
(
<number id="11753E1AFF80FFD5FEC5FB4A1E15FA85" box="[322,334,1275,1299]" pageId="9" pageNumber="10" value="1.0">1</number>
Ƥ, 
<number id="11753E1AFF80FFD5FEFFFB4A1EDDFA85" box="[376,390,1275,1299]" pageId="9" pageNumber="10" value="2.0">2</number>
