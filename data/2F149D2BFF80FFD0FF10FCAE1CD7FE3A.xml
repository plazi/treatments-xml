articles 
<number id="11753E1AFF80FFD5FE1FF8EE1EFDF8E1" box="[408,422,1887,1911]" pageId="9" pageNumber="10" value="1.0">1</number>
and 
<number id="11753E1AFF80FFD5FE59F8EE1EB7F8E1" box="[478,492,1887,1911]" pageId="9" pageNumber="10" value="2.0">2</number>
