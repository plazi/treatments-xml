<taxonomicName id="02F54D76EE2FFFA2FD56FEA7FCD4FEA0" box="[710,853,268,293]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="16" pageNumber="17" phylum="Nematoda" rank="species" species="stramenti">
<emphasis id="F781EAE7EE2FFFA2FD56FEA7FCD4FEA0" box="[710,853,268,293]" italics="true" pageId="16" pageNumber="17">T. stramenti</emphasis>
</taxonomicName>
and 
<taxonomicName id="02F54D76EE2FFFA2FC01FEA7FB83FEA0" box="[913,1026,268,293]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="16" pageNumber="17" phylum="Nematoda" rank="species" species="yeatesi" status="sp. nov.">
<emphasis id="F781EAE7EE2FFFA2FC01FEA7FB83FEA0" box="[913,1026,268,293]" italics="true" pageId="16" pageNumber="17">T. yeatesi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="ECB2579CEE2FFFA2FB9AFEA0FBE8FEA0" box="[1034,1129,267,293]" pageId="16" pageNumber="17" rank="species">
<emphasis id="F781EAE7EE2FFFA2FB9AFEA0FBE8FEA0" bold="true" box="[1034,1129,267,293]" pageId="16" pageNumber="17">sp. nov.</emphasis>
</taxonomicNameLabel>
