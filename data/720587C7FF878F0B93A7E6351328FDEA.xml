<taxonomicName id="3DAC4D52FF878F0B91D8E6E412A4FD85" authorityName="Distant" authorityYear="1906" box="[744,945,505,530]" class="Insecta" family="Cicadidae" genus="Odopoea" kingdom="Animalia" order="Hemiptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="venturii">
