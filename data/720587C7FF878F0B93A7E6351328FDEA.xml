<taxonomicName id="3DAC4D52FF878F0B90B5E6C81565FE7A" box="[901,1136,469,494]" class="Insecta" family="Cicadidae" genus="Edholmbergi" kingdom="Animalia" order="Hemiptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="lebruni">
