<taxonomicName id="3DAC4D52FF878F0B91BBE6C8125FFE79" authority="Haupt, 1918" authorityName="Sanborn &amp; Heath" authorityYear="2014" baseAuthorityName="Haupt" baseAuthorityYear="1918" box="[651,842,469,494]" class="Insecta" family="Cicadidae" genus="Adusella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="signata">
