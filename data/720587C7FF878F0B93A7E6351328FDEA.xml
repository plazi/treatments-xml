<taxonomicName id="3DAC4D52FF878F0B979FE6CB143EFE79" authorityName="Distant" authorityYear="1906" box="[1199,1323,470,493]" class="Insecta" family="Cicadidae" genus="Odopoea" kingdom="Animalia" order="Hemiptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="venturii">
<emphasis id="C8D8EAC3FF878F0B979FE6CB143EFE79" box="[1199,1323,470,493]" italics="true" pageId="50" pageNumber="51">O. venturii</emphasis>
</taxonomicName>
