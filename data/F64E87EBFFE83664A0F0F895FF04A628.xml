<quantity id="B91F9B18FFE93663A25AF8D7FD69A32A" box="[573,653,1829,1854]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="96" unit="mm" value="0.2">0.2 mm</quantity>
