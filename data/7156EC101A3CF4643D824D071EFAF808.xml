<taxonomicName id="3EFF26851A3CF46438594BFF1D32FACE" authority="Marsh." authorityName="Marsh." class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="populifolia">
<emphasis id="CB8B81141A3CF46438594BFF1D5EFACE" italics="true" pageId="44" pageNumber="45">Betula populifolia</emphasis>
Marsh.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A3CF4643C6A4A101E42FACE" authorityName="S.F.Gray" authorityYear="1822" box="[383,516,1306,1332]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
