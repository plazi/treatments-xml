<emphasis id="CB8B81141A3CF4643F8D4A111F54FACE" box="[664,786,1307,1332]" italics="true" pageId="44" pageNumber="45">Vaccinium</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A3CF4643E5D4A101FF9FACE" box="[840,959,1306,1332]" class="Magnoliopsida" family="Ericaceae" kingdom="Plantae" order="Ericales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="family">Ericaceae</taxonomicName>
), alfalfa (
<taxonomicName id="3EFF26851A3CF46439384A1118B6FACE" box="[1069,1264,1307,1332]" class="Magnoliopsida" family="Fabaceae" genus="Medicago" kingdom="Plantae" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="sativa">
<emphasis id="CB8B81141A3CF46439384A1118B6FACE" box="[1069,1264,1307,1332]" italics="true" pageId="44" pageNumber="45">Medicago sativa</emphasis>
</taxonomicName>
L., 
<taxonomicName id="3EFF26851A3CF464380B4A1019D6FACE" box="[1310,1424,1306,1332]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
