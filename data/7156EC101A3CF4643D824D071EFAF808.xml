<taxonomicName id="3EFF26851A3CF4643CBC4A4A1E67FAA0" box="[425,545,1344,1370]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
