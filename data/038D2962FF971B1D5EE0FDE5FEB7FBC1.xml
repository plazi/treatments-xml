<quantity id="4CDC3591FF971B1D5A75FC45FB9EFC1B" box="[1049,1108,976,1003]" metricMagnitude="1" metricUnit="m" metricValue="4.70408" pageId="12" pageNumber="49" unit="in" value="1852.0">1852</quantity>
