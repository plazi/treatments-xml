<taxonomicName id="4C24E3F7FF971B1D5F60FC3BFDCEFC37" authority="Grote, 1872" authorityName="Grote" authorityYear="1872" box="[268,516,941,968]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="49" phylum="Arthropoda" rank="species" species="habilis">
