<taxonomicName id="4C24E3F7FF971B1D5D63FC44FE6CFBFE" authority="Guenee, 1852 in McDunnough (1938)" authorityName="Guenee, 1852 in McDunnough" authorityYear="1938" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="49" phylum="Arthropoda" rank="species" species="palaeogama">
