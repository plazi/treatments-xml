<paragraph blockId="6.[140,754,870,897]" box="[140,754,870,897]" pageId="6" pageNumber="7">
<heading box="[140,754,870,897]" fontSize="11" level="2" pageId="6" pageNumber="7" reason="3">
<taxonomicName authority="(Hermann, 1783)" baseAuthorityName="Hermann" baseAuthorityYear="1783" box="[140,754,870,897]" class="Cestoda" family="Anoplocephalidae" genus="Paranoplocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="6" pageNumber="7" phylum="Platyhelminthes" rank="species" species="omphalodes">
<emphasis bold="true" box="[140,754,870,897]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[140,354,870,897]" italics="true" pageId="6" pageNumber="7">Paranoplocephala</emphasis>
cf. 
<emphasis bold="true" box="[396,536,870,897]" italics="true" pageId="6" pageNumber="7">omphalodes</emphasis>
(Hermann, 1783)
</emphasis>
</taxonomicName>
