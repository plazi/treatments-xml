<subSubSection pageId="3" pageNumber="42" type="etymology">
<paragraph blockId="3.[151,1437,151,861]" pageId="3" pageNumber="42">
<emphasis bold="true" box="[199,334,799,825]" pageId="3" pageNumber="42">Etymology.</emphasis>
