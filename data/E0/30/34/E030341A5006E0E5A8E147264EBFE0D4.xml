<paragraph pageId="169" pageNumber="170">
<taxonomicName class="Insecta" family="Carabidae" genus="Scaphinotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="subSpecies" species="behrensi" subSpecies="malkini">Scaphinotus behrensi malkini</taxonomicName>
