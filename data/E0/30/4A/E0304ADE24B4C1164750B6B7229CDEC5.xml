<geoCoordinate direction="north" orientation="latitude" precision="5" value="47.0876">47.0876°N</geoCoordinate>
, 
<geoCoordinate direction="west" orientation="longitude" precision="5" value="-65.8613">65.8613°W</geoCoordinate>
, 11-26.VI.2013, C. Alderson &amp; V. Webster // Old 
<taxonomicName class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pinus banksiana" order="Pinales" pageId="4" pageNumber="343" phylum="Tracheophyta" rank="species" species="banksiana">Pinus banksiana</taxonomicName>
