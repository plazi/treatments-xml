<subSubSection pageId="0" pageNumber="55" type="type species">
<paragraph pageId="0" pageNumber="55">Type species.</paragraph>
<paragraph pageId="0" pageNumber="55">
<taxonomicName authority="Tong &amp; Li, 2021" authorityName="Tong &amp; Li" authorityYear="2021" class="Arachnida" family="Oonopidae" genus="Paramolotra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paramolotra pome" order="Araneae" pageId="0" pageNumber="55" phylum="Arthropoda" rank="species" species="pome">
