<paragraph pageId="null" pageNumber="631">
Blasse 
<taxonomicName class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="631" phylum="Tracheophyta" rank="genus">Orchis</taxonomicName>
</paragraph>
