<geoCoordinate degrees="103" direction="east" minutes="51" orientation="longitude" precision="1" seconds="13.17" value="103.85366">103°51'13.17&quot;E</geoCoordinate>
, 
<geoCoordinate degrees="23" direction="north" minutes="20" orientation="latitude" precision="1" seconds="29.35" value="23.341486">23°20'29.35&quot;N</geoCoordinate>
