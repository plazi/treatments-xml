<taxonomicName authorityName="Morrone" authorityYear="1992" class="Insecta" family="Curculionidae" genus="Lamiarhinus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="32" pageNumber="47" phylum="Arthropoda" rank="species" species="aelficus">
