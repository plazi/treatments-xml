<taxonomicName authority="Balboa &amp; Ayala" authorityName="Ayala" authorityYear="2017" box="[151,579,1868,1895]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="413" phylum="Arthropoda" rank="species" species="nigerrima" status="new species">
