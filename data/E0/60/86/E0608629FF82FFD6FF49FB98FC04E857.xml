<collectingCountry box="[935,1116,1319,1343]" name="Czech Republic" pageId="21" pageNumber="22">Czech Republic</collectingCountry>
, 
<collectingCountry box="[1127,1234,1319,1343]" name="Denmark" pageId="21" pageNumber="22">Denmark</collectingCountry>
, 
<collectingCountry box="[1245,1332,1319,1343]" name="Estonia" pageId="21" pageNumber="22">Estonia</collectingCountry>
, 
<collectingCountry box="[1344,1432,1319,1343]" name="Finland" pageId="21" pageNumber="22">Finland</collectingCountry>
, 
<collectingCountry box="[151,230,1357,1380]" name="France" pageId="21" pageNumber="22">France</collectingCountry>
, 
<collectingCountry box="[241,346,1356,1380]" name="Germany" pageId="21" pageNumber="22">Germany</collectingCountry>
, 
<collectingCountry box="[357,456,1357,1380]" name="Hungary" pageId="21" pageNumber="22">Hungary</collectingCountry>
, 
<collectingCountry box="[467,542,1356,1380]" name="Russia" pageId="21" pageNumber="22">Russia</collectingCountry>
(Leningrad Oblast and Ulyanovsk Oblast), 
<collectingCountry box="[1033,1134,1356,1380]" name="Slovakia" pageId="21" pageNumber="22">Slovakia</collectingCountry>
, 
<collectingCountry box="[1145,1236,1356,1380]" name="Sweden" pageId="21" pageNumber="22">Sweden</collectingCountry>
