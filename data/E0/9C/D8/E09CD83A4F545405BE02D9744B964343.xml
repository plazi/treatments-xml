<taxonomicName class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="598" pageNumber="599" phylum="Arthropoda" rank="species" species="femoralis">
