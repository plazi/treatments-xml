and 
<taxonomicName class="Dicotyledoneae" family="Salicaceae" genus="S." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="S. polaris" order="Salicales" pageId="0" pageNumber="17" phylum="Angiospermae" rank="species" species="polaris">
<emphasis italics="true" pageId="0" pageNumber="17">S. polaris</emphasis>
</taxonomicName>
as the main hosts. 
<taxonomicName authorityName="Bruchet; Bull. mens. Soc. linn. Lyon 39 (6 (Suppl.)): 43" authorityYear="1970" baseAuthorityName="J. Favre" class="Agaricomycetes" family="Hymenogastraceae" genus="Hebeloma" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hebeloma marginatulum" order="Ostropales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="marginatulum">
