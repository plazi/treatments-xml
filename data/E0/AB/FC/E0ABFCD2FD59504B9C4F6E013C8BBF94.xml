<taxonomicName class="Dicotyledoneae" family="Salicaceae" genus="S." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="S. reticulata" order="Salicales" pageId="0" pageNumber="17" phylum="Angiospermae" rank="species" species="reticulata">
<emphasis italics="true" pageId="0" pageNumber="17">S. reticulata</emphasis>
</taxonomicName>
and 
<taxonomicName class="Dicotyledoneae" family="Salicaceae" genus="S." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="S. planifolia" order="Salicales" pageId="0" pageNumber="17" phylum="Angiospermae" rank="species" species="planifolia">
<emphasis italics="true" pageId="0" pageNumber="17">S. planifolia</emphasis>
