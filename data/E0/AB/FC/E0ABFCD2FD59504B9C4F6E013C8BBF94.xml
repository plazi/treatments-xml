<taxonomicName class="Dicotyledoneae" family="Betulaceae" genus="Betula" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Betula" order="Fagales" pageId="0" pageNumber="17" phylum="Angiospermae" rank="genus">
