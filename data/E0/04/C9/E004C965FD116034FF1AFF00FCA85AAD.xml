<collectingCountry box="[359,458,1916,1940]" name="Thailand" pageId="38" pageNumber="39">Thailand</collectingCountry>
(
<collectingRegion box="[473,608,1916,1940]" country="Thailand" name="Chiang Mai" pageId="38" pageNumber="39">Chiang Mai</collectingRegion>
and 
<collectingRegion box="[662,860,1916,1940]" country="Thailand" name="Kamphaeng Phet" pageId="38" pageNumber="39">Kamphaeng Phet</collectingRegion>
