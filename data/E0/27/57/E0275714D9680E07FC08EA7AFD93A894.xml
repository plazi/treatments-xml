<emphasis box="[883,1109,1364,1383]" italics="true" pageId="22" pageNumber="701">Belonuchus rufoniger</emphasis>
Fauvel
</taxonomicName>
; D, 
<taxonomicName authority="(Erichson)" baseAuthorityName="Erichson" baseAuthorityYear="1840" class="Insecta" family="Staphylinidae" genus="Remus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="701" phylum="Arthropoda" rank="species" species="pruinosus">
<emphasis box="[1238,1421,1364,1382]" italics="true" pageId="22" pageNumber="701">Remus pruinosus</emphasis>
(Erichson)
</taxonomicName>
; E, 
<taxonomicName authority="Schillhammer" authorityName="Schillhammer" authorityYear="2004" box="[297,752,1393,1412]" class="Insecta" family="Staphylinidae" genus="Hesperosoma" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="701" phylum="Arthropoda" rank="species" species="flavoterminale">
