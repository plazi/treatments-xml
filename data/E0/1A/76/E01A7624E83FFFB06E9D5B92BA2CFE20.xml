<taxonomicName authority="BORDONI 2009" authorityName="BORDONI" authorityYear="2009" box="[610,909,381,403]" class="Insecta" family="Staphylinidae" genus="Indomorphus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="1764" phylum="Arthropoda" rank="species" species="yunnanus">
