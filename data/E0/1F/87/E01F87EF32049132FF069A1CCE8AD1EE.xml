<paragraph blockId="0.[151,638,1363,1729]" pageId="0" pageNumber="160">
<emphasis bold="true" box="[175,377,1469,1490]" pageId="0" pageNumber="160">World Distribution.</emphasis>
<collectingCountry box="[391,464,1469,1490]" name="Algeria" pageId="0" pageNumber="160">Algeria</collectingCountry>
, 
<collectingCountry box="[475,532,1469,1490]" name="Egypt" pageId="0" pageNumber="160">Egypt</collectingCountry>
(including 
<collectingRegion box="[151,200,1496,1517]" country="Egypt" name="Shamal Sina'" pageId="0" pageNumber="160">Sinai</collectingRegion>
), 
<collectingCountry box="[215,253,1496,1517]" name="Iran" pageId="0" pageNumber="160">Iran</collectingCountry>
