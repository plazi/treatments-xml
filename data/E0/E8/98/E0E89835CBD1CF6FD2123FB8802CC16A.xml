<taxonomicName LSID="urn:lsid:zoobank.org:act:1D28692E-D4C1-454F-A2FA-6AD0AF1AE2FA" class="Arachnida" family="Leptonetidae" genus="Tayshaneta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tayshaneta oconnorae" order="Araneae" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="oconnorae">Tayshaneta oconnorae</taxonomicName>
