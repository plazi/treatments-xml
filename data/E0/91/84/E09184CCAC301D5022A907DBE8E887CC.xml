<subSubSection pageId="0" pageNumber="215" type="nomenclature">
<paragraph pageId="0" pageNumber="215" type="mainText">
<taxonomicName class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="215" phylum="Arthropoda" rank="subSpecies" species="appeninigenus" subSpecies="anoplus">
