: Ramat Magshimim (
<number box="[1338,1427,731,757]" pageId="11" pageNumber="12" value="762905.0">762905</number>
/ 
<number box="[151,243,771,797]" pageId="11" pageNumber="12" value="634421.0">634421</number>
), 
<taxonomicName box="[263,350,771,797]" class="Arachnida" family="Cybaeidae" genus="Paracedicus" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="15" phylum="Arthropoda" rank="species" species="geshur" status="n. sp.">Geshur</taxonomicName>
.
</paragraph>
</subSubSection>
