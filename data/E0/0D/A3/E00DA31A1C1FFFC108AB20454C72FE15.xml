<typeStatus box="[385,489,1772,1798]" pageId="26" pageNumber="27">holotype</typeStatus>
+ 
<number box="[522,537,1772,1798]" pageId="26" pageNumber="27" value="1.0">1</number>
ɗ; 
<typeStatus box="[572,675,1772,1798]" pageId="26" pageNumber="27">holotype</typeStatus>
listed first): total length 
<number box="[968,1008,1772,1798]" pageId="26" pageNumber="27" value="6.4">6.4</number>
, 
<number box="[1021,1061,1772,1798]" pageId="26" pageNumber="27" value="5.8">5.8</number>
; carapace length 
<number box="[1265,1304,1772,1798]" pageId="26" pageNumber="27" value="2.8">2.8</number>
, 
<number box="[1317,1357,1772,1798]" pageId="26" pageNumber="27" value="2.9">2.9</number>
, width 
<number box="[151,191,1812,1838]" pageId="26" pageNumber="27" value="2.2">2.2</number>
, 2.0, index 
