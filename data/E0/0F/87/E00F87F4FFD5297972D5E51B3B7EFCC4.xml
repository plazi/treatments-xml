<emphasis bold="true" box="[136,231,611,635]" italics="true" pageId="2" pageNumber="263">cubensis</emphasis>
Saussure, 1869
</taxonomicName>
[
<taxonomicName authorityName="de Saussure" authorityYear="1869" box="[414,519,612,635]" class="Insecta" family="Liturgusidae" genus="Gonatista" kingdom="Animalia" order="Mantodea" pageId="2" pageNumber="263" phylum="Arthropoda" rank="genus">
