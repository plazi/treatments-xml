: 
<materialsCitation pageId="3" pageNumber="264">
&quot; 
<specimenCount box="[912,934,524,550]" pageId="3" pageNumber="264" type="female">♀</specimenCount>
<collectingCountry box="[947,1023,525,549]" name="Sri Lanka" pageId="3" pageNumber="264">Ceylan</collectingCountry>
<superScript attach="left" box="[1034,1073,523,549]" fontSize="8" pageId="3" pageNumber="264">Vge</superScript>
Humbert&quot; and &quot; 
<taxonomicName box="[396,482,558,581]" class="Entomophthoromycetes" family="Entomophthoraceae" genus="Empusa" kingdom="Fungi" order="Entomophthorales" pageId="3" pageNumber="264" phylum="Entomophthoromycota" rank="genus">Empusa</taxonomicName>
