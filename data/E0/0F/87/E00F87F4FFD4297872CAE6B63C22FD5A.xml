Trincomalie 
<collectingCountry box="[370,446,589,613]" name="Sri Lanka" pageId="3" pageNumber="264">Ceylan</collectingCountry>
<superScript attach="left" box="[459,499,587,613]" fontSize="8" pageId="3" pageNumber="264">Vge</superScript>
Humbert&quot; and &quot; 
<taxonomicName box="[696,782,590,613]" class="Entomophthoromycetes" family="Entomophthoraceae" genus="Empusa" kingdom="Fungi" order="Entomophthorales" pageId="3" pageNumber="264" phylum="Entomophthoromycota" rank="genus">Empusa</taxonomicName>
