<emphasis box="[751,993,1374,1400]" italics="true" pageId="132" pageNumber="133">
<taxonomicName authorityName="Yosii" authorityYear="1966" box="[751,896,1374,1400]" class="Collembola" family="Dicyrtomidae" genus="Calvatomina" kingdom="Animalia" order="Symphypleona" pageId="132" pageNumber="133" phylum="Arthropoda" rank="genus">Calvatomina</taxonomicName>
