<taxonomicNameLabel box="[423,483,1094,1115]" pageId="3" pageNumber="249" rank="species">sp. n.</taxonomicNameLabel>
;
</emphasis>
B, 
<taxonomicName box="[525,737,1093,1115]" class="Arachnida" family="Eukoeneniidae" genus="Eukoenenia" kingdom="Animalia" order="Palpigradi" pageId="3" pageNumber="249" phylum="Arthropoda" rank="species" species="cavatica" status="sp. n.">
