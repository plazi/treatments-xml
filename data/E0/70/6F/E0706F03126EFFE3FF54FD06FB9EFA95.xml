<taxonomicName authority="(Monterosato, 1879)" authorityName="Monterosato" authorityYear="1879" baseAuthorityName="Monterosato" baseAuthorityYear="1879" box="[382,882,759,784]" class="Polyplacophora" family="Leptochitonidae" genus="Leptochiton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pholadomyoida" pageId="20" pageNumber="21" phylum="Mollusca" rank="species" species="cimicoides">
