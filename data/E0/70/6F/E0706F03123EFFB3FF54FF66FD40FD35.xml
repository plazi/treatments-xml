</emphasis>
[
<taxonomicName authority="Monterosato, 1884" authorityName="Monterosato" authorityYear="1884" box="[435,905,439,464]" class="Gastropoda" family="Pyramidellidae" genus="Trabecula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="68" pageNumber="69" phylum="Mollusca" rank="species" species="jeffreysiana">
