<taxonomicName baseAuthorityName="da Costa" baseAuthorityYear="1778" box="[588,815,583,608]" class="Gastropoda" family="Phasianellidae" genus="Tricolia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeogastropoda" pageId="23" pageNumber="24" phylum="Mollusca" rank="subSpecies" species="pullus" subSpecies="picta">
