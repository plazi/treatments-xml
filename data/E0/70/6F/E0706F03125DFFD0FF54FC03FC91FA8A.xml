<taxonomicName baseAuthorityName="Donovan" baseAuthorityYear="1804" box="[920,1112,1300,1323]" class="Gastropoda" family="Naticidae" genus="Euspira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="intricata">
