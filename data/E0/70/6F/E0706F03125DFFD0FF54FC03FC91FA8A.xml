<taxonomicName authority="(Donovan, 1804)" authorityName="Donovan" authorityYear="1804" baseAuthorityName="Donovan" baseAuthorityYear="1804" box="[422,815,1335,1359]" class="Gastropoda" family="Naticidae" genus="Euspira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="intricata">
