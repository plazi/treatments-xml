<location LSID="urn:lsid:plazi:treatment:E0706F03124AFFC6FF54F9C5FE96FEA0:6D0688CE124AFFC7FE31F8DEFDEAF88D" box="[498,591,1839,1864]" municipality="Described" name="Palermo" pageId="48" pageNumber="49" stateProvince="Sicily">Palermo</location>
