<taxonomicName authority="Monterosato" authorityName="Monterosato" authorityYear="1880" box="[932,1315,943,968]" class="Gastropoda" family="Buccinidae" genus="Epidromus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="63" pageNumber="64" phylum="Mollusca" rank="species" species="gladiolus">
