<taxonomicName authority="(Lamarck, 1822)" authorityName="Lamarck" authorityYear="1822" baseAuthorityName="Lamarck" baseAuthorityYear="1822" class="Gastropoda" family="Muricidae" genus="Aspella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="63" pageNumber="64" phylum="Mollusca" rank="species" species="anceps">
