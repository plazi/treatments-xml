<taxonomicName baseAuthorityName="Lamarck" baseAuthorityYear="1822" box="[385,498,980,1004]" class="Gastropoda" family="Muricidae" genus="Aspella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="63" pageNumber="64" phylum="Mollusca" rank="species" species="anceps">
