<taxonomicName authority="(Monterosato, 1869)" authorityName="Monterosato" authorityYear="1869" baseAuthorityName="Monterosato" baseAuthorityYear="1869" box="[181,670,223,248]" class="Gastropoda" family="Tonnidae" genus="Eudolium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="47" pageNumber="48" phylum="Mollusca" rank="species" species="crosseanum">
