<taxonomicName authority="(Monterosato, 1875)" authorityName="Monterosato" authorityYear="1875" baseAuthorityName="Monterosato" baseAuthorityYear="1875" box="[428,845,652,677]" class="Gastropoda" family="Peraclidae" genus="Peracle" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thecosomata" pageId="74" pageNumber="75" phylum="Mollusca" rank="species" species="diversa">
