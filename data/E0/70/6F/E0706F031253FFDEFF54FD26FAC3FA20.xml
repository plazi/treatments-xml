(
<bibRefCitation box="[999,1164,1447,1472]" pageId="41" pageNumber="42" refId="ref87628" refString="Philippi, R. A. (1844) Enumeratio molluscorum Siciliae cum viventium tum in tellure tertiaria fossilium, quae in itinere suo observavit. Volumen secundum continens addenda et emendanda, nec non comparationem faunae recentis Siciliae cum faunis aliarum terrarum et com fauna periodi tertiariae. Eduard Anton, Halle [Halis Saxorum], iv + 303 pp., pls. XIII - XXVIII." type="book">Philippi, 1844</bibRefCitation>
)
</taxonomicName>
and compared it to 
<taxonomicName authority="(P. Fischer, 1788)" authorityName="P. Fischer" authorityYear="1788" baseAuthorityName="P. Fischer" baseAuthorityYear="1788" class="Gastropoda" family="Cerithiidae" genus="Cerithium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neotaenioglossa" pageId="41" pageNumber="42" phylum="Mollusca" rank="species" species="monterosatoi">
