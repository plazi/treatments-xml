<quantity box="[1238,1372,1621,1645]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" metricValueMax="1.3" metricValueMin="1.2" pageId="50" pageNumber="51" unit="mm" value="1.25" valueMax="1.3" valueMin="1.2">1.2–1.3mm</quantity>
