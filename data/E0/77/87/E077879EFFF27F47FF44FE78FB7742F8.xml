), 
<quantity box="[484,611,649,673]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" metricValueMax="2.2" metricValueMin="1.6" pageId="100" pageNumber="101" unit="mm" value="1.9" valueMax="2.2" valueMin="1.6">1.6–2.2mm</quantity>
