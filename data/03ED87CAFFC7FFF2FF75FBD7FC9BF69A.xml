<caption id="DF3B6654FFC6FFF2FF75FB5FFC2EF53A" httpUri="https://zenodo.org/record/243842/files/figure.png" pageId="3" pageNumber="498" targetBox="[151,1436,193,1244]" targetPageId="3">
<paragraph id="8BFB36DCFFC6FFF2FF75FB5FFC2EF53A" blockId="3.[151,1436,1266,1319]" pageId="3" pageNumber="498">
<emphasis id="B930EACEFFC6FFF2FF75FB5FFECAF515" bold="true" box="[151,317,1266,1289]" pageId="3" pageNumber="498">FIGURES 5–8.</emphasis>
<taxonomicName id="4C444D5FFFC6FFF2FEA4FB5EFDF3F515" box="[326,516,1267,1288]" class="Insecta" family="Ichneumonidae" genus="Prochas" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="498" phylum="Arthropoda" rank="species" species="shimborii" status="sp. nov.">
<emphasis id="B930EACEFFC6FFF2FEA4FB5EFDF3F515" box="[326,516,1267,1288]" italics="true" pageId="3" pageNumber="498">Prochas shimborii</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20357B5FFC6FFF2FDEFFB5EFD96F515" box="[525,609,1267,1288]" pageId="3" pageNumber="498" rank="species">
<emphasis id="B930EACEFFC6FFF2FDEFFB5EFD96F515" bold="true" box="[525,609,1267,1288]" pageId="3" pageNumber="498">sp. nov.</emphasis>
</taxonomicNameLabel>
: 
<emphasis id="B930EACEFFC6FFF2FD90FB5EFD89F515" bold="true" box="[626,638,1267,1288]" pageId="3" pageNumber="498">5</emphasis>
, ♀ holotype habitus; 
<emphasis id="B930EACEFFC6FFF2FC84FB5EFC85F515" bold="true" box="[870,882,1267,1288]" pageId="3" pageNumber="498">6</emphasis>
, ♀ holotype head; 
<emphasis id="B930EACEFFC6FFF2FBA2FB5EFBBAF515" bold="true" box="[1088,1101,1267,1288]" pageId="3" pageNumber="498">7</emphasis>
, ♀ paratype mesopleuron; 
