<typeStatus id="872E6382FFD52D6C9C90D199FDABF939" box="[151,255,1730,1756]" pageId="2" pageNumber="3">holotype</typeStatus>
and a 
<typeStatus id="872E6382FFD52D6C9D48D199FCE1F939" box="[335,437,1730,1756]" pageId="2" pageNumber="3">paratype</typeStatus>
