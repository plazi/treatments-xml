<collectionCode id="3E8445E5FFD52D6C9C90D1C6FD83F952" box="[151,215,1693,1719]" httpUri="http://grbio.org/cool/h8hj-4rxx" name="National Museum of Scotland - Natural Sciences" pageId="2" pageNumber="3">NMS</collectionCode>
