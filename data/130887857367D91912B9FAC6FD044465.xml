<emphasis id="A9D5EA817365D91F12B9F95BFEFA4583" bold="true" box="[151,273,1739,1763]" pageId="3" pageNumber="597">FIGURE 2.</emphasis>
<taxonomicName id="5CA14D107365D91F1338F95BFE104583" authorityName="He, Schillhammer &amp; Li" authorityYear="2021" box="[278,507,1739,1763]" class="Insecta" family="Staphylinidae" genus="Eucibdelus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="597" phylum="Arthropoda" rank="species" species="flavipennis">
<emphasis id="A9D5EA817365D91F1338F95BFE104583" box="[278,507,1739,1763]" italics="true" pageId="3" pageNumber="597">Eucibdelus flavipennis</emphasis>
</taxonomicName>
, morphology; 
