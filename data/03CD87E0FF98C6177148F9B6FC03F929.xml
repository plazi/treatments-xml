<taxonomicName id="4C644D75FF98C617713CF999FC75F9A8" authorityName="Forel" authorityYear="1892" box="[843,969,1638,1663]" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="248" phylum="Arthropoda" rank="species" species="smithii">
