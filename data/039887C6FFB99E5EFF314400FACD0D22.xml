<paragraph id="8B8E36D0FFB99E5EFF31444EFD9209E5" blockId="83.[151,623,618,641]" box="[151,623,618,641]" pageId="83" pageNumber="567">
<treatmentCitationGroup id="AB2111FEFFB99E5EFF31444EFD9209E5" box="[151,623,618,641]" pageId="83" pageNumber="567">
<taxonomicName id="4C314D53FFB99E5EFF31444EFF1D09E5" box="[151,224,618,641]" class="Polychaeta" family="Terebellidae" genus="Lanice" kingdom="Animalia" order="Terebellida" pageId="83" pageNumber="567" phylum="Annelida" rank="genus">
<emphasis id="B945EAC2FFB99E5EFF31444EFF1D09E5" box="[151,224,618,641]" italics="true" pageId="83" pageNumber="567">Lanice</emphasis>
</taxonomicName>
.— 
<treatmentCitation id="0A9010C1FFB99E5EFF59444EFDBC09E5" author="Hutchings" box="[255,577,618,641]" page="17" pageId="83" pageNumber="567" year="1988">
<bibRefCitation id="EFA04B21FFB99E5EFF59444EFDE609E5" author="Hutchings" box="[255,539,618,641]" pageId="83" pageNumber="567" refString="Hutchings, P. A. &amp; Glasby, C. J. (1988) The Amphitritinae (Polychaeta: Terebellidae) from Australia. Records of the Australian Museum, 40 (1), 1 - 60. http: // dx. doi. org / 10.3853 / j. 0067 - 1975.40.1988.150" type="journal article" year="1988">
Hutchings &amp; Glasby 
<number id="3DF924F7FFB99E5EFE44444EFDE609E5" box="[482,539,618,641]" pageId="83" pageNumber="567" value="1988.0">1988</number>
</bibRefCitation>
: 
<number id="3DF924F7FFB99E5EFD80444EFDBC09E5" box="[550,577,618,641]" pageId="83" pageNumber="567" value="17.5" valueMax="18.0" valueMin="17.0">17</number>
</treatmentCitation>
