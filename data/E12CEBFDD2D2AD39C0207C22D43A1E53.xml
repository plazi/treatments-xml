<taxonomicName id="F079CF0ED2F787F341D45EBE24928756" class="Gastropoda" family="Polyceridae" genus="Roboastra" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Roboastra luteolineata" order="Nudibranchia" pageId="4" pageNumber="5" phylum="Mollusca" rank="species" species="luteolineata">Roboastra luteolineata</taxonomicName>
