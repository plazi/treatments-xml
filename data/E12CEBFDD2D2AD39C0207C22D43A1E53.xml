<taxonomicName id="0197FF3995AB05A7C1FF3626FC0361A6" class="Gastropoda" family="Goniodorididae" genus="Trapania" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trapania miltabrancha" order="Nudibranchia" pageId="4" pageNumber="5" phylum="Mollusca" rank="species" species="miltabrancha">Trapania miltabrancha</taxonomicName>
