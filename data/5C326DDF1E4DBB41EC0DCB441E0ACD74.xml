<normalizedToken id="1D72CF0094AE090A73D162ACDCF7DEBE" originalValue="Rundblättriges" pageId="null" pageNumber="896">Rundblaettriges</normalizedToken>
<normalizedToken id="454F865534BF7C5044797B17CDDE05B1" originalValue="Wintergrün" pageId="null" pageNumber="896">Wintergruen</normalizedToken>
</paragraph>
