<paragraph id="8B84984CEE7CFF9864ABFA57FA71FA28" blockId="3.[151,1436,1156,1501]" pageId="3" pageNumber="328">
<keyLead id="30CA3B79EE7CFF9864ABFA57FA71FA28" pageId="3" pageNumber="328">
<number id="3DF38A6BEE7CFF9864ABFA57FF48FA4A" box="[151,165,1449,1471]" pageId="3" pageNumber="328" value="2.0">2</number>
