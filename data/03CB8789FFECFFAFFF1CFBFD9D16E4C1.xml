<taxonomicName id="4C624D1CFFECFFAFFF46FA7398A5E4C1" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[205,428,1411,1436]" class="Insecta" family="Tachinidae" genus="Spallanzania" kingdom="Animalia" order="Diptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="picea">
