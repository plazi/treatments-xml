<taxonomicName id="4C624D1CFFECFFAFFD29FAAD9A2EE42A" baseAuthorityName="Herting &amp; Dely-Draskovits" baseAuthorityYear="1993" box="[674,807,1373,1399]" class="Insecta" family="Tachinidae" kingdom="Animalia" order="Diptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="family">Tachinidae</taxonomicName>
