<taxonomicName id="4C624D1CFFECFFAFFC4BFAAD9DBBE42B" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[960,1202,1373,1398]" class="Magnoliopsida" family="Tamaricaceae" genus="Reaumuria" kingdom="Plantae" order="Caryophyllales" pageId="93" pageNumber="94" phylum="Tracheophyta" rank="species" species="blondeli">
