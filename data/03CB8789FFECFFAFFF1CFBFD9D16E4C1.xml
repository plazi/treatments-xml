<taxonomicName id="4C624D1CFFECFFAFFE56FA739B64E4C1" baseAuthorityName="Robineau-Desvoidy" baseAuthorityYear="1830" box="[477,621,1411,1436]" class="Insecta" family="Tachinidae" genus="Gonia" kingdom="Animalia" order="Diptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="picea">
