<taxonomicName id="8D974D11FFC7FFB75D619E0C291E4E1E" authority=", Baker, Bay, Dixie, Duval, Escambia, Flagler, Franklin, Hillsborough, Jefferson, Lake, Levy, Marion, St. Johns, Volusia, and Walton" authorityName="Baker, Bay, Dixie, Duval, Escambia, Flagler, Franklin, Hillsborough, Jefferson, Lake, Levy, Marion, St. Johns, Volusia, and Walton" class="Insecta" family="Eulophidae" genus="Alachua" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="21" phylum="Arthropoda" rank="genus">Alachua, Baker, Bay, Dixie, Duval, Escambia, Flagler, Franklin, Hillsborough, Jefferson, Lake, Levy, Marion, St. Johns, Volusia, and Walton</taxonomicName>
