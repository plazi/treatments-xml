: 
<taxonomicName id="8D974D11FFC4FFB45D0498622979481B" authority="County" authorityName="County" box="[611,803,1866,1892]" class="Insecta" family="Eulophidae" genus="Alachua" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="20" phylum="Arthropoda" rank="genus">
