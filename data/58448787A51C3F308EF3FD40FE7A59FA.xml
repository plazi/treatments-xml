<typeStatus id="0F568833A51C3F308EF3FCCAFF2D5D08" box="[151,201,863,884]" pageId="4" pageNumber="5">Type</typeStatus>
genus: 
<taxonomicName id="17ED4D12A51C3F308F7CFCCBFE0F5D08" authority="Haller, 1878" authorityName="Haller" authorityYear="1878" box="[280,491,862,884]" class="Arachnida" family="Syringophilidae" genus="Picobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
