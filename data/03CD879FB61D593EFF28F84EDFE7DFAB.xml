<subSubSection id="C37E6502B617593EFF78FD38DBD6DF7E" pageId="25" pageNumber="276" type="etymology">
<paragraph id="8BDB3689B617593EFF78FD38DBD6DF7E" blockId="25.[151,1437,151,1005]" pageId="25" pageNumber="276">
<emphasis id="B910EA9BB617593EFF78FD38DA71DF53" bold="true" box="[199,334,764,789]" pageId="25" pageNumber="276">Etymology.</emphasis>
