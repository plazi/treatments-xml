<taxonomicName id="F82D1C48E1050DA3E1D296195703A827" class="Insecta" family="Tettigoniidae" genus="Thyridorhoptrum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thyridorhoptrum carbonarium" order="Orthoptera" pageId="2" pageNumber="19" phylum="Arthropoda" rank="species" species="carbonarium">Thyridorhoptrum carbonarium</taxonomicName>
