<taxonomicName id="0B2EAB4960BE61E9F783DAF0670F9F44" class="Insecta" family="Tettigoniidae" genus="Thyridorhoptrum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thyridorhoptrum senegalense" order="Orthoptera" pageId="2" pageNumber="19" phylum="Arthropoda" rank="species" species="senegalense">Thyridorhoptrum senegalense</taxonomicName>
and 
<taxonomicName id="339F8CD645B72443DDB8784A02063DE2" class="Insecta" family="Tettigoniidae" genus="Thyridorhoptrum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thyridorhoptrum baileyi" order="Orthoptera" pageId="2" pageNumber="19" phylum="Arthropoda" rank="species" species="baileyi">Thyridorhoptrum baileyi</taxonomicName>
