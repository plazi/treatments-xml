, 
<taxonomicName id="EFF01C4ADC0A1FDC20F11E5084AA3184" baseAuthorityName="L. Pfeiffer" baseAuthorityYear="1842" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="25" pageNumber="26" phylum="Mollusca" rank="species" species="livescens" subGenus="Mesembrinus">
