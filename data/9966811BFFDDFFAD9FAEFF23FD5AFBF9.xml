<taxonomicName id="D6CF4B8EFFDDFFAD9EE1FC6CFDBDFC05" authorityName="Ito" authorityYear="2020" box="[472,548,982,1007]" class="Insecta" family="Hydroptilidae" genus="Stactobiella" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="567" phylum="Arthropoda" rank="species" species="amami">
