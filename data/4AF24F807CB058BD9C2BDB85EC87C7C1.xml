<taxonomicName id="F87D0647B724A85B084D13F2C13F8D06" authority="Cumming et al., 2021" authorityName="gen. et" authorityYear="2021" class="Insecta" family="Phylliidae" genus="Cryptophyllium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptophyllium khmer" order="Phasmatodea" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="khmer">
