<taxonomicName id="132063B5A34ADAA69F0CE9B883F9F2BE" authority="Cumming et al., 2021" authorityName="Cumming &amp; Bank &amp; Bresseel &amp; Constant &amp; Tirant &amp; Dong &amp; Sonet &amp; Bradler" authorityYear="2021" baseAuthorityName="Wood-Mason" baseAuthorityYear="1875" class="Insecta" family="Phylliidae" genus="Cryptophyllium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptophyllium westwoodii" order="Phasmatodea" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="westwoodii">
