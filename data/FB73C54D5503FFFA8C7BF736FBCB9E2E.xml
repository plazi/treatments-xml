<collectingRegion id="B11EBAB95503FFFA8C36F05CFE329C63" box="[339,476,1605,1626]" country="Costa Rica" name="Guanacaste" pageId="43" pageNumber="265">Guanacaste</collectingRegion>
, 
<collectingCountry id="0BCD34CB5503FFFA8C8FF05CFD859C62" box="[490,619,1605,1627]" name="Costa Rica" pageId="43" pageNumber="265">Costa Rica</collectingCountry>
