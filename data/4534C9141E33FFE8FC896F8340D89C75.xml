<quantity id="0A65D5E71E30FFE8FF20693A45729B12" box="[152,258,786,813]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" metricValueMax="1.4" metricValueMin="1.1" pageId="7" pageNumber="966" unit="mm" value="12.5" valueMax="14.0" valueMin="11.0">11-14 µm</quantity>
(mean 
<quantity id="0A65D5E71E30FFE8FEE9693A45EA9B12" box="[337,410,786,813]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" pageId="7" pageNumber="966" unit="mm" value="12.0">12 µm</quantity>
