<quantity id="0A65D5E71E30FFE8FF4268DA45F29B32" box="[250,386,754,781]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" metricValueMax="1.41" metricValueMin="1.13" pageId="7" pageNumber="966" unit="mm" value="127.0" valueMax="141.0" valueMin="113.0">113-141 µm</quantity>
