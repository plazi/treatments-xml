</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="4C72ED9CFFADE16AFE4FF9C8BE68F95B" authority="Parisi, 1916" authorityName="Parisi" authorityYear="1916" box="[400,819,1662,1688]" class="Malacostraca" family="Panopeidae" genus="Lophoxanthus" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="51" phylum="Arthropoda" rank="species" species="erosus">
