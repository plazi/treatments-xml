<typeStatus id="54C928BDFFADE16AFE3FF912BF4EF97C" box="[480,533,1701,1727]" pageId="7" pageNumber="51">type</typeStatus>
material of 
<taxonomicName id="4C72ED9CFFADE16AFD73F912BEEDF97D" box="[684,950,1701,1726]" class="Malacostraca" family="Xanthidae" genus="Leptodius" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="51" phylum="Arthropoda" rank="species" species="granulosus">
