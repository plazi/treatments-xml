<taxonomicName id="4C72ED9CFFADE16AFD7AF9EFB910F9B1" authority="Miers, 1884" authorityName="Miers" authorityYear="1884" box="[677,1099,1624,1650]" class="Malacostraca" family="Xanthidae" genus="Xantho" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="51" phylum="Arthropoda" rank="species" species="macgillivrayi">
<emphasis id="B9064A0DFFADE16AFD7AF9EFBEF1F9B2" box="[677,938,1624,1649]" italics="true" pageId="7" pageNumber="51">Xantho macgillivrayi</emphasis>
<bibRefCitation id="EFE3EBEEFFADE16AFC6CF9EFB910F9B1" author="Miers" box="[947,1099,1624,1650]" pageId="7" pageNumber="54" refString="Miers, E. J. (1884) Crustacea. In: Report on the zoological collections made in the Indo-Pacific Ocean during the voyage of H. M. S. Alert 1881 - 1882. London. 178 - 322, pls. 18 - 34." type="book chapter" year="1884">
Miers, 
<number id="3DBA8438FFADE16AFBD4F9EFB910F9B1" box="[1035,1099,1624,1650]" pageId="7" pageNumber="51" value="1884.0">1884</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C72ED9CFFADE16AFB86F9EFBC15F95B" authority="Rathbun, 1898" authorityName="Rathbun" authorityYear="1898" class="Malacostraca" family="Panopeidae" genus="Lophopanopeus" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="51" phylum="Arthropoda" rank="species" species="japonicus">
