<taxonomicName id="283128A6D5DB75CB8A28DE57903B75F7" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="53" pageNumber="54" phylum="Angiospermae" rank="species" species="shawii">
