<typeStatus id="54A353CEFFD3FFCBFF44D5F2FF6F3F20" box="[151,201,509,530]" pageId="1" pageNumber="136">Type</typeStatus>
species: 
<taxonomicName id="4C1896EFFFD3FFCBFEF5D5F3FDCE3F20" authority="Kieffer, 1919" authorityName="Kieffer" authorityYear="1919" box="[294,616,508,530]" class="Insecta" family="Chironomidae" genus="Prodiamesa" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="136" phylum="Arthropoda" rank="species" species="fulva">
<emphasis id="B96C317EFFD3FFCBFEF5D5F3FE7E3F20" box="[294,472,508,530]" italics="true" pageId="1" pageNumber="136">Prodiamesa fulva</emphasis>
<bibRefCitation id="EF89909DFFD3FFCBFE0DD5F3FDCE3F20" author="Kieffer" box="[478,616,508,530]" pageId="1" pageNumber="136" refString="Kieffer, J. J. (1919) Chironomidae d'Europe conserves au Musee National Hongrois de Budapest. Annales Historico - Naturales Musei Nationalis Hungarici, 17, 1 - 160." type="journal article" year="1919">
