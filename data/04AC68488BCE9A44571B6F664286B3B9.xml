<paragraph id="5D076F8916ECE2B2789C3898CD2263F6" pageNumber="19">
<taxonomicName id="9DDED0FB278355BD6549D3A88E9E0AA9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:148985" class="Insecta" family="Formicidae" genus="Centromyrmex" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Centromyrmex sellaris longiventris Santschi" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="variety" species="sellaris" variety="longiventris">Centromyrmex sellaris var. longiventris Santschi</taxonomicName>
