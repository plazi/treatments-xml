<quantity id="CC5F6281FFCEFF82FD784A0C6916FE8A" box="[653,776,332,357]" metricMagnitude="-2" metricUnit="m" metricValue="1.65" metricValueMax="1.7" metricValueMin="1.6" pageId="7" pageNumber="362" unit="mm" value="16.5" valueMax="17.0" valueMin="16.0">16–17 mm</quantity>
, n= 3; male forewing: 
<quantity id="CC5F6281FFCEFF82FBF34A0C6EB4FE8A" box="[1030,1194,332,357]" metricMagnitude="-2" metricUnit="m" metricValue="1.0875" metricValueMax="1.19" metricValueMin="0.985" pageId="7" pageNumber="362" unit="mm" value="10.875" valueMax="11.9" valueMin="9.85">9.85–11.9 mm</quantity>
