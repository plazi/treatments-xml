stratum and 
<typeStatus id="E80AF987A955FFB02AA8C78EFE7FF93E" box="[419,470,1731,1757]" pageId="85" pageNumber="86">type</typeStatus>
locality.
</emphasis>
<location id="326E11FEA955FFB0294BC78EFDC5F93E" LSID="urn:lsid:plazi:treatment:BF18F633A955FFB32B9CC58BFDF8FBF0:326E11FEA955FFB0294BC78EFDC5F93E" box="[576,620,1731,1757]" country="Sweden" county="Limhamn" municipality="Danian Faxekalk" name="The" pageId="85" pageNumber="86">The</location>
middle 
<collectingMunicipality id="D76ADD5FA955FFB029CCC78EFC2FF93E" box="[711,902,1731,1757]" pageId="85" pageNumber="86">Danian Faxekalk</collectingMunicipality>
at 
<collectingCounty id="DE6F3FA9A955FFB028A3C78EFBB1F93E" box="[936,1048,1731,1757]" pageId="85" pageNumber="86">Limhamn</collectingCounty>
, southern 
<collectingCountry id="4FA607B5A955FFB02F80C78EFB4EF93E" box="[1163,1255,1731,1757]" name="Sweden" pageId="85" pageNumber="86">Sweden</collectingCountry>
