<taxonomicName id="A695A45415E11C4170FF213AE0B59A64" authorityName="Ashmead" authorityYear="1900" class="Insecta" family="Braconidae" genus="Glyptomorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glyptomorpha thoracica" order="Hymenoptera" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="thoracica">
