<taxonomicName id="4C784D597A48FFCEFE54FCACFF0CFC0D" authorityName="Waldock" authorityYear="1995" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="mungaich">
