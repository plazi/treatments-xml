<taxonomicName id="4C10ECE2FFDBFFABFE85FC66FDF4FC7D" authority="Westwood, 1842" authorityName="Westwood" authorityYear="1842" box="[283,608,998,1023]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="1" pageNumber="298" phylum="Arthropoda" rank="genus">
