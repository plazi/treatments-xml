<taxonomicName id="209C4D7932071B61A8B45CA6FBEEE1FC" box="[956,1129,1500,1522]" class="Liliopsida" family="Arecaceae" genus="Eremospatha" kingdom="Plantae" order="Arecales" pageId="41" pageNumber="42" phylum="Tracheophyta" rank="species" species="undetermined">
