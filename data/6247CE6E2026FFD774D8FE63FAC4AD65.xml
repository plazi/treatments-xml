<number id="5C266D5F2026FFD774ACFAA8FEFEAFE8" box="[227,281,1389,1415]" pageId="9" pageNumber="31" value="0.43">0.43</number>
, 
<number id="5C266D5F2026FFD7756AFAA8FEBCAFE8" box="[293,347,1389,1415]" pageId="9" pageNumber="31" value="0.6">0.60</number>
, 
<number id="5C266D5F2026FFD77527FAA8FE79AFE8" box="[360,414,1389,1415]" pageId="9" pageNumber="31" value="0.57">0.57</number>
