<emphasis id="B934EAF6FFB59D4AFF076647FEE4FA92" bold="true" box="[199,352,1326,1351]" pageId="1" pageNumber="346">
<typeStatus id="54FB8846FFB59D4AFF076647FE86FA92" box="[199,258,1326,1351]" pageId="1" pageNumber="346">Type</typeStatus>
locality
</emphasis>
. 
<collectingCountry id="F3577674FFB59D4AFEB16647FE29FA92" box="[369,429,1326,1351]" name="India" pageId="1" pageNumber="346">India</collectingCountry>
, 
<collectingRegion id="4984F806FFB59D4AFE7B6647FDF5FA92" box="[443,625,1326,1351]" country="India" name="Andhra Pradesh" pageId="1" pageNumber="346">Andhra Pradesh</collectingRegion>
