<location id="8E9F603FFFB59D4AFAE86647FF57FABE" LSID="urn:lsid:plazi:treatment:03E987F2FFB59D42FF576085FAF6FCC1:8E9F603FFFB59D4AFAE86647FF57FABE" country="India" county="State" municipality="Borra Caves in Visakhapatnam District" name="Karaiguda Cave" pageId="1" pageNumber="346" stateProvince="Andhra Pradesh">Karaiguda Cave</location>
, 18°18'.33.3''N, 0.83°01'31.9''E, 
<elevation id="006DD1D7FFB59D4AFD82663BFD73FABF" box="[578,759,1362,1387]" metricMagnitude="2" metricUnit="m" metricValue="8.379999999999999" pageId="1" pageNumber="346" unit="m" value="838.0">
elevation 
<quantity id="4CB89B01FFB59D4AFD70663AFD73FABF" box="[688,759,1363,1387]" metricMagnitude="2" metricUnit="m" metricValue="8.379999999999999" pageId="1" pageNumber="346" unit="m" value="838.0">838 m</quantity>
</elevation>
(
<figureCitation id="137B2A61FFB59D4AFCC7663BFCC0FABE" box="[775,836,1362,1387]" captionStart="FIGURE 1" captionStartId="2.[151,250,1478,1500]" captionTargetBox="[206,1391,339,1437]" captionTargetId="figure@2.[150,1436,301,1457]" captionTargetPageId="2" captionText="FIGURE 1. Map showing the type locality of Indobathynella spp. in Andhra Pradesh. Indobathynella socrates n. sp. (*); Indobathynella prehensilis (Ɨ) and distribution of Indobathynella prehensilis (Δ)" httpUri="https://zenodo.org/record/2590272/files/figure.png" pageId="1" pageNumber="346">Fig.1</figureCitation>
