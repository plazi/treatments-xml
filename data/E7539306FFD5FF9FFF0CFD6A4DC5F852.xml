<taxonomicName id="A8FA5993FFD5FF9FFF75FD6549DAFD0E" box="[238,407,727,753]" class="Insecta" family="Aderidae" genus="Palaeocnopus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="249" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="5D8EFE02FFD5FF9FFF75FD6549DAFD0E" bold="true" box="[238,407,727,753]" italics="true" pageId="10" pageNumber="249">Palaeocnopus</emphasis>
