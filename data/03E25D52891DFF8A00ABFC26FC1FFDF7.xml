<collectingCountry id="F35CACD4891CFF8A022EFED1FDB5FECC" box="[530,614,256,279]" name="Sweden" pageId="1" pageNumber="68">Ruidian</collectingCountry>
Town, Tengchong, Yunnan (
<specimenCount id="9D4D27CD891CFF8A03A2FED1FBE3FECC" box="[926,1072,256,279]" pageId="1" pageNumber="68" type="generic">
<number id="3D83FE63891CFF8A03A2FED1FC69FECC" box="[926,954,256,279]" pageId="1" pageNumber="68" value="23.0">23</number>
specimens
</specimenCount>
