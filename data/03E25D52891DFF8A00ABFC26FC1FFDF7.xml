<specimenCount id="9D4D27CD891CFF8A05B2FED1FED5FEE2" pageId="1" pageNumber="68" type="generic">
<number id="3D83FE63891CFF8A05B2FED1FA48FECC" box="[1422,1435,256,279]" pageId="1" pageNumber="68" value="4.0">4</number>
