<date id="FFF5CA84891CFF8A0480FED1FA8AFECC" box="[1212,1369,256,279]" pageId="1" pageNumber="68" value="1978-02">
February 
<number id="3D83FE63891CFF8A0518FED1FA8AFECC" box="[1316,1369,256,279]" pageId="1" pageNumber="68" value="1978.0">1978</number>
</date>
