<taxonomicName id="8FEE4D41FF99FFB2FE9EFBBC675CCF73" authorityName=", Coley" authorityYear="2015" box="[270,543,1051,1077]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="cajennense">
