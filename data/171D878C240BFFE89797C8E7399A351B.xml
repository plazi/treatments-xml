<taxonomicNameLabel id="B6F357F32409FFE89515CF4A391534B6" box="[560,633,632,653]" pageId="2" pageNumber="1179" rank="species">nov.sp.</taxonomicNameLabel>
und 
<taxonomicName id="58B44D192409FFE89588CF4A383934B7" authority="Klausnitzer, 2012" authorityName="Klausnitzer" authorityYear="2012" box="[685,853,631,653]" class="Insecta" family="Scirtidae" genus="Prionocyphon" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1179" phylum="Arthropoda" rank="species" species="coccinuloides" status="nov.sp.">
