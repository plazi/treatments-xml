<taxonomicName id="58B44D192409FFE8969BCF4A394534B7" authority="Klausnitzer, 2012" authorityName="Klausnitzer" authorityYear="2012" box="[446,553,631,653]" class="Insecta" family="Scirtidae" genus="Prionocyphon" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1179" phylum="Arthropoda" rank="species" species="samueli" status="nov.sp.">
