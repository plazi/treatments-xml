<taxonomicName id="4C638795FF13BDEBFEC83E9AFD9256AA" authorityName="Det. S. Maulik" authorityYear="1926" box="[300,530,1673,1698]" class="Insecta" family="Chrysomelidae" genus="Chaetocnema" kingdom="Animalia" order="Coleoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="alticola">
