<heading id="A0728188B109FFA7FF11FB763FF7FB5E" bold="true" box="[151,415,1197,1223]" fontSize="11" level="1" pageId="1" pageNumber="498" reason="1">
<taxonomicName id="3C854D67B109FFA7FF11FB763FF7FB5E" authority="Gill, 1864" authorityName="Gill" authorityYear="1864" box="[151,415,1197,1223]" class="Actinopterygii" family="Paralepididae" genus="Arctozenus" kingdom="Animalia" order="Aulopiformes" pageId="1" pageNumber="498" phylum="Chordata" rank="genus">
<emphasis id="C9F1EAF6B109FFA7FF11FB763FF7FB5E" bold="true" box="[151,415,1197,1223]" pageId="1" pageNumber="498">
<emphasis id="C9F1EAF6B109FFA7FF11FB763F76FB5E" bold="true" box="[151,286,1197,1223]" italics="true" pageId="1" pageNumber="498">Arctozenus</emphasis>
<bibRefCitation id="9F144B15B109FFA7FEA0FB763FF7FB5E" author="Gill, T. N." box="[294,415,1197,1223]" pageId="1" pageNumber="498" pagination="187 - 189" refId="ref11217" refString="Gill, T. N. (1864) Note on the paralepidoids and microstomatoids, and on some peculiarities of Arctic ichthyology. Proceedings of the Academy of Natural Sciences of Philadelphia, 16, 187 - 189." type="journal article" year="1864">Gill, 1864</bibRefCitation>
</emphasis>
