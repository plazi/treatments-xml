<collectingCountry id="F330FDAAFF868211FE0BFF57FDA4436D" box="[446,575,233,257]" name="Philippines" pageId="14" pageNumber="1598">Philippines</collectingCountry>
<collectingCounty id="62F9C5B6FF868211FDFDFF57FD51436D" box="[584,714,233,257]" pageId="14" pageNumber="1598">Expedition</collectingCounty>
, 
<date id="FF999BFAFF868211FD62FF57FCE1436D" box="[727,890,233,257]" pageId="14" pageNumber="1598" value="1908-05-13">
