<taxonomicName id="4C7FB5B8FFDBFFA0FF74FD05FE75FCA3" authority="Johannsen, 1905: 135" authorityName="Johannsen" authorityPageNumber="135" authorityYear="1905" box="[151,511,762,784]" class="Insecta" family="Chironomidae" genus="Ablabesmyia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="481" phylum="Arthropoda" rank="genus">
