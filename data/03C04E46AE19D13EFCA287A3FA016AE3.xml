<location id="8EB6A98BAE19D13EFBA5801EFD1B6A69" LSID="urn:lsid:plazi:treatment:03C04E46AE19D13EFCA287A3FA016AE3:8EB6A98BAE19D13EFBA5801EFD1B6A69" box="[1084,1146,1319,1343]" country="United Kingdom" municipality="Cretaceous Period" name="Latin" pageId="2" pageNumber="3">Latin</location>
