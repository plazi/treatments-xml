<emphasis id="F6A98832FFAEFFA55904F178819AFDEE" bold="true" box="[112,242,603,625]" pageId="7" pageNumber="320">
<emphasis id="F6A98832FFAEFFA55904F1788184FDEE" bold="true" box="[112,236,603,625]" italics="true" pageId="7" pageNumber="320">
<typeStatus id="1B66EA82FFAEFFA55904F17881F7FDEF" box="[112,159,604,625]" pageId="7" pageNumber="320">Type</typeStatus>
species
</emphasis>
.
</emphasis>
— 
<taxonomicName id="03DD2FA3FFAEFFA5586CF178830BFDEE" authority="Song &amp; Mendes &amp; Huang, 2012" authorityName="Song &amp; Mendes &amp; Huang" authorityYear="2012" box="[280,611,603,625]" class="Insecta" family="Meinertellidae" genus="Machilontus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeognatha" pageId="7" pageNumber="320" phylum="Arthropoda" rank="species" species="lii" status="new species" subGenus="Protumidolontus">
