<taxonomicName id="00F44D1E255EE077FD2BBD53FC8CF8A9" baseAuthorityName="Kumata" baseAuthorityYear="1967" box="[709,865,1809,1835]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="laciniatae">
