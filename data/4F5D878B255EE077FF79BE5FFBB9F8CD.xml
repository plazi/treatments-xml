<taxonomicName id="00F44D1E255EE077FECBBC8BFE46F961" baseAuthorityName="Ermolaev" baseAuthorityYear="1981" box="[293,427,1737,1763]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="pumilae">
