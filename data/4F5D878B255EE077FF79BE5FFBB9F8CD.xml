<taxonomicName id="00F44D1E255EE077FBCCBD50FAA7F8A9" box="[1058,1354,1809,1835]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="variety" species="davidiana" variety="japonica">
