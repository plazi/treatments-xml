<taxonomicName id="00F44D1E255EE077FD20BCAFFCC3F885" baseAuthorityName="Kumata" baseAuthorityYear="1963" box="[718,814,1773,1799]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="ulmi">
