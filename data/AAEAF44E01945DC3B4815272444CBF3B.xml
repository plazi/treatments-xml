<taxonomicName id="4465B50AA37E2DE21D4A6882D6DEA2E1" authorityName="Zhao &amp; Li" authorityYear="2018" class="Arachnida" family="Telemidae" genus="Pinelema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pinelema spirulata" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="spirulata">Pinelema spirulata</taxonomicName>
