<taxonomicName id="73C34D0B1D6AFFA6161D3474373BFABE" authority="Fain and Lukoschus, 1979" authorityName="Fain and Lukoschus" authorityYear="1979" box="[314,837,1335,1359]" class="Arachnida" family="Psoroptidae" genus="Petauralges" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="rackae">
