<quantity id="4CF79B33FFFAFFD3C2CCFD0AFF61FD0B" box="[181,247,723,743]" metricMagnitude="3" metricUnit="m" metricValue="1.405" pageId="21" pageNumber="176" unit="m" value="1405.0">1405m</quantity>
