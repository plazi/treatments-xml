<taxonomicName id="13914D5BFFDABB4C2F812ECC0B50FD42" authority="(Costa, 1853)" baseAuthorityName="Costa" baseAuthorityYear="1853" box="[179,602,691,717]" class="Malacostraca" family="Dexaminidae" genus="Dexamine" kingdom="Animalia" order="Amphipoda" pageId="10" pageNumber="383" phylum="Arthropoda" rank="species" species="spiniventris">
