<collectingCountry id="F32A7653FF88FFC1FEE6FB6362D30635" box="[381,470,1279,1303]" name="Bolivia" pageId="8" pageNumber="7">Bolivia</collectingCountry>
, 
<collectingRegion id="49F9F821FF88FFC1FE79FA9C60160635" box="[482,787,1279,1303]" country="Bolivia" name="La Paz" pageId="8" pageNumber="7">Departamento de La Paz</collectingRegion>
(
<figureCitation id="13062A46FF88FFC1FCBFFB63607E0635" box="[804,891,1279,1303]" captionStart="Figura 12" captionStartId="24.[192,276,1879,1901]" captionTargetBox="[192,1440,647,1465]" captionTargetId="figure@24.[192,1440,647,1465]" captionTargetPageId="24" captionText="Figura 12. Distribución geográfica de las especies de Microogenius Gutiérrez en la región andina." figureDoi="http://doi.org/10.5281/zenodo.3670517" httpUri="https://zenodo.org/record/3670517/files/figure.png" pageId="8" pageNumber="7">Fig. 12</figureCitation>
).
</paragraph>
