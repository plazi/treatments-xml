<collectingCountry id="EAD9D3A4FFEDC270FC4C1FF8FBAAF983" box="[942,1069,661,687]" name="Guatemala" pageId="29" pageNumber="30">Guatemala</collectingCountry>
and 
<collectingCountry id="EAD9D3A4FFEDC270FB841FF8FB5BF983" box="[1126,1244,661,687]" name="Honduras" pageId="29" pageNumber="30">Honduras</collectingCountry>
