<taxonomicName id="2394EBC36E49FFC8E4DFF910FA2431D5" authority="proxima" authorityName="proxima (Mabille" authorityYear="1877" box="[1206,1436,1731,1755]" class="Insecta" family="Hesperiidae" genus="Celaenorrhinus" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subSpecies" species="proxima" subSpecies="proxima">
