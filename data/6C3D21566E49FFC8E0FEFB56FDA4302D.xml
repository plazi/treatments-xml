<taxonomicName id="2394EBC36E49FFC8E10BF910FDFF31D5" authority="proxima" authorityName="proxima (Mabille" authorityYear="1877" box="[354,583,1731,1755]" class="Insecta" family="Hesperiidae" genus="Celaenorrhinus" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subSpecies" species="proxima" subSpecies="proxima">
<emphasis id="D6E04C526E49FFC8E10BF910FDFF31D5" box="[354,583,1731,1755]" italics="true" pageId="5" pageNumber="6">C. proxima proxima</emphasis>
</taxonomicName>
(
