<taxonomicName id="FB895AF15361FFEEFA4FFA91FE8A68F2" class="Insecta" family="Aphrophoridae" genus="Tremapterus" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="179" phylum="Arthropoda" rank="species" species="perfasciata" subGenus="Nyanja">
<emphasis id="0EFDFD605361FFEEFA4FFA91FA5968CD" box="[1415,1435,1398,1421]" italics="true" pageId="28" pageNumber="179">T.</emphasis>
(
<emphasis id="0EFDFD605361FFEEFF68FA7DFF7868F1" box="[160,186,1434,1457]" italics="true" pageId="28" pageNumber="179">N.</emphasis>
) 
<emphasis id="0EFDFD605361FFEEFF02FA7DFE8A68F2" box="[202,328,1434,1458]" italics="true" pageId="28" pageNumber="179">perfasciata</emphasis>
</taxonomicName>
from 
<collectingCountry id="449E61E25361FFEEFE44FA7EFE2568F2" box="[396,487,1433,1458]" name="Uganda" pageId="28" pageNumber="179">Uganda</collectingCountry>
