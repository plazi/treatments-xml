<taxonomicName id="53ED4D68341A7C3FFE75643DFD0F071A" baseAuthorityName="Becker" baseAuthorityYear="1901" box="[445,694,1358,1381]" class="Insecta" family="Phoridae" genus="Beckerina" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="umbrimargo">
