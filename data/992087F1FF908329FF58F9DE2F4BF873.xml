<taxonomicName id="D6894D64FF908329FDDAF9F22F9EF9B0" authorityName="Burmeister" authorityYear="1838" box="[533,717,1593,1620]" class="Insecta" family="Tettigoniidae" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="subFamily" subFamily="Phaneropterinae">Phaneropterinae</taxonomicName>
