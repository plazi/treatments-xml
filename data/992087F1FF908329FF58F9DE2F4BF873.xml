<taxonomicName id="D6894D64FF908329FC98F9DE2906F9CB" authority="Hebard, 1922" authorityName="Hebard" authorityYear="1922" box="[855,1109,1557,1584]" class="Insecta" family="Phaneropteridae" genus="Elimaea" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="bakeri">
