<emphasis id="55AEEAABFFFDFFECFF1CCC99FE6EFDFB" bold="true" box="[151,407,502,526]" pageId="8" pageNumber="451">Differential diagnosis.</emphasis>
<taxonomicName id="A0DA4D3AFFFDFFECFE2ACC98FD2AFDFB" authorityName="Kazantsev" authorityYear="2009" box="[417,723,503,526]" class="Insecta" family="Cantharidae" genus="Laemoglyptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="451" phylum="Arthropoda" rank="species" species="manasluensis">
