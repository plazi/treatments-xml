, 
<taxonomicName id="736EA9C609DCF2DFBBBAABE7E3ABDF45" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33811" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole jujuyensis Forel" lsidName-HNS="Pheidole jujuyensis Forel" order="Hymenoptera" pageNumber="335" phylum="Arthropoda" rank="species" species="jujuyensis">jujuyensis</taxonomicName>
