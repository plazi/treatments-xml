<location id="05D5F14D7879742CFFE4AC9913F7F1E2" LSID="urn:lsid:plazi:treatment:BC7D7FFF74C25896A285A134C22A8F60:05D5F14D7879742CFFE4AC9913F7F1E2" country="Australia" county="Queenstown" latitude="-41.532" longLatPrecision="69" longitude="146.426" municipality="King River" name="Same" stateProvince="Tasmania">Same</location>
