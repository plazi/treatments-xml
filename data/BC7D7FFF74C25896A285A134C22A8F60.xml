<location id="BE1A6F4573C2175E1EF1A2EE68AD872B" LSID="urn:lsid:plazi:treatment:BC7D7FFF74C25896A285A134C22A8F60:BE1A6F4573C2175E1EF1A2EE68AD872B" country="Australia" county="Queenstown" latitude="-41.532" longLatPrecision="69" longitude="146.426" municipality="King River" name="In" stateProvince="Tasmania">In</location>
