<typeStatus id="54C3881EFF99BA2BE0DFFBA2E590FB84" box="[151,210,1072,1096]" pageId="2" pageNumber="388">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C784D3FFF99BA2BE173FBA2E79EFB8B" authority="Schulze, 1886" authorityName="Schulze" authorityYear="1886" box="[315,732,1071,1096]" class="Hexactinellida" family="Rossellidae" genus="Bathydorus" kingdom="Animalia" order="Lyssacinosida" pageId="2" pageNumber="388" phylum="Porifera" rank="species" species="fimbriatus">
<emphasis id="B90CEAAEFF99BA2BE173FBA2E774FB84" box="[315,566,1072,1096]" italics="true" pageId="2" pageNumber="388">Bathydorus fimbriatus</emphasis>
