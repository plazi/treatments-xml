<taxonomicName id="FA9FDF8E2E254D09A8DAFEBEFE65FEFE" authority="Martinez &amp; Pereira, 1959" authorityName="Martinez &amp; Pereira" authorityYear="1959" class="Insecta" family="Scarabaeidae" genus="Phacosomoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="374" phylum="Arthropoda" rank="genus">
