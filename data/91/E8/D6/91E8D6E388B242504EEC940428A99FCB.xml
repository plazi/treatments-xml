<taxonomicName class="Demospongiae" family="Chalinidae" genus="Reniera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Reniera mucosa" order="Haplosclerida" pageId="39" pageNumber="40" phylum="Porifera" rank="species" species="mucosa">Reniera mucosa</taxonomicName>
