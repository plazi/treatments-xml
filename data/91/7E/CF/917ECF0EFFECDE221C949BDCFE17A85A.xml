<emphasis bold="true" box="[160,340,645,670]" inLineHeading="true" italics="true" pageId="6" pageNumber="2395" reason="3">Type Material.</emphasis>
<materialsCitation ID-GBIF-Occurrence="2846531308" collectingDate="2015-08-05" country="China" county="Jinsha County" elevation="770" latitude="27.9" location="Yuanyanggu" longLatPrecision="1234" longitude="106.0" municipality="Lengshuihe Natural Reserve" pageId="6" pageNumber="2395" specimenCount="1" specimenCount-female="1" stateProvince="Guizhou" typeStatus="holotype">
