<taxonomicName baseAuthorityName="Clark" baseAuthorityYear="1862" class="Insecta" family="Dytiscidae" genus="Paroster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paroster insculptilis" order="Coleoptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="species" species="insculptilis">
