<taxonomicName class="Insecta" family="Dytiscidae" genus="Rhantus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhantus simulans" order="Coleoptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="species" species="simulans">
