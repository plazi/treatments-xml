<taxonomicName class="Insecta" family="Dytiscidae" genus="Exocelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exocelina ater" order="Coleoptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="species" species="ater">
<emphasis italics="true" pageId="0" pageNumber="11">Exocelina ater</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="W. S. MacLeay" baseAuthorityYear="1825" class="Insecta" family="Dytiscidae" genus="Rhantus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhantus suturalis" order="Coleoptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="species" species="suturalis">
