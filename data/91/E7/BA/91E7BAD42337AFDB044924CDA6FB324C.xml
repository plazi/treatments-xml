<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Hemiptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Hemiptera" order="Malvales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="genus">Hemiptera</taxonomicName>
