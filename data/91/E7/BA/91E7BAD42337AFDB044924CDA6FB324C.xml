<taxonomicName class="Gastropoda" family="Enidae" genus="Mirus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mirus" order="Stylommatophora" pageId="100" pageNumber="101" phylum="Mollusca" rank="genus">Mirus</taxonomicName>
