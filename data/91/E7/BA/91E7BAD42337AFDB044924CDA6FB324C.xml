<taxonomicName class="Insecta" family="Miridae" genus="Miris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Miris" order="Hemiptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="genus">Miris</taxonomicName>
