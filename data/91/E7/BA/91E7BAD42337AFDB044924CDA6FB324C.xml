<taxonomicName class="Insecta" family="Staphylinidae" genus="Imirus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Imirus" order="Coleoptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="genus">Imirus</taxonomicName>
