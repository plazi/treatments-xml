<subSubSection pageId="5" pageNumber="356" type="materials_examined">
<paragraph blockId="5.[151,1436,255,1181]" pageId="5" pageNumber="356">
<emphasis bold="true" box="[151,388,255,280]" pageId="5" pageNumber="356">Material examined:</emphasis>
DZUP ASC 
<number box="[540,582,255,279]" pageId="5" pageNumber="356" value="193.0">193</number>
– 
<number box="[612,626,255,279]" pageId="5" pageNumber="356" value="1.0">1</number>
ind.; 
<collectingCountry box="[693,733,255,279]" name="Iceland" pageId="5" pageNumber="356">Isla</collectingCountry>
Canales de Afuera; 10.0 m, under rock; 
<date box="[1198,1309,255,279]" pageId="5" pageNumber="356" value="2009-01-13">
<number box="[1198,1223,255,279]" pageId="5" pageNumber="356" value="13.0">13</number>
/i/ 
