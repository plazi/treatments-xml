<quantity box="[453,620,728,752]" metricMagnitude="-3" metricUnit="m" metricValue="1.7149999999999999" metricValueMax="1.72" metricValueMin="1.71" pageId="3" pageNumber="412" unit="mm" value="1.7149999999999999" valueMax="1.72" valueMin="1.71">1.71–1.72 mm</quantity>
; HL 
<quantity box="[678,843,727,752]" metricMagnitude="-4" metricUnit="m" metricValue="2.7" metricValueMax="2.8" metricValueMin="2.6" pageId="3" pageNumber="412" unit="mm" value="0.27" valueMax="0.28" valueMin="0.26">0.26–0.28 mm</quantity>
, HW 
<quantity box="[910,1076,728,752]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" metricValueMax="3.1" metricValueMin="2.9" pageId="3" pageNumber="412" unit="mm" value="0.3" valueMax="0.31" valueMin="0.29">0.29–0.31 mm</quantity>
, AnL 
<quantity box="[1148,1251,728,752]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="3" pageNumber="412" unit="mm" value="0.7">0.70 mm</quantity>
, relative lengths of antennomeres: 1.0: 1.0: 
<number box="[475,510,764,789]" pageId="3" pageNumber="412" value="0.5">0.5</number>
: 
<number box="[535,570,764,789]" pageId="3" pageNumber="412" value="0.6">0.6</number>
