<subSubSection type="nomenclature">
<paragraph pageNumber="195">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181673" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole hoelldobleri Wilson" order="Hymenoptera" pageNumber="195" phylum="Arthropoda" rank="species" species="hoelldobleri" status="new species">Pheidole hoelldobleri</taxonomicName>
