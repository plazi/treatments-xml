) in 
<collectingCountry box="[341,419,1060,1085]" name="Poland" pageId="18" pageNumber="469">Poland</collectingCountry>
(
<bibRefCitation author="Skoracki" box="[437,677,1060,1085]" pageId="18" pageNumber="469" refString="Skoracki, M., Bochkov, A. V. &amp; Sikora, B. (2004) New records of the cheyletoid mites (Acari: Cheyletoidea) from passeriform birds in Poland. Belgian Journal of Entomology, 6, 83 - 90." type="journal article" year="2004">
