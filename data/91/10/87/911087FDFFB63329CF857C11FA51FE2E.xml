<subSubSection box="[151,437,1135,1162]" pageId="1" pageNumber="234" type="nomenclature">
<paragraph blockId="1.[151,437,1135,1162]" box="[151,437,1135,1162]" pageId="1" pageNumber="234">
<heading bold="true" box="[151,437,1135,1162]" fontSize="11" level="1" pageId="1" pageNumber="234" reason="1">
<emphasis bold="true" box="[151,437,1135,1162]" pageId="1" pageNumber="234">
<taxonomicName box="[151,320,1135,1161]" class="Insecta" family="Leptopodidae" genus="Leptosaldinea" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="234" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,320,1135,1161]" italics="true" pageId="1" pageNumber="234">Leptosaldinea</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[327,437,1136,1162]" pageId="1" pageNumber="234" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
