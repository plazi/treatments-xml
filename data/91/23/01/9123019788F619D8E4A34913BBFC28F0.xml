<taxonomicName class="Insecta" family="Melolonthidae" genus="Plectris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Plectris" order="Coleoptera" pageId="127" pageNumber="128" phylum="Arthropoda" rank="genus">Plectris</taxonomicName>
