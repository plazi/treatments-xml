<emphasis bold="true" box="[522,578,1843,1864]" pageId="8" pageNumber="338">n. sp.</emphasis>
</taxonomicNameLabel>
, ♀ (HT): 
<emphasis bold="true" box="[679,691,1843,1864]" pageId="8" pageNumber="338">1</emphasis>
, dorsal habitus; 
<emphasis bold="true" box="[857,869,1843,1864]" pageId="8" pageNumber="338">2</emphasis>
, lateral habitus; 
<emphasis bold="true" box="[1037,1050,1843,1864]" pageId="8" pageNumber="338">3</emphasis>
, head, frontal; 
<emphasis bold="true" box="[1202,1215,1843,1864]" pageId="8" pageNumber="338">4</emphasis>
, head, lateral; 
<emphasis bold="true" box="[1363,1376,1843,1864]" pageId="8" pageNumber="338">5</emphasis>
, head and mesosoma, dorsal; 
<emphasis bold="true" box="[391,404,1874,1895]" pageId="8" pageNumber="338">6</emphasis>
, propodeum; 
<emphasis bold="true" box="[547,559,1874,1895]" pageId="8" pageNumber="338">7</emphasis>
, antenna; 
