<taxonomicName id="4C4E4D75FFAEFFCAB684EF9DFD40FFE9" authority="Norrbom &amp; Uchoa" authorityName="Norrbom &amp; Uchoa" box="[151,701,392,418]" class="Insecta" family="Tephritidae" genus="Anastrepha" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="62" phylum="Arthropoda" rank="species" species="matogrossensis" status="new species">
<emphasis id="B93AEAE4FFAEFFCAB684EF9DFE2EFFEA" bold="true" box="[151,467,393,418]" italics="true" pageId="1" pageNumber="62">Anastrepha matogrossensis</emphasis>
Norrbom &amp; Uchôa
</taxonomicName>
, 
<taxonomicNameLabel id="A209579FFFAEFFCAB4D9EF9DFCA9FFE9" box="[714,852,392,417]" pageId="1" pageNumber="62" rank="species">new species</taxonomicNameLabel>
