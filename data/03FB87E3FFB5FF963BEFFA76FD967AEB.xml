<taxonomicName id="4C524D76FFB5FF963A0CF9E0FDB97A0F" authority="Meigen, 1800" authorityName="Meigen" authorityYear="1800" box="[372,594,1626,1651]" class="Malacostraca" family="Leucosiidae" genus="Iphis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAE7FFB5FF963A0CF9E0FE477A0F" box="[372,428,1626,1651]" italics="true" pageId="21" pageNumber="22">Iphis</emphasis>
<bibRefCitation id="EFC34B04FFB5FF963ACCF9E0FDB97A0F" author="Meigen, J. W." bookContentInfo="40 pp." box="[436,594,1626,1651]" journalOrPublisher="J. J. Fuchs, Paris" pageId="21" pageNumber="22" refId="ref61040" refString="Meigen, J. W. (1800) Nouvelle classification des mouches a deux ailes (Diptera L.) d'apres un plan tout nouveau. Par J. G. Meigen. &quot; An VIII (1800 v. s.) &quot;. J. J. Fuchs, Paris, 40 pp. [before 22 September] [Dated from Thompson et al. (1999: 476).] https: // doi. org / 10.5962 / bhl. title. 119764" title="Nouvelle classification des mouches a deux ailes (Diptera L.) d'apres un plan tout nouveau. Par J. G. Meigen. &quot; An VIII (1800 v. s.) &quot;." type="book" year="1800">Meigen, 1800</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C524D76FFB5FF963925F9E6FCB87A0F" authority="Meigen, 1800" authorityName="Meigen" authorityYear="1800" box="[605,851,1626,1651]" class="Sordariomycetes" family="Melanconidaceae" genus="Titania" higherTaxonomySource="GBIF" kingdom="Fungi" order="Diaporthales" pageId="21" pageNumber="22" phylum="Ascomycota" rank="genus">
<emphasis id="B926EAE7FFB5FF963925F9E6FD407A0F" box="[605,683,1628,1651]" italics="true" pageId="21" pageNumber="22">Titania</emphasis>
<bibRefCitation id="EFC34B04FFB5FF9639CCF9E0FCB87A0F" author="Meigen, J. W." bookContentInfo="40 pp." box="[692,851,1626,1651]" journalOrPublisher="J. J. Fuchs, Paris" pageId="21" pageNumber="22" refId="ref61040" refString="Meigen, J. W. (1800) Nouvelle classification des mouches a deux ailes (Diptera L.) d'apres un plan tout nouveau. Par J. G. Meigen. &quot; An VIII (1800 v. s.) &quot;. J. J. Fuchs, Paris, 40 pp. [before 22 September] [Dated from Thompson et al. (1999: 476).] https: // doi. org / 10.5962 / bhl. title. 119764" title="Nouvelle classification des mouches a deux ailes (Diptera L.) d'apres un plan tout nouveau. Par J. G. Meigen. &quot; An VIII (1800 v. s.) &quot;." type="book" year="1800">Meigen, 1800</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="4C524D76FFB5FF9638F6F9E0FB807A0F" authority="Meigen, 1800" authorityName="Meigen" authorityYear="1800" box="[910,1131,1626,1651]" class="Malacostraca" family="Tylidae" genus="Tylos" kingdom="Animalia" order="Isopoda" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAE7FFB5FF9638F6F9E0FC2C7A0F" box="[910,967,1626,1651]" italics="true" pageId="21" pageNumber="22">Tylos</emphasis>
<bibRefCitation id="EFC34B04FFB5FF9638B7F9E0FB807A0F" author="Meigen, J. W." bookContentInfo="40 pp." box="[975,1131,1626,1651]" journalOrPublisher="J. J. Fuchs, Paris" pageId="21" pageNumber="22" refId="ref61040" refString="Meigen, J. W. (1800) Nouvelle classification des mouches a deux ailes (Diptera L.) d'apres un plan tout nouveau. Par J. G. Meigen. &quot; An VIII (1800 v. s.) &quot;. J. J. Fuchs, Paris, 40 pp. [before 22 September] [Dated from Thompson et al. (1999: 476).] https: // doi. org / 10.5962 / bhl. title. 119764" title="Nouvelle classification des mouches a deux ailes (Diptera L.) d'apres un plan tout nouveau. Par J. G. Meigen. &quot; An VIII (1800 v. s.) &quot;." type="book" year="1800">Meigen, 1800</bibRefCitation>
