<taxonomicName id="26A334CC655B7921FC1EF75E4D0DF93A" authority="Aguiar &amp; Melo, 2011" authorityName="Aguiar &amp; Melo" authorityYear="2011" box="[970,1316,1661,1680]" class="Insecta" family="Apidae" genus="Paratetrapedia" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="389" phylum="Arthropoda" rank="species" species="tocantinensis" status="sp. nov.">
<emphasis id="D3D7935D655B7921FC1EF75E4D0DF93A" bold="true" box="[970,1316,1661,1680]" italics="true" pageId="38" pageNumber="389">Paratetrapedia tocantinensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="C8E42E26655B7921FAFAF75E4DAEF93A" box="[1326,1415,1661,1680]" pageId="38" pageNumber="389" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="26A334CC655B7921FF44F7B949ABF907" baseAuthorityName="FRIESE" baseAuthorityYear="1923" box="[144,386,1690,1709]" class="Insecta" family="Apidae" genus="Paratetrapedia" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="389" phylum="Arthropoda" rank="species" species="romani">
<emphasis id="D3D7935D655B7921FF44F7B949ABF907" box="[144,386,1690,1709]" italics="true" pageId="38" pageNumber="389">Paratetrapedia romani</emphasis>
</taxonomicName>
, and 
<taxonomicName id="26A334CC655B7921FE16F7B94AD9F907" authority="Aguiar &amp; Melo, 2011" authorityName="Aguiar &amp; Melo" authorityYear="2011" box="[450,752,1690,1709]" class="Insecta" family="Apidae" genus="Paratetrapedia" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="389" phylum="Arthropoda" rank="species" species="flavifrons" status="sp. nov.">
