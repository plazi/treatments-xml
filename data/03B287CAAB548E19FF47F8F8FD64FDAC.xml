<collectingRegion id="49DFF83EAB558E19FDE6FEA3FCB2FE3C" box="[566,641,367,392]" country="India" name="Kerala" pageId="5" pageNumber="415">Kerala</collectingRegion>
(
<collectionCode id="ED0AAE19AB558E19FD43FEBCFD3FFE3C" box="[659,780,368,392]" pageId="5" pageNumber="415">DABFUK</collectionCode>
), 
<collectingCountry id="F30C764CAB558E19FCF3FEA3FD6CFE3C" box="[803,863,367,392]" name="India" pageId="5" pageNumber="415">India</collectingCountry>
