<location id="8ED2601F95460853E10DF913E9A0F924" LSID="urn:lsid:plazi:treatment:03A487D295460850E0F1FE63ECA4FE32:8ED2601F95460853E10DF913E9A0F924" box="[363,500,1734,1760]" country="Ascension Island" name="The Piggery" pageId="21" pageNumber="22">The Piggery</location>
