<quantity id="E61B26AEFFE8FF8FB007FB7982ADFB13" box="[838,941,1216,1240]" metricMagnitude="-4" metricUnit="m" metricValue="4.2" pageId="45" pageNumber="46" unit="mm" value="0.42">0.42 mm</quantity>
