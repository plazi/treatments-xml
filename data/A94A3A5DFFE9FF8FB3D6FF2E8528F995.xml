<quantity id="E61B26AEFFE8FF8FB20EFB7980B6FB13" box="[335,438,1216,1240]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="45" pageNumber="46" unit="mm" value="0.7">0.70 mm</quantity>
; hind wing: length 
<quantity id="E61B26AEFFE8FF8FB1CFFB7983F4FB13" box="[654,756,1216,1240]" metricMagnitude="-3" metricUnit="m" metricValue="1.71" pageId="45" pageNumber="46" unit="mm" value="1.71">1.71 mm</quantity>
