<taxonomicName id="CEED4D7FFFBAFFDFC1BFC00EE2F3F40D" box="[996,1153,1295,1321]" class="Pinopsida" family="Pinaceae" genus="Cedrus" kingdom="Plantae" order="Pinales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="libani">Cedrus libani</taxonomicName>
