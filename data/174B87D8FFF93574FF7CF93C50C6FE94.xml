<taxonomicName id="58E24D4DFFF93575FE54F848502BF823" box="[447,602,1938,1963]" class="Insecta" family="Staphylinidae" genus="Stylogymnusa" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="317" phylum="Arthropoda" rank="genus">
