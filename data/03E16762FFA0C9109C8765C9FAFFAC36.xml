<taxonomicName id="4C48ADF7FFA0C9109D566746FE0DAC6A" authorityName="Eversmann" authorityYear="1844" box="[326,471,786,811]" class="Insecta" family="Gelechiidae" genus="Lita" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="fuscella">
