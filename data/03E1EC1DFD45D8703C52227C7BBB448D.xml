; 6 – 
<taxonomicName id="4C482688FD46D8713D7622C57A0A40C0" authority="Constantin, 2010" authorityName="Constantin" authorityYear="2010" box="[590,838,1703,1722]" class="Insecta" family="Cantharidae" genus="Pygodiscodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="79" phylum="Arthropoda" rank="species" species="touroulti">
