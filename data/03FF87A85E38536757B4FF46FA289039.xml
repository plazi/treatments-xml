<taxonomicName id="4C564D3D5E385367562FFE8CFD529302" authority="Hope, 1831" authorityName="Hope" authorityYear="1831" box="[268,743,337,359]" class="Insecta" family="Carabidae" genus="Calochroa" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="subSpecies" species="flavomaculata" subSpecies="flavomaculata">
<emphasis id="B922EAAC5E385367562FFE8CFDE09302" box="[268,597,337,359]" italics="true" pageId="8" pageNumber="209">C. flavomaculata flavomaculata</emphasis>
(
<bibRefCitation id="EFC74B4F5E3853675546FE8FFD559302" author="Hope" box="[613,736,337,359]" pageId="8" pageNumber="209" refString="Hope, F. W. (1831) Synopsis of the new species of Nepal insects in the collection of Major General Hardwicke. Zoological Miscellany, 1, 21 - 32." type="journal article" year="1831">
Hope, 
<number id="3D9E24995E385367558FFE8FFD559302" box="[684,736,337,359]" pageId="8" pageNumber="209" value="1831.0">1831</number>
</bibRefCitation>
)
</taxonomicName>
