<taxonomicName id="4C184D37FFA4DF38FF1581E4FDF952E5" authority="(Dodero, 1904)" authorityName="Dodero" authorityYear="1904" baseAuthorityName="Dodero" baseAuthorityYear="1904" box="[151,577,692,717]" class="Insecta" family="Leiodidae" genus="Neobathyscia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="fabianii">
