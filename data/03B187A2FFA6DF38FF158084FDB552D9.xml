<taxonomicName id="4C184D37FFA4DF38FEA0813CFDA652AC" authority="Sphodrinae" authorityName="Sphodrinae" box="[290,542,620,645]" class="Insecta" family="Carabidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="family">Carabidae Sphodrinae</taxonomicName>
, 
<taxonomicName id="4C184D37FFA4DF38FDA8813DFB2752AC" authority="(Kuster, 1846)" authorityName="Kuster" authorityYear="1846" baseAuthorityName="Kuster" baseAuthorityYear="1846" box="[554,1183,620,645]" class="Insecta" family="Carabidae" genus="Laemostenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="schreibersi" subGenus="Antisphodrus">
<emphasis id="B96CEAA6FFA4DF38FDA8813DFD0452AD" box="[554,700,621,644]" italics="true" pageId="63" pageNumber="64">Laemostenus</emphasis>
(
<emphasis id="B96CEAA6FFA4DF38FD4C813CFCDB52AC" box="[718,867,620,645]" italics="true" pageId="63" pageNumber="64">Antisphodrus</emphasis>
) 
<emphasis id="B96CEAA6FFA4DF38FCF7813CFC4852AC" box="[885,1008,620,645]" italics="true" pageId="63" pageNumber="64">schreibersi</emphasis>
(Küster, 1846)
</taxonomicName>
; 
<taxonomicName id="4C184D37FFA4DF38FB2E813CFAA652AC" box="[1196,1310,620,645]" class="Insecta" family="Carabidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="family">Carabidae</taxonomicName>
<taxonomicName id="4C184D37FFA4DF38FAA4813CFA2052AC" authorityName="Bonelli" authorityYear="1810" box="[1318,1432,620,645]" class="Insecta" family="Carabidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="subFamily" subFamily="Trechinae">Trechinae</taxonomicName>
, 
<taxonomicName id="4C184D37FFA4DF38FF1581DFFD155281" authority="vicentinus (Gestro, 1907)" authorityName="vicentinus (Gestro" authorityYear="1907" box="[151,685,655,680]" class="Insecta" family="Carabidae" genus="Orotrechus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="subSpecies" species="vicentinus" subSpecies="vicentinus">
