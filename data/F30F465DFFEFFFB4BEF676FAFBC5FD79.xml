<taxonomicName id="BCA68CC8FFEFFFAABA0277A9FAC6F863" baseAuthorityName="Moulton" baseAuthorityYear="1928" box="[1123,1328,1925,1951]" class="Insecta" family="Phlaeothripidae" genus="Liothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="364" phylum="Arthropoda" rank="species" species="kuwanai">
