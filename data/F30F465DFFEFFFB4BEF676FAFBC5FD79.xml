<collectingCountry id="03B1B7DBFFEFFFAABC397786FD6EF838" box="[600,664,1962,1988]" name="Japan" pageId="15" pageNumber="364">Japan</collectingCountry>
as well as 
<collectingRegion id="B96239A9FFEFFFAABD727785FC92F83F" box="[787,868,1961,1987]" country="China" name="Hainan" pageId="15" pageNumber="364">Hainan</collectingRegion>
