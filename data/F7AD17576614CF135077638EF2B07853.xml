<taxonomicName id="2A2478DD7D29E222D48906420C1F1A6D" LSID-ZBK="urn:lsid:zoobank.org:act:87028B01-5D5B-4004-918B-492D3059F90E" class="Elasmobranchii" family="Scyliorhinidae" genus="Haploblepharus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Haploblepharus Garman 1913:101" order="Carcharhiniformes" pageNumber="41" phylum="Chordata" rank="genus">Haploblepharus Garman</taxonomicName>
