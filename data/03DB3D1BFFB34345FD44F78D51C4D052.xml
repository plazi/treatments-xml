<taxonomicName id="4C72F78EFFB34343FED8F72A5772DD29" authority="LeConte, 1858: 28" authorityName="LeConte" authorityPageNumber="28" authorityYear="1858" box="[313,1102,2288,2320]" class="Insecta" family="Carabidae" genus="Brachinus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="glabripennis">
