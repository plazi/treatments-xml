Male, 
<collectingCountry id="F304761C5D6DFFC8FE11D75D7FDFFC06" box="[469,644,880,906]" name="Costa Rica" pageId="13" pageNumber="14">COSTA RICA</collectingCountry>
. PUNTARENAS, Peninsula de 
<taxonomicName id="4C134D0F5D6DFFC8FC3CD75D7973FC06" box="[1016,1064,880,906]" class="Insecta" family="Carabidae" genus="Aspasiola" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="29" phylum="Arthropoda" rank="species" species="osa">Osa</taxonomicName>
, P.N. Corcovado, Estación Sirena, upper Ollas Trail, 
<quantity id="4CEB9B695D6DFFC8FD4BD7B47E57FC3F" box="[655,780,921,947]" metricMagnitude="1" metricUnit="m" metricValue="9.0" metricValueMax="15.0" metricValueMin="3.0" pageId="13" pageNumber="14" unit="m" value="90.0" valueMax="150.0" valueMin="30.0">30–150 m</quantity>
, 
<geoCoordinate id="EE27504B5D6DFFC8FCDFD7B47E91FC3F" box="[795,970,921,947]" direction="north" orientation="latitude" pageId="13" pageNumber="14" precision="15" value="8.483334">
08° 
<number id="3DDB24AB5D6DFFC8FC8BD7B47E34FC3F" box="[847,879,921,947]" pageId="13" pageNumber="14" value="29.0">29</number>
' 00” N
</geoCoordinate>
, 
