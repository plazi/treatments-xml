<paragraph id="8B877956FFF099608377F966FD02CA61" blockId="25.[264,1324,798,1905]" pageId="25" pageNumber="26">
<emphasis id="B94CA544FFF099608377F966FDEBCB90" bold="true" box="[312,521,1718,1744]" italics="true" pageId="25" pageNumber="26">
<collectingCountry id="F32F39C6FFF099608377F966FE97CB90" box="[312,373,1718,1744]" name="Malta" pageId="25" pageNumber="26">Male</collectingCountry>
