<taxonomicName id="605C58078561A695C709C6F0EC4E218F" class="Polychaeta" family="Orbiniidae" genus="Scoloplos" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scoloplos typicus" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="typicus">Scoloplos typicus</taxonomicName>
