<taxonomicName id="C15F4C915A09589651BF809514233291" class="Polychaeta" family="Orbiniidae" genus="Scoloplos" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scoloplos armiger" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="armiger">Scoloplos armiger</taxonomicName>
