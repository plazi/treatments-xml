<paragraph id="8DDCADB46904DDD1BD5897B3F9F07050" pageId="null" pageNumber="109">
Artengruppe des 
<taxonomicName id="4A8289AC8C5E94FD33F44CC2B33904F5" authority="L." class="Magnoliopsida" family="Papaveraceae" genus="Papaver" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="109" phylum="Tracheophyta" rank="species" species="alpinum">
Papaver 
<normalizedToken id="FA8A8620DBDC00A0BD25BF1AE1667121" originalValue="alpínum" pageId="null" pageNumber="109">alpinum</normalizedToken>
<authorityName id="F5ED4D15012C690A661E2E2D115A9394" pageId="null" pageNumber="109">L.</authorityName>
</taxonomicName>
