<taxonomicName id="4C1E3EE61421FFC20173FBD1F4EBFBF7" authorityName="STUBEN" authorityYear="2008" box="[595,753,1071,1092]" class="Insecta" family="Curculionidae" genus="Echinoacalles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="83" phylum="Arthropoda" rank="genus">Echinoacalles</taxonomicName>
