<taxonomicName id="E0F64D56DE49FFD5B030C495FD7D91F9" authority="Brum, 1978" authorityName="Brum" authorityYear="1978" box="[312,733,1249,1273]" class="Insecta" family="Cerambycidae" genus="Paraleiopus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="macrochelis">
