<taxonomicName id="63E463FAFFB0FF8DFE72F891FD469B12" authority="(Forbes, 1848)" baseAuthorityName="Forbes" baseAuthorityYear="1848" box="[466,737,1865,1888]" class="Hydrozoa" family="Rathkeidae" genus="Lizzia" kingdom="Animalia" order="Anthoathecata" pageId="4" pageNumber="396" phylum="Cnidaria" rank="species" species="blondina">
