. – 
<taxonomicName id="4C47C9A3034A2B4BFC1BC1F9FBCEFC36" box="[927,1128,908,932]" class="Liliopsida" family="Cyperaceae" genus="Cyperus" kingdom="Plantae" order="Poales" pageId="3" pageNumber="148" phylum="Tracheophyta" rank="species" species="gypsophilus">
