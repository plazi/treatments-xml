<paragraph id="8B87302FFF82FF89D0E0FDFFFEEB9B93" blockId="12.[132,680,600,656]" pageId="12" pageNumber="353">
<materialsCitation id="3B503A72FF82FF89D0E0FDFFFEEB9B93" collectionCode="MNKM" pageId="12" pageNumber="353" specimenCount="1" typeStatus="holotype">
<emphasis id="B94CEC3DFF82FF89D0E0FDFFFECB9B73" box="[132,281,600,624]" italics="true" pageId="12" pageNumber="353">
<typeStatus id="54838E8DFF82FF89D0E0FDFFFF099B73" box="[132,219,600,624]" pageId="12" pageNumber="353">Holotype</typeStatus>
