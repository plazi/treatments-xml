<subSubSection id="C37DDB925610FF9FFEF3C9743382329E" pageId="13" pageNumber="14" type="etymology">
<paragraph id="8BD888195610FF9FFEF3C97431A732E5" blockId="13.[264,1323,283,470]" pageId="13" pageNumber="14">
<emphasis id="B913540B5610FF9FFEF3C97431F232CE" bold="true" box="[312,445,363,389]" pageId="13" pageNumber="14">Etymology</emphasis>
