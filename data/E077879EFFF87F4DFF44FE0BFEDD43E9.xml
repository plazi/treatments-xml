<taxonomicName id="AFDE4D0BFFF87F4DFA88F9CEFEB14385" class="Insecta" family="Agromyzidae" genus="Liriomyza" kingdom="Animalia" order="Diptera" pageId="110" pageNumber="111" phylum="Arthropoda" rank="species" species="venegasiae">
