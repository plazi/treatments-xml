<taxonomicName id="E5AF4D78826FFFB9FF63FB503C0AFB74" authority="Lacroix, 1978: 250" authorityName="Lacroix" authorityPageNumber="250" authorityYear="1978" box="[140,611,1187,1214]" class="Insecta" family="Lucanidae" genus="Chiasognathus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="42" phylum="Arthropoda" rank="species" species="beneshi">
