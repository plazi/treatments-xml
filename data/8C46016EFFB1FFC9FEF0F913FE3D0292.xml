<subSubSection id="4CF5E3F3FF8FFFC9FEA0FDECFE3D0292" pageId="33" pageNumber="234" type="etymology">
<paragraph id="0450B078FF8FFFC9FEA0FDECFE3D0292" blockId="33.[151,1436,151,645]" pageId="33" pageNumber="234">
<emphasis id="369B6C6AFF8FFFC9FEA0FDECFEB60277" bold="true" box="[199,334,583,608]" pageId="33" pageNumber="234">Etymology.</emphasis>
