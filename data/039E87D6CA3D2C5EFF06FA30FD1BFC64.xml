<number id="3DFF24E7CA3B2C5EFD53FC44FD74FC2C" box="[706,755,908,932]" pageId="5" pageNumber="56" value="17.7">17.7</number>
–19.0, female 
<number id="3DFF24E7CA3B2C5EFC30FC44FB93FC2C" box="[929,1044,908,932]" pageId="5" pageNumber="56" value="21.299999999999997" valueMax="21.9" valueMin="20.7">20.7–21.9</number>
; pronotum length: male 
<number id="3DFF24E7CA3B2C5EFAD0FC44FA1FFC2C" box="[1345,1432,908,932]" pageId="5" pageNumber="56" value="3.25" valueMax="3.4" valueMin="3.1">3.1–3.4</number>
, female 
<number id="3DFF24E7CA3B2C5EFF7BFC67FEC6FC4F" box="[234,321,943,967]" pageId="5" pageNumber="56" value="4.4" valueMax="4.5" valueMin="4.3">4.3–4.5</number>
; tegmen length: male 
<number id="3DFF24E7CA3B2C5EFDA6FC67FD09FC4F" box="[567,654,943,967]" pageId="5" pageNumber="56" value="3.0" valueMax="3.3" valueMin="2.7">2.7–3.3</number>
, female 
<number id="3DFF24E7CA3B2C5EFD7DFC67FCC4FC4F" box="[748,835,943,967]" pageId="5" pageNumber="56" value="3.1500000000000004" valueMax="3.2" valueMin="3.1">3.1–3.2</number>
; hind femur length: male 
<number id="3DFF24E7CA3B2C5EFBF3FC67FB40FC4F" box="[1122,1223,943,967]" pageId="5" pageNumber="56" value="10.05" valueMax="10.4" valueMin="9.7">9.7–10.4</number>
