<typeStatus id="9D2D41C9813A0B31FF78C3CBFF7FD3C3" box="[151,202,1443,1464]" pageId="34" pageNumber="185">Type</typeStatus>
genus 
<taxonomicName id="859684E8813A0B31FEFEC3CAFE44D3C3" authority="Weber 1909" authorityName="Weber" authorityYear="1909" box="[273,497,1442,1464]" class="Actinopterygii" family="Apogonidae" genus="Siphamia" kingdom="Animalia" order="Perciformes" pageId="34" pageNumber="185" phylum="Chordata" rank="genus">
<emphasis id="70E22379813A0B31FEFEC3CAFEC4D3C3" box="[273,369,1442,1464]" italics="true" pageId="34" pageNumber="185">Siphamia</emphasis>
<bibRefCitation id="2607829A813A0B31FE98C3CAFE44D3C3" author="Weber, M." box="[375,497,1442,1464]" pageId="34" pageNumber="185" pagination="143 - 169" refId="ref36463" refString="Weber, M. (1909) Diagnosen neuer Fische der Siboga-Expedition. Notes from the Leyden Museum, 31, 143 - 169." type="journal article" year="1909">Weber 1909</bibRefCitation>
</taxonomicName>
