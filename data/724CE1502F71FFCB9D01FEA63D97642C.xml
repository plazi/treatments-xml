<collectionCode id="9CF4C8832F71FFCB9E27FE353B0562DE" LSID="urn:lsid:biocol.org:col:34838" box="[558,622,399,425]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="22" pageNumber="299">SMF</collectionCode>
