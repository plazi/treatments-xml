<quantity id="3D1DFDA32F71FFCB9F66FE0D3AA062A6" box="[879,971,439,465]" metricMagnitude="3" metricUnit="m" metricValue="3.05" pageId="22" pageNumber="299" unit="m" value="3050.0">3050 m</quantity>
