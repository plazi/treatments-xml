<emphasis id="C8918C542F71FFCB9D01F94638BA6463" bold="true" box="[264,465,1788,1812]" pageId="22" pageNumber="299">FIGURES 64–67.</emphasis>
<taxonomicName id="3DE52BC52F71FFCB9DD3F9473BD86463" box="[474,691,1789,1812]" class="Arachnida" family="Linyphiidae" genus="Taibainus" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="299" phylum="Arthropoda" rank="species" species="shanensis" status="gen. n.">
<emphasis id="C8918C542F71FFCB9DD3F9473BD86463" box="[474,691,1789,1812]" italics="true" pageId="22" pageNumber="299">Taibainus shanensis</emphasis>
</taxonomicName>
<emphasis id="C8918C542F71FFCB9EB5F9463A246463" bold="true" box="[700,847,1788,1812]" pageId="22" pageNumber="299">
<taxonomicNameLabel id="D3A2312F2F71FFCB9EB5F9463A636463" box="[700,776,1788,1812]" pageId="22" pageNumber="299" rank="genus">gen. n.</taxonomicNameLabel>
