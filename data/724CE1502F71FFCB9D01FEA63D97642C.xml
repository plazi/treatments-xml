<taxonomicName id="3DE52BC52F71FFCB9E60FE0D3A7D62A7" box="[617,790,439,464]" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" kingdom="Plantae" order="Ericales" pageId="22" pageNumber="299" phylum="Tracheophyta" rank="genus">
