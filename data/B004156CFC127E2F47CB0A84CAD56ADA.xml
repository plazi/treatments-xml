<taxonomicName id="6C8D3A4DFFF17C956A1F1130C36731F7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33034" class="Insecta" family="Formicidae" genus="Oecophylla" kingdom="Animalia" lsidName-HNS="Oecophylla longinoda (Latreille)" order="Hymenoptera" pageNumber="230" phylum="Arthropoda" rank="species" species="longinoda">Oecophylla longinoda (Latreille)</taxonomicName>
