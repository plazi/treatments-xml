</taxonomicName>
and 
<taxonomicName id="4C24B2AF7818714DFCBFBDED98E1F093" authority="Sartori &amp; Derleth, 2010" authorityName="Sartori &amp; Derleth" authorityYear="2010" box="[846,938,189,214]" class="Insecta" family="Leptophlebiidae" genus="Isca" kingdom="Animalia" order="Ephemeroptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="lea" status="sp. nov.">
