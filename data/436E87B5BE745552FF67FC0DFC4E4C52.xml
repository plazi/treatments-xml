</taxonomicName>
(
<taxonomicName id="0CC74D20BE745552FCBCFBD6FC5E4F46" box="[844,986,1111,1136]" class="Actinopterygii" family="Haemulidae" kingdom="Animalia" order="Perciformes" pageId="18" pageNumber="169" phylum="Chordata" rank="family">Haemulidae</taxonomicName>
