<taxonomicName id="0CC74D20BE745552FE21FBFCFDF54FA3" box="[465,625,1148,1173]" class="Trematoda" family="Opecoelidae" genus="Hamacreadium" kingdom="Animalia" order="Plagiorchiida" pageId="18" pageNumber="169" phylum="Platyhelminthes" rank="species" species="heterorchis">
