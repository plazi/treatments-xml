<collectionCode id="ED67FDF528564B02FEBDFE9BFEA0E658" box="[287,375,332,356]" httpUri="http://grbio.org/cool/6yis-bi95" name="Museo Nacional de Costa Rica" pageId="31" pageNumber="232">MNCR</collectionCode>
