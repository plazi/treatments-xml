<taxonomicName id="4C394D48F9058433FB4035F3B76BF897" authorityName="Broad &amp; Shaw" authorityYear="2016" baseAuthorityName="Haller" baseAuthorityYear="1885" box="[1268,1391,1784,1810]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="adustus">
<emphasis id="B94DEAD9F9058433FB4035F3B76BF897" box="[1268,1391,1784,1810]" italics="true" pageId="12" pageNumber="13">E. adustus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C394D48F9058433FF093414B35EF8B2" authorityName="Aubert" authorityYear="1966" box="[189,346,1821,1847]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="cerebrator">
<emphasis id="B94DEAD9F9058433FF093414B35EF8B2" box="[189,346,1821,1847]" italics="true" pageId="12" pageNumber="13">E. cerebrator</emphasis>
