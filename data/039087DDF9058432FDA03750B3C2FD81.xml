<taxonomicName id="4C394D48F9058433FE273414B02FF8B2" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[403,555,1821,1847]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="ramidulus">
