<collectionCode id="ED3AE29BCB46C277CD3AF903FC25C7DB" box="[859,941,1768,1791]" country="USA" httpUri="http://grbio.org/cool/kd99-229r" name="R. M. Bohart Museum of Entomology" pageId="90" pageNumber="91" type="Museum">UCDC</collectionCode>
