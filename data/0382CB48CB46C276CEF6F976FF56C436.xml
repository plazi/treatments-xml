<materialsCitation id="3B437003CB46C277CEF6F903FC30C7DB" box="[151,952,1768,1791]" collectionCode="UCDC" collectorName="E. A. Sugden" country="Costa Rica" location="Costa Rica" pageId="90" pageNumber="91" specimenCount="1" typeStatus="holotype">
