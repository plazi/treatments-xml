<taxonomicName id="4C794D05FFCBFFE6545BA3E2FB0755EA" authority="Bonaparte, 1850" authorityName="Bonaparte" authorityYear="1850" box="[856,1186,1236,1263]" class="Amphibia" family="Pelobatidae" kingdom="Animalia" order="Anura" pageId="14" pageNumber="745" phylum="Chordata" rank="family">PELOBATIDAE Bonaparte, 1850</taxonomicName>
