<taxonomicName id="66AB4D7AFFAFFFBF8DCBFBE8FEFAA745" authority="(Raddi)" baseAuthorityName="Raddi" baseAuthorityYear="1823" class="Amphibia" family="Ceratophryidae" genus="Ceratophrys" kingdom="Animalia" order="Anura" pageId="75" pageNumber="76" phylum="Chordata" rank="species" species="aurita">
