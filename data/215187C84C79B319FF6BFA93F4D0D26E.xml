<taxonomicName id="6EF84D5D4C79B319FD7FF91AF445D381" authorityName="Erichson" authorityYear="1840" box="[643,784,1742,1765]" class="Insecta" family="Staphylinidae" genus="Philonthus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="pruinosus">
