<taxonomicName id="70CA4D3CFF45E2E18155FB1CD9F061C1" box="[1050,1278,1231,1256]" class="Magnoliopsida" family="Rhizophoraceae" genus="Rhizophora" kingdom="Plantae" order="Malpighiales" pageId="168" pageNumber="169" phylum="Tracheophyta" rank="species" species="mangle">
