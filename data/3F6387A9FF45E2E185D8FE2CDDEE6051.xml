<quantity id="70329B5AFF45E2E18174FB54D9636189" box="[1083,1133,1159,1184]" metricMagnitude="1" metricUnit="m" metricValue="7.5" pageId="168" pageNumber="169" unit="m" value="75.0">75m</quantity>
