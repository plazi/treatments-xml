<paragraph id="8BF536EDFF9D97346DADFA34EF11AFC5" blockId="17.[151,586,1418,1473]" box="[151,586,1450,1473]" pageId="17" pageNumber="18">
<treatmentCitationGroup id="AB5A11C3FF9D97346DADFA34EF11AFC5" box="[151,586,1450,1473]" pageId="17" pageNumber="18">
<treatmentCitation id="0AEB10FCFF9D97346DADFA34EF1DAFC5" author="Foote, R. H." box="[151,582,1450,1473]" page="246" pageId="17" pageNumber="18" year="1965">
