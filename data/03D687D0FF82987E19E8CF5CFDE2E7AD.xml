<taxonomicName id="4C7F4D45FF82987E1991CF5CFE52E420" authority="Vayssiere" authorityName="Vayssiere" box="[238,480,1285,1312]" family="Monophlebidae" genus="Vrydahga" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="337" rank="genus">
