<taxonomicName id="37BEEB8CB6C5036BA4754BB050971A50" class="Insecta" family="Braconidae" genus="Philoplitis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Philoplitis adustipalpus" order="Hymenoptera" pageId="4" pageNumber="129" phylum="Arthropoda" rank="species" species="adustipalpus">Philoplitis adustipalpus</taxonomicName>
