<paragraph id="61E72AEFC071B897BB6065B9B96C3711" pageId="14" pageNumber="47">
<taxonomicName id="FD1B1F43688B59A9ADC0C9CF3BA88913" authorityName="H. Magn" authorityYear="1935" class="Lecanoromycetes" family="Acarosporaceae" genus="Sarcogyne" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Sarcogyne algoviae" order="Acarosporales" pageId="14" pageNumber="47" phylum="Ascomycota" rank="species" species="algoviae">Sarcogyne algoviae</taxonomicName>
