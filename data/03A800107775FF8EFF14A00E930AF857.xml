<geoCoordinate id="EE35D7C17775FF8EFF6DA6479112FADF" box="[238,367,1324,1349]" direction="west" orientation="longitude" pageId="34" pageNumber="35" precision="5" value="-56.7933">56.7933°W</geoCoordinate>
, depth 
<quantity id="4CF91CE37775FF8EFE3CA6479186FADE" box="[447,507,1324,1349]" metricMagnitude="1" metricUnit="m" metricValue="6.4" pageId="34" pageNumber="35" unit="m" value="64.0">
<elevation id="002C56357775FF8EFE3CA6479186FADE" box="[447,507,1324,1349]" metricMagnitude="1" metricUnit="m" metricValue="6.4" pageId="34" pageNumber="35" unit="m" value="64.0">64 m</elevation>
</quantity>
, bottom sand, 
<date id="FFBF97C67775FF8EFD1DA6469354FADF" box="[670,809,1324,1349]" pageId="34" pageNumber="35" value="1966-05-07">
<collectingDate id="EFFB6E2E7775FF8EFD1DA6469354FADF" box="[670,809,1324,1349]" pageId="34" pageNumber="35" value="1966-05-07">7 May 1966</collectingDate>
