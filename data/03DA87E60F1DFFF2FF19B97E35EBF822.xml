<taxonomicName id="4C734D730F1DFFF2FF19BE3F3646FB83" authority=": Cerretti et al. (2017" authorityName=": Cerretti et al." authorityYear="2017" box="[151,618,1048,1073]" class="Insecta" family="Calliphoridae" genus="Mesembrinella" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="patriciae">
