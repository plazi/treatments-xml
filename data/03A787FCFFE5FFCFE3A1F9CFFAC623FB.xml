<taxonomicName id="4C0E4D69FFE5FFCEE1FAF882FC80245F" authorityName="P.Miller" authorityYear="1754" box="[716,786,1881,1907]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="genus">
<emphasis id="B97AEAF8FFE5FFCEE1FAF882FC80245F" box="[716,786,1881,1907]" italics="true" pageId="12" pageNumber="13">Malus</emphasis>
</taxonomicName>
