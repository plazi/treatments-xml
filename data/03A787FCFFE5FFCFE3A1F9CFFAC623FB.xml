<taxonomicName id="4C0E4D69FFE5FFCEE07AF882FC2B245F" box="[844,953,1881,1907]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
