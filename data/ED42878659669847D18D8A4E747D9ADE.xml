<taxonomicName id="A2EB4D1359669847D2118F7D711E9A35" authorityName="Newport" authorityYear="1844" box="[779,1084,1117,1143]" class="Chilopoda" family="Scolopendridae" genus="Cormocephalus" kingdom="Animalia" order="Scolopendromorpha" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="westwoodi">
<emphasis id="579FEA8259669847D2118F7D711E9A35" box="[779,1084,1117,1143]" italics="true" pageId="51" pageNumber="52">Cormocephalus westwoodi</emphasis>
</taxonomicName>
