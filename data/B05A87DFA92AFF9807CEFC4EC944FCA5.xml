<taxonomicName id="FFF34D4AA92AFF86066FFBAACA02FBAF" authority="Frey &amp; Boll 1873: 218" authorityName="Frey &amp; Boll" authorityPageNumber="218" authorityYear="1873" box="[310,635,1085,1110]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="gemmea">
<emphasis id="0A87EADBA92AFF86066FFBAAC9F3FBAF" box="[310,394,1086,1109]" italics="true" pageId="14" pageNumber="215">gemmea</emphasis>
<bibRefCitation id="5C624B38A92AFF8606C8FBAACA02FBAF" author="Frey, H. &amp; Boll, J." box="[401,635,1085,1110]" pageId="14" pageNumber="215" pagination="201 - 224" refId="ref14038" refString="Frey, H. &amp; Boll, J. (1873) Nordamerikanische Tineen. Entomologische Zeitung, Stettin, 34 (4 - 6), 201 - 224." type="journal article" year="1873">Frey &amp; Boll 1873: 218</bibRefCitation>
</taxonomicName>
. 
<taxonomicName id="FFF34D4AA92AFF8607CEFBC8CA63FB8E" authority="(Frey &amp; Boll)" baseAuthorityName="Frey &amp; Boll" baseAuthorityYear="1873" box="[151,538,1115,1140]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="gemmea">
