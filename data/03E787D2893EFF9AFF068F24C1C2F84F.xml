<specimenCode id="DBE89EBF893EFF9AFD548C1BC110F9D7" box="[709,840,1632,1658]" collectionCode="ZMB" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="56" pageNumber="57" type="Museum">ZMB 4148</specimenCode>
