<date id="FFF01004893EFF9AFD498CF3C109F90F" box="[728,849,1672,1698]" pageId="56" pageNumber="57" value="1867-07">
<collectingDate id="EFB4E9EC893EFF9AFD498CF3C109F90F" box="[728,849,1672,1698]" pageId="56" pageNumber="57" value="1867-07">July 1867</collectingDate>
