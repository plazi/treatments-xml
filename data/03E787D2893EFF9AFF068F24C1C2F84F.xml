<collectingRegion id="498AF826893EFF9AFE948CF3C30DF90F" box="[261,341,1672,1698]" country="China" name="Fujian" pageId="56" pageNumber="57">Fujian</collectingRegion>
), 
<collectingCountry id="F3597654893EFF9AFEFD8CF3C3EEF90F" box="[364,438,1672,1698]" name="China" pageId="56" pageNumber="57">China</collectingCountry>
