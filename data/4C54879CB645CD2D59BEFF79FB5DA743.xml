<collectingCountry id="BCEA761AB645CD2D5A20FDF0FCABA66B" box="[777,862,528,554]" name="Bolivia" pageId="24" pageNumber="25">Bolivia</collectingCountry>
, 
<collectingCountry id="BCEA761AB645CD2D5A42FDF0FC47A66B" box="[875,946,528,554]" name="Brazil" pageId="24" pageNumber="25">Brazil</collectingCountry>
, 
<collectingCountry id="BCEA761AB645CD2D5AE9FDF0FB9FA66B" box="[960,1130,528,554]" name="French Guiana" pageId="24" pageNumber="25">French Guiana</collectingCountry>
