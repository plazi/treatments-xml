<collectingCountry id="F347B9D1FFB3C5614561D69815A2F909" box="[1108,1196,1734,1758]" name="Mexico" pageId="12" pageNumber="565">Mexico</collectingCountry>
, 
<collectingCountry id="F347B9D1FFB3C561458CD699141BF909" box="[1209,1301,1735,1758]" name="Panama" pageId="12" pageNumber="565">Panama</collectingCountry>
, 
<collectingCountry id="F347B9D1FFB3C5614417D6981496F909" box="[1314,1432,1734,1758]" name="Venezuela" pageId="12" pageNumber="565">Venezuela</collectingCountry>
