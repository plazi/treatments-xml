<taxonomicName id="4C364D30675CFF874C36FDBCFB66FD6A" authority="Craven, 1880" authorityName="Craven" authorityYear="1880" box="[847,1220,633,659]" class="Magnoliopsida" family="Salicaceae" genus="Helix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="symmetrica">
