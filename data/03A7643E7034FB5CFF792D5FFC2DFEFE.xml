<number id="3DC6C70F7034FB5DFBDB2A24FB44FA39" box="[1077,1174,1390,1415]" pageId="8" pageNumber="9" value="337.5" valueMax="364.0" valueMin="311.0">311–364</number>
(n= 
<number id="3DC6C70F7034FB5DFB232A25FB09FA39" box="[1229,1243,1391,1415]" pageId="8" pageNumber="9" value="3.0">3</number>
