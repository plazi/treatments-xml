<taxonomicName id="4C1E62E0FFA6FF98FDB191ABFC21F874" authority="Cockerell, 1897" authorityName="Cockerell" authorityYear="1897" box="[564,916,2001,2027]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Hemihalictus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
