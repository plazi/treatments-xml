<taxonomicName id="4C1E62E0FFA6FF98FD5891D6FBAFF859" authority="Robertson, 1902" authorityName="Robertson" authorityYear="1902" box="[733,1050,1964,1990]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="subGenus" subGenus="Evylaeus">
