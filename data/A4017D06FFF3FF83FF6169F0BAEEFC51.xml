<quantity id="EB5061F5FFFDFF80FD116B8DBBD4F860" box="[743,909,1986,2014]" metricMagnitude="-3" metricUnit="m" metricValue="3.585" metricValueMax="3.61" metricValueMin="3.56" pageId="41" pageNumber="42" unit="mm" value="3.585" valueMax="3.61" valueMin="3.56">3.56–3.61 mm</quantity>
