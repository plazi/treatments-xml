<quantity id="EB5061F5FFFDFF80FECF6B8DB986F860" box="[313,479,1986,2014]" metricMagnitude="-4" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="41" pageNumber="42" unit="mm" value="0.14" valueMax="0.15" valueMin="0.13">0.13–0.15 mm</quantity>
