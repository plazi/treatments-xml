<taxonomicName id="945715D1773A44213EAA9DE4E298F130" class="Insecta" family="Ichneumonidae" genus="Xorides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xorides" order="Hymenoptera" pageId="0" pageNumber="39" phylum="Arthropoda" rank="genus">Xorides</taxonomicName>
