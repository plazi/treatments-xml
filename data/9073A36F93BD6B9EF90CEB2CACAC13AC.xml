<taxonomicName id="AC5F1B6EBECC771BC87137B0412EFA87" class="Insecta" family="Eulophidae" genus="Ichneumon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ichneumon indicatorius" order="Hymenoptera" pageId="0" pageNumber="39" phylum="Arthropoda" rank="species" species="indicatorius">Ichneumon indicatorius</taxonomicName>
