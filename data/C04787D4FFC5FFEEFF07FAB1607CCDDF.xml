<taxonomicName id="8FEE4D41FFC5FFEEFBA5F9FD619FCD33" authorityName="Emel'yanova &amp; Kozlovskaya" authorityYear="1967" box="[1077,1244,1626,1652]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="maslovi">
