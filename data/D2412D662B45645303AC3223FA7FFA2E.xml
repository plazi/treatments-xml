<taxonomicName id="9DE8E7F32B45645301363222FD47FBD7" authorityName="Gorochov" authorityYear="1986" box="[525,676,1086,1112]" class="Insecta" family="Phalangopsidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="414" phylum="Arthropoda" rank="tribe" tribe="Luzaropsini">Luzaropsini</taxonomicName>
