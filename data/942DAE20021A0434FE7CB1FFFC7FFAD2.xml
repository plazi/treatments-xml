<taxonomicName id="DB8464B5021A0434FE21B1FFFD69FC53" authority="Kissinger" authorityName="Kissinger" authorityYear="2005" box="[465,669,941,962]" class="Insecta" family="Brentidae" genus="Acarapion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="81" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="2EF0C324021A0434FE21B1FFFDCDFC53" bold="true" box="[465,569,941,962]" italics="true" pageId="10" pageNumber="81">Acarapion</emphasis>
Kissinger
</taxonomicName>
, 
<taxonomicNameLabel id="35C37E5F021A0434FD59B1FFFCE4FC53" box="[681,784,941,962]" pageId="10" pageNumber="81" rank="genus">new genus</taxonomicNameLabel>
