<specimenCount id="9D2EFD71FF98FFA2FD0F7ADFE48666A8" box="[737,765,975,996]" pageId="12" pageNumber="1599" type="female">1♀</specimenCount>
, same data as 
<typeStatus id="5493885AFF98FFA2FC6F7ADEE5AB66A8" box="[897,976,974,996]" pageId="12" pageNumber="1599">holotype</typeStatus>
