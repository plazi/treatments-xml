<emphasis id="B923EAC95C5CFF8FFF6AF9A0FE5CF9D3" bold="true" box="[197,438,1605,1631]" pageId="21" pageNumber="22">Geographical data.</emphasis>
<materialsCitation id="3B3F3C865C5CFF8FFE11F9A0FEEFF908" country="Brazil" location="Tocantins State" pageId="21" pageNumber="22" specimenCount="1" stateProvince="Tocantins" typeStatus="paratype">
