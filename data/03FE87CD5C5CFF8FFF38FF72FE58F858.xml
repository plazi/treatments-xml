from 
<collectingRegion id="4993F8395C5CFF8FFBBAF9A0FB27F9D3" box="[1045,1229,1605,1631]" country="Brazil" name="Tocantins" pageId="21" pageNumber="22">Tocantins State</collectingRegion>
, 
<collectingCountry id="F340764B5C5CFF8FFB75F9A0FACFF9D3" box="[1242,1317,1605,1631]" name="Brazil" pageId="21" pageNumber="22">Brazil</collectingCountry>
