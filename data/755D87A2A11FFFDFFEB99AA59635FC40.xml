, J 
<number id="4B3C2493A11FFFDFFC2C991697D2F800" box="[925,971,599,625]" pageId="61" pageNumber="62" value="107.0">107</number>
, 
<accessionNumber id="E2A7AB57A11FFFDFFC6E99169024F800" box="[991,1085,599,625]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/Z11229" pageId="61" pageNumber="62">Z11229</accessionNumber>
, 
