<taxonomicName id="9E70C502DB3BBE9177DBC2217D0FB98F" class="Hexanauplia" family="Laophontidae" genus="Folioquinpes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Folioquinpes mangalis" order="Harpacticoida" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="mangalis">Folioquinpes mangalis</taxonomicName>
