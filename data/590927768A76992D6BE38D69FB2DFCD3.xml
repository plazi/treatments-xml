<collectingCountry id="A9B7D6F08A76992D69C48F53FCF0FCD3" box="[688,823,837,863]" name="Micronesia" pageId="3" pageNumber="16">Micronesia</collectingCountry>
, 
<collectingCountry id="A9B7D6F08A76992D68378F53FC40FCD3" box="[835,903,837,863]" name="Palau" pageId="3" pageNumber="16">Palau</collectingCountry>
, 
<collectingCountry id="A9B7D6F08A76992D68E78F53FBCFFCD3" box="[915,1032,837,863]" name="Indonesia" pageId="3" pageNumber="16">Indonesia</collectingCountry>
, Marianas and 
<collectingCountry id="A9B7D6F08A76992D6FCD8F53FB21FCD3" box="[1209,1254,837,863]" name="Fiji" pageId="3" pageNumber="16">Fiji</collectingCountry>
