<taxonomicName id="4C7E4D6C3568FFE619E2EF591235E355" authority="Blainville, 1824" authorityName="Blainville" authorityYear="1824" class="Gastropoda" family="Lymnaeidae" genus="Limnaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hygrophila" pageId="11" pageNumber="56" phylum="Mollusca" rank="species" species="truncatula">
