</taxonomicName>
and 
<taxonomicName id="4C7E4D6C356AFFE41FD3E91C162FE5B4" authority="(VinarSki et al. 2013)" authorityName="VinarSki et al." authorityYear="2013" box="[949,1302,1900,1925]" class="Gastropoda" family="Lymnaeidae" genus="Lymnaea" kingdom="Animalia" order="Hygrophila" pageId="9" pageNumber="54" phylum="Mollusca" rank="genus">
<emphasis id="B90AEAFD356AFFE41FD3E91C1722E5B4" box="[949,1051,1901,1924]" italics="true" pageId="9" pageNumber="54">Lymnaea</emphasis>
(VinarSki 
<emphasis id="B90AEAFD356AFFE418F4E91C17FAE5B5" box="[1170,1219,1900,1925]" italics="true" pageId="9" pageNumber="54">et al</emphasis>
. 2013)
</taxonomicName>
