<emphasis id="CA93EB24FFF40C01077DD1DAFA14FF50" bold="true" box="[1299,1436,210,232]" pageId="2" pageNumber="136">
<taxonomicName id="3FE74CB5FFF40C01077DD1DAFAF1FF5F" authorityName="Enock" authorityYear="1909" box="[1299,1401,210,232]" class="Insecta" family="Mymaridae" genus="Cleruchus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="136" phylum="Arthropoda" rank="genus">
