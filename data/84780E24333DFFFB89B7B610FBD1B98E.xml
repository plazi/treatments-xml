<taxonomicName id="CBD1C4B1333DFFFB89B7B687FDB8BEEC" authority="Disney, 2000:91" authorityName="Disney" authorityPageNumber="91" authorityYear="2000" box="[151,551,929,951]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="312" phylum="Arthropoda" rank="species" species="modesta">
