<subSubSection id="3EE6638CFFA8FFE1FF42DFA8FBB50BCF" box="[199,1097,704,729]" pageId="5" pageNumber="176" type="etymology">
<paragraph id="76433007FFA8FFE1FF42DFA8FBB50BCF" blockId="5.[151,1437,598,2031]" box="[199,1097,704,729]" pageId="5" pageNumber="176">
<emphasis id="4488EC15FFA8FFE1FF42DFA8FEB20BCF" bold="true" box="[199,334,704,729]" pageId="5" pageNumber="176">Etymology.</emphasis>
