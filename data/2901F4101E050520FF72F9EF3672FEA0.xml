<th id="5C492E381E07FAD8FF72FB4C366EFB75" box="[151,555,1247,1271]" gridcol="0" gridrow="0" pageId="6" pageNumber="51">Character</th>
<th id="5C492E381E07FAD8FD6FFB4C3752FB75" box="[650,791,1247,1271]" gridcol="1" gridrow="0" pageId="6" pageNumber="51">
<taxonomicName id="66A83E851E070521FD6FFB733752FB75" box="[650,791,1248,1271]" class="Entognatha" family="Isotomidae" genus="Narynia" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="51" phylum="Arthropoda" rank="species" species="andreevae">
<emphasis id="93DC99141E070521FD6FFB733752FB75" box="[650,791,1248,1271]" italics="true" pageId="6" pageNumber="51">
N. 
<taxonomicName id="66A83E851E070521FD4CFB733752FB75" box="[681,791,1248,1271]" pageId="6" pageNumber="51" rank="species" species="andreevae">andreevae</taxonomicName>
</emphasis>
</taxonomicName>
</th>
<th id="5C492E381E07FAD8FCB6FB4C30C1FB75" box="[851,1156,1247,1271]" gridcol="2" gridrow="0" pageId="6" pageNumber="51">
<emphasis id="93DC99141E070521FCB6FB733031FB75" box="[851,1140,1248,1271]" italics="true" pageId="6" pageNumber="51">
<taxonomicName id="66A83E851E070521FCB6FB7337AFFB75" box="[851,1002,1248,1271]" class="Entognatha" family="Isotomidae" genus="Narynia" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="51" phylum="Arthropoda" rank="species" species="kolimiensis">
N. 
<taxonomicName id="66A83E851E070521FC97FB7337AFFB75" box="[882,1002,1248,1271]" pageId="6" pageNumber="51" rank="species" species="kolimiensis">kolimiensis</taxonomicName>
</taxonomicName>
<taxonomicName id="66A83E851E070521FBF7FB733031FB75" box="[1042,1140,1248,1271]" class="Entognatha" family="Isotomidae" genus="Narynia" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="51" phylum="Arthropoda" rank="species" species="setosa">
N. 
<taxonomicName id="66A83E851E070521FBD4FB733031FB75" box="[1073,1140,1248,1271]" pageId="6" pageNumber="51" rank="species" species="setosa">setosa</taxonomicName>
</taxonomicName>
</emphasis>
