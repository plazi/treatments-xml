<taxonomicName id="3543B8BF85E8ED0A01F646F3725C80FF" class="Insecta" family="Carabidae" genus="Bembidium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="410" pageNumber="411" phylum="Arthropoda" rank="species" species="fortestriatum">
