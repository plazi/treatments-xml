<treatment id="166C0B45FF9FFFF8FC26FDA05580F99B" LSID="urn:lsid:plazi:treatment:166C0B45FF9FFFF8FC26FDA05580F99B" httpUri="http://treatment.plazi.org/id/166C0B45FF9FFFF8FC26FDA05580F99B" lastPageNumber="6" pageId="5" pageNumber="6">
