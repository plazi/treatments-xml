nities. 
<taxonomicName id="47A34D37FFA09B75652C459DFE30A3CD" authority="Urhan, 2002" authorityName="Urhan" authorityYear="2002" box="[292,502,383,407]" class="Arachnida" family="Zerconidae" genus="Prozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="2131" phylum="Arthropoda" rank="species" species="denizliensis" status="sp. nov." subGenus="Plumatozercon">P. (P.) denizliensis</taxonomicName>
