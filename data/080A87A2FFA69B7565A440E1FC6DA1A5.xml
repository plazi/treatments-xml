<taxonomicName id="47A34D37FFA09B75673A459DFE87A3ED" authority="(Halbert, 1923)" baseAuthorityName="Halbert" baseAuthorityYear="1923" class="Arachnida" family="Zerconidae" genus="Prozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="2131" phylum="Arthropoda" rank="species" species="tragardhi" subGenus="Plumatozercon">
