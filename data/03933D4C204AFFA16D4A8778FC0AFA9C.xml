<quantity id="4CC221BF204BFFA16A72846EFAADFDB5" box="[1295,1399,541,567]" metricMagnitude="-4" metricUnit="m" metricValue="7.7" pageId="19" pageNumber="20" unit="mm" value="0.77">0.77 mm</quantity>
