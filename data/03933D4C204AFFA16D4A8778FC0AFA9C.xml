<potCollectionCode id="596ED254204BFFA16E6C8431FEEDFDDE" box="[273,311,578,604]" pageId="19" pageNumber="20">GL</potCollectionCode>
/ 
<potCollectionCode id="596ED254204BFFA16E428431FEBCFDDE" box="[319,358,578,604]" pageId="19" pageNumber="20">AL</potCollectionCode>
