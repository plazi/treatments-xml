<potCollectionCode id="596ED254204BFFA16C228431FC58FDDE" box="[863,898,578,604]" pageId="19" pageNumber="20">LL</potCollectionCode>
<quantity id="4CC221BF204BFFA16CF48431FC2EFDDE" box="[905,1012,578,604]" metricMagnitude="-4" metricUnit="m" metricValue="3.7" pageId="19" pageNumber="20" unit="mm" value="0.37">0.37 mm</quantity>
