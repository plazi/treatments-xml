<subSubSection id="12A930D8214DC67305D2FE4873957104" pageId="1" pageNumber="57" type="materials_examined">
<paragraph id="5A0C6353214DC67305D2FE48752C704D" blockId="1.[151,1437,506,1064]" box="[151,805,506,532]" pageId="1" pageNumber="57">
