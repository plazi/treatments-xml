<collectionCode id="ADBFAE13A833FFECFCF1FC76FC060AA2" box="[868,955,931,952]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="43" pageNumber="43" type="Museum">USNM</collectionCode>
