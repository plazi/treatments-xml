<location id="CE71600DA833FFECFCA2FC15FCCD0ACF" LSID="urn:lsid:plazi:treatment:430787C0A833FFECFCB9FE48FB000D9F:CE71600DA833FFECFCA2FC15FCCD0ACF" box="[823,880,960,981]" municipality="Ridgway" name="Baur" pageId="43" pageNumber="43">Baur</location>
