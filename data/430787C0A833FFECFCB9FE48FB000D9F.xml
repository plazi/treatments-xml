<specimenCode id="9B089EADA833FFECFC81FB87FB930D7D" box="[788,1070,1106,1127]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="43" pageNumber="43">AMNH 522554–522556</specimenCode>
