<taxonomicName id="0CAE4D55A833FFECFC83FBC2FCDF0D34" authorityName="Ridgway" authorityYear="1894" box="[790,866,1047,1070]" class="Aves" family="Thraupidae" genus="Certhidea" kingdom="Animalia" order="Passeriformes" pageId="43" pageNumber="43" phylum="Chordata" rank="species" species="luteola">
<emphasis id="F9DAEAC4A833FFECFC83FBC2FCDF0D34" box="[790,866,1047,1070]" italics="true" pageId="43" pageNumber="43">luteola</emphasis>
</taxonomicName>
