in 
<collectionCode id="ADBFAE13A833FFECFCD9FBE0FC160D50" box="[844,939,1077,1098]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="43" pageNumber="43">AMNH</collectionCode>
