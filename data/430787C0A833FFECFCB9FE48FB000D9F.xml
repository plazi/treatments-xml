<materialsCitation id="7BC63C8BA833FFECFCE7FBCDFB040D9F" collectionCode="AMNH" country="Saint Kitts and Nevis" location="Baur" pageId="43" pageNumber="43" specimenCode="AMNH 522541-522544, AMNH 522554-522556" specimenCount="1" typeStatus="paratype">
