<taxonomicName id="4BA34D04FFC1FFD85F1816EB4F8EB851" box="[583,759,764,788]" class="Insecta" family="Staphylinidae" genus="Stenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="267" phylum="Arthropoda" rank="species" species="cylindricollis">
