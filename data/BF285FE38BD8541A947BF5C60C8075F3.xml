<taxonomicName id="BD234620413CCFB97A090B96EFA6E4CB" LSID="http://zoobank.org/99E34843-09D9-4EBB-8EB4-42860C733415" authority="Awad" authorityName="Awad" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Synopeas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synopeas klingunculum" order="Hymenoptera" pageId="0" pageNumber="5" phylum="Arthropoda" rank="species" species="klingunculum" status="sp. nov.">Synopeas klingunculum Awad</taxonomicName>
