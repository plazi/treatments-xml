<paragraph id="2C2936F8DE1DFFEF85DFFE1C79CAFDFD" blockId="25.[160,1156,496,521]" box="[160,1156,496,521]" pageId="25" pageNumber="1537">
<emphasis id="1EE2EAEADE1DFFEF85DFFE1C7DB6FDFD" bold="true" box="[160,248,496,521]" inLineHeading="true" italics="true" pageId="25" pageNumber="1537" reason="3">Length.</emphasis>
Male 
<typeStatus id="F32D885ADE1DFFEF8439FE1D7CE3FDFD" box="[326,429,497,521]" pageId="25" pageNumber="1537">holotype</typeStatus>
, 
<quantity id="EB6E9B1DDE1DFFEF84C8FE1D7F41FDFD" box="[439,527,497,521]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="25" pageNumber="1537" unit="mm" value="5.9">5.9 mm</quantity>
