<typeStatus id="F32D885ADE1DFFEF86C7FE1D7950FDFD" box="[952,1054,497,521]" pageId="25" pageNumber="1537">paratype</typeStatus>
, 
<quantity id="EB6E9B1DDE1DFFEF8156FE1D79CEFDFD" box="[1065,1152,497,521]" metricMagnitude="-3" metricUnit="m" metricValue="6.3" pageId="25" pageNumber="1537" unit="mm" value="6.3">6.3 mm</quantity>
