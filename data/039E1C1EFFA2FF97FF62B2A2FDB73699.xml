(
<emphasis id="B943711AFFA2FF97FEE5B56BFE833660" box="[272,300,1087,1112]" italics="true" pageId="3" pageNumber="104">H.</emphasis>
) 
<emphasis id="B943711AFFA2FF97FECEB56AFE023660" box="[315,429,1086,1112]" italics="true" pageId="3" pageNumber="104">hentscheli</emphasis>
</taxonomicName>
