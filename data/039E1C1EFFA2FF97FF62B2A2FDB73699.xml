<quantity id="4CCF00EDFFA2FF97FC3EB536FBAB3645" box="[971,1028,1122,1149]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="104" unit="m" value="20.0">20 m</quantity>
to 
<quantity id="4CCF00EDFFA2FF97FBD2B536FBC13645" box="[1063,1134,1122,1149]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="104" unit="m" value="600.0">600 m</quantity>
depth (
<bibRefCitation id="EFA6D0F9FFA2FF97FB35B536FAD43644" author="Lohmann, H." box="[1216,1403,1122,1148]" pageId="3" pageNumber="104" pagination="1 - 14" refId="ref17654" refString="Lohmann, H. (1907 a) Uber einige faunistische Ergebnisse der Deutschen Sudpolar-Expedition unter besonderer Berucksichtigung der Meeresmilben. Schriften des Naturwissenchaftlichen Vereins fur Schleswig-Holstein, 14, 1 - 14." type="journal article" year="1907">Lohmann 1907a</bibRefCitation>
, b; Newell 1984; 
<bibRefCitation id="EFA6D0F9FFA2FF97FE90B5D2FDA43699" author="Bartsch, I." box="[357,523,1158,1185]" pageId="3" pageNumber="104" pagination="45 - 58" refId="ref13163" refString="Bartsch, I. (1993 b) Halacaridae (Acari) von Malaysia. Beschreibung von drei Arten der Gattung Copidognathus. Entomologische Mitteilungen aus dem Zoologischen Museum Hamburg, 11, 45 - 58." type="journal article" year="1993">Bartsch 1993b</bibRefCitation>
