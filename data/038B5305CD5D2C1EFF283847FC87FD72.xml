<typeStatus id="54995CB1CD522C1EFDDF3CF7FD4CFD72" box="[608,656,728,752]" pageId="9" pageNumber="36">type</typeStatus>
locality (
<figureCitation id="1319FE96CD522C1EFD453CF8FC93FD72" box="[762,847,727,752]" captionStart="FIGURE 65" captionStartId="16.[151,250,1945,1967]" captionTargetBox="[225,1342,1239,1915]" captionTargetId="figure@16.[225,1342,1239,1915]" captionTargetPageId="16" captionText="FIGURE 65. Distributional records for H. taczanowskii (yellow square), H. tridactylus sp. nov. (red circle), H. poseidon sp. nov. (black circle), H. femoratus sp. nov. (black triangle), H. terraemediae sp. nov. (orange pentagon)." httpUri="https://zenodo.org/record/288651/files/figure.png" pageId="9" pageNumber="36">
Fig. 
<number id="3DEAF034CD522C1EFC903CF8FC93FD72" box="[815,847,727,752]" pageId="9" pageNumber="36" value="65.0">65</number>
