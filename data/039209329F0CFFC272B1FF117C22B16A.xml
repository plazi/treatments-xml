<number id="3DF3AA039F0CFFC271EBFB007AE6B3F2" box="[973,1010,1158,1182]" pageId="12" pageNumber="65" value="1.1">1.1</number>
, 
<number id="3DF3AA039F0CFFC271D8FB017D07B3F3" box="[1022,1043,1159,1183]" pageId="12" pageNumber="65" value="3.0">3</number>
<number id="3DF3AA039F0CFFC2763FFB007D2BB3F2" box="[1049,1087,1158,1182]" pageId="12" pageNumber="65" value="1.2">1.2</number>
; forewing length: Ƥ 
<number id="3DF3AA039F0CFFC27703FB007C5EB3F2" box="[1317,1354,1158,1182]" pageId="12" pageNumber="65" value="6.5">6.5</number>
, 
<number id="3DF3AA039F0CFFC27770FB017C7FB3F3" box="[1366,1387,1159,1183]" pageId="12" pageNumber="65" value="3.0">3</number>
