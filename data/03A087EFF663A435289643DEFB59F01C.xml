<taxonomicName id="4C094D7AF663A4352CA840EBFAFAF7B3" baseAuthorityName="Galiano" baseAuthorityYear="1986" box="[1193,1317,715,741]" class="Arachnida" family="Salticidae" genus="Peckhamia" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="156" phylum="Arthropoda" rank="genus">
