<paragraph id="4A65369F771EEC1D6D0CFCBEFDF657D1" blockId="8.[264,1325,703,969]" pageId="8" pageNumber="9">
<taxonomicName id="8DDA4D1C771EEC1D6D0CFCBEFD965748" box="[312,628,823,848]" class="Insecta" family="Hoplopleuridae" genus="Hoplopleura" kingdom="Animalia" order="Phthiraptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="hesperomydis">
