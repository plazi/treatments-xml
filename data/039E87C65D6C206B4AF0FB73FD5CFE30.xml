<taxonomicName id="4C374D535D6D206B4B4CFE74FE28FE30" authority="Cao &amp; Yin, 2008" authorityName="Cao &amp; Yin" authorityYear="2008" box="[299,477,404,428]" class="Insecta" family="Pamphagidae" genus="Sinohaplotropis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="30" phylum="Arthropoda" rank="genus">
