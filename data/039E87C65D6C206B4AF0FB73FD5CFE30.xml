. 
<emphasis id="B943EAC25D6D206B4955FE90FC53FE14" box="[818,934,368,392]" italics="true" pageId="3" pageNumber="30">elunchuna</emphasis>
</taxonomicName>
conspecific with 
<taxonomicName id="4C374D535D6D206B4E0BFE91FAEFFE14" box="[1132,1306,368,392]" class="Insecta" family="Pamphagidae" genus="Haplotropis" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="30" phylum="Arthropoda" rank="species" species="brunneriana">
