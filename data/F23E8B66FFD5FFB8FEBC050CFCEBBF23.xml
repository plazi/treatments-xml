<subSubSection id="328D69FBFFD5FFB8FEBC058CFCEBBF23" pageId="16" pageNumber="17" type="etymology">
<paragraph id="7A283A70FFD5FFB8FEBC058CFCB6BF53" blockId="16.[264,1323,1241,1387]" pageId="16" pageNumber="17">
<emphasis id="48E3E662FFD5FFB8FEBC058CFE16BEBB" bold="true" box="[264,405,1241,1267]" pageId="16" pageNumber="17">Etymology.</emphasis>
<emphasis id="48E3E662FFD5FFB8FE14058CFE7BBEBA" box="[416,504,1241,1266]" italics="true" pageId="16" pageNumber="17">spiralis</emphasis>
