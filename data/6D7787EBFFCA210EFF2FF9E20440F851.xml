<taxonomicName id="22DE4D7EFFCA210EFDB0F8F505E0F899" box="[535,686,1853,1879]" class="Magnoliopsida" family="Oleaceae" genus="Chionanthus" kingdom="Plantae" order="Lamiales" pageId="7" pageNumber="174" phylum="Tracheophyta" rank="species" species="macrothyrsus">
