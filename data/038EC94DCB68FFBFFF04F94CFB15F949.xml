<date id="FF995E9BCB68FFBEFA93FA64FAB8F9E3" box="[1280,1397,1555,1579]" pageId="10" pageNumber="332" value="2014-11-02">2/XI/2014</date>
, 4‒ 
<quantity id="4CDFD5BECB68FFBEFF04FA41FF08F986" box="[151,197,1590,1615]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="10" pageNumber="332" unit="m" value="5.0">5 m</quantity>
