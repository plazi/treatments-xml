<taxonomicName id="59814D3AFF8F071A1807B5AEFE1FF92A" authority="Lowry &amp; Stoddart, 2010" authorityName="Lowry &amp; Stoddart" authorityYear="2010" box="[296,494,1760,1783]" class="Malacostraca" family="Izinkalidae" genus="Izinkala" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="66" phylum="Arthropoda" rank="species" species="griffithsi" status="sp. nov.">
