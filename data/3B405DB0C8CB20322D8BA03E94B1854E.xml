<normalizedToken id="65211C8A5E655177387D7C76EF14A387" originalValue="Stieldrüsen" pageId="null" pageNumber="427">Stieldruesen</normalizedToken>
und feinen, kurzen Stacheln; 
<normalizedToken id="B976A3536114133FC3EFF3CF8E6553A9" originalValue="Blütenstand" pageId="null" pageNumber="427">Bluetenstand</normalizedToken>
