<taxonomicName id="A9854D69DC66CD47FB668703553DFACF" authorityName="Ansari" authorityYear="1955" box="[1239,1368,1352,1378]" class="Insecta" family="Philopteridae" genus="Brueelia" kingdom="Animalia" order="Psocodea" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="guldum">
<emphasis id="5CF1EAF8DC66CD47FB668703553DFACF" box="[1239,1368,1352,1378]" italics="true" pageId="31" pageNumber="32">Br. guldum</emphasis>
