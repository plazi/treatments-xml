<taxonomicName id="4C7B3AB2FFCDFFF7FF42FB1364F6D696" authority="Khalaim &amp; Saaksjarvi" authorityName="Khalaim &amp; Saaksjarvi" box="[151,684,1231,1258]" class="Insecta" family="Ichneumonidae" genus="Stethantyx" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="102" phylum="Arthropoda" rank="species" species="undulata" status="sp. nov.">
<emphasis id="B90F9D23FFCDFFF7FF42FB1367D0D695" bold="true" box="[151,394,1231,1257]" italics="true" pageId="7" pageNumber="102">Stethantyx undulata</emphasis>
Khalaim &amp; Sääksjärvi
</taxonomicName>
, 
<taxonomicNameLabel id="A23C2058FFCDFFF7FD6FFB0C654DD696" box="[698,791,1232,1258]" pageId="7" pageNumber="102" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
