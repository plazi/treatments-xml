<subSubSection id="C36112BAFFC3FFF9FF12F9C4626BD4E4" pageId="9" pageNumber="104" type="distribution">
<paragraph id="8BC44131FFC3FFF9FF12F9C464B1D44D" blockId="9.[151,1437,1380,1688]" box="[199,747,1560,1585]" pageId="9" pageNumber="104">
<emphasis id="B90F9D23FFC3FFF9FF12F9C4673AD44D" bold="true" box="[199,352,1560,1585]" pageId="9" pageNumber="104">Distribution.</emphasis>
<collectingCountry id="F36C01A1FFC3FFF9FEB2F9C4679ED44D" box="[359,452,1560,1585]" name="Ecuador" pageId="9" pageNumber="104">Ecuador</collectingCountry>
