</emphasis>
<location id="8960D6DB212CFF9CECF3CF0439079A13" LSID="urn:lsid:plazi:treatment:04163116212CFF9DEFF4CE9E3F9D9F2C:8960D6DB212CFF9CECF3CF0439079A13" box="[923,1031,1792,1814]" country="Spain" county="Cuenca" municipality="Las Hoyas" name="Complete" pageId="2" pageNumber="3">Complete</location>
