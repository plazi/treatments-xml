</taxonomicName>
; 
<taxonomicName id="4BBFFB83212CFF9CECD3CB2438859E33" authority="Estes, 1981" authorityName="Estes" authorityYear="1981" box="[955,1413,800,822]" class="Amphibia" family="Albanerpetontidae" genus="Albanerpeton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gymnophiona" pageId="2" pageNumber="3" phylum="Chordata" rank="species" species="megacephalus">
