<subSubSection id="C3347BA4FFFE6E2CA2A5FA7BB39BFA79" pageId="2" pageNumber="66" type="description">
<paragraph id="8B91282FFFFE6E2CA2A5FA7BB39BFA79" blockId="2.[151,1437,1413,1726]" pageId="2" pageNumber="66">
<emphasis id="B95AF43DFFFE6E2CA2A5FA7BB15EFA01" bold="true" box="[201,311,1413,1438]" pageId="2" pageNumber="66">Variation</emphasis>
