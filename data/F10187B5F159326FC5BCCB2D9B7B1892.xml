1. 
<taxonomicName id="BEA84D20F159326FC590CB2D9BBA1956" authority="Barbosa Rodrigues (1891: 127)" authorityName="Barbosa Rodrigues" authorityPageNumber="127" authorityYear="1891" box="[164,751,1590,1616]" class="Liliopsida" family="Orchidaceae" genus="Cyrtopodium" kingdom="Plantae" order="Asparagales" pageId="28" pageNumber="29" phylum="Tracheophyta" rank="species" species="album">
