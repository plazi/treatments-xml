<subSubSection id="C36DEBB0FFE5D71DFF13FC4A2E04DFDC" box="[151,833,939,963]" pageId="23" pageNumber="24" type="type_taxon">
<paragraph id="8BC8B83BFFE5D71DFF13FC4A2E04DFDC" blockId="23.[151,833,939,963]" box="[151,833,939,963]" pageId="23" pageNumber="24">
