<subSubSection box="[151,682,473,499]" pageId="1" pageNumber="225" type="nomenclature">
<paragraph blockId="1.[151,682,473,499]" box="[151,682,473,499]" pageId="1" pageNumber="225">
<heading bold="true" box="[151,682,473,499]" fontSize="11" level="1" pageId="1" pageNumber="225" reason="1">
<emphasis bold="true" box="[151,682,473,499]" pageId="1" pageNumber="225">
<taxonomicName authority="Pseudoneoribates" authorityName="Pseudoneoribates" box="[151,521,473,499]" class="Arachnida" family="Parakalummidae" genus="Neoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="225" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,281,473,499]" italics="true" pageId="1" pageNumber="225">Neoribates</emphasis>
(
<emphasis bold="true" box="[298,511,473,499]" italics="true" pageId="1" pageNumber="225">Pseudoneoribates</emphasis>
