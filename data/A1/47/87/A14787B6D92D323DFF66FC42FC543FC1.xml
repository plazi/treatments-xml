<paragraph blockId="1.[151,565,1014,1073]" box="[151,267,1049,1073]" pageId="1" pageNumber="15">
(Figs 
<number box="[215,258,1049,1073]" pageId="1" pageNumber="15" value="4.0" valueMax="7.0" valueMin="1.0">1–7</number>
)
</paragraph>
