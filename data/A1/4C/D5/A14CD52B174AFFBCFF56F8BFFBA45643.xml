/ 
<taxonomicName box="[1244,1342,836,860]" class="Insecta" family="Aclopidae" genus="Aclopus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="parvulus">
<emphasis box="[1244,1342,836,860]" italics="true" pageId="15" pageNumber="16">parvulus</emphasis>
</taxonomicName>
/ Ohaus 
<typeStatus box="[151,279,871,895]" pageId="15" pageNumber="16">NEOTYPE</typeStatus>
