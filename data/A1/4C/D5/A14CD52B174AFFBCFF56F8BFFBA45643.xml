<date box="[185,385,836,860]" pageId="15" pageNumber="16" value="1973-02-27">
<number box="[185,213,836,860]" pageId="15" pageNumber="16" value="27.0">27</number>
February 
<number box="[329,385,836,860]" pageId="15" pageNumber="16" value="1973.0">1973</number>
</date>
/ Frank A. Enders”; “Pitfall trap / creosotebush / desert / S-C- 
<number box="[1089,1101,836,860]" pageId="15" pageNumber="16" value="3.0">3</number>
” “ 
<taxonomicName box="[1131,1221,836,860]" class="Insecta" family="Aclopidae" genus="Aclopus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
<emphasis box="[1131,1221,836,860]" italics="true" pageId="15" pageNumber="16">Aclopus</emphasis>
