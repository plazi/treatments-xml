<subSubSection pageId="10" pageNumber="11" type="etymology">
<paragraph blockId="10.[151,1437,1027,2024]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[199,334,1856,1880]" pageId="10" pageNumber="11">Etymology.</emphasis>
