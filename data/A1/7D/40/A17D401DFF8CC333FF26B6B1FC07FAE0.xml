. 
<materialsCitation ID-GBIF-Occurrence="3080398301" collectionCode="CSCLR" collectorName="Cores &amp; Resources" country="China" location="Samples Center of Land" municipality="Sanhe" pageId="2" pageNumber="573" specimenCount="1" stateProvince="Hebei" typeStatus="holotype">
