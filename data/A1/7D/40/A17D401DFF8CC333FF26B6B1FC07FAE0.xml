<location LSID="urn:lsid:plazi:treatment:A17D401DFF8CC333FF26B6B1FC07FAE0:2C0BA7D0FF8CC332FB7CB76DFF59FE10" country="China" municipality="Sanhe" name="Samples Center of Land" pageId="2" pageNumber="573" stateProvince="Hebei">Samples Center of Land</location>
