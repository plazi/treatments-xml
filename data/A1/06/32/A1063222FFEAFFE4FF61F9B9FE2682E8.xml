. 
<taxonomicName box="[298,563,1478,1501]" class="Cestoda" family="Catenotaeniidae" genus="Catenotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="8" pageNumber="9" phylum="Platyhelminthes" rank="species" species="dendritica">
<emphasis box="[298,563,1478,1501]" italics="true" pageId="8" pageNumber="9">Catenotaenia dendritica</emphasis>
</taxonomicName>
from 
<taxonomicName box="[631,809,1478,1501]" class="Mammalia" family="Sciuridae" genus="Sciurus" kingdom="Animalia" order="Rodentia" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="vulgaris">
<emphasis box="[631,809,1478,1501]" italics="true" pageId="8" pageNumber="9">Sciurus vulgaris</emphasis>
</taxonomicName>
