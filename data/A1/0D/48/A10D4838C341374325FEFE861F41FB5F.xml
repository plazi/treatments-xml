); 
<collectingCountry box="[497,609,1093,1117]" name="Myanmar" pageId="27" pageNumber="2894">Myanmar</collectingCountry>
(
<collectingRegion box="[627,781,1092,1116]" country="Myanmar" name="Kachin" pageId="27" pageNumber="2894">Kachin State</collectingRegion>
); 
<collectingCountry box="[803,906,1092,1116]" name="Thailand" pageId="27" pageNumber="2894">Thailand</collectingCountry>
(
<collectingRegion country="Thailand" name="Mae Hong Son" pageId="27" pageNumber="2894">Mae Hong Son Prov.</collectingRegion>
