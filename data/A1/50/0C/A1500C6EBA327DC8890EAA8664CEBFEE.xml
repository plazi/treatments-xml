an 
<normalizedToken originalValue="±" pageId="null" pageNumber="176">+/-</normalizedToken>
waagrecht abstehenden, 20-40 mm langen Stielen, mit 4 
<normalizedToken originalValue="gezähnten" pageId="null" pageNumber="176">gezaehnten</normalizedToken>
<normalizedToken originalValue="Flügeln" pageId="null" pageNumber="176">Fluegeln</normalizedToken>
; 
