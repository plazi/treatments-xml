<subSubSection pageId="20" pageNumber="21" type="nomenclature">
<paragraph pageId="20" pageNumber="21">
<taxonomicName LSID="A15B2C08-1A63-DE96-FD9E-B6BA9E8F204C" authority="Rodrigues &amp; Kellner, 2013" authorityName="Rodrigues &amp; Kellner" authorityYear="2013" genus="Anhangueria" lsidName="" pageId="20" pageNumber="21" rank="genus" status="new clade">Anhangueria</taxonomicName>
