<td colspan="1" pageId="3" pageNumber="148" rowspan="1">
<taxonomicName class="Insecta" family="Acanthopteroctetidae" genus="Acanthopteroctetes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acanthopteroctetes" order="Lepidoptera" pageId="3" pageNumber="148" phylum="Arthropoda" rank="genus">Acanthopteroctetes</taxonomicName>
</td>
</tr>
<tr pageId="3" pageNumber="148">
<td colspan="1" pageId="3" pageNumber="148" rowspan="1">
