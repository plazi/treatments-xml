<emphasis bold="true" box="[199,442,1369,1394]" pageId="28" pageNumber="29">
<location LSID="urn:lsid:plazi:treatment:A12C6E25AC1B3B762CBED2FBFDA3A910:2C5A89E8AC1B3B742CEED735FE8FAA7E" box="[199,303,1369,1394]" country="Mexico" latitude="15.939878" longLatPrecision="1" longitude="-96.42996" name="Material" pageId="28" pageNumber="29" stateProvince="Oaxaca">Material</location>
