<location LSID="urn:lsid:plazi:treatment:A12C6E25AC3C3B542CBED02BFE45AE84:2C5A89E8AC3C3B532CEED559FE8FA842" box="[199,303,1845,1870]" country="Mexico" latitude="16.37195" longLatPrecision="1" longitude="-98.1771" name="Material" pageId="59" pageNumber="60" stateProvince="Oaxaca">Material</location>
