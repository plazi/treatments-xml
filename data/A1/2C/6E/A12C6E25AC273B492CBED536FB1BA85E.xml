<emphasis bold="true" box="[289,345,1777,1802]" pageId="33" pageNumber="34">sp.n.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName box="[359,484,1778,1801]" class="Insecta" family="Pyrgomorphidae" genus="Sphenarium" kingdom="Animalia" order="Orthoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="rugosum">
<emphasis box="[359,484,1778,1801]" italics="true" pageId="33" pageNumber="34">S. rugosum</emphasis>
</taxonomicName>
, 
<taxonomicName box="[498,678,1778,1801]" class="Insecta" family="Pyrgomorphidae" genus="Sphenarium" kingdom="Animalia" order="Orthoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="purpurascens">
<emphasis box="[498,678,1778,1801]" italics="true" pageId="33" pageNumber="34">S. purpurascens</emphasis>
</taxonomicName>
) and distant species (
