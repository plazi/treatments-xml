<emphasis bold="true" box="[1373,1428,1741,1766]" pageId="33" pageNumber="34">sp.n.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName box="[151,283,1778,1801]" class="Insecta" family="Pyrgomorphidae" genus="Sphenarium" kingdom="Animalia" order="Orthoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="tarascum" status="sp.n.">
<emphasis box="[151,283,1778,1801]" italics="true" pageId="33" pageNumber="34">S. tarascum</emphasis>
</taxonomicName>
