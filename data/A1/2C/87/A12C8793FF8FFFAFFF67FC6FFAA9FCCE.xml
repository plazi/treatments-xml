<paragraph blockId="14.[151,816,1086,1109]" box="[151,816,1086,1109]" pageId="14" pageNumber="15">
<emphasis bold="true" box="[151,290,1086,1109]" pageId="14" pageNumber="15">
<typeStatus box="[151,204,1087,1108]" pageId="14" pageNumber="15">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Frey, 1991" authorityName="Frey" authorityYear="1991" box="[297,644,1087,1109]" class="Branchiopoda" genus="Celsinotum" kingdom="Animalia" order="Diplostraca" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="parooensis">
