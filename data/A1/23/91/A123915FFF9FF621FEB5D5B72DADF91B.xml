<number box="[151,210,870,894]" pageId="8" pageNumber="66" value="1909.0">1909</number>
</taxonomicName>
, 
<taxonomicName authority="Schlegel, 1837" authorityName="Schlegel" authorityYear="1837" box="[221,603,870,895]" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="66" phylum="Chordata" rank="species" species="purpurascens">
<emphasis box="[221,409,871,895]" italics="true" pageId="8" pageNumber="66">O. purpurascens</emphasis>
(Schlegel, 
<number box="[536,596,870,894]" pageId="8" pageNumber="66" value="1837.0">1837</number>
)
</taxonomicName>
, 
<taxonomicName authority="Gunther, 1864" authorityName="Gunther" authorityYear="1864" box="[616,934,870,894]" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="66" phylum="Chordata" rank="species" species="signatus">
