<number box="[740,799,834,858]" pageId="8" pageNumber="66" value="1917.0">1917</number>
</bibRefCitation>
, 
<taxonomicName authority="Werner, 1913" authorityName="Werner" authorityYear="1913" box="[810,1148,834,859]" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="66" phylum="Chordata" rank="species" species="praefrontalis">
