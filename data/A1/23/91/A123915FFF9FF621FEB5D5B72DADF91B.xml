<number box="[866,926,870,894]" pageId="8" pageNumber="66" value="1864.0">1864</number>
)
</taxonomicName>
, 
<taxonomicName authority="Dumeril, Bibron &amp; Dumeril, 1854" authorityName="Dumeril, Bibron &amp; Dumeril" authorityYear="1854" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="66" phylum="Chordata" rank="species" species="trilineatus">
