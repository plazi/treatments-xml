</taxonomicName>
, 
<taxonomicName authority="Werner, 1909" authorityName="Werner" authorityYear="1909" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="66" phylum="Chordata" rank="species" species="pulcherrimus">
<emphasis box="[1159,1342,835,859]" italics="true" pageId="8" pageNumber="66">O. pulcherrimus</emphasis>
