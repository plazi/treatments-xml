<paragraph blockId="1.[151,914,1798,1821]" box="[151,914,1798,1821]" pageId="1" pageNumber="430">
<emphasis bold="true" box="[151,293,1798,1820]" pageId="1" pageNumber="430">Type species:</emphasis>
<taxonomicName authority="Spinola, 1808" authorityName="Spinola" authorityYear="1808" box="[299,663,1798,1821]" class="Insecta" family="Vespidae" genus="Odynerus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="430" phylum="Arthropoda" rank="species" species="spiricornis">
