<taxonomicName id="22E6B2C72F96CD8136464723474EA334" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:238793" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole recondita Clouse" order="Hymenoptera" pageNumber="10" phylum="Arthropoda" rank="species" species="recondita">Pheidole recondita</taxonomicName>
<taxonomicNameLabel id="61940FA58F433BA5F0F7216D56B437E2" rank="species">n. sp.</taxonomicNameLabel>
