<collectingCountry id="F3365EA29070A10BFC610A77FBFDF8AB" box="[999,1088,1832,1855]" name="Norway" pageId="8" pageNumber="73">Norway</collectingCountry>
, 
<collectingCountry id="F3365EA29070A10BFBCC0A77FB1DF8AB" box="[1098,1184,1832,1855]" name="Finland" pageId="8" pageNumber="73">Finland</collectingCountry>
