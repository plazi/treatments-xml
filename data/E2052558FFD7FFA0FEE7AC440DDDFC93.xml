<collectingCountry id="12BBD4DEFFD7FFA0FE3CAC940DFFFD53" box="[415,470,698,722]" name="India" pageId="10" pageNumber="643">India</collectingCountry>
(
<collectingRegion id="A8685AACFFD7FFA0FE47AC940E47FD53" box="[484,622,698,722]" country="India" name="Uttarakhand" pageId="10" pageNumber="643">Uttarakhand</collectingRegion>
