, 
<collectingCountry id="12BBD4DEFFD7FFA0FD56AC940F02FD50" box="[757,811,698,721]" name="Laos" pageId="10" pageNumber="643">Laos</collectingCountry>
, 
<collectingCountry id="12BBD4DEFFD7FFA0FC95AC940FBCFD53" box="[822,917,698,722]" name="Thailand" pageId="10" pageNumber="643">Thailand</collectingCountry>
and 
<collectingCountry id="12BBD4DEFFD7FFA0FC6AAC94080AFD53" box="[969,1059,698,722]" name="Vietnam" pageId="10" pageNumber="643">Vietnam</collectingCountry>
(BAL- THASAR 1964; 
