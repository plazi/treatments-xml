24e -g, 
<taxonomicName id="72FBFAAF6D42D124C1F1D05BCD841843" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34015" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole smythiesii Forel" order="Hymenoptera" pageNumber="89" phylum="Arthropoda" rank="species" species="smythiesii" subGenus.completionEvidence="missing, matching species" subGenus.innerRound="1" subGenus.outerRound="1">Pheidole smythiesii</taxonomicName>
