<paragraph id="591D4515FFA21775FF091B8FFDA6FDBF" blockId="23.[189,1399,415,515]" pageId="23" pageNumber="127">
<taxonomicName id="9EA23E96FFA21775FF091B8FFE6DFE05" authority="Korn &amp; Miao &amp; Bockwinkel, 2022" authorityName="Korn &amp; Miao &amp; Bockwinkel" authorityYear="2022" box="[189,485,415,441]" class="Cephalopoda" family="Koninckioceratidae" genus="Planetoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nautilida" pageId="23" pageNumber="127" phylum="Mollusca" rank="species" species="transforme" status="sp. nov.">
