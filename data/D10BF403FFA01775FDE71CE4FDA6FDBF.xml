other species such 
<emphasis id="6BD69907FFA21775FBE31BD5FA9CFE62" box="[1111,1300,452,478]" italics="true" pageId="23" pageNumber="127">
as 
<taxonomicName id="9EA23E96FFA21775FBCD1BD5FA9CFE62" authority="Korn &amp; Miao &amp; Bockwinkel, 2022" authorityName="Korn &amp; Miao &amp; Bockwinkel" authorityYear="2022" box="[1145,1300,452,478]" class="Cephalopoda" family="Koninckioceratidae" genus="Planetoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nautilida" pageId="23" pageNumber="127" phylum="Mollusca" rank="species" species="destrictum" status="sp. nov.">P. destrictum</taxonomicName>
</emphasis>
<taxonomicNameLabel id="70E5247CFFA21775FAA81BD4FAFFFE62" box="[1308,1399,452,478]" pageId="23" pageNumber="127" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="9EA23E96FFA21775FF5B1BF9FDAFFDBE" authority="(Histon 1999)" baseAuthorityName="Histon" baseAuthorityYear="1999" box="[239,551,488,514]" class="Cephalopoda" family="Koninckioceratidae" genus="Planetoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nautilida" pageId="23" pageNumber="127" phylum="Mollusca" rank="species" species="globatum">
<emphasis id="6BD69907FFA21775FF5B1BF9FEF4FDBE" box="[239,380,488,514]" italics="true" pageId="23" pageNumber="127">P. globatum</emphasis>
(
<bibRefCitation id="3D3338E4FFA21775FE3F1BF8FD97FDBE" author="Histon K." box="[395,543,488,514]" pageId="23" pageNumber="127" pagination="63 - 129" refId="ref10353" refString="Histon K. 1999. A revision of A. H. Foord's monograph of Irish Carboniferous nautiloid cephalopods (1897 - 1901). Part 2. Monographs of the Palaeontographical Society 153: 63 - 129." type="journal article" year="1999">Histon 1999</bibRefCitation>
