<quantity id="62139B5BFFBEFFF9FCC6873914F6FAF1" box="[873,983,1340,1364]" metricMagnitude="2" metricUnit="m" metricValue="6.365" metricValueMax="8.36" metricValueMin="4.37" pageId="6" pageNumber="367" unit="m" value="636.5" valueMax="836.0" valueMin="437.0">437-836 m</quantity>
