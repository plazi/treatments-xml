<paragraph id="8BE5571EFF9EFD73FE45C02925382E07" blockId="7.[332,1263,393,1737]" box="[396,631,1537,1561]" pageId="7" pageNumber="28">
<emphasis id="B92E8B0CFF9EFD73FE45C02925382E07" box="[396,631,1537,1561]" italics="true" pageId="7" pageNumber="28">
<heading id="D0ADE072FF9EFD73FE45C029253B2E07" box="[396,628,1537,1561]" pageId="7" pageNumber="28">
<taxonomicName id="4C5A2C9DFF9EFD73FE45C029253B2E07" authorityName="Dallas" authorityYear="1851" baseAuthorityName="Fabricius" baseAuthorityYear="1787" box="[396,628,1537,1561]" class="Insecta" family="Pentatomidae" genus="Antiteuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="28" phylum="Arthropoda" rank="species" species="tripterus">Antiteuchus tripterus</taxonomicName>
</heading>
