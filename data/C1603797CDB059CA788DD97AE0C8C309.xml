<paragraph id="3F5366974BFB9A8C9F8F2FCEDCCB6745" pageId="null" pageNumber="516">
<taxonomicName id="8C7455A9FB13267E18AFC508E1781662" authority="Schreber" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="516" phylum="Tracheophyta" rank="species" species="pallescens">
Trifolium 
<normalizedToken id="A3871D59FFB5A242BC4E7903026634B7" originalValue="palléscens" pageId="null" pageNumber="516">pallescens</normalizedToken>
Schreber
</taxonomicName>
</paragraph>
