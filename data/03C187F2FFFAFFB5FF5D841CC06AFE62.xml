<emphasis id="B91CEAF6FFFAFFB3FF5D822BC434F888" bold="true" box="[151,275,1862,1887]" pageId="3" pageNumber="551">FIGURE 1.</emphasis>
<taxonomicName id="4C684D67FFFAFFB3FED1822AC452F889" authorityName="Brodsky" authorityYear="1908" baseAuthorityName="Ehrenberg" baseAuthorityYear="1833" box="[283,373,1863,1887]" class="Oligohymenophorea" family="Frontoniidae" genus="Frontonia" kingdom="Protozoa" order="Hymenostomatida" pageId="3" pageNumber="551" phylum="Ciliophora" rank="species" species="leucas">
