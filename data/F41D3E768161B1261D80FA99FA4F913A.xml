<taxonomicName id="BBB4F4E38162B1261947FC1FFA4F9680" authority="Bowman &amp; Orsi, 1992" authorityName="Bowman &amp; Orsi" authorityYear="1992" box="[1104,1436,914,935]" class="Malacostraca" family="Mysidae" genus="Deltamysis" kingdom="Animalia" order="Mysida" pageId="18" pageNumber="341" phylum="Arthropoda" rank="genus">
