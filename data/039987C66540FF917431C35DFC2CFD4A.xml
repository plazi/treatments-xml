, PL 
<quantity id="4CC89B35654FFF91764AC7FFFC72FE72" box="[883,993,444,470]" metricMagnitude="-3" metricUnit="m" metricValue="1.21" pageId="8" pageNumber="9" unit="mm" value="1.21">1.21 mm</quantity>
, PW 
<quantity id="4CC89B35654FFF91711CC7FFFB00FE72" box="[1061,1171,444,470]" metricMagnitude="-3" metricUnit="m" metricValue="1.65" pageId="8" pageNumber="9" unit="mm" value="1.65">1.65 mm</quantity>
