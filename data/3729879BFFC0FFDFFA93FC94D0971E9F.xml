<td id="42615DB3FFCD0008FBB3FE30D5F31CA3" box="[439,498,439,465]" gridcol="1" gridrow="3" pageId="40" pageNumber="41">0-1</td>
<td id="42615DB3FFCD0008F813FE30D6551CA3" box="[535,596,439,465]" gridcol="2" gridrow="3" pageId="40" pageNumber="41">1-0</td>
