<quantity id="4CD19B50180DF32152E77E1F65019947" box="[965,1106,1813,1838]" metricMagnitude="2" metricUnit="m" metricValue="3.38" metricValueMax="5.85" metricValueMin="0.9099999999999999" pageId="26" pageNumber="407" unit="m" value="338.0" valueMax="585.0" valueMin="91.0">91 to 585 m</quantity>
