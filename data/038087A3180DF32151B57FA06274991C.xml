<emphasis id="B95DEAA7180DF32153857E5763E9991F" box="[679,698,1886,1909]" italics="true" pageId="26" pageNumber="407">=</emphasis>
<quantity id="4CD19B50180DF32153E37E546248991C" box="[705,795,1885,1910]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" pageId="26" pageNumber="407" unit="mm" value="5.1">5.1 mm</quantity>
).
