<quantity id="4CD19B50180DF32150EE7FFB63369963" box="[460,613,1777,1802]" metricMagnitude="2" metricUnit="m" metricValue="5.215" metricValueMax="8.6" metricValueMin="1.83" pageId="26" pageNumber="407" unit="m" value="521.5" valueMax="860.0" valueMin="183.0">183 to 860 m</quantity>
