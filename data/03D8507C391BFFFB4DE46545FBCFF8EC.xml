<collectingRegion id="49B52F883919FFFB4D88676FFEB8F9A8" box="[251,319,1554,1579]" country="Brazil" name="Bahia" pageId="14" pageNumber="275">Bahia</collectingRegion>
, 
<collectingCountry id="F366A1FA3919FFFB4C3D676FFE15F9A8" box="[334,402,1554,1579]" name="Brazil" pageId="14" pageNumber="275">Brazil</collectingCountry>
(
<geoCoordinate id="EE4587AD3919FFFB4CD6676FFD90F9A9" box="[421,535,1554,1578]" direction="south" orientation="latitude" pageId="14" pageNumber="275" precision="15" value="-9.510834">9°30'39&quot;S</geoCoordinate>
<geoCoordinate id="EE4587AD3919FFFB4F51676FFD37F9A9" box="[546,688,1554,1578]" direction="west" orientation="longitude" pageId="14" pageNumber="275" precision="15" value="-38.536667">38°32'12&quot;W</geoCoordinate>
