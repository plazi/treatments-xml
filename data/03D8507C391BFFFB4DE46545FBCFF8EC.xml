<collectingRegion id="49B52F883919FFFB4D866492FE5CF984" box="[245,475,1518,1543]" country="Brazil" name="Mato Grosso do Sul" pageId="14" pageNumber="275">Mato Grosso do Sul</collectingRegion>
, 
<collectingCountry id="F366A1FA3919FFFB4C946493FDAAF984" box="[487,557,1518,1543]" name="Brazil" pageId="14" pageNumber="275">Brazil</collectingCountry>
) and from the Ecological Reserve of Raso da 
<taxonomicName id="4C719AE93919FFFB49446493FAB1F984" authority="Municipality" authorityName="Municipality" box="[1079,1334,1518,1543]" class="Insecta" family="Geometridae" genus="Catarina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="275" phylum="Arthropoda" rank="genus">Catarina, Municipality</taxonomicName>
