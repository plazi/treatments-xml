<taxonomicName id="4C719AE93919FFFB4C54659EFD76FB7A" authority="Malogolowkin" authorityName="Malogolowkin" box="[295,753,1251,1273]" class="Insecta" family="Drosophilidae" genus="Rhinoleucophenga" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="275" phylum="Arthropoda" rank="species" species="personata">
<emphasis id="B9053D783919FFFB4C54659EFDD5FB7A" box="[295,594,1251,1273]" italics="true" pageId="14" pageNumber="275">
