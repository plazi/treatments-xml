<taxonomicName id="D9737F23D27FA03924D2ED190FEE72CB" class="Anthozoa" family="Primnoidae" genus="Callozostron" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Callozostron carlottae" order="Alcyonacea" pageId="5" pageNumber="6" phylum="Cnidaria" rank="species" species="carlottae">Callozostron carlottae</taxonomicName>
