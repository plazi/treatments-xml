<taxonomicName id="D8DBC4B8FFCFD638C97CBBC006A4F3FF" authorityName="M. Sars" authorityYear="1870" box="[540,712,223,248]" class="Maxillopoda" family="Saccopsidae" genus="Melinnacheres" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="ergasiloides">
<emphasis id="2DAF6329FFCFD638C97CBBC006A4F3FF" box="[540,712,223,248]" italics="true" pageId="36" pageNumber="37">M. ergasiloides</emphasis>
</taxonomicName>
from 
<taxonomicName id="D8DBC4B8FFCFD638C86DBBFE07ABF3FF" baseAuthorityName="M. Sars" baseAuthorityYear="1870" box="[781,967,223,248]" class="Polychaeta" family="Ampharetidae" genus="Melinna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Terebellida" pageId="36" pageNumber="37" phylum="Annelida" rank="species" species="cristata">
