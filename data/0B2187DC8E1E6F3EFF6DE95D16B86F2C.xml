<subSubSection id="CB9265418E1E6F3EFF3DEDA716DF6C81" box="[199,1268,1416,1441]" pageId="2" pageNumber="543" type="etymology">
<paragraph id="833736CA8E1E6F3EFF3DEDA716DF6C81" blockId="2.[151,1437,477,1549]" box="[199,1268,1416,1441]" pageId="2" pageNumber="543">
<emphasis id="B1FCEAD88E1E6F3EFF3DEDA7136D6C80" bold="true" box="[199,326,1416,1441]" pageId="2" pageNumber="543">Etymology</emphasis>
