<taxonomicName id="4C214D24FFB7FFF6381BF8829A32C47D" box="[325,540,1857,1881]" class="Asteroidea" family="Echinasteridae" genus="Henricia" kingdom="Animalia" order="Spinulosida" pageId="11" pageNumber="12" phylum="Echinodermata" rank="species" species="ohshimai">
