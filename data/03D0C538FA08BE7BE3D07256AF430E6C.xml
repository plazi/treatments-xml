<number id="3DB16609FA08BE7BE14C74DDA8600F22" box="[523,537,1533,1557]" pageId="2" pageNumber="48" value="8.0">8</number>
setae. Dorsocentrals 
<number id="3DB16609FA08BE7BE04F74DDA95C0F22" box="[776,805,1533,1557]" pageId="2" pageNumber="48" value="11.0">11</number>
