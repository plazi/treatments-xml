<taxonomicName id="4C5B4D086C64FFFDB4CFFB5FEA4EFAB9" box="[151,332,1267,1293]" class="Magnoliopsida" family="Lamiaceae" genus="Phlomis" kingdom="Plantae" order="Lamiales" pageId="66" pageNumber="67" phylum="Tracheophyta" rank="species" species="viscosa">
