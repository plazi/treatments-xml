and 
<taxonomicName id="4C5B4D086C64FFFDB786FB07EF9DFB70" authorityName="de Jussieu" authorityYear="1789" box="[990,1183,1195,1221]" class="Magnoliopsida" family="Scrophulariaceae" kingdom="Plantae" order="Lamiales" pageId="66" pageNumber="67" phylum="Tracheophyta" rank="family">Scrophulariaceae</taxonomicName>
