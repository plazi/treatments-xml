<location id="043560731E30920EFEB4FB26FDDDFAA7" LSID="urn:lsid:plazi:treatment:894387BE1E30920EFE7AFB64FEF0FA49:043560731E30920EFEB4FB26FDDDFAA7" box="[313,529,1266,1287]" municipality="New" name="Zoological Museum" pageId="9" pageNumber="259" stateProvince="Sao Paulo">Zoological Museum</location>
