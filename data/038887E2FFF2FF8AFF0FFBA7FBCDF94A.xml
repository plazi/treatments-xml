</emphasis>
<taxonomicName id="4C214D77FFF0FF8BFE4CF9D4FD1FF9A9" authorityName="Mengual &amp; Ghorpadé" authorityYear="2010" box="[463,749,1610,1636]" class="Insecta" family="Syrphidae" genus="Eosphaerophoria" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="49" phylum="Arthropoda" rank="species" species="adornata">
