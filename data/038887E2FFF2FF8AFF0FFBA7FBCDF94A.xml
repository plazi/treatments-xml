<emphasis id="B955EAE6FFF3FF88FD18FAC0FD5AFAB9" bold="true" box="[667,680,1374,1396]" pageId="9" pageNumber="48">7</emphasis>
<taxonomicName id="4C214D77FFF3FF88FD2CFAC1FC6AFAB9" authorityName="Mengual &amp; Ghorpadé" authorityYear="2010" box="[687,920,1375,1396]" class="Insecta" family="Syrphidae" genus="Eosphaerophoria" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="adornata">
