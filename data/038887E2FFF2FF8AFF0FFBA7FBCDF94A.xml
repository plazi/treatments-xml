<emphasis id="B955EAE6FFF3FF88FB98FAA0FBDAFA99" bold="true" box="[1051,1064,1342,1364]" pageId="9" pageNumber="48">6</emphasis>
<taxonomicName id="4C214D77FFF3FF88FBADFAA1FF5DFAB9" authorityName="Thompson" authorityYear="2008" class="Insecta" family="Syrphidae" genus="Anu" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="una">
