<taxonomicName id="4C214D77FFF3FF88FD07FAA1FCDCFA99" baseAuthorityName="Miller" baseAuthorityYear="1921" box="[644,814,1343,1364]" class="Insecta" family="Syrphidae" genus="Allograpta" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="ventralis">
