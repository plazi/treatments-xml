<collectingCountry id="12BBD4DEFFD9FFAEFB84AAA4084FFB23" box="[1063,1126,1162,1186]" name="China" pageId="4" pageNumber="637">China</collectingCountry>
and 
<collectingCountry id="12BBD4DEFFD9FFAEFF66AA840D06FB43" box="[197,303,1194,1218]" name="Indonesia" pageId="4" pageNumber="637">Indonesia</collectingCountry>
