<taxonomicName id="4C234D7B9914292B8211FD9A8A92FDD9" authorityName="Dendy" authorityYear="1892" box="[828,963,563,587]" class="Calcarea" family="Heteropiidae" kingdom="Animalia" order="Leucosolenida" pageId="11" pageNumber="198" phylum="Porifera" rank="family">Heteropiidae</taxonomicName>
