<geoCoordinate id="EE235007FF31FF159908FF64FB9830B4" box="[373,473,150,177]" degrees="27" direction="north" minutes="54" orientation="latitude" pageId="160" pageNumber="161" precision="925" value="27.9">27°54’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FF31FF15999BFF65F81C30B4" box="[486,605,151,177]" degrees="114" direction="west" minutes="18" orientation="longitude" pageId="160" pageNumber="161" precision="925" value="-114.3">114°18’W</geoCoordinate>
