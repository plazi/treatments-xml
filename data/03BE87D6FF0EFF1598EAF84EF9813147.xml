(
<quantity id="4CEF9B25FF0EFF2A994AF812FB3C37FE" box="[311,381,2016,2043]" metricMagnitude="-1" metricUnit="m" metricValue="2.4384" pageId="159" pageNumber="160" unit="in" value="9.6">9.6 in</quantity>
) TL (
<bibRefCitation id="EF864B31FF0EFF2A99BFF812F96A37FF" author="Rodriguez-Romero, J. &amp; Palacios-Salgado, D. S. &amp; Gutierrez Sanchez, F. J." box="[450,811,2016,2042]" pageId="159" pageNumber="160" pagination="2 - 4" refId="ref317289" refString="Rodriguez-Romero, J., Palacios-Salgado, D. S. &amp; Gutierrez Sanchez, F. J. (2008) First record of the spottail moray Gymnothorax equatorialis (Anguilliformes: Muraenidae) off the west coast of Baja California Sur, Mexico. Marine Biodiversity Records, 1, 2 - 4. https: // doi. org / 10.1017 / s 1755267207007981" type="journal article" year="2008">
Rodríguez-Romero 
<emphasis id="B963EAD2FF0EFF2A9ADAF813F8A237FF" box="[679,739,2016,2042]" italics="true" pageId="159" pageNumber="160">et al.</emphasis>
2008
</bibRefCitation>
). From 
<quantity id="4CEF9B25FF0EFF2A9BF4F812F99B37FF" box="[905,986,2016,2042]" metricMagnitude="3" metricUnit="m" metricValue="4.8" pageId="159" pageNumber="160" unit="km" value="4.8">4.8 km</quantity>
(
<quantity id="4CEF9B25FF0EFF2A9B93F812FE6737FE" box="[1006,1062,2016,2043]" metricMagnitude="3" metricUnit="m" metricValue="4.828032" pageId="159" pageNumber="160" unit="mi" value="3.0">3 mi</quantity>
