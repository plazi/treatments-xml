<collectionCode id="ED7AAE078702AA1D4300F8C4DE17AD75" box="[1309,1399,1846,1872]" country="USA" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="25" pageNumber="26" type="Museum">NMNH</collectionCode>
