</taxonomicName>
and 
<taxonomicName id="14944D5EFF9DFF99FEC0FE78FE7494D3" box="[338,425,477,500]" class="Insecta" family="Naucoridae" genus="Neomacrocoris" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="vuga">
