<collectingRegion id="1C614E83FFC6961C8B42FB4AC6DC2754" country="Iran" name="Fars" pageId="5" pageNumber="55">Fars province</collectingRegion>
of 
<collectingCountry id="A6B2C0F1FFC6961C8DE7FAA7C68B2754" box="[966,1014,1306,1329]" name="Iran" pageId="5" pageNumber="55">Iran</collectingCountry>
