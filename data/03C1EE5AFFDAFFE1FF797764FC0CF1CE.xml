<taxonomicName id="4C6824CFFFD9FFDFFD3C77F1FCD4F787" authorityName="Gorochov" authorityYear="2002" box="[722,824,1964,1985]" class="Insecta" family="Haglidae" genus="Metriogryllacris" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="comes">
<emphasis id="B91C835EFFD9FFDFFD3C77F1FCD4F787" box="[722,824,1964,1985]" italics="true" pageId="30" pageNumber="31">M. comes</emphasis>
