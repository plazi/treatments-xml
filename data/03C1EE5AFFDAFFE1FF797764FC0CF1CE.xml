<taxonomicName id="4C6824CFFFD9FFDFFC9A77F6FB9EF787" authorityName="Gorochov" authorityYear="2004" box="[884,1138,1963,1985]" class="Insecta" family="Haglidae" genus="Furcilarnaca" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="superfurca">
