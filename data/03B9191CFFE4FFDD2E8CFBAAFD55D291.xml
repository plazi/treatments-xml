<taxonomicName id="4C10D389FFE4FFDE2FEBFBF6FE38D429" authority="Simon, 1897: 153" authorityName="Simon" authorityPageNumber="153" authorityYear="1897" box="[163,462,1126,1149]" class="Arachnida" family="Liocranidae" genus="Sesieutes" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="70" phylum="Arthropoda" rank="genus">
<emphasis id="B9647418FFE4FFDE2FEBFBF6FEF3D429" box="[163,261,1126,1149]" italics="true" pageId="1" pageNumber="70">Sesieutes</emphasis>
Simon, 1897: 153
</taxonomicName>
; Deelemen-Reinhold 2001: 446. 
<typeStatus id="54AB16A8FFE4FFDE2C67FBF6FC92D429" box="[815,868,1126,1149]" pageId="1" pageNumber="70">Type</typeStatus>
