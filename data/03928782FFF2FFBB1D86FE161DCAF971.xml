<taxonomicName id="4C3B4D17FFF2FFBB1EE7FD7F1897FD65" baseAuthorityName="Mayr" baseAuthorityYear="1876" box="[1014,1180,687,712]" class="Insecta" family="Encyrtidae" genus="Syrphophagus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="40" phylum="Arthropoda" rank="species" species="aphidivorus">
