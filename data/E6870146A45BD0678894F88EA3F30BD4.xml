<taxonomicName id="280A0F79E1EE705BC25A607E7E849E8B" class="Diplopoda" family="Polyxenidae" genus="Unixenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Unixenus attemsi" order="Polyxenida" pageId="15" pageNumber="120" phylum="Arthropoda" rank="species" species="attemsi">Unixenus attemsi</taxonomicName>
