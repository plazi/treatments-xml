<taxonomicName id="B0BE90A28194FC01920955413E875DA7" class="Diplopoda" family="Polyxenidae" genus="Unixenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Unixenus corticolus" order="Polyxenida" pageId="15" pageNumber="120" phylum="Arthropoda" rank="species" species="corticolus">Unixenus corticolus</taxonomicName>
