<taxonomicName id="7000B2A351B73D88F89E3C39193E835A" class="Diplopoda" family="Polyxenidae" genus="Unixenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Unixenus mjoebergi" order="Polyxenida" pageId="15" pageNumber="120" phylum="Arthropoda" rank="species" species="mjoebergi">Unixenus mjoebergi</taxonomicName>
