<normalizedToken id="A2E0B4271243C21ED11E1CFBB4B2E4F6" originalValue="à">a</normalizedToken>
<normalizedToken id="0764A88E27EDB09BFCCE563534A0EFEB" originalValue="Sérajewo”">Serajewo&quot;</normalizedToken>
