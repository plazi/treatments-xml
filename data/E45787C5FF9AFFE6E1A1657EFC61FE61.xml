<taxonomicName id="ABFE4D50FF9DFFE7E3056504FD47F878" box="[563,705,1912,1937]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="cintalapa">
