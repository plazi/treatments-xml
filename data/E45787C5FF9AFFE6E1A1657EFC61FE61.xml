<taxonomicName id="ABFE4D50FF9DFFE7E3E56504FCC2F879" box="[723,836,1913,1936]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="tacana">
