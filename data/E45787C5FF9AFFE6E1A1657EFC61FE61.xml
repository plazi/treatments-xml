<taxonomicName id="ABFE4D50FF9DFFE7E0136795FC99FA17" authority="F.O. Pickard-Cambridge 1902" authorityName="F.O. Pickard-Cambridge" authorityYear="1902" box="[293,799,1512,1534]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="simplex">
