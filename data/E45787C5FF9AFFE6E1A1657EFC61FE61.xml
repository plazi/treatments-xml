</taxonomicName>
, and 
<taxonomicName id="ABFE4D50FF9DFFE7E2BF6504FC7FF879" box="[905,1017,1912,1936]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="triunfo">
