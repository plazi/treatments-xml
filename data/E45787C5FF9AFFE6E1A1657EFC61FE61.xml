<taxonomicName id="ABFE4D50FF9DFFE7E3716528FD42F884" box="[583,708,1876,1901]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="laticava">
