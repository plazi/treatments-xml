; MW, 
<number id="3DE57068772BFFF0E291AF1B2C7BF9C9" box="[1197,1318,1595,1621]" pageId="9" pageNumber="46" value="0.355" valueMax="0.38" valueMin="0.33">0.33–0.38</number>
; AWV, 
<number id="3DE57068772BFFF0E76AAF422892F9E0" box="[342,463,1634,1660]" pageId="9" pageNumber="46" value="0.465" valueMax="0.56" valueMin="0.37">0.37–0.56</number>
; GL, 
<number id="3DE57068772BFFF0E42DAF422BD7F9E0" box="[529,650,1634,1660]" pageId="9" pageNumber="46" value="0.26" valueMax="0.3" valueMin="0.22">0.22–0.30</number>
