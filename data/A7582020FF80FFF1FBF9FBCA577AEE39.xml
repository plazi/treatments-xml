<taxonomicName id="E8F1EAB5FF8EFFF1FECCF99D5783EF25" authorityName="Kurbatov &amp; Cuccodoro &amp; Sabella" authorityYear="2018" box="[307,442,1585,1609]" class="Insecta" family="Staphylinidae" genus="Achilia" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="174" phylum="Arthropoda" rank="species" species="fiura">
