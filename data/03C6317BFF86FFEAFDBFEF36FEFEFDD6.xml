<quantity id="4C972D88FF86FFEBFE38EE91FDA9F80D" box="[442,531,2001,2027]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="32" pageNumber="33" unit="m" value="1200.0">1200 m</quantity>
