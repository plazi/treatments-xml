<quantity id="4C972D88FF86FFEBFBDBEEEAFB41F822" box="[1113,1275,1962,1989]" metricMagnitude="3" metricUnit="m" metricValue="1.365" metricValueMax="1.52" metricValueMin="1.21" pageId="32" pageNumber="33" unit="m" value="1365.0" valueMax="1520.0" valueMin="1210.0">1210–1520 m</quantity>
