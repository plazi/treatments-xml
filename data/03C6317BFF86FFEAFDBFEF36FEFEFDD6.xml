<quantity id="4C972D88FF86FFEBFD6CEEEAFCFDF822" box="[750,839,1962,1988]" metricMagnitude="1" metricUnit="m" metricValue="4.79552" pageId="32" pageNumber="33" unit="in" value="1888.0">1888 in</quantity>
<collectingCountry id="F378C0FDFF86FFEBFCCCEEEAFC78F822" box="[846,962,1962,1988]" name="Myanmar" pageId="32" pageNumber="33">Myanmar</collectingCountry>
, 
<collectingRegion id="49AB4E8FFF86FFEBFC4CEEEAFBB3F822" box="[974,1033,1962,1988]" country="Myanmar" name="Shan" pageId="32" pageNumber="33">Shan</collectingRegion>
