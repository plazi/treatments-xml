<taxonomicName id="123F0B463B1F642992C86B6D90CEC9D6" class="Arachnida" family="Gnaphosidae" genus="Pterotricha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pterotricha" order="Araneae" pageId="0" pageNumber="151" phylum="Arthropoda" rank="genus">Pterotricha</taxonomicName>
spp.: a) 
<taxonomicName id="D277303259003CE03B4DFCD2EF433241" genus="P." lsidName="P. arabica" pageId="0" pageNumber="151" rank="species" species="arabica">P. arabica</taxonomicName>
sp. n.; b) 
<taxonomicName id="3F0D352DFF201762A8089CA0F15A899F" genus="P." lsidName="P. esyunini" pageId="0" pageNumber="151" rank="species" species="esyunini">P. esyunini</taxonomicName>
