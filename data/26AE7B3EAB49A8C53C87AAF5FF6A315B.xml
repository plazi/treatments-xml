<taxonomicName id="7C8A0D0E2732588107FE62F440ABC623" genus="P." lsidName="P. nadolnyi" pageId="0" pageNumber="151" rank="species" species="nadolnyi">P. nadolnyi</taxonomicName>
sp. n.; d) 
<taxonomicName id="6C29837D5BAC4E8AD3B6884DE85AAA9C" genus="P." lsidName="P. stevensi" pageId="0" pageNumber="151" rank="species" species="stevensi">P. stevensi</taxonomicName>
