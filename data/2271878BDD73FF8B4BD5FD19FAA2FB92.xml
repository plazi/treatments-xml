<collectionCode id="CCC9AE58DD73FF8B4A83FBEEFD8CFB92" LSID="urn:lsid:biocol.org:col:34992" box="[449,537,1062,1088]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="29" pageNumber="54">RMNH</collectionCode>
