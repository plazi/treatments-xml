<quantity id="D7629B6FF845FF90FDA1FD62DE37FD31" box="[517,602,741,764]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="2" pageNumber="67" unit="mm" value="2.6">2.6 mm</quantity>
