<emphasis id="22EEEA98F845FF90FF63FD63DD3FFD31" bold="true" box="[199,338,740,764]" pageId="2" pageNumber="67">Description.</emphasis>
BL 
<quantity id="D7629B6FF845FF90FE24FD62DDB8FD31" box="[384,469,741,764]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="2" pageNumber="67" unit="mm" value="5.3">5.3 mm</quantity>
