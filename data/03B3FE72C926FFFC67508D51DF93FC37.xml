<quantity id="4CE2E281C926FFFF640F8C9EDCE1F9C5" box="[349,433,1531,1555]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="24" pageNumber="424" unit="m" value="1000.0">1000 m</quantity>
