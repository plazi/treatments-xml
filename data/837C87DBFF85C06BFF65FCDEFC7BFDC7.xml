<taxonomicName id="CCD54D4EFF85C06BFDE6FBB5FD69FBA0" box="[634,672,1098,1124]" class="Not" higherTaxonomySource="GBIF" kingdom="Chromista" pageId="17" pageNumber="340" phylum="Myzozoa" rank="class">not</taxonomicName>
