<taxonomicName id="70CA4D3CFFDEE27A85D8FE2CDFAD6731" authority="Haeckel, 1879" authorityName="Haeckel" authorityYear="1879" box="[151,675,511,536]" class="Hydrozoa" family="Bougainvilliidae" genus="Bougainvillia" kingdom="Animalia" order="Anthoathecata" pageId="51" pageNumber="52" phylum="Cnidaria" rank="species" species="platygaster">
<emphasis id="85BEEAADFFDEE27A85D8FE2CDCCD6731" box="[151,451,511,536]" italics="true" pageId="51" pageNumber="52">Bougainvillia platygaster</emphasis>
