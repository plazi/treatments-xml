<taxonomicName id="4C0CFFE6FFE8FFCDFF0CFCF9FDC9FC29" baseAuthorityName="Smirnov" baseAuthorityYear="1996" box="[219,523,893,915]" class="Branchiopoda" genus="Pleuroxus" kingdom="Animalia" order="Diplostraca" pageId="8" pageNumber="779" phylum="Arthropoda" rank="species" species="quasidenticulatus">
<emphasis id="B9785877FFE8FFCDFF0CFCF9FEF9FC29" box="[219,315,893,914]" italics="true" pageId="8" pageNumber="779">Pleuroxus</emphasis>
cf. 
