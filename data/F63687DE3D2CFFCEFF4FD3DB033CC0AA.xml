<taxonomicName id="B99F4D4B3D2DFFD1FD55D5A603A3C2FA" authorityName="Simon" authorityYear="1900" box="[653,754,223,248]" class="Arachnida" family="Salticidae" genus="Thiodina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="316" phylum="Arthropoda" rank="genus">
