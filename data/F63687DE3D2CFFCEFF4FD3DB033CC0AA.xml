<taxonomicName id="B99F4D4B3D2DFFD1FE3BD4350331C367" authorityName="Simon" authorityYear="1902" box="[483,608,332,357]" class="Arachnida" family="Salticidae" genus="Cyllodania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="316" phylum="Arthropoda" rank="genus">
