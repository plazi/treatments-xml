<taxonomicName id="B99F4D4B3D2DFFD1FCDDD5A602F0C2FA" authorityName="Mello-Leitao" authorityYear="1917" box="[773,929,223,248]" class="Arachnida" family="Salticidae" genus="Arachnomura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="316" phylum="Arthropoda" rank="genus">
