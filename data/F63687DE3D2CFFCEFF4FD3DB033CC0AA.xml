<taxonomicName id="B99F4D4B3D2DFFD1FB8ED47D0582C31F" authorityName="Simon" authorityYear="1902" box="[1110,1235,260,285]" class="Arachnida" family="Salticidae" genus="Cyllodania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="316" phylum="Arthropoda" rank="genus">
