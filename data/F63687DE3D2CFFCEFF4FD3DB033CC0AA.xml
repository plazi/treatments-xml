<taxonomicName id="B99F4D4B3D2DFFD1FC6BD5A60502C2FA" authorityName="Simon" authorityYear="1902" box="[947,1107,223,248]" class="Arachnida" family="Salticidae" genus="Atomosphyrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="316" phylum="Arthropoda" rank="genus">
