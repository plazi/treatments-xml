and 
<taxonomicName id="0E8BAB89FF9EFFEF7C5533F24788DA83" baseAuthorityName="Viets" baseAuthorityYear="1928" box="[576,734,457,486]" class="Arachnida" family="Halacaridae" genus="Caspihalacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="hyrcanus">
