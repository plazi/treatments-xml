<taxonomicName id="0E8BAB89FF9EFFEF7EC733F144ABDA83" authorityName="Bartsch &amp; Gerecke" authorityYear="2011" box="[210,509,457,486]" class="Arachnida" family="Halacaridae" genus="Halacarellus" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="fontinalis">
