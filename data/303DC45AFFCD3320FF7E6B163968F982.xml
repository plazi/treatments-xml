<quantity id="7F6CD8A9FFCD3320FE936B413B82FADD" box="[351,435,1345,1367]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="4" pageNumber="1611" unit="mm" value="4.5" valueMax="5.0" valueMin="4.0">4-5 mm</quantity>
