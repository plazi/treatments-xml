<taxonomicName id="7F940ECFFFCD3320FCA36B4239DAFADC" authority="Malicky, 2014" authorityName="Malicky" authorityYear="2014" box="[879,1003,1345,1367]" class="Insecta" family="Philopotamidae" genus="Wormaldia" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="1611" phylum="Arthropoda" rank="species" species="saraguel" status="nov.sp.">
