<emphasis id="B95344267E68FFE4FF3F7744DCD2FB7D" bold="true" box="[192,972,1134,1158]" pageId="26" pageNumber="25">
<taxonomicName id="4C27E3B77E68FFE4FF3F7744DDCEFB7D" authority="(Hewitson, [1866])" authorityName="Cong &amp; Zhang &amp; Shen &amp; Grishin" authorityYear="2019" baseAuthorityName="Cong &amp; Zhang &amp; Shen &amp; Grishin" baseAuthorityYear="2019" box="[192,720,1134,1158]" class="Insecta" family="Hesperiidae" genus="Neoxeniades" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="25" phylum="Arthropoda" rank="species" species="ethoda" status="new combination">
