<geoCoordinate id="ACD505C304F5FF76FD2DFBF875071467" box="[649,789,1077,1104]" degrees="35" direction="north" minutes="35" orientation="latitude" pageId="165" pageNumber="166" precision="15" seconds="00" value="35.583332">35°35’00”N</geoCoordinate>
