<geoCoordinate id="ACD505C304F5FF76FC84FBFB75B01467" box="[800,930,1078,1104]" degrees="6" direction="west" minutes="21" orientation="longitude" pageId="165" pageNumber="166" precision="15" seconds="46" value="-6.3627777">6°21’46”W</geoCoordinate>
