<taxonomicName id="0EE1188704F5FF76FAD4FBDF76C81467" authorityName=", Koehler &amp; Vaney" authorityYear="1910" class="Gastropoda" family="Bursidae" genus="Talisman" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="165" pageNumber="166" phylum="Mollusca" rank="genus">
