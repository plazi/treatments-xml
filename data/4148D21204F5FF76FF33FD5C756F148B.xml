<emphasis id="FB95BF1604F5FF76FC09FBFA75D31467" box="[941,961,1079,1104]" italics="true" pageId="165" pageNumber="166">c.</emphasis>
<quantity id="0E19CEE104F5FF76FC6CFBF872021467" box="[968,1040,1077,1104]" metricMagnitude="2" metricUnit="m" metricValue="1.5" pageId="165" pageNumber="166" unit="m" value="150.0">150 m</quantity>
