<typeStatus id="114D886AFFA97444FE0BFA5EFDB6F9F7" box="[411,516,1521,1547]" pageId="23" pageNumber="24">holotype</typeStatus>
.
</emphasis>
Bill length 
<quantity id="090E9B2DFFA97444FD1EFA5EFD57F9F0" box="[654,741,1521,1548]" metricMagnitude="-3" metricUnit="m" metricValue="9.6" pageId="23" pageNumber="24" unit="mm" value="9.6">9.6 mm</quantity>
, wing length 
<quantity id="090E9B2DFFA97444FCECFA5EFC50F9F0" box="[892,994,1521,1548]" metricMagnitude="-2" metricUnit="m" metricValue="8.07" pageId="23" pageNumber="24" unit="mm" value="80.7">80.7 mm</quantity>
, tail length 
<quantity id="090E9B2DFFA97444FBF5FA5EFB79F9F0" box="[1125,1227,1521,1548]" metricMagnitude="-2" metricUnit="m" metricValue="4.09" pageId="23" pageNumber="24" unit="mm" value="40.9">40.9 mm</quantity>
, tarsus length 
<quantity id="090E9B2DFFA97444FAFBFA5EFF74F9CC" metricMagnitude="-2" metricUnit="m" metricValue="4.01" pageId="23" pageNumber="24" unit="mm" value="40.1">40.1 mm</quantity>
.
</paragraph>
<caption id="9A896640FFA97444FF07F818FD30F80F" ID-DOI="http://doi.org/10.5281/zenodo.3954733" ID-Zenodo-Dep="3954733" httpUri="https://zenodo.org/record/3954733/files/figure.png" pageId="23" pageNumber="24" startId="23.[151,250,1975,1999]" targetBox="[156,1432,1607,1947]" targetPageId="23">
