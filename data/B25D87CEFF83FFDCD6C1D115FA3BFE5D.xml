, PW = 
<quantity id="FD0C9B3DFF83FFDED706D2CAFE18F88C" box="[336,438,1765,1792]" metricMagnitude="-3" metricUnit="m" metricValue="1.56" pageId="72" pageNumber="73" unit="mm" value="1.56">1.56 mm</quantity>
, PA = 90°, EL = 
<quantity id="FD0C9B3DFF83FFDED425D2CAFD77F88C" box="[627,729,1765,1792]" metricMagnitude="-3" metricUnit="m" metricValue="2.07" pageId="72" pageNumber="73" unit="mm" value="2.07">2.07 mm</quantity>
, EA = 160°, SL = 
<quantity id="FD0C9B3DFF83FFDED5F1D2CAFBA0F88C" box="[935,1038,1765,1792]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="72" pageNumber="73" unit="mm" value="0.85">0.85 mm</quantity>
