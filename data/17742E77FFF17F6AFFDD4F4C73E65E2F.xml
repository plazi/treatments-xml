<taxonomicName id="58DDE4E2FFF07F68FE0E4C35712C5F26" authorityName="El-Naggar" authorityYear="1987" box="[611,768,1869,1892]" class="Actinopterygii" family="Claroteidae" genus="Protoancylodiscoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="158" phylum="Chordata" rank="species" species="mansourensis">
