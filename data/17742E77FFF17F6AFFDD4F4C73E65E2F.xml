<quantity id="58253284FFF07F68FD2B4CCF73B95F8D" box="[326,405,1975,1999]" metricMagnitude="-1" metricUnit="m" metricValue="4.82" pageId="5" pageNumber="158" unit="mm" value="482.0">482 µm</quantity>
respectivement chez 
<taxonomicName id="58DDE4E2FFF07F68FE074CCF712C5F8C" authorityName="El-Naggar" authorityYear="1987" box="[618,768,1975,1998]" class="Actinopterygii" family="Claroteidae" genus="Protoancylodiscoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Siluriformes" pageId="5" pageNumber="158" phylum="Chordata" rank="species" species="mansourensis">
