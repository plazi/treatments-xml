</subSubSection>
<subSubSection id="C2DF0117563FFC4F7EF3CD2AD8DAAE45" pageId="58" pageNumber="59" type="reference_group">
<paragraph id="876AF8ED1D635FD2FAF6571BF572A3CB" pageId="58" pageNumber="59">
<taxonomicName id="C18CF0F0DBB8E14B5C5284FD0E369DD5" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis Graelsii" order="Hymenoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="Graelsii">Chrysis Graelsii</taxonomicName>
