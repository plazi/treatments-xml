(
<figureCitation id="1365FFFAFFADC8464D4E15EDFD7ADD34" box="[556,676,1512,1537]" captionStart="Figures 15–18" captionStartId="12.[160,235,1406,1429]" captionTargetBox="[163,1131,142,1389]" captionTargetId="figure@12.[163,1153,142,1389]" captionTargetPageId="12" captionText="Figures 15–18. Zaphanta adults; a = dorsal, b = ventral. 15. Z. stiletto Ƌ holotype, Brazil, Rondônia, Calama, Rio Madeira (NHMUK). 16. Z. machaera Ƌ holotype, Brazil, Mato Grosso, Chapada dos Guimarães, 800 m (VOB). 17. Z. machaera ♀, Brazil, Mato Grosso, Chapada dos Guimarães, 800 m (VOB). 18. Z. beckeri Ƌ holotype, Brazil, Rondônia, Ariquemes, 180 m (VOB). Scale bar = 1 cm." figureDoi="http://doi.org/10.5281/zenodo.3673473" httpUri="https://zenodo.org/record/3673473/files/figure.png" pageId="26" pageNumber="1234">Figures 18</figureCitation>
, 34, 42) 
<uri id="FFCFEF7DFFADC8464E64160FFBC3DD16" box="[262,1053,1546,1571]" pageId="26" pageNumber="1234">
urn:lsid:zoobank.org:act: 
<uuid id="FFF8D9AAFFADC8464D73160FFBC3DD16" box="[529,1053,1546,1571]" pageId="26" pageNumber="1234">11AA3541-C7A4-4292-A5CA-10B89C5FAD2C</uuid>
