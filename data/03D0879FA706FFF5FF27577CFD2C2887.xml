<collectionCode id="ED68AE4CA706FFF5FE71531FFDC12861" LSID="urn:lsid:biocol.org:col:34697" box="[449,532,1308,1334]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="17" pageNumber="42">BPBM</collectionCode>
