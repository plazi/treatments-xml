<emphasis id="B956EAC2FFE3A6248A0816D223760EBC" bold="true" box="[820,1110,1741,1765]" pageId="3" pageNumber="62">
<taxonomicName id="4C224D53FFE3A6248A0816D2232C0EBC" baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[820,1036,1741,1765]" class="Mammalia" family="Cricetidae" genus="Arvicola" kingdom="Animalia" order="Rodentia" pageId="3" pageNumber="62" phylum="Chordata" rank="species" species="amphibius">
<emphasis id="B956EAC2FFE3A6248A0816D2232C0EBC" bold="true" box="[820,1036,1741,1765]" italics="true" pageId="3" pageNumber="62">Arvicola amphibius</emphasis>
</taxonomicName>
group
</emphasis>
