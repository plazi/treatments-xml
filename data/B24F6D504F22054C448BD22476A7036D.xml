Current name: 
<taxonomicName id="73E64A6822091845E0D9EC6D6E735242" authority="(Zanted.) Greuter &amp; Pignatti" authorityName="Greuter &amp; Pignatti" baseAuthorityName="Zanted." class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" phylum="Tracheophyta" rank="species" species="pradica">
<emphasis id="62C4B189960EBC30EA16C4886F334D56" bold="true" italics="true" pageNumber="348">Stachys pradica</emphasis>
(Zanted.) Greuter &amp; Pignatti
</taxonomicName>
(
<taxonomicName id="386A4807B658E84B041375CD91BCCFC6" class="Magnoliopsida" family="Lamiaceae" kingdom="Plantae" order="Lamiales" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
