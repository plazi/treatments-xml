<taxonomicName id="BEA84D20F158326EC17ECC119C391E22" box="[1098,1388,266,292]" class="Liliopsida" family="Orchidaceae" genus="Cyrtopodium" kingdom="Plantae" order="Asparagales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="parviflorum">
