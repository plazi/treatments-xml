<taxonomicName id="BEA84D20F158326EC62DCC119D5A1E22" box="[793,1039,266,292]" class="Liliopsida" family="Orchidaceae" genus="Otostylis" kingdom="Plantae" order="Asparagales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="brachystalix">
