<quantity id="56189B425D62FD7AD8B3966D81F6F9F9" box="[687,766,1569,1596]" metricMagnitude="-2" metricUnit="m" metricValue="6.6" pageId="30" pageNumber="185" unit="mm" value="66.0">66 mm</quantity>
