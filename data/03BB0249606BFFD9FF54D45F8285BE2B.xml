and 
<collectingCountry id="F305F3CF606AFFD9FBACD12F85F8BF4B" box="[1135,1271,548,573]" name="Puerto Rico" pageId="15" pageNumber="166">Puerto Rico</collectingCountry>
, from 
<quantity id="4CEA1EBA606AFFD9FAFCD12E8493BF4A" box="[1343,1436,548,572]" metricMagnitude="0" metricUnit="m" metricValue="6.0" metricValueMax="8.0" metricValueMin="4.0" pageId="15" pageNumber="166" unit="m" value="6.0" valueMax="8.0" valueMin="4.0">4 to 8 m</quantity>
in depth (
<bibRefCitation id="EF83CEAE606AFFD9FEC2D14D80E8BF16" author="Pulitzer-Finali" box="[257,487,583,608]" pageId="15" pageNumber="166" refString="Pulitzer-Finali, G. (1986) A collection of West Indian Demospongiae (Porifera). In appendix, a list of the Demospongiae hitherto recorded from the West Indies. Annali del Museo civico di storia naturale Giacomo Doria, 86, 65 - 216." type="journal article" year="1986">
Pulitzer-Finali 
<number id="3DDAA178606AFFD9FE68D14D80E8BF16" box="[427,487,583,608]" pageId="15" pageNumber="166" value="1986.0">1986</number>
</bibRefCitation>
); 
<collectingCountry id="F305F3CF606AFFD9FE3FD14D8349BF16" box="[508,582,583,608]" name="Belize" pageId="15" pageNumber="166">Belize</collectingCountry>
, Hispaniola, 
<collectingCountry id="F305F3CF606AFFD9FD1AD1428239BF16" box="[729,822,584,608]" name="Curaçao" pageId="15" pageNumber="166">Curaçao</collectingCountry>
on coral rubble and mangroves, 
<quantity id="4CEA1EBA606AFFD9FB6BD14D85F2BF29" box="[1192,1277,583,608]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="12.0" metricValueMin="6.0" pageId="15" pageNumber="166" unit="m" value="9.0" valueMax="12.0" valueMin="6.0">6–12 m</quantity>
deep (
<bibRefCitation id="EF83CEAE606AFFD9FA8BD14D801BBFF2" author="Rutzler" pageId="15" pageNumber="166" refString="Rutzler, K., Diaz, M. C., Soest, R. W. M. van, Zea, S., Smith, K. P., Alvarez, B. &amp; Wulff, J. L. (2000) Diversity of sponge fauna in mangrove ponds, Pelican Cays, Belize. Atoll Research Bulletin, 476, 230 - 248. http: // dx. doi. org / 10.5479 / si. 00775630.467.229" type="journal article" year="2000">
Rützler 
<emphasis id="B9666F4D606AFFD9FF54D16781DFBFF3" box="[151,208,620,645]" italics="true" pageId="15" pageNumber="166">et al.</emphasis>
<number id="3DDAA178606AFFD9FF14D166801BBFF2" box="[215,276,620,644]" pageId="15" pageNumber="166" value="2000.0">2000</number>
</bibRefCitation>
