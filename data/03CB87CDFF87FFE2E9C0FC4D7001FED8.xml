<paragraph id="8BDD36DBFF87FFEFE990F9C87609F909" blockId="71.[151,1437,1191,2010]" box="[199,1137,1659,1686]" pageId="71" pageNumber="72">
<emphasis id="B916EAC9FF87FFEFE990F9C87330F90A" bold="true" box="[199,328,1660,1686]" pageId="71" pageNumber="72">Etymology</emphasis>
