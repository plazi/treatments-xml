, 
<quantity id="61729B7AFFF72A74FC38FBCEFBD6FBC2" box="[993,1067,1083,1104]" metricMagnitude="3" metricUnit="m" metricValue="1.258824" pageId="0" pageNumber="272" unit="ft" value="4130.0">
