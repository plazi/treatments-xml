<collectingCountry id="BAA5764F9E351964FECCFCDAFE1CFB20" box="[359,426,1235,1260]" name="China" pageId="38" pageNumber="39">China</collectingCountry>
(
<collectingRegion id="0076F83D9E351964FE13FCDDFDA7FB20" box="[440,529,1236,1260]" country="China" name="Yunnan" pageId="38" pageNumber="39">Yunnan</collectingRegion>
