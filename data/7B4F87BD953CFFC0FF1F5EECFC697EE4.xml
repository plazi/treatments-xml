<paragraph id="F35936AB953CFFC0FF1F5F9BFB607CCF" blockId="42.[151,1436,341,428]" box="[151,1204,405,428]" pageId="42" pageNumber="43">
<treatmentCitationGroup id="D3F61185953CFFC0FF1F5F9BFB607CCF" box="[151,1204,405,428]" pageId="42" pageNumber="43">
<taxonomicName id="34E64D28953CFFC0FF1F5F9BFEBF7CCF" authorityName="Gallardo" authorityYear="1969" box="[151,363,405,428]" class="Polychaeta" family="Serpulidae" genus="Salmacina" kingdom="Animalia" order="Sabellida" pageId="42" pageNumber="43" phylum="Annelida" rank="species" species="chilensis">
