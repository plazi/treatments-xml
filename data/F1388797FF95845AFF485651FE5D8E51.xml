</taxonomicName>
in 
<collectingCountry id="01867611FF96845AFF2D5294FEA18E19" box="[242,305,1015,1041]" name="Chile" pageId="66" pageNumber="67">Chile</collectingCountry>
, based on samples from 
<taxonomicName id="BE914D02FF96845AFDBD5294FCE08E19" authorityName="Lesson" authorityYear="1825" box="[610,880,1015,1041]" class="Aves" family="Diomedeidae" genus="Diomedea" kingdom="Animalia" order="Procellariiformes" pageId="66" pageNumber="67" phylum="Chordata" rank="species" species="epomophora">
