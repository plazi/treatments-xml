<paragraph id="A7D29F1CEDEB1835553A3BDDDFA7A727" pageId="17" pageNumber="28">
<taxonomicName id="DF92FC5510452253D8FF5FD6ECC87C9E" baseAuthorityName="Draparnaud" baseAuthorityYear="1805" class="Gastropoda" family="Physidae" genus="Physella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Basommatophora" pageId="17" pageNumber="28" phylum="Mollusca" rank="species" species="acuta">
