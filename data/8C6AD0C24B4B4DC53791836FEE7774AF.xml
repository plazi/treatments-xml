<taxonomicName id="74CF73187B3CB199E65E162D31F6F392" authorityName="Montfort" authorityYear="1810" class="Gastropoda" family="Lymnaeidae" genus="Radix" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Basommatophora" pageId="17" pageNumber="28" phylum="Mollusca" rank="genus">
