, 
<taxonomicName id="7BDA5E46C3D4AA19C7B944834E1CF39E" authorityName="Gloeer &amp; Pesic" authorityYear="2009" class="Gastropoda" family="Bithyniidae" genus="Pseudobithynia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Littorinimorpha" pageId="17" pageNumber="28" phylum="Mollusca" rank="species" species="zagrosia">
