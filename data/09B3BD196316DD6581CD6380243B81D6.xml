<taxonomicName id="CB9403495E2A71B9774E1077F330EA7D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25301" class="Insecta" family="Formicidae" genus="Anochetus" kingdom="Animalia" lsidName-HNS="Anochetus princeps Emery" order="Hymenoptera" pageId="6" pageNumber="379" phylum="Arthropoda" rank="species" species="princeps">Anochetus princeps</taxonomicName>
