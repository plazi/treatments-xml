<taxonomicName id="4E3831559FF9048F7AC88CF80DEBF837" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis mucronata" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="mucronata">Chrysis mucronata</taxonomicName>
