<taxonomicName id="98E17CD8343CAA99C51944515C15E3ED" authorityName="Y. Lin &amp; S. Li" authorityYear="2020" class="Arachnida" family="Symphytognathidae" genus="Crassignatha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crassignatha changyan" order="Araneae" pageId="0" pageNumber="63" phylum="Arthropoda" rank="species" species="changyan">
<emphasis id="65ED060FB07602D1FAC6CAF03463A550" italics="true" pageId="0" pageNumber="63">Crassignatha changyan</emphasis>
</taxonomicName>
sp. nov. 
<emphasis id="AB8C3890C3F410CBC7A3495C747BF59C" bold="true" pageId="0" pageNumber="63">A</emphasis>
habitus, dorsal 
<emphasis id="0E152D5109F36D3932B65514BC7103A5" bold="true" pageId="0" pageNumber="63">B</emphasis>
habitus, ventral 
<emphasis id="CB88D7E4663AC4987DA445175CA40111" bold="true" pageId="0" pageNumber="63">C</emphasis>
habitus, lateral 
<emphasis id="7000767DB5B8FCFFB70E487981F31A27" bold="true" pageId="0" pageNumber="63">D</emphasis>
epigyne, ventral 
