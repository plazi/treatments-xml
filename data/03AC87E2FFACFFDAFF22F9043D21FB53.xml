<bibRefCitation id="EF944B05FFACFFDAFCDBFBEB3D51FB26" author="Mees, G. F." box="[804,1078,1132,1154]" pageId="23" pageNumber="23" pagination="3 - 168" refId="ref156789" refString="Mees, G. F. 1961 a. A systematic review of the Indo-Australian Zosteropidae (Part II). Zoologische Verhandelingen 50: 3 - 168, 4 pls." type="book chapter" year="1961">Mees, 1961a: 145–147</bibRefCitation>
; 
<bibRefCitation id="EF944B05FFACFFDAFBB6FBEB3A5EFB3B" author="van Balen, S." pageId="23" pageNumber="23" pagination="402 - 485" refId="ref162714" refString="van Balen, S. 2008. Family Zosteropidae (whiteeyes). In J. del Hoyo, A. Elliot, and D. Christie (editors), Handbook of birds of the world, vol. 13, Penduline-tits to shrikes: 402 - 485. Barcelona: Lynx Edicions, 879 pp., 60 pls., 536 photographs." type="book chapter" year="2008">van Balen, 2008: 463</bibRefCitation>
), as species in the superspecies 
<taxonomicName id="4C054D77FFACFFDAFD42FB213B95FB7E" authority="(Mayr and Diamond, 2001: 396 - 397)" baseAuthorityName="Mayr and Diamond" baseAuthorityPageNumber="396 - 397" baseAuthorityYear="2001" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="23" phylum="Chordata" rank="species" species="luteirostris">
<emphasis id="B971EAE6FFACFFDAFD42FB213A51FB19" box="[701,822,1190,1213]" italics="true" pageId="23" pageNumber="23">luteirostris</emphasis>
(
<bibRefCitation id="EF944B05FFACFFDAFCB6FB203B8DFB7E" author="Mayr, E. &amp; J. M. Diamond" pageId="23" pageNumber="23" refId="ref156273" refString="Mayr, E., and J. M. Diamond. 2001. The birds of northern Melanesia. Oxford: Oxford University Press, 492 pp." type="book" year="2001">Mayr and Diamond, 2001: 396– 397</bibRefCitation>
)
</taxonomicName>
, or as subspecies in 
<taxonomicName id="4C054D77FFACFFDAFC26FB423D14FB7E" baseAuthorityName="Mayr and Diamond" baseAuthorityYear="2001" box="[985,1139,1219,1242]" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="23" pageNumber="23" phylum="Chordata" rank="species" species="luteirostris">
<emphasis id="B971EAE6FFACFFDAFC26FB423D14FB7E" box="[985,1139,1219,1242]" italics="true" pageId="23" pageNumber="23">Z. luteirostris</emphasis>
</taxonomicName>
(
<bibRefCitation id="EF944B05FFACFFDAFB7DFB433A57FB52" author="Mayr, E." pageId="23" pageNumber="23" pagination="289 - 326" refId="ref156145" refString="Mayr, E. 1967. Family Zosteropidae, Indo-Australian taxa. In R. A. Paynter, Jr. (editor), Check-list of birds of the world, vol. 12: 289 - 326. Cambridge, MA: Museum of Comparative Zoology, ix + 495 pp." type="book chapter" year="1967">Mayr, 1967: 312</bibRefCitation>
; 
<bibRefCitation id="EF944B05FFACFFDAFCC1FB663D5CFB53" author="Dickinson, E. C." box="[830,1083,1249,1271]" pageId="23" pageNumber="23" refId="ref149554" refString="Dickinson, E. C. (editor). 2003. The Howard and Moore complete checklist of the birds of the world. 3 rd ed. London: Christopher Helm, 1039 pp." type="book" year="2003">Dickinson, 2003: 628</bibRefCitation>
