<taxonomicName id="4C484D0CFFF9E925EE90FA7E9B509452" authority="de Bruijn, 1966" authorityName="de Bruijn" authorityYear="1966" class="Mammalia" family="Gliridae" genus="Muscardinus" kingdom="Animalia" order="Rodentia" pageId="8" pageNumber="315" phylum="Chordata" rank="species" species="hispanicus">
