<taxonomicName id="58E52ED0FFE62E450C139C789194FACD" authorityName="Soleglad and Fet" authorityYear="2008" box="[1126,1228,1313,1333]" class="Arachnida" family="Vaejovidae" genus="Thorellius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scorpiones" pageId="21" pageNumber="21" phylum="Arthropoda" rank="genus">
<emphasis id="AD918941FFE62E450C139C789194FACD" box="[1126,1228,1313,1333]" italics="true" pageId="21" pageNumber="21">Thorellius</emphasis>
