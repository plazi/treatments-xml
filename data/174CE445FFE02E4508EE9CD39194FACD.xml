<emphasis id="AD918941FFE62E450BEC9B4E96E9FDD1" box="[921,945,535,553]" italics="true" pageId="21" pageNumber="21">Et</emphasis>
<subScript id="03615716FFE62E450BC49B4796E2FDD4" attach="left" box="[945,954,542,556]" fontSize="6" pageId="21" pageNumber="21">4</subScript>
