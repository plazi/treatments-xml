<taxonomicName id="58E52ED0FFE62E450A869B6B9602FDBC" baseAuthorityName="Williams" baseAuthorityYear="1980" box="[755,858,560,580]" class="Arachnida" family="Vaejovidae" genus="Paravaejovis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scorpiones" pageId="21" pageNumber="21" phylum="Arthropoda" rank="species" species="pumilis">
<emphasis id="AD918941FFE62E450A869B6B9602FDBC" box="[755,858,560,580]" italics="true" pageId="21" pageNumber="21">P. pumilis</emphasis>
</taxonomicName>
............... 
<taxonomicName id="58E52ED0FFE62E450C3E9B6B9194FDBC" authorityName="Williams" authorityYear="1980" box="[1099,1228,562,580]" class="Arachnida" family="Vaejovidae" genus="Paravaejovis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scorpiones" pageId="21" pageNumber="21" phylum="Arthropoda" rank="genus">
<emphasis id="AD918941FFE62E450C3E9B6B9194FDBC" box="[1099,1228,562,580]" italics="true" pageId="21" pageNumber="21">Paravaejovis</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
