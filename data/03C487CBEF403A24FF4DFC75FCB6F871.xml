<paragraph id="8BD236DDEF403A24FC9AF89CFCB6F871" blockId="17.[810,1452,445,1968]" pageId="17" pageNumber="18">
<taxonomicName id="4C6D4D5EEF403A24FC9AF89CFC2EF8AD" authorityName="Allman" authorityYear="1847" box="[863,1003,1874,1900]" class="Maxillopoda" family="Notodelphyidae" genus="Notodelphys" kingdom="Animalia" order="Cyclopoida" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis id="B919EACFEF403A24FC9AF89CFC2EF8AD" box="[863,1003,1874,1900]" italics="true" pageId="17" pageNumber="18">Notodelphys</emphasis>
</taxonomicName>
