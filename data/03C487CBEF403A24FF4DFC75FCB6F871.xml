<taxonomicName id="4C6D4D5EEF403A24FC54FB20FB65FAC9" authority="Thorell, 1859" authorityName="Thorell" authorityYear="1859" box="[913,1184,1261,1288]" class="Maxillopoda" family="Notodelphyidae" genus="Notodelphys" kingdom="Animalia" order="Cyclopoida" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="allmani">
