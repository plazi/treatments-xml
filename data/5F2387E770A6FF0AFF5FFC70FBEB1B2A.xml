<taxonomicName id="108A4D7270A5FF04FEEEF86CFE161FDC" authorityName="New" authorityYear="1985" box="[294,510,1993,2017]" class="Insecta" family="Myrmeleontidae" genus="Glenoleon" kingdom="Animalia" order="Neuroptera" pageId="166" pageNumber="167" phylum="Arthropoda" rank="species" species="maculatus">
