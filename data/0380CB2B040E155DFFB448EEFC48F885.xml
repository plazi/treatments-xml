<taxonomicName id="4C2901BE040E155DFD6A497EFD2CF885" box="[585,711,1879,1905]" class="Gastropoda" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="8" phylum="Mollusca" rank="order">Orthoptera</taxonomicName>
