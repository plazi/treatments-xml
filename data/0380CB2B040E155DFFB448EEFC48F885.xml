<taxonomicName id="4C2901BE040E155DFC86491AFB00F8B9" authority="Jousseaume, 1884" authorityName="Jousseaume" authorityYear="1884" box="[933,1259,1843,1869]" class="Gastropoda" family="Triphoridae" genus="Euthymia" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="8" phylum="Mollusca" rank="genus">
