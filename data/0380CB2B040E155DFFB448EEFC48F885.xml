<taxonomicName id="4C2901BE040E155DFE74497EFDD1F885" authority="Stal, 1875" authorityName="Stal" authorityYear="1875" box="[343,570,1878,1905]" class="Gastropoda" family="Triphoridae" genus="Euthymia" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="8" phylum="Mollusca" rank="genus">
