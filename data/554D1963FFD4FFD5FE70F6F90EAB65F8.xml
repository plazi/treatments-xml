<quantity id="1A1C0590FFD6FFD5FCF9F2630C4A6028" box="[805,867,360,383]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="42" pageNumber="804" unit="mm" value="7.0">7 mm</quantity>
