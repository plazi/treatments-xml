<quantity id="1A1C0590FFD6FFD5FDB7F26C0D9F6028" box="[619,694,359,383]" metricMagnitude="-2" metricUnit="m" metricValue="9.4" pageId="42" pageNumber="804" unit="mm" value="94.0">94 mm</quantity>
