<quantity id="1A1C0590FFD6FFD5FE12F26C0D0E6028" box="[462,551,359,383]" metricMagnitude="-1" metricUnit="m" metricValue="1.22" pageId="42" pageNumber="804" unit="mm" value="122.0">122 mm</quantity>
