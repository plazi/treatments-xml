<taxonomicName id="DDCFBFFC1F4DED34B833FA52FDDA3E68" authority="Hsu, Mao &amp; Li and Schim" authorityName="Hsu, Mao &amp; Li and Schim" class="Magnoliopsida" family="Fagaceae" genus="Cyclobalanopsis" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="2" phylum="Tracheophyta" rank="variety" species="stewardiana" variety="longicaudata">
