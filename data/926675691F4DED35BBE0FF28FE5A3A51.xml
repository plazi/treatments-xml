<taxonomicName id="DDCFBFFC1F4DED34BAA7FA52FF6C3E48" authority="Franch" authorityName="Franch" box="[464,823,1517,1542]" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ericales" pageId="2" phylum="Tracheophyta" rank="species" species="delavayi">
