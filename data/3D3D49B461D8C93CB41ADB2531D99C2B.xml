<taxonomicName id="1124B771A2B6C5F6AD44DB06940F6671" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Picea" order="Pinales" pageId="0" pageNumber="35" phylum="Tracheophyta" rank="genus">Picea</taxonomicName>
