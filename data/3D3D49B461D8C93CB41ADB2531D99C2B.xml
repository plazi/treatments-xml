, 
<taxonomicName id="CF9964A66426591E7BB84C39903381D5" class="Pinopsida" family="Pinaceae" genus="Tsuga" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Tsuga" order="Pinales" pageId="0" pageNumber="35" phylum="Tracheophyta" rank="genus">Tsuga</taxonomicName>
