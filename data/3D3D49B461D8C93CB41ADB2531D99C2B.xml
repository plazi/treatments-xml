<taxonomicName id="0D2A56A032B070821E29EA511048C59D" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fagus" order="Fagales" pageId="0" pageNumber="35" phylum="Tracheophyta" rank="genus">Fagus</taxonomicName>
