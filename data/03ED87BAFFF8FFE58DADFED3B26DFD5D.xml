<typeStatus id="54FF880EFFF8FFE58CEAFDB9B239FDA1" box="[927,1066,513,534]" pageId="1" pageNumber="322">SYNTYPES</typeStatus>
<taxonomicName id="4C444D2FFFF8FFE58B4FFDB9B5C9FD87" authority="Motschulsky" authorityName="Motschulsky" authorityYear="1855" class="Insecta" family="Staphylinidae" genus="Bryaxis" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="322" phylum="Arthropoda" rank="species" species="consanguinea">Bryaxis consanguinea Motschulsky</taxonomicName>
// [ZIN]. The left male is a different species of 
<taxonomicName id="4C444D2FFFF8FFE58B6FFD8EB500FDD1" authority="Brendel" authorityName="Brendel" authorityYear="1865" class="Insecta" family="Staphylinidae" genus="Decarthron" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="322" phylum="Arthropoda" rank="genus">
