<subSubSection id="C356BDA8FF8CED4BFF2EFCC29F3CFC7E" pageId="48" type="materials_examined">
<paragraph id="8BF3EE23FF8CED4BFF2EFCC29F3CFC7E" blockId="48.[120,649,875,1721]" pageId="48">
TYPE: Male 
<typeStatus id="54F75081FF8CED4BFEACFCC59C46FC1B" box="[282,375,875,899]" pageId="48">holotype</typeStatus>
from 
<collectingCountry id="F35BAEB3FF8CED4BFE0AFCC59F1BFC1B" box="[444,554,875,899]" name="Venezuela" pageId="48">Venezuela</collectingCountry>
: 
<collectingRegion id="498820C1FF8CED4BFD80FCC59FB7FC1B" box="[566,646,875,899]" country="Venezuela" name="Bolivar" pageId="48">Bolivar</collectingRegion>
: 
<quantity id="4CB443C6FF8CED4BFFCEFC229D88FC3C" box="[120,185,908,932]" metricMagnitude="4" metricUnit="m" metricValue="1.2" pageId="48" unit="km" value="12.0">12 km</quantity>
N Luepa, 
<quantity id="4CB443C6FF8CED4BFEA9FC229C5AFC3C" box="[287,363,908,932]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="48" unit="m" value="1500.0">1500 m</quantity>
(5.852155°, -61.51719722°), Gran Sabana, 
<date id="FFF2C8E3FF8CED4BFEAFFC009CE2FC5E" box="[281,467,942,966]" pageId="48" value="1987-06-01" valueMax="1987-06-11" valueMin="1987-06-01">June 1–11, 1987</date>
