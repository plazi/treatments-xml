<taxonomicName id="4C0056CDEB3C273A0BCCFCF3FD47FC83" authority="sect. Epipactis" box="[386,666,780,806]" class="Liliopsida" family="Orchidaceae" genus="Epipactis" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="28" phylum="Tracheophyta" rank="section" section="Epipactis">
