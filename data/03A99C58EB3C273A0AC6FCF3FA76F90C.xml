sect. 
<emphasis id="B974F15CEB3C273A0861FCF3FD47FC83" bold="true" box="[559,666,780,806]" italics="true" pageId="6" pageNumber="28">Epipactis</emphasis>
</taxonomicName>
in the 
<collectingRegion id="49C4E3ACEB3C273A08A2FCF3FC9BFC83" box="[748,838,780,806]" country="Ukraine" name="Crimea" pageId="6" pageNumber="28">Crimea</collectingRegion>
