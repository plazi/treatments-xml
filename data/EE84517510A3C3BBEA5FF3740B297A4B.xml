<normalizedToken id="97234010C8555205A4D0A9A32B0F1FD4" originalValue="Сыръ-дарьей">Syr-darei</normalizedToken>
<normalizedToken id="B6499B53FF673619E542EBE75A3C4A39" originalValue="и">i</normalizedToken>
