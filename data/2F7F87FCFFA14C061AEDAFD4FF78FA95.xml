Genus 
<taxonomicName id="60D64D69FFA14C061A97AFD3FD81FC9A" authority="Kethley" authorityName="Kethley" box="[237,550,755,782]" class="Arachnida" family="Syringophilidae" genus="Syringophiloidus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="58" phylum="Arthropoda" rank="genus">
