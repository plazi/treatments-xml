<paragraph id="B92C36C1FF8E6B14AE8B2045FD034ACC" blockId="58.[325,788,1364,1421]" box="[438,677,1396,1421]" pageId="58" pageNumber="539">
(®gures 
<quantity id="7E6B9B24FF8E6B14AD2E2045FDE54ACC" box="[531,579,1396,1420]" metricMagnitude="1" metricUnit="m" metricValue="1.9" pageId="58" pageNumber="539" unit="m" value="19.0">19m</quantity>
, n, 20k)
</paragraph>
