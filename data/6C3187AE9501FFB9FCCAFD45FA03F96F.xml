<taxonomicName id="23984D3B9501FFB9FAF8F995FC25F9B3" authorityName="WAGNER" authorityYear="1858" class="Reptilia" family="Metriorhynchidae" genus="Cricosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crocodylia" pageId="51" pageNumber="852" phylum="Chordata" rank="genus">
