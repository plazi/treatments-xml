<taxonomicName id="D0A607B5CE91BE1C08A4153B605EFC68" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25512" class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" lsidName-HNS="Aphaenogaster strioloides Forel" order="Hymenoptera" pageNumber="71" phylum="Arthropoda" rank="species" species="strioloides">strioloides</taxonomicName>
