<subSubSection id="CFDA0FBB6F31FFC7FF13BE6692D4FD8B" pageId="3" pageNumber="67" type="nomenclature">
<paragraph id="877F5C306F31FFC7FF13BE669205FE94" blockId="3.[151,336,295,321]" box="[151,336,295,321]" pageId="3" pageNumber="67">
<heading id="DC37EB5C6F31FFC7FF13BE669205FE94" bold="true" box="[151,336,295,321]" fontSize="11" level="1" pageId="3" pageNumber="67" reason="1">
<emphasis id="B5B480226F31FFC7FF13BE669205FE94" bold="true" box="[151,336,295,321]" pageId="3" pageNumber="67">
<taxonomicName id="40C027B36F31FFC7FF13BE669243FE94" box="[151,278,295,321]" class="Insecta" family="Heptageniidae" genus="Afronurus" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="67" phylum="Arthropoda" rank="genus">
