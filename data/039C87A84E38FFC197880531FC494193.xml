<paragraph id="8B8A36BE4E38FFFE978805CCFDEE42D0" blockId="31.[151,637,1903,1963]" box="[151,637,1938,1963]" pageId="31" pageNumber="232">
(Figs. 
<number id="3DFD24994E38FFFE97C105CDFE8B42D0" box="[222,280,1939,1963]" pageId="31" pageNumber="232" value="9.31">9, 31</number>
, 
<number id="3DFD24994E38FFFE963B05CCFEFF42D0" box="[292,364,1938,1963]" pageId="31" pageNumber="232" value="50.69">50, 69</number>
, 87, 115, 133, 180– 
<number id="3DFD24994E38FFFE955405CDFDE742D0" box="[587,628,1939,1963]" pageId="31" pageNumber="232" value="183.0">183</number>
)
</paragraph>
