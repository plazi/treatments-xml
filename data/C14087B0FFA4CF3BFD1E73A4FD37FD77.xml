<collectionCode id="2FF8AE63FFA4CF26FD33709AFC26FCFB" box="[728,912,750,773]" pageId="15" pageNumber="16">TYPE LOCALITY</collectionCode>
: 
<location id="4C36607DFFA4CF26FC4D709AFCF1FCDD" LSID="urn:lsid:plazi:treatment:C14087B0FFA4CF3BFD1E73A4FD37FD77:4C36607DFFA4CF26FC4D709AFCF1FCDD" country="Jamaica" county="Entrance" municipality="Jackson's Bay" name="Brown Dust Passage of Drum Cave" pageId="15" pageNumber="16" stateProvince="Portland">Brown Dust Passage of Drum Cave</location>
, near 
<location id="4C36607DFFA4CF26FC647178FC40FCDD" LSID="urn:lsid:plazi:treatment:C14087B0FFA4CF3BFD1E73A4FD37FD77:4C36607DFFA4CF26FC647178FC40FCDD" box="[911,1014,780,803]" country="Jamaica" county="Entrance" municipality="Jackson's Bay" name="Entrance" pageId="15" pageNumber="16" stateProvince="Portland">Entrance</location>
#3 (fig. 1). 
<location id="4C36607DFFA4CF26FB6C7178FD4CFCBE" LSID="urn:lsid:plazi:treatment:C14087B0FFA4CF3BFD1E73A4FD37FD77:4C36607DFFA4CF26FB6C7178FD4CFCBE" country="Jamaica" county="Entrance" municipality="Jackson's Bay" name="Drum Cave" pageId="15" pageNumber="16" stateProvince="Portland">Drum Cave</location>
, part of the ‘‘upper’’ sequence of the ~ 
<locationDeviation id="58E42F84FFA4CF26FD3A7132FB94FCA3" box="[721,1058,838,861]" location="Jackson's Bay cave" pageId="15" pageNumber="16">
