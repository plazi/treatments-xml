<location id="4C36607DFFA4CF26FCC771F5FCC7FC66" LSID="urn:lsid:plazi:treatment:C14087B0FFA4CF3BFD1E73A4FD37FD77:4C36607DFFA4CF26FCC771F5FCC7FC66" box="[812,881,897,920]" country="Jamaica" county="Entrance" municipality="Jackson's Bay" name="Ridge" pageId="15" pageNumber="16" stateProvince="Portland">Ridge</location>
