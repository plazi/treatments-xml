<taxonomicName id="4C740FA26A05FFB5504D3EB705DEF943" box="[610,717,1738,1759]" class="Insecta" family="Lonchaeidae" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="360" phylum="Arthropoda" rank="tribe" tribe="Earomyiini">Earomyiini</taxonomicName>
