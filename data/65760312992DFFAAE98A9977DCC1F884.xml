<quantity id="2A271FE1992DFFAAE8329E30DE32FAA7" box="[303,339,1277,1306]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="30" pageNumber="31" unit="in" value="1.0">1 in</quantity>
