<taxonomicName id="4C774D7DED60FFD514DBFC99FD89FC9A" box="[407,552,849,875]" class="Demospongiae" family="Raspailiidae" kingdom="Animalia" order="Poecilosclerida" pageId="2" pageNumber="39" phylum="Porifera" rank="family">Raspailiidae</taxonomicName>
