: 
<taxonomicName id="868565D0FF5D4B34187035B61E74F85F" authority="Assing, 2015" authorityName="Assing" authorityYear="2015" box="[316,526,1999,2020]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="63" phylum="Arthropoda" rank="species" species="abruptum" status="spec. nov.">
