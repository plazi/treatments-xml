<subSubSection id="F79A6506FF07FF15FAC3FECCD00B1C17" box="[199,1034,331,358]" pageId="226" pageNumber="227" type="etymology">
<paragraph id="BF3F368DFF07FF15FAC3FECCD00B1C17" blockId="226.[151,1437,259,1474]" box="[199,1034,331,358]" pageId="226" pageNumber="227">
<emphasis id="8DF4EA9FFF07FF15FAC3FECCD54F1C17" bold="true" box="[199,334,331,357]" pageId="226" pageNumber="227">Etymology.</emphasis>
