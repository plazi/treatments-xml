species.
</emphasis>
<taxonomicName id="4C124D0BFF89E97D4774434544BD3037" authority="Lukashevich, 1995: 170" authorityName="Lukashevich" authorityPageNumber="170" authorityYear="1995" box="[298,744,1853,1878]" class="Insecta" family="Ptychopteridae" genus="Zhiganka" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="567" phylum="Arthropoda" rank="species" species="comitans">
