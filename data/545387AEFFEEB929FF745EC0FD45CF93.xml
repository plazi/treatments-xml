<taxonomicName id="1BFA4D3BFFEEB92EFCAF5979FC28CC63" box="[844,970,1823,1847]" class="Malacostraca" family="Tanaididae" genus="Hexapleomera" kingdom="Animalia" order="Tanaidacea" pageId="3" pageNumber="321" phylum="Arthropoda" rank="species" species="urashima">
