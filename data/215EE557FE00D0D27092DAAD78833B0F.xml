<subSubSection id="A994AA96283EB25B64BF1E18223015A7" type="nomenclature">
<paragraph id="4E553C63E017D5456519FDD6D97B0CA6" pageId="0" pageNumber="131" type="mainText">
<typeStatus id="0ACAF3DD9434381680104AE538769FC3" pageId="0" pageNumber="131">Type</typeStatus>
species: 
<taxonomicName id="BF899D8F1F561F02BA30CD2A1E0688ED" authority="Simon, 1902" authorityName="Simon" class="Arachnida" family="Penestomidae" genus="Penestomus" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="131" phylum="Arthropoda" rank="species" species="planus">Penestomus planus Simon</taxonomicName>
