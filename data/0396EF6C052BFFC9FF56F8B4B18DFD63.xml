<collectingCountry id="F3281EEA052AFFC9FF23FE18B50AFE11" box="[226,283,474,496]" name="China" pageId="1" pageNumber="389">China</collectingCountry>
(
<collectingRegion id="49FB9098052AFFC9FEE9FE18B561FE11" box="[296,368,474,496]" country="China" name="Hainan" pageId="1" pageNumber="389">Hainan</collectingRegion>
