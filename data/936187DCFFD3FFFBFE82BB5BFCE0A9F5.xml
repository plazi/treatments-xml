<quantity id="DC309B2FFFD3FFFAFD40BE3EFCCDAB14" box="[687,787,1640,1663]" metricMagnitude="-2" metricUnit="m" metricValue="5.01" pageId="28" pageNumber="1543" unit="mm" value="50.1">50.1 mm</quantity>
