<taxonomicName id="A7FB4D03582CFFC6FE5AFA303353FED7" authority="Walker" authorityName="Walker" box="[441,584,1438,1462]" class="Insecta" family="Lymantriidae" genus="Arna" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="genus">
