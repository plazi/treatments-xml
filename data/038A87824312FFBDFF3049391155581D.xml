: 
<number id="3DEB24B34311FFBDFC404D62103B5941" box="[999,1044,573,597]" pageId="6" pageNumber="451" value="280.0">280</number>
, egg, nymphs and adults described and 
<taxonomicName id="4C234D174311FFBDFF6D4D3E1510596D" box="[202,319,609,633]" class="Insecta" family="Echinophthiriidae" genus="Antarctophthirus" kingdom="Animalia" order="Phthiraptera" pageId="6" pageNumber="451" phylum="Arthropoda" rank="species" species="illustrated">illustrated</taxonomicName>
); Kim (
<number id="3DEB24B34311FFBDFE3E4D3E15F9596D" box="[409,470,609,633]" pageId="6" pageNumber="451" value="1972.0">1972</number>
: 
<number id="3DEB24B34311FFBDFE434D3E1630596D" box="[484,543,609,633]" pageId="6" pageNumber="451" value="2027.0">2027</number>
, population dynamics); Kim 
<emphasis id="B957EA864311FFBDFCD44D3D1781596D" box="[883,942,609,633]" italics="true" pageId="6" pageNumber="451">et al.</emphasis>
(
<number id="3DEB24B34311FFBDFC664D3E17D1596D" box="[961,1022,609,633]" pageId="6" pageNumber="451" value="1974.0">1974</number>
: 
<number id="3DEB24B34311FFBDFBAC4D3E1018596D" box="[1035,1079,609,633]" pageId="6" pageNumber="451" value="281.0">281</number>
, mercury contamination); Kim (
<number id="3DEB24B34311FFBDFF384DDA14F35989" box="[159,220,645,669]" pageId="6" pageNumber="451" value="1975.0">1975</number>
: 
<number id="3DEB24B34311FFBDFF4F4DDA153A5989" box="[232,277,645,669]" pageId="6" pageNumber="451" value="504.0">504</number>
, ecology; adaptation; population dynamics); Lyons 
<emphasis id="B957EA864311FFBDFCFA4DD917BA5989" box="[861,917,645,669]" italics="true" pageId="6" pageNumber="451">et al.</emphasis>
(
<number id="3DEB24B34311FFBDFC014DDA17CC5989" box="[934,995,645,669]" pageId="6" pageNumber="451" value="1978.0">1978</number>
: 
<number id="3DEB24B34311FFBDFC494DDA10345989" box="[1006,1051,645,669]" pageId="6" pageNumber="451" value="463.0">463</number>
, control by pesticides); Kim 
<emphasis id="B957EA864311FFBDFAC44DD911B35989" box="[1379,1436,645,669]" italics="true" pageId="6" pageNumber="451">et al.</emphasis>
(
<number id="3DEB24B34311FFBDFF384DF614F359D5" box="[159,220,681,705]" pageId="6" pageNumber="451" value="1986.0">1986</number>
: 
<number id="3DEB24B34311FFBDFF404DF6152959D5" box="[231,262,681,705]" pageId="6" pageNumber="451" value="44.0">44</number>
, redescription, figures, biology); Durden &amp; Musser (
<number id="3DEB24B34311FFBDFCF34DF617BF59D5" box="[852,912,681,705]" pageId="6" pageNumber="451" value="1994.0">1994</number>
