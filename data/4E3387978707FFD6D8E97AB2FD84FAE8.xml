<taxonomicName id="019A4D028707FFD6D8A37D68FE58FA7D" authorityName="Caporiacco" authorityYear="1955" baseAuthorityName="Caporiacco" baseAuthorityYear="2020" box="[221,480,1091,1117]" class="Arachnida" family="Sparassidae" genus="Sparianthis" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="190" phylum="Arthropoda" rank="species" species="accentuata">
