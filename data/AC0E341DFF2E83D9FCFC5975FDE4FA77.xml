<taxonomicName id="E3A7FE88FF2E83DEFB2F5ED4FA05F937" box="[1259,1429,1716,1740]" class="Magnoliopsida" family="Brassicaceae" genus="Aethionema" kingdom="Plantae" order="Brassicales" pageId="147" pageNumber="147" phylum="Tracheophyta" rank="species" species="cardiophyllum">
