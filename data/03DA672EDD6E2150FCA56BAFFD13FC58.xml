<quantity id="4C8B7BDDDD6E2150FCA568E3FF50FD00" box="[178,250,700,722]" metricMagnitude="3" metricUnit="m" metricValue="3.1" pageId="9" pageNumber="608" unit="m" value="3100.0">3100m</quantity>
, 
<date id="FFCDF0F8DD6E2150FD1168E3FECAFD00" box="[262,352,700,722]" pageId="9" pageNumber="608" value="1965-07-05">5.7.1965</date>
, 5 14. Tuokexun, 
<date id="FFCDF0F8DD6E2150FE7E68E3FD7BFD00" box="[617,721,700,722]" pageId="9" pageNumber="608" value="1958-06-21">21.6.1958</date>
, 1. Hapahe, 
<quantity id="4C8B7BDDDD6E2150FF7068E3FC09FD00" box="[871,931,700,722]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="9" pageNumber="608" unit="m" value="500.0">500m</quantity>
