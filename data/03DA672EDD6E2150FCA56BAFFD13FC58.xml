<quantity id="4C8B7BDDDD6E2150FECA68DDFCB3FD4A" box="[733,793,642,664]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="9" pageNumber="608" unit="m" value="900.0">900m</quantity>
