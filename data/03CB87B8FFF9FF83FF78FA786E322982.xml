(
<bibRefCitation id="EFF34B5FFFFBFF83FBF4FAF26E0B2A16" author="Drake" box="[1051,1309,1355,1380]" pageId="7" pageNumber="8" refString="Drake, C. J. &amp; Harris, H. M. (1933) New American Veliidae (Hemiptera). Proceedings of the Biological Society of Washington, 46, 45 - 54." type="journal article" year="1933">
