<taxonomicName id="4C624D2DFFFBFF83FC5CFAF56E332A16" authority="Drake &amp; Harris, 1933" authorityName="Drake &amp; Harris" authorityYear="1933" box="[947,1317,1355,1380]" class="Insecta" family="Veliidae" genus="Paravelia" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="kahli">
