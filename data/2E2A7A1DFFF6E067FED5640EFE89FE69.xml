<taxonomicName id="6183B088FFF8E067FD19604EFCC6FE42" box="[708,783,364,389]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="cerula">
