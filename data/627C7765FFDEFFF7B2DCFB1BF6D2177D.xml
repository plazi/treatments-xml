<taxonomicName id="2DD5BDF0FFDEFFF7B2F3FA7FF39A175A" box="[184,449,1529,1554]" class="Asteroidea" family="Goniasteridae" genus="Evoplosoma" kingdom="Animalia" order="Valvatida" pageId="10" pageNumber="221" phylum="Echinodermata" rank="species" species="forcipifera">
