<taxonomicName id="2DD5BDF0FFDEFFF7B3ADFA7CF0BD175A" box="[486,742,1529,1554]" class="Asteroidea" family="Goniasteridae" genus="Hippasteria" kingdom="Animalia" order="Valvatida" pageId="10" pageNumber="221" phylum="Echinodermata" rank="species" species="muscipula">
