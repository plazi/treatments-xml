<taxonomicName id="4C754D4EFEB6FE85743DFF78FB06FA9E" authorityName="Bochkov, Fain and Skoracki" authorityYear="2004" box="[1055,1191,225,248]" class="Arachnida" family="Syringophilidae" genus="Mironovia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="314" pageNumber="315" phylum="Arthropoda" rank="species" species="coturnae">
