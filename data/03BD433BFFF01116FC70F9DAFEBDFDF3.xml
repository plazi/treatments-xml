<taxonomicName id="4C1489AEFFF01117FC70F9DAFB1FF9F0" authority="Klapalek, 1913" authorityName="Klapalek" authorityYear="1913" box="[951,1272,1591,1616]" class="Insecta" family="Styloperlidae" genus="Cerconychia" kingdom="Animalia" order="Plecoptera" pageId="0" pageNumber="10" phylum="Arthropoda" rank="genus">
