, 
<taxonomicName id="4C4A4D43FFB8FFC4DD81F359E7D20FE7" authority="(S. Moricand, 1834)" baseAuthorityName="S. Moricand" baseAuthorityYear="1834" box="[151,674,1398,1421]" class="Gastropoda" family="Odontostomidae" genus="Burringtonia" kingdom="Animalia" order="Stylommatophora" pageId="20" pageNumber="77" phylum="Mollusca" rank="species" species="pantagruelina">
