<taxonomicName id="89DD9E557DBA4AA1BCB6640AA2CAAA2A" class="Insecta" family="Yponomeutidae" genus="Eucalantica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucalantica polita" order="Lepidoptera" pageId="9" pageNumber="84" phylum="Arthropoda" rank="species" species="polita">Eucalantica polita</taxonomicName>
