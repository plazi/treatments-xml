<heading id="4B259F20FFEAFFEAFF51FBF4FDACFBAF" bold="true" box="[151,629,1125,1151]" fontSize="11" level="1" pageId="3" pageNumber="78" reason="1">
<taxonomicName id="D7D253CFFFEAFFEAFF51FBF4FDACFBAF" authority="Constantino, 1992" authorityName="Constantino" authorityYear="1992" box="[151,629,1125,1151]" class="Insecta" family="Termitidae" genus="Amitermes" kingdom="Animalia" order="Isoptera" pageId="3" pageNumber="78" phylum="Arthropoda" rank="species" species="aporema">
