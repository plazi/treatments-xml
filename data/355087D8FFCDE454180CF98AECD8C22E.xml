<quantity id="7A019B2BFFCEE4541906FB4EECC1C12E" box="[859,1002,1254,1281]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="55" pageNumber="56" unit="mm" value="0.45" valueMax="0.5" valueMin="0.4">0.4–0.5 mm</quantity>
