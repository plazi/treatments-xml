<quantity id="7A019B2BFFCEE45419CCFB37ECC7C096" box="[913,1004,1183,1209]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="55" pageNumber="56" unit="mm" value="0.2">0.2 mm</quantity>
