<collectingCountry id="7BBF7616FF86A71CFEE1FCBC1F9A0A3C" box="[359,426,799,824]" name="China" pageId="4" pageNumber="531">China</collectingCountry>
(
<collectingRegion id="C16CF864FF86A71CFE3FFCBC1C2A0A3C" box="[441,538,799,824]" country="China" name="Sichuan" pageId="4" pageNumber="531">Sichuan</collectingRegion>
