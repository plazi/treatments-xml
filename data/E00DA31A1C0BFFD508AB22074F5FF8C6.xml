<paragraph id="681B120C1C0AFFD508FA275D4F7AFDCE" blockId="15.[151,1436,155,633]" pageId="15" pageNumber="16">
<taxonomicName id="AFA4698F1C0AFFD508FA275D4C7FFD9E" box="[198,317,528,553]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="genus">
<emphasis id="5AD0CE1E1C0AFFD508FA275D4C7FFD9E" box="[198,317,528,553]" italics="true" pageId="15" pageNumber="16">Epigynum</emphasis>
</taxonomicName>
