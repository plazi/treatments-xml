<subSubSection id="20BE41871C0AFFD508FA2312494CF97A" pageId="15" pageNumber="16" type="diagnosis">
<paragraph id="681B120C1C0AFFD508FA2312494CF97A" blockId="15.[151,1437,1631,1905]" pageId="15" pageNumber="16">
<emphasis id="5AD0CE1E1C0AFFD508FA23124C04F9CE" bold="true" box="[198,326,1631,1657]" pageId="15" pageNumber="16">Diagnosis.</emphasis>
