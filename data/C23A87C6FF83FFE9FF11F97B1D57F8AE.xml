<date id="3E2D1010FF83FFE8FF41F84B1D5FF80C" box="[199,345,2011,2036]" pageId="48" pageNumber="49" value="1974-04-26">
<number id="FC5B24F7FF83FFE8FF41F84B1CE5F80B" box="[199,227,2011,2036]" pageId="48" pageNumber="49" value="26.0">26</number>
Apr 
<number id="FC5B24F7FF83FFE8FE9BF84B1D5FF80C" box="[285,345,2011,2035]" pageId="48" pageNumber="49" value="1974.0">1974</number>
