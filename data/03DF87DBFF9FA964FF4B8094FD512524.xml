<taxonomicName id="4C764D4EFF9FA965FE858562FD502138" box="[345,723,1876,1899]" class="Insecta" family="Drosophilidae" genus="Stegana" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="apicosetosa" status="sp. nov." subGenus="Oxyphortica">
