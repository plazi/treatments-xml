<subSubSection id="DDAC6558FF9FFFFDFF72FEDB33AB45A9" pageId="13" pageNumber="592" type="etymology">
<paragraph id="950936D3FF9FFFFDFF72FEDB33AB45A9" blockId="13.[151,1437,151,392]" pageId="13" pageNumber="592">
<emphasis id="A7C2EAC1FF9FFFFDFF72FEDB33054544" bold="true" box="[199,334,332,357]" inLineHeading="true" pageId="13" pageNumber="592" reason="1">Etymology.</emphasis>
<emphasis id="A7C2EAC1FF9FFFFDFEE3FEDB33B74545" box="[342,508,332,356]" italics="true" pageId="13" pageNumber="592">Calidopectoris</emphasis>
