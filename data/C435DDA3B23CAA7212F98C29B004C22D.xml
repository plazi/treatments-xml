<taxonomicName id="555B54E888E372E0BF4300BA49EE888A" LSID-ZBK="urn:lsid:zoobank.org:act:22BEB194-156A-4DF7-A3A1-A7CF248B9EB2" class="Actinopterygii" family="Pimelodidae" genus="Pimelodus" kingdom="Animalia" lsidName-ZBK="Pimelodus commersonii Lacépède 1803:95, 103" order="Siluriformes" pageNumber="65" phylum="Chordata" rank="species" species="commersonii">Pimelodus commersonii</taxonomicName>
