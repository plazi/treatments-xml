, 
<taxonomicName id="9A2ACF3ED084413EB195D0B6332C18EF" LSID-ZBK="urn:lsid:zoobank.org:act:4781B086-6A2E-487B-8D97-1D1A856A235A" class="Actinopterygii" family="Ariidae" genus="Galeichthys" kingdom="Animalia" lsidName-ZBK="Galeichthys Valenciennes 1840:28" order="Siluriformes" pageNumber="63" phylum="Chordata" rank="genus">Galeichthys</taxonomicName>
and 
<taxonomicName id="2F022CEF09BB3A698B46C4A77533298D" class="Actinopterygii" family="Ariidae" genus="Netuma" kingdom="Animalia" order="Siluriformes" pageNumber="63" phylum="Chordata" rank="species" species="bilineatus">Netuma bilineatus</taxonomicName>
, 
<taxonomicName id="6F031D3843DAAB8315AFE8CD99705026" class="Insecta" family="Cerambycidae" genus="Sciades" kingdom="Animalia" order="Coleoptera" pageNumber="63" phylum="Arthropoda" rank="genus">Sciades</taxonomicName>
