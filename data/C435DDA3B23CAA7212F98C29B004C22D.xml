<taxonomicName id="DE240BE822D2D186572E17EEE61AA263" LSID-ZBK="urn:lsid:zoobank.org:act:57F8F227-2C96-4E3F-B4D1-3CDE8E5DE40F" class="Actinopterygii" family="Ariidae" genus="Cephalocassis" kingdom="Animalia" lsidName-ZBK="Cephalocassis Bleeker 1857:473" order="Siluriformes" pageNumber="63" phylum="Chordata" rank="genus">Cephalocassis</taxonomicName>
and 
<taxonomicName id="19DF4BCB45E81337E541E04C6409BD43" LSID-ZBK="urn:lsid:zoobank.org:act:4453B6E6-6001-42AB-8D66-002DF12E8C99" class="Actinopterygii" family="Ariidae" genus="Cryptarius" kingdom="Animalia" lsidName-ZBK="Cryptarius Kailola 2004:134" order="Siluriformes" pageNumber="63" phylum="Chordata" rank="genus">Cryptarius</taxonomicName>
