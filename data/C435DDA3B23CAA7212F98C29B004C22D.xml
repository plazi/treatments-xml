<taxonomicName id="B82D9829AD49B8F0B7D87A83A0DB51CD" LSID-ZBK="urn:lsid:zoobank.org:act:98999FD3-F940-4038-8A57-33EA663CFE04" class="Actinopterygii" family="Pimelodidae" genus="Pimelodus" kingdom="Animalia" lsidName-ZBK="Pimelodus barbus Lacépède 1803:94, 102" order="Siluriformes" pageNumber="65" phylum="Chordata" rank="species" species="barbus">Pimelodus barbus</taxonomicName>
