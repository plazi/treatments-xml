<taxonomicName id="4C1EB8FF13795304FBFBF879FB31F81F" authorityName="Moret" authorityYear="1998" box="[1034,1207,1922,1948]" class="Insecta" family="Carabidae" genus="Dyscolus" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="verecundus">
