<taxonomicName id="BD4C510D7D4B177FC43EDF0AF19B4952" authority="Shull" class="Insecta" family="Thripidae" genus="Scirtothrips" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scirtothrips" order="Thysanoptera" pageId="10" pageNumber="37" phylum="Arthropoda" rank="genus">Scirtothrips Shull</taxonomicName>
