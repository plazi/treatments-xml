<subSubSection id="C302D3A8FFFBFFA2FF00FD9EFBFCFDE9" box="[199,1074,583,608]" pageId="3" pageNumber="66" type="etymology">
<paragraph id="8BA78023FFFBFFA2FF00FD9EFBFCFDE9" blockId="3.[151,1436,151,860]" box="[199,1074,583,608]" pageId="3" pageNumber="66">
<emphasis id="B96C5C31FFFBFFA2FF00FD9EFE80FDD6" bold="true" box="[199,334,584,608]" pageId="3" pageNumber="66">Etymology.</emphasis>
The name 
<taxonomicName id="4C18FBA0FFFBFFA2FE0CFD9FFDA7FDD6" box="[459,617,585,608]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="spiniparameri">
<emphasis id="B96C5C31FFFBFFA2FE0CFD9FFDA7FDD6" box="[459,617,585,608]" italics="true" pageId="3" pageNumber="66">spiniparameri</emphasis>
</taxonomicName>
refers to the parameres being spine-like.
</paragraph>
</subSubSection>
<subSubSection id="C302D3A8FFFBFFA2FF00FDBAFE8CFCEA" pageId="3" pageNumber="66" type="discussion">
<paragraph id="8BA78023FFFBFFA2FF00FDBAFE8CFCEA" blockId="3.[151,1436,151,860]" pageId="3" pageNumber="66">
<emphasis id="B96C5C31FFFBFFA2FF00FDBAFEF5FD32" bold="true" box="[199,315,620,644]" pageId="3" pageNumber="66">Remarks.</emphasis>
<taxonomicName id="4C18FBA0FFFBFFA2FE84FDBAFDBEFD32" box="[323,624,620,644]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="spiniparameri" status="sp. nov.">
<emphasis id="B96C5C31FFFBFFA2FE84FDBAFDBEFD32" box="[323,624,620,644]" italics="true" pageId="3" pageNumber="66">Psephenops spiniparameri</emphasis>
</taxonomicName>
<emphasis id="B96C5C31FFFBFFA2FDBFFDBAFD1DFD32" bold="true" box="[632,723,620,644]" pageId="3" pageNumber="66">
<taxonomicNameLabel id="A25FE14AFFFBFFA2FDBFFDBAFD1DFD32" box="[632,723,620,644]" pageId="3" pageNumber="66" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
can be separated easily from other known 
<taxonomicName id="4C18FBA0FFFBFFA2FB7AFDBAFA8DFD32" box="[1213,1347,620,644]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="genus">
<emphasis id="B96C5C31FFFBFFA2FB7AFDBAFA8DFD32" box="[1213,1347,620,644]" italics="true" pageId="3" pageNumber="66">Psephenops</emphasis>
</taxonomicName>
species by the following features [other species in brackets]: antennae moniliform [filiform, 
<taxonomicName id="4C18FBA0FFFBFFA2FBB5FD46FB7DFD1E" box="[1138,1203,656,680]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="lupita">
<emphasis id="B96C5C31FFFBFFA2FBB5FD46FB7DFD1E" box="[1138,1203,656,680]" italics="true" pageId="3" pageNumber="66">lupita</emphasis>
</taxonomicName>
]; pronotum without tubercles [with 
<number id="3DD09204FFFBFFA2FE8BFD62FE94FD7A" box="[332,346,692,716]" pageId="3" pageNumber="66" value="3.0">3</number>
small tubercles, 
<taxonomicName id="4C18FBA0FFFBFFA2FDE4FD62FD5AFD7A" box="[547,660,692,716]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="grouvellei">
<emphasis id="B96C5C31FFFBFFA2FDE4FD62FD5AFD7A" box="[547,660,692,716]" italics="true" pageId="3" pageNumber="66">grouvellei</emphasis>
</taxonomicName>
]; protibiae with denticle [without denticle, 
<taxonomicName id="4C18FBA0FFFBFFA2FB57FD63FB33FD7A" box="[1168,1277,693,716]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="prestonae">
<emphasis id="B96C5C31FFFBFFA2FB57FD63FB33FD7A" box="[1168,1277,693,716]" italics="true" pageId="3" pageNumber="66">prestonae</emphasis>
</taxonomicName>
]; tarsal lobes enlarged [tarsal lobes short, 
<taxonomicName id="4C18FBA0FFFBFFA2FE12FD0EFD8BFD46" box="[469,581,728,752]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="grouvellei">
<emphasis id="B96C5C31FFFBFFA2FE12FD0EFD8BFD46" box="[469,581,728,752]" italics="true" pageId="3" pageNumber="66">grouvellei</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C18FBA0FFFBFFA2FD93FD0EFD5BFD46" box="[596,661,728,752]" class="Insecta" family="Psephenidae" genus="Psephenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="lupita">
<emphasis id="B96C5C31FFFBFFA2FD93FD0EFD5BFD46" box="[596,661,728,752]" italics="true" pageId="3" pageNumber="66">lupita</emphasis>
</taxonomicName>
, 
