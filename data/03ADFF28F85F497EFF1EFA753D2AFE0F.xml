<taxonomicName id="4C0435BDF85E497CFE96FA8D3DB5FABF" box="[287,447,1354,1376]" class="Insecta" family="Tenebrionidae" genus="Odocnemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="malgorzatae">
