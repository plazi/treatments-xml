<taxonomicName id="4C6A4D67FFA1FF94FBE24DDE535AFCB9" box="[1070,1314,764,789]" class="Magnoliopsida" family="Boraginaceae" genus="Heliotropium" kingdom="Plantae" order="Boraginales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="aucheri">
