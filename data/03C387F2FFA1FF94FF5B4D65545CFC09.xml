<taxonomicName id="4C6A4D67FFA1FF94FC604DDE5259FCBF" box="[940,1057,764,789]" class="Magnoliopsida" family="Fabaceae" genus="Alhagi" kingdom="Plantae" order="Fabales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="undetermined">
