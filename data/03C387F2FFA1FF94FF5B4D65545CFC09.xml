<taxonomicName id="4C6A4D67FFA1FF94FAFE4DDF568EFC94" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Tetradiclidaceae" genus="Peganum" kingdom="Plantae" order="Sapindales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="harmala">
<emphasis id="B91EEAF6FFA1FF94FAFE4DDF568EFC94" italics="true" pageId="45" pageNumber="46">Peganum harmala</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6A4D67FFA1FF94FEC84C3D57E1FC9B" box="[260,409,799,824]" class="Magnoliopsida" family="Crassulaceae" genus="Rosularia" kingdom="Plantae" order="Saxifragales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="undetermined">
