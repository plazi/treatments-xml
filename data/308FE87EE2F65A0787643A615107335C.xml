<geoCoordinate id="AA7EF345BDA4365A43CAAA519EB4CDFC" degrees="32" direction="north" minutes="06" orientation="latitude" precision="1" seconds="43.32" value="32.11203">32°06'43.32&quot;N</geoCoordinate>
