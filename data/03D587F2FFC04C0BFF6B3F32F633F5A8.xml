<emphasis id="B908EAF6FFC04C0BF9AC3E8DF8E4F3C8" box="[1545,1820,3066,3105]" italics="true" pageId="15" pageNumber="364">approximately</emphasis>
<geoCoordinate id="EE485023FFC04C0BFBA23947FB6EF3BE" box="[1031,1174,3120,3159]" degrees="10" direction="south" minutes="39" orientation="latitude" pageId="15" pageNumber="364" precision="925" value="-10.65">10°39’S</geoCoordinate>
, 
<geoCoordinate id="EE485023FFC04C0BFB103947FAA4F3BE" box="[1205,1372,3120,3159]" degrees="65" direction="west" minutes="23" orientation="longitude" pageId="15" pageNumber="364" precision="925" value="-65.38333">65°23’W</geoCoordinate>
