<paragraph id="8BC336E4FFC04C0BFBA63E5EFB58F31F" blockId="15.[1026,1821,2857,3424]" pageId="15" pageNumber="364">
<emphasis id="B908EAF6FFC04C0BFBA63E5EFB36F4B9" box="[1027,1230,2857,2896]" pageId="15" pageNumber="364">Taxonomy</emphasis>
. 
<emphasis id="B908EAF6FFC04C0BFB513E5EF8E4F4B9" box="[1268,1820,2857,2896]" italics="true" pageId="15" pageNumber="364">
<taxonomicName id="4C7C4D67FFC04C0BFB513E5EF95FF4B9" authorityName="Wagner" authorityYear="1843" box="[1268,1703,2857,2896]" class="Mammalia" family="Emballonuridae" genus="Emballonura" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="364" phylum="Chordata" rank="species" species="brevirostris">Emballonura brevirostris</taxonomicName>
Wagner
</emphasis>
, 1843, “ 
<materialsCitation id="3B143CB9FFC04C0BFB5C3E2AFB58F31F" ID-GBIF-Occurrence="2595781768" country="Brazil" latitude="-10.65" location="Marabitanas" longLatPrecision="1297" longitude="-65.38333" municipality="Bolivia" pageId="15" pageNumber="364" specimenCount="1" typeStatus="type">
<location id="8EA3603FFFC04C0BFB5C3E2AFA1FF46D" LSID="urn:lsid:plazi:treatment:03D587F2FFC04C0BFF6B3F32F633F5A8:8EA3603FFFC04C0BFB5C3E2AFA1FF46D" box="[1273,1511,2909,2948]" country="Brazil" latitude="-10.65" longLatPrecision="1297" longitude="-65.38333" municipality="Bolivia" name="Marabitanas" pageId="15" pageNumber="364">Marabitanas</location>
