<paragraph id="8F97716C5D21362C1DC8CC393A067E22" pageId="60" pageNumber="135">
<taxonomicName id="45226D1ED702723E5779B845526E3217" authority="Chapin, 1945" authorityName="Chapin" authorityYear="1945" class="Insecta" family="Cleridae" genus="Onychotillus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Onychotillus" order="Coleoptera" pageId="60" pageNumber="135" phylum="Arthropoda" rank="genus">Onychotillus Chapin, 1945</taxonomicName>
