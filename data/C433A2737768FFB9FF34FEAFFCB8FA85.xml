<taxonomicName id="8B9A68E67768FFB9FF34FE76FD28FE3D" authority="Moulton &amp; Adler, 2004" authorityName="Moulton &amp; Adler" authorityYear="2004" box="[151,692,438,463]" class="Insecta" family="Simuliidae" genus="Paracnephia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="342" phylum="Arthropoda" rank="species" species="gladiator">
