<taxonomicName id="CBC74D321D4D441DA22477A1FED618D7" authority="Serene &amp; Umali, 1965" authorityName="Serene &amp; Umali" authorityYear="1965" class="Malacostraca" family="Albuneidae" genus="Paralbunea" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="dayriti">
<emphasis id="3EB3EAA31D4D441DA22477A1FB1618B7" box="[1026,1233,666,689]" italics="true" pageId="3" pageNumber="52">Paralbunea dayriti</emphasis>
