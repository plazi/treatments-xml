<taxonomicName id="5DB2FCBCFFD0FF95FD22F4ADA53EFBC3" baseAuthorityName="Shaw" baseAuthorityYear="1802" box="[670,783,1014,1034]" class="Reptilia" family="Colubridae" genus="Lycodon" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="1259" phylum="Chordata" rank="species" species="jara">
