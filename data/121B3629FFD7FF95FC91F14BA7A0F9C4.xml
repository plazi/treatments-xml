<taxonomicName id="5DB2FCBCFFD0FF95FE1BF34FA478FBE1" baseAuthorityName="Cantor" baseAuthorityYear="1839" box="[423,585,1044,1064]" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="1259" phylum="Chordata" rank="species" species="cyclurus">Oligodon cyclurus</taxonomicName>
