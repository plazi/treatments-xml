<quantity id="4CCE9B46FFDE4732FD42FED2FCEAFE89" box="[717,800,266,290]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="125" unit="mm" value="10.0">10 mm</quantity>
