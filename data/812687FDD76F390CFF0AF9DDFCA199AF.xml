<taxonomicName id="CE8F4D68D76D390CFBD9FF4DFB6798D7" box="[1092,1188,188,212]" class="Insecta" family="Pseudococcidae" genus="Antonina" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="crawi">
