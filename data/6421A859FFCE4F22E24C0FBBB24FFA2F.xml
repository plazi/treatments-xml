<taxonomicName id="2B8862CCFFCE4F22E2880C29B4D5FDAD" authority="Matheron, 1843" authorityName="Matheron" authorityYear="1843" class="Gastropoda" family="Lymnaeidae" genus="Lymnaea" kingdom="Animalia" pageId="33" pageNumber="50" phylum="Mollusca" rank="species" species="aquensis">
