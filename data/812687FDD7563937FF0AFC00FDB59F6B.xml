<taxonomicName id="CE8F4D68D7563937FD7FF9A4FC8C9E6F" box="[738,847,1620,1644]" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="betulae">
