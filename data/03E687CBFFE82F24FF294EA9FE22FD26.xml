<taxonomicName id="4C4F4D5EFFE82F23FDF24975FD6CF8FD" authorityName="Bayarsaikhan &amp; Bae" authorityYear="2019" box="[588,727,1860,1884]" class="Insecta" family="Arctiidae" genus="Diduga" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="360" phylum="Arthropoda" rank="species" species="hanoiensis">
