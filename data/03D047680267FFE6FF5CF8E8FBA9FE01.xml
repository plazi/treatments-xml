<taxonomicName id="4C798DFD0264FFE6FD66FF68FC82FF7E" authority="Takaoka &amp; Sofian-Azirun &amp; Ya’Cob &amp; Chen &amp; Lau &amp; Pham, 2014" authorityName="Takaoka &amp; Sofian-Azirun &amp; Ya’Cob &amp; Chen &amp; Lau &amp; Pham" authorityYear="2014" box="[685,869,223,248]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="351" phylum="Arthropoda" rank="species" species="hongthaii" status="sp. nov." subGenus="Gomphostilbia">
<emphasis id="B90D2A6C0264FFE6FD66FF68FD5CFF71" box="[685,699,224,247]" italics="true" pageId="18" pageNumber="365">S</emphasis>
