<taxonomicName id="4C234D2FFFCAFF960328F97B397029CB" box="[344,541,1700,1726]" class="Actinopterygii" family="Loricariidae" genus="Gymnotocinclus" kingdom="Animalia" order="Siluriformes" pageId="2" pageNumber="339" phylum="Chordata" rank="genus">
