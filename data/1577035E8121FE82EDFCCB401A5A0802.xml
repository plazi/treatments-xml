<date id="E96094888121FE82ECE6CBFF1B6C0B92" box="[397,561,1494,1518]" pageId="293" pageNumber="294" value="2013-06-02" valueMax="2013-06-03" valueMin="2013-06-02">2–3 June 2013</date>
