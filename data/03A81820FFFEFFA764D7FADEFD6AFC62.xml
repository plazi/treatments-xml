<emphasis id="B9757524FFFEFFA4661EF9C3FD03F903" box="[557,646,1649,1672]" italics="true" pageId="1" pageNumber="6">Terpnosia</emphasis>
</taxonomicName>
to 
<taxonomicName id="4C01D2B5FFFEFFA464BFF93EFE95F928" authority="Lee, 2013" authorityName="Lee" authorityYear="2013" box="[140,272,1676,1699]" class="Insecta" family="Cicadidae" genus="Miniterpnosia" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="6" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="B9757524FFFEFFA464BFF93EFE95F928" box="[140,272,1676,1699]" italics="true" pageId="1" pageNumber="6">Miniterpnosia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A246C85FFFFEFFA46524F93EFEDFF92F" box="[279,346,1676,1700]" pageId="1" pageNumber="6" rank="genus">n. gen.</taxonomicNameLabel>
to become 
<taxonomicName id="4C01D2B5FFFEFFA465F8F93EFCF3FF64" authority="(Chou &amp; Lei, 1997)" authorityName="Lee" authorityYear="2013" baseAuthorityName="Chou &amp; Lei" baseAuthorityYear="1997" box="[459,886,215,1699]" class="Insecta" family="Cicadidae" genus="Miniterpnosia" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="6" phylum="Arthropoda" rank="species" species="mega" status="n. comb.">
