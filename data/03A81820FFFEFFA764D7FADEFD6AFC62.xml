(
<bibRefCitation id="EF90D4C7FFFEFFA4669FFF65FCEAFF64" author="CHOU I. &amp; LEI Z. &amp; LI L. &amp; LU X. &amp; YAO W." box="[684,879,215,239]" pageId="1" pageNumber="6" pagination="100" refId="ref3198" refString="CHOU I., LEI Z., LI L., LU X. &amp; YAO W. 1997. - The Cicadidae of China (Homoptera: Cicadoidea). Ilustrataj Insect-faunoj 2. Tianze Eldoneio, Hong Kong: 210, 367, pl. 9 (100). (In Chinese with English summary.)" type="journal article" year="1997">Chou &amp; Lei, 1997</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel id="A246C85FFFFEFFA4674CFF65FC5EFF64" box="[895,987,215,239]" pageId="1" pageNumber="6" rank="species">n. comb.</taxonomicNameLabel>
<taxonomicName id="4C01D2B5FFFEFFA467D1FF6AFBEFFF64" authority="Lee, 2013" authorityName="Lee" authorityYear="2013" box="[994,1130,216,239]" class="Insecta" family="Cicadidae" genus="Miniterpnosia" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="6" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="B9757524FFFEFFA467D1FF6AFBEFFF64" box="[994,1130,216,239]" italics="true" pageId="1" pageNumber="6">Miniterpnosia</emphasis>
</taxonomicName>
