<taxonomicName id="4C654D40B25EDF03FE8CFE49FD2A8E14" authority="(Fabricius, 1775)" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[301,702,506,533]" class="Insecta" family="Tortricidae" genus="Acleris" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="313" phylum="Arthropoda" rank="species" species="emargana">
