<taxonomicName id="4C73DBDDFFD61815FF17FB4BFE0EAB6D" authority="Krauss, 1892" authorityName="Krauss" authorityYear="1892" box="[198,489,1257,1283]" class="Insecta" family="Tettigoniidae" genus="Ariagona" kingdom="Animalia" order="Orthoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
