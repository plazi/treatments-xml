<quantity id="CA38A19FFF87127B31DC1EA2FE43FA6A" box="[226,384,1440,1462]" metricMagnitude="0" metricUnit="m" metricValue="2.18" metricValueMax="2.7199999999999998" metricValueMin="1.64" pageId="23" pageNumber="509" unit="mm" value="2180.0" valueMax="2720.0" valueMin="1640.0">
