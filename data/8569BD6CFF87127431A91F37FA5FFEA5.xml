<geoCoordinate id="68F46ABDFF87127B30591E45FE68FA82" box="[359,427,1351,1373]" degrees="2,500" direction="west" orientation="longitude" pageId="23" pageNumber="509" precision="55" value="-2.5">–2,500</geoCoordinate>
µm
</quantity>
long..................................................................... 
<taxonomicName id="CAC077F9FF87127B34761E4AFA5FFA83" baseAuthorityName="Spiridonov" baseAuthorityYear="1989" box="[1352,1436,1351,1373]" class="Secernentea" family="Thelastomatidae" genus="Heth" kingdom="Animalia" order="Oxyurida" pageId="23" pageNumber="509" phylum="Nematoda" rank="species" species="imias">
<emphasis id="3FB4D068FF87127B34761E4AFA5FFA83" bold="true" box="[1352,1436,1351,1373]" italics="true" pageId="23" pageNumber="509">H. imias</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
