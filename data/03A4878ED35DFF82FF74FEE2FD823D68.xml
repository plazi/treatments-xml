<collectionCode id="ED1CAE5DD35CFF82FF74F9A3FF6B3D77" box="[151,234,1547,1568]" pageId="11" pageNumber="60">PLATE</collectionCode>
<number id="3DC524BFD35CFF82FF13F9A3FE813D77" box="[240,256,1547,1568]" pageId="11" pageNumber="60" value="7.0">7</number>
