<subSubSection id="C34D655BFFB3C44C7BC5F863FCFE65B7" box="[151,869,2004,2026]" pageId="1" pageNumber="451" type="materials_examined">
<paragraph id="8BE836D0FFB3C44C7BC5F863FCFE65B7" blockId="1.[151,869,1973,2026]" box="[151,869,2004,2026]" pageId="1" pageNumber="451">
