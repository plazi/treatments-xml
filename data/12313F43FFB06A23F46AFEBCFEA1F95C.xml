<taxonomicName id="5D98F5D6FFB26A23F6EFFA85FDA5FA9C" box="[445,584,1289,1311]" class="Malacostraca" family="Processidae" kingdom="Animalia" order="Decapoda" pageId="27" pageNumber="27" phylum="Arthropoda" rank="family">Processidae</taxonomicName>
). Yellow Sea, Beibu Gulf, and 
<collectingRegion id="585C40B7FFB26A23F697FAABFF52FAD8" country="China" name="Guangdong" pageId="27" pageNumber="27">Guangdong Province</collectingRegion>
, 
<collectingCountry id="E28FCEC5FFB26A23F799FAC9FEF9FAD8" box="[203,276,1349,1371]" name="China" pageId="27" pageNumber="27">China</collectingCountry>
, 
<quantity id="5D6023B0FFB26A23F672FAC9FE6BFAD9" box="[288,390,1349,1370]" metricMagnitude="1" metricUnit="m" metricValue="7.05" metricValueMax="9.4" metricValueMin="4.7" pageId="27" pageNumber="27" unit="m" value="70.5" valueMax="94.0" valueMin="47.0">
47– 
<specimenCount id="8C9E45DCFFB26A23F61BFAC9FE6BFAD9" box="[329,390,1349,1370]" pageId="27" pageNumber="27" type="generic">94 m</specimenCount>
