<taxonomicName id="4C77C3B8FFE4D71CFEAEFA452FC8D9A3" authority="Kraepelin, 1903" authorityName="Kraepelin" authorityYear="1903" box="[298,653,1444,1468]" class="Chilopoda" family="Cryptopidae" genus="Mimops" kingdom="Animalia" order="Scolopendromorpha" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="orientalis">
