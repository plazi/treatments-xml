<taxonomicName id="4C1E4D0CFF876908FCEAFABCFB940563" authority="Spangler, 1979" authorityName="Spangler" authorityYear="1979" box="[806,1051,1373,1395]" class="Insecta" family="Hydrophilidae" genus="Anticura" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="690" phylum="Arthropoda" rank="genus">
