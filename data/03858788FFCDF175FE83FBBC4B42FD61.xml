(as 
<taxonomicName id="4C2C4D1DFFCCF175FD34FE334A94FE01" authorityName="Gabb" authorityYear="1866" box="[673,721,473,498]" class="Bivalvia" family="Lucinidae" genus="Here" kingdom="Animalia" order="Lucinoida" pageId="9" pageNumber="130" phylum="Mollusca" rank="genus">
