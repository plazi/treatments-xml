<taxonomicName id="4C2C4D1DFFCCF175FE6AFDD24AA3FDA1" baseAuthorityName="Sacco" baseAuthorityYear="1901" box="[511,742,568,594]" class="Bivalvia" family="Lucinidae" genus="Barbierella" kingdom="Animalia" order="Lucinoida" pageId="9" pageNumber="130" phylum="Mollusca" rank="species" species="miobarbieri">
<emphasis id="B958EA8CFFCCF175FE6AFDD24AA3FDA1" box="[511,742,568,594]" italics="true" pageId="9" pageNumber="130">Barbierella miobarbieri</emphasis>
</taxonomicName>
(as 
<taxonomicName id="4C2C4D1DFFCCF175FF11FDB248A3FD81" authorityName="Agassiz" authorityYear="1846" box="[132,230,600,626]" class="Bivalvia" family="Lucinidae" genus="Phacoides" kingdom="Animalia" order="Lucinoida" pageId="9" pageNumber="130" phylum="Mollusca" rank="genus">
<emphasis id="B958EA8CFFCCF175FF11FDB248A3FD81" box="[132,230,600,626]" italics="true" pageId="9" pageNumber="130">Phacoides</emphasis>
