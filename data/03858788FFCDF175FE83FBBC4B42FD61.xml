(
<taxonomicName id="4C2C4D1DFFCCF175FF61FDB24931FD81" authorityName="Dall" authorityYear="1901" box="[244,372,600,626]" class="Bivalvia" family="Lucinidae" genus="Pleurolucina" kingdom="Animalia" order="Lucinoida" pageId="9" pageNumber="130" phylum="Mollusca" rank="genus">
<emphasis id="B958EA8CFFCCF175FF61FDB24931FD81" box="[244,372,600,626]" italics="true" pageId="9" pageNumber="130">Pleurolucina</emphasis>
