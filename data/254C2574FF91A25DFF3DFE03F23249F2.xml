<emphasis id="9F914870FF91A25DFF3DFE03F5174FDF" bold="true" box="[151,790,475,502]" pageId="14" pageNumber="173">
<taxonomicName id="6AE5EFE1FF91A25DFF3DFE03F4B04FDF" authority="Bian, Shi &amp; Chang" authorityName="Bian, Shi &amp; Chang" box="[151,689,475,502]" class="Insecta" family="Tettigoniidae" genus="Phlugiolopsis" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="173" phylum="Arthropoda" rank="species" species="uncicercis" status="sp. nov.">
