<quantity id="4C8D9B64D953D40B7E36FF21FDBF549E" box="[372,537,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="2.8499999999999996" metricValueMax="2.86" metricValueMin="2.84" pageId="9" pageNumber="109" unit="mm" value="2.8499999999999996" valueMax="2.86" valueMin="2.84">2.84–2.86 mm</quantity>
, HL 
<quantity id="4C8D9B64D953D40B7D13FF21FD50549E" box="[593,758,188,213]" metricMagnitude="-4" metricUnit="m" metricValue="5.8" metricValueMax="6.0" metricValueMin="5.6" pageId="9" pageNumber="109" unit="mm" value="0.58" valueMax="0.6" valueMin="0.56">0.56–0.60 mm</quantity>
, HW 
<quantity id="4C8D9B64D953D40B7C7AFF21FC7B549E" box="[824,989,188,213]" metricMagnitude="-4" metricUnit="m" metricValue="5.8999999999999995" metricValueMax="6.1" metricValueMin="5.699999999999999" pageId="9" pageNumber="109" unit="mm" value="0.59" valueMax="0.61" valueMin="0.57">0.57–0.61 mm</quantity>
, PL 
<quantity id="4C8D9B64D953D40B7B53FF21FB10549E" box="[1041,1206,188,213]" metricMagnitude="-4" metricUnit="m" metricValue="6.35" metricValueMax="6.4" metricValueMin="6.3" pageId="9" pageNumber="109" unit="mm" value="0.635" valueMax="0.64" valueMin="0.63">0.63–0.64 mm</quantity>
, PW 
<quantity id="4C8D9B64D953D40B7BB1FF21FA3E549E" box="[1267,1432,188,213]" metricMagnitude="-4" metricUnit="m" metricValue="5.8" metricValueMax="5.8999999999999995" metricValueMin="5.699999999999999" pageId="9" pageNumber="109" unit="mm" value="0.58" valueMax="0.59" valueMin="0.57">0.57–0.59 mm</quantity>
, EL 
<quantity id="4C8D9B64D953D40B7F82FF7DFEC354BD" box="[192,357,224,248]" metricMagnitude="-4" metricUnit="m" metricValue="7.25" metricValueMax="7.4" metricValueMin="7.1" pageId="9" pageNumber="109" unit="mm" value="0.725" valueMax="0.74" valueMin="0.71">0.71–0.74 mm</quantity>
, EW 
<quantity id="4C8D9B64D953D40B7EE1FF7DFDEE54BD" box="[419,584,224,248]" metricMagnitude="-3" metricUnit="m" metricValue="1.095" metricValueMax="1.12" metricValueMin="1.07" pageId="9" pageNumber="109" unit="mm" value="1.095" valueMax="1.12" valueMin="1.07">1.07–1.12 mm</quantity>
, AL 
<quantity id="4C8D9B64D953D40B7DC2FF7DFC8354BD" box="[640,805,224,248]" metricMagnitude="-4" metricUnit="m" metricValue="9.1" metricValueMax="9.3" metricValueMin="8.9" pageId="9" pageNumber="109" unit="mm" value="0.91" valueMax="0.93" valueMin="0.89">0.89–0.93 mm</quantity>
, AW 
<quantity id="4C8D9B64D953D40B7C26FF42FBAC54BD" box="[868,1034,223,248]" metricMagnitude="-3" metricUnit="m" metricValue="1.255" metricValueMax="1.26" metricValueMin="1.25" pageId="9" pageNumber="109" unit="mm" value="1.255" valueMax="1.26" valueMin="1.25">1.25–1.26 mm</quantity>
.
</paragraph>
