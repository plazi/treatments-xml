<taxonomicName id="4C754D02D953D40B7D77F8F2FC9E53CF" authorityName="Huang &amp; Li &amp; Yin" authorityYear="2018" box="[565,824,1903,1925]" class="Insecta" family="Staphylinidae" genus="Pselaphodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="109" phylum="Arthropoda" rank="species" species="thailandicus">
