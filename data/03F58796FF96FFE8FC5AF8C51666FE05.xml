<emphasis id="B928EA92FF96FFE6FC5AF8C51244F8F2" bold="true" box="[956,1311,1844,1869]" pageId="6" pageNumber="178">
Genus 
<taxonomicName id="4C5C4D03FF96FFE6FBEDF8C41244F8F2" authority="Leach, 1815" authorityName="Leach" authorityYear="1815" box="[1035,1311,1844,1869]" class="Insecta" family="Staphylinidae" genus="Scaphisoma" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="178" phylum="Arthropoda" rank="genus">
<emphasis id="B928EA92FF96FFE6FBEDF8C413CBF8F2" bold="true" box="[1035,1168,1844,1869]" italics="true" pageId="6" pageNumber="178">Scaphisoma</emphasis>
