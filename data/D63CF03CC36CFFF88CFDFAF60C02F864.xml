<collectionCode id="3884D9EFC36CFFF88A90F8DC0ADEF8FA" box="[1184,1228,1889,1915]" country="Sweden" name="University of Gothenburg" pageId="26" pageNumber="27" type="Herbarium">GB</collectionCode>
