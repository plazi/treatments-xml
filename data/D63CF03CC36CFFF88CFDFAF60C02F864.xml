<quantity id="996DECCFC36CFFF88A25F8DC0A42F8FA" box="[1045,1104,1889,1915]" metricMagnitude="1" metricUnit="m" metricValue="5.011419999999999" pageId="26" pageNumber="27" unit="in" value="1973.0">1973</quantity>
