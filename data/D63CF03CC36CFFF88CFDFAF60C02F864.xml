<taxonomicName id="99953AA9C36CFFF88D2DF8DF0A42F8FA" authority="Nordsieck, 1973" authorityName="Nordsieck" authorityYear="1973" box="[797,1104,1889,1915]" class="Bivalvia" family="Protocuspidariidae" genus="Laevipilina" higherTaxonomySource="CoL" kingdom="Animalia" order="Tryblidiida" pageId="26" pageNumber="27" phylum="Mollusca" rank="species" species="ionica">
