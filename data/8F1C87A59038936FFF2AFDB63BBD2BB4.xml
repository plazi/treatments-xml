<number id="B17D2494903B9369FBFEFAE73C422CED" box="[1091,1124,1385,1408]" pageId="2" pageNumber="205" value="0.8">0.8</number>
(
<number id="B17D2494903B9369FBCEFAE73CB32CED" box="[1139,1173,1385,1408]" pageId="2" pageNumber="205" value="1.2">1.2</number>
: 
<number id="B17D2494903B9369FB17FAE73CE92CED" box="[1194,1231,1385,1408]" pageId="2" pageNumber="205" value="1.4">1.4</number>
); clypeus 
<number id="B17D2494903B9369FA87FAE73D7D2CED" box="[1338,1371,1385,1408]" pageId="2" pageNumber="205" value="1.4">1.4</number>
