: Kurian dissected 
<specimenCount id="9D42FD713B27447D337DFD78FD4EC4C9" box="[519,643,727,753]" pageId="27" pageNumber="28" type="female">one female</specimenCount>
(
<typeStatus id="54FF885A3B27447D33E8FD78FD3AC4CA" box="[658,759,727,754]" pageId="27" pageNumber="28">holotype</typeStatus>
) and 
<specimenCount id="9D42FD713B27447D324DFD78FC50C4C9" box="[823,925,727,753]" pageId="27" pageNumber="28" type="male">one male</specimenCount>
