<taxonomicName id="0DF3BA8FD96DA715FF5483DA4026FC12" box="[151,341,941,966]" class="Insecta" family="Phaneropteridae" genus="Adapantus" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="transmarinus">
