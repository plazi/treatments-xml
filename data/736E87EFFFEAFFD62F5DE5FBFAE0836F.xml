<figureCitation id="63FC2A7CFFE5FFD62D38E183FC7185A7" box="[877,954,644,670]" captionStart="FIGURES 1 ­ 8" captionStartId="2.[264,382,1813,1837]" captionTargetBox="[266,1307,465,1791]" captionTargetId="figure@2.[249,1324,449,1803]" captionTargetPageId="2" captionText="FIGURES 1 ­ 8. Helicopsyche woldai sp. n., male holotype. 1, right wings; 2, abdominal segment VI, lateral view; 3, abdominal VIth sternal process, ventral view; 4, genitalia, lateral view; 5, genitalia, dorsal view; 6, genitalia, ventral view; 7, phallus, lateral view; 8, phallus, ventral view." httpUri="https://zenodo.org/record/157030/files/figure.png" pageId="8" pageNumber="9">Figs 7</figureCitation>
, 
<figureCitation id="63FC2A7CFFE5FFD62D93E183FC2F85A7" box="[966,996,644,670]" captionStart="FIGURES 9 ­ 16" captionStartId="5.[264,382,1617,1641]" captionTargetBox="[272,1322,318,1589]" captionTargetId="figure@5.[268,1343,302,1602]" captionTargetPageId="5" captionText="FIGURES 9 ­ 16. Helicopsyche fridae sp. n., male holotype. 9, right wings; 10, abdominal segment VI, lateral view; 11, abdominal VIth sternal process, ventral view; 12, genitalia, lateral view; 13, genitalia, dorsal view; 14, genitalia, ventral view; 15, phallus, lateral view; 16, phallus, ventral view." httpUri="https://zenodo.org/record/157031/files/figure.png" pageId="8" pageNumber="9">15</figureCitation>
) ....................................... 
<number id="4D0F24DEFFE5FFD62B49E183FAE085A7" box="[1308,1323,644,670]" pageId="8" pageNumber="9" value="3.0">3</number>
</keyLead>
