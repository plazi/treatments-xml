<taxonomicName id="3B8ADE9DFFD31203A17272F14EC6F81D" authority="Park &amp; Prins, 2019" authorityName="Park &amp; Prins" authorityYear="2019" box="[284,485,1968,1990]" class="Insecta" family="Lecithoceridae" genus="Thubdora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="455" phylum="Arthropoda" rank="genus" species="ealaensis" status="sp. nov.">
