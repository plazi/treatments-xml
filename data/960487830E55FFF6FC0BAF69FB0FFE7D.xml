<paragraph id="1E1236950E55FFF7FCAEADA2FA97F92A" blockId="110.[807,1386,1713,1735]" box="[807,1386,1713,1735]" pageId="110" pageNumber="251">
<emphasis id="2CD9EA870E55FFF7FCAEADA2FC52F92A" box="[807,943,1714,1735]" italics="true" pageId="110" pageNumber="251">Description:</emphasis>
see under 
<taxonomicName id="D9AD4D160E55FFF7FBBCADA2FA98F92A" authority="(Walter, 1944)" baseAuthorityName="Walter" baseAuthorityYear="1944" box="[1077,1381,1713,1735]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="110" pageNumber="251" phylum="Arthropoda" rank="species" species="oblongus">
