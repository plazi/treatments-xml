<treatmentCitationGroup id="AB6411D01852FFBEFF189821CEA7FEA0" pageId="8" pageNumber="309">
<taxonomicName id="4C744D7D1852FFBEFF189821CD46FE8F" authority="Champion 1913: 168" authorityName="Champion" authorityPageNumber="168" authorityYear="1913" box="[151,603,258,281]" class="Insecta" family="Salpingidae" genus="Othnius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="immaculatus">
<emphasis id="B900EAEC1852FFBEFF189821CE67FE8E" bold="true" box="[151,378,258,281]" italics="true" pageId="8" pageNumber="309">Othnius immaculatus</emphasis>
<treatmentCitation id="0AD510EF1852FFBEFE0E9821CD46FE8F" author="" box="[385,603,258,280]" page="168" pageId="8" pageNumber="309">
<bibRefCitation id="EFE54B0F1852FFBEFE0E9821CD35FE8F" box="[385,552,258,280]" pageId="8" pageNumber="309" refId="ref33400" refString="Champion, G. C. (1913) Notes on various Central American Coleoptera, with descriptions of new genera and species. Transactions of the Entomological Society of London, 1913, 58 - 169, plates 3 - 4. https: // doi. org / 10.1111 / j. 1365 - 2311.1913. tb 02781. x" type="journal article">Champion 1913</bibRefCitation>
