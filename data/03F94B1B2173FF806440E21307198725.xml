<typeStatus id="54EB44AF2173FF806435E28104CC80A5" box="[351,407,917,940]" pageId="11" pageNumber="162">types</typeStatus>
from 
<collectingRegion id="499434EF2173FF8064B1E28107DE80A5" box="[475,645,917,940]" country="Indonesia" name="Sulawesi Selatan" pageId="11" pageNumber="162">South Sulawesi</collectingRegion>
