<typeStatus id="54E58807D464FFECFEC117D4FEBA6C8C" box="[293,375,1441,1462]" pageId="40" pageNumber="41">paratype</typeStatus>
, slide RMNH.INS.23848 
<emphasis id="B92AEAB7D464FFECFD9717D5FD406C8C" bold="true" box="[627,653,1440,1462]" pageId="40" pageNumber="41">60</emphasis>
<taxonomicName id="4C5E4D26D464FFECFD7017D4FCC56C8F" authorityName="Van Nieukerken, A. Lastuvka &amp; Z. Lastuvka" authorityYear="2010" box="[660,776,1440,1462]" class="Insecta" family="Nepticulidae" genus="Ectoedemia" kingdom="Animalia" order="Lepidoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="phaeolepis">
<emphasis id="B92AEAB7D464FFECFD7017D4FCC56C8F" box="[660,776,1440,1462]" italics="true" pageId="40" pageNumber="41">E. phaeolepis</emphasis>
