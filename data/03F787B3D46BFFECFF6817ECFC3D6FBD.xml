<heading id="D0A981C9D46BFFE3FF6817ECFD6F6C8F" box="[140,674,1433,1461]" fontSize="11" level="2" pageId="39" pageNumber="40" reason="3">
<taxonomicName id="4C5E4D26D46BFFE3FF6817ECFD6F6C8F" authority="(Bedell)" baseAuthorityName="Bedell" baseAuthorityYear="1848" box="[140,674,1433,1461]" class="Insecta" family="Nepticulidae" genus="Ectoedemia" kingdom="Animalia" order="Lepidoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="quinquella" subGenus="Ectoedemia">
