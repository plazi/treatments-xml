</figureCitation>
; 
<number id="3D9424F2FFCFB238FEC6FF01C733FF81" box="[279,352,186,211]" pageId="10" pageNumber="240" value="16.5" valueMax="17.0" valueMin="16.0">16–17</number>
; 
<number id="3D9424F2FFCFB238FEBDFF00C7E0FF81" box="[364,435,187,211]" pageId="10" pageNumber="240" value="32.5" valueMax="34.0" valueMin="31.0">31–34</number>
)
</paragraph>
