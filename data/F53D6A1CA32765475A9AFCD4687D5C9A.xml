<taxonomicName id="BA94A089A32765475998FB5668305DD5" box="[917,1086,1235,1262]" class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="22" pageNumber="517" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
