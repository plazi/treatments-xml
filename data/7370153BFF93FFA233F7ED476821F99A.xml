<taxonomicName id="3CD9DFAEFF93FFA231ADEEB96962FD18" authorityName="Brunner von Wattenwyl" authorityYear="1893" box="[717,863,726,751]" class="Insecta" family="Diapheromeridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phasmida" pageId="41" pageNumber="42" phylum="Arthropoda" rank="subFamily" subFamily="Necrosciinae">Necrosciinae</taxonomicName>
