<date id="FFBB1034FFC0FFB6FE4AF9D33B3EF9CD" box="[437,601,1619,1641]" pageId="123" pageNumber="123" value="1956-01">
<collectingDate id="EFFFE9DCFFC0FFB6FE4AF9D33B3EF9CD" box="[437,601,1619,1641]" pageId="123" pageNumber="123" value="1956-01">January 1956</collectingDate>
