<taxonomicName id="2FC1FBB3AC6AFFAAFF64FC61FD8BFC78" authority="(BEZZI, 1914)" baseAuthorityName="BEzzi" baseAuthorityYear="1914" box="[182,544,905,929]" class="Insecta" family="Sciaridae" genus="Bradysia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="288" phylum="Arthropoda" rank="species" species="forficulata">
