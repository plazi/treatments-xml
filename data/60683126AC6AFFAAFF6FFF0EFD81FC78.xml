<taxonomicName id="2FC1FBB3AC6AFFAAFE71FC81FD5EFC58" authority="TUOMIKOSKI" authorityName="TUOMIKOSKI" authorityYear="1960" box="[419,757,873,897]" class="Insecta" family="Sciaridae" genus="Bradysia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="288" phylum="Arthropoda" rank="species" species="nocturna">
