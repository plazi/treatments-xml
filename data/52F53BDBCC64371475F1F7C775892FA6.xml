<subSubSection id="BBAB904A69779DBDA97E07E43C684E6C" pageId="8" pageNumber="9" type="nomenclature">
<paragraph id="A12CDF6083FCB4BE5A9D9F5898346D7C" pageId="8" pageNumber="9">
<taxonomicName id="DFA1D668EB9047908F26B01091C20E04" LSID="urn:lsid:zoobank.org:act:98EE02FA-5A90-4675-A4E2-A2236F8787FC" authority="Kawahara &amp; Rubinoff" authorityName="Butler" authorityYear="1881" class="Insecta" family="Cosmopterigidae" genus="Hyposmocoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hyposmocoma makawao" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="makawao">
<pageBreakToken id="E696B3DE0AA1AB1F135EB8639D2EFA75" pageId="8" pageNumber="9" start="start">Hyposmocoma</pageBreakToken>
makawao Kawahara &amp; Rubinoff
</taxonomicName>
<taxonomicNameLabel id="EC62DB794816F0C6C5E6406E80F99D37" pageId="8" pageNumber="9">sp. n.</taxonomicNameLabel>
Figs 4815
