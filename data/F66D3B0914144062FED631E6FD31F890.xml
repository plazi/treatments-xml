<taxonomicName id="B9C4F19C14134063FE073046FD7BFB86" box="[473,738,1126,1149]" class="Insecta" family="Tenebrionidae" genus="Ascelosodis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="emarginata" status="sp. nov.">Ascelosodis emarginata</taxonomicName>
