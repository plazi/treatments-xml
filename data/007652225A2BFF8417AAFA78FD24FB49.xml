province
</collectingRegion>
; and 
<collectorName id="252A86E25A2AFF8415BDFDCCFD05FD8E" box="[640,754,595,620]" pageId="11" pageNumber="312">Genesis II</collectorName>
and 
<collectorName id="252A86E25A2AFF841415FDCCFC08FD8E" box="[808,1023,595,620]" pageId="11" pageNumber="312">Cerro de la Muerte</collectorName>
, 
<collectingRegion id="4A1B2DD65A2AFF841334FDCCFB39FD89" box="[1033,1230,595,620]" country="Costa Rica" name="Cartago" pageId="11" pageNumber="312">Cartago province</collectingRegion>
, both provinces of 
<collectingCountry id="F0C8A3A45A2AFF8417AAFDE8FEE2FD75" box="[151,277,631,656]" name="Costa Rica" pageId="11" pageNumber="312">Costa Rica</collectingCountry>
. At Chirripó National Park, 
<collectingRegion id="4A1B2DD65A2AFF84156EFDE8FCD6FD75" box="[595,801,631,656]" country="Costa Rica" name="San Jose" pageId="11" pageNumber="312">
<collectingRegion id="4A1B2DD65A2AFF84156EFDE8FD42FD6A" box="[595,693,631,655]" country="Uruguay" name="San Jose" pageId="11" pageNumber="312">San José</collectingRegion>
province
