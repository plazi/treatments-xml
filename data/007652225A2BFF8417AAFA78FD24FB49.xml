). 
<location id="8D00B5EF5A2AFF841546FD94FD50FDC1" LSID="urn:lsid:plazi:treatment:007652225A2BFF8417AAFA78FD24FB49:8D00B5EF5A2AFF841546FD94FD50FDC1" box="[635,679,523,548]" country="Costa Rica" county="Central Volcanic Cordillera" municipality="Talamanca Cordillera" name="The" pageId="11" pageNumber="312" stateProvince="Heredia province">The</location>
original description mentions 
<collectingMunicipality id="6804794E5A2AFF84132FFD94FAFEFDC1" box="[1042,1289,523,548]" pageId="11" pageNumber="312">Talamanca Cordillera</collectingMunicipality>
and 
<collectingCounty id="61019BB85A2AFF841277FD94FE83FDAD" pageId="11" pageNumber="312">Central Volcanic Cordillera</collectingCounty>
, 
<elevation id="03F204075A2AFF841642FDB0FE21FDA2" box="[383,470,559,583]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="11" pageNumber="312" unit="m" value="2000.0">
<quantity id="4F274ED15A2AFF841642FDB0FE21FDA2" box="[383,470,559,583]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="11" pageNumber="312" unit="m" value="2000.0">2000 m</quantity>
</elevation>
, more precisely 
<locationDeviation id="99D2FA165A2AFF8415B2FDB0FC40FDAD" box="[655,951,559,584]" pageId="11" pageNumber="312">
<quantity id="4F274ED15A2AFF8415B2FDB0FD3FFDAD" box="[655,712,559,584]" metricMagnitude="3" metricUnit="m" metricValue="6.0" pageId="11" pageNumber="312" unit="km" value="6.0">6 km</quantity>
ENE of Vara Blanca
</locationDeviation>
, 
<collectingRegion id="4A1B2DD65A2AFF8414FFFDB0FB70FDAD" box="[962,1159,559,584]" country="Costa Rica" name="Heredia" pageId="11" pageNumber="312">Heredia province</collectingRegion>
; Paraíso del Quetzal and 
<collectorName id="252A86E25A2AFF8417AAFDCCFE99FD8E" box="[151,366,595,620]" pageId="11" pageNumber="312">Cerro de la Muerte</collectorName>
, 
<collectingRegion id="4A1B2DD65A2AFF841645FDCCFDB1FD89" box="[376,582,595,620]" country="Costa Rica" name="San Jose" pageId="11" pageNumber="312">
