2. 
<taxonomicName id="E312CE325EF972428E36D613DD2CA74E" LSID="924FE8BA-E283-DD98-CBD1-E7B1FA7618DC" authority="G. A. Levin" authorityName="G. A. Levin" authorityYear="2013" class="Dicotyledoneae" family="Euphorbiaceae" genus="Drypetes" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Drypetes asymmetricarpa" order="Geraniales" pageId="6" pageNumber="81" phylum="Angiospermae" rank="species" species="asymmetricarpa" status="sp. nov.">Drypetes asymmetricarpa G. A. Levin</taxonomicName>
<taxonomicNameLabel id="232C949EF477A3D5A9B362F53B1EDC6E" pageId="6" pageNumber="81">sp. nov.</taxonomicNameLabel>
