<taxonomicName id="5EEC4D5CFFBFFFC3FCF8A123FBD1F81F" authority="Evans, 1964" authorityName="Evans" authorityYear="1964" box="[787,1087,1918,1943]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="plaumanni">
