<taxonomicName id="5EEC4D5CFFBFFFC3FB9DA123FA76F81F" authority="Ashmead" authorityName="Ashmead" box="[1142,1432,1918,1943]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="tuberculatus">
