<number id="FB76D270777D8B2EFF7D93F0FEB0E2BB" box="[184,261,1712,1738]" pageId="4" pageNumber="5" value="16.0" valueMax="17.0" valueMin="15.0">15–17</number>
; III, 
<number id="FB76D270777D8B2EFEF993F0FE3CE2BB" box="[316,393,1712,1738]" pageId="4" pageNumber="5" value="25.0" valueMax="28.0" valueMin="22.0">22–28</number>
