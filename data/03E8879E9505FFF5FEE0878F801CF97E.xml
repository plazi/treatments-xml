<subSubSection id="C35B65039505FFF5FED0816E801CF97E" pageId="8" pageNumber="9" type="description">
<paragraph id="8BFE36889505FFF5FED0816E862FFA36" blockId="8.[264,1323,918,1705]" pageId="8" pageNumber="9">
<taxonomicName id="4C414D0B9505FFF5FED0816E8628FA46" authority="Pronotal" authorityName="Pronotal" box="[312,598,1399,1425]" class="Insecta" family="Chrysomelidae" genus="Stegnocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
