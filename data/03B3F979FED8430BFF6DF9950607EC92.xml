<taxonomicName id="4C1A33ECFEDE430BFD1EF85E0694ECE8" box="[740,897,1925,1950]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="301" pageNumber="302" phylum="Arthropoda" rank="species" species="consobrina">
