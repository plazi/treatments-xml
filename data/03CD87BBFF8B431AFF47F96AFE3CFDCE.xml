<quantity id="4C9C9B48FF8B431EFAA4F86EFA04F83F" box="[1396,1436,1971,1996]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="2" pageNumber="425" unit="in" value="1.0">1 in</quantity>
<collectionCode id="ED75AE68FF8B431EFF47F805FF73F81C" box="[151,235,2008,2031]" country="Italy" httpUri="http://grbio.org/cool/ek3c-66y7" name="Museo di Zoologia dell'Universita &amp;quot;La Sapienza&amp;quot;" pageId="2" pageNumber="425">MZUR</collectionCode>
, 
<quantity id="4C9C9B48FF8B431EFF26F80AFEB9F81C" box="[246,289,2006,2031]" metricMagnitude="-1" metricUnit="m" metricValue="1.016" pageId="2" pageNumber="425" unit="in" value="4.0">4 in</quantity>
HMIM) [
<collectingMethod id="52254EBAFF8B431EFE5EF80BFD3AF81C" box="[398,674,2006,2031]" pageId="2" pageNumber="425">all collected by hand net</collectingMethod>
