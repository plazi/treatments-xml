<pageBreakToken id="E50CC555BB57D0C8A4996FF6AC4E7D6F" pageId="null" pageNumber="720" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="5CD5AA2DD81F95886EB45163BE39715C" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="720" phylum="Tracheophyta" rank="genus">Rumex</taxonomicName>
