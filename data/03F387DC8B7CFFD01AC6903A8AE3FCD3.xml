<subSubSection id="C34065418B7FFFD01A9694BB8C32FCA8" pageId="14" pageNumber="415" type="diagnosis">
<paragraph id="8BE536CA8B7FFFD01A9694BB8C04FC85" blockId="14.[151,1436,151,848]" box="[199,1035,749,774]" pageId="14" pageNumber="415">
