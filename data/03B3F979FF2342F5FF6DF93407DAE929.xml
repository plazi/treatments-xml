<taxonomicName id="4C1A33ECFF2042F5FC0AFEA80149EAFE" box="[1008,1116,368,392]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="211" pageNumber="212" phylum="Arthropoda" rank="species" species="gnoma">
<emphasis id="B96E947DFF2042F5FC0AFEA80149EAFE" box="[1008,1116,368,392]" italics="true" pageId="211" pageNumber="212">G. gnoma</emphasis>
</taxonomicName>
