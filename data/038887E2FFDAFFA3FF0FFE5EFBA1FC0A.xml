<quantity id="4CD99B11FFD8FFA3FDB9FF38FD6AFF0C" box="[570,664,166,193]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="34" pageNumber="73" unit="mm" value="4.5">4.5 mm</quantity>
