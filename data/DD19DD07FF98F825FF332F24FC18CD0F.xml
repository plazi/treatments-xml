<emphasis id="67C4B003FF98F825FF332F24FD4ECF42" bold="true" box="[151,739,1294,1320]" pageId="2" pageNumber="56">
<taxonomicName id="92B01792FF98F825FF332F24FD29CF42" authority="Beaucournu &amp; Perrichot" authorityName="Beaucournu &amp; Perrichot" box="[151,644,1294,1320]" class="Insecta" family="Pulicidae" genus="Eospilopsyllus" kingdom="Animalia" order="Siphonaptera" pageId="2" pageNumber="56" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="67C4B003FF98F825FF332F24FEE8CF42" bold="true" box="[151,325,1294,1320]" italics="true" pageId="2" pageNumber="56">Eospilopsyllus</emphasis>
Beaucournu &amp; Perrichot
</taxonomicName>
, 
<taxonomicNameLabel id="7CF70D78FF98F825FD362F24FD4ECF42" box="[658,739,1294,1320]" pageId="2" pageNumber="56" rank="genus">n. gen.</taxonomicNameLabel>
