<taxonomicName id="4C7E3589FFFEFFD8FBA6FE6B35F7FAEC" authorityName="in Erichson" authorityYear="1848" box="[1070,1163,1285,1308]" class="Insecta" family="Scarabaeidae" genus="Copris" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="52" phylum="Arthropoda" rank="species" species="coenosa">
