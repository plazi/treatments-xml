<taxonomicName id="4C7E3589FFFEFFD8FD8CFF8B32A8FB0D" authority="in Erichson (1848: 564)" authorityName="in Erichson" authorityPageNumber="564" authorityYear="1848" box="[516,980,1253,1277]" class="Insecta" family="Scarabaeidae" genus="Copris" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="52" phylum="Arthropoda" rank="species" species="coenosa">
