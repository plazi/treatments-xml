<taxonomicName id="66E44D30D3668708FD8EDE2B8C90FB78" authorityName="Kent" authorityYear="1881" box="[530,732,1151,1175]" class="Oligohymenophorea" family="Opisthonectidae" genus="Telotrochidium" kingdom="Protozoa" order="Peritrichida" pageId="16" pageNumber="949" phylum="Ciliophora" rank="genus">
<emphasis id="9390EAA1D3668708FD8EDE2B8C90FB78" bold="true" box="[530,732,1151,1175]" italics="true" pageId="16" pageNumber="949">Telotrochidium</emphasis>
</taxonomicName>
? sp.
</heading>
</paragraph>
