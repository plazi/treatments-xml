<paragraph id="A81536A2FFF2FF900FA15D9B42CBBBEE" blockId="50.[651,930,1454,1479]" box="[651,930,1454,1479]" pageId="50" pageNumber="2352">
<heading id="F35D81CEFFF2FF900FA15D9B42CBBBEE" box="[651,930,1454,1479]" centered="true" fontSize="10" level="2" pageId="50" pageNumber="2352" reason="3">
<taxonomicName id="6FAA4D21FFF2FF900FA15D9B4246BBEE" authority="Hendrycks &amp; Conlan, 2003" authorityName="Hendrycks &amp; Conlan" authorityYear="2003" box="[651,815,1454,1478]" class="Malacostraca" family="Eusiridae" genus="Triquetramana" kingdom="Animalia" order="Amphipoda" pageId="50" pageNumber="2352" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="9ADEEAB0FFF2FF900FA15D9B4246BBEE" bold="true" box="[651,815,1454,1478]" italics="true" pageId="50" pageNumber="2352">Triquetramana</emphasis>
</taxonomicName>
<taxonomicNameLabel id="81ED57CBFFF2FF900E125D9A42CBBBEE" box="[824,930,1455,1479]" pageId="50" pageNumber="2352" rank="genus">gen. nov.</taxonomicNameLabel>
