, Figs. 
<number id="784ED5360915FFDC35D3FDB56019563E" box="[1226,1324,533,556]" pageId="6" pageNumber="55" value="301.5" valueMax="303.0" valueMin="300.0">300–303</number>
)
</bibRefCitation>
. 
<taxonomicName id="0986BC920915FFDC318EFD95661C565E" authority="Sasa, Kawai et Ueno, 1988: 59" authorityName="Sasa, Kawai et Ueno" authorityPageNumber="59" authorityYear="1988" box="[151,809,565,588]" class="Insecta" family="Chironomidae" genus="Trissopelopia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="oyabetrispinosa">
