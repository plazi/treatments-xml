<number id="784ED5360915FFDC3217FD95661C565E" box="[782,809,565,588]" pageId="6" pageNumber="55" value="59.5" valueMax="60.0" valueMin="59.0">59</number>
</treatmentCitation>
</taxonomicName>
–60, Plate 
<number id="784ED5360915FFDC3283FD95668D565E" box="[922,952,565,588]" pageId="6" pageNumber="55" value="20.0">20</number>
