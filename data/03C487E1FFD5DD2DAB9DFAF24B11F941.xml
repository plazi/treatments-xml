<taxonomicName id="4C6D4D74FFD5DD2DAB9DFAA84A90FAE4" authority="Richard 1892 b, p. 222" authorityName="Richard" authorityPageNumber="222" authorityYear="1892" box="[151,686,1360,1383]" class="Branchiopoda" family="Moinidae" genus="Moinodaphnia" kingdom="Animalia" order="Diplostraca" pageId="12" pageNumber="49" phylum="Arthropoda" rank="species" species="mocquerysi">
