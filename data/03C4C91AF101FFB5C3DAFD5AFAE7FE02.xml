<subSubSection id="C3772B87F100FFB5C38AFE86FA64FE26" box="[199,1417,294,321]" pageId="7" pageNumber="566" type="etymology">
<paragraph id="8BD2780CF100FFB5C38AFE86FA64FE26" blockId="7.[151,1436,150,357]" box="[199,1417,294,321]" pageId="7" pageNumber="566">
<emphasis id="B919A41EF100FFB5C38AFE86FEA5FE26" bold="true" box="[199,328,294,320]" pageId="7" pageNumber="566">Etymology</emphasis>
