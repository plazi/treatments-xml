<taxonomicName id="09E288CFFF8A2456FE66FE6F170CFE68" box="[509,608,484,509]" class="Insecta" family="Nitidulidae" genus="Apetasimus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="conditus">
