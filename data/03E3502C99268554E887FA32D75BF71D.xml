41. 
<taxonomicName id="4C4A9AB999268554E8D1FA33D712F589" authority="Meigen, 1824" authorityName="Meigen" authorityYear="1824" box="[193,579,1484,1511]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="atratus">
