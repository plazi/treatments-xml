<paragraph id="708E31195ACB2D9E9D49483FEC573E20" pageId="6" pageNumber="23">
<taxonomicName id="3C68E5CC2DC5A13056DB5C578BA281A0" class="Insecta" family="Gelechiidae" genus="Aponoea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aponoea obtusipalpis" order="Lepidoptera" pageId="6" pageNumber="23" phylum="Arthropoda" rank="species" species="obtusipalpis">Aponoea obtusipalpis</taxonomicName>
