<emphasis id="B9269335F37AFFDEFF7D22F790B65DBE" bold="true" box="[151,674,730,757]" pageId="3" pageNumber="564">
Genus 
<taxonomicName id="4C5234A4F37AFFDEFF0722F690B65DBE" authority="Herrich-Schaffer, 1853" authorityName="Herrich-Schaffer" authorityYear="1853" box="[237,674,730,757]" class="Insecta" family="Lecithoceridae" genus="Lecithocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="564" phylum="Arthropoda" rank="genus">
