<heading id="D0EE81CC9E45746AFA612B6DD2E2DE5F" bold="true" box="[264,732,1793,1819]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<taxonomicName id="4C194D239E45746AFA612B6DD2E2DE5F" authority="Wygodzinsky, 1947" authorityName="Wygodzinsky" authorityYear="1947" box="[264,732,1793,1819]" class="Insecta" family="Reduviidae" genus="Heniartes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="cachabi">
