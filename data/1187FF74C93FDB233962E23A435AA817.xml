<taxonomicName id="64AB3B0559C1023F01B796021F12863B" class="Magnoliopsida" family="Theaceae" genus="Camellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Camellia japonica" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="japonica">Camellia japonica</taxonomicName>
L., 
<taxonomicName id="FE526B9DF6571402DCDC6A604ED50321" class="Magnoliopsida" family="Theaceae" genus="Camellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Camellia oleifera" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="oleifera">Camellia oleifera</taxonomicName>
Abel, 
<taxonomicName id="A4F6F780296208B8A071CA38F6D665AD" class="Magnoliopsida" family="Theaceae" genus="Camellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Camellia reticulata" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="reticulata">Camellia reticulata</taxonomicName>
Lindl., 
<taxonomicName id="54F4F9CB0A5628D4AD06DD041ABD235B" class="Magnoliopsida" family="Theaceae" genus="Camellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Camellia sasanqua" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="sasanqua">Camellia sasanqua</taxonomicName>
Thunb., 
