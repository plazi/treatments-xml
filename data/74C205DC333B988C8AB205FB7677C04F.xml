<taxonomicName id="0B18CE5E473505FABA37D609B3E35F98" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24881" class="Insecta" family="Formicidae" genus="Messor" kingdom="Animalia" lsidName-HNS="Messor Forel" order="Hymenoptera" pageNumber="68" phylum="Arthropoda" rank="genus">Messor</taxonomicName>
