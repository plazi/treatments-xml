<paragraph id="5B3AE70CFFACFFD2AC99F932E45DF920" blockId="4.[151,1436,1102,1694]" box="[159,1306,1672,1694]" pageId="4" pageNumber="31">
Ƥ 
<number id="ED4DF52BFFACFFD2AD73F932E0D3F920" box="[373,404,1673,1694]" pageId="4" pageNumber="31" value="8.1">8.1</number>
