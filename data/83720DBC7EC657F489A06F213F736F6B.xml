<subSubSection id="12D9275CC367AA52D7E1C7912AF4E885" pageId="0" pageNumber="1" type="description">
<paragraph id="936628945227A764752EE01E8F0D60B1" pageId="0" pageNumber="1">Description.</paragraph>
<paragraph id="E653F3FD792AA41851768669B1D8C7AE" pageId="0" pageNumber="1">
<emphasis id="DAFEF248F53E91EEF22CFCCD0E659C42" bold="true" pageId="0" pageNumber="1">
Male (
<emphasis id="A801FE03A9622D96F64995E8425C619E" italics="true" pageId="0" pageNumber="1">holotype</emphasis>
)
</emphasis>
