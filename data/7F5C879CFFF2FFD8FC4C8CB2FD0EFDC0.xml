<taxonomicName id="30F54D09FFF1FFDEFE068467FE0AF7B5" authorityName="Plant" authorityYear="2010" box="[389,489,2091,2112]" class="Insecta" family="Empididae" genus="Anaclastoctedon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="17" phylum="Arthropoda" rank="species" species="antarai">
<emphasis id="C581EA98FFF1FFDEFE068467FE0AF7B5" box="[389,489,2091,2112]" italics="true" pageId="2" pageNumber="17">A. antarai</emphasis>
