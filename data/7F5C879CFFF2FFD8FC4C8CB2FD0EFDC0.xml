<typeStatus id="284E8828FFF2FFDDFCEB8D52FC7CFEC0" box="[872,927,286,309]" pageId="1" pageNumber="16">Type</typeStatus>
species, 
<taxonomicName id="30F54D09FFF2FFDDFB818D52FB38FEC0" authority="Plant, 2010" authorityName="Plant" authorityYear="2010" box="[1026,1243,286,309]" class="Insecta" family="Empididae" genus="Anaclastoctedon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="16" phylum="Arthropoda" rank="species" species="lek" status="new species">
