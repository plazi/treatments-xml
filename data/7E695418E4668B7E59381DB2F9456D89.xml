<taxonomicName id="2A0F867CAC131B19192D3DFC1DB0FD31" authorityName="Tryon" authorityYear="1867" class="Gastropoda" family="Bulimulidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="132" pageNumber="133" phylum="Mollusca" rank="family">Bulimulidae</taxonomicName>
, 
<taxonomicName id="71BA7A4B4E484B9A15E72C91723ED222" baseAuthorityName="Reeve" baseAuthorityYear="1850" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="132" pageNumber="133" phylum="Mollusca" rank="species" species="puellaris" subGenus="Mesembrinus">
<emphasis id="B31EA9EFFE51ED68C300C576C82EA3C8" italics="true" pageId="132" pageNumber="133">Drymaeus (Mesembrinus) puellaris</emphasis>
</taxonomicName>
(Reeve, 1850).
</paragraph>
</subSubSection>
