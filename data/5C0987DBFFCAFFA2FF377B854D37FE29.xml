<taxonomicName id="13A04D4EFFCBFFA2FE857CA54B01FF4C" authority="Sinitshenkova &amp; Aristov" authorityName="Sinitshenkova &amp; Aristov" box="[293,798,152,174]" class="Insecta" family="Vogesonymphidae" genus="Palegonympha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="284" phylum="Arthropoda" rank="species" species="triassica" status="sp. nov.">
<emphasis id="E6D4EADFFFCBFFA2FE857CA54A05FF4C" box="[293,538,152,174]" italics="true" pageId="3" pageNumber="284">Palegonympha triassica</emphasis>
Sinitshenkova &amp; Aristov
</taxonomicName>
, 
<taxonomicNameLabel id="FDE757A4FFCBFFA2FC877CA44B67FF4C" box="[807,888,153,174]" pageId="3" pageNumber="284" rank="species">sp. nov.</taxonomicNameLabel>
