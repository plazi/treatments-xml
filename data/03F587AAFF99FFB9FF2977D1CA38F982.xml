<taxonomicName id="4C5C4D3FFF99FFB9FE8B761AC940FEB6" authority="Setiadi, 2019" authorityName="Setiadi" authorityYear="2019" box="[309,537,298,323]" class="Asteroidea" family="Asterinidae" genus="Limnasterias" kingdom="Animalia" order="Valvatida" pageId="2" pageNumber="394" phylum="Echinodermata" rank="species" species="oinops" status="sp. nov.">
