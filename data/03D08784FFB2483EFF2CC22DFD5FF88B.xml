<collectingCountry id="F36E7602FFB2483EFEDCC437FE72F88B" box="[359,430,1878,1903]" name="China" pageId="3" pageNumber="449">China</collectingCountry>
: 
<collectingRegion id="49BDF870FFB2483EFE02C437FDCFF88B" box="[441,531,1878,1903]" country="China" name="Sichuan" pageId="3" pageNumber="449">Sichuan</collectingRegion>
(
<figureCitation id="13422A17FFB2483EFD99C437FDA9F88B" box="[546,629,1878,1903]" captionStart="FIGURE 16" captionStartId="4.[151,249,895,917]" captionTargetBox="[226,1360,193,873]" captionTargetId="figure@4.[226,1360,193,873]" captionTargetPageId="4" captionText="FIGURE 16. Distribution of Gonioctena (Brachyphytodecta) davidi sp. nov., G. (B.) kangdingensis sp. nov. and G. (B.) flavipennis." httpUri="https://zenodo.org/record/892446/files/figure.png" pageId="3" pageNumber="449">Fig. 16</figureCitation>
