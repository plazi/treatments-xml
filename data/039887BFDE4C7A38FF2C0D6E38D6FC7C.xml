<taxonomicName id="4C314D2ADE4C7A39FDAC0E1B39DAF8D9" authorityName="Mugrabi &amp; Azevedo" authorityYear="2016" box="[588,662,1796,1820]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="108" pageNumber="559" phylum="Arthropoda" rank="species" species="wetliva">
<emphasis id="B945EABBDE4C7A39FDAC0E1B39DAF8D9" box="[588,662,1796,1820]" italics="true" pageId="108" pageNumber="559">wetliva</emphasis>
