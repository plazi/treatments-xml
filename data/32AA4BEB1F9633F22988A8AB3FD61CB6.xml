<taxonomicName id="F9AD97A46CBB532A0C9AE4B86DB93E6F" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Asteraceae" genus="Tagetes" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="patula">Tagetes patula</taxonomicName>
