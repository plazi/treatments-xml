<emphasis id="B900EA8AEA5D63525E43EC2DD7049509" bold="true" box="[264,812,1793,1819]" pageId="1" pageNumber="32">
Family 
<taxonomicName id="4C744D1BEA5D63525E2CEC2DD7049509" authority="Kraepelin, 1905" authorityName="Kraepelin" authorityYear="1905" box="[359,812,1793,1819]" class="Arachnida" family="Heteroscorpionidae" kingdom="Animalia" order="Scorpiones" pageId="1" pageNumber="32" phylum="Arthropoda" rank="family">
Heteroscorpionidae Kraepelin, 
<number id="3DBC24BFEA5D63525DBAEC2DD7049509" box="[753,812,1793,1819]" pageId="1" pageNumber="32" value="1905.0">1905</number>
</taxonomicName>
