<taxonomicName id="268E4D51FFEC3B62FF6DF97CA680873D" authority="Braet, 2016" authorityName="Braet" authorityYear="2016" box="[254,428,1772,1792]" class="Insecta" family="Braconidae" genus="Betylobracon" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="418" phylum="Arthropoda" rank="species" species="novotnyi" status="n.sp.">
