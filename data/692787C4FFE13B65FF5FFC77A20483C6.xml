<taxonomicNameLabel id="C8C957BBFFEC3B62FE22F97CA6F4873D" box="[433,472,1772,1792]" pageId="11" pageNumber="418" rank="species">n.sp.</taxonomicNameLabel>
, 
<typeStatus id="3E358870FFEC3B62FE4DF97CA504873D" box="[478,552,1772,1792]" pageId="11" pageNumber="418">holotype</typeStatus>
