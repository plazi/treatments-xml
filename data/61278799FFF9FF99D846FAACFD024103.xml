<location id="EC516054FFF9FF98DB21FA15F8E040D2" LSID="urn:lsid:plazi:treatment:61278799FFF9FF99D846FAACFD024103:EC516054FFF9FF98DB21FA15F8E040D2" box="[1588,1848,1435,1472]" country="Brazil" name="Nova Teutonia" pageId="22" pageNumber="19" stateProvince="Santa Catarina">
