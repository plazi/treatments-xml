<quantity id="55669B059D649223FF2EFA58C97F2E63" box="[189,297,1510,1537]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="120" pageNumber="121" unit="mm" value="2.7">2.70 mm</quantity>
