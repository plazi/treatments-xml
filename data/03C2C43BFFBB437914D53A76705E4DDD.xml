<taxonomicName id="4C6B0EAEFFBB4379101C3A3C70C04D41" box="[1081,1212,788,812]" class="Arachnida" family="Hubbardiidae" genus="Stenochrus" kingdom="Animalia" order="Schizomida" pageId="37" pageNumber="37" phylum="Arthropoda" rank="species" species="sbordonii">
