</collectingRegion>
: 
<collectingMunicipality id="6BDEACB8BF6FD15C84F3FE672DE54A74" box="[499,665,511,536]" pageId="39" pageNumber="40">Rivеr Amаghu</collectingMunicipality>
bеlоw 
<location id="8EDA6019BF6FD15C87EEFE672C204A74" LSID="urn:lsid:plazi:treatment:03AC87D4BF6FD15C8597FF0F2A6F4A0C:8EDA6019BF6FD15C87EEFE672C204A74" box="[750,860,511,536]" municipality="Rivir Amaghu" name="Noravank" pageId="39" pageNumber="40" stateProvince="Vayots Dzor">Nоrаvаnk</location>
сhurсh, N39° 
<collectingDate id="EFFFE9EABF6FD15C86FBFD982B4D4A74" box="[1019,1073,511,536]" pageId="39" pageNumber="40" value="1941-01">
<date id="FFBB1002BF6FD15C86FBFD982B4D4A74" box="[1019,1073,511,536]" pageId="39" pageNumber="40" value="1941-01">41 Е</date>
</collectingDate>
45° 
<elevation id="0028D1F1BF6FD15C8159FE672BA54A7B" box="[1113,1241,511,536]" metricMagnitude="1" metricUnit="m" metricValue="1.4146" pageId="39" pageNumber="40" unit="m" value="14.146">
<quantity id="4CFD9B27BF6FD15C8159FE672BA54A7B" box="[1113,1241,511,536]" metricMagnitude="1" metricUnit="m" metricValue="1.4146" pageId="39" pageNumber="40" unit="m" value="14.146">14, 1460 m</quantity>
</elevation>
, 
<collectingDate id="EFFFE9EABF6FD15C81E6FE672A1C4A74" box="[1254,1376,511,536]" pageId="39" pageNumber="40" value="2010-06-24">
<date id="FFBB1002BF6FD15C81E6FE672A1C4A74" box="[1254,1376,511,536]" pageId="39" pageNumber="40" value="2010-06-24">24.vi.2010</date>
