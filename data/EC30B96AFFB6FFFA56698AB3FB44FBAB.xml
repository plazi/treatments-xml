<taxonomicName id="A39973FFFFB6FFFB55AD8814FDFEF8CC" box="[432,607,1858,1885]" class="Magnoliopsida" family="Betulaceae" genus="Ostrya" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="carpinifolia">
