<taxonomicName id="A39973FFFFB6FFFA56598886FDF7FEB5" authority="(P. Micheli ex Spach) C. K. Schneid., Illustriertes Handbuch der Laubholzkunde" authorityName="C. K. Schneid., Illustriertes Handbuch der Laubholzkunde" baseAuthorityName="Spach" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" lastPageId="33" lastPageNumber="34" order="Fagales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="variety" species="ostrya" variety="italica">
