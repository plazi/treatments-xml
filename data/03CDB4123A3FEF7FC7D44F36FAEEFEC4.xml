<paragraph id="8BDB05043A3FEF74C7D44F36FCEDF8F1" blockId="7.[151,787,1807,1866]" box="[151,787,1807,1833]" pageId="7" pageNumber="24">
<heading id="D093B2683A3FEF74C7D44F36FCEDF8F1" bold="true" box="[151,787,1807,1833]" fontSize="11" level="1" pageId="7" pageNumber="24" reason="1">
<emphasis id="B910D9163A3FEF74C7D44F36FCEDF8F1" bold="true" box="[151,787,1807,1833]" pageId="7" pageNumber="24">
<taxonomicName id="4C647E873A3FEF74C7D44F36FE2EF8F1" authority="Boldrini &amp; Pes &amp; Francischetti &amp; Salles, 2012" authorityName="Boldrini &amp; Pes &amp; Francischetti &amp; Salles" authorityYear="2012" box="[151,464,1807,1833]" class="Insecta" family="Baetidae" genus="Camelobaetidius" kingdom="Animalia" order="Ephemeroptera" pageId="7" pageNumber="24" phylum="Arthropoda" rank="species" species="spinosus" status="sp. nov.">
