<taxonomicName id="FFCF4D4DFFA566495913905F3CE1FCA8" box="[581,690,803,827]" class="Magnoliopsida" family="Moraceae" genus="Dorstenia" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="210" phylum="Tracheophyta" rank="genus">Dorstenia</taxonomicName>
<emphasis id="0ABBEADCFFA566495BF8903F3C4AFCC8" box="[174,537,835,859]" italics="true" pageId="3" pageNumber="210">
tubicina 
<collectionCode id="5EDEAE0BFFA566495BB0903F3EA5FCC8" box="[230,246,835,859]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="3" pageNumber="210">R</collectionCode>
. et 
