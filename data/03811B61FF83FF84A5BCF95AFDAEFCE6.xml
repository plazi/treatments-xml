<collectingRegion id="49EC6495FF83FF84A172F9ADFAD0FD3E" box="[1113,1316,725,750]" country="Saudi Arabia" name="Makkah" pageId="10" pageNumber="351">Makkah Province</collectingRegion>
, 
<collectingCountry id="F33FEAE7FF83FF84A019F9AEFA9EFD3E" box="[1330,1386,726,750]" name="Saudi Arabia" pageId="10" pageNumber="351">KSA</collectingCountry>
