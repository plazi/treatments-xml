<quantity id="6F359B7DFF8CC61E6683FD02FDC7FD54" box="[491,553,642,664]" metricMagnitude="3" metricUnit="m" metricValue="1.513" pageId="32" pageNumber="797" unit="m" value="1513.0">1513m</quantity>
