<quantity id="6F359B7DFF8CC61E655AFD1AFD9EFD63" box="[562,624,666,687]" metricMagnitude="3" metricUnit="m" metricValue="1.7469999999999999" pageId="32" pageNumber="797" unit="m" value="1747.0">1747m</quantity>
