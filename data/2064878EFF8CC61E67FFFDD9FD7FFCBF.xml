<quantity id="6F359B7DFF8CC61E67ACFD32FEEDFD04" box="[196,259,690,712]" metricMagnitude="3" metricUnit="m" metricValue="1.285" pageId="32" pageNumber="797" unit="m" value="1285.0">1285m</quantity>
, 8.7.17; Reichraming, Schieferstein, Gipfel, 
<quantity id="6F359B7DFF8CC61E65FCFD32FD3AFD04" box="[660,724,690,712]" metricMagnitude="3" metricUnit="m" metricValue="1.153" pageId="32" pageNumber="797" unit="m" value="1153.0">1153m</quantity>
