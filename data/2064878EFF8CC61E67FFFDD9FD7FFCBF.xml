<quantity id="6F359B7DFF8CC61E6488FD02FBF0FD54" box="[992,1054,642,664]" metricMagnitude="3" metricUnit="m" metricValue="1.54" pageId="32" pageNumber="797" unit="m" value="1540.0">1540m</quantity>
