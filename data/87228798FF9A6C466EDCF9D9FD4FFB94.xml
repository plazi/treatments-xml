<quantity id="C8739B6BFF9B6C456C95F95AFD0DF92F" box="[649,760,1704,1728]" metricMagnitude="-3" metricUnit="m" metricValue="1.86" pageId="17" pageNumber="16" unit="mm" value="1.86">1.86 mm</quantity>
