<quantity id="F34989E5FF883F5EAA56FD4CBBBFFBF7" box="[849,995,712,734]" metricMagnitude="-2" metricUnit="m" metricValue="1.085" metricValueMax="1.22" metricValueMin="0.95" pageId="5" pageNumber="452" unit="mm" value="10.85" valueMax="12.2" valueMin="9.5">9.5–12.2 mm</quantity>
