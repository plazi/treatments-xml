<emphasis id="06C5F812FF883F5EABC2FD29BB1DFBE8" bold="true" box="[709,833,685,706]" pageId="5" pageNumber="452">Description.</emphasis>
Length 
<quantity id="F34989E5FF883F5EAAA2FD29BC1BFBE8" box="[933,1095,685,707]" metricMagnitude="-2" metricUnit="m" metricValue="2.2" metricValueMax="2.4899999999999998" metricValueMin="1.91" pageId="5" pageNumber="452" unit="mm" value="22.0" valueMax="24.9" valueMin="19.1">19.1–24.9 mm</quantity>
