<heading id="3F2781DEFF9B8E21FECBEEA231D649A1" box="[337,896,941,965]" centered="true" fontSize="10" level="2" pageId="12" pageNumber="1361" reason="3">
<taxonomicName id="A3D04D31FF9B8E21FECBEEA230A349A1" authority="Gledhill &amp; Vidrine, 2002" authorityName="Gledhill &amp; Vidrine" authorityYear="2002" box="[337,757,941,965]" class="Arachnida" family="Unionicolidae" genus="Unionicola" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="1361" phylum="Arthropoda" rank="species" species="planicurvata" status="new species" subGenus="Mutelicola">
