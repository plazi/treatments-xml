<taxonomicName id="4C2901BE04A515F6FB134A30FAEFFBC7" authorityName="Meunier" authorityYear="1880" box="[1072,1284,1049,1075]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="172" pageNumber="173" phylum="Mollusca" rank="species" species="tricarinata">
