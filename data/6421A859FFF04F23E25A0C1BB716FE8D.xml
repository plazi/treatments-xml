<taxonomicName id="2B8862CCFFF04F1CE52C0AD8B530FAF3" authority="Lamarck, 1804" authorityName="Lamarck" authorityYear="1804" class="Gastropoda" family="Turridae" genus="Pleurotoma" kingdom="Animalia" order="Neogastropoda" pageId="31" pageNumber="48" phylum="Mollusca" rank="species" species="plicata">
