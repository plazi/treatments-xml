<taxonomicName id="B1B14D2B3923FFC8ED22FC561362FC4B" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[273,634,952,975]" class="Insecta" family="Caenidae" genus="Caenis" kingdom="Animalia" order="Ephemeroptera" pageId="20" pageNumber="334" phylum="Arthropoda" rank="species" species="horaria">
