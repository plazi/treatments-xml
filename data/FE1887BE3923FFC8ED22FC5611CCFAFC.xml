<taxonomicName id="B1B14D2B3923FFC8ECF2FAAF1329FADC" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[193,561,1345,1368]" class="Insecta" family="Caenidae" genus="Caenis" kingdom="Animalia" order="Ephemeroptera" pageId="20" pageNumber="334" phylum="Arthropoda" rank="species" species="horaria">
