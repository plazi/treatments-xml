<taxonomicName id="1EB94D5EFFB82712FF3A6438FD24FDB1" authority="Gerstaecker" authorityName="Gerstaecker" box="[255,729,545,583]" class="Insecta" family="Mydidae" genus="Mitrodetus" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="54" pageNumber="93" phylum="Arthropoda" rank="genus">
