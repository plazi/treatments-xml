<taxonomicName id="E3CDE7813A54FFD634F2FE50DF432AD1" authority="Risso, 1810" authorityName="Risso" authorityYear="1810" box="[151,536,474,499]" class="Actinopterygii" family="Epigonidae" genus="Epigonus" kingdom="Animalia" order="Perciformes" pageId="29" pageNumber="322" phylum="Chordata" rank="species" species="telescopus">
