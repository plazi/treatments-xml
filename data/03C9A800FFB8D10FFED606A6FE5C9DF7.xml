<materialsCitation id="3B08134BFFB8D10FFFB50759FF0A9BEB" ID-GBIF-Occurrence="3053565301" collectionCode="MNHN, MGPV" pageId="17" pageNumber="244" specimenCount="1" typeStatus="holotype">
<typeStatus id="54DBA7B4FFB8D10FFFB50759FF359B4B" box="[113,219,812,835]" pageId="17" pageNumber="244">HOLOTYPE</typeStatus>
. — 
<collectionCode id="ED7181D3FFB8D10FFEC2075EFEB79B4A" box="[262,345,811,835]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="17" pageNumber="244">MNHN</collectionCode>
