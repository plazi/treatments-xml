wide. Palpalae pectinate, palpalae 
<emphasis id="B92D674DAA52FFDDFE1E5941FE3FCE16" box="[475,505,577,602]" italics="true" pageId="19" pageNumber="320">dF</emphasis>
and 
<emphasis id="B92D674DAA52FFDDFDF05941FD91CE16" box="[565,599,577,602]" italics="true" pageId="19" pageNumber="320">dG</emphasis>
subequal, 
<number id="3D91A978AA52FFDDFD105941FCDDCE15" box="[725,795,577,601]" pageId="19" pageNumber="320" value="19.0" valueMax="20.0" valueMin="18.0">18–20</number>
long; palpalae 
<emphasis id="B92D674DAA52FFDDFC0B5941FC3FCE16" box="[974,1017,577,602]" italics="true" pageId="19" pageNumber="320">l”G</emphasis>
<number id="3D91A978AA52FFDDFBC65941FB8ECE16" box="[1027,1096,577,602]" pageId="19" pageNumber="320" value="14.5" valueMax="15.0" valueMin="14.0">14–15</number>
