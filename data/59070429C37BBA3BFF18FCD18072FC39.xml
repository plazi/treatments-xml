<taxonomicName id="16AECEBCC37BBA3BFDA8FC2F828DFC62" authority="Modeer, 1778: 23" authorityName="Modeer (Modeer" authorityPageNumber="23" authorityYear="1778" box="[551,913,914,940]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="alni">
