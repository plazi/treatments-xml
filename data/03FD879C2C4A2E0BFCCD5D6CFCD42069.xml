<geoCoordinate id="EE60504D2C402E0BFE905DE4FE202391" box="[350,469,1471,1495]" degrees="36, 103" direction="east" orientation="longitude" pageId="25" pageNumber="25" precision="55" value="36.103">36, 103E</geoCoordinate>
