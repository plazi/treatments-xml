<quantity id="4CFD718BC80C214E4AA3EC783683FA93" box="[902,1005,1302,1327]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="13" pageNumber="464" unit="mm" value="0.35">0.35 mm</quantity>
; EW 
<quantity id="4CFD718BC80C214E4D0EEC7931FCFA93" box="[1067,1170,1303,1327]" metricMagnitude="-3" metricUnit="m" metricValue="1.32" pageId="13" pageNumber="464" unit="mm" value="1.32">1.32 mm</quantity>
; EL 
<quantity id="4CFD718BC80C214E4DE2EC783040FA93" box="[1223,1326,1302,1327]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="13" pageNumber="464" unit="mm" value="1.6">1.60 mm</quantity>
