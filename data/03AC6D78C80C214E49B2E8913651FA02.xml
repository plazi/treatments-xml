<quantity id="4CFD718BC80C214E4BC9EC79363DFA93" box="[748,851,1303,1327]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="13" pageNumber="464" unit="mm" value="0.9">0.90 mm</quantity>
