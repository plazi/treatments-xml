e of Pond Run Tower, 
<date id="FFA40CA3FFA2FA6E842A07E8FD454CE8" box="[567,730,192,218]" pageId="6" pageNumber="7" value="1989-08-24">
<number id="3DD23844FFA2FA6E842A07E8FDCB4CE8" box="[567,596,192,218]" pageId="6" pageNumber="7" value="24.0">24</number>
Aug. 
<number id="3DD23844FFA2FA6E848207E8FD454CE8" box="[671,730,192,218]" pageId="6" pageNumber="7" value="1989.0">1989</number>
</date>
(
<quantity id="4CE28786FFA2FA6E84F107E8FC8B4CE8" box="[748,788,192,218]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="6" pageNumber="7" unit="m" value="1.0">1m</quantity>
, 
<number id="3DD23844FFA2FA6E853F07E8FCB24CE8" box="[802,813,192,218]" pageId="6" pageNumber="7" value="1.0">1</number>
