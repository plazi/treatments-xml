<taxonomicName id="403568BEFA65A4D9E99617F0FA508374" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis distinctissima" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="distinctissima">Chrysis distinctissima</taxonomicName>
