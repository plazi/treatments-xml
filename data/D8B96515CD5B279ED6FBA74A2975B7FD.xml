<taxonomicName id="433AEA5A07B924E6419B5731042BC01A" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis distinctissima" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="distinctissima">Chrysis distinctissima</taxonomicName>
