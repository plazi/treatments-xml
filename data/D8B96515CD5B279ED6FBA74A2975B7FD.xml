<taxonomicName id="4CA503F907A53165BC2FFA0E9501EEAC" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis intricans" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="intricans">Chrysis intricans</taxonomicName>
