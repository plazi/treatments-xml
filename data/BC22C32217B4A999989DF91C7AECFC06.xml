<paragraph id="3434723417B4A99B989DF91C7B60F90D" blockId="133.[151,803,1751,1818]" box="[151,803,1751,1777]" pageId="133" pageNumber="134">
<heading id="6F7CC55817B4A99B989DF91C7B60F90D" bold="true" box="[151,803,1751,1777]" fontSize="11" level="1" pageId="133" pageNumber="134" reason="1">
<emphasis id="06FFAE2617B4A99B989DF91C7B60F90D" bold="true" box="[151,803,1751,1777]" pageId="133" pageNumber="134">
<taxonomicName id="F38B09B717B4A99B989DF91C7AC5F90D" authority="Olah &amp; Schefter" authorityName="Olah &amp; Schefter" authorityYear="2008" box="[151,646,1751,1777]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="yathetima" status="new species">
<emphasis id="06FFAE2617B4A99B989DF91C79F0F90D" bold="true" box="[151,435,1751,1777]" italics="true" pageId="133" pageNumber="134">Hydropsyche yathetima</emphasis>
Oláh &amp; Schefter
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D17B4A99B9A99F91C7B60F90D" box="[659,803,1751,1777]" pageId="133" pageNumber="134" rank="species">new species</taxonomicNameLabel>
