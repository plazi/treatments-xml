10359´26´´E, 
<quantity id="4CDA8EA7CF3BB73FFECC472303F62F57" box="[299,423,1365,1387]" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="1.9" metricValueMin="1.35" pageId="20" pageNumber="1295" unit="m" value="1625.0" valueMax="1900.0" valueMin="1350.0">1350-1900 m</quantity>
, 
<date id="FF9C0582CF3BB73FFE57472300472F56" box="[432,534,1365,1387]" pageId="20" pageNumber="1295" value="2013-06-28">28.06.2013</date>
