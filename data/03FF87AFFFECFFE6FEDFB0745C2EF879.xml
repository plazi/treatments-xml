<tableCitation id="C6D40302FFEFFFE6FC82B1EE5DBAFA89" box="[853,943,1175,1201]" captionStart="TABLE 6" captionStartId="17.[264,352,283,307]" captionTargetBox="[269,1308,345,696]" captionTargetPageId="17" captionText="TABLE 6. Wing measurements (mean value, in mm) of the Mycomyiella species." httpUri="http://table.plazi.org/id/DF296631FFECFFE5FEDFB4625A65FF0B" pageId="18" pageNumber="19" tableUuid="DF296631FFECFFE5FEDFB4625A65FF0B">
Table 
<number id="3D9E249EFFEFFFE6FC4AB1EE5DBAFA89" box="[925,943,1175,1201]" pageId="18" pageNumber="19" value="6.0">6</number>
</tableCitation>
