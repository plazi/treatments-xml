<number id="3D9E249EFFEFFFE6FC6CB1C65DDFFAE1" box="[955,970,1215,1241]" pageId="18" pageNumber="19" value="5.0">5</number>
<number id="3D9E249EFFEFFFE6FC06B1C65A5FFAE1" box="[977,1098,1215,1241]" pageId="18" pageNumber="19" value="0.565" valueMax="0.64" valueMin="0.49">0.49–0.64</number>
, 
<quantity id="4CAE9B5CFFEFFFE6FB82B1C65AABFAE1" box="[1109,1214,1215,1241]" metricMagnitude="-4" metricUnit="m" metricValue="5.699999999999999" pageId="18" pageNumber="19" unit="mm" value="0.57">0.57 mm</quantity>
