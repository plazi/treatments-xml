<number id="3D9E249EFFEFFFE6FDE5B1C65CBEFAE1" box="[562,683,1215,1241]" pageId="18" pageNumber="19" value="2.3200000000000003" valueMax="2.49" valueMin="2.15">2.15–2.49</number>
, 
<quantity id="4CAE9B5CFFEFFFE6FD61B1C65D0AFAE1" box="[694,799,1215,1241]" metricMagnitude="-3" metricUnit="m" metricValue="2.38" pageId="18" pageNumber="19" unit="mm" value="2.38">2.38 mm</quantity>
