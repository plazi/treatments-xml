<date id="8840B680FFA6FFA0FC62BE19921CFC45" box="[967,1099,895,921]" pageId="11" pageNumber="30" value="1892-03-29">
<number id="4A368267FFA6FFA0FC62BE1995B1FC45" box="[967,998,895,921]" pageId="11" pageNumber="30" value="29.0">29</number>
