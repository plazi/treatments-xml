<taxonomicName id="3BFEEBC3FFA6FFA0FE2EB921957CFBBD" authority="Jacobson" authorityName="Jacobson" box="[395,811,1095,1121]" class="Insecta" family="Chrysomelidae" genus="Longitarsus" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="30" phylum="Arthropoda" rank="species" species="tmetopterus">
