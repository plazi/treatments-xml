<date id="2A0983881A6DFF8C9306CAD3BF9A3995" box="[337,473,399,425]" pageId="20" pageNumber="21" value="1998-11-30">
<number id="E87FB76F1A6DFF8C9306CAD3BF323995" box="[337,369,399,425]" pageId="20" pageNumber="21" value="30.0">30</number>
.XI. 
<number id="E87FB76F1A6DFF8C93CCCAD3BF9A3995" box="[411,473,399,425]" pageId="20" pageNumber="21" value="1998.0">1998</number>
</date>
