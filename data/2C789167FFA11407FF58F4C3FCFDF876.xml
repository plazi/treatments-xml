<caption id="F0AE70F9FFA21401FF58F1D2FBBEF80A" httpUri="https://zenodo.org/record/219102/files/figure.png" pageId="2" pageNumber="275" targetBox="[161,1415,195,1940]" targetPageId="2">
<paragraph id="A46E2071FFA21401FF58F1D2FBBEF80A" blockId="2.[151,1435,1982,2035]" pageId="2" pageNumber="275">
