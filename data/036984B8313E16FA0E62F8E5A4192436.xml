<taxonomicName id="80F2BC094A0BC37516CA7934FD0DF2F8" LSID="http://zoobank.org/05582597-527B-4EBB-A824-9343C1646DC8" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus cora" order="Hymenoptera" pageId="223" pageNumber="224" phylum="Arthropoda" rank="species" species="cora">Heterospilus cora Marsh</taxonomicName>
