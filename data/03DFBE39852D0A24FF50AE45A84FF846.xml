<materialsCitation id="3B1E0572852E0A24FF00A85CA80FFECE" ID-GBIF-Occurrence="2013952602" box="[199,512,332,357]" country="South Africa" location="South Africa" pageId="2" pageNumber="398" specimenCount="1" typeStatus="holotype">
<emphasis id="B902D33D852E0A24FF00A85CAB66FECE" bold="true" box="[199,361,332,357]" pageId="2" pageNumber="398">
<typeStatus id="54CDB18D852E0A24FF00A85CAB0DFECE" box="[199,258,332,357]" pageId="2" pageNumber="398">Type</typeStatus>
locality:
</emphasis>
