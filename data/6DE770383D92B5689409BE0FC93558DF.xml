<geoCoordinate id="5AD26FB838F71EE345B5F8692877AA36" direction="north" orientation="latitude" precision="925" value="44.083332">44°05'N</geoCoordinate>
, 
<geoCoordinate id="0AD348E02DEE5E4DE56BE91E92F9B19D" direction="east" orientation="longitude" precision="925" value="77.03333">77°02'E</geoCoordinate>
