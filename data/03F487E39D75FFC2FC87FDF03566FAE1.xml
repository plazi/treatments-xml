<geoCoordinate id="EE6950329D75FFC2FB2DFD9D3422FD5B" box="[1159,1305,625,651]" degrees="077" direction="west" minutes="44" orientation="longitude" pageId="9" pageNumber="660" precision="15" seconds="04" value="-77.73444">077°44′04″W</geoCoordinate>
