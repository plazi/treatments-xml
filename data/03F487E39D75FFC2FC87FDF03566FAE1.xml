<quantity id="4CA59B109D75FFC2FA89FD9D345CFD5B" box="[1315,1383,627,651]" metricMagnitude="2" metricUnit="m" metricValue="1.03" pageId="9" pageNumber="660" unit="m" value="103.0">103 m</quantity>
; 
<date id="FFE310359D75FFC2FAD8FD9D32A3FD7C" pageId="9" pageNumber="660" value="2016-04-13">13 Apr. 2016</date>
