<taxonomicName id="4536886D3869334835C7DDF4AEC1D974" class="Insecta" family="Chalcididae" genus="Chalcis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcis" order="Hymenoptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="genus">Chalcis</taxonomicName>
