<taxonomicName id="2186EB1B447F128DB5073B5FE5D41768" class="Insecta" family="Chalcididae" genus="Brachymeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachymeria" order="Hymenoptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="genus">Brachymeria</taxonomicName>
