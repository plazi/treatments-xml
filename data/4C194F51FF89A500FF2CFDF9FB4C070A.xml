<figureCitation id="5C8BE2C2FF87A500FD41FF64FCAA068C" box="[762,892,185,210]" captionStart="FIGURES 13 – 22" captionStartId="8.[151,264,1915,1937]" captionTargetBox="[323,1277,196,1877]" captionTargetId="figure@8.[323,1278,196,1878]" captionTargetPageId="8" captionText="FIGURES 13 – 22. SEM photos of Pseudoparonella tanimbarica: 13, eyes; 14, scales on antennae; 15, scales on head; 16, scales on terga; 17, scales on manubrium; 18, scales on dens; 19, scales on leg; 20, spiny chaetae on trochanteral organ; 21, fore claw; 22, mucro." httpUri="https://zenodo.org/record/236364/files/figure.png" pageId="9" pageNumber="570">
Figs 
<number id="7278EC60FF87A500FC89FF64FCAA068C" box="[818,892,185,209]" pageId="9" pageNumber="570" value="16.5" valueMax="19.0" valueMin="14.0">14‒19</number>
</figureCitation>
), trochanteral organ (
<figureCitation id="5C8BE2C2FF87A500FBD2FF64FB6A068C" box="[1129,1212,185,210]" captionStart="FIGURES 13 – 22" captionStartId="8.[151,264,1915,1937]" captionTargetBox="[323,1277,196,1877]" captionTargetId="figure@8.[323,1278,196,1878]" captionTargetPageId="8" captionText="FIGURES 13 – 22. SEM photos of Pseudoparonella tanimbarica: 13, eyes; 14, scales on antennae; 15, scales on head; 16, scales on terga; 17, scales on manubrium; 18, scales on dens; 19, scales on leg; 20, spiny chaetae on trochanteral organ; 21, fore claw; 22, mucro." httpUri="https://zenodo.org/record/236364/files/figure.png" pageId="9" pageNumber="570">
