<typeStatus id="225D881699207071FFF767E5C0A4FC33" box="[113,162,907,930]" pageId="13" pageNumber="86">TYPE</typeStatus>
SPECIES. — 
<taxonomicName id="3AE64D3799207071FE9D67E5C17FFC31" authority="Vršanský, 2009" authorityName="Vršanský" authorityYear="2009" box="[283,377,906,930]" class="Insecta" family="Mesoblattinidae" genus="Sivis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="13" pageNumber="86" phylum="Arthropoda" rank="species" species="odpo" status="n. sp.">
