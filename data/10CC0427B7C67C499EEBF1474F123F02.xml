<taxonomicName id="5757A9DE6BBD09CD639457172F4A179A" class="Polychaeta" family="Sigalionidae" genus="Pisione" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pisione remota" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="remota">Pisione remota</taxonomicName>
(Southern, 1914) | 
<taxonomicName id="66218EBE6364D0AF8008A16308EA703E" genus="Praegeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Praegeria remota" pageId="0" pageNumber="20997" rank="species" species="remota">Praegeria remota</taxonomicName>
