<taxonomicName id="C2BDE6FFFFCB543A2E17FCDBFB1DFD61" authority="Bourgoin" authorityName="Bourgoin" authorityYear="1922" box="[794,1169,684,710]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="erythrostictus">Agrilus erythrostictus Bourgoin</taxonomicName>
