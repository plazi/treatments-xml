<taxonomicName id="4C6A4D27FFCBFFDDFCAEFF52FBDD43D4" authorityName="Howard" authorityYear="1898" box="[895,1025,211,236]" class="Insecta" family="Encyrtidae" genus="Astymachus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="669" phylum="Arthropoda" rank="genus">
