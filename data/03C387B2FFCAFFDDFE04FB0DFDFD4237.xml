<taxonomicName id="4C6A4D27FFCBFFDDFEE4FF52FDC943D4" authorityName="Hayat in Hayat and Veenakumari" authorityYear="2015" box="[309,533,211,236]" class="Insecta" family="Encyrtidae" genus="Astymachus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="669" phylum="Arthropoda" rank="species" species="eximius">
