<taxonomicName id="681AF0D5C84E3E579DB6A8516D1A6087" authorityName="Murdock &amp; C. W. Chen" authorityYear="2020" baseAuthorityName="Murdock &amp; Chen &amp; Huang &amp; Glenny" baseAuthorityYear="2020" class="Polypodiopsida" family="Marattiaceae" genus="Ptisana" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ptisana papuana" order="Marattiales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="papuana">
