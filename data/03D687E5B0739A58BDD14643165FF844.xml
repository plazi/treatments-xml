<quantity id="4C879B16B0739A58BC7744291284F9FC" box="[305,407,1621,1645]" metricMagnitude="-4" metricUnit="m" metricValue="3.3" pageId="35" pageNumber="74" unit="mm" value="0.33">0.33 mm</quantity>
