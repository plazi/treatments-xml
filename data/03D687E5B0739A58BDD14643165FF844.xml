<quantity id="4C879B16B0739A58BE5544291069F9FC" box="[787,890,1621,1645]" metricMagnitude="-4" metricUnit="m" metricValue="4.3" pageId="35" pageNumber="74" unit="mm" value="0.43">0.43 mm</quantity>
