<quantity id="4C879B16B0739A58BC9544291129F9FC" box="[467,570,1621,1645]" metricMagnitude="-4" metricUnit="m" metricValue="2.9" pageId="35" pageNumber="74" unit="mm" value="0.29">0.29 mm</quantity>
