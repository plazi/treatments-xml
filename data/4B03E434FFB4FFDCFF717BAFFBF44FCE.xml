<taxonomicName id="04AA2EA1FFB4FFDBFAD77AD9FEB54904" authority="Szumik, 1997" authorityName="Szumik" authorityYear="1997" class="Insecta" family="Scelembiidae" genus="Biguembia" kingdom="Animalia" order="Embioptera" pageId="11" pageNumber="349" phylum="Arthropoda" rank="species" species="cocum">
