<taxonomicName id="DB974D55FFC3FF9BFF5D6C03FF4FF12C" authorityName="Pessagno" authorityYear="1969" box="[151,253,1322,1349]" class="Polycystina" family="Cuniculiformidae" genus="Cassideus" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="37" pageNumber="42" phylum="Radiozoa" rank="species" species="riedeli">
