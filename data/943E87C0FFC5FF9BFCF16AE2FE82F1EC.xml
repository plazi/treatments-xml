. 
<emphasis id="2EE3EAC4FFC3FF9BFF706C02FF4FF12C" box="[186,253,1323,1349]" italics="true" pageId="37" pageNumber="42">riedeli</emphasis>
</taxonomicName>
and 
<taxonomicName id="DB974D55FFC3FF9BFEF06C03FE05F12C" box="[314,439,1322,1349]" class="Polycystina" family="Cuniculiformidae" genus="Cassideus" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="37" pageNumber="42" phylum="Radiozoa" rank="species" species="yoloensis">
