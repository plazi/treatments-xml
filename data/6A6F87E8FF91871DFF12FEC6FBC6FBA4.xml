, 
<collectingRegion id="2002F81CFF91871DFC8BFB96FCCCFBA4" box="[782,870,1087,1112]" country="Taiwan" name="Taitung" pageId="3" pageNumber="424">Taitung</collectingRegion>
, 
<collectingRegion id="2002F81CFF91871DFCF5FB96FC79FBA4" box="[880,979,1087,1112]" country="Taiwan" name="Taoyuan" pageId="3" pageNumber="424">Taoyuan</collectingRegion>
)—Unknown.
