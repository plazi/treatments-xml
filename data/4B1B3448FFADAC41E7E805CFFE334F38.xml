<taxonomicName id="04B2FEDDFFADAC44E7E805CFFEBA48AB" box="[151,272,1501,1527]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="495" phylum="Arthropoda" rank="genus">
