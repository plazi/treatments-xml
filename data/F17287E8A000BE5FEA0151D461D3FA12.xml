<number id="CF1324D9A001BE5FEF2C5239675CFAD4" box="[1061,1076,1289,1315]" pageId="1" pageNumber="2" value="2.0">2</number>
, JV 
<number id="CF1324D9A001BE5FEF6A5239671AFAD4" box="[1123,1138,1289,1315]" pageId="1" pageNumber="2" value="4.0">4</number>
, ZV 
<number id="CF1324D9A001BE5FEFA4523967DFFAD4" box="[1197,1207,1289,1315]" pageId="1" pageNumber="2" value="1.0">1</number>
, ZV 
<number id="CF1324D9A001BE5FEFE252396791FAD4" box="[1259,1273,1289,1315]" pageId="1" pageNumber="2" value="2.0">2</number>
