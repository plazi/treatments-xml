<number id="CF1324D9A001BE5FE8CA531160B9FBCC" box="[963,977,1057,1083]" pageId="1" pageNumber="2" value="5.0">5</number>
, ZV 
<number id="CF1324D9A001BE5FEF025311677DFBCC" box="[1035,1045,1057,1083]" pageId="1" pageNumber="2" value="1.0">1</number>
, ZV 
<number id="CF1324D9A001BE5FEF4253116731FBCC" box="[1099,1113,1057,1083]" pageId="1" pageNumber="2" value="2.0">2</number>
and ZV 
<number id="CF1324D9A001BE5FEFB6531167A4FBCC" box="[1215,1228,1057,1083]" pageId="1" pageNumber="2" value="3.0">3</number>
