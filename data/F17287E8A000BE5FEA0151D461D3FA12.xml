<number id="CF1324D9A001BE5FEF0654E46777FC19" box="[1039,1055,980,1006]" pageId="1" pageNumber="2" value="4.0">4</number>
and S 
<number id="CF1324D9A001BE5FEF6054E46710FC19" box="[1129,1144,980,1006]" pageId="1" pageNumber="2" value="5.0">5</number>
) and 
<number id="CF1324D9A001BE5FEFB254E467A1FC19" box="[1211,1225,980,1006]" pageId="1" pageNumber="2" value="2.0">2</number>
pairs of setae in r–R series (r 
<number id="CF1324D9A001BE5FEAF454CA6163FBE3" box="[509,523,1018,1044]" pageId="1" pageNumber="2" value="3.0">3</number>
and R 
<number id="CF1324D9A001BE5FE95654CA6102FBE3" box="[607,618,1018,1044]" pageId="1" pageNumber="2" value="1.0">1</number>
