<taxonomicName id="30C2B9D2FF97F215856E68C1FB173C7F" authority="Fallen, 1807" authorityName="Fallen" authorityYear="1807" box="[878,1212,1014,1039]" class="Insecta" family="Saldidae" genus="Halosalda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="370" phylum="Arthropoda" rank="species" species="halophila">
<emphasis id="C5B61E43FF97F215856E68C1FBAE3C7F" box="[878,1029,1014,1039]" italics="true" pageId="3" pageNumber="370">H. halophila</emphasis>
(Fallén, 
<number id="410AD076FF97F215827B68C1FB1F3C7F" box="[1147,1204,1015,1039]" pageId="3" pageNumber="370" value="1807.0">1807</number>
)
</taxonomicName>
