, 
<materialsCitation id="3B473CB7FF8EA7483AEB024DFB0A7260" box="[665,1264,1100,1125]" collectionCode="USNM" pageId="26" pageNumber="830" specimenCode="USNM 1003325" specimenCount="1" typeStatus="holotype">
