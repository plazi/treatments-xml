<collectionCode id="D9993082FFE57525FE24F998FE7DE34C" box="[478,501,1602,1626]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="21" pageNumber="108" type="Herbarium">G</collectionCode>
