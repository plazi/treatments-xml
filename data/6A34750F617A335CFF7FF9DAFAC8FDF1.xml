<subSubSection id="AA8797926171335CFF2FFE36FC0EFE1E" box="[199,963,474,502]" pageId="8" pageNumber="425" type="distribution">
<paragraph id="E222C4196171335CFF2FFE36FC0EFE1E" blockId="8.[151,1437,150,537]" box="[199,963,474,502]" pageId="8" pageNumber="425">
<emphasis id="D0E9180B6171335CFF2FFE36FE94FE1C" bold="true" box="[199,345,474,500]" pageId="8" pageNumber="425">Distribution</emphasis>
. Known from Northern 
<collectingCountry id="9A8A84896171335CFD82FE37FD03FE1D" box="[618,718,475,501]" name="Thailand" pageId="8" pageNumber="425">Thailand</collectingCountry>
