<emphasis id="B91EEA93FFC8FF9DF049FF4CFC2AFF0A" box="[564,911,152,173]" italics="true" pageId="3" pageNumber="23">
<taxonomicName id="4C6A4D02FFC8FF9DF049FF4CFCA9FF0A" authorityName="Joharchi &amp; Pirani" authorityYear="2013" box="[564,780,152,173]" class="Magnoliopsida" family="Caryophyllaceae" genus="Acanthophyllum" kingdom="Plantae" order="Caryophyllales" pageId="3" pageNumber="23" phylum="Tracheophyta" rank="species" species="yasamin-nassehiae">A. yasamin-nassehiae</taxonomicName>
, 
