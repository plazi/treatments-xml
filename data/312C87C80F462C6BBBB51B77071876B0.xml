<collectingCountry id="C192764E0F462C6BBE251BE4060276ED" box="[1287,1338,1412,1437]" name="Italy" pageId="10" pageNumber="161">Italy</collectingCountry>
(
<bibRefCitation id="DD144B2F0F462C6BBE6E1BE403EB76B0" author="Doberl" pageId="10" pageNumber="161" refString="Doberl, M. (2010) Alticinae. In: Lobl, I. &amp; Smetana, A. (Eds.), Catalogue of Palaearctic Coleoptera. Uol. 6. Chrysomeloidea. Apollo Books, Stenstrup, pp. 491 - 562." type="journal article" year="2010">Döberl 2010</bibRefCitation>
), and very recently in 
<collectingRegion id="7B41F83C0F462C6BBAEE1BC7010276B0" box="[460,570,1447,1472]" country="Spain" name="Catalunya" pageId="10" pageNumber="161">Catalonia</collectingRegion>
