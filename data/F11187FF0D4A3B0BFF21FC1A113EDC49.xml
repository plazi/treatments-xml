<taxonomicName id="BEB84D6A0D4A3B0BFF3EFC6C114EDC3F" authority="Blandford" authorityName="Blandford" box="[218,540,1017,1038]" class="Insecta" family="Curculionidae" genus="Amphicranus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="233" phylum="Arthropoda" rank="species" species="filiformis">
