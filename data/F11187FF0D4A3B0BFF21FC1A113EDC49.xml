17. 
<emphasis id="4BCCEAFB0D4A3B0BFF0CFC1A11C0DB95" bold="true" box="[232,658,911,932]" pageId="7" pageNumber="233">
<taxonomicName id="BEB84D6A0D4A3B0BFF0CFC1A11DCDB95" authority="Blandford, 1905" authorityName="Blandford" authorityYear="1905" box="[232,654,911,932]" class="Insecta" family="Curculionidae" genus="Amphicranus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="233" phylum="Arthropoda" rank="species" species="brevipennis">
