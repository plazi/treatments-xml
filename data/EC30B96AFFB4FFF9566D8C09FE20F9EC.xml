<taxonomicName id="A39973FFFFB4FFF956E4894CFC76F9A4" authorityName="Correll, Wrightia" authorityYear="1965" box="[761,983,1562,1589]" class="Magnoliopsida" family="Betulaceae" genus="Ostrya" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="chisosensis">
