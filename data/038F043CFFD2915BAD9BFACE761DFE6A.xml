. 
<taxonomicName id="4C26CEA9FFD39158AC28F8E1763EF8C0" box="[292,531,1875,1898]" class="Polychaeta" family="Syllidae" genus="Haplosyllis" kingdom="Animalia" order="Phyllodocida" pageId="13" pageNumber="14" phylum="Annelida" rank="species" species="cephalata">
