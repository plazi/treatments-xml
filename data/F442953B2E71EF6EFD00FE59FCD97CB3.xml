<taxonomicName id="BBEB5FAE2E71EF6EFD57FE59FC387FE2" box="[753,909,433,459]" class="Elasmobranchii" kingdom="Animalia" order="Squaliformes" pageId="12" pageNumber="13" phylum="Chordata" rank="order">Squaliformes</taxonomicName>
