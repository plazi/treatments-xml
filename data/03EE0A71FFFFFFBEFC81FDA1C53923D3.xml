<taxonomicName id="4C47C0E4FFFFFFA1FB78F8E6C1972726" authorityName="Pretzmann" authorityYear="1965" box="[1196,1443,1876,1897]" class="Malacostraca" family="Pseudothelphusidae" genus="Phallangothelphusa" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="988" phylum="Arthropoda" rank="genus">
