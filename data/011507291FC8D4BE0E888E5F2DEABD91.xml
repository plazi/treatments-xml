<paragraph id="EC1DBB48FD6AFA584C13403F60E67BAA" pageNumber="642">
Etymology NL 
<taxonomicName id="AF7B998E2DBC7E1E3474DE7155AEFDCB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182001" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole gnomus Wilson" lsidName-HNS="Pheidole gnomus Wilson" order="Hymenoptera" pageNumber="642" phylum="Arthropoda" rank="species" species="gnomus">gnomus</taxonomicName>
