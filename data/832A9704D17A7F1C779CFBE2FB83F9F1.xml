<collectingCountry id="73946682D17A7F1C7538F98BFDEEF9EB" box="[563,634,1549,1570]" name="Guinea-Bissau" pageId="20" pageNumber="230">Guinea</collectingCountry>
, 
<collectingCountry id="73946682D17A7F1C779CF9AEFF76F9F4" box="[151,226,1576,1597]" name="Mexico" pageId="20" pageNumber="230">Mexico</collectingCountry>
, 
<collectingCountry id="73946682D17A7F1C77FDF9AEFECBF9F4" box="[246,351,1576,1597]" name="Guatemala" pageId="20" pageNumber="230">Guatemala</collectingCountry>
, 
<collectingCountry id="73946682D17A7F1C7678F9AEFDD6F9F4" box="[371,578,1576,1597]" name="Dominican Republic" pageId="20" pageNumber="230">Dominican Republic</collectingCountry>
