<paragraph id="8B814667CE00FFDFFF6BC445FDB3FAEC" blockId="38.[151,547,1298,1364]" box="[151,547,1298,1324]" pageId="38" pageNumber="39">
<heading id="D0C9F10BCE00FFDFFF6BC445FDB3FAEC" bold="true" box="[151,547,1298,1324]" fontSize="11" level="1" pageId="38" pageNumber="39" reason="1">
<emphasis id="B94A9A75CE00FFDFFF6BC445FDB3FAEC" bold="true" box="[151,547,1298,1324]" pageId="38" pageNumber="39">
<taxonomicName id="4C3E3DE4CE00FFDFFF6BC445FE2FFAEC" authority="Martin, 2008" authorityName="Martin" authorityYear="2008" box="[151,447,1298,1324]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="pseudugesii" status="sp. nov.">
<emphasis id="B94A9A75CE00FFDFFF6BC445FE2FFAEC" bold="true" box="[151,447,1298,1324]" italics="true" pageId="38" pageNumber="39">Aleurodicus pseudugesii</emphasis>
</taxonomicName>
