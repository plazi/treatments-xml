<emphasis id="C9EEEA83FF9CE231FF529250D7165860" bold="true" box="[199,352,1302,1327]" pageId="16" pageNumber="397">Distribution.</emphasis>
<collectingCountry id="838D7601FF9CE231FEF99250D7AA5860" box="[364,476,1302,1327]" name="Mongolia" pageId="16" pageNumber="397">Mongolia</collectingCountry>
, 
<collectingCountry id="838D7601FF9CE231FE789250D4425860" box="[493,564,1302,1327]" name="China" pageId="16" pageNumber="397">China</collectingCountry>
: 
<collectingRegion id="395EF873FF9CE231FDD19250D4C15860" box="[580,695,1302,1327]" country="China" name="Shandong" pageId="16" pageNumber="397">Shandong</collectingRegion>
, 
<collectingRegion id="395EF873FF9CE231FD529250D48D5860" box="[711,763,1302,1327]" country="China" name="Jilin" pageId="16" pageNumber="397">Jilin</collectingRegion>
; 
<collectingCountry id="838D7601FF9CE231FC9E9250D52C5860" box="[779,858,1302,1327]" name="Russia" pageId="16" pageNumber="397">Russia</collectingCountry>
