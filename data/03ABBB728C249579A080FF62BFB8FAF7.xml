<taxonomicName id="4C0271E78C24957AA0E8FAAFBDBFFAB6" authority="GRIMMER 1841" authorityName="GRIMMER" authorityYear="1841" box="[217,494,1301,1321]" class="Insecta" family="Staphylinidae" genus="Oxytelus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="808" phylum="Arthropoda" rank="species" species="pallipennis">
