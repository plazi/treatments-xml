<taxonomicName id="520C5652BE5B758D15292FBD3339DF9D" authorityName="Graham" authorityYear="1956" class="Insecta" family="Asteraceae" genus="Rhicnocoelia" kingdom="Animalia" lsidName="" order="Diptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="family">Asteraceae</taxonomicName>
(
<taxonomicName id="15B0D3FBC05CAEACBB1AD76303B770C8" class="Magnoliopsida" family="Euphorbiaceae" genus="Onopordum" kingdom="Plantae" lsidName="Onopordum nervosum" order="Malpighiales" pageId="0" pageNumber="77092" phylum="Tracheophyta" rank="species" species="nervosum">
<emphasis id="12B90BA4027DF7398EEC365CF2A02062" italics="true" pageId="0" pageNumber="77092">Onopordum nervosum</emphasis>
</taxonomicName>
Boiss.), 
<taxonomicName id="5D972510E2239916ABE9258C906C1E08" class="Magnoliopsida" family="Euphorbiaceae" genus="Onopordum" kingdom="Plantae" lsidName="" order="Malpighiales" pageId="0" pageNumber="77092" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
