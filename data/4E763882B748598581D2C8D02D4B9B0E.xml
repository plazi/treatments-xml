<taxonomicName id="199AF19DDD2A5606AF25200233E73A74" class="Magnoliopsida" family="Tamaricaceae" genus="Triticum" kingdom="Plantae" lsidName="Triticum" order="Caryophyllales" pageId="0" pageNumber="77092" phylum="Tracheophyta" rank="genus">
