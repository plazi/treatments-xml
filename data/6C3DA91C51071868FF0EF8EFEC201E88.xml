<taxonomicName id="239463895106186BFEEEF8B6ED191B06" authorityName="Ho" authorityYear="2019" baseAuthorityName="Fabricius" baseAuthorityYear="1801" box="[375,545,1833,1857]" class="Insecta" family="Lampyridae" genus="Atyphella" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="striata" status="comb. nov.">
