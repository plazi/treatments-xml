<taxonomicName id="34914D12E25EFFEFFDA336DF8E9AF89E" box="[591,687,1773,1796]" class="Amphibia" family="Bufonidae" genus="Werneria" kingdom="Animalia" order="Anura" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">
