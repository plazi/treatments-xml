<taxonomicName id="4C690F86FFCDFFFBDB48015E97C835A1" authorityName="Johanson &amp; Mary &amp; Sjöberg &amp; Malm" authorityYear="2020" box="[584,648,873,899]" class="Insecta" family="Leptoceridae" genus="Oecetis" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="species" species="millei">
