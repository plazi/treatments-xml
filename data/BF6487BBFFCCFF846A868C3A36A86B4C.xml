<paragraph id="377236ADFFCDFF866B1F8F21378F6E55" blockId="13.[92,1046,902,1349]" pageId="13" pageNumber="2018">
<taxonomicName id="F0CD4D2EFFCDFF866B1F8F2136FC6E3A" authorityName="Brindle" authorityYear="1971" box="[92,196,1300,1322]" class="Insecta" family="Spongiphoridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dermaptera" pageId="13" pageNumber="2018" phylum="Arthropoda" rank="subFamily" subFamily="Geracinae">Geracinae</taxonomicName>
