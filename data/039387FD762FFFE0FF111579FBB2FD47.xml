2. 
<taxonomicName id="4C3A4D687628FFE1FE9F119EFDF9F869" authority="Park &amp; Byun" authorityName="Park &amp; Byun" authorityYear="2021" box="[281,628,1938,1963]" class="Insecta" family="Gelechiidae" genus="Encolapta" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="najuensis" status="sp. nov.">
