<quantity id="0407B587FFA57757FAD0F5D6FA6E439F" box="[1347,1416,1765,1789]" metricMagnitude="-1" metricUnit="m" metricValue="1.9" pageId="15" pageNumber="304" unit="cm" value="19.0">19 cm</quantity>
