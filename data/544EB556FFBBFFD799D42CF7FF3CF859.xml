<taxonomicName id="1BE77FC3FFBBFFD798162E28FDA6F811" authorityName="- Eglitis" authorityYear="1956" box="[341,634,1911,1937]" class="Arachnida" family="Eriophyidae" genus="Eriophyes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="17" pageNumber="228" phylum="Arthropoda" rank="species" species="macrorhynchus">
<emphasis id="EE93D852FFBBFFD798162E28FDA6F811" box="[341,634,1911,1937]" italics="true" pageId="17" pageNumber="228">Eriophyes macrorhynchus</emphasis>
</taxonomicName>
