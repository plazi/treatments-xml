<taxonomicName id="1BE77FC3FFBBFFD798C32EC3FD91F835" box="[384,589,1947,1974]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="17" pageNumber="228" phylum="Tracheophyta" rank="species" species="saccharinum">
<emphasis id="EE93D852FFBBFFD798C32EC3FD91F835" box="[384,589,1947,1974]" italics="true" pageId="17" pageNumber="228">Acer saccharinum</emphasis>
</taxonomicName>
were 
<taxonomicName id="1BE77FC3FFBBFFD79BD32EC3FBCEF835" authority="Shimer, 1869" authorityName="Shimer" authorityYear="1869" box="[656,1042,1947,1973]" class="Arachnida" family="Eriophyidae" genus="Vasates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="17" pageNumber="228" phylum="Arthropoda" rank="species" species="quadripedes">
<emphasis id="EE93D852FFBBFFD79BD32EC3FCA9F835" box="[656,885,1947,1973]" italics="true" pageId="17" pageNumber="228">Vasates quadripedes</emphasis>
Shimer, 1869
</taxonomicName>
not 
<taxonomicName id="1BE77FC3FFBBFFD79D002EC3FF00F859" authority="(Nalepa, 1889)" authorityName="Nalepa" authorityYear="1889" baseAuthorityName="Nalepa" baseAuthorityYear="1889" class="Arachnida" family="Eriophyidae" genus="Aceria" kingdom="Animalia" order="Prostigmata" pageId="17" pageNumber="228" phylum="Arthropoda" rank="species" species="macrorhyncha">
<emphasis id="EE93D852FFBBFFD79D002EC3FAEAF835" box="[1091,1334,1947,1974]" italics="true" pageId="17" pageNumber="228">Aceria macrorhyncha</emphasis>
(Nalepa, 1889)
</taxonomicName>
.
</paragraph>
