<taxonomicName id="4C302EEDFF92FF8EFEC31E61FD9FF884" authority="Blahnik &amp; Holzenthal, 2008" authorityName="Blahnik &amp; Holzenthal" authorityYear="2008" box="[302,525,1813,1836]" class="Insecta" family="Glossosomatidae" genus="Mortoniella" kingdom="Animalia" order="Trichoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="rodmani" status="new species">
<emphasis id="B944897CFF92FF8EFEC31E61FD9FF884" box="[302,525,1813,1836]" italics="true" pageId="52" pageNumber="53">Mortoniella rodmani</emphasis>
</taxonomicName>
, 
