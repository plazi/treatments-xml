<number id="495C806358460B6B873AF96FFB17F968" box="[1105,1171,1724,1750]" pageId="9" pageNumber="56" value="0.032">0.032</number>
(n= 
<number id="495C806358460B6B87A3F96FFB53F968" box="[1224,1239,1724,1750]" pageId="9" pageNumber="56" value="6.0">6</number>
); short lateral marginal seta of last segment 
<number id="495C806358460B6B81D1F937FD78F940" box="[698,764,1764,1790]" pageId="9" pageNumber="56" value="0.042">0.042</number>
(n= 
<number id="495C806358460B6B805BF937FCBBF940" box="[816,831,1764,1790]" pageId="9" pageNumber="56" value="6.0">6</number>
). 
<emphasis id="CDE04E5658460B6B803CF937FC65F943" box="[855,993,1764,1789]" italics="true" pageId="9" pageNumber="56">Dimensions</emphasis>
