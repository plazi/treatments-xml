<taxonomicName id="B88F4D71FFA4FFFEFD8FFB0DFC9F48C4" authority="(Harold, 1879)" authorityName="Harold" authorityYear="1879" baseAuthorityName="Harold" baseAuthorityYear="1879" box="[583,873,1230,1255]" class="Insecta" family="Meloidae" genus="Ceroctis" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="interna">
<emphasis id="4DFBEAE0FFA4FFFEFD8FFB0DFD4E48C5" box="[583,696,1231,1254]" italics="true" pageId="51" pageNumber="52">C. interna</emphasis>
