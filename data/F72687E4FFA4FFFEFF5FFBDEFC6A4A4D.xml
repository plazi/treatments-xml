<taxonomicName id="B88F4D71FFA4FFFEFF2FFB0DFDCD48C4" authority="(Duvivier, 1890)" authorityName="Duvivier" authorityYear="1890" baseAuthorityName="Duvivier" baseAuthorityYear="1890" box="[231,571,1230,1255]" class="Insecta" family="Meloidae" genus="Ceroctis" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="congoana">
<emphasis id="4DFBEAE0FFA4FFFEFF2FFB0DFE8348C5" box="[231,373,1231,1254]" italics="true" pageId="51" pageNumber="52">C. congoana</emphasis>
