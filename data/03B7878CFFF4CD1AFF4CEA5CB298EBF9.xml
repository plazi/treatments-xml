<quantity id="4CE69B7FFFF4CD1DFACCEC3BB41FED2F" box="[1303,1388,1102,1126]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="35" pageNumber="36" unit="m" value="2000.0">2000 m</quantity>
