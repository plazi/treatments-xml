<caption id="A3E167F9FFC7DB01FF6F8207DF79FA89" pageId="13" pageNumber="54" startId="13.[151,263,1281,1304]" targetBox="[258,1319,221,1235]" targetPageId="13">
<paragraph id="F7213771FFC7DB01FF6F8207DF79FA89" blockId="13.[151,1436,1281,1334]" pageId="13" pageNumber="54">
<emphasis id="C5EAEB63FFC7DB01FF6F8207DC52FAA7" bold="true" box="[151,341,1281,1304]" pageId="13" pageNumber="54">FIGURES 34–38.</emphasis>
<taxonomicName id="309E4CF2FFC7DB01FE9B8204DF27FAA8" authorityName="Henry &amp; Carpintero" authorityYear="2012" box="[355,544,1281,1303]" class="Insecta" family="Miridae" genus="Myiomma" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="54" phylum="Arthropoda" rank="species" species="pallipes">
<emphasis id="C5EAEB63FFC7DB01FE9B8204DF27FAA8" box="[355,544,1281,1303]" italics="true" pageId="13" pageNumber="54">Myiomma pallipes</emphasis>
