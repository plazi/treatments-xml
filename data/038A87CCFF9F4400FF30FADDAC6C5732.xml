<typeStatus id="54988878FF9F4401FF30FA98A89655BA" box="[151,201,1399,1423]" pageId="4" pageNumber="5">Type</typeStatus>
Species: 
<taxonomicName id="4C234D59FF9F4401FE8CFA98AA9D55BA" authority="Tonnoir 1925: 234" authorityName="Tonnoir" authorityPageNumber="234" authorityYear="1925" box="[299,706,1398,1423]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="aurantiaca">
