<heading id="7F418184FFFEFF9CFF61FD12FD01B427" bold="true" box="[151,699,762,788]" fontSize="11" level="1" pageId="26" pageNumber="477" reason="1">
<taxonomicName id="E3B64D6BFFFEFF9CFF61FD12FD01B427" authority="(Fabricius, 1798)" baseAuthorityName="Fabricius" baseAuthorityYear="1798" box="[151,699,762,788]" class="Insecta" family="Cynipidae" genus="Plagiotrochus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="477" phylum="Arthropoda" rank="species" species="quercusilicis">
