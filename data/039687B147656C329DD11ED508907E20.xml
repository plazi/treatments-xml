<collectingCountry id="F328763747676C329C211D2508A179F8" box="[359,426,871,897]" name="China" pageId="27" pageNumber="28">China</collectingCountry>
(
<collectingRegion id="49FBF84547676C329CFF1D250B1579F8" box="[441,542,871,897]" country="China" name="Guizhou" pageId="27" pageNumber="28">Guizhou</collectingRegion>
, 
<collectingRegion id="49FBF84547676C329F6F1D250B8D79F8" box="[553,646,871,897]" country="China" name="Sichuan" pageId="27" pageNumber="28">Sichuan</collectingRegion>
, 
<collectingRegion id="49FBF84547676C329FD61D250BE279F8" box="[656,745,871,897]" country="China" name="Yunnan" pageId="27" pageNumber="28">Yunnan</collectingRegion>
, 
<collectingRegion id="49FBF84547676C329FB21D250A5679FB" box="[756,861,871,898]" country="China" name="Zhejiang" pageId="27" pageNumber="28">Zhejiang</collectingRegion>
