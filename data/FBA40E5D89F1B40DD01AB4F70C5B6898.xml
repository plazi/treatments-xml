1. 
<normalizedToken id="D6985EE6C90E29D839375279C12E7FAD" originalValue="Blütenköpfe" pageId="null" pageNumber="496">Bluetenkoepfe</normalizedToken>
ohne 
<normalizedToken id="8DE1574E5877F0114F0667A5F1509E2F" originalValue="zungenförmige" pageId="null" pageNumber="496">zungenfoermige</normalizedToken>
<normalizedToken id="FBE26BC0943B0B0F1033960355FFE14C" originalValue="Blüten" pageId="null" pageNumber="496">Blueten</normalizedToken>
