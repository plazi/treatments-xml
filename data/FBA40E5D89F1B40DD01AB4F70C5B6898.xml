<normalizedToken id="39169F61E32CA645C42E619D049A390C" originalValue="äußere" pageId="null" pageNumber="496">aeussere</normalizedToken>
<normalizedToken id="E0973915F344B2F5D549DA9DF539CB7C" originalValue="Hüllblätter" pageId="null" pageNumber="496">Huellblaetter</normalizedToken>
