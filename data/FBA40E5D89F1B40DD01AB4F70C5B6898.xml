<normalizedToken id="6A62A810A2AB06EF58C7C6DE041E0042" originalValue="Blätter" pageId="null" pageNumber="496">Blaetter</normalizedToken>
<normalizedToken id="96620895352575CA9C0286BC7073631A" originalValue="kürzer" pageId="null" pageNumber="496">kuerzer</normalizedToken>
