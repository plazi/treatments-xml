<taxonomicName id="89EBD881A411FFE5FAC6DE4CFE92FD38" authority="Fiori" authorityName="Fiori" class="Insecta" family="Staphylinidae" genus="Staphylinus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="subSpecies" species="strumosus" subSpecies="siculus">
