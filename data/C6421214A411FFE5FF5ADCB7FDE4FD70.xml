<taxonomicName id="89EBD881A411FFE5FD17DEB0FC6AFD38" authority="Eppelsheim" authorityName="Eppelsheim" box="[730,975,654,679]" class="Insecta" family="Staphylinidae" genus="Lomechusoides" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="species" species="teres">
