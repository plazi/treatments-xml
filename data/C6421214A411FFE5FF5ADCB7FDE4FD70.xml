<taxonomicName id="89EBD881A411FFE5FE8EDEB0FD6AFD38" authority="Fabricius" authorityName="Fabricius" box="[323,719,654,679]" class="Insecta" family="Staphylinidae" genus="Staphylinus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="subSpecies" species="strumosus" subSpecies="strumosus">
