<collectingCountry id="32807602FFE9FF995E069E6B2BCC4E22" box="[353,406,323,349]" name="United States of America" pageId="56" pageNumber="55">USA</collectingCountry>
(
<collectingRegion id="8853F870FFE9FF995EC39E6B28594E21" box="[420,515,323,350]" country="United States of America" name="Arizona" pageId="56" pageNumber="55">Arizona</collectingRegion>
