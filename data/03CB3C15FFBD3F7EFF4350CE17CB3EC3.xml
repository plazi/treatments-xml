<number id="3DAA9F24FFBD3F7EFEBD511C1325385B" box="[361,373,867,889]" pageId="5" pageNumber="396" value="9.0">9</number>
greatly enlarged relative to 
<number id="3DAA9F24FFBD3F7EFD56511C10CA385B" box="[642,666,867,889]" pageId="5" pageNumber="396" value="10.0">10</number>
&amp; 
<number id="3DAA9F24FFBD3F7EFD6D511C1080385B" box="[697,720,867,889]" pageId="5" pageNumber="396" value="11.0">11</number>
.................................. 
<taxonomicName id="4C62F680FFBD3F7EFBBD511C17CC385B" authority="Motschulsky" authorityName="Motschulsky" box="[1129,1436,867,889]" class="Insecta" family="Endomychidae" genus="Phymaphoroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="396" phylum="Arthropoda" rank="genus">
<emphasis id="B9165111FFBD3F7EFBBD511C17CC385B" bold="true" box="[1129,1436,867,889]" pageId="5" pageNumber="396">
