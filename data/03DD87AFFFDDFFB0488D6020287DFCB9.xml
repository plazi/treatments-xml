<taxonomicName id="4C744D3AFFDDFFB04A2D60B329CBFC9D" box="[567,681,761,786]" class="Polychaeta" family="Serpulidae" genus="Pileolaria" kingdom="Animalia" order="Sabellida" pageId="102" pageNumber="103" phylum="Annelida" rank="genus">
<emphasis id="B900EAABFFDDFFB04A2D60B329CBFC9D" box="[567,681,761,786]" italics="true" pageId="102" pageNumber="103">Pileolaria</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C744D3AFFDDFFB04AA760B32882FC9D" authority="Simplara" authorityName="Simplara" box="[701,992,761,786]" class="Polychaeta" family="Serpulidae" genus="Protolaeospira" kingdom="Animalia" order="Sabellida" pageId="102" pageNumber="103" phylum="Annelida" rank="genus">
<emphasis id="B900EAABFFDDFFB04AA760B32807FC9D" box="[701,869,761,786]" italics="true" pageId="102" pageNumber="103">Protolaeospira</emphasis>
, 
<emphasis id="B900EAABFFDDFFB04B6060B32882FC9D" box="[890,992,761,786]" italics="true" pageId="102" pageNumber="103">Simplara</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C744D3AFFDDFFB04BEC60B32F3FFC9D" box="[1014,1117,761,786]" class="Polychaeta" family="Serpulidae" genus="Spirorbis" kingdom="Animalia" order="Sabellida" pageId="102" pageNumber="103" phylum="Annelida" rank="genus">
<emphasis id="B900EAABFFDDFFB04BEC60B32F3FFC9D" box="[1014,1117,761,786]" italics="true" pageId="102" pageNumber="103">Spirorbis</emphasis>
