<taxonomicName id="4C04778C5C0BFFD74F74A388FEAEF830" authority="Kasparyan &amp; Kopelke, 2010" authorityName="Kasparyan &amp; Kopelke" authorityYear="2010" class="Insecta" family="Ichneumonidae" genus="Saotis" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="granulator">
<emphasis id="B970D01D5C0BFFD74F74A388FB0FF81C" box="[1068,1228,1965,1992]" italics="true" pageId="3" pageNumber="4">S. granulator</emphasis>
