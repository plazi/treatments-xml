<taxonomicName id="4C334D5CFFAAFFB5FC52FDAAFB98FDEE" authority="BLAGODEROV &amp; GRIMALDI, 2004" authorityName="BLAGODEROV &amp; GRIMALDI" authorityYear="2004" box="[994,1081,531,554]" class="Insecta" family="Mycetophilidae" genus="Izleiina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus" status="n.gen.">
