<paragraph id="1D0F895EFFC6FFFEFF5EFAA3E7E915A0" blockId="29.[162,693,1401,1423]" box="[162,693,1401,1423]" pageId="29" pageNumber="30">
<emphasis id="2FC4554CFFC6FFFEFF5EFAA3E47015A1" box="[162,300,1402,1423]" italics="true" pageId="29" pageNumber="30">
<typeStatus id="C20B37FCFFC6FFFEFF5EFAA3E58B15A1" box="[162,215,1402,1423]" pageId="29" pageNumber="30">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="DAB0F2DDFFC6FFFEFEC7FAA3E7E915A0" authority="Hale, 1944" authorityName="Hale" authorityYear="1944" box="[315,693,1401,1423]" class="Malacostraca" family="Bodotriidae" genus="Pomacuma" kingdom="Animalia" order="Cumacea" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="cognatum">
