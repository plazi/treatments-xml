<taxonomicName id="7FE54D58FF84FF9CFCECB2E2FC46C0B2" authorityName="Fernald" authorityYear="1903" box="[848,983,1963,1988]" class="Insecta" family="Margarodidae" genus="Gueriniella" kingdom="Animalia" order="Hemiptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="genus">
<emphasis id="8A91EAC9FF84FF9CFCECB2E2FC46C0B2" box="[848,983,1963,1988]" italics="true" pageId="66" pageNumber="67">Gueriniella</emphasis>
</taxonomicName>
