<taxonomicName id="419831FBFFD65861FB13FE92FA86FEB5" box="[1191,1307,259,283]" class="Polychaeta" family="Serpulidae" genus="Spirobranchus" kingdom="Animalia" order="Sabellida" pageId="23" pageNumber="24" phylum="Annelida" rank="species" species="kraussii">
