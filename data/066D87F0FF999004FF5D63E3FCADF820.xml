<taxonomicName id="49C44D65FF999004FAC4611FFA24F8D9" box="[1294,1429,1861,1885]" class="Insecta" family="Phoridae" genus="Coniceromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="bilineata">
