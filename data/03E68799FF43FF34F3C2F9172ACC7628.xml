<taxonomicName id="4C4F4D0CFF42FF34F1F5FE212D0175CB" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[672,777,476,500]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="161" pageNumber="162" phylum="Arthropoda" rank="species" species="doddi" status="sp. nov.">
