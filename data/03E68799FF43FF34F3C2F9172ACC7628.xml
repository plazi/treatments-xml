<taxonomicName id="4C4F4D0CFF42FF34F212FE212C2B75CB" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[327,547,476,500]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="161" pageNumber="162" phylum="Arthropoda" rank="species" species="joycei">
