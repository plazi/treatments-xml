, 
<emphasis id="7DD46F43CE37E269D68A1463FDF82E11" box="[457,617,343,368]" italics="true" pageId="2" pageNumber="183">nomen novum</emphasis>
pro 
<taxonomicName id="88A0C8D2CE37E269D5EB1463FC072E10" authority="Menge, 1869" authorityName="Menge" authorityYear="1869" box="[680,918,343,369]" class="Liliopsida" family="Iridaceae" genus="Lethia" kingdom="Plantae" order="Asparagales" pageId="2" pageNumber="183" phylum="Tracheophyta" rank="genus">
