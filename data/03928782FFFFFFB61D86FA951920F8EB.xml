<taxonomicName id="4C3B4D17FFFFFFB61E4CF8E11FF2F8E8" authorityName="Foerster" authorityYear="1848" box="[861,1017,1838,1861]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="remota">
