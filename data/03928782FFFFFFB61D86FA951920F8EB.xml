Here, 
<taxonomicName id="4C3B4D17FFFFFFB61C1AF8E11DF0F8EB" baseAuthorityName="Erdos" baseAuthorityYear="1957" box="[267,507,1837,1862]" class="Insecta" family="Encyrtidae" genus="Microterys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="annulatus">
