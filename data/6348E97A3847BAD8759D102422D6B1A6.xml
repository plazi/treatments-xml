<subSubSection id="9CFBDA684EC868867B2093BB1DCCE383" pageId="null" pageNumber="41" type="nomenclature">
<paragraph id="F8E8360544A8866750D639E9D0F5B608" pageId="null" pageNumber="41">
<taxonomicName id="9E41D17F5A8B08B339A47189FB1AB1C2" authority="L." class="Magnoliopsida" family="Ranunculaceae" genus="Caltha" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="41" phylum="Tracheophyta" rank="species" species="palustris">
