<uri id="FFF53AD7FF88177AFEAAFCA3ED7E935C" box="[366,1120,853,877]" pageId="35" pageNumber="2023">
urn:lsid:zoobank.org:act: 
<uuid id="FFC20C00FF88177AFDA9FCA3ED7E935C" box="[621,1120,853,877]" pageId="35" pageNumber="2023">072E3AFF-31E4-404B-B904-25BB58BBF335</uuid>
