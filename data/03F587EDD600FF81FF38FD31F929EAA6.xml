<taxonomicName id="4C5C4D78D601FF81F80AFE0DF734E9BD" box="[1934,2245,399,437]" class="Arachnida" family="Liocranidae" genus="Agroeca" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="proxima">
