<taxonomicName id="4C5C4D78D601FF81F823FE70FA02EA42" authority="Kulczynski" authorityName="Kulczynski" class="Arachnida" family="Liocranidae" genus="Agraecina" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="striata">
