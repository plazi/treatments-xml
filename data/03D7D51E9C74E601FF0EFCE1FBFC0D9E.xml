<taxonomicName id="4C7E1F8B9C74E601FD17FA38FCAC0D9E" authorityName="Schmidt &amp; Anweiler" authorityYear="2010" box="[661,771,1434,1460]" class="Insecta" family="Pantheidae" genus="Charadra" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="179" phylum="Arthropoda" rank="species" species="moneta">
<emphasis id="B90AB81A9C74E601FD17FA38FCAC0D9E" box="[661,771,1434,1460]" italics="true" pageId="18" pageNumber="179">C. moneta</emphasis>
