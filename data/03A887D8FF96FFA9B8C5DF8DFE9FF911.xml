<treatmentCitationGroup id="AB1111E0FF96FFA9B98FDC10FD5EFDEA" box="[144,682,553,577]" pageId="23" pageNumber="296">
<treatmentCitation id="0AA010DFFF96FFA9B98FDC10FE1AFDEA" author="Huber BA" box="[144,494,553,577]" page="321" pageId="23" pageNumber="296" year="2000">
