<taxonomicName id="8FEE4D41FFA4FF8FFF67F86560DFCC9A" authority="Ash, Elliot, Godfrey, Burmej, Abdad, Northover, Wayne, Morris, Clode, Lymbery and Thompson, 2017" authorityName="Ash, Elliot, Godfrey, Burmej, Abdad, Northover, Wayne, Morris, Clode, Lymbery and Thompson" authorityYear="2017" box="[247,1436,1986,2013]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="woyliei">
