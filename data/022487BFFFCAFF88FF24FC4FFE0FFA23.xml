<taxonomicName id="4D8D4D2AFFCAFF88FC04FB66FB96FB43" authorityName="Ausserer" authorityYear="1871" box="[951,1056,1159,1185]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
