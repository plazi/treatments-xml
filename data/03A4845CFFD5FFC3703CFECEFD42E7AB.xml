<taxonomicName id="4C0D4EC9FFD5FFC37039FE89FCE1E479" authority="Reuter, 1871" authorityName="Reuter" authorityYear="1871" box="[518,797,348,367]" class="Insecta" family="Anthocoridae" genus="Calliodis" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="picturatus">
