<collectingCountry id="F30B1789FFA7FF8ACABEF8497F07F83D" box="[701,789,1975,1999]" name="Finland" pageId="7" pageNumber="47">Finland</collectingCountry>
, 
<collectingCountry id="F30B1789FFA7FF8ACB24F8497F49F83D" box="[807,859,1975,1999]" name="Italy" pageId="7" pageNumber="47">Italy</collectingCountry>
, 
<collectingCountry id="F30B1789FFA7FF8ACB6FF8497FC6F83D" box="[876,980,1975,1999]" name="Romania" pageId="7" pageNumber="47">Romania</collectingCountry>
, 
<collectingCountry id="F30B1789FFA7FF8ACBE6F8497835F83D" box="[997,1063,1975,1999]" name="Spain" pageId="7" pageNumber="47">Spain</collectingCountry>
, 
<collectingCountry id="F30B1789FFA7FF8ACC3AF8497880F83D" box="[1081,1170,1975,1999]" name="Sweden" pageId="7" pageNumber="47">Sweden</collectingCountry>
and Russian Far East (
<bibRefCitation id="EF8D2AE8FFA7FF8AC89DF8247D80F800" author="Saether" box="[158,402,2010,2034]" pageId="7" pageNumber="48" refString="Saether, O. A. &amp; Spies, M. (2004) Fauna Europaea: Chironomidae. Fauna Europaea version 1.1. Available from: htpp: // www. faunaeur. org." type="book" year="2004">
Saether &amp; Spies 
<number id="3DD4453EFFA7FF8AC956F8247D80F800" box="[341,402,2010,2034]" pageId="7" pageNumber="47" value="2004.0">2004</number>
