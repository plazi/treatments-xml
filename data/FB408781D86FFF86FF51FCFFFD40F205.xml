22216, 8: 
<quantity id="B4119B72D86DFF86FB0FFC3CFA86F7E5" box="[1203,1323,989,1012]" metricMagnitude="-2" metricUnit="m" metricValue="5.9" metricValueMax="8.2" metricValueMin="3.6" pageId="7" pageNumber="288" unit="mm" value="59.0" valueMax="82.0" valueMin="36.0">36–82 mm</quantity>
