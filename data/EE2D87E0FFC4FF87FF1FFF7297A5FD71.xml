<taxonomicName id="A1844D75FFC4FF87FE1CFD4C9035FD71" box="[404,516,681,704]" class="Eutardigrada" family="Hypsibiidae" genus="Platicrista" kingdom="Animalia" order="Parachela" pageId="48" pageNumber="49" phylum="Tardigrada" rank="genus">
