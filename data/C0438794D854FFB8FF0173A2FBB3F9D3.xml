<taxonomicName id="8FEA4D01D855FFB8FDE670CCFCA4F8D7" authorityName="Shen &amp; Leschen" authorityYear="2020" box="[624,768,1731,1757]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="10" phylum="Arthropoda" rank="species" species="petila" subGenus="Byraxis">
