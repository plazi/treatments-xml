<taxonomicName id="7A9E4DE5FFB60D72CC1BF9E80672324B" authority="(Barraud)" authorityName="Reinert &amp; Harbach &amp; Kitching" authorityYear="2006" baseAuthorityName="Reinert &amp; Harbach &amp; Kitching" baseAuthorityYear="2006" box="[1074,1325,1560,1582]" class="Insecta" family="Culicidae" genus="Collessius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="shortti">
