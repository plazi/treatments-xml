<taxonomicName id="7A9E4DE5FFB60D72CC00FA2B06443194" authority="(Yamada)" authorityName="Reinert &amp; Harbach &amp; Kitching" authorityYear="2006" baseAuthorityName="Yamada" baseAuthorityYear="1921" box="[1065,1307,1499,1521]" class="Insecta" family="Culicidae" genus="Collessius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="hatorii">
<emphasis id="8FEAEA74FFB60D72CC00FA2B07FC3195" box="[1065,1187,1499,1520]" italics="true" pageId="64" pageNumber="65">Co. hatorii</emphasis>
