<taxonomicName id="7A9E4DE5FFB60D72CD43FA0A00B1324B" authority="(Lei)" authorityName="Reinert &amp; Harbach &amp; Kitching" authorityYear="2006" baseAuthorityName="Reinert &amp; Harbach &amp; Kitching" baseAuthorityYear="2006" class="Insecta" family="Culicidae" genus="Collessius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="ningheensis">
<emphasis id="8FEAEA74FFB60D72CD43FA0A00F33248" italics="true" pageId="64" pageNumber="65">Co. ningheensis</emphasis>
