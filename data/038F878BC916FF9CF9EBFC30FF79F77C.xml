<taxonomicName id="4C264D1EC916FF9CF850FBF4FD15F65F" authority="Pallas, 1766" authorityName="Pallas" authorityYear="1766" box="[300,652,1068,1093]" class="Anthozoa" family="Antipathidae" genus="Antipathes" kingdom="Animalia" order="Antipatharia" pageId="5" pageNumber="6" phylum="Cnidaria" rank="species" species="dichotoma">
