<elevation id="0048D1C0F419FF81FB332E60FAE39C21" box="[1225,1294,1120,1144]" metricMagnitude="2" metricUnit="m" metricValue="2.04" pageId="6" pageNumber="89" unit="m" value="204.0">204 m</elevation>
