; head 
<quantity id="4CB89ED7328107E8746347C7FC0BA250" box="[848,942,1524,1548]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="129" pageNumber="122" unit="mm" value="0.3">0.30 mm</quantity>
in length and 
<quantity id="4CB89ED7328107E877424427FF6BA270" box="[113,206,1556,1580]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="129" pageNumber="122" unit="mm" value="0.6">0.60 mm</quantity>
