<quantity id="4CB89ED7328107E876974427FDA4A270" box="[420,513,1556,1580]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="129" pageNumber="122" unit="mm" value="0.7">0.70 mm</quantity>
