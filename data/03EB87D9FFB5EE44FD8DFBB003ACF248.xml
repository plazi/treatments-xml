., 
<quantity id="4CBA9B2AFFB5EE44FD28FB710552F26C" box="[647,859,1242,1283]" metricMagnitude="3" metricUnit="m" metricValue="1.15" metricValueMax="1.4" metricValueMin="0.9" pageId="6" pageNumber="55" unit="m" value="1150.0" valueMax="1400.0" valueMin="900.0">900-1400 m</quantity>
