<taxonomicName id="202127FD50157366CD6A0A03F662374F" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia" order="Zingiberales" pageId="212" pageNumber="213" phylum="Tracheophyta" rank="genus">Heliconia</taxonomicName>
sp. (
<taxonomicName id="21742929916B26DE2B7B25F1434D01E9" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="212" pageNumber="213" rank="genus">Heliconiaceae</taxonomicName>
) (Calvert and Calvert 1909); 
<taxonomicName id="C426D9999E542FDD43E3EFE4F2BE3BF6" class="Liliopsida" family="Zingiberaceae" genus="Renealmia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Renealmia" order="Zingiberales" pageId="212" pageNumber="213" phylum="Tracheophyta" rank="genus">Renealmia</taxonomicName>
sp. (
<bibRefCitation id="CBF869E13B29072DDD0E8A9205142478" pageId="212" pageNumber="213">Staines 1996</bibRefCitation>
); 
