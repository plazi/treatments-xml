<taxonomicName id="4C176CDCCF0EEB53FD78FD01FCFEB7E1" box="[690,832,644,669]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromalus" kingdom="Animalia" order="Mesostigmata" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="longisetatus">
