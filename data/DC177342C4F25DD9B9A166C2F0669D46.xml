<paragraph id="B25597E123AB061D52E20EA80A2762A8" pageId="0" pageNumber="5">
<taxonomicName id="26E6CBD42853BC515B80C03D18BCF640" LSID="http://zoobank.org/898560E2-5367-4892-8352-EAF65149DD90" authority="Awad" authorityName="Awad" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Synopeas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synopeas pulupulu" order="Hymenoptera" pageId="0" pageNumber="5" phylum="Arthropoda" rank="species" species="pulupulu" status="sp. nov.">Synopeas pulupulu Awad</taxonomicName>
