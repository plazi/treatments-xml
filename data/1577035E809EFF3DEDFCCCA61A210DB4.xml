<quantity id="5A261FAD809EFF3DEF10CD861B8D0DBB" box="[635,720,943,968]" metricMagnitude="1" metricUnit="m" metricValue="2.05" metricValueMax="3.6" metricValueMin="0.5" pageId="154" pageNumber="155" unit="m" value="20.5" valueMax="36.0" valueMin="5.0">5–36 m</quantity>
