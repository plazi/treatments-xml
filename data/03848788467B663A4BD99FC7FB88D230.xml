<taxonomicName id="4C2D4D1D467B663A4BD99FC7FDC0D5E3" authority="(Aube, 1833)" baseAuthorityName="Aube" baseAuthorityYear="1833" box="[140,597,905,932]" class="Insecta" family="Staphylinidae" genus="Brachygluta" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="121" phylum="Arthropoda" rank="species" species="abdominalis">
<emphasis id="B959EA8C467B663A4BD99FC7FE23D5E3" bold="true" box="[140,438,905,932]" italics="true" pageId="6" pageNumber="121">Brachygluta abdominalis</emphasis>
