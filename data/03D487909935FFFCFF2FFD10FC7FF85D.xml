<quantity id="4C859B639935FFFDFACFFC9DFF4AFC1F" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="4" pageNumber="56" unit="mm" value="4.2">4.2 mm</quantity>
), EBISCO stn DW 2606, 
