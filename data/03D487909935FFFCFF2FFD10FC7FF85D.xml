<emphasis id="B909EA949935FFFDFF2FFCC5FE0DFCEF" bold="true" box="[151,398,826,852]" pageId="4" pageNumber="56">Material examined.</emphasis>
<specimenCount id="9D7BFD0F9935FFFDFE20FCC5FDADFCEF" box="[408,558,826,852]" pageId="4" pageNumber="56" type="generic">
<number id="3DB524A19935FFFDFE20FCC5FE25FCEF" box="[408,422,826,852]" pageId="4" pageNumber="56" value="3.0">3</number>
