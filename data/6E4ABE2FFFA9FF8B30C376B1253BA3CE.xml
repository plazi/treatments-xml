<collectingMunicipality id="06389543FFA9FF8B310A72672559A363" box="[350,523,1290,1315]" pageId="7" pageNumber="208">South America</collectingMunicipality>
in 
<collectingCountry id="9EF44FA9FFA9FF8B326C726725C5A363" box="[568,663,1290,1315]" name="Ecuador" pageId="7" pageNumber="208">Ecuador</collectingCountry>
