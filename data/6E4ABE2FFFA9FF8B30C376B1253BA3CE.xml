<collectingRegion id="2427C1DBFFA9FF8B340373AE2382A29B" box="[1111,1232,1218,1243]" country="United States of America" name="New York" pageId="7" pageNumber="208">New York</collectingRegion>
south to 
<collectingRegion id="2427C1DBFFA9FF8B351F73AF22CEA29B" box="[1355,1436,1218,1243]" country="United States of America" name="Florida" pageId="7" pageNumber="208">Florida</collectingRegion>
(Borkent &amp; Grogan 2009), the 
<location id="E33C59E2FFA9FF8B31A4738B25E0A2BF" LSID="urn:lsid:plazi:treatment:6E4ABE2FFFA9FF8B30C376B1253BA3CE:E33C59E2FFA9FF8B31A4738B25E0A2BF" box="[496,690,1254,1279]" country="the Bahamas, Bermuda, Jamaica, Haiti, Puerto Rico, Dominica" municipality="South America" name="Caribbean region" pageId="7" pageNumber="208" stateProvince="California">Caribbean region</location>
in 
<collectingCountry id="9EF44FA9FFA9FF8B3281738B22CAA2BF" box="[725,1432,1254,1279]" isEnumeration="true" pageId="7" pageNumber="208">the Bahamas, Bermuda, Jamaica, Haiti, Puerto Rico, Dominica</collectingCountry>
, 
<collectingCountry id="9EF44FA9FFA9FF8B30C3726727ABA363" box="[151,249,1290,1315]" name="Grenada" pageId="7" pageNumber="208">Grenada</collectingCountry>
