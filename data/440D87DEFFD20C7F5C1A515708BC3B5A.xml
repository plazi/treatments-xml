<taxonomicName id="0BA44D4BFFD20C7F5A5B52C6080C3ABC" authorityName="J.Gaertner" authorityYear="1788" box="[1290,1381,648,672]" class="Liliopsida" family="Poaceae" genus="Eleusine" kingdom="Plantae" order="Poales" pageId="8" pageNumber="301" phylum="Tracheophyta" rank="genus">
