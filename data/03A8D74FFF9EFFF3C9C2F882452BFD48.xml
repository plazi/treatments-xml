<taxonomicName id="4C011DDAFF9EFFF2C873F8E94627F82A" authority="Pfitzner, 1906" authorityName="Pfitzner" authorityYear="1906" box="[294,595,1915,1937]" class="Insecta" family="Hepialidae" genus="Triodia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="remota">
