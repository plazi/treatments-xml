<quantity id="C4553D9CFFEBFFF4FB1FFE77FAEEE5F8" box="[1241,1312,406,430]" metricMagnitude="2" metricUnit="m" metricValue="7.029999999999999" pageId="26" pageNumber="87" unit="m" value="703.0">703 m</quantity>
