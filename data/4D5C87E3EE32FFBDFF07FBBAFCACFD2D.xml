<taxonomicName id="02F54D76EE30FFBDFDBCFE2BFD31FE1C" box="[556,688,384,409]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="kaikoura" status="sp. nov.">
<emphasis id="F781EAE7EE30FFBDFDBCFE2BFD31FE1C" box="[556,688,384,409]" italics="true" pageId="15" pageNumber="16">T. kaikoura</emphasis>
</taxonomicName>
<taxonomicNameLabel id="ECB2579CEE30FFBDFD27FED4FC95FE1C" box="[695,788,383,409]" pageId="15" pageNumber="16" rank="species">
<emphasis id="F781EAE7EE30FFBDFD27FED4FC95FE1C" bold="true" box="[695,788,383,409]" pageId="15" pageNumber="16">sp. nov.</emphasis>
</taxonomicNameLabel>
