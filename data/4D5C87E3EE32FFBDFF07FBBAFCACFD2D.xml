<taxonomicName id="02F54D76EE30FFBDFDADFEA7FD1FFEA0" box="[573,670,268,293]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="longa">
<emphasis id="F781EAE7EE30FFBDFDADFEA7FD1FFEA0" box="[573,670,268,293]" italics="true" pageId="15" pageNumber="16">T. longa</emphasis>
</taxonomicName>
, 
<taxonomicName id="02F54D76EE30FFBDFD3FFEA7FCBFFEA0" box="[687,830,268,293]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="stramenti">
<emphasis id="F781EAE7EE30FFBDFD3FFEA7FCBFFEA0" box="[687,830,268,293]" italics="true" pageId="15" pageNumber="16">T. stramenti</emphasis>
</taxonomicName>
and 
<taxonomicName id="02F54D76EE30FFBDFCEAFEA7FC7EFEA0" box="[890,1023,268,293]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="kaikoura" status="sp. nov.">
