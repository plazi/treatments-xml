<taxonomicNameLabel id="ECB2579CEE30FFBDFCBEFF15FC0DFF5D" box="[814,908,190,216]" pageId="15" pageNumber="16" rank="species">
<emphasis id="F781EAE7EE30FFBDFCBEFF15FC0DFF5D" bold="true" box="[814,908,190,216]" pageId="15" pageNumber="16">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="02F54D76EE30FFBDFC0BFF14FBB2FF5D" box="[923,1075,191,216]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="manurewa" status="sp. nov.">
