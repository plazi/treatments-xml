<paragraph id="C54A36F5EE30FFBDFF55FEA7FD19FEF6" blockId="15.[151,1437,151,680]" pageId="15" pageNumber="16">
<taxonomicName id="02F54D76EE30FFBDFF55FEA7FEB7FEA0" box="[197,310,268,293]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="yeatesi" status="sp. nov.">
