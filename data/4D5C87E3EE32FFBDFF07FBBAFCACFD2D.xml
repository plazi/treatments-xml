<taxonomicName id="02F54D76EE30FFBDFDA6FE5FFD3DFD88" box="[566,700,500,525]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="15" pageNumber="16" phylum="Nematoda" rank="species" species="kaikoura" status="sp. nov.">
<emphasis id="F781EAE7EE30FFBDFDA6FE5FFD3DFD88" box="[566,700,500,525]" italics="true" pageId="15" pageNumber="16">T. kaikoura</emphasis>
</taxonomicName>
<taxonomicNameLabel id="ECB2579CEE30FFBDFD54FE58FCA3FD88" box="[708,802,499,525]" pageId="15" pageNumber="16" rank="species">
<emphasis id="F781EAE7EE30FFBDFD54FE58FCA3FD88" bold="true" box="[708,802,499,525]" pageId="15" pageNumber="16">sp. nov.</emphasis>
</taxonomicNameLabel>
in having a narrower body (
<number id="733D24D2EE30FFBDFBE2FE5FFB3CFD8B" box="[1138,1213,500,526]" pageId="15" pageNumber="16" value="52.0" valueMax="55.0" valueMin="49.0">49–55</number>
