<taxonomicName id="4C5F4D25FFAEFFA8FEE5FD84FD49FD9E" authority="(Hoberlandt 1957)" baseAuthorityName="Hoberlandt" baseAuthorityYear="1957" box="[299,735,584,606]" class="Insecta" family="Aradidae" genus="Parabergrothista" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="34" phylum="Arthropoda" rank="species" species="mollis">
