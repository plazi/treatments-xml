<subSubSection id="4FF0F1E8FFDCFF96A7ADFB51FD9FD906" box="[189,601,1214,1288]" pageId="69" pageNumber="70" type="distribution">
<paragraph id="0755A263FFDCFF96A7ADFB51FE90D8D9" blockId="69.[189,342,1214,1240]" box="[189,342,1214,1240]" pageId="69" pageNumber="70">
<heading id="5C1D150FFFDCFF96A7ADFB51FE90D8D9" bold="true" box="[189,342,1214,1240]" fontSize="11" level="3" pageId="69" pageNumber="70" reason="3">
