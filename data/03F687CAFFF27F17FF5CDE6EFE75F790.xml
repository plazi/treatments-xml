(
<figureCitation id="13642A59FFF27F18FF54DE0AFEA7F66E" box="[159,296,862,889]" captionStart="FIGURES 32–42" captionStartId="22.[151,264,1852,1877]" captionTargetBox="[151,1436,181,1826]" captionTargetId="figure@22.[151,1436,181,1826]" captionTargetPageId="22" captionText="FIGURES 32–42. Agaveocoris spp. 32, A. roseus, n. sp., male (dorsal aspect). 33, A. roseus, n. sp., male (lateral aspect). 34, A. roseus, female (dorsal aspect). 35, A. rostratus, n. sp., male (dorsal aspect). 36, A. rostratus, n. sp., male (lateral aspect). 37, A. rostratus, n. sp., female (dorsal aspect). 38, A. schaffneri, n. sp., male (dorsal aspect). 39, A. schaffneri, n. sp., male (lateral aspect). 40, A. schaffneri n. sp., female (dorsal aspect). 41, A. scutellatus n. sp., male (dorsal aspect). 42, A. scutellatus, n. sp., female (dorsal aspect)." figureDoi="http://doi.org/10.5281/zenodo.3816472" httpUri="https://zenodo.org/record/3816472/files/figure.png" pageId="23" pageNumber="224">Figs. 35–37</figureCitation>
, 
<figureCitation id="13642A59FFF27F18FEF8DE0AFEF4F66E" box="[307,379,863,889]" captionStart="FIGURES 80–93" captionStartId="37.[151,264,1910,1935]" captionTargetBox="[176,1416,185,1880]" captionTargetId="figure@37.[168,1420,181,1885]" captionTargetPageId="37" captionText="FIGURES 80–93. Male genitalia of Agaveocoris spp. A. dimidiata, n. sp.: 80, endosoma. 81, left paramere. 82, right paramere. A. distanti, n. sp.: 83, endosoma. 84, left paramere. 85, right paramere. A. marginalis, n. sp.: 86, endosoma. 87, left paramere. 88, right paramere. A. roseus, n. sp.: 89, right paramere. 90, left paramere. A. rostratus, n. sp.: 91, endosoma. 92, left paramere. 93, right paramere." figureDoi="http://doi.org/10.5281/zenodo.3816480" httpUri="https://zenodo.org/record/3816480/files/figure.png" pageId="23" pageNumber="224">91–93</figureCitation>
, 
<figureCitation id="13642A59FFF27F18FE4CDE0AFE3DF66D" box="[391,434,863,890]" captionStart="FIGURES 119–128" captionStartId="45.[151,264,1749,1774]" captionTargetBox="[189,1398,454,1722]" captionTargetId="figure@45.[189,1398,453,1725]" captionTargetPageId="45" captionText="FIGURES 119–128. Male genital tubercles of Agaveocoris spp. 119, A. agavis. 120, A. barberi. 121, A. barrerai, n. sp. 122, A. bimaculatus, n. sp. 123, A. dimidiatus, n. sp. 124, A. distanti. 125, A. roseus, n. sp. 126, A. rostratus, n. sp. 127, A. schaffneri, n. sp. 128, A. scutellatus, n. sp." figureDoi="http://doi.org/10.5281/zenodo.3816492" httpUri="https://zenodo.org/record/3816492/files/figure.png" pageId="23" pageNumber="224">126</figureCitation>
