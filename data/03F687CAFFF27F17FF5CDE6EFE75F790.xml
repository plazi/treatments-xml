<quantity id="4CA79B39FFFD7F17FE5CDC7CFDB3F454" box="[407,572,297,323]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" metricValueMax="6.6" metricValueMin="6.4" pageId="24" pageNumber="225" unit="mm" value="0.65" valueMax="0.66" valueMin="0.64">0.64–0.66 mm</quantity>
