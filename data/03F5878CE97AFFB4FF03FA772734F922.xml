<heading id="D0AB81F6E97AFFB7FF03FA7727F2F89E" bold="true" box="[151,669,1447,1472]" fontSize="10" level="2" pageId="9" pageNumber="45" reason="2">
<emphasis id="B928EA88E97AFFB7FF03FA7727F2F89E" bold="true" box="[151,669,1447,1472]" pageId="9" pageNumber="45">
<taxonomicName id="4C5C4D19E97AFFB7FF03FA772758F89E" authority="Khalaim &amp; Broad" authorityName="Khalaim &amp; Broad" box="[151,567,1447,1472]" class="Insecta" family="Ichneumonidae" genus="Allophrys" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="45" phylum="Arthropoda" rank="species" species="noyesi" status="sp. nov.">
<emphasis id="B928EA88E97AFFB7FF03FA772438F89E" bold="true" box="[151,343,1447,1472]" italics="true" pageId="9" pageNumber="45">Allophrys noyesi</emphasis>
Khalaim &amp; Broad
</taxonomicName>
, 
<taxonomicNameLabel id="A21B57F3E97AFFB7FDD7FA7827F2F89E" box="[579,669,1448,1472]" pageId="9" pageNumber="45" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
