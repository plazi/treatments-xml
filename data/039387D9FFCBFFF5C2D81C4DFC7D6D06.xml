, 
<taxonomicName id="4C3A4D4CFFCFFFF6C1811EACFC096FD6" authorityName="Huber &amp; Benjamin" authorityYear="2005" box="[754,960,941,960]" class="Arachnida" family="Pholcidae" genus="Wanniyala" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="3316" phylum="Arthropoda" rank="species" species="agrabopath">
<emphasis id="B94EEADDFFCFFFF6C1811EACFC096FD6" box="[754,960,941,960]" italics="true" pageId="13" pageNumber="3316">Wanniyala agrabopath</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3A4D4CFFCFFFF6C0A21EACFBF76FD6" authorityName="Huber &amp; Benjamin" authorityYear="2005" box="[977,1086,941,960]" class="Arachnida" family="Pholcidae" genus="Wanniyala" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="3316" phylum="Arthropoda" rank="species" species="hakgala">
