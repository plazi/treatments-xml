<taxonomicName id="4C424D6DF17CFFBAFBDFFC3B78F74BE3" baseAuthorityName="Champion" baseAuthorityYear="1925" box="[1145,1263,983,1005]" class="Insecta" family="Staphylinidae" genus="Anthobium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="466" phylum="Arthropoda" rank="species" species="rugosum">
