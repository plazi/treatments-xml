<taxonomicName id="8FEE4D41FFF1FFDAFBF1F8DD618CCCD3" authorityName="Bowdich" authorityYear="1821" box="[1121,1231,1914,1940]" class="Mammalia" kingdom="Animalia" order="Carnivora" pageId="24" pageNumber="25" phylum="Chordata" rank="order">Carnivora</taxonomicName>
