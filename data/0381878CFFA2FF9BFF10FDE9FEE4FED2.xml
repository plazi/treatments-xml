) and 
<taxonomicName id="4C284D19FFA7FF9BFE96FF51FEDDFF7F" baseAuthorityName="Estrada &amp; Solervicens" baseAuthorityYear="1999" box="[273,384,152,176]" class="Insecta" family="Melyridae" genus="Arthrobrachus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="541" phylum="Arthropoda" rank="species" species="tibialis">
