<taxonomicName id="4C764D6FFFE74C71FCB8EC016CBCF9C9" baseAuthorityName="Swain" baseAuthorityYear="1946" box="[818,908,1611,1637]" class="Ostracoda" family="Cyprideidae" genus="Paracypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="obovata">
