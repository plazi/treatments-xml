<quantity id="E130C5E0FFE9FF8EFE324955FE76FDFE" box="[415,507,519,545]" metricMagnitude="3" metricUnit="m" metricValue="1.28" pageId="72" pageNumber="73" unit="m" value="1280.0">1280 m</quantity>
