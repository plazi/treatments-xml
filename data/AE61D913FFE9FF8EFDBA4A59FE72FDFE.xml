<taxonomicName id="E1C81386FFE9FF8EFC1D4AB6FB80FE21" baseAuthorityName="Verhoeff" baseAuthorityYear="1908" box="[944,1037,484,510]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="72" pageNumber="73" phylum="Tracheophyta" rank="genus">
