<quantity id="E130C5E0FFE9FF8EFD844AB6FD04FE21" box="[553,649,484,510]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="72" pageNumber="73" unit="m" value="1400.0">1400 m</quantity>
