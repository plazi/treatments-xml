<taxonomicName id="E1C81386FFE9FF8EFB314AB6FA8CFE21" box="[1180,1281,484,510]" class="Magnoliopsida" family="Berberidaceae" genus="Berberis" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ranunculales" pageId="72" pageNumber="73" phylum="Tracheophyta" rank="genus">
