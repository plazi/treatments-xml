<taxonomicName id="E1C81386FFE9FF8EFB8C4AB6FBD5FE21" box="[1057,1112,484,510]" class="Magnoliopsida" family="Aceraceae" genus="Acer" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="72" pageNumber="73" phylum="Tracheophyta" rank="genus">
<emphasis id="14BCB417FFE9FF8EFB8C4AB6FBD5FE21" box="[1057,1112,484,510]" italics="true" pageId="72" pageNumber="73">Acer</emphasis>
