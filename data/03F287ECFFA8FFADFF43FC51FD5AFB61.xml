<paragraph id="8BE436FAFFA8FFADFF43FBA0FD5AFB61" blockId="12.[140,1108,1117,1249]" pageId="12" pageNumber="93">
<emphasis id="B92FEAE8FFA8FFADFF43FBA0FEE1FBF7" bold="true" box="[140,290,1117,1143]" pageId="12" pageNumber="93">
