, 
<date id="FFAC168DFF8E8848FD22FBB8FCC3C55D" box="[647,795,1069,1109]" pageId="1" pageNumber="196" value="2018-09">
<collectingDate id="EFE8EF65FF8E8848FD22FBB8FCC3C55D" box="[647,795,1069,1109]" pageId="1" pageNumber="196" value="2018-09">Sept. 18</collectingDate>
</date>
, 
<collectorName id="26E7559BFF8E8848FC92FBB8FC64C55D" box="[823,956,1069,1109]" pageId="1" pageNumber="196">Swezey</collectorName>
, holotype female; 
<collectedFrom id="380E73B0FF8E8848FAB3FBB8F8CAC55D" box="[1302,1810,1069,1109]" pageId="1" pageNumber="196">&quot;ex rotten bark of dug-dug&quot;</collectedFrom>
, 
<collectorName id="26E7559BFF8E8848FE17FBF5FDE0C580" box="[434,568,1120,1160]" pageId="1" pageNumber="196">Swezey</collectorName>
, allotopotype male, in poor condition
</materialsCitation>
