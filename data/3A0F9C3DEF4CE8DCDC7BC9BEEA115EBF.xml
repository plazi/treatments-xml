<taxonomicName id="21794D79516F743A9FF19142EBF645F4" class="Reptilia" family="Pythonidae" genus="Liasis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liasis mackloti subsp. savuensis" order="Squamata" pageId="15" pageNumber="44" phylum="Chordata" rank="subSpecies" species="mackloti" subSpecies="savuensis">Liasis mackloti savuensis</taxonomicName>
; in 
<taxonomicName id="178983F59953E86868F2DBDB304A577C" class="Reptilia" family="Pythonidae" genus="Liasis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liasis mackloti subsp. dunni" order="Squamata" pageId="15" pageNumber="44" phylum="Chordata" rank="subSpecies" species="mackloti" subSpecies="dunni">Liasis mackloti dunni</taxonomicName>
