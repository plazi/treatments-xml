<taxonomicName id="DF9787ADA4DC1446C35396228DD68B55" class="Reptilia" family="Pythonidae" genus="Liasis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liasis mackloti subsp. mackloti" order="Squamata" pageId="15" pageNumber="44" phylum="Chordata" rank="subSpecies" species="mackloti" subSpecies="mackloti">Liasis mackloti mackloti</taxonomicName>
