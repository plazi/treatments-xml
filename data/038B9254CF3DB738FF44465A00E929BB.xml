<quantity id="4CDA8EA7CF3DB739FD7D4627008A2E5B" box="[666,731,1105,1126]" metricMagnitude="3" metricUnit="m" metricValue="2.2" pageId="18" pageNumber="1293" unit="m" value="2200.0">2200m</quantity>
