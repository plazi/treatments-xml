<taxonomicName id="8CEF9EA9F659543EBEE73D06699ADE9F" class="Insecta" family="Ichneumonidae" genus="Epirhyssa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Epirhyssa gavinbroadi" order="Hymenoptera" pageId="0" pageNumber="33" phylum="Arthropoda" rank="species" species="gavinbroadi">
