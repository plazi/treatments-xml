<paragraph id="8BCC36F00F5FFFB0FF19BB7936E1FEC5" blockId="101.[151,1437,257,374]" box="[151,717,349,374]" pageId="101" pageNumber="102">
<taxonomicName id="4C734D730F5FFFB0FF19BB793651FEC5" authority=": Cerretti et al. (2017" authorityName=": Cerretti et al." authorityYear="2017" box="[151,637,349,374]" class="Insecta" family="Calliphoridae" genus="Mesembrinella" kingdom="Animalia" order="Diptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="vogelsangi">
