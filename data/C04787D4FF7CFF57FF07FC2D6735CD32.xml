<emphasis id="7A9AEAD0FF7CFF57FF07FC2D65EFC8E3" bold="true" box="[151,172,906,932]" pageId="149" pageNumber="150">9.</emphasis>
<taxonomicName id="8FEE4D41FF7CFF57FF57FC2D67DEC8E2" authority="(Supino, 1897 a)" baseAuthorityName="Supino" baseAuthorityYear="1897" box="[199,669,906,933]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="asiatica">
