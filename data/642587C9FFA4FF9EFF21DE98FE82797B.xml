<taxonomicName id="2B8C4D5CFFA2FF9FFD9CDFE0FD557DD9" authority="Viraktamath &amp; Webb, 2007" authorityName="Viraktamath &amp; Webb" authorityYear="2007" box="[554,683,1986,2009]" class="Insecta" family="Cicadellidae" genus="Pythamus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="55" phylum="Arthropoda" rank="species" species="montanus" status="sp. nov.">
