<taxonomicName id="4C0F4D33077D0517FE12BC674CF2680F" authority="Wulp, 1885" authorityName="Wulp" authorityYear="1885" box="[470,752,540,562]" class="Insecta" family="Stratiomyidae" genus="Stratiomys" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="252" phylum="Arthropoda" rank="species" species="flaviscutellata">
