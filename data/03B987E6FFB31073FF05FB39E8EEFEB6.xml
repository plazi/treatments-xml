<number id="3DD824D7FFB31070FCA4F832EE48F873" box="[822,828,1984,1996]" pageId="1" pageNumber="590" value="3.0">3</number>
(Fig. 
<number id="3DD824D7FFB31070FCE4F841EEF0F877" box="[886,900,1971,1992]" pageId="1" pageNumber="590" value="6.0">6</number>
); 
<collectingCountry id="F3077660FFB31070FC04F841EEB5F877" box="[918,961,1971,1992]" name="Iran" pageId="1" pageNumber="590">Iran</collectingCountry>
........................ 
