<quantity id="802B9B17FFFF897B7965A3B9BEE5FD81" box="[1135,1222,412,438]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="41" pageNumber="42" unit="mm" value="10.0">10 mm</quantity>
