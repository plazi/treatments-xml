<taxonomicName id="80D34D71FFFF897B7F65A3CCB8E4FE35" authorityName="Germar" authorityYear="1836" box="[623,711,489,514]" class="Insecta" family="Cicadellidae" genus="Thlasia" kingdom="Animalia" order="Hemiptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
<emphasis id="75A7EAE0FFFF897B7F65A3CCB8E4FE35" box="[623,711,489,514]" italics="true" pageId="41" pageNumber="42">Thlasia</emphasis>
</taxonomicName>
