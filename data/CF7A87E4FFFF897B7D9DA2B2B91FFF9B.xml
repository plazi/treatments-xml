<taxonomicName id="80D34D71FFFF897B7FCFA3B9B94DFD82" authorityName="Stal, and Evans" authorityYear="1966" box="[709,878,412,437]" class="Insecta" family="Cicadellidae" genus="Petalocephala" kingdom="Animalia" order="Hemiptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
