<paragraph id="3067019D03ABDDE06A1F5D612D7D91A3" pageId="358" pageNumber="359">
<taxonomicName id="A99FFA23887665A8A6E9943759E9B9F7" authorityName="Casey" authorityYear="1918" class="Hexapoda" family="Carabidae" genus="Bembidion" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="358" pageNumber="359" phylum="Arthropoda" rank="species" species="illini">Bembidion illini</taxonomicName>
