<taxonomicName id="7984E2A7DA273E410E80F88FFAECF8A5" authorityName="Muller &amp; Henle" authorityYear="1837" box="[1238,1378,1806,1828]" class="Chondrichthyes" family="Hemiscylliidae" genus="Chiloscyllium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orectolobiformes" pageId="29" pageNumber="230" phylum="Chordata" rank="genus">
<emphasis id="8CF04536DA273E410E80F88FFAECF8A5" box="[1238,1378,1806,1828]" italics="true" pageId="29" pageNumber="230">Chiloscyllium</emphasis>
