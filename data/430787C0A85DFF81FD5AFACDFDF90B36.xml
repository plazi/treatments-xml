<typeStatus id="14158874A85EFF81FE6FFEDAFDD6083E" box="[506,619,271,292]" pageId="70" pageNumber="70">paratypes</typeStatus>
(
<collectionCode id="ADBFAE13A85EFF81FFF0FEF9FF0D085B" box="[101,176,300,321]" pageId="70" pageNumber="70">ICZN</collectionCode>
