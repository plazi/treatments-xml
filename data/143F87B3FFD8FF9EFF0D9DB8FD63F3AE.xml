<taxonomicName id="5B964D26FFD8FF9EFC6B9CD4FBC1F842" authorityName="Cuvier" authorityYear="1825" box="[1010,1117,1992,2021]" class="Mammalia" family="Herpestidae" genus="Crossarchus" kingdom="Animalia" order="Carnivora" pageId="20" pageNumber="325" phylum="Chordata" rank="species" species="obscurus">obscurus</taxonomicName>
