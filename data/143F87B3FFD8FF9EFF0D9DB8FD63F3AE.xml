<taxonomicName id="5B964D26FFD8FF9EFBB9930EFB53F794" authorityName="Goldman" authorityYear="1984" box="[1056,1231,2066,2099]" class="Mammalia" family="Herpestidae" genus="Crossarchus" kingdom="Animalia" order="Carnivora" pageId="20" pageNumber="325" phylum="Chordata" rank="species" species="platycephalus">platycephalus</taxonomicName>
