<taxonomicName id="4C544D3C5551FFF711CC8B138317F902" authorityName="Dubatolov" authorityYear="1987" box="[951,1103,1663,1687]" class="Insecta" family="Arctiidae" genus="Ocnogynodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="564" phylum="Arthropoda" rank="genus">
