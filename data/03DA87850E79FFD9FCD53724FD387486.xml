<collectingCountry id="F36476030E79FFD9FE563694FDE974FE" box="[561,615,1437,1459]" name="China" pageId="4" pageNumber="671">China</collectingCountry>
, 
<collectingRegion id="49B7F8710E79FFD9FE163694FD3774FF" box="[625,697,1437,1458]" country="China" name="Yunnan" pageId="4" pageNumber="671">Yunnan</collectingRegion>
, Fengshuiming Mts., 
<quantity id="4C8B9B760E79FFD9FF103694FC3874FE" box="[887,950,1437,1459]" metricMagnitude="3" metricUnit="m" metricValue="2.46" pageId="4" pageNumber="671" unit="m" value="2460.0">2460m</quantity>
