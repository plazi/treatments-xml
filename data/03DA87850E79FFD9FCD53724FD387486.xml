<paragraph id="8BCC36930E79FFD9FCD53724FD1E750E" blockId="4.[178,656,1069,1091]" box="[178,656,1069,1091]" pageId="4" pageNumber="671">
<heading id="D08481FF0E79FFD9FCD53724FD1E750E" box="[178,656,1069,1091]" fontSize="8" level="2" pageId="4" pageNumber="671" reason="2">
<emphasis id="B907EA810E79FFD9FCD53724FD1E750E" bold="true" box="[178,656,1069,1091]" pageId="4" pageNumber="671">
<taxonomicName id="4C734D100E79FFD9FCD53724FEFB750E" authority="Malicky, 2015" authorityName="Malicky" authorityYear="2015" box="[178,373,1069,1091]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="671" phylum="Arthropoda" rank="species" species="eyael" status="nov.sp.">
<emphasis id="B907EA810E79FFD9FCD53724FEFB750E" bold="true" box="[178,373,1069,1091]" italics="true" pageId="4" pageNumber="671">Hydropsyche eyael</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23457FA0E79FFD9FD1B3724FE46750F" box="[380,456,1069,1090]" pageId="4" pageNumber="671" rank="species">nov.sp.</taxonomicNameLabel>
(
<taxonomicName id="4C734D100E79FFD9FDB23724FD07750E" authorityName="Curtis" authorityYear="1835" box="[469,649,1069,1091]" class="Insecta" family="Hydropsychidae" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="671" phylum="Arthropoda" rank="family">Hydropsychidae</taxonomicName>
