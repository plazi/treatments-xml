<collectingCountry id="CAFE8BE3FF89A704810AFDCE2AD1FD9E" box="[359,434,550,575]" name="Russia" pageId="29" pageNumber="230">Russia</collectingCountry>
(
<figureCitation id="2AD2D7F6FF89A70481ACFDCE2976FD9E" box="[449,533,550,575]" captionStart="FIGURES 94 – 99" captionStartId="28.[151,263,1980,2003]" captionTargetBox="[172,1436,202,1957]" captionTargetId="figure@28.[151,1436,193,1959]" captionTargetPageId="28" captionText="FIGURES 94 – 99. Nanna cryophila, sp. nov.: 94 — fore femur, anterior view; 95 — male sternite 5; 96 — epandrium, cerci and surstyli, lateral view; 97 — same, dorsal view; 98 — male sternite 4; 99 — distribution map." httpUri="https://zenodo.org/record/236928/files/figure.png" pageId="29" pageNumber="230">
Fig. 
<number id="0421D954FF89A7048198FDCF2976FD9E" box="[501,533,551,575]" pageId="29" pageNumber="230" value="99.0">99</number>
</figureCitation>
