(
<quantity id="4CCA9B5EEB45FF8DFC25F8882D82FCAE" box="[1018,1167,718,744]" metricMagnitude="-1" metricUnit="m" metricValue="1.63" metricValueMax="2.16" metricValueMin="1.1" pageId="9" pageNumber="922" unit="mm" value="163.0" valueMax="216.0" valueMin="110.0">110–216 mm</quantity>
; 
