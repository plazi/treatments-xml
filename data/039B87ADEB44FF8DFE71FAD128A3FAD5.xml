<quantity id="4CCA9B5EEB45FF8DFB93F8A82D99FD4E" box="[1100,1172,750,776]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" pageId="9" pageNumber="922" unit="mm" value="30.0">30 mm</quantity>
