spp. 
<normalizedToken id="1A490F6A8923A7028D6E62E142FD32A3" originalValue="A–B">A-B</normalizedToken>
<taxonomicName id="D8452C7CA18273B29515D98EBCC9E895" genus="T." lsidName="T. kusad" pageId="2" pageNumber="115" rank="species" species="kusad">T. kusad</taxonomicName>
sp. n. 
<normalizedToken id="754895DEAD0EB1BE0C15FB08F52E0962" originalValue="C–D">C-D</normalizedToken>
<taxonomicName id="EF94CA84E533E6A77174E88B09793B5E" genus="T." lsidName="T. striatus" pageId="2" pageNumber="115" rank="species" species="striatus">T. striatus</taxonomicName>
<normalizedToken id="6C58352C52C5CBAD04F167EC123F3755" originalValue="E–F">E-F</normalizedToken>
