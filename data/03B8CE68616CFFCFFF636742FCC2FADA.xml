<date id="FFAF59BE616CFFCFFD1167F9FCAAFB60" box="[741,852,1173,1199]" pageId="4" pageNumber="119" value="2011-06-20">
<collectingDate id="EFEBA056616CFFCFFD1167F9FCAAFB60" box="[741,852,1173,1199]" pageId="4" pageNumber="119" value="2011-06-20">20.6.2011</collectingDate>
</date>
