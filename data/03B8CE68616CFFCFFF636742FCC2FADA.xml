<collectorName id="26E41AA8616CFFCFFEF767DBFEA7FB1E" box="[259,345,1207,1233]" pageId="4" pageNumber="119">Landry</collectorName>
, 
<collectorName id="26E41AA8616CFFCFFE9167DBFE44FB1E" box="[357,442,1207,1233]" pageId="4" pageNumber="119">Pastore</collectorName>
&amp; 
<collectorName id="26E41AA8616CFFCFFE2B67DBFDA8FB1E" box="[479,598,1207,1233]" pageId="4" pageNumber="119">Baumgart</collectorName>
; 2.- Avusy, derrière Forestal, 
<date id="FFAF59BE616CFFCFFC5867DAFBE7FB1E" box="[940,1049,1206,1233]" pageId="4" pageNumber="119" value="2011-06-15">
<collectingDate id="EFEBA056616CFFCFFC5867DAFBE7FB1E" box="[940,1049,1206,1233]" pageId="4" pageNumber="119" value="2011-06-15">15.6.2011</collectingDate>
</date>
, MVL, leg. 
<collectorName id="26E41AA8616CFFCFFF3E67B5FEDEFB3C" box="[202,288,1241,1267]" pageId="4" pageNumber="119">Landry</collectorName>
, 
<collectorName id="26E41AA8616CFFCFFEDB67B5FE7AFB3C" box="[303,388,1241,1267]" pageId="4" pageNumber="119">Pastore</collectorName>
&amp; 
<collectorName id="26E41AA8616CFFCFFE5967B5FDD6FB3C" box="[429,552,1241,1267]" pageId="4" pageNumber="119">Baumgart.</collectorName>
