<paragraph id="8BAE7F7E616CFFCFFF63673DFCC2FADA" blockId="4.[151,1134,1070,1301]" pageId="4" pageNumber="119">
<materialsCitation id="3B797523616CFFCFFF63673DFCC6FADA" collectingDate="2011-06-15" collectingDateMax="2011-06-20" collectingDateMin="2011-06-15" collectorName="Landry &amp; Pastore &amp; Baumgart &amp; Baumgart." country="Switzerland" location="Geneve" pageId="4" pageNumber="119" specimenCount="1" stateProvince="Geneve">
