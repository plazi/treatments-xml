<emphasis id="C290EAA13706FFACE117EC8DFE43F89E" bold="true" box="[317,428,1779,1800]" pageId="15" pageNumber="16">Figs 28–31.</emphasis>
<taxonomicName id="37E44D303706FFACE19AEC8AFC87F89F" authority="BALOGH, 1963: 28" authorityName="BALOGH" authorityPageNumber="28" authorityYear="1963" box="[432,872,1780,1801]" class="Arachnida" family="Punctoribatidae" genus="Punctoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="longiporosus">
