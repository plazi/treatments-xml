<paragraph id="255B2142CD286A2AF2DD43CBFD58FB46" blockId="76.[189,765,1249,1276]" box="[189,765,1249,1276]" pageId="76" pageNumber="77">
<taxonomicName id="E2E45AC1CD286A2AF2DD43CBFE0AFB46" authority="Kröger, 2013" authorityName="Kröger" authorityYear="2013" box="[189,431,1249,1275]" class="Cephalopoda" family="Uranoceratidae" genus="Siljanoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Barrandeocerida" pageId="76" pageNumber="77" phylum="Mollusca" rank="species" species="varians" status="sp. nov.">
<emphasis id="1790FD50CD286A2AF2DD43CBFE0AFB46" box="[189,431,1249,1275]" italics="true" pageId="76" pageNumber="77">Siljanoceras varians</emphasis>
</taxonomicName>
