<subSubSection id="99C96513FFAEFF82FF36F2C7844AF506" box="[199,837,1519,1545]" pageId="18" pageNumber="399" type="etymology">
<paragraph id="D16C3698FFAEFF82FF36F2C7844AF506" blockId="18.[151,1437,1230,1617]" box="[199,837,1519,1545]" pageId="18" pageNumber="399">
