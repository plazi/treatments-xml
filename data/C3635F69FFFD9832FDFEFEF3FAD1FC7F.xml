<collectionCode id="2DDB76BAFFFE9832FDCDFC60FD7DFC7F" box="[639,731,920,946]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="50" pageNumber="51">MNHN</collectionCode>
