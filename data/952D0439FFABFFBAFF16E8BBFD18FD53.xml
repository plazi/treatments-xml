<quantity id="DA7C18CAFFABFFBAFD46EB04FD25FD8D" box="[674,735,540,560]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="33" pageNumber="264" unit="m" value="750.0">750 m</quantity>
