<collectionCode id="ED1FAE71F2BEF1D8EEFE1741FD805C31" box="[420,513,301,322]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="138" pageNumber="138">AMNH</collectionCode>
