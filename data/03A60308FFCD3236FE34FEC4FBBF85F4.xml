<paragraph id="8BB0B21EFFCD3231FEF8FD8DFC6A8644" blockId="43.[313,1269,406,623]" box="[313,949,574,596]" pageId="43" pageNumber="347">
<treatmentCitationGroup id="AB1F9530FFCD3231FEF8FD8DFC6A8644" box="[313,949,574,596]" pageId="43" pageNumber="347">
<taxonomicName id="4C0FC99DFFCD3231FEF8FD8DFDAA8644" authority="(MacLeay)" baseAuthorityName="MacLeay" box="[313,629,574,596]" class="Insecta" family="Scarabaeidae" genus="Pachysoma" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="347" phylum="Arthropoda" rank="species" species="ritchiei">
