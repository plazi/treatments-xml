<paragraph id="8BCD94236F1BFFF60BFCFA6AFAE6FA1A" blockId="101.[189,1399,1449,1511]" pageId="101" pageNumber="102">
<taxonomicName id="4C72EFA06F1BFFF60BFCFA6AFE1DFA3F" authorityName="Marshall" authorityYear="2019" baseAuthorityName="Hennig" baseAuthorityYear="1935" box="[189,429,1449,1475]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="braueri">
