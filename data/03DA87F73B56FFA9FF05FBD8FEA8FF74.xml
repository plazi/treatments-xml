Distribution in 
<collectingCountry id="F36476713B56FFA8FEECF9ADFE6EF9CC" box="[382,457,1590,1615]" name="China" pageId="60" pageNumber="61">China</collectingCountry>
.
</emphasis>
<collectingRegion id="49B7F8033B56FFA8FE47F9ADFD8EF9CC" box="[469,553,1590,1615]" country="China" name="Jiangxi" pageId="60" pageNumber="61">Jiangxi</collectingRegion>
, 
<collectingRegion id="49B7F8033B56FFA8FDA7F9ADFD2AF9CC" box="[565,653,1590,1615]" country="China" name="Jiangsu" pageId="60" pageNumber="61">Jiangsu</collectingRegion>
.
