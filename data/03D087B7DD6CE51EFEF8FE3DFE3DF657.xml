<taxonomicName id="4C794D22DD6FE51EFDE5FC05FD92F1D1" box="[514,618,979,1003]" class="Actinopterygii" family="Sphyraenidae" genus="Sphyraena" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="25" phylum="Chordata" rank="genus">
<emphasis id="B90DEAB3DD6FE51EFDE5FC05FD92F1D1" box="[514,618,979,1003]" italics="true" pageId="2" pageNumber="25">Sphyraena</emphasis>
