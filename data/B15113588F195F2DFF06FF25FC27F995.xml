<taxonomicName id="FEF8D9CD8F195F2DFBD7FD1AFB38F9B1" box="[1094,1214,680,707]" class="Insecta" family="Noctuidae" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="466" phylum="Arthropoda" rank="family">Noctuidae</taxonomicName>
