<collectingRegion id="FB3C6CAC8F195F2DFF06FD7FFE85F995" box="[151,259,717,743]" country="Argentina" name="Tucuman" pageId="15" pageNumber="466">Tucumán</collectingRegion>
, 
<collectingCountry id="41EFE2DE8F195F2DFE9DFD7EFEFDF994" box="[268,379,716,742]" name="Argentina" pageId="15" pageNumber="466">Argentina</collectingCountry>
(
<bibRefCitation id="5D69DFBF8F195F2DFE1BFD7FFD4CF995" author="Townes, H. K. &amp; Townes, M." box="[394,714,716,743]" pageId="15" pageNumber="466" pagination="1 - 367" refId="ref36931" refString="Townes, H. K. &amp; Townes, M. (1966) A catalogue and reclassification of the Neotropic Ichneumonidae. Memoirs of the American Entomological Institute, 8, 1 - 367." type="journal article" year="1966">Townes &amp; Townes 1966: 24</bibRefCitation>
; 
<bibRefCitation id="5D69DFBF8F195F2DFD44FD7FFC12F995" author="Porter, C. C." box="[725,916,716,743]" pageId="15" pageNumber="466" pagination="1 - 192" refId="ref36175" refString="Porter, C. C. (1970 a) A revision of the South American species of Coccygomimus (Hymenoptera, Ichneumonidae). Studia Entomologica, 13 (1 - 4), 1 - 192." type="journal article" year="1970">Porter 1970a: 62</bibRefCitation>
