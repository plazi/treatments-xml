<quantity id="4C919B568243FFC8D950FC5AFFE3FB9A" box="[624,711,1015,1040]" metricMagnitude="3" metricUnit="m" metricValue="2.1" pageId="12" pageNumber="13" unit="m" value="2100.0">2100 m</quantity>
