<taxonomicName id="4BD808B04057FFB2FEB2ACDDFC5191E5" authority="Pritchard &amp; Baker, 1962: 277" authorityName="Pritchard &amp; Baker" authorityPageNumber="277" authorityYear="1962" box="[264,936,684,707]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="talinga" subGenus="Amblyseius">
