<collectionCode id="ED7AAE07870FAA10447FFEADD9DFAB5C" box="[610,703,351,377]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="20" pageNumber="21">AMNH</collectionCode>
