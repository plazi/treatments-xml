<taxonomicName id="0B824D5FFFE6FFA2FCC0A89DE4D6FA43" box="[790,951,1478,1502]" class="Insecta" family="Cicadellidae" genus="Dryodurgades" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="49" phylum="Arthropoda" rank="genus">
