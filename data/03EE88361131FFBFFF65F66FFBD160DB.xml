<taxonomicName id="4C4742A31132FFBFFF4CF165FD366093" authority="Rebel, 1901" authorityName="Rebel" authorityYear="1901" box="[190,655,621,647]" class="Insecta" family="Pieridae" genus="Colias" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="475" phylum="Arthropoda" rank="subSpecies" species="myrmidone" subSpecies="balcanica">
