<collectingRegion id="953BF1BBFF89FF87FEE0C9A468C2F9A5" box="[276,498,1534,1558]" country="Brazil" name="Rio Grande do Sul" pageId="11" pageNumber="194">Rio Grande do Sul</collectingRegion>
