<treatmentCitationGroup id="AB75A772FF82611AB7D83F9C2625799F" pageId="12" pageNumber="13">
<taxonomicName id="4C65FBDFFF82611AB7D83F9C27B579B8" authority="Kingdon, 1997: 55" authorityName="Kingdon" authorityPageNumber="55" authorityYear="1997" box="[151,660,422,448]" class="Mammalia" family="Cercopithecidae" genus="Miopithecus" kingdom="Animalia" order="Primates" pageId="12" pageNumber="13" phylum="Chordata" rank="species" species="ogouensis">
<emphasis id="B9115C4EFF82611AB7D83F9C248679B8" box="[151,423,423,448]" italics="true" pageId="12" pageNumber="13">Miopithecus ogouensis</emphasis>
<treatmentCitation id="0AC4A64DFF82611AB6FF3F9D27B579B8" author="Kingdon" box="[432,660,422,448]" page="55" pageId="12" pageNumber="13" year="1997">
<bibRefCitation id="EFF4FDADFF82611AB6FF3F9D274779B8" author="Kingdon" box="[432,614,422,448]" pageId="12" pageNumber="30" refString="Kingdon, J. (1997) The Kingdon field guide to African mammals. Academic Press, London." type="book" year="1997">
Kingdon, 
<number id="3DAD927BFF82611AB5683F9D274779B8" box="[551,614,422,448]" pageId="12" pageNumber="13" value="1997.0">1997</number>
</bibRefCitation>
