<geoCoordinate id="EE74503AFF95FFAEFB563814FA82F994" box="[1236,1284,1574,1598]" degrees="2" direction="south" minutes="7" orientation="latitude" pageId="7" pageNumber="291" precision="925" value="-2.1166666">S2 7</geoCoordinate>
(5 – 8), 
<geoCoordinate id="EE74503AFF95FFAEFAE63814FA12F994" box="[1380,1428,1574,1598]" degrees="4" direction="south" minutes="7" orientation="latitude" pageId="7" pageNumber="291" precision="925" value="-4.116667">S4 7</geoCoordinate>
