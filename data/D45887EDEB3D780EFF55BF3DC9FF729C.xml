</taxonomicNameLabel>
; square — 
<taxonomicName id="9BF14D78EB32780DFAA3BDC4CC89755E" authority="Nadein, 2013" authorityName="Nadein" authorityYear="2013" class="Insecta" family="Chrysomelidae" genus="Ivalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="393" phylum="Arthropoda" rank="species" species="lescheni" status="sp. nov.">
<emphasis id="6E85EAE9EB32780DFAA3BDC4CC89755E" italics="true" pageId="9" pageNumber="393">Ivalia lescheni</emphasis>
</taxonomicName>
<taxonomicNameLabel id="75B65792EB32780DFF32BDE3CD23755F" box="[240,320,1267,1288]" pageId="9" pageNumber="393" rank="species">
<emphasis id="6E85EAE9EB32780DFF32BDE3CD58755F" bold="true" box="[240,315,1267,1288]" pageId="9" pageNumber="393">sp. nov</emphasis>
