<taxonomicName id="E0F64D56DE72FFEEB55DC281FAAB960D" authorityName="Gutu" authorityYear="1981" box="[1109,1291,757,781]" class="Malacostraca" family="Kalliapseudidae" genus="Bacescapseudes" kingdom="Animalia" order="Tanaidacea" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
