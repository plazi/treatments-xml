<typeStatus id="8C308839FFA60D21FDB1FD076660C7F1" box="[585,638,708,731]" pageId="1" pageNumber="32">Type</typeStatus>
species: 
<taxonomicName id="948B4D18FFA60D21FD18FD076072C7F1" authority="Banks, 1920: 325" authorityName="Banks" authorityPageNumber="325" authorityYear="1920" box="[736,1132,708,731]" class="Insecta" family="Sialidae" genus="Protosialis" kingdom="Animalia" order="Megaloptera" pageId="1" pageNumber="32" phylum="Arthropoda" rank="species" species="minora">
<emphasis id="61FFEA89FFA60D21FD18FD0767B9C7F1" box="[736,935,708,731]" italics="true" pageId="1" pageNumber="32">Protosialis minora</emphasis>
