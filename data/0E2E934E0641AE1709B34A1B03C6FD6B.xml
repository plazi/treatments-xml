<taxonomicName id="418759DB0646AE1A08AD4BAE02CDF819" authorityName="Ueno" authorityYear="2006" box="[393,646,1989,2010]" class="Insecta" family="Carabidae" genus="Aspidaphaenops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="248" phylum="Arthropoda" rank="species" species="reflexus">
