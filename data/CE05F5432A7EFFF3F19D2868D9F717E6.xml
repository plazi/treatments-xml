, 
<quantity id="8154E9B02A7EFFF0F2C52820DE2711B0" box="[975,1035,1903,1927]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="21" pageNumber="92" unit="m" value="20.0">20 m</quantity>
