<taxonomicName id="4AE611E5FF6AFF77B49C1CE97B5F2045" baseAuthorityName="Baroni Urbani" baseAuthorityYear="1978" box="[498,621,1780,1810]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="173" pageNumber="174" phylum="Arthropoda" rank="species" species="goniops">
