(
<figureCitation id="137E639AFFF9FFAEFF4DB412FF32CDCF" box="[159,238,1900,1925]" captionStart="FIGURES 1–2" captionStartId="3.[151,264,1855,1878]" captionTargetBox="[370,1206,411,1837]" captionTargetPageId="3" captionText="FIGURES 1–2. Wings. 1, Neuronema ypsilum sp. nov.; 2, Neuronema maculosum sp. nov." figureDoi="http://doi.org/10.5281/zenodo.6163439" httpUri="https://zenodo.org/record/6163439/files/figure.png" pageId="4" pageNumber="561">Figs. 2</figureCitation>
, 
<figureCitation id="137E639AFFF9FFAEFF28B412FE9DCDCF" box="[250,321,1900,1925]" captionStart="FIGURES 12–19" captionStartId="5.[151,264,1743,1766]" captionTargetBox="[166,1423,331,1706]" captionTargetId="figure-176@5.[151,1436,314,1723]" captionTargetPageId="5" captionText="FIGURES 12–19. Neuronema maculosum sp. nov. 12, male terminalia, lateral view; 13, gonarcus, dorsal view; 14, ditto, lateral view; 15, parabaculum, dorsal view; 16, ditto, lateral view; 17, hypandrium internum, ventral view; 18, female terminalia, lateral view; 19, subgenitale, ventral view." figureDoi="http://doi.org/10.5281/zenodo.6163445" httpUri="https://zenodo.org/record/6163445/files/figure.png" pageId="4" pageNumber="561">12–19</figureCitation>
