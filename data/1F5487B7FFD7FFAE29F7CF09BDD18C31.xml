species: 
<taxonomicName id="50FD4D22FFD7FFAE284DCFEEBA598AF9" authority="Cockerell, 1900: 173" authorityName="Cockerell" authorityPageNumber="173" authorityYear="1900" box="[301,797,685,708]" class="Insecta" family="Coccidae" genus="Cryptinglisia" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="lounsburyi">
