<taxonomicName id="84FB9CAE9D4ACA5DFC76FABFFC29FAC2" authorityName=", S. R. Steinhauser &amp; G. T. Austin" authorityYear="1993" box="[925,970,1340,1365]" class="Insecta" family="Hesperiidae" genus="Artines" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="rica">
