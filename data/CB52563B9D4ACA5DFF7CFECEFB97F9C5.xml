<specimenCount id="55FD2CA49D4ACA5DFB68FD37FAF8FD59" box="[1155,1307,692,718]" pageId="16" pageNumber="17" type="generic">12 specimens</specimenCount>
(males and females) from Turrialba, 
<collectingRegion id="813F29CF9D4ACA5DFE5EFD5BFDF3FD65" box="[437,528,728,754]" country="Costa Rica" name="Cartago" pageId="16" pageNumber="17">Cartago</collectingRegion>
, 
<collectingCountry id="3BECA7BD9D4ACA5DFDF6FD5BFD7FFD65" box="[541,668,728,754]" name="Costa Rica" pageId="16" pageNumber="17">Costa Rica</collectingCountry>
