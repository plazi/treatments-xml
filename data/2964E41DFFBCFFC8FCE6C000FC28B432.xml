<emphasis id="93B98919FFBCFFC8FCA6C53BFC45B63D" box="[809,913,1037,1058]" italics="true" pageId="5" pageNumber="1397">Sicyonis</emphasis>
</taxonomicName>
; 
<taxonomicName id="66CD2E88FFBCFFC8FC2CC53BFBCFB63D" authorityName="Gosse" authorityYear="1859" box="[931,1051,1037,1058]" class="Anthozoa" family="Actinostolidae" genus="Stomphia" kingdom="Animalia" order="Actiniaria" pageId="5" pageNumber="1397" phylum="Cnidaria" rank="genus">
<emphasis id="93B98919FFBCFFC8FC2CC53BFBCFB63D" box="[931,1051,1037,1058]" italics="true" pageId="5" pageNumber="1397">Stomphia</emphasis>
</taxonomicName>
; 
<taxonomicName id="66CD2E88FFBCFFC8FBA0C53BFA5EB63D" authority="Carlgren, 1921" authorityName="Carlgren" authorityYear="1921" box="[1071,1418,1037,1059]" class="Anthozoa" family="Actinostolidae" genus="Synsicyonis" kingdom="Animalia" order="Actiniaria" pageId="5" pageNumber="1397" phylum="Cnidaria" rank="genus">
<emphasis id="93B98919FFBCFFC8FBA0C53BFB15B63D" box="[1071,1217,1037,1058]" italics="true" pageId="5" pageNumber="1397">Synsicyonis</emphasis>
<bibRefCitation id="C55C28FAFFBCFFC8FB43C53BFA5EB63D" author="Carlgren O" box="[1228,1418,1037,1059]" pageId="5" pageNumber="1397" pagination="1 - 241" refId="ref14709" refString="Carlgren O. 1921. Actiniaria I. Danish Ingolf-Expedition 5: 1 - 241." type="journal article" year="1921">Carlgren, 1921</bibRefCitation>
</taxonomicName>
; 
<taxonomicName id="66CD2E88FFBCFFC8FCA6C51AFB91B65E" authority="Hertwig, 1882" authorityName="Hertwig" authorityYear="1882" box="[809,1093,1068,1090]" class="Anthozoa" family="Actinostolidae" genus="Tealidium" kingdom="Animalia" order="Actiniaria" pageId="5" pageNumber="1397" phylum="Cnidaria" rank="genus">
<emphasis id="93B98919FFBCFFC8FCA6C51AFC4AB65E" box="[809,926,1068,1089]" italics="true" pageId="5" pageNumber="1397">Tealidium</emphasis>
<bibRefCitation id="C55C28FAFFBCFFC8FC2BC51AFB91B65E" author="Hertwig R" box="[932,1093,1068,1090]" pageId="5" pageNumber="1397" refId="ref16441" refString="Hertwig R. 1882. Die actinien der challenger expedition. Jena: Gustav Fischer." type="book" year="1882">Hertwig, 1882</bibRefCitation>
