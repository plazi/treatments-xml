<paragraph id="8BB44E600607FFDAF5F35D8C6B707B9D" blockId="23.[178,1086,645,726]" pageId="23" pageNumber="364">
<taxonomicName id="4C0B35E30607FFDAF5F35D8C6E6B7BD0" box="[178,304,646,667]" class="Magnoliopsida" family="Apocynaceae" genus="Vinca" kingdom="Plantae" order="Gentianales" pageId="23" pageNumber="364" phylum="Tracheophyta" rank="species" species="major">
