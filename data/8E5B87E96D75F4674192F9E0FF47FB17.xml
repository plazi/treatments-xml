<paragraph id="064D36FF6D75F4644192F903FE85F976" blockId="17.[151,762,1640,1700]" box="[151,307,1675,1700]" pageId="17" pageNumber="459">
(
<figureCitation id="9EC92A7A6D75F464419AF903FF47F976" box="[159,241,1675,1700]" captionStart="FIGURE 13" captionStartId="18.[151,250,1973,1995]" captionTargetBox="[340,1247,1276,1908]" captionTargetId="figure@18.[340,1248,1276,1952]" captionTargetPageId="18" captionText="FIGURE 13. Plumarius albinervis sp. nov., ♂. A, Head, frontal view; B, Head, ventral view; C, Head, lateral view; D, Genital capsule, ventral view. Scale bars = 0.5 mm." httpUri="https://zenodo.org/record/257590/files/figure.png" pageId="17" pageNumber="459">
Figs 
<number id="B03A24D86D75F46441D2F904FF47F976" box="[215,241,1676,1700]" pageId="17" pageNumber="459" value="13.0">13</number>
</figureCitation>
