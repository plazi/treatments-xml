</taxonomicName>
(
<emphasis id="D380EAA07715470CFE42FF13FE14FF30" bold="true" box="[490,508,207,231]" italics="true" pageId="20" pageNumber="209">C</emphasis>
. × 
<taxonomicName id="26F44D317715470CFDB7FF13FCC5FF30" authority="F. SCHULTZ" authorityName="F. SCHULTZ" box="[543,813,207,231]" class="Magnoliopsida" family="Asteraceae" genus="Cirsium" kingdom="Plantae" order="Asterales" pageId="20" pageNumber="209" phylum="Tracheophyta" rank="species" species="bipontinum">
