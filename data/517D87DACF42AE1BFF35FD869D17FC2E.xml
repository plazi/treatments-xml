<taxonomicName id="1ED44D4FCF42AE1BFDFEFCE39BDBFCF6" authority="Distant" authorityName="Distant" box="[604,965,786,812]" class="Insecta" family="Cicadellidae" genus="Prasutagus" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="pulchellus">
