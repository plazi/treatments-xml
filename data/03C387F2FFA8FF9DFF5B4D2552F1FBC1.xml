<location id="8EB5603FFFA8FF9DFADD4C2853E5FC8E" LSID="urn:lsid:plazi:treatment:03C387F2FFA8FF9DFF5B4D2552F1FBC1:8EB5603FFFA8FF9DFADD4C2853E5FC8E" box="[1297,1437,777,802]" country="Iran" county="Southern" municipality="Caspian Sea" name="Kopet Dagh" pageId="36" pageNumber="37" stateProvince="Alborz">Kopet Dagh</location>
