<bibRefCitation id="BCCF3DEBCCED9C167C24955F525AA31E" author="Grearson, KJ" journalOrPublisher="Zootaxa" pageId="103" pageNumber="104" pagination="72 - 88" title="Review of seasonal polyphenism in the Symphyta (Hymenoptera), exemplified by Pristiphoraleucopus (Hellen, 1948) (Tenthredinidae)." volume="3502" year="2012">Grearson and Liston 2012</bibRefCitation>
), 
<taxonomicName id="D8F330E71456060AB0E6039DDF7310C3" class="Magnoliopsida" family="Malvaceae" genus="Tilia" higherTaxonomySource="CoL" hybrid-sign="x" kingdom="Plantae" lsidName="Tilia vulgaris" order="Malvales" pageId="68" pageNumber="69" phylum="Tracheophyta" rank="species" species="vulgaris">
