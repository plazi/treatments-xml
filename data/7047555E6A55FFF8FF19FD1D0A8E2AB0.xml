<taxonomicName id="3FEE9FCB6A55FFF8FEAFFBA10A0A2BDD" baseAuthorityName="Bosquet" baseAuthorityYear="1852" box="[286,515,1134,1160]" class="Ostracoda" family="Trachyleberididae" genus="Kingmaina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="27" pageNumber="936" phylum="Arthropoda" rank="species" species="forbesiana">
