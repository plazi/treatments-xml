<taxonomicName id="3FEE9FCB6A55FFF8FEE4FB410A3B2BFD" baseAuthorityName="Bosquet" baseAuthorityYear="1852" box="[341,562,1166,1192]" class="Ostracoda" family="Trachyleberididae" genus="Kingmaina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="27" pageNumber="936" phylum="Arthropoda" rank="species" species="forbesiana">
