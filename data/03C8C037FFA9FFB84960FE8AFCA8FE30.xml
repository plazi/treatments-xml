<taxonomicName id="4C610AA2FFA9FFB94A8BF9AEFDA8F9A0" authorityName="Nakane, Fukuda and Hayashi" authorityYear="1981" box="[454,611,1565,1586]" class="Insecta" family="Lycidae" genus="Cautires" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="1492" phylum="Arthropoda" rank="species" species="yausai">
