<taxonomicName id="4C610AA2FFA9FFB949B6F9E1FC2DF9F5" box="[763,998,1618,1639]" class="Insecta" family="Lycidae" genus="Metanoeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="1492" phylum="Arthropoda" rank="species" species="pendleburyi">
