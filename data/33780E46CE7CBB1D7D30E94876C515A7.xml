<taxonomicName id="150A2B567AB63B5CF8A624453538FF79" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181968" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole agricola Wilson" lsidName-HNS="Pheidole agricola Wilson" order="Hymenoptera" pageNumber="561" phylum="Arthropoda" rank="species" species="agricola">agricola</taxonomicName>
