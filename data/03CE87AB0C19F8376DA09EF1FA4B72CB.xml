<taxonomicName id="4C674D3E0C1AF8376F909971FC89756B" authorityName="Skelley" authorityYear="2009" box="[752,866,870,894]" class="Insecta" family="Erotylidae" genus="Cubyrus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="genus">
<emphasis id="B913EAAF0C1AF8376F909971FC89756B" bold="true" box="[752,866,870,894]" italics="true" pageId="10" pageNumber="9">Cubyrus</emphasis>
</taxonomicName>
<taxonomicName id="4C674D3E0C1AF8376E089971FC24756B" authority="Skelley, 2009" authorityName="Skelley" authorityYear="2009" box="[872,975,870,894]" genus="Skelley" pageId="10" pageNumber="9" rank="genus" status="n. gen.">Skelley</taxonomicName>
