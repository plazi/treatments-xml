<taxonomicName id="2C772E758B436641898B8C6A2AA7A022" class="Insecta" family="Cetoniidae" genus="Lophorrhinides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lophorrhinides" order="Coleoptera" pageId="1" pageNumber="76" phylum="Arthropoda" rank="genus">Lophorrhinides</taxonomicName>
