<number id="DC791733FFE75160FA64FD73499ACA17" box="[1408,1436,765,791]" pageId="20" pageNumber="21" value="11.0">11</number>
and 
<quantity id="AD49A8F1FFE75160FF2CFCAB4D02CA3F" box="[200,260,805,831]" metricMagnitude="1" metricUnit="m" metricValue="9.0" pageId="20" pageNumber="21" unit="m" value="90.0">90 m</quantity>
(
<bibRefCitation id="0E2078E5FFE75160FEF7FCAB4DBACA3F" author="Coelho" box="[275,444,805,831]" pageId="20" pageNumber="40" refString="Coelho, P. A. (1997 a) Revisao das especies de Thalassinidea encontradas em Pernambuco, Brasil (Crustacea, Decapoda). Trabalhos Oceanograficos da Universidade Federal de Pernambuco, 25, 137 - 161." type="journal article" year="1997" yearSuffix="a">
Coelho 
<number id="DC791733FFE75160FE95FCAB4DA8CA3F" box="[369,430,805,831]" pageId="20" pageNumber="21" value="1997.0">1997</number>
a
</bibRefCitation>
; Melo 
<number id="DC791733FFE75160FDE9FCAB4E4ACA3F" box="[525,588,805,831]" pageId="20" pageNumber="21" value="1999.0">1999</number>
).
</paragraph>
</subSubSection>
