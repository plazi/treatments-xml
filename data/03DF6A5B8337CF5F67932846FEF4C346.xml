<subSubSection id="C36C88C68337CF5A67C32915FC20C562" box="[199,898,1795,1820]" pageId="9" pageNumber="10" type="etymology">
<paragraph id="8BC9DB4D8337CF5A67C32915FC20C562" blockId="9.[151,1437,1723,1999]" box="[199,898,1795,1820]" pageId="9" pageNumber="10">
<emphasis id="B902075F8337CF5A67C32915FEECC562" bold="true" box="[199,334,1795,1819]" inLineHeading="true" pageId="9" pageNumber="10" reason="1">Etymology.</emphasis>
