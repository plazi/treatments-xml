<heading id="6F7CC558171CA933989DFD547B53FD45" bold="true" box="[151,784,671,697]" fontSize="11" level="1" pageId="45" pageNumber="46" reason="1">
<emphasis id="06FFAE26171CA933989DFD547B53FD45" bold="true" box="[151,784,671,697]" pageId="45" pageNumber="46">
<taxonomicName id="F38B09B7171CA933989DFD547A30FD45" authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,627,671,697]" class="Insecta" family="Hydropsychidae" genus="Hydromanicus" kingdom="Animalia" order="Trichoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="palnis" status="new species">
<emphasis id="06FFAE26171CA933989DFD5479D9FD45" bold="true" box="[151,410,671,697]" italics="true" pageId="45" pageNumber="46">Hydromanicus palnis</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D171CA9339A8AFD547B53FD45" box="[640,784,671,697]" pageId="45" pageNumber="46" rank="species">new species</taxonomicNameLabel>
</emphasis>
