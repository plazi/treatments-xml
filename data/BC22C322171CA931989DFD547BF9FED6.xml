<taxonomicName id="F38B09B7171FA9309962F8667B49F83F" authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[360,778,1964,1988]" class="Insecta" family="Hydropsychidae" genus="Hydromanicus" kingdom="Animalia" order="Trichoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="palnis" status="new species">
