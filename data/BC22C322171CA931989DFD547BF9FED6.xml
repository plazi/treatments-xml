<subSubSection id="7C9121BF171EA93198CFFF5C793EFF26" pageId="47" pageNumber="48" type="materials_examined">
<paragraph id="34347234171EA93198CFFF5C793EFF26" blockId="47.[151,1436,151,298]" pageId="47" pageNumber="48">
