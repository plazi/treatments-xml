<subSubSection id="C3346555CD1AFFFDFF204812B124FA64" pageId="19" pageNumber="20" type="etymology">
<paragraph id="8B9136DECD1AFFFDFF204812B124FA64" blockId="19.[151,1437,1255,2036]" pageId="19" pageNumber="20">
<emphasis id="B95AEACCCD1AFFFDFF204812B20CFA40" bold="true" box="[199,334,1400,1424]" pageId="19" pageNumber="20">Etymology.</emphasis>
