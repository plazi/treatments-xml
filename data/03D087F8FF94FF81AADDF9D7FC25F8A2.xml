<specimenCount id="9D7FFD67FF94FF81AB19F9A0FE3FF942" box="[339,421,1658,1684]" pageId="7" pageNumber="8" type="generic">
<quantity id="4C819B0BFF94FF81AB19F9A0FE3BF942" box="[339,417,1658,1684]" metricMagnitude="2" metricUnit="m" metricValue="9.5" pageId="7" pageNumber="8" unit="m" value="950.0">950 m</quantity>
