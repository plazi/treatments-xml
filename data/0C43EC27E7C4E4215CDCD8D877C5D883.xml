<taxonomicName id="C5F0DAB02E681BF80E9BF1D032BF2B2C" LSID="http://zoobank.org/85FD7AC1-06A4-41EA-8F3D-B8A2E2FB221C" authority="D. - Y. Zhou &amp; S. - J. Zhang" class="Insecta" family="Staphylinidae" genus="Horaeomorphus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Horaeomorphus hujiayaoi" order="Coleoptera" pageId="11" pageNumber="62" phylum="Arthropoda" rank="species" species="hujiayaoi">Horaeomorphus hujiayaoi D.-Y. Zhou &amp; S.-J. Zhang</taxonomicName>
