<typeStatus id="195D548BFFE8874BFC6BC919FC16F420" box="[905,961,1581,1607]" pageId="19" pageNumber="20">Type</typeStatus>
species: 
<taxonomicName id="01E691AAFFE8874BFBC4C919FAA9F420" authority="Reitter" authorityName="Reitter" authorityYear="1884" box="[1062,1406,1581,1607]" class="Insecta" family="Leiodidae" genus="Dissochaetus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="hetschkoi">
