<paragraph id="3E82F684A93D2CE3A6A3C381FADA2B8B" pageId="null" pageNumber="329">
<pageBreakToken id="503C3618BC6AEDECEDB20BBFEB05D95D" pageId="null" pageNumber="329">Artengruppe</pageBreakToken>
der 
<taxonomicName id="4CBF05EAD22A5A1FC86B7D1EC2AE0305" authority="L." class="Magnoliopsida" family="Caprifoliaceae" genus="Valeriana" kingdom="Plantae" order="Dipsacales" pageId="null" pageNumber="329" phylum="Tracheophyta" rank="species" species="officinalis">
Valeriana 
<normalizedToken id="0674D74CA9A0FF8ED096FF7E580916E6" originalValue="officinális" pageId="null" pageNumber="329">officinalis</normalizedToken>
<authorityName id="5948316078EB2A65B29D6A449A8EE60F" pageId="null" pageNumber="329">L.</authorityName>
</taxonomicName>
</paragraph>
