<quantity id="0F1C9B0A9A37FFCBFA8183BD6633E119" box="[1340,1399,1266,1290]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="33" pageNumber="34" unit="m" value="30.0">30 m</quantity>
