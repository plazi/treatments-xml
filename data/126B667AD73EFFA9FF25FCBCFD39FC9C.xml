<subSubSection id="D2D884E7D73EFFABFF25FC2FFC67FC58" box="[151,922,972,999]" pageId="1" pageNumber="202" type="etymology">
<paragraph id="9A7DD76CD73EFFABFF25FC2FFC67FC58" blockId="1.[151,1437,972,2043]" box="[151,922,972,999]" pageId="1" pageNumber="202">
<emphasis id="A8B60B7ED73EFFABFF25FC2FFEE3FC58" bold="true" box="[151,286,972,998]" pageId="1" pageNumber="202">Etymology.</emphasis>
