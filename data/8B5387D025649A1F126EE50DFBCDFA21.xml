<subSubSection id="4BE0654D25649A1D1174E5CBFC6FFC7A" pageId="9" pageNumber="1619" type="reference_group">
<paragraph id="034536C625649A1D1174E5CBFC28FC06" blockId="9.[313,1064,826,973]" box="[313,1001,923,946]" pageId="9" pageNumber="1619">
<taxonomicName id="C4FA4D4525649A1D1174E5CBFCCEFC06" authority="Herdman, 1886: 140" authorityName="Herdman" authorityPageNumber="140" authorityYear="1886" box="[313,783,924,946]" class="Ascidiacea" family="Polycitoridae" genus="Cystodytes" kingdom="Animalia" order="Aplousobranchia" pageId="9" pageNumber="1619" phylum="Chordata" rank="species" species="philippinensis">
<emphasis id="318EEAD425649A1D1174E5CBFDF5FC06" box="[313,564,924,945]" italics="true" pageId="9" pageNumber="1619">Cystodytes philippinensis</emphasis>
Herdman, 1886: 140
</taxonomicName>
