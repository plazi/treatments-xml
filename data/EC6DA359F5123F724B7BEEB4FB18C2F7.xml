35. 
<taxonomicName id="A3C469CCF5123F734B10EEB4FCAAC36F" authority="Usinger, 1946" authorityName="Usinger" authorityYear="1946" box="[368,846,476,519]" class="Insecta" family="Reduviidae" genus="Hadrocranella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="41" phylum="Arthropoda" rank="species" species="pallidicoxa" status="new species">Hadrocranella pallidicoxa</taxonomicName>
,
</emphasis>
