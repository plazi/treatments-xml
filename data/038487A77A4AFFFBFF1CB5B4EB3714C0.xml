<date id="FF9310717A4AFFFBFF14B0FEEFC015A8" box="[159,237,1734,1759]" pageId="22" pageNumber="23" value="1176-10">X1176</date>
) (
<collectionCode id="ED3CAE747A4AFFFBFE8FB0FFEE6E15A8" box="[260,323,1735,1759]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="22" pageNumber="23">ZSM</collectionCode>
