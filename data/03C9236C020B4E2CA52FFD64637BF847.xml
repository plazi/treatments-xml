<taxonomicName id="4C60E9F9020B4E2CA51BF86E63B1F807" authority="BORDONI, 2002" authorityName="BORDONI" authorityYear="2002" box="[866,1187,1952,1976]" class="Insecta" family="Staphylinidae" genus="Metolinus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="258" phylum="Arthropoda" rank="species" species="apomontis">
<emphasis id="B9144E68020B4E2CA51BF86E64E0F808" box="[866,1010,1952,1975]" italics="true" pageId="3" pageNumber="258">M. apomontis</emphasis>
