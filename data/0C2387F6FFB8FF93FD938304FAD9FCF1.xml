<taxonomicName id="438A4D63FFB8FF93FD3E8152FBBCFCF0" authority="Heslop-Harrison, 1961" authorityName="Heslop-Harrison" authorityYear="1961" box="[701,1107,826,852]" class="Insecta" family="Liviidae" genus="Parapsylla" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="161" phylum="Arthropoda" rank="genus">
