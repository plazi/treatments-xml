), 
<collectingCountry id="96C5638EFFF79342CA11FA8A6BBFFABC" box="[874,950,1340,1366]" name="France" pageId="14" pageNumber="35">France</collectingCountry>
(
<bibRefCitation id="8A435EEFFFF79342CAB0FA8D6C41FABF" author="Lado, C." box="[971,1096,1339,1365]" pageId="14" pageNumber="35" pagination="117 - 185" refId="ref14479" refString="Lado, C. (1994) A checklist of myxomycetes of the Mediterranean countries. Mycotaxon 52: 117 - 185." type="journal article" year="1994">Lado 1994</bibRefCitation>
), 
<collectingCountry id="96C5638EFFF79342CD18FA8D6CA2FABF" box="[1123,1195,1339,1365]" name="Belize" pageId="14" pageNumber="35">Belize</collectingCountry>
