<collectingCountry id="96C5638EFFF79342C9F3FAE968E4FA93" box="[136,237,1375,1401]" name="Tanzania" pageId="14" pageNumber="35">Tanzania</collectingCountry>
(
<bibRefCitation id="8A435EEFFFF79342C878FAE96A35FA93" author="Mitchell, D. W. &amp; Stampfer, J. F." box="[259,572,1375,1401]" pageId="14" pageNumber="35" pagination="291 - 293" refId="ref14910" refString="Mitchell, D. W. &amp; Stampfer, J. F. (2004) Some Myxomycetes from Tanzania. Systematics and Geography of Plants 74: 291 - 293." type="journal article" year="2004">Mitchell &amp; Stampfer 2004</bibRefCitation>
), 
<collectingCountry id="96C5638EFFF79342CB22FAD66A90FA90" box="[601,665,1376,1402]" name="Japan" pageId="14" pageNumber="35">Japan</collectingCountry>
(Yamamoto 2006), 
<collectingCountry id="96C5638EFFF79342CAF1FAE96BCCFA93" box="[906,965,1375,1401]" name="Cuba" pageId="14" pageNumber="35">Cuba</collectingCountry>
(
<bibRefCitation id="8A435EEFFFF79342CAA0FAE96CCAFA93" author="Camino, M. &amp; Moreno, G. &amp; Castillo, A. &amp; Mitchell, D. W. &amp; Minter, D. W." box="[987,1219,1375,1401]" pageId="14" pageNumber="35" pagination="75 - 102" refId="ref13849" refString="Camino, M., Moreno, G., Castillo, A., Mitchell, D. W. &amp; Minter, D. W. (2008) Additions to the myxomycete biota of Cuba. 1. Mycotaxon 106: 75 - 102." type="journal article" year="2008">
