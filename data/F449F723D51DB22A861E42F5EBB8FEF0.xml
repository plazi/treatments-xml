<taxonomicName id="BBE03DB6D51DB25587B442B3EC77FD6D" authority="Bates 1889: 370" authorityName="Bates" authorityPageNumber="370" authorityYear="1889" box="[685,1027,725,747]" class="Insecta" family="Cetoniidae" genus="Euphoria" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="fulveola">
<emphasis id="4E949A27D51DB25587B442B3EB2FFD6C" box="[685,859,726,747]" italics="true" pageId="63" pageNumber="64">Euphoria fulveola</emphasis>
<materialsCitation id="CC884C68D51DB255867B42B0EC77FD6D" box="[866,1027,725,746]" collectionCode="BMNH" pageId="63" pageNumber="64" specimenCount="2" typeStatus="lectotype">Bates 1889: 370</materialsCitation>
</taxonomicName>
