<taxonomicName id="22E54D4AA642FFF2B75251F9FE3908FA" authorityName="Pohl" authorityYear="1827" box="[341,483,1684,1711]" class="Magnoliopsida" family="Euphorbiaceae" genus="Cnidoscolus" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="181" phylum="Tracheophyta" rank="genus">
