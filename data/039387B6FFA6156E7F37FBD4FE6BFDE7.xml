</taxonomicName>
, 
<taxonomicName id="4C3A4D23FFA2156E7CF8FE75FC92FE60" authorityName="Hebauer" authorityYear="2002" box="[676,821,413,436]" class="Insecta" family="Hydrophilidae" genus="Laccobius" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="622" phylum="Arthropoda" rank="species" species="eliogentilii">
