<date id="FFAB105CE862FFC07E3EFB0EFE07FB65" box="[321,504,1196,1223]" pageId="32" pageNumber="433" value="1963-11" valueMax="1963-12-31" valueMin="1963-11-00">Nov.–Dec. 1963</date>
