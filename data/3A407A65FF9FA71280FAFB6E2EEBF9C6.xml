<taxonomicName id="75E9B0F0FF9FA7128237F9EF2867F9BE" box="[602,772,1542,1567]" class="Insecta" family="Scathophagidae" genus="Gonarcticus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="212" phylum="Arthropoda" rank="species" species="abdominalis">
