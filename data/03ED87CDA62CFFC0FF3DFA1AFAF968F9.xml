(
<emphasis id="B930EAC9A62EFFC0FD63FE06F9A76885" box="[713,733,405,428]" italics="true" pageId="23" pageNumber="24">T.</emphasis>
) 
<emphasis id="B930EAC9A62EFFC0FD5FFE07F8E26884" box="[757,920,404,429]" italics="true" pageId="23" pageNumber="24">neoperturbans</emphasis>
<bibRefCitation id="EFD54B2AA62EFFC0FC0DFE06FF656885" author="Ree" box="[935,1055,404,428]" pageId="23" pageNumber="24" refString="Ree, H. (2009) Three new and four unrecorded species of Chironomidae (Insecta: Diptera) in Korea. Korean Journal of Systematic Zoology, 25 (3), 243 - 254. http: // dx. doi. org / 10.5635 / KJSZ. 2009.25.3.243" type="journal article" year="2009">
Ree, 
<number id="3D8C24FCA62EFFC0FC4DFE07FF656885" box="[999,1055,404,428]" pageId="23" pageNumber="24" value="2009.0">2009</number>
