<tableCitation id="C6C60360A62EFFC0FE83FE24FA0468F9" box="[297,382,439,464]" captionStart="TABLE 3" captionStartId="23.[151,239,509,532]" captionTargetBox="[159,1322,560,790]" captionTargetPageId="23" captionText="TABLE 3. Comparison between P. (T.) mengmanense Zhang &amp; Wang sp. n. and P. (T.) neoperturbans Ree." httpUri="http://table.plazi.org/id/DF3B6653A62EFFC0FF3DFE6EFE6A6B3D" pageId="23" pageNumber="24" tableUuid="DF3B6653A62EFFC0FF3DFE6EFE6A6B3D">
Table 
<number id="3D8C24FCA62EFFC0FEC4FE2BFA0468F9" box="[366,382,440,464]" pageId="23" pageNumber="24" value="3.0">3</number>
