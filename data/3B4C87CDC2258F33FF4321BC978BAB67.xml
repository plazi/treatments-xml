</taxonomicName>
species except 
<taxonomicName id="74E54D58C2248F33FB0C26C4967CADE3" box="[1240,1401,404,428]" class="Arachnida" family="Theridiidae" genus="Phycosoma" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="39" phylum="Arthropoda" rank="species" species="hainanensis">
