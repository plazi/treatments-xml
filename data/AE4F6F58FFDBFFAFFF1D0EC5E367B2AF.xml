<taxonomicName id="E1E6A5CDFFDDFFAFFBCE09E7E350B3C7" box="[1092,1325,1222,1247]" class="Insecta" family="Carabidae" genus="Trechisibus" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="70" phylum="Arthropoda" rank="species" species="parvulus" status="n. sp.">
