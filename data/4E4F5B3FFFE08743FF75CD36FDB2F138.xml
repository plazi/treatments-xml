= 
<taxonomicName id="01E691AAFFE08743FF18CD82FD51F0A8" authority="Portevin, 1907: 70" authorityName="Portevin" authorityPageNumber="70" authorityYear="1907" box="[250,646,693,719]" class="Insecta" family="Leiodidae" genus="Dissochaetus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="magnicornis">
<emphasis id="F492363BFFE08743FF18CD82FE7EF0A8" box="[250,425,694,719]" italics="true" pageId="27" pageNumber="28">D. magnicornis</emphasis>
<bibRefCitation id="A27797D8FFE08743FE53CD81FD51F0A8" author="Portevin, G." box="[433,646,693,719]" pageId="27" pageNumber="28" pagination="67 - 82" refId="ref105571" refString="Portevin, G. (1907) Clavicornes nouveaux du groupe des necrophages. Iie Memoire. Annales de la Societe Entomologique de France, 76, 67 - 82." type="journal article" year="1907">Portevin, 1907: 70</bibRefCitation>
</taxonomicName>
; 
<bibRefCitation id="A27797D8FFE08743FD70CD81FC82F0A8" author="Hatch, M. H." box="[658,853,693,719]" pageId="27" pageNumber="28" pagination="63 - 244" refId="ref100328" refString="Hatch, M. H. (1928) Silphidae II. In: Schenkling, S. (Ed.), Coleopterorum Catalogus, Pars 95. W. Junk, Berlin, pp. 63 - 244." type="book chapter" year="1928">Hatch, 1928: 163</bibRefCitation>
syn. (of “ 
<emphasis id="F492363BFFE08743FC2ACD82FC34F0A8" box="[968,995,694,719]" italics="true" pageId="27" pageNumber="28">D.</emphasis>
” 
<taxonomicName id="01E691AAFFE08743FC15CD82FB23F0A8" authority="Kirsch [1871]" authorityName="Kirsch" authorityYear="1871" box="[1015,1268,692,719]" class="Insecta" family="Leiodidae" genus="Zeadolopus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="exiguus">
<emphasis id="F492363BFFE08743FC15CD82FB9AF0A8" box="[1015,1101,694,719]" italics="true" pageId="27" pageNumber="28">exiguus</emphasis>
<bibRefCitation id="A27797D8FFE08743FBB7CD81FB23F0A8" author="Kirsch, T." box="[1109,1268,692,719]" pageId="27" pageNumber="28" pagination="337 - 378" refId="ref101812" refString="Kirsch, T. (1871 (1870 )) Beitrage zur Kaferfauna von Bogota (Sechstes Stuck). Berliner Entomologische Zeitschrift, 14, 337 - 378." type="journal article" year="1871">Kirsch [1871]</bibRefCitation>
</taxonomicName>
, now in 
<taxonomicName id="01E691AAFFE08743FAB4CD81FEEEF094" class="Insecta" family="Leiodidae" genus="Paulipalpina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
<emphasis id="F492363BFFE08743FAB4CD81FEEEF094" italics="true" pageId="27" pageNumber="28">Paulipalpina</emphasis>
</taxonomicName>
