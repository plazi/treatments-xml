<taxonomicName id="01E691AAFFE08743FC87CDEEFBD5F094" baseAuthorityName="Matthews" baseAuthorityYear="1888" box="[869,1026,730,755]" class="Insecta" family="Leiodidae" genus="Dissochaetus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="semipiceus">
