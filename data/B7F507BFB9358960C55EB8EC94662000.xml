<geoCoordinate id="5187834B50C7AADD1A890B6E2AC1F063" direction="north" orientation="latitude" precision="5" value="45.6848">45.6848°N</geoCoordinate>
, 
<geoCoordinate id="0FC4D8103D71817C8E04605CC42DB67E" direction="west" orientation="longitude" precision="5" value="-66.8821">66.8821°W</geoCoordinate>
