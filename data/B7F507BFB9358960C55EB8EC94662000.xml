<geoCoordinate id="DC2D709B6659F14740957CD9FCBFDDD6" direction="north" orientation="latitude" precision="5" value="45.6741">45.6741°N</geoCoordinate>
, 
<geoCoordinate id="FB49CFE99256911A8E1DC2CB6E09379D" direction="west" orientation="longitude" precision="5" value="-66.8661">66.8661°W</geoCoordinate>
