<taxonomicName id="DA924D550E492D34FE97A1E4BFA4DE92" authority="Asenjo &amp; Ribeiro-Costa, 2013" authorityName="Asenjo &amp; Ribeiro-Costa" authorityYear="2013" box="[349,471,207,231]" class="Insecta" family="Staphylinidae" genus="Leptochirus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="1271" phylum="Arthropoda" rank="species" species="milton" status="sp. nov." subGenus="Tropiochirus">
