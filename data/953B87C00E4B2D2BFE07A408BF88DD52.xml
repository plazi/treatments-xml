and 
<emphasis id="2FE6EAC40E492D34FC96A1FBBD06DE92" box="[860,885,208,231]" italics="true" pageId="15" pageNumber="1271">L.</emphasis>
<taxonomicName id="DA924D550E492D34FCB6A1E4BD8DDE92" authority="Asenjo &amp; Ribeiro-Costa, 2013" authorityName="Asenjo &amp; Ribeiro-Costa" authorityYear="2013" box="[892,1022,207,231]" class="Insecta" family="Staphylinidae" genus="Leptochirus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="1271" phylum="Arthropoda" rank="species" species="marina" status="sp. nov." subGenus="Tropiochirus">
(
<emphasis id="2FE6EAC40E492D34FC4FA1FBBDE4DE92" box="[901,919,208,231]" italics="true" pageId="15" pageNumber="1271">T</emphasis>
.) 
<emphasis id="2FE6EAC40E492D34FC7AA1FBBD8DDE92" box="[944,1022,208,231]" italics="true" pageId="15" pageNumber="1271">marina</emphasis>
