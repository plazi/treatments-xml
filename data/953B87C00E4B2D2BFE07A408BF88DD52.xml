, 
<emphasis id="2FE6EAC40E492D34FD8BA1FBBC29DE92" box="[577,602,208,231]" italics="true" pageId="15" pageNumber="1271">L.</emphasis>
<taxonomicName id="DA924D550E492D34FDABA1E4BD51DE92" authorityName="Wendeler" authorityYear="1957" box="[609,802,207,231]" class="Insecta" family="Staphylinidae" genus="Leptochirus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="1271" phylum="Arthropoda" rank="species" species="venezuelanus" subGenus="Tropiochirus">
(
<emphasis id="2FE6EAC40E492D34FDA0A1FBBC0FDE92" box="[618,636,208,231]" italics="true" pageId="15" pageNumber="1271">T</emphasis>
.) 
