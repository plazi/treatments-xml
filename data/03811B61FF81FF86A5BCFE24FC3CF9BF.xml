<taxonomicName id="4C28D1F4FF81FF86A4F5FD2FFDD8F9BE" box="[478,556,1623,1646]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="349" phylum="Tracheophyta" rank="genus">
