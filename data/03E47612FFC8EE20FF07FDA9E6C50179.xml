: 
<quantity id="4CB56AE1FFC8EE20FE55FBA0E05F0276" box="[453,554,1123,1149]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="6" pageNumber="75" unit="mm" value="1.6">1.60 mm</quantity>
, 
<collectionCode id="ED5C5FC1FFC8EE20FDA4FBA0E0160276" box="[564,611,1123,1149]" pageId="6" pageNumber="75">RW</collectionCode>
