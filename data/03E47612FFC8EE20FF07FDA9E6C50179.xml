: 
<quantity id="4CB56AE1FFC8EE20FD52FB68E15C02CE" box="[706,809,1195,1221]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="6" pageNumber="75" unit="mm" value="2.1">2.10 mm</quantity>
