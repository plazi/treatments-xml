<quantity id="4CB56AE1FFC8EE20FD8CFB68E0F602CE" box="[540,643,1195,1221]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="6" pageNumber="75" unit="mm" value="1.4">1.40 mm</quantity>
