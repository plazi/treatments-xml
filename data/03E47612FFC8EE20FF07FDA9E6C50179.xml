<paragraph id="8BF2C704FFC8EE20FF57FC34E2AA0252" blockId="6.[151,1437,726,1906]" pageId="6" pageNumber="75">
<emphasis id="B9391B16FFC8EE20FF57FC34E34E021A" bold="true" box="[199,315,1015,1041]" pageId="6" pageNumber="75">
<typeStatus id="54F679A6FFC8EE20FF57FC34E343021A" box="[199,310,1015,1041]" pageId="6" pageNumber="75">Holotype</typeStatus>
