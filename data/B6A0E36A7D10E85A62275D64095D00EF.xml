<taxonomicName id="2259CCB9CA70B0B373D6305F2B6516BB" lsidName="" pageId="261" pageNumber="262" rank="tribe" tribe="Somatodini">Somatodini</taxonomicName>
); 
<taxonomicName id="EA81913CB39FEF22CFE628818B83B1AB" lsidName="" pageId="261" pageNumber="262" rank="subFamily" subFamily="Somatodinae">Somatodinae</taxonomicName>
