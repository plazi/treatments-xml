<typeStatus id="CE19884834613231FDE17865F8667A6C" box="[565,613,1595,1619]" pageId="4" pageNumber="338">type</typeStatus>
locality: 
<collectingCountry id="69B5767A34613231FD067864F93A7A6C" box="[722,825,1594,1619]" name="Slovenia" pageId="4" pageNumber="338">Slovenia</collectingCountry>
