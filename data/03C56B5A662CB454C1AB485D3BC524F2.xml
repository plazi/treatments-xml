<taxonomicName id="4C6CA1CF662CB454C5754E163CC7242E" baseAuthorityName="Korotyaev &amp; Gultekin" baseAuthorityYear="2007" box="[1097,1245,1382,1407]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="121" pageNumber="122" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
