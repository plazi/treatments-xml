<number id="8D5A5C14FFFFCD3AFC53293CFC14EC21" box="[973,987,1300,1324]" pageId="2" pageNumber="45" value="2.0">2</number>
+ 
<number id="8D5A5C14FFFFCD3AFC75293CFC36EC21" box="[1003,1017,1300,1324]" pageId="2" pageNumber="45" value="2.0">2</number>
<emphasis id="09E69221FFFFCD3AFC61293CFBDAEC21" box="[1023,1045,1300,1324]" italics="true" pageId="2" pageNumber="45">lp</emphasis>
<number id="8D5A5C14FFFFCD3AFB8B290BFBE7EC3D" box="[1045,1064,1315,1328]" pageId="2" pageNumber="45" value="2.3">
<emphasis id="09E69221FFFFCD3AFB8B290BFBE7EC3D" box="[1045,1064,1315,1328]" italics="true" pageId="2" pageNumber="45">2,3</emphasis>
</number>
on pronotum; 
<number id="8D5A5C14FFFFCD3AFB52293CFB17EC21" box="[1228,1240,1300,1324]" pageId="2" pageNumber="45" value="1.0">1</number>
+ 
