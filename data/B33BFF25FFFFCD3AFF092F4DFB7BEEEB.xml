<emphasis id="09E69221FFFFCD3AFCAB293DFC91EC21" box="[821,862,1301,1324]" italics="true" pageId="2" pageNumber="45">ma,</emphasis>
<number id="8D5A5C14FFFFCD3AFCFA293CFCBDEC21" box="[868,882,1300,1324]" pageId="2" pageNumber="45" value="2.0">2</number>
+ 
<number id="8D5A5C14FFFFCD3AFC1C293CFC5FEC21" box="[898,912,1300,1324]" pageId="2" pageNumber="45" value="2.0">2</number>
<emphasis id="09E69221FFFFCD3AFC09293CFC62EC21" box="[919,941,1300,1324]" italics="true" pageId="2" pageNumber="45">la</emphasis>
<number id="8D5A5C14FFFFCD3AFC32290BFC70EC3D" box="[940,959,1315,1328]" pageId="2" pageNumber="45" value="2.3">
<emphasis id="09E69221FFFFCD3AFC32290BFC70EC3D" box="[940,959,1315,1328]" italics="true" pageId="2" pageNumber="45">2,3</emphasis>
</number>
