<number id="CA3A24B03312FFBD974EFDF0B544FD97" box="[777,827,585,609]" pageId="16" pageNumber="417" value="1.4">1.40</number>
, PEW 
<number id="CA3A24B03312FFBD97D5FDF0B5BBFD94" box="[914,964,585,610]" pageId="16" pageNumber="417" value="1.45">1.45</number>
, EFL 
<number id="CA3A24B03312FFBD9056FDF0B23BFD97" box="[1041,1092,585,609]" pageId="16" pageNumber="417" value="0.8">0.80</number>
, AL 
<number id="CA3A24B03312FFBD90C2FDF0B2C7FD97" box="[1157,1208,585,609]" pageId="16" pageNumber="417" value="2.8">2.80</number>
, AW 
<number id="CA3A24B03312FFBD90B8FDF0B34EFD94" box="[1279,1329,585,610]" pageId="16" pageNumber="417" value="1.58">1.58</number>
, L 
<number id="CA3A24B03312FFBD9111FDF0B31FFD97" box="[1366,1376,585,609]" pageId="16" pageNumber="417" value="1.0">1</number>
<number id="CA3A24B03312FFBD912BFDF0B3E3FD94" box="[1388,1436,585,610]" pageId="16" pageNumber="417" value="5.23">5.23</number>
(
<number id="CA3A24B03312FFBD94D9FDD3B6B0FD75" box="[158,207,618,643]" pageId="16" pageNumber="417" value="1.5">1.50</number>
+ 
<number id="CA3A24B03312FFBD9498FDD3B770FD75" box="[223,271,618,643]" pageId="16" pageNumber="417" value="1.13">1.13</number>
+ 
<number id="CA3A24B03312FFBD9559FDD3B731FD75" box="[286,334,618,643]" pageId="16" pageNumber="417" value="1.25">1.25</number>
+ 
<number id="CA3A24B03312FFBD951AFDD3B7F0FD75" box="[349,399,618,643]" pageId="16" pageNumber="417" value="0.85">0.85</number>
+ 
<number id="CA3A24B03312FFBD95D9FDD3B7ADFD75" box="[414,466,618,643]" pageId="16" pageNumber="417" value="0.5">0.50</number>
).
</paragraph>
<paragraph id="7C4D36973312FFBD9480FD35B7F1FD53" blockId="16.[151,1436,151,677]" box="[199,398,652,677]" pageId="16" pageNumber="417">
