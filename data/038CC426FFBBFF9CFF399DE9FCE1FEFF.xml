<emphasis id="B951A922FFBBFF9DFF399DE9FD95FF51" bold="true" box="[151,524,152,178]" pageId="20" pageNumber="21">
<taxonomicName id="4C250EB3FFBBFF9DFF399DE9FEA5FF51" authorityName="Smith &amp; Paterson &amp; Brock" authorityYear="2018" box="[151,316,152,178]" class="Trilobita" family="Leiostegiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptychopariida" pageId="20" pageNumber="21" phylum="Arthropoda" rank="family">Leiostegiidae</taxonomicName>
