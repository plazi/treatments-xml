<emphasis id="B927EAC8FF8D2376FDB7F84AFC9A68C9" bold="true" box="[634,860,1949,1974]" pageId="22" pageNumber="215">
<collectingRegion id="4997F838FF8D2376FDB7F84AFC9168C9" box="[634,855,1949,1974]" country="Australia" name="Western Australia" pageId="22" pageNumber="215">Western Australia</collectingRegion>
