<taxonomicName id="34CF4D32FFEFFF84FB0CD7DEFA99F869" box="[1161,1305,1984,2005]" class="Collembola" family="Entomobryidae" genus="Lepidocyrtinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
