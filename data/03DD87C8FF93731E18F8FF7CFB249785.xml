<date id="FFCA101EFF93731F19B6FB9AFDC893B5" box="[388,541,1137,1161]" pageId="42" pageNumber="319" value="1895-08-09">9 August 1895</date>
; 
<emphasis id="B900EACCFF93731F1A15FB9AFDB193B5" bold="true" box="[551,612,1137,1161]" pageId="42" pageNumber="319">S.616</emphasis>
