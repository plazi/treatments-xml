<date id="FFCA101EFF93731F1AF5FC17FF08930D" pageId="42" pageNumber="319" value="1880-01">January 1880</date>
; 
<emphasis id="B900EACCFF93731F18D4FBF2FEF4930D" bold="true" box="[230,289,1049,1073]" pageId="42" pageNumber="319">S.420</emphasis>
