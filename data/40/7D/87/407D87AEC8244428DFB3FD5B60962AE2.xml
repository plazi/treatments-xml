<taxonomicName authorityName="Eschscholtzi Fleutiaux" authorityYear="1920" box="[836,914,836,861]" class="Insecta" family="Eucnemidae" genus="Balgus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="455" phylum="Arthropoda" rank="genus">
<emphasis box="[836,914,836,861]" italics="true" pageId="4" pageNumber="455">Balgus</emphasis>
