included 
<taxonomicName authorityName="Erichson" authorityYear="1847" box="[674,815,1859,1883]" class="Insecta" family="Coccinellidae" genus="Hinda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="26" phylum="Arthropoda" rank="species" species="regularis">
<emphasis box="[674,692,1860,1883]" italics="true" pageId="29" pageNumber="26">S</emphasis>
. 
<emphasis box="[704,815,1859,1883]" italics="true" pageId="29" pageNumber="26">regularis</emphasis>
</taxonomicName>
in their review of 
<taxonomicName box="[1033,1112,1859,1883]" class="Insecta" family="Coccinellidae" genus="Hinda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="26" phylum="Arthropoda" rank="genus">
