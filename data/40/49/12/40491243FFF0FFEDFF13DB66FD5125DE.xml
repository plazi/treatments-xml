<typeStatus box="[192,260,323,347]" pageId="19" pageNumber="16">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Mulsant, 1850" authorityName="Mulsant" authorityYear="1850" box="[378,846,323,347]" class="Insecta" family="Coccinellidae" genus="Cleothera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="16" phylum="Arthropoda" rank="species" species="decemsignata">
<emphasis box="[378,665,323,347]" italics="true" pageId="19" pageNumber="16">Cleothera decemsignata</emphasis>
<bibRefCitation author="Mulsant, M. E." box="[670,846,323,347]" pageId="19" pageNumber="16" pagination="1 - 1104" refId="ref90542" refString="Mulsant, M. E. 1850. Species de Coleopteres trimeres securipalpes. Annales des Sciences Physiques et Naturelles, Lyon 2: 1 - 1104." type="journal article" year="1850">Mulsant, 1850</bibRefCitation>
