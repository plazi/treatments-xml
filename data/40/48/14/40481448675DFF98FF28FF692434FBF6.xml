<paragraph blockId="2.[151,1437,543,1072]" pageId="2" pageNumber="263">
<emphasis bold="true" box="[151,397,543,568]" pageId="2" pageNumber="263">Species composition.</emphasis>
C. 
<taxonomicName authority="(Verhoeff, 1900)" authorityName="Verhoeff" authorityYear="1900" baseAuthorityName="Verhoeff" baseAuthorityYear="1900" box="[438,818,543,568]" class="Malacostraca" family="Trichoniscidae" genus="Cyphonethes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="263" phylum="Arthropoda" rank="species" species="herzegowinensis">
<emphasis box="[438,623,543,568]" italics="true" pageId="2" pageNumber="263">herzegowinensis</emphasis>
(
<bibRefCitation box="[638,809,543,568]" pageId="2" pageNumber="263" refId="ref10106" refString="Verhoeff, K. (1900) Uber palaarktische Isopoden (2. Aufsatz). Zoologischer Anzeiger, 23, 117 - 130." type="journal article">Verhoeff, 1900</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Verhoeff, 1900)" authorityName="Karaman &amp; Horvatović" authorityYear="2018" baseAuthorityName="Verhoeff" baseAuthorityYear="1900" box="[832,1170,543,568]" class="Malacostraca" family="Trichoniscidae" genus="Titanethes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="263" phylum="Arthropoda" rank="species" species="biseriatus" status="comb. nov.">
<emphasis box="[832,976,543,568]" italics="true" pageId="2" pageNumber="263">C. biseriatus</emphasis>
(
<bibRefCitation box="[991,1162,543,568]" pageId="2" pageNumber="263" refId="ref10106" refString="Verhoeff, K. (1900) Uber palaarktische Isopoden (2. Aufsatz). Zoologischer Anzeiger, 23, 117 - 130." type="journal article">Verhoeff, 1900</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel box="[1178,1307,543,568]" pageId="2" pageNumber="263">
<emphasis bold="true" box="[1178,1307,543,568]" pageId="2" pageNumber="263">comb. nov.</emphasis>
</taxonomicNameLabel>
