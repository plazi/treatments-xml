<taxonomicName authorityName="Schioedte" authorityYear="1849" box="[326,441,580,605]" class="Malacostraca" family="Trichoniscidae" genus="Titanethes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="263" phylum="Arthropoda" rank="genus">
<emphasis box="[326,441,580,605]" italics="true" pageId="2" pageNumber="263">Titanethes</emphasis>
</taxonomicName>
and 
<taxonomicName authority="Karaman &amp; Horvatović, 2018" authorityName="Karaman &amp; Horvatović" authorityYear="2018" box="[495,610,581,604]" class="Malacostraca" family="Trichoniscidae" genus="Cyphonethes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="263" phylum="Arthropoda" rank="species" species="tajanus" status="n. sp.">
<emphasis box="[495,610,581,604]" italics="true" pageId="2" pageNumber="263">C. tajanus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[617,680,580,605]" pageId="2" pageNumber="263" rank="species">
<emphasis bold="true" box="[617,680,580,605]" pageId="2" pageNumber="263">n. sp.</emphasis>
</taxonomicNameLabel>
described here.
</paragraph>
