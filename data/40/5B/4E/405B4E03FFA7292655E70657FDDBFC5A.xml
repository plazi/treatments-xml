<taxonomicName authority="Nico M. Franz, 2001" authorityName="Nico M. Franz" authorityYear="2001" box="[494,720,1545,1566]" class="Insecta" family="Curculionidae" genus="Staminodeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="425" phylum="Arthropoda" rank="species" species="forcipis" status="new species">Staminodeus forcipis</taxonomicName>
,
</emphasis>
<taxonomicNameLabel box="[732,862,1544,1565]" pageId="14" pageNumber="425" rank="species">
