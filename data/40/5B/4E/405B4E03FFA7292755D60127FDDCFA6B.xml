<collectingRegion box="[424,509,1465,1486]" country="Panama" name="Panama" pageId="14" pageNumber="425">
<collectingCountry box="[424,509,1465,1486]" name="Panama" pageId="14" pageNumber="425">Panama</collectingCountry>
</collectingRegion>
, 
<collectingRegion box="[522,710,1465,1486]" country="Panama" name="Chiriqui" pageId="14" pageNumber="425">Provincia Chiriquı</collectingRegion>
