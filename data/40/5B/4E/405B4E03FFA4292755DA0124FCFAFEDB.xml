<collectingCountry box="[443,566,1524,1545]" name="Costa Rica" pageId="13" pageNumber="424">Costa Rica</collectingCountry>
, 
<collectingRegion box="[583,850,1524,1545]" country="Costa Rica" name="Puntarenas" pageId="13" pageNumber="424">Provincia de Puntarenas</collectingRegion>
