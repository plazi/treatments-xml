<taxonomicName authority="Schedl" authorityName="Schedl" authorityYear="1935" box="[504,800,505,527]" class="Insecta" family="Curculionidae" genus="Xylechinus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="formosanus">
