<taxonomicName authority="Reitter, 1913" authorityName="Reitter" authorityYear="1913" box="[151,554,694,719]" class="Insecta" family="Leiodidae" genus="Rybinskiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="407" phylum="Arthropoda" rank="species" species="bodoana" subGenus="Sintania">
