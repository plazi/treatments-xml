: 
<taxonomicName authority="Gistel, 1856" authorityName="Gistel" authorityYear="1856" box="[214,447,186,208]" class="Insecta" family="Curculionidae" genus="Archarius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="30" phylum="Arthropoda" rank="genus">
