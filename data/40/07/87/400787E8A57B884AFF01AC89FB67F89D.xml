<emphasis bold="true" box="[199,345,1972,1997]" pageId="1" pageNumber="410">Distribution</emphasis>
. 
<collectingCountry box="[359,419,1972,1997]" name="India" pageId="1" pageNumber="410">India</collectingCountry>
, 
<collectingCountry box="[430,500,1972,1997]" name="Nepal" pageId="1" pageNumber="410">Nepal</collectingCountry>
, 
