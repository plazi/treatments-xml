<paragraph blockId="11.[151,397,927,954]" box="[151,397,927,954]" pageId="11" pageNumber="162">
<heading bold="true" box="[151,397,927,954]" fontSize="11" level="1" pageId="11" pageNumber="162" reason="1">
<emphasis bold="true" box="[151,397,927,954]" pageId="11" pageNumber="162">
<taxonomicName authority="Bird, 2020" authorityName="Bird" authorityYear="2020" box="[151,307,927,954]" class="Malacostraca" family="Leptocheliidae" genus="Psalidichelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tanaidacea" pageId="11" pageNumber="162" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[151,307,927,954]" italics="true" pageId="11" pageNumber="162">Psalidichelia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[315,397,927,953]" pageId="11" pageNumber="162" rank="genus">n. gen.</taxonomicNameLabel>
