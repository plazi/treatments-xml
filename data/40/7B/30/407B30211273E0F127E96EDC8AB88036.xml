<subSubSection pageId="24" pageNumber="69" type="nomenclature">
<paragraph pageId="24" pageNumber="69">
<taxonomicName LSID="http://species-id.net/wiki/Ophioderma_cinerea" authority="Mueller &amp; Troschel, 1842" authorityName="Mueller &amp; Troschel" authorityYear="1842" class="Ophiuroidea" family="Ophiodermatidae" genus="Ophioderma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ophioderma cinerea" order="Ophiacanthida" pageId="24" pageNumber="69" phylum="Echinodermata" rank="species" species="cinerea">
<pageBreakToken pageId="24" pageNumber="69" start="start">Ophioderma</pageBreakToken>
cinerea 
<normalizedToken originalValue="Müller">Mueller</normalizedToken>
&amp; Troschel, 1842
</taxonomicName>
Figure 13
<normalizedToken originalValue="f–j">f-j</normalizedToken>
, 14e
</paragraph>
