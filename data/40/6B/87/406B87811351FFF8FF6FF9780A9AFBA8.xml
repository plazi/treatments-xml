; 
<bibRefCitation author="Kluge" box="[297,440,1032,1058]" lastPageId="6" lastPageNumber="26" pageId="6" pageNumber="25" refString="Kluge, N. Yu. (1997) Order Ephemeroptera - mayflies. In.: Tsalolikhin S. J. (Ed.), Key to Freshwater Invertebrates of Russian and Adjacent Lands, St. Petersburg, pp. 175 - 220, 304 - 329 (in Russian)." type="book chapter" year="1997">
Kluge 
<number box="[378,440,1032,1058]" pageId="6" pageNumber="23" value="1997.0">1997</number>
