, 
<collectingCountry box="[796,848,925,951]" name="Italy" pageId="7" pageNumber="8">Italy</collectingCountry>
, 
<collectingRegion box="[861,927,925,951]" country="Italy" name="Sicilia" pageId="7" pageNumber="8">Sicily</collectingRegion>
, Balkan Peninsula, 
<collectingCountry box="[1151,1244,925,951]" name="Ukraine" pageId="7" pageNumber="8">Ukraine</collectingCountry>
, southern part of European 
<collectingCountry box="[267,342,961,987]" name="Russia" pageId="7" pageNumber="8">Russia</collectingCountry>
[
<collectingRegion box="[357,478,961,987]" country="Russia" name="Astrakhan'" pageId="7" pageNumber="8">Astrakhan</collectingRegion>
, 
<collectingRegion box="[491,578,961,987]" country="Russia" name="Saratov" pageId="7" pageNumber="8">Saratov</collectingRegion>
, 
<collectingRegion box="[590,709,961,987]" country="Russia" name="Volgograd" pageId="7" pageNumber="8">Volgograd</collectingRegion>
, 
<collectingRegion box="[722,899,961,987]" country="Russia" name="Orenburg" pageId="7" pageNumber="8">Orenburg Prov.</collectingRegion>
, 
<collectingRegion box="[911,1099,960,987]" country="Russia" name="Krasnodar" pageId="7" pageNumber="8">Krasnodar Kray</collectingRegion>
]), North and Central 
<collectingCountry name="Kazakhstan" pageId="7" pageNumber="8">Kazakhstan</collectingCountry>
, Middle East, Caucasus, 
<collectingCountry box="[479,609,997,1023]" name="Turkey" pageId="7" pageNumber="8">Asia Minor</collectingCountry>
, deserts and semideserts (including mountainous) of Central Asia.
</paragraph>
