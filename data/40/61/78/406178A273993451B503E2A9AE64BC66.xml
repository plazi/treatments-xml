<taxonomicName class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis aequinoctialis" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="aequinoctialis">Chrysis aequinoctialis</taxonomicName>
Spin. 
<normalizedToken originalValue="inédite">inedite</normalizedToken>
