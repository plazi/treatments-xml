<number box="[958,965,762,774]" pageId="1" pageNumber="41" value="1.0">1</number>
except in 
<taxonomicName authority="Allard, 1995" authorityName="Allard" authorityYear="1995" box="[1063,1394,765,787]" class="Insecta" family="Cetoniidae" genus="Clithria" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="41" phylum="Arthropoda" rank="species" species="bacchusi">
