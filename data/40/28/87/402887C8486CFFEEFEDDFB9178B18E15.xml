<quantity box="[427,571,1233,1259]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" metricValueMax="4.7" metricValueMin="3.5" pageId="1" pageNumber="48" unit="mm" value="4.1" valueMax="4.7" valueMin="3.5">3.5–4.7 mm</quantity>
