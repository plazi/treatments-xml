<paragraph blockId="1.[151,1437,1743,2028]" box="[151,428,1743,1768]" pageId="1">
<emphasis bold="true" box="[151,333,1743,1768]" pageId="1">Measurements.</emphasis>
Table 
<number box="[408,422,1743,1767]" pageId="1" value="1.0">1</number>
