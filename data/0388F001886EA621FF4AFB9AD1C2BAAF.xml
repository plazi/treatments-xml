(Figs. 
<number id="3DE95330886EA621FF03FB7ED55DB88B" box="[222,266,1158,1183]" pageId="6" pageNumber="497" value="3.6">3, 6</number>
, 
<number id="3DE95330886EA621FECBFB7ED52DB88B" box="[278,378,1158,1183]" pageId="6" pageNumber="497" value="20.125" valueMax="31.0" valueMin="9.25">9, 25–31</number>
