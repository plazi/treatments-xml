<paragraph id="8BDB1745FFF5FFFCFF41193BFCF7DA13" blockId="28.[151,1437,1122,1725]" box="[151,778,1303,1329]" pageId="28" pageNumber="29">
<taxonomicName id="4C646CC6FFF5FFFCFF41193BFE4CDA13" box="[151,433,1303,1329]" class="Insecta" family="Curculionidae" genus="Anisus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="spirorbis" subGenus="Anisus">
