<paragraph id="65043681FFE8FFDAFF23FE2EB9A4FDB7" blockId="12.[151,788,502,565]" box="[151,392,502,529]" pageId="12" pageNumber="163">
<heading id="3E4C81EDFFE8FFDAFF23FE2EB9A4FDB7" bold="true" box="[151,392,502,529]" fontSize="11" level="1" pageId="12" pageNumber="163" reason="1">
<emphasis id="57CFEA93FFE8FFDAFF23FE2EB9A4FDB7" bold="true" box="[151,392,502,529]" pageId="12" pageNumber="163">
<taxonomicName id="A2BB4D02FFE8FFDAFF23FE2EB93FFDB7" authority="Jaschhof &amp; Jaschhof, 2020" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2020" box="[151,275,502,529]" class="Insecta" family="Cecidomyiidae" genus="Oelandyla" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="163" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="57CFEA93FFE8FFDAFF23FE2EB93FFDB7" bold="true" box="[151,275,502,529]" italics="true" pageId="12" pageNumber="163">Oelandyla</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4CFC57E8FFE8FFDAFEAEFE2FB9A4FDB7" box="[282,392,503,529]" pageId="12" pageNumber="163" rank="genus">gen. nov.</taxonomicNameLabel>
