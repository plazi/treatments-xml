<emphasis id="3285EAA6FFD88A3EFAB3FD39FA19DBDC" box="[1407,1436,664,703]" italics="true" pageId="23" pageNumber="302">C</emphasis>
<collectingCountry id="78E67624FFD88A3EFA69FD39F9CEDBDC" box="[1445,1611,664,703]" name="Vietnam" pageId="23" pageNumber="302">Vietnam</collectingCountry>
