[= 
<location id="052E606FFFD98A3FF745EE8CF6C7C837" LSID="urn:lsid:plazi:treatment:885887A2FFD98A3EF89CEFB2FE0DD1D4:052E606FFFD98A3FF745EE8CF6C7C837" box="[2185,2370,4397,4436]" name="Thailand" pageId="22" pageNumber="301">Thailand</location>
