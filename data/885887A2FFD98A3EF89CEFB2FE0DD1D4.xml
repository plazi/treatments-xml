<collectingRegion id="C235F856FFD88A3EF9B2FD8EF8A4DB35" box="[1662,1825,559,598]" country="China" name="Guangxi" pageId="23" pageNumber="302">Guangxi</collectingRegion>
, 
<collectingRegion id="C235F856FFD88A3EFBDCFDC5FB1CDBE8" box="[1040,1177,612,651]" country="China" name="Hunan" pageId="23" pageNumber="302">Hunan</collectingRegion>
