<collectionCode id="21BBAE10D507B159FC9DA5B5FC63FB2E" box="[893,978,1207,1231]" country="Japan" httpUri="http://grbio.org/cool/dk9x-54ca" name="Invertebrate Collection of the Hokkaido University Museum" pageId="9" pageNumber="22">ICHUM</collectionCode>
