<typeStatus id="D30388319F4BFF8A4D4F5E8A79A3FD5E" box="[381,493,712,738]" pageId="14" pageNumber="15">Holotype</typeStatus>
, 
<specimenCount id="1ABEFD1A9F4BFF8A4DCA5E857A40FD5D" box="[504,526,711,737]" pageId="14" pageNumber="15" type="male">♂</specimenCount>
. Labels: 1) 
<collectingCountry id="74AF76039F4BFF8A4EAB5E8A7ABEFD5E" box="[665,752,712,738]" name="Sri Lanka" pageId="14" pageNumber="15">Ceylon</collectingCountry>
, 
<collectorName id="A14D53459F4BFF8A4EC95E8A7BAAFD5E" box="[763,996,712,738]" pageId="14" pageNumber="15">W. Prov. Labugama</collectorName>
, 24 mls. ESE Colombo, 9.III.62, leg. 
<collectorName id="A14D53459F4BFF8A4CA55EB2790CFCB6" box="[151,322,752,778]" pageId="14" pageNumber="15">A. Perera Loc.</collectorName>
17; 2) 
<taxonomicName id="CBB84D109F4BFF8A4DA75EB27A3FFCB5" box="[405,625,752,777]" class="Insecta" family="Chrysomelidae" genus="Phaelota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="ogloblini" status="sp. nov.">
<emphasis id="3ECCEA819F4BFF8A4DA75EB27A3FFCB5" box="[405,625,752,777]" italics="true" pageId="14" pageNumber="15">Phaelota ogloblini</emphasis>
</taxonomicName>
