<collectingCountry id="3AAEC7CFE055DC75E48494AE0C45363C" box="[665,775,1004,1030]" name="Australia" pageId="43" pageNumber="44">Australia</collectingCountry>
, 
<collectingCountry id="3AAEC7CFE055DC75E50A94AE0C18363C" box="[791,858,1004,1030]" name="Chile" pageId="43" pageNumber="44">Chile</collectingCountry>
, Marion Island, Crozet Islands, 
<collectingCountry id="3AAEC7CFE055DC75E2FA94AE0E143614" name="South Africa" pageId="43" pageNumber="44">South Africa</collectingCountry>
