<collectingCountry id="BCEA761AB644CD2C5B65F9B2FD5EA22D" box="[588,683,1618,1644]" name="Ecuador" pageId="25" pageNumber="26">Ecuador</collectingCountry>
, French Guiana, 
<collectingCountry id="BCEA761AB644CD2C5A41F9B2FC37A22D" box="[872,962,1618,1644]" name="Guyana" pageId="25" pageNumber="26">Guyana</collectingCountry>
, 
<collectingCountry id="BCEA761AB644CD2C5AE5F9B2FBB7A22D" box="[972,1090,1618,1644]" name="Nicaragua" pageId="25" pageNumber="26">Nicaragua</collectingCountry>
