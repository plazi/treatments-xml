, 
<collectingCountry id="BCEA761AB644CD2C59BEF9B2FF19A22D" box="[151,236,1618,1644]" name="Bolivia" pageId="25" pageNumber="26">Bolivia</collectingCountry>
, 
<collectingCountry id="BCEA761AB644CD2C59DEF9B2FECBA22D" box="[247,318,1618,1644]" name="Brazil" pageId="25" pageNumber="26">Brazil</collectingCountry>
, 
<collectingCountry id="BCEA761AB644CD2C5860F9B2FE4FA22D" box="[329,442,1618,1644]" name="Colombia" pageId="25" pageNumber="26">Colombia</collectingCountry>
, 
<collectingCountry id="BCEA761AB644CD2C58ECF9B2FDB4A22D" box="[453,577,1618,1644]" name="Costa Rica" pageId="25" pageNumber="26">Costa Rica</collectingCountry>
