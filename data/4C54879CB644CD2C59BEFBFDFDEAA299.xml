, 
<collectingCountry id="BCEA761AB644CD2C5D9AF9B3FAE9A22C" box="[1203,1308,1619,1645]" name="Paraguay" pageId="25" pageNumber="26">Paraguay</collectingCountry>
, 
<collectingCountry id="BCEA761AB644CD2C5C0EF9B3FAA8A22C" box="[1319,1373,1619,1645]" name="Peru" pageId="25" pageNumber="26">Peru</collectingCountry>
, 
<collectingCountry id="BCEA761AB644CD2C5C4EF9B3FF3BA2D0" name="Uruguay" pageId="25" pageNumber="26">Uruguay</collectingCountry>
, and 
<collectingCountry id="BCEA761AB644CD2C5824F996FE75A2D1" box="[269,384,1654,1680]" name="Venezuela" pageId="25" pageNumber="26">Venezuela</collectingCountry>
(
<bibRefCitation id="A06C4B7BB644CD2C58B9F996FDCCA2D1" author="Metcalf, Z. P." box="[400,569,1654,1680]" pageId="25" pageNumber="26" pagination="1 - 585" refId="ref106666" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
; 
<bibRefCitation id="A06C4B7BB644CD2C5B6EF996FC69A2D1" author="Duffels, J. P. &amp; van der Laan, P. A." box="[583,924,1653,1681]" pageId="25" pageNumber="26" refId="ref104819" refString="Duffels, J. P. &amp; van der Laan, P. A. (1985) Catalogue of the Cicadoidea (Homoptera, Auchenorhyncha) 1956 - 1980. Series Entomologica 34. Dr. W. Junk Publishers, Dordrect, 414 pp." type="book" year="1985">Duffels &amp; van der Laan 1985</bibRefCitation>
; 
<bibRefCitation id="A06C4B7BB644CD2C5A80F996FB2BA2D1" author="Boulard, M. &amp; Martinelli, N. M." box="[937,1246,1654,1680]" pageId="25" pageNumber="26" pagination="219 - 232" refId="ref103499" refString="Boulard, M. &amp; Martinelli, N. M. (2011) Nouvelles cigales neotropicales de la sous-tribu des Guyalinina (Rhynchota, Cicadidae, Cicadinae, Fidicinini). Lambillionea, 61, 219 - 232." type="journal article" year="2011">Boulard &amp; Martinelli 2011</bibRefCitation>
; 
