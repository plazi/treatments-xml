<taxonomicName id="4C4A4D6EFFBD9714692CFADDE822AF58" authority="(Rondani 1868)" baseAuthorityName="Rondani" baseAuthorityYear="1868" box="[1046,1401,1346,1372]" class="Insecta" family="Tephritidae" genus="Rachiptera" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="rubiginosa">
