<taxonomicName id="4C4A4D6EFFBD97146FA2FADDEE83AF58" authority="(Hendel 1914)" authorityName="Norrbom &amp; Sutton &amp; Steck &amp; Monzón" authorityYear="2010" baseAuthorityName="Norrbom &amp; Sutton &amp; Steck &amp; Monzón" baseAuthorityYear="2010" box="[664,984,1346,1372]" class="Insecta" family="Tephritidae" genus="Rachiptera" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="parallela">
