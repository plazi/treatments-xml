<paragraph id="8BF536EDFFBD97146DFCFAF6EDB8AC38" blockId="49.[151,1436,1234,1970]" pageId="49" pageNumber="50">
<taxonomicName id="4C4A4D6EFFBD97146DFCFAF6EC13AF85" box="[198,328,1384,1409]" class="Insecta" family="Tephritidae" genus="Rachiptera" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="genus">
