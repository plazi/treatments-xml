<taxonomicName id="4C4A4D6EFFBD97146F33FA80EE24AF33" authority="(Rondani 1868)" baseAuthorityName="Rondani" baseAuthorityYear="1868" box="[521,895,1309,1335]" class="Insecta" family="Tephritidae" genus="Rachiptera" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="baccharidis">
<emphasis id="B93EEAFFFFBD97146F33FA80EF40AF33" box="[521,539,1310,1335]" italics="true" pageId="49" pageNumber="50">R</emphasis>
. 
<emphasis id="B93EEAFFFFBD97146F16FA80EFEDAF33" box="[556,694,1310,1335]" italics="true" pageId="49" pageNumber="50">baccharidis</emphasis>
(Rondani 1868)
</taxonomicName>
, 
<taxonomicName id="4C4A4D6EFFBD97146EB4FA80E9B5AF33" authority="(Hendel 1914)" baseAuthorityName="Hendel" baseAuthorityYear="1914" box="[910,1262,1309,1335]" class="Insecta" family="Tephritidae" genus="Rachiptera" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="bimaculata">
