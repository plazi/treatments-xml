<collectionCode id="E5C8AE11FFC9FA561AC2F8D2FC79F8DB" box="[921,989,1878,1904]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33074" lsid="urn:lsid:biocol.org:col:33074" name="China Agricultural University" pageId="5" pageNumber="6">CAU</collectionCode>
)
