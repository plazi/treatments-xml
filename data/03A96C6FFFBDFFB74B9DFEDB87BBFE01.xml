<taxonomicName id="4C00A6FAFFBDFFB74840FE6886E6FE7C" authorityName="" authorityYear="1810" box="[842,981,404,429]" class="Magnoliopsida" family="Santalaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Santalales" pageId="11" pageNumber="12" phylum="Magnoliopsida" rank="family">Santalaceae</taxonomicName>
