<taxonomicName id="4C00A6FAFFBDFFB74E8DFE6985EEFE01" class="Magnoliopsida" family="Santalaceae" genus="Santalum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Santalales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="album">
