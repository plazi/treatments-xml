<taxonomicName id="4C00A6FAFFBDFFB74975FE68860AFE7C" box="[639,825,404,429]" class="Magnoliopsida" family="Santalaceae" genus="Santalum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Santalales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="album">
