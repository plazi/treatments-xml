<subSubSection id="93952FDBFFEEFFE3A0C6A093FF95FA57" box="[199,957,1481,1506]" pageId="16" pageNumber="485" type="etymology">
<paragraph id="DB307C50FFEEFFE3A0C6A093FF95FA57" blockId="16.[151,1437,1410,2005]" box="[199,957,1481,1506]" pageId="16" pageNumber="485">
<emphasis id="E9FBA042FFEEFFE3A0C6A093FD66FA57" bold="true" box="[199,334,1481,1506]" pageId="16" pageNumber="485">Etymology.</emphasis>
