Lat. 
<taxonomicName id="4C194D2F6860BF6FFE06FEFA12F4FEF9" authorityName="Ruta &amp; Yoshitomi" authorityYear="2010" box="[400,504,352,377]" class="Insecta" family="Scirtidae" genus="Exochomoscirtes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="globosus">
