<taxonomicName id="09995FB72C05FFCBB99FFCD50683FCEF" authority="Kroyer 1862" authorityName="Kroyer" authorityYear="1862" box="[232,483,772,793]" class="Actinopterygii" family="Liparidae" genus="Careproctus" kingdom="Animalia" order="Scorpaeniformes" pageId="2" pageNumber="700" phylum="Chordata" rank="genus">
