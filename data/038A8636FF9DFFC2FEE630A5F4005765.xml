<taxonomicName id="4C234CA3FF9DFFC0FF733097F4A457D1" authority="Thorei Grateloup, 1847" authorityName="Thorei Grateloup" authorityYear="1847" box="[151,442,440,465]" class="Gastropoda" family="Fasciolariidae" genus="Fusus" kingdom="Animalia" order="Neogastropoda" pageId="40" pageNumber="247" phylum="Mollusca" rank="genus">
