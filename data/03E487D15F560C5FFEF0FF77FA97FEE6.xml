<taxonomicName id="4C4D4D445F560C58FF26FA44FD48FA3B" authority="Tillyard" authorityName="Tillyard, Dinmore" authorityYear="1916" box="[321,651,1449,1474]" class="Insecta" family="Archijassidae" genus="Mesojassus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="416" phylum="Arthropoda" rank="species" species="ipsviciensis">
