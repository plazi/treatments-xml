<taxonomicName id="5BAE01CF33D7E1978DD2944B5A9311C7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147916" class="Insecta" family="Formicidae" genus="Aphaenogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Aphaenogaster swammerdami spinipes Santschi" order="Hymenoptera" pageNumber="123" phylum="Arthropoda" rank="variety" species="Schwammerdami" subGenus="Ichnomyrmex" variety="spinipes">Aphaenogaster (Ichnomyrmex) Schwammerdami For. var. spinipes</taxonomicName>
<taxonomicNameLabel id="CF0B6AEAED73DC553CCB0F503D7A0FF4" rank="variety">n. var.</taxonomicNameLabel>
