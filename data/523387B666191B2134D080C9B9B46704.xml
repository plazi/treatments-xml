<taxonomicName id="1D9A4D2366161B21366484C8B839636E" authority="Keeton, 1959" authorityName="Keeton" authorityYear="1959" box="[273,595,754,774]" class="Diplopoda" family="Xystodesmidae" genus="Brachoria" kingdom="Animalia" order="Polydesmida" pageId="56" pageNumber="873" phylum="Arthropoda" rank="species" species="plecta">
