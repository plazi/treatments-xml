<collectingCountry id="AD86E6FBAF75FFB74F1CFE92FD80FE06" box="[564,637,364,390]" name="China" pageId="9" pageNumber="40">China</collectingCountry>
, 
<geoCoordinate id="B0A5C0ACAF75FFB74FA1FE92FCE6FE06" box="[649,795,364,390]" degrees="33" direction="north" minutes="57" orientation="latitude" pageId="9" pageNumber="40" precision="15" seconds="39" value="33.960835">33°57’39”N</geoCoordinate>
, 
<geoCoordinate id="B0A5C0ACAF75FFB74E0FFE92FC38FE06" box="[807,965,364,390]" degrees="104" direction="east" minutes="19" orientation="longitude" pageId="9" pageNumber="40" precision="15" seconds="45" value="104.32916">104°19’45”E</geoCoordinate>
, 
<date id="A12F80ABAF75FFB74EF9FE92FB4BFE06" box="[977,1206,364,390]" pageId="9" pageNumber="40" value="2005-09-11">11 September 2005</date>
, collected by Z.­W. Song and X.­F. Xue. 
<emphasis id="E7E57A79AF75FFB74F47FE6DFD11FE2D" bold="true" box="[623,748,403,429]" pageId="9" pageNumber="40">
