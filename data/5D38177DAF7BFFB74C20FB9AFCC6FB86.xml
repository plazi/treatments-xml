(
<taxonomicName id="1291DDE8AF75FFB74E50FEBAFC1AFEDE" box="[888,999,324,350]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="9" pageNumber="40" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
), Guan’e Gou, Dangchang County, 
<collectingRegion id="17556889AF75FFB74C43FE92FDD5FE06" box="[363,552,364,390]" country="China" name="Gansu" pageId="9" pageNumber="40">Gansu Province</collectingRegion>
