<taxonomicName id="675AE41BAF1CE85C7194306DCCE867F2" authorityName="Pulawski" authorityYear="2012" baseAuthorityName="Pulawski" baseAuthorityYear="2012" class="Insecta" family="Crabronidae" genus="Larrisson" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="36" pageNumber="71" phylum="Arthropoda" rank="species" species="nedymus" status="comb. n.">Larrisson nedymus</taxonomicName>
