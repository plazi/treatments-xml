, 
<taxonomicName id="B13B258B3B62E16CE8F09C465A8D914C" baseAuthorityName="Reeve" baseAuthorityYear="1848" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="141" pageNumber="142" phylum="Mollusca" rank="species" species="roseatus" subGenus="Mesembrinus">
