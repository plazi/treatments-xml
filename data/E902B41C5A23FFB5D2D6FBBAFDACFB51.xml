<taxonomicName id="A6AB7E895A23FFB5D2AFFBB9FD9AFBE4" authority="Imamura, 1984" authorityName="Imamura" authorityYear="1984" box="[238,632,1081,1108]" class="Arachnida" family="Hygrobatidae" genus="Gondwanabates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="569" phylum="Arthropoda" rank="genus">
