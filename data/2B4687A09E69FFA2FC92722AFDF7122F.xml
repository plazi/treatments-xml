<location id="A630606D9E6AFFA2FEB27600FE90147D" LSID="urn:lsid:plazi:treatment:2B4687A09E69FFA2FC92722AFDF7122F:A630606D9E6AFFA2FEB27600FE90147D" box="[257,358,538,560]" county="Rothschild" municipality="Doherty" name="Lombok" pageId="110" pageNumber="110">Lombok</location>
