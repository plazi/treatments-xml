<location id="A630606D9E6AFFA2FF787622FEC61403" LSID="urn:lsid:plazi:treatment:2B4687A09E69FFA2FC92722AFDF7122F:A630606D9E6AFFA2FF787622FEC61403" box="[203,304,568,590]" county="Rothschild" municipality="Doherty" name="Doherty" pageId="110" pageNumber="110">Doherty</location>
