<specimenCode id="F3499ECD9E6AFFA2FF69764CFE601421" box="[218,406,598,620]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="110" pageNumber="110">AMNH 722006</specimenCode>
