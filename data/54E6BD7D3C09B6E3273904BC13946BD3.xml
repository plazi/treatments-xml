, 
<geoCoordinate id="BC5FDECCE2AE39248EC5FF76149BBA0A" direction="east" orientation="longitude" precision="925" value="102.583336">101°95'E</geoCoordinate>
, 1-28.iv.2006, P. Cechovsky leg. 
<taxonomicName id="64512C746C54750B2ADF4FE82CCFD069" class="Insecta" family="Melolonthidae" genus="Tetraserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetraserica" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">Tetraserica</taxonomicName>
