<taxonomicName id="46D72BC2BF299DF0D79456F97225EF2B" authorityName="Balat" authorityYear="1955" class="insecta" family="Philopteridae" genus="Brueelia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brueelia blagovescenskyi" order="Phthiraptera" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="blagovescenskyi">
