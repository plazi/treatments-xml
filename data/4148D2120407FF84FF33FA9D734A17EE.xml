<geoCoordinate id="ACD505C30407FF84FDADF85774881783" box="[521,666,1946,1972]" degrees="28" direction="west" minutes="28" orientation="longitude" pageId="87" pageNumber="88" precision="15" seconds="30" value="-28.475">28°28’30”W</geoCoordinate>
