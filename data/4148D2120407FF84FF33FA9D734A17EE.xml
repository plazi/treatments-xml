<specimenCount id="DFE7A88D0407FF84FC9AF857759F1782" box="[830,909,1946,1973]" pageId="87" pageNumber="88" type="generic">
<quantity id="0E19CEE10407FF84FC9AF857759B1782" box="[830,905,1946,1973]" metricMagnitude="2" metricUnit="m" metricValue="8.23" pageId="87" pageNumber="88" unit="m" value="823.0">823 m</quantity>
