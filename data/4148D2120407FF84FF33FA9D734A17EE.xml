<taxonomicName id="0EE118870407FF84FBBEF873735A17EF" authorityName="$ Sladen" authorityYear="1883" box="[1050,1352,1982,2008]" class="Asteroidea" family="Astropectinidae" genus="Astropecten" kingdom="Animalia" order="Paxillosida" pageId="87" pageNumber="88" phylum="Echinodermata" rank="species" species="hermatophilus">
