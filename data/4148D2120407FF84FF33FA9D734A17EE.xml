<quantity id="0E19CEE10407FF84FD62F870752517EE" box="[710,823,1981,2009]" metricMagnitude="2" metricUnit="m" metricValue="1.28" metricValueMax="1.65" metricValueMin="0.9099999999999999" pageId="87" pageNumber="88" unit="m" value="128.0" valueMax="165.0" valueMin="91.0">91–165 m</quantity>
