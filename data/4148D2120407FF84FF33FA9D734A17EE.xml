<geoCoordinate id="ACD505C30407FF84FED6F85777EF1783" box="[370,509,1946,1972]" degrees="38" direction="north" minutes="38" orientation="latitude" pageId="87" pageNumber="88" precision="15" seconds="00" value="38.633335">38°38’00”N</geoCoordinate>
