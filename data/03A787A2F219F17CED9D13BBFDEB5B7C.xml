<specimenCode id="DBA89ECFF21AF17CEF071458FE9B5F39" box="[93,282,564,586]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="46" pageNumber="46">AMNH 657018</specimenCode>
