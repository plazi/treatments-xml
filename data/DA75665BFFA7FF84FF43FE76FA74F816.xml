<typeStatus id="8D6769EFFFA7FF84FDB4F8F3FD74F8CE" box="[608,656,1843,1869]" pageId="2" pageNumber="168">type</typeStatus>
locality in 
<collectingRegion id="901819AFFFA7FF84FCDBF8F3FC29F8CE" box="[783,973,1843,1869]" country="China" name="Hainan" pageId="2" pageNumber="168">Hainan Province</collectingRegion>
, 
<collectingCountry id="2ACB97DDFFA7FF84FC0DF8F3FBC4F8CE" box="[985,1056,1843,1869]" name="China" pageId="2" pageNumber="168">China</collectingCountry>
