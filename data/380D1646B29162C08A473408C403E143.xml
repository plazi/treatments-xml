<taxonomicName id="E23C298ECCA9BCAF340560247BF9F773" class="Polychaeta" family="Polynoidae" genus="Harmothoe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Harmothoe" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="genus">Harmothoe</taxonomicName>
