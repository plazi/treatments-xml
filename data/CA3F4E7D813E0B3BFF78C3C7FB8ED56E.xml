MNHN 8686; 
<quantity id="856E528E813E0B35FB76C072FB43D04B" box="[1177,1270,1562,1584]" metricMagnitude="-2" metricUnit="m" metricValue="5.47" pageId="38" pageNumber="189" unit="mm" value="54.7">54.7 mm</quantity>
SL, 
<collectingCountry id="3A81BFFB813E0B35FAC0C072FA2DD04B" box="[1327,1432,1562,1584]" name="Sri Lanka" pageId="38" pageNumber="189">Sri Lanka</collectingCountry>
