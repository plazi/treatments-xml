</taxonomicName>
x 
<taxonomicName id="4C0FEBA24906FF92FB44EB03FB013626" baseAuthorityName="Banks" baseAuthorityYear="1939" box="[1168,1255,549,573]" class="Insecta" family="Perlidae" genus="Phanoperla" kingdom="Animalia" order="Plecoptera" pageId="5" pageNumber="16" phylum="Arthropoda" rank="species" species="anomala">
<emphasis id="B97B4C334906FF92FB44EB03FB013626" box="[1168,1255,549,573]" italics="true" pageId="5" pageNumber="16">anomala</emphasis>
