<taxonomicName id="4C0FEBA24906FF92FC63EB03FB943626" baseAuthorityName="Banks" baseAuthorityYear="1924" box="[951,1138,549,573]" class="Insecta" family="Perlidae" genus="Phanoperla" kingdom="Animalia" order="Plecoptera" pageId="5" pageNumber="16" phylum="Arthropoda" rank="species" species="bakeri">
