(
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[698,809,1187,1213]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="214" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
