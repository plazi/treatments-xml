<number box="[451,464,1013,1036]" pageId="17" pageNumber="18" value="4.0">4</number>
major spines on antero-dorsal and 
<number box="[841,854,1013,1036]" pageId="17" pageNumber="18" value="4.0">4</number>
or 
<number box="[890,903,1013,1036]" pageId="17" pageNumber="18" value="3.0">3</number>
