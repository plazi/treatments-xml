, 
<taxonomicName class="Arachnida" family="Chthoniidae" genus="Paraliochthonius" higherTaxonomySource="CoL" kingdom="Animalia" order="Pseudoscorpiones" pageId="5" pageNumber="42" phylum="Arthropoda" rank="genus">Paraliochthonius</taxonomicName>
und 
<taxonomicName class="Arachnida" family="Brachychthoniidae" genus="Poecilochthonius" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" phylum="Arthropoda" rank="genus">Poecilochthonius</taxonomicName>
<normalizedToken originalValue="vorläufig" pageId="5" pageNumber="42">vorlaeufig</normalizedToken>
