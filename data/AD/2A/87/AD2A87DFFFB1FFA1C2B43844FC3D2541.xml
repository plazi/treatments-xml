<taxonomicName authority="Boulay &amp; Pierrat (1900: 128)" authorityName="Boulay &amp; Pierrat" authorityPageNumber="128" authorityYear="1900" box="[311,891,1081,1107]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="59" phylum="Tracheophyta" rank="species" species="multibracteatus">
