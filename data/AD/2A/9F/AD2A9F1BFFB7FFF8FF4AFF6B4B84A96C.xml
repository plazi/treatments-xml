<quantity box="[1353,1431,1597,1621]" metricMagnitude="2" metricUnit="m" metricValue="7.8" pageId="13" pageNumber="400" unit="m" value="780.0">780 m</quantity>
, 
<date box="[151,289,1632,1657]" pageId="13" pageNumber="400" value="1986-08-21">
