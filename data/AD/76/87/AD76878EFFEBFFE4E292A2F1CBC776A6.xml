<caption pageId="5" pageNumber="306" startId="5.[151,250,1864,1888]" targetBox="[151,1436,786,1829]" targetPageId="5">
<paragraph blockId="5.[151,1437,1864,1924]" pageId="5" pageNumber="306">
<emphasis bold="true" box="[151,271,1864,1888]" pageId="5" pageNumber="306">FIGURE 3</emphasis>
. 
<taxonomicName authority="(Cockerell, 1914)" baseAuthorityName="Cockerell" baseAuthorityYear="1914" box="[284,695,1864,1888]" class="Insecta" family="Chrysopidae" genus="Archaeochrysa" kingdom="Animalia" order="Neuroptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="species" species="fracta">
