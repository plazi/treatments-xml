<location LSID="urn:lsid:plazi:treatment:AD76878EFFE4FFE9E292A100CDAA7493:20006043FFE4FFE8E3AAA78BCCDD726D" box="[431,669,1517,1543]" country="United States of America" county="Teller County" municipality="Florissant" name="Florissant Formation" pageId="10" pageNumber="311" stateProvince="Colorado">Florissant Formation</location>
