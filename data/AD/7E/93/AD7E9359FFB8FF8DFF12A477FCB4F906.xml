(mean = 
<quantity box="[239,328,1496,1517]" metricMagnitude="-4" metricUnit="m" metricValue="7.7" pageId="6" pageNumber="1022" unit="mm" value="0.77">0.77 mm</quantity>
, 
<emphasis box="[340,352,1497,1518]" italics="true" pageId="6" pageNumber="1022">n</emphasis>
