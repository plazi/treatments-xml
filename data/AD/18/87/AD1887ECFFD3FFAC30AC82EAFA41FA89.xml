<collectingCountry box="[1138,1235,934,955]" name="Indonesia" pageId="28" pageNumber="329">Indonesia</collectingCountry>
, 
<collectingCountry box="[1245,1302,934,955]" name="China" pageId="28" pageNumber="329">China</collectingCountry>
