<accessionNumber box="[595,741,779,806]" httpUri="http://www.ncbi.nlm.nih.gov/protein/PMU26835" pageId="64" pageNumber="65">
<collectionCode box="[595,661,779,805]" country="Sweden" httpUri="http://grbio.org/cool/32ji-dbbk" name="Paleontological Museum of Uppsala" pageId="64" pageNumber="65">PMU</collectionCode>
