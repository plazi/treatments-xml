<taxonomicName authority="(Balashov, 1975)" authorityName="Kröger" authorityYear="2013" baseAuthorityName="Balashov" baseAuthorityYear="1975" class="Cephalopoda" family="Eriditidae" genus="Nathorstoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthocerida" pageId="37" pageNumber="38" phylum="Mollusca" rank="species" species="bacotense" status="comb. nov.">
