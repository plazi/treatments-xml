(
<bibRefCitation author="Balashov E. G." box="[197,381,1732,1759]" pageId="37" pageNumber="38" pagination="63 - 101" refId="ref46594" refString="Balashov E. G. 1975. Cefalopody molodovskogo i kitayogorodskogo gorizontov Podolii. Voprosy Paleontologii 7: 63 - 101." type="journal article" year="1975">Balashov, 1975</bibRefCitation>
)
</taxonomicName>
</taxonomicName>
<taxonomicNameLabel box="[396,526,1732,1759]" pageId="37" pageNumber="38" rank="species">comb. nov.</taxonomicNameLabel>
</paragraph>
