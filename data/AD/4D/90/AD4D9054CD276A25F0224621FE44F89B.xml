<paragraph blockId="67.[189,1398,545,642]" pageId="67" pageNumber="68">
<taxonomicName authorityName="Strand" authorityYear="1934" box="[189,314,546,571]" class="Cephalopoda" family="Tarphycerida (awaiting allocation)" genus="Tyrioceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tarphycerida" pageId="67" pageNumber="68" phylum="Mollusca" rank="genus">
