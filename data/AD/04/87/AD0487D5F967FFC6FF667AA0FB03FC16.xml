<number box="[830,863,360,383]" pageId="2" pageNumber="141" value="0.4">0.4</number>
long and 
<number box="[972,1006,360,383]" pageId="2" pageNumber="141" value="0.2">0.2</number>
wide, 
<number box="[1079,1112,360,383]" pageId="2" pageNumber="141" value="0.3">0.3</number>
from margin; II, 
<number box="[1301,1334,360,383]" pageId="2" pageNumber="141" value="0.4">0.4</number>
long and 
<number box="[151,184,394,417]" pageId="2" pageNumber="141" value="0.2">0.2</number>
wide, 
<number box="[258,291,394,417]" pageId="2" pageNumber="141" value="0.5">0.5</number>
from margin; III, 
