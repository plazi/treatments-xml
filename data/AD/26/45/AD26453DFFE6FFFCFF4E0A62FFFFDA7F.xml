b= 
<quantity box="[923,1037,980,1005]" metricMagnitude="-1" metricUnit="m" metricValue="1.2065" metricValueMax="1.3716" metricValueMin="1.0413999999999999" pageId="70" pageNumber="71" unit="in" value="4.75" valueMax="5.4" valueMin="4.1">4.1–5.4 in</quantity>
<taxonomicName authorityName="Gerlach" authorityYear="1958" baseAuthorityName="Schuurmans Stekhoven &amp; De Coninck" baseAuthorityYear="1933" box="[1043,1158,980,1004]" class="Adenophorea" family="Leptolaimidae" genus="Leptolaimus" kingdom="Animalia" order="Araeolaimida" pageId="70" pageNumber="71" phylum="Nematoda" rank="species" species="elegans">
<emphasis box="[1043,1158,980,1004]" italics="true" pageId="70" pageNumber="71">L. elegans</emphasis>
