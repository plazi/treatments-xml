<subSubSection pageId="26" pageNumber="27" type="etymology">
<paragraph blockId="26.[151,1437,151,285]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[199,334,223,248]" pageId="26" pageNumber="27">Etymology.</emphasis>
