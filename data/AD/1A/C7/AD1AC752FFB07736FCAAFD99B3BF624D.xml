<collectionCode box="[1001,1077,1272,1293]" pageId="35" pageNumber="684">IZIKO</collectionCode>
, South African Museum, 
<collectingRegion box="[1383,1441,1272,1293]" country="South Africa" name="Western Cape" pageId="35" pageNumber="684">Cape</collectingRegion>
