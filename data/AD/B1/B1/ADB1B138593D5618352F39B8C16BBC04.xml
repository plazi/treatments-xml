<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182070" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole rhytifera Wilson" lsidName-HNS="Pheidole rhytifera Wilson" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="rhytifera">rhytifera</taxonomicName>
