<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182009" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole antillana Forel" lsidName-HNS="Pheidole antillana Forel" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="antillana">antillana</taxonomicName>
of the Lesser Antilles and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182012" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole avia Forel" lsidName-HNS="Pheidole avia Forel" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="avia">avia</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182018" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole bucculenta Forel" lsidName-HNS="Pheidole bucculenta Forel" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="bucculenta">bucculenta</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33780" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hetschkoi Emery" lsidName-HNS="Pheidole hetschkoi Emery" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="hetschkoi">hetschkoi</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182042" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole heyeri Forel" lsidName-HNS="Pheidole heyeri Forel" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="heyeri">heyeri</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182045" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hortonae Wilson" lsidName-HNS="Pheidole hortonae Wilson" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="hortonae">hortonae</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181992" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole mamore Mann" lsidName-HNS="Pheidole mamore Mann" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="mamore">mamore</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182067" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole praeses Wilson" lsidName-HNS="Pheidole praeses Wilson" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="praeses">praeses</taxonomicName>
