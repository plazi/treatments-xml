<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182076" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole sarcina Forel" lsidName-HNS="Pheidole sarcina Forel" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="sarcina">sarcina</taxonomicName>
