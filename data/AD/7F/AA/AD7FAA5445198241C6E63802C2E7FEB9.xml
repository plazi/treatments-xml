<number box="[1041,1082,1735,1759]" pageId="1" pageNumber="2" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
): 
<number box="[1104,1218,1735,1759]" pageId="1" pageNumber="2" value="23.73" valueMax="24.46" valueMin="23.0">23–24, 46</number>
– 
<number box="[1233,1303,1735,1759]" pageId="1" pageNumber="2" value="48.3">48, 30</number>
– 
<number pageId="1" pageNumber="2" value="47.385000000000005" valueMax="62.18" valueMin="32.59">32, 59–62, 18</number>
