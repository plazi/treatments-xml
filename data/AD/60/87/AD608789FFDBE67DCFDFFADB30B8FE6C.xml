<typeStatus box="[151,201,1365,1386]" pageId="4" pageNumber="265">Type</typeStatus>
species: 
<taxonomicName authority="Fabricius, 1781" authorityName="Fabricius" authorityYear="1781" box="[294,678,1364,1386]" class="Insecta" family="Buprestidae" genus="Buprestis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="265" phylum="Arthropoda" rank="species" species="cyanicornis">
<emphasis box="[294,513,1365,1386]" italics="true" pageId="4" pageNumber="265">Buprestis cyanicornis</emphasis>
Fabricius, 1781
</taxonomicName>
(currently 
<taxonomicName authority="(Scopoli, 1772))" authorityName="" authorityYear="1772" baseAuthorityName="Scopoli" baseAuthorityYear="1772" box="[791,1313,1364,1386]" class="Insecta" family="Buprestidae" genus="Anthaxia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="265" phylum="Arthropoda" rank="species" species="hungarica" subGenus="Cratomerus">
<emphasis box="[791,883,1364,1386]" italics="true" pageId="4" pageNumber="265">Anthaxia</emphasis>
(
<emphasis box="[897,1018,1364,1385]" italics="true" pageId="4" pageNumber="265">Cratomerus</emphasis>
) 
<emphasis box="[1033,1137,1364,1386]" italics="true" pageId="4" pageNumber="265">hungarica</emphasis>
(Scopoli, 1772))
</taxonomicName>
; subsequent designation by 
<bibRefCitation author="Duponchel, P. A. J." box="[354,549,1394,1416]" pageId="4" pageNumber="265" refId="ref11995" refString="Duponchel, P. A. J. (1844) In: D'Orbigny, C. (ed.), Dictionnaire universel d'histoire naturelle Tome 4 (C-DIC). Bureau principal de l'editeur, Paris, 752 pp." type="book" year="1844">Duponchel, (1844)</bibRefCitation>
