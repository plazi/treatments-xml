<subSubSection pageId="24" pageNumber="25" type="materials_examined">
<paragraph blockId="24.[151,1436,151,717]" pageId="24" pageNumber="25">
<emphasis bold="true" box="[199,425,295,320]" pageId="24" pageNumber="25">Material examined</emphasis>
(n= 
<number box="[474,489,296,320]" pageId="24" pageNumber="25" value="4.0">4</number>
). 
