<emphasis bold="true" box="[151,968,798,824]" pageId="9" pageNumber="51">
<taxonomicName authority="Smith 1852" authorityName="Smith" authorityYear="1852" box="[151,510,798,824]" class="Insecta" family="Vespidae" genus="Icaria" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="51" phylum="Arthropoda" rank="species" species="variegata">
