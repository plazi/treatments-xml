(Smith 
<number box="[442,502,798,824]" pageId="9" pageNumber="51" value="1852.0">1852</number>
)
</taxonomicName>
[
<taxonomicName authority="Fabricius 1804" authorityName="Fabricius" authorityYear="1804" box="[528,960,798,824]" class="Insecta" family="Vespidae" genus="Ropalidia" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="51" phylum="Arthropoda" rank="species" species="fasciata">
