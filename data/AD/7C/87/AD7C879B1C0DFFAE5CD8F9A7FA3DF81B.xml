<emphasis bold="true" box="[151,308,1553,1579]" italics="true" pageId="12" pageNumber="54">Vespa ducalis</emphasis>
Smith 
<number box="[393,450,1552,1578]" pageId="12" pageNumber="54" value="1852.0">1852</number>
</taxonomicName>
[
<taxonomicName authority="Smith 1852" authorityName="Smith" authorityYear="1852" box="[466,766,1552,1579]" class="Insecta" family="Vespidae" genus="Vespa" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="54" phylum="Arthropoda" rank="species" species="ducalis">
