<number id="C805046FFFA4B56D630433DA26F5FF37" box="[893,967,188,213]" pageId="4" pageNumber="216" value="15.5" valueMax="16.0" valueMin="15.0">15−16</number>
; 
<number id="C805046FFFA4B56D63AA33DA212CFF36" box="[979,1054,188,212]" pageId="4" pageNumber="216" value="39.5" valueMax="40.0" valueMin="39.0">39−40</number>
