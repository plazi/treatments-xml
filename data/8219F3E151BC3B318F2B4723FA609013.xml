<geoCoordinate id="84BD53F8327278E0751CB49735620870" direction="east" orientation="longitude" precision="925" value="145.46666">145°28'E</geoCoordinate>
<normalizedToken id="6332CC1AA8486A4CE34A44F441CE006D" originalValue="±">+/-</normalizedToken>
