<paragraph id="50C4C5AD9815A94673786316D6E142D5" pageId="4" pageNumber="43">
<taxonomicName id="9223C49E22A9B82A936ED249E3B334E7" authorityName="Kenfack &amp; Nguema" authorityYear="2019" class="Magnoliopsida" family="Lecythidaceae" genus="Rhaptopetalum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rhaptopetalum rabiense" order="Ericales" pageId="4" pageNumber="43" phylum="Tracheophyta" rank="species" species="rabiense">
