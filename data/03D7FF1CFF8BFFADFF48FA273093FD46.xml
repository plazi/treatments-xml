<paragraph id="8BC14E0AFF8BFFADFF48F9503093FD46" blockId="64.[192,1440,574,694]" pageId="64" pageNumber="63">
<emphasis id="B90A9218FF8BFFADFF48F9503074FDA6" bold="true" box="[192,264,574,598]" pageId="64" pageNumber="63">Note.</emphasis>
<taxonomicName id="4C7E3589FF8BFFADFE99F9503376FDA6" authorityName="Harold" authorityYear="1868" box="[273,522,574,598]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="64" pageNumber="63" phylum="Arthropoda" rank="species" species="podagricus">
