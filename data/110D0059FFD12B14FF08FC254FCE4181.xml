with clypeus), carapace 
<number id="2F6CA368FFD12B14FB0AFAB948F34376" box="[1173,1222,1302,1326]" pageId="10" pageNumber="211" value="1.91">1.91</number>
long, 
<number id="2F6CA368FFD12B14FA81FAB9497A4376" box="[1310,1359,1302,1326]" pageId="10" pageNumber="211" value="2.09">2.09</number>
