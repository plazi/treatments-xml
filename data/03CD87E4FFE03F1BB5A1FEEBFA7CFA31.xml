<paragraph id="8BDB36F2FFE03F1BB518FE06FC09FDB4" blockId="28.[806,1421,381,526]" pageId="28" pageNumber="579">
<materialsCitation id="3B0C3CAFFFE03F1BB518FE06FC09FDB4" collectionCode="AMNH, R" pageId="28" pageNumber="579" specimenCode="FR 4804" specimenCount="1" typeStatus="holotype">
