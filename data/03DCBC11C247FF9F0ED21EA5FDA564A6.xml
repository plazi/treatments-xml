<quantity id="4C8DA0E2C246FFA00FCE1A1DFE036243" box="[395,498,1375,1401]" metricMagnitude="3" metricUnit="m" metricValue="2.572" pageId="31" pageNumber="32" unit="m" value="2572.0">2572 m</quantity>
, 
<collectionCode id="ED6495C2C246FFA00C441A1DFD936243" box="[513,610,1375,1401]" pageId="31" pageNumber="32">ALVIN</collectionCode>
Dive 
