<taxonomicName id="4C204D1298495A4BDAB2FB5CD11CFB11" authority="J. A. Allen, 1917: 448" authorityName="J. A. Allen" authorityPageNumber="448" authorityYear="1917" box="[211,777,1239,1265]" class="Mammalia" family="Vespertilionidae" genus="Glauconycteris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="85" pageNumber="86" phylum="Chordata" rank="species" species="humeralis">
