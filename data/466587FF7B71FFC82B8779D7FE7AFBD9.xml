<taxonomicName id="09CC4D6A7B71FFCE29F97E2EFC18FAF4" authority="Gardner" authorityName="Gardner" box="[675,1017,1357,1383]" class="Magnoliopsida" family="Chloranthaceae" genus="Sarcandra" kingdom="Plantae" order="Chloranthales" pageId="2" pageNumber="115" phylum="Tracheophyta" rank="species" species="chloranthoides">
<emphasis id="FCB8EAFB7B71FFCE29F97E2EFC7BFAF4" box="[675,922,1357,1383]" italics="true" pageId="2" pageNumber="115">Sarcandra chloranthoides</emphasis>
