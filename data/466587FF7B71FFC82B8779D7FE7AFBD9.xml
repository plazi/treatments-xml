, 
<taxonomicName id="09CC4D6A7B71FFCE283D7E0EFB54FA14" authority="M. P. Nayar" authorityName="M. P. Nayar" box="[871,1205,1389,1415]" class="Magnoliopsida" family="Melastomataceae" genus="Sonerila" kingdom="Plantae" order="Myrtales" pageId="2" pageNumber="115" phylum="Tracheophyta" rank="species" species="sadasivanii">
