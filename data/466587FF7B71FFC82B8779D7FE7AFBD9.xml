<taxonomicName id="09CC4D6A7B71FFCE29907FCCFC92FB7B" authority="(Gamble) N. C. Nair" authorityName="N. C. Nair" baseAuthorityName="Gamble" class="Magnoliopsida" family="Meliaceae" genus="Aglaia" kingdom="Plantae" order="Sapindales" pageId="2" pageNumber="115" phylum="Tracheophyta" rank="variety" species="elaeagnoidea" variety="bourdillonii">
