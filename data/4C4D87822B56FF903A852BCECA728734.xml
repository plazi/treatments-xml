<taxonomicName id="03E44D172B55FF9039B12C7DCC808431" box="[931,996,357,382]" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" kingdom="Plantae" order="Rosales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="genus">
