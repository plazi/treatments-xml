<taxonomicName id="03E44D172B55FF9038FD2C7DCC248431" box="[751,832,357,382]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="genus">
