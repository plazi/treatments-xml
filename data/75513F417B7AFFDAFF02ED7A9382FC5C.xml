<subSubSection id="B5E2DDDC7B7DFFDAFF37E950927AFDBC" pageId="12" pageNumber="13" type="diagnosis">
<paragraph id="FD478E577B7DFFDAFF37E950927AFDBC" blockId="12.[140,1108,307,932]" pageId="12" pageNumber="13">
<emphasis id="CF8C52457B7DFFDAFF37E9509015FE6B" bold="true" box="[185,312,377,403]" pageId="12" pageNumber="13">
<emphasis id="CF8C52457B7DFFDAFF37E950901EFE6B" box="[185,307,377,403]" inLineHeading="true" pageId="12" pageNumber="13" reason="1">Diagnosis</emphasis>
.
</emphasis>
<taxonomicName id="3AF8F5D47B7DFFDAFECDE952931EFE6C" baseAuthorityName="Herrich-Schaffer" baseAuthorityYear="1852" box="[323,563,378,404]" class="Insecta" family="Noctuidae" genus="Lasionycta" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="skraelingia">
