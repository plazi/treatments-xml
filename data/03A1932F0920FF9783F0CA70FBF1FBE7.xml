<taxonomicName id="4C0859BA0920FF978765CC5CFB3DFD85" authorityName="Miller and Wolfe" authorityYear="2018" box="[1092,1163,565,587]" class="Insecta" family="Dytiscidae" genus="Desmopachria" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="101" phylum="Arthropoda" rank="species" species="delongi">
