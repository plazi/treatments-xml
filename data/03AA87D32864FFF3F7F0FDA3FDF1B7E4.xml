<paragraph id="8BBC36C52864FFF3F7F0FD09FDECB299" blockId="36.[151,1437,654,2011]" box="[151,591,654,679]" pageId="36" pageNumber="37">
<taxonomicName id="4C034D462864FFF3F7F0FD09FE8BB299" box="[151,296,654,679]" class="Insecta" family="Papilionidae" kingdom="Animalia" order="Lepidoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="family">Papilionidae</taxonomicName>
