<subSubSection id="B6C340E7FF8DFFDDFF39EE6FDA8B4AEC" pageId="10" pageNumber="215" type="diagnosis">
<paragraph id="FE66136CFF8DFFDDFF39EE6FDA8B4AEC" blockId="10.[151,1437,1685,2036]" pageId="10" pageNumber="215">
<emphasis id="CCADCF7EFF8DFFDDFF39EE6FDA134AC7" bold="true" box="[199,314,1901,1927]" pageId="10" pageNumber="215">Diagnosis</emphasis>
