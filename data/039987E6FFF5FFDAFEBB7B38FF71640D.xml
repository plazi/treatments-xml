</taxonomicName>
, 
<taxonomicName id="4C304D73FFF5FFD9FB5E7E7CFA326510" authorityName="Wheeler" authorityYear="1919" baseAuthorityName="Emery" baseAuthorityYear="1895" box="[1277,1436,796,817]" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="902" phylum="Arthropoda" rank="species" species="californica">
<emphasis id="B944EAE2FFF5FFD9FB5E7E7CFA326510" box="[1277,1436,796,817]" italics="true" pageId="9" pageNumber="902">C. californica</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C304D73FFF5FFD9FC987E5BFC74656E" box="[827,986,826,848]" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="902" phylum="Arthropoda" rank="species" species="cedrosensis">
