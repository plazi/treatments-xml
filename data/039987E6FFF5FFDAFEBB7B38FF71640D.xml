<taxonomicName id="4C304D73FFF5FFD9FC467E7CFB436510" authorityName="Buren" authorityYear="1968" box="[997,1261,796,817]" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="902" phylum="Arthropoda" rank="species" species="browni">
