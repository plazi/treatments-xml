<taxonomicName id="4C304D73FFF5FFD9FC447E5BFBE76571" baseAuthorityName="Fitch" baseAuthorityYear="1855" box="[999,1097,827,848]" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="902" phylum="Arthropoda" rank="species" species="cerasi">
<emphasis id="B944EAE2FFF5FFD9FC447E5BFBE76571" box="[999,1097,827,848]" italics="true" pageId="9" pageNumber="902">C. cerasi</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C304D73FFF5FFD9FBF57E5BFB09656E" authorityName="Buren" authorityYear="1968" box="[1110,1191,826,848]" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="902" phylum="Arthropoda" rank="species" species="colei">
<emphasis id="B944EAE2FFF5FFD9FBF57E5BFB09656E" box="[1110,1191,826,848]" italics="true" pageId="9" pageNumber="902">C. colei</emphasis>
</taxonomicName>
