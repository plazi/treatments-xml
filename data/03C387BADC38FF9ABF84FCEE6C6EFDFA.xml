<taxonomicName id="4C6A4D2FDC3AFF9ABC85FE2F6BFAFE5D" box="[723,932,443,468]" class="Magnoliopsida" family="Scrophulariaceae" genus="Eremophila" kingdom="Plantae" order="Lamiales" pageId="15" pageNumber="448" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B91EEABEDC3AFF9ABC85FE2F6B0EFE5A" box="[723,848,445,468]" italics="true" pageId="15" pageNumber="448">Eremophila</emphasis>
