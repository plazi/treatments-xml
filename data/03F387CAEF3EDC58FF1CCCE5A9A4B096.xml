<taxonomicName id="4C5A4D5FEF3CDC58FDA6CB6DABC0B0BA" authorityName="Vilkamaa &amp; Halenius &amp; Ševčík" authorityYear="2019" box="[557,644,622,647]" class="Insecta" family="Sciaridae" genus="Pseudoaerumnosa" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="exacuta">
