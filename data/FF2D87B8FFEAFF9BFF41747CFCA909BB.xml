<taxonomicName id="B0844D2DFFF4FF9BFFE77646FE85095B" box="[113,334,1578,1604]" class="Trilobita" family="Ellipsocephalidae" genus="Ellipsocephalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptychopariida" pageId="16" pageNumber="521" phylum="Arthropoda" rank="species" species="kiaeri">
