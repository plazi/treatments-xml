<taxonomicName id="4C5B4D086C6DFFF4B008FA33EE35FA0C" authorityName="Giraud" authorityYear="1861" box="[1104,1335,1439,1465]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="manicata">
