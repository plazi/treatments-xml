<taxonomicName id="4C5B4D086C6DFFF4B545FA68EAA8FA68" authorityName="Giraud" authorityYear="1861" box="[285,426,1476,1501]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="manicata">
