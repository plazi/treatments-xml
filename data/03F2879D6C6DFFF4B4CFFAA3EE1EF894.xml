<collectingCountry id="F34C761B6C6DFFF4B0F4FA6FEE0CFA68" box="[1196,1294,1475,1501]" name="Lebanon" pageId="75" pageNumber="76">Lebanon</collectingCountry>
females.” Mount 
<collectingCountry id="F34C761B6C6DFFF4B4B4FA4BEA50F9B4" box="[236,338,1511,1537]" name="Lebanon" pageId="75" pageNumber="76">Lebanon</collectingCountry>
