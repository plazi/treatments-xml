5. 
<taxonomicName id="52C3EE17F74B38647AE05958CD221AFA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143364" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName-HNS="Polyrhachis nudatus Smith" order="Hymenoptera" pageId="4" pageNumber="71" phylum="Arthropoda" rank="species" species="nudatus">Polyrhachis nudatus</taxonomicName>
