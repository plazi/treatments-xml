<taxonomicName id="C77CB910BE7A2A28475BDCC9424EE051" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33642" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole calens Forel" lsidName-HNS="Pheidole calens Forel" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="calens">calens</taxonomicName>
