, 
<taxonomicName id="EEC1E0F4B85FADF58FD09719954FE41E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181972" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole cavigenis Wheeler" lsidName-HNS="Pheidole cavigenis Wheeler" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="cavigenis">cavigenis</taxonomicName>
, 
<taxonomicName id="053CE29C02A796E4A98A48E3318BD24B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33670" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole clementensis Gregg" lsidName-HNS="Pheidole clementensis Gregg" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="clementensis">clementensis</taxonomicName>
, 
<taxonomicName id="9BC7B032A9A6693487EC0B40CD257069" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33691" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole creightoni Gregg" lsidName-HNS="Pheidole creightoni Gregg" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="creightoni">creightoni</taxonomicName>
, 
<taxonomicName id="C485DA6BE372E72B1FBC27DF53FAE6BC" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181973" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hoplitica Wilson" lsidName-HNS="Pheidole hoplitica Wilson" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="hoplitica">hoplitica</taxonomicName>
, 
<taxonomicName id="26062E58EE50A0BAE5166C6C9980EA32" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33833" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole littoralis Cole" lsidName-HNS="Pheidole littoralis Cole" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="littoralis">littoralis</taxonomicName>
, 
<taxonomicName id="2DE1806488690FC820C2CB291D5C355F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33868" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole micula Wheeler" lsidName-HNS="Pheidole micula Wheeler" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="micula">micula</taxonomicName>
, 
<taxonomicName id="5F1482C2FB7D921895D78F69607A6F98" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33930" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole pilifera (Roger)" lsidName-HNS="Pheidole pilifera (Roger)" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="pilifera">pilifera</taxonomicName>
, 
<taxonomicName id="35264837C5A8F1DD10A1E6EBF3AA5331" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181975" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole polymorpha Wilson" lsidName-HNS="Pheidole polymorpha Wilson" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="polymorpha">polymorpha</taxonomicName>
, 
<taxonomicName id="550C544D28156DE3B7C7C125A8C523E4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33980" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole rugulosa Gregg" lsidName-HNS="Pheidole rugulosa Gregg" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="rugulosa">rugulosa</taxonomicName>
, 
<taxonomicName id="04D859E724CE726F38BA45638F4FC657" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33998" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole senex Gregg" lsidName-HNS="Pheidole senex Gregg" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="senex">senex</taxonomicName>
, 
<taxonomicName id="2322093AB47B73B0286360D6A5A409CB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142583" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole soritis Wheeler" lsidName-HNS="Pheidole soritis Wheeler" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="soritis">soritis</taxonomicName>
, 
<taxonomicName id="173C8E2B3D309AE4311A2D8367A2F4E6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34049" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole tepicana Pergande" lsidName-HNS="Pheidole tepicana Pergande" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="tepicana">tepicana</taxonomicName>
, and 
<taxonomicName id="732EA010933E3717E6DD3315A3DD26AE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181977" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole torosa Wilson" lsidName-HNS="Pheidole torosa Wilson" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="torosa">torosa</taxonomicName>
, which complex is characterized by the following traits. Major: dorsal head surface extensively sculptured; occipital lobes horizontally rugulose (or, in 
<taxonomicName id="406843D30D05228A5ACCC29540539C9D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33651" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole carrolli Naves" lsidName-HNS="Pheidole carrolli Naves" order="Hymenoptera" pageNumber="564" phylum="Arthropoda" rank="species" species="carrolli">carrolli</taxonomicName>
