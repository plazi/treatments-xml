<taxonomicName id="0E7E8C87620B59AA91C5DB582269FD53" class="Magnoliopsida" family="Balsaminaceae" genus="Impatiens" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Impatiens noli-tangere" order="Ericales" pageId="5" pageNumber="46" phylum="Tracheophyta" rank="species" species="noli-tangere">Impatiens noli-tangere</taxonomicName>
