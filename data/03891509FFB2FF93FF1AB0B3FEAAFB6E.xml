<quantity id="4CD809FAFFB0FF93FDA0B1B7FD25FF37" box="[557,748,150,177]" metricMagnitude="-2" metricUnit="m" metricValue="3.1915" metricValueMax="3.34" metricValueMin="3.043" pageId="3" pageNumber="598" unit="mm" value="31.915" valueMax="33.4" valueMin="30.43">30.43–33.40 µm</quantity>
