<taxonomicName id="4C20DF9CFFB2FF91FE72B685FD19F83A" authorityName="Bobrov &amp; Mazei" authorityYear="2021" box="[511,720,1956,1980]" class="Tubulinea" family="Cryptodifflugiidae" genus="Meisterfeldia" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Arcellinida" pageId="1" pageNumber="596" phylum="Amoebozoa" rank="species" species="bitsevi">
