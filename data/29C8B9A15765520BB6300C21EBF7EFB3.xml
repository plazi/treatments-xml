<emphasis id="7C147F820494B8875B0D3ED846EBCAEF" italics="true" pageId="0" pageNumber="407">Hyperolius argentovittis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4DBB0635405A226494111735F8410A95" authorityName="Ahl" authorityYear="1931" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius callichromus" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="species" species="callichromus">
<emphasis id="2EE32D907ED295648CE1F176F4B731E2" italics="true" pageId="0" pageNumber="407">Hyperolius callichromus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4BBA9D5C2BC665C8B978ABD72CE1A881" authorityName="Ahl" authorityYear="1931" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius decipiens" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="species" species="decipiens">
<emphasis id="F34A2BB18D43714048EE6C889F94354F" italics="true" pageId="0" pageNumber="407">Hyperolius decipiens</emphasis>
</taxonomicName>
, 
<taxonomicName id="2EE8B4D34AE5B1952CE6B5B91A5639BF" authorityName="Ahl" authorityYear="1931" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius graueri" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="species" species="graueri">
<emphasis id="B6C618664F02031F0FCD7F9E1A1B0370" italics="true" pageId="0" pageNumber="407">Hyperolius graueri</emphasis>
</taxonomicName>
.
</paragraph>
