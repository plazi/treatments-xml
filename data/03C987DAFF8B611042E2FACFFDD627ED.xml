<heading id="D09781A0FF8B611342E2FACFFE5F2643" bold="true" box="[178,585,1316,1338]" fontSize="9" level="2" pageId="0" pageNumber="1587" reason="3">
<emphasis id="B914EADEFF8B611342E2FACFFE5F2643" bold="true" box="[178,585,1316,1338]" pageId="0" pageNumber="1587">
<taxonomicName id="4C604D4FFF8B611342E2FACFFD1B2643" authorityName="FABRICIUS" authorityYear="1775" box="[178,269,1316,1338]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="1587" phylum="Arthropoda" rank="genus">
<emphasis id="B914EADEFF8B611342E2FACFFD1B2643" bold="true" box="[178,269,1316,1338]" italics="true" pageId="0" pageNumber="1587">Andrena</emphasis>
</taxonomicName>
(subgen.?) 
<taxonomicName id="4C604D4FFF8B611343C0FACFFDE02643" authority="Scheuchl, 2011" authorityName="Scheuchl" authorityYear="2011" box="[400,502,1316,1338]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="1587" phylum="Arthropoda" rank="species" species="ebmerella" status="nov.sp.">
<emphasis id="B914EADEFF8B611343C0FACFFDE02643" bold="true" box="[400,502,1316,1338]" italics="true" pageId="0" pageNumber="1587">ebmerella</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22757A5FF8B611343ADFACEFE5F2643" box="[509,585,1317,1338]" pageId="0" pageNumber="1587" rank="species">nov.sp.</taxonomicNameLabel>
