<quantity id="956A9B7E721BFFE7FEC765BCFE33FB50" box="[322,480,1167,1192]" metricMagnitude="3" metricUnit="m" metricValue="4.225" metricValueMax="4.35" metricValueMin="4.1" pageId="34" pageNumber="35" unit="m" value="4225.0" valueMax="4350.0" valueMin="4100.0">4100–4350 m</quantity>
