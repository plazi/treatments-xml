<quantity id="796A9B33FFEDFFC7B87DFDE9FDF2FDD7" box="[538,623,536,560]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="89" pageNumber="1576" unit="mm" value="5.3">5.3 mm</quantity>
