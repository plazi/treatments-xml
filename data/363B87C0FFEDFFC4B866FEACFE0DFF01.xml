<quantity id="796A9B33FFEDFFC7B97EFDE9FCC4FDD7" box="[793,857,536,560]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="89" pageNumber="1576" unit="mm" value="5.0">5 mm</quantity>
