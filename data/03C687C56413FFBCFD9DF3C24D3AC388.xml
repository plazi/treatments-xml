<quantity id="4C979B366414FFBCFEF5F5CD48FFC5F6" box="[338,397,905,931]" metricMagnitude="1" metricUnit="m" metricValue="5.04698" pageId="36" pageNumber="37" unit="in" value="1987.0">1987</quantity>
