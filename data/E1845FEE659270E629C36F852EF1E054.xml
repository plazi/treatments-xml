<taxonomicName id="07FDF8E1114DFF4D1D12EAE40F57D433" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Calendula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="484" phylum="Tracheophyta" rank="species" species="arvensis">
