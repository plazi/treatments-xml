<quantity id="4CF79B33FFBBFF92C0B9FA6DFD62FA24" box="[704,756,1460,1480]" metricMagnitude="2" metricUnit="m" metricValue="4.25" pageId="84" pageNumber="239" unit="m" value="425.0">425m</quantity>
