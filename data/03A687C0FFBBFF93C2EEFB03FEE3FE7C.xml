<quantity id="4CF79B33FFBBFF92C2CCFA40FF7AFA41" box="[181,236,1433,1453]" metricMagnitude="2" metricUnit="m" metricValue="4.25" pageId="84" pageNumber="239" unit="m" value="425.0">425m</quantity>
