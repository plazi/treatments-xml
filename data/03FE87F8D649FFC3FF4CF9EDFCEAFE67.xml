<taxonomicName id="4C574D6DD64BFFC3FE1FFEF0FDCCFED3" box="[452,606,296,320]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="subcollina">
