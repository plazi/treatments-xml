<taxonomicName id="4C574D6DD64BFFC3FAA6FEDCFE84FED3" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="submirifica">
<emphasis id="B923EAFCD64BFFC3FAA6FEDCFE84FED3" italics="true" pageId="7" pageNumber="46">M. submirifica</emphasis>
</taxonomicName>
