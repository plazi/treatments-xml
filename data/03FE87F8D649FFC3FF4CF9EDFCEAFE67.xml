<emphasis id="B923EAFCD64BFFC3FF02FEDCFAD4FE8F" box="[217,1350,260,284]" italics="true" pageId="7" pageNumber="46">
<taxonomicName id="4C574D6DD64BFFC3FF02FEDCFEE5FE8F" box="[217,375,261,284]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="aconcinna">M. aconcinna</taxonomicName>
