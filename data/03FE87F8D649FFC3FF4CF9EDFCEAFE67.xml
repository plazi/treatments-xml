<taxonomicName id="4C574D6DD64BFFC3FB10FEDCFAD4FE8F" box="[1227,1350,260,284]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="seducta">M. seducta</taxonomicName>
</emphasis>
