<taxonomicName id="4C574D6DD64BFFC3FD4FFEF0FC95FED3" box="[660,775,296,320]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="collina">
