<taxonomicName id="4C574D6DD64BFFC3FC27FEA8FBE0FE1B" box="[1020,1138,368,392]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="collina">
