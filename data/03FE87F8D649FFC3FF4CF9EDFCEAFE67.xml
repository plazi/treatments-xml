<taxonomicName id="4C574D6DD64BFFC3FE59FEDCFE5FFE8F" box="[386,461,260,284]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="chi">M. chi</taxonomicName>
