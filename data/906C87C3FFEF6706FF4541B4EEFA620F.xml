<typeStatus id="C77E8877FFF16706FF6A47F6EEE76249" box="[154,244,749,773]" pageId="17" pageNumber="284">holotype</typeStatus>
and 
<typeStatus id="C77E8877FFF16706FED847F6EF936249" box="[296,384,749,773]" pageId="17" pageNumber="284">paratype</typeStatus>
