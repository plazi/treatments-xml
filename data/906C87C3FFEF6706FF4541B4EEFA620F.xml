<taxonomicName id="DFC54D56FFF06707FEC24089EFF464E4" authorityName="Smith" authorityYear="2008" box="[306,487,1426,1448]" class="Insecta" family="Curculionidae" genus="Aulacus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="283" phylum="Arthropoda" rank="species" species="costaricensis">
