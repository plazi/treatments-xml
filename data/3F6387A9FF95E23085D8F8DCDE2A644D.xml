<taxonomicName id="70CA4D3CFF95E23185D8F8DCDF3B6201" authority="Hutton, 1873" authorityName="Hutton" authorityYear="1873" box="[151,565,1807,1832]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="120" pageNumber="121" phylum="Cnidaria" rank="species" species="simplex">
<emphasis id="85BEEAADFF95E23185D8F8DCDF3B6201" bold="true" box="[151,565,1807,1832]" pageId="120" pageNumber="121">
<emphasis id="85BEEAADFF95E23185D8F8DCDC8E6201" bold="true" box="[151,384,1807,1832]" italics="true" pageId="120" pageNumber="121">Sertularella simplex</emphasis>
