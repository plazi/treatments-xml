<subSubSection id="C3556504FFCDFFBBF3C2F8F52B15737C" pageId="46" pageNumber="47" type="etymology">
<paragraph id="8BF0368FFFCDFFBBF3C2F8F52B15737C" blockId="46.[151,1436,1799,1968]" pageId="46" pageNumber="47">
<emphasis id="B93BEA9DFFCDFFBBF3C2F8F52F157320" box="[151,285,1800,1824]" inLineHeading="true" pageId="46" pageNumber="47" reason="1">
