<taxonomicName id="BE914D02FFDA8416FB0B5613FAC48D82" authorityName="Brunnich" authorityYear="1764" box="[1236,1364,1904,1930]" class="Aves" family="Stercorariidae" genus="Catharacta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Charadriiformes" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
