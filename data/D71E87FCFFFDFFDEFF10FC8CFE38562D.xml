<taxonomicName id="98B74D69FFFDFFDEFE28F8CCFDD9564D" authorityName="Peck &amp; Cook" authorityYear="2014" box="[504,607,1827,1851]" class="Insecta" family="Leiodidae" genus="Zeadolopus" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="26" phylum="Arthropoda" rank="species" species="cubensis">
