<taxonomicName id="4FA3F9D2FFBA440EFB1BF8D8FDE6A68E" authority="Yin &amp; Shen, 2020" authorityName="Yin &amp; Shen" authorityYear="2020" box="[282,546,1897,1923]" class="Insecta" family="Staphylinidae" genus="Sathytes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="huapingensis" status="sp. nov.">
