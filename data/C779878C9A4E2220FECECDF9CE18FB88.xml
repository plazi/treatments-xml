</taxonomicName>
<taxonomicName id="88D04D199A4E2220FECECCBECDAEFC38" authority="Hardy" authorityName="Hardy" box="[264,491,993,1019]" class="Insecta" family="Pipunculidae" genus="Dasydorylas" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="eremita" status="n. comb.">
