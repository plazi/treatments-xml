(
<figureCitation id="1365FFFAFFAEC8454D7A1175FD51DABC" box="[536,655,368,393]" captionStart="Figures 15–18" captionStartId="12.[160,235,1406,1429]" captionTargetBox="[163,1131,142,1389]" captionTargetId="figure@12.[163,1153,142,1389]" captionTargetPageId="12" captionText="Figures 15–18. Zaphanta adults; a = dorsal, b = ventral. 15. Z. stiletto Ƌ holotype, Brazil, Rondônia, Calama, Rio Madeira (NHMUK). 16. Z. machaera Ƌ holotype, Brazil, Mato Grosso, Chapada dos Guimarães, 800 m (VOB). 17. Z. machaera ♀, Brazil, Mato Grosso, Chapada dos Guimarães, 800 m (VOB). 18. Z. beckeri Ƌ holotype, Brazil, Rondônia, Ariquemes, 180 m (VOB). Scale bar = 1 cm." figureDoi="http://doi.org/10.5281/zenodo.3673473" httpUri="https://zenodo.org/record/3673473/files/figure.png" pageId="25" pageNumber="1233">Figures 16</figureCitation>
, 17, 33, 42) 
<uri id="FFCFEF7DFFAEC8454E6F1197FBC8DA9E" box="[269,1046,402,427]" pageId="25" pageNumber="1233">
urn:lsid:zoobank.org:act: 
<uuid id="FFF8D9AAFFAEC8454D711197FBC8DA9E" box="[531,1046,402,427]" pageId="25" pageNumber="1233">89132977-91DC-4424-9C0E-98A954E467CA</uuid>
