<geoCoordinate id="07F8B8D90319A384282C89DA3697E1EE" direction="north" orientation="latitude" precision="1" value="35.122116">35°07.327'N</geoCoordinate>
, 
<geoCoordinate id="5E33CC8D392C8BFF8528176D8C025E3D" direction="east" orientation="longitude" precision="1" value="139.54964">139°32.978'E</geoCoordinate>
