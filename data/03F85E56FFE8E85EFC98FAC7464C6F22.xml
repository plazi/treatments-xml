<heading id="D0A6582CFFE8E85FFC98FAC740456E32" box="[800,1128,1374,1397]" centered="true" fontSize="36" level="2" pageId="26" pageNumber="27" reason="8">
<taxonomicName id="4C5194C3FFE8E85FFC98FAC740456E32" authority="Filhol, 1881" authorityName="Filhol" authorityYear="1881" box="[800,1128,1374,1397]" class="Mammalia" family="Ursidae" genus="Amphicynodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carnivora" pageId="26" pageNumber="27" phylum="Chordata" rank="genus">
