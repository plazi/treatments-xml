<taxonomicName id="22A5BDF1FFBCFF8F05F3FE3FFC56FE31" baseAuthorityName="O. Pickard-Cambridge" baseAuthorityYear="1892" box="[718,1019,475,501]" class="Arachnida" family="Theraphosidae" genus="Megaphobema" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="mesomelas">
