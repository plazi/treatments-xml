<taxonomicName id="22A5BDF1FFBCFF8F03D1FE3FFA38FE32" box="[1260,1429,475,502]" class="Arachnida" family="Theraphosidae" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="family">Theraphosidae</taxonomicName>
