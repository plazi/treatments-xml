<taxonomicName id="4C19DEEAFFFAFFD0772568C646F3FD61" authorityName="Cairns" authorityYear="2018" box="[382,449,729,755]" class="Anthozoa" family="Primnoidae" genus="Narella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="20" pageNumber="21" phylum="Cnidaria" rank="species" species="ferula">
