<taxonomicName id="80C2A1B24435CD5EFD6A6AFD145DB2EF" box="[661,858,1714,1739]" class="Insecta" family="Cynipidae" genus="Ufo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="191" phylum="Arthropoda" rank="species" species="cerroneuroteri">
<emphasis id="75B606234435CD5EFD6A6AFD145DB2EF" box="[661,858,1714,1739]" italics="true" pageId="12" pageNumber="191">U. cerroneuroteri</emphasis>
