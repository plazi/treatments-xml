<taxonomicName id="6AB236EBFFC4FFC250DFF98AFCFC2BDC" authority="Rauser" authorityName="Rauser" box="[458,779,1643,1666]" class="Insecta" family="Nemouridae" genus="Protonemura" kingdom="Animalia" order="Plecoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="albanica">
