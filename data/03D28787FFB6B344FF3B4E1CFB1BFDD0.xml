<taxonomicName id="4C7B4D12FFB7B347FE4C4E31FDB4F8D4" box="[480,613,1801,1823]" class="Insecta" family="Cicadellidae" genus="Japanagallia" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="genus">
