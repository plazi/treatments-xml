<taxonomicName id="69E64D628B60876F4C920E38FC78FEAD" box="[963,1023,261,284]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="nigra">
<emphasis id="9C92EAF38B60876F4C920E38FC78FEAD" box="[963,1023,261,284]" italics="true" pageId="51" pageNumber="52">nigra</emphasis>
</taxonomicName>
