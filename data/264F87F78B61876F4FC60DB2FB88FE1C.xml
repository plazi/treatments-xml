<taxonomicName id="69E64D628B60876F4C5A0E38FC33FEAC" box="[779,948,260,285]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="chorizanthis">
<emphasis id="9C92EAF38B60876F4C5A0E38FC33FEAC" box="[779,948,260,285]" italics="true" pageId="51" pageNumber="52">R. chorizanthis</emphasis>
</taxonomicName>
