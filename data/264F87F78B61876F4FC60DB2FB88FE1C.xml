<taxonomicName id="69E64D628B60876F4DB00E4CFCECFE39" box="[737,875,367,392]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="disparilis">
<emphasis id="9C92EAF38B60876F4DB00E4CFCECFE39" box="[737,875,367,392]" italics="true" pageId="51" pageNumber="52">R. disparilis</emphasis>
</taxonomicName>
