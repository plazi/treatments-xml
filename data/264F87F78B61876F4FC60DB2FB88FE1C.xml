<taxonomicName id="69E64D628B60876F4BD80E14FB41FEF1" box="[1161,1222,297,320]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="nigra">
<emphasis id="9C92EAF38B60876F4BD80E14FB41FEF1" box="[1161,1222,297,320]" italics="true" pageId="51" pageNumber="52">nigra</emphasis>
