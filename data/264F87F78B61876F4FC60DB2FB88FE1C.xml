<taxonomicName id="69E64D628B60876F4AD50E70FEA5FE39" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="chorizanthis">
<emphasis id="9C92EAF38B60876F4AD50E70FEA5FE39" italics="true" pageId="51" pageNumber="52">R. chorizanthis</emphasis>
