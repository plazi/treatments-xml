<taxonomicName id="69E64D628B60876F4C920E14FBC9FEF1" box="[963,1102,295,320]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="disparilis">
