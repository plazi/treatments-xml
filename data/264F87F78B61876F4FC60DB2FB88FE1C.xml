<taxonomicName id="69E64D628B60876F4B370FE2FB23FF49" box="[1126,1188,223,248]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="rutila">
