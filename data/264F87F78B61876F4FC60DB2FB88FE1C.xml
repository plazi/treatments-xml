<taxonomicName id="69E64D628B60876F4BD30E39FB70FEAC" box="[1154,1271,260,285]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="solanicola">
