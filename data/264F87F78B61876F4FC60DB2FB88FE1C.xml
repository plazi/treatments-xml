<taxonomicName id="69E64D628B60876F4D7B0E70FD0CFED5" box="[554,651,333,356]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="nigra">
