<taxonomicName id="69E64D628B60876F4B5C0E39FBCCFEAC" box="[1037,1099,260,285]" class="Insecta" family="Psyllidae" genus="Russelliana" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="rutila">
