(
<taxonomicName id="915A97189BE5976FD29477A304325B90" class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="493" phylum="Tracheophyta" rank="species" species="aurantiacus">
