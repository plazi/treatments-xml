<taxonomicName id="4C414D38FF387AE9FF36FB4CFB00FB66" authorityName="Gonzalez-Sponga" authorityYear="1999" box="[189,347,1218,1244]" class="Arachnida" family="Pholcidae" genus="Priscula" kingdom="Animalia" order="Araneae" pageId="232" pageNumber="233" phylum="Arthropoda" rank="species" species="andinensis">
