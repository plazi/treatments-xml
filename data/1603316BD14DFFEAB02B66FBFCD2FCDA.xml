<paragraph id="9E15807DD14CFFEAB01B62CAFCD2FCDA" blockId="5.[264,1323,644,870]" pageId="5" pageNumber="40">
<taxonomicName id="59AAFBFED14CFFEAB01B62CAFDB6FD79" box="[312,534,684,709]" class="Mammalia" family="Vespertilionidae" genus="Eptesicus" kingdom="Animalia" order="Chiroptera" pageId="5" pageNumber="40" phylum="Chordata" rank="species" species="furinalis">
