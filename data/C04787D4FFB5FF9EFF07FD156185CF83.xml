<taxonomicName id="8FEE4D41FFB5FF9EFBBFFBC46433CFE6" authority="in Borght-Elbl (1977)" authorityName="in Borght-Elbl" authorityYear="1977" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="falsomarmoreum">
