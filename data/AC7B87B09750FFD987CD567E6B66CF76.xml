<taxonomicName id="E3D24D25975EFFD987B857A26B38CDBA" authority="Gnezdilov, 2021" authorityName="Gnezdilov" authorityYear="2021" box="[929,1093,1231,1249]" class="Insecta" family="Cicadellidae" genus="Calodia" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="303" phylum="Arthropoda" rank="species" species="makayensis" status="n. sp.">
