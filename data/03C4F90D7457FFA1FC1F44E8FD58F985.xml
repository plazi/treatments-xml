<taxonomicName id="4C6D33987456FFA6FE1C45C2FE52F571" authorityName="Osten Sacken" authorityYear="1860" box="[392,499,2030,2049]" class="Insecta" family="Limoniidae" genus="Epiphragma" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="460" phylum="Arthropoda" rank="genus">
