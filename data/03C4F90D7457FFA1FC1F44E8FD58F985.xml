<taxonomicName id="4C6D33987456FFA6FC0B4A25FB19F56C" authority="Alexander, 1955" authorityName="Alexander" authorityYear="1955" box="[927,1208,2057,2076]" class="Insecta" family="Limoniidae" genus="Epiphragma" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="460" phylum="Arthropoda" rank="species" species="trichomerum">
