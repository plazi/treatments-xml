<subSubSection id="247ABD0E4F9784A24C2170B7938ADF98" type="nomenclature">
<paragraph id="2A3657CE6E7CFEE528B5469086D19172" pageNumber="58">
Type-species: 
<taxonomicName id="BA1AF1F9E721736AE2354CAAA4B2B714" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141038" authority="Emery" authorityName="Emery" class="Insecta" family="Formicidae" genus="Sifolinia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sifolinia laurae Emery" order="Hymenoptera" pageNumber="58" phylum="Arthropoda" rank="species" species="laurae">
