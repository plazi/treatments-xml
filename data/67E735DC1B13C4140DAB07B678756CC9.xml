<taxonomicName id="8D0F0670D32EBBB5580A279F42656562" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141038" authority="Emery" authorityName="Emery" class="Insecta" family="Formicidae" genus="Sifolinia" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sifolinia laurae Emery" order="Hymenoptera" pageNumber="58" phylum="Arthropoda" rank="species" species="laurae">laurae Emery</taxonomicName>
