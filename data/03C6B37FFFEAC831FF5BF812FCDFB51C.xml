<taxonomicName id="4C6F79EAFFE9C831FBE7FF46FAE3B4B0" authority="Pallas, 1766" authorityName="Pallas" authorityYear="1766" box="[1067,1385,151,177]" class="Hydrozoa" family="Hydridae" genus="Hydra" kingdom="Animalia" order="Anthoathecata" pageId="2" pageNumber="153" phylum="Cnidaria" rank="species" species="vulgaris">
