<taxonomicName id="4C6F79EAFFE9C831FB0BFF0EFEA2B51C" authority="Cuvier, 1798" authorityName="Cuvier" authorityYear="1798" class="Charophyceae" family="Characeae" genus="Cristatella" kingdom="Plantae" order="Charales" pageId="2" pageNumber="153" phylum="Charophyta" rank="species" species="mucedo">
