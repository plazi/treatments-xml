<taxonomicName id="44E618B948C4B8AB2E818470AE0F104F" authorityName="Van Benthem Jutting" authorityYear="1959" class="Gastropoda" family="Cyclophoridae" genus="Chamalycaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chamalycaeus crassicollis" order="Gastropoda" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="crassicollis">Chamalycaeus crassicollis</taxonomicName>
