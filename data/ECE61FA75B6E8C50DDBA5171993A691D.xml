<taxonomicName id="CD6D1DFAC097C3B7745B2C9BBD35C935" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143684" class="Insecta" family="Formicidae" genus="Proceratium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Proceratium mancum Mann" lsidName-HNS="Proceratium mancum Mann" order="Hymenoptera" pageNumber="82" phylum="Arthropoda" rank="species" species="mancum">Proceratium mancum Mann</taxonomicName>
