<taxonomicName id="4F78DD1DD6156B2A8BDC8916CDF244AB" authority="Lam." class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="455" phylum="Tracheophyta" rank="species" species="diffusa">
