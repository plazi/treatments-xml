<taxonomicName id="097291807D87EF54130A61BCBDBF664B" class="Magnoliopsida" family="Quillajaceae" genus="Quillaja" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quillaja saponaria" order="Fabales" pageId="0" pageNumber="57" phylum="Tracheophyta" rank="species" species="saponaria">
<emphasis id="F84E0EC356C0C2172EF20A5FBE551D66" italics="true" pageId="0" pageNumber="57">Quillaja saponaria</emphasis>
</taxonomicName>
Molina (
<taxonomicName id="67E39A746D93E4E89E19B829FA637C13" authorityName="Molina" authorityYear="1782" class="Magnoliopsida" family="Quillajaceae" genus="Quillaja" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Fabales" pageId="0" pageNumber="57" phylum="Tracheophyta" rank="family">Quillajaceae</taxonomicName>
