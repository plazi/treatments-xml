<quantity id="4CA3E3BB81050D102049A85EFB37FCB6" box="[1128,1209,800,824]" metricMagnitude="3" metricUnit="m" metricValue="1.95" pageId="46" pageNumber="47" unit="m" value="1950.0">1950m</quantity>
, Nr 
<collectingCountry id="F34C0ECE81050D1020CCA861FACCFCB6" box="[1261,1346,799,824]" name="Algeria" pageId="46" pageNumber="47">Algiers</collectingCountry>
, Sahara Central, 
<taxonomicName id="4C5B35DD81050D1024DAA83CFE2EFCD5" box="[251,416,834,859]" class="Magnoliopsida" family="Cistaceae" genus="Helianthemum" kingdom="Plantae" order="Malvales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="genus">
<emphasis id="B92F924C81050D1024DAA83CFE2EFCD5" box="[251,416,834,859]" italics="true" pageId="46" pageNumber="47">Helianthemum</emphasis>
</taxonomicName>
sp., 
<date id="FFE5689E81050D1025FCA83CFDD0FCD5" box="[477,606,834,859]" pageId="46" pageNumber="47" value="1949-04">
April 
<number id="3D935C7981050D102605A83DFDD0FCD5" box="[548,606,835,859]" pageId="46" pageNumber="47" value="1949.0">1949</number>
</date>
, Balachowsky (MNHN): 
<number id="3D935C7981050D1027AAA83CFC1DFCD5" box="[907,915,834,859]" pageId="46" pageNumber="47" value="1.0">1</number>
/ 
<number id="3D935C7981050D1027BAA83CFC26FCD5" box="[923,936,834,859]" pageId="46" pageNumber="47" value="2.0">2</number>
