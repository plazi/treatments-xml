<taxonomicName id="4C674D31D301FFCEEA189CB1FA2D22AF" box="[1311,1408,405,428]" class="Mammalia" family="Muridae" genus="Rattus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="11" pageNumber="506" phylum="Chordata" rank="species" species="rattus">
