in 
<collectingRegion id="49A3F850D301FFCEEF909C93FE8622D3" box="[151,299,439,464]" country="Mexico" name="Distrito Federal" pageId="11" pageNumber="506">
<collectingCountry id="F3707622D301FFCEEF909C93FF4122D3" box="[151,236,439,464]" name="Mexico" pageId="11" pageNumber="506">México</collectingCountry>
