<taxonomicName id="4C77EE8AFFA1FFBBF2DAF9AE5355EAA5" baseAuthorityName="Chevreux" baseAuthorityYear="1926" box="[751,848,1639,1664]" class="Malacostraca" family="Uristidae" genus="Schisturella" kingdom="Animalia" order="Amphipoda" pageId="15" pageNumber="516" phylum="Arthropoda" rank="species" species="abyssi">
