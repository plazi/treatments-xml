<collectingCountry id="91D268FC19685847216B38F9A2FAF097" box="[304,412,668,691]" name="Colombia" pageId="36" pageNumber="293">Colombia</collectingCountry>
, French 
<collectingCountry id="91D268FC1968584721AC38F9A12BF097" box="[503,589,668,691]" name="Guyana" pageId="36" pageNumber="293">Guyana</collectingCountry>
, and 
<collectingCountry id="91D268FC1968584722DF38F9A193F097" box="[644,757,668,691]" name="Venezuela" pageId="36" pageNumber="293">Venezuela</collectingCountry>
