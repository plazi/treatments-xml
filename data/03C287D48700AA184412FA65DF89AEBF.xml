<taxonomicName id="4C6B4D418700AA1F4260F96FDF91AC9D" box="[1149,1265,1693,1720]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
