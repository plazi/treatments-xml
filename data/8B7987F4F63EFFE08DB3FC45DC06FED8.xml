<subSubSection id="4BCA6569F63BFFE18DE3F88DDBE3F813" box="[199,1216,1919,1945]" pageId="46" pageNumber="47" type="etymology">
<paragraph id="036F36E2F63BFFE18DE3F88DDBE3F813" blockId="46.[151,1436,1919,2025]" box="[199,1216,1919,1945]" pageId="46" pageNumber="47">
