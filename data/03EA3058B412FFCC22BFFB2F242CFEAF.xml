<taxonomicName id="4C43FACDB414FFCC2013FEE623EAFEAF" box="[571,736,260,284]" class="Insecta" family="Ichneumonidae" genus="Lathrolestes" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="cruentocaudus">
