<taxonomicName id="2C8D4D63282BEC79DCEB195F2054F8A4" authority="Chen, 1984" authorityName="Chen" authorityYear="1984" box="[724,1073,1852,1875]" class="Arachnida" family="Amaurobiidae" genus="Coelotes" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="xinhuiensis">
