<number id="3DAEED19FFBBCF21FF4896F68BA0FB5E" box="[210,225,1157,1183]" pageId="16" pageNumber="17" value="4.0">4</number>
<emphasis id="B912232CFFBBCF21FF7296F68A45FB5F" box="[232,260,1157,1182]" italics="true" pageId="16" pageNumber="17">av</emphasis>
and 
<number id="3DAEED19FFBBCF21FEA796F68A0DFB5E" box="[317,332,1157,1183]" pageId="16" pageNumber="17" value="1.0">1</number>
