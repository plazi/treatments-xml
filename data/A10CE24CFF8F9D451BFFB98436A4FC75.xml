<taxonomicName id="EEA528D9FF849D441A36B8353654F8A1" box="[350,787,1801,1823]" class="Arachnida" family="Parakalummidae" genus="Neoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="12" pageNumber="236" phylum="Arthropoda" rank="species" species="kontschani" status="sp. nov." subGenus="Pseudoneoribates">
