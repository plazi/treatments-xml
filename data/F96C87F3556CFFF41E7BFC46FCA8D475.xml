long. Gonocoxite 
<quantity id="B63D9B00556AFFF41E7BFBEAFE28D66D" box="[264,473,1072,1098]" metricMagnitude="2" metricUnit="m" metricValue="1.3406749999999998" metricValueMax="1.43135" metricValueMin="1.25" pageId="13" pageNumber="14" unit="m" value="134.0675" valueMax="143.135" valueMin="125.0">125–143, 135 m</quantity>
