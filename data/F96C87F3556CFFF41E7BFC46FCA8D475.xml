<quantity id="B63D9B00556AFFF41D82FBEAFC64D66D" box="[753,917,1072,1098]" metricMagnitude="1" metricUnit="m" metricValue="4.1205" metricValueMax="4.340999999999999" metricValueMin="3.9" pageId="13" pageNumber="14" unit="m" value="41.205" valueMax="43.41" valueMin="39.0">39–43, 41 m</quantity>
