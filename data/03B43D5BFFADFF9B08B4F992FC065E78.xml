<taxonomicName id="4C1DF7CEFFADFF9B0C52F904FEA75E1F" authority="Trin. (Emeljanov, 1959)" authorityName="Trin. (Emeljanov" authorityYear="1959" class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="0" pageNumber="441" phylum="Tracheophyta" rank="species" species="communis">
