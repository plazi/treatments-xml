<collectingCountry id="F3747653044CFFED64D548804C002AE1" box="[423,536,730,756]" name="Indonesia" pageId="17" pageNumber="94">Indonesia</collectingCountry>
, East 
<collectingCountry id="F3747653044CFFED672B48804CA72AE1" box="[601,703,730,756]" name="Malaysia" pageId="17" pageNumber="94">Malaysia</collectingCountry>
, West 
<collectingCountry id="F3747653044CFFED667548804D722AE1" box="[775,874,730,756]" name="Malaysia" pageId="17" pageNumber="94">Malaysia</collectingCountry>
(
<collectingRegion id="49A7F821044CFFED660B48804DC92AE0" box="[889,977,730,757]" country="Malaysia" name="Pahang" pageId="17" pageNumber="94">Pahang</collectingRegion>
), 
<collectingCountry id="F3747653044CFFED669148804A4B2AE1" box="[995,1107,730,756]" name="Singapore" pageId="17" pageNumber="94">Singapore</collectingCountry>
(
<bibRefCitation id="EFF24B32044CFFED65E148A74F372B02" author="Paulian R" box="[147,303,765,791]" pageId="17" pageNumber="94" pagination="479 - 514" refId="ref11443" refString="Paulian R (1978) Revision des Ceratocanthidae [Col. Scarabaeoidea] II - Les especes orientales et australiennes. Annales de la Societe Entomologique de France (N. S.) 14: 479 - 514." type="journal article" year="1978">Paulian 1978</bibRefCitation>
