<taxonomicName id="F69D4D79FFB1FFC3FF5DDC3BFE69F9A9" authority="Newman, 2000" authorityName="Newman" authorityYear="2000" box="[217,491,1621,1643]" class="Rhabditophora" family="Euryleptidae" genus="Maritigrella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="8" pageNumber="197" phylum="Platyhelminthes" rank="species" species="marygarsonae" status="sp. nov.">
