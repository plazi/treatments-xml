<taxonomicName id="41F29A98513FFD872B4ED93521538796" class="Arachnida" family="Gnaphosidae" genus="Pterotricha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pterotricha lentiginosa" order="Araneae" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="lentiginosa">Pterotricha lentiginosa</taxonomicName>
: 
<bibRefCitation id="AB4DB9D18CB90348E8E058B91931F43A" author="Roewer, CF" journalOrPublisher="Sitzungsberichte der Oesterreichischen Akademie der Wissenschaften (I)" pageId="0" pageNumber="17" pagination="751 - 782" title="Die Araneen der Oesterreichischen Iran-Expedition 1949 / 50." volume="164" year="1955">Roewer 1955</bibRefCitation>
