<quantity id="4C981EF59A7FFFF032830B34FE20FCE2" box="[425,511,867,891]" metricMagnitude="3" metricUnit="m" metricValue="1.127" pageId="12" pageNumber="413" unit="m" value="1127.0">1127 m</quantity>
