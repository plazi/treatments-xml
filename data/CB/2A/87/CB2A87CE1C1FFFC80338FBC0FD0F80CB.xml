<emphasis bold="true" box="[296,309,931,948]" pageId="24" pageNumber="571">A</emphasis>
, 
<taxonomicName authority="csikii Banninger" authorityName="Banninger" box="[320,523,931,948]" class="Insecta" family="Carabidae" genus="Eustra" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="571" phylum="Arthropoda" rank="subSpecies" species="csikii" subSpecies="csikii">
<emphasis box="[320,429,931,948]" italics="true" pageId="24" pageNumber="571">E. csikii csikii</emphasis>
Bänninger
</taxonomicName>
, de Hoa Binh. Extrémité du gonocoxite et gonostyle (
<emphasis bold="true" box="[974,1000,931,948]" pageId="24" pageNumber="571">sty</emphasis>
) 
<emphasis bold="true" box="[1021,1034,931,948]" pageId="24" pageNumber="571">B</emphasis>
, mêmes formations chez 
<taxonomicName authority="matanga Andrewes, 1919" authorityName="matanga Andrewes" authorityYear="1919" box="[151,476,952,969]" class="Insecta" family="Carabidae" genus="Eustra" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="571" phylum="Arthropoda" rank="subSpecies" species="matanga" subSpecies="matanga">
