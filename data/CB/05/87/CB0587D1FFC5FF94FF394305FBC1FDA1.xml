<subSubSection pageId="20" pageNumber="21" type="discussion">
<paragraph blockId="20.[151,1436,151,608]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[199,335,439,464]" pageId="20" pageNumber="21">Comments.</emphasis>
