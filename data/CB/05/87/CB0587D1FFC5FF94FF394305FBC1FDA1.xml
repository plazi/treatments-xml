<collectingRegion box="[675,761,439,464]" country="Mexico" name="Mexico" pageId="20" pageNumber="21">
<collectingCountry box="[675,761,439,464]" name="Mexico" pageId="20" pageNumber="21">Mexico</collectingCountry>
</collectingRegion>
