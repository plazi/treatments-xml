and 10% 
<taxonomicName authorityName="Du Roi" authorityYear="1771" baseAuthorityName="Mill." box="[1045,1172,1227,1252]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="mariana">
