<td align="left" pageId="null" pageNumber="433" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="433" phylum="Tracheophyta" rank="species" species="schleicheri">
