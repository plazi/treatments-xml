<taxonomicName authority="Casey, 1890" authorityName="Casey" authorityYear="1890" class="Insecta" family="Cerylonidae" genus="Lapethus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lapethus" order="Coleoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="genus">Lapethus</taxonomicName>
