<taxonomicName class="Insecta" family="Murmidiidae" genus="Mychocerinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mychocerinus" order="Coleoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="genus">Mychocerinus</taxonomicName>
