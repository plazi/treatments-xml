<taxonomicName class="Insecta" family="Cerylonidae" genus="Mychocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mychocerus" order="Coleoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="genus">Mychocerus</taxonomicName>
