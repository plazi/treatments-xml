<taxonomicName box="[693,811,621,644]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="5" pageNumber="374" phylum="Arthropoda" rank="genus">mesosoma</taxonomicName>
length/width = 
<number box="[998,1051,620,644]" pageId="5" pageNumber="370" value="0.78">0.78</number>
