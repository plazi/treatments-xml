<taxonomicName authority="Ashmead, 1904" authorityName="Asmead" authorityYear="1904" class="Insecta" family="Braconidae" genus="Glyptapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Glyptapanteles" order="Hymenoptera" pageId="0" pageNumber="127" phylum="Arthropoda" rank="genus">
