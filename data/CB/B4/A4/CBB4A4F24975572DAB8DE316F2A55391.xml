<emphasis italics="true" pageId="0" pageNumber="81">C. variistriatus</emphasis>
</taxonomicName>
sp. nov. (
<typeStatus>holotype</typeStatus>
; TL: 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="6.6" unit="mm" value="6.6">6.6 mm</quantity>
) 
