<taxonomicName class="Insecta" family="Dytiscidae" genus="Copelatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Copelatus" order="Coleoptera" pageId="0" pageNumber="81" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="81">Copelatus</emphasis>
</taxonomicName>
<emphasis bold="true" pageId="0" pageNumber="81">9</emphasis>
<taxonomicName genus="C." lsidName="C. urceolus" pageId="0" pageNumber="81" rank="species" species="urceolus">
<emphasis italics="true" pageId="0" pageNumber="81">C. urceolus</emphasis>
</taxonomicName>
sp. nov. (
<typeStatus>holotype</typeStatus>
, TL: 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="5.1" unit="mm" value="5.1">5.1 mm</quantity>
) 
<emphasis bold="true" pageId="0" pageNumber="81">10</emphasis>
