<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34070" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole tristis (Smith)" lsidName-HNS="Pheidole tristis (Smith)" order="Hymenoptera" pageNumber="686" phylum="Arthropoda" rank="species" species="tristis">tristis</taxonomicName>
, and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182095" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole zoster Wilson" lsidName-HNS="Pheidole zoster Wilson" order="Hymenoptera" pageNumber="686" phylum="Arthropoda" rank="species" species="zoster">zoster</taxonomicName>
