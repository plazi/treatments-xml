<paragraph blockId="5.[151,1435,1764,1817]" pageId="5">
<emphasis bold="true" box="[151,271,1765,1787]" pageId="5">FIGURE 2</emphasis>
<taxonomicName authority="Li &amp; Yeates, 2018" authorityName="Li &amp; Yeates" authorityYear="2018" box="[278,510,1764,1787]" class="Insecta" family="Bombyliidae" genus="Marmasoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" phylum="Arthropoda" rank="species" species="hortorum" status="sp. nov.">
<emphasis box="[278,510,1764,1787]" italics="true" pageId="5">Marmasoma hortorum</emphasis>
</taxonomicName>
