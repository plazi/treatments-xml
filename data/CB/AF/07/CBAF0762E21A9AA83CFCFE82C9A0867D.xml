<paragraph pageId="null" pageNumber="129">
<taxonomicName authority="(L.) Hoffm." class="Polypodiopsida" family="Aspleniaceae" genus="Asplenium" kingdom="Plantae" order="Polypodiales" pageId="null" pageNumber="129" phylum="Tracheophyta" rank="species" species="septentrionale">
<pageBreakToken pageId="null" pageNumber="129" start="start">Asplenium</pageBreakToken>
<normalizedToken originalValue="septentrionále" pageId="null" pageNumber="129">septentrionale</normalizedToken>
(
<authorityName pageId="null" pageNumber="129">L.</authorityName>
) Hoffm.
</taxonomicName>
</paragraph>
