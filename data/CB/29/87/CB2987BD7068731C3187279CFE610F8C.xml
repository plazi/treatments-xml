<paragraph blockId="11.[151,994,151,177]" box="[151,994,151,177]" pageId="11" pageNumber="40">
<heading bold="true" box="[151,994,151,177]" fontSize="11" level="1" pageId="11" pageNumber="40" reason="1">
<emphasis bold="true" box="[151,994,151,177]" pageId="11" pageNumber="40">
<taxonomicName authority="Willis and Packer" authorityName="Willis and Packer" box="[151,843,151,177]" class="Insecta" family="Colletidae" genus="Chilicola" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="40" phylum="Arthropoda" rank="species" species="mirzamalae" status="new species" subGenus="Chilioediscelis">
<emphasis bold="true" box="[151,613,151,177]" italics="true" pageId="11" pageNumber="40">Chilicola (Chilioediscelis) mirzamalae</emphasis>
Willis and Packer
</taxonomicName>
