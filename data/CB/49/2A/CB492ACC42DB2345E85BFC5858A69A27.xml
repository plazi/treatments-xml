<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:151509" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing" genus.completionRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole radoszkowskii opacissima Forel" order="Hymenoptera" pageNumber="70" phylum="Arthropoda" rank="species" species="opacissima">opacissima</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:151616" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing" genus.completionRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole triconstricta paranana Santschi" order="Hymenoptera" pageNumber="70" phylum="Arthropoda" rank="species" species="paranana">paranana</taxonomicName>
