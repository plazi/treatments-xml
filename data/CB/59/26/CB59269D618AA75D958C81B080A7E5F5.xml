<taxonomicName class="Malacostraca" family="Potamidae" genus="Longpotamon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Longpotamon" order="Decapoda" pageId="2" pageNumber="63" phylum="Arthropoda" rank="genus">Longpotamon</taxonomicName>
