<taxonomicName class="Malacostraca" family="Potamidae" genus="Tenuilapotamon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tenuilapotamon" order="Decapoda" pageId="2" pageNumber="63" phylum="Arthropoda" rank="genus">Tenuilapotamon</taxonomicName>
