<quantity box="[214,277,192,218]" metricMagnitude="3" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="29" unit="km" value="9.0">9 km</quantity>
. 
<number box="[289,350,192,218]" pageId="4" pageNumber="29" value="1190.0">1190</number>
