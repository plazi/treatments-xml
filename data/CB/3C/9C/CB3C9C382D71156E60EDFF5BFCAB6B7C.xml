(
<collectionCode box="[1328,1423,192,218]" httpUri="http://grbio.org/cool/pyng-6456" name="Museo Argentino de Ciencias Naturales Bernardino Rivadavia" pageId="4" pageNumber="29">MACN</collectionCode>
); 
<specimenCount box="[151,233,232,258]" pageId="4" pageNumber="29" type="male">
<number box="[151,166,232,258]" pageId="4" pageNumber="29" value="1.0">1</number>
male
</specimenCount>
, same data as 
<typeStatus box="[400,503,232,258]" pageId="4" pageNumber="29">holotype</typeStatus>
(
<collectionCode box="[518,595,232,258]" pageId="4" pageNumber="29">IFML</collectionCode>
); 
<specimenCount box="[615,697,232,258]" pageId="4" pageNumber="29" type="male">
<number box="[615,630,232,258]" pageId="4" pageNumber="29" value="1.0">1</number>
male
</specimenCount>
, same data as of 
