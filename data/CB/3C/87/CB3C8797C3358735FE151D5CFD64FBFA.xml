<geoCoordinate box="[874,939,201,225]" degrees="0.083" direction="north" orientation="latitude" pageId="16" pageNumber="3559" precision="55" value="0.083">0.083</geoCoordinate>
<geoCoordinate box="[939,1020,201,225]" degrees="0.098" direction="west" orientation="longitude" pageId="16" pageNumber="3559" precision="55" value="-0.098">–0.098</geoCoordinate>
