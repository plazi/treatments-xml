2. 
<taxonomicName authorityName="Tendeiro" authorityYear="1965" box="[516,664,1150,1174]" class="Insecta" family="Philopteridae" genus="Columbicola" kingdom="Animalia" order="Psocodea" pageId="15" pageNumber="3558" phylum="Arthropoda" rank="species" species="guimaraesi">
