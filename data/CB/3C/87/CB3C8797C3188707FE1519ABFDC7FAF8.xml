<taxonomicName authority="Tendeiro 1984: 85" authorityName="Tendeiro" authorityPageNumber="85" authorityYear="1984" box="[165,663,492,516]" class="Insecta" family="Philopteridae" genus="Columbicola" kingdom="Animalia" order="Psocodea" pageId="34" pageNumber="3577" phylum="Arthropoda" rank="species" species="effeminatus">
