<typeStatus box="[757,805,1781,1805]" pageId="8" pageNumber="398">type</typeStatus>
and size (
<tableCitation box="[918,1011,1780,1805]" captionStart="TABLE 3" captionStartId="9.[151,239,152,175]" captionText="TABLE 3. Comparison of characters between Clathria (C.) ieoenssis sp. nov. and C. (C.) murphyi Hooper, 1996." pageId="8" pageNumber="398">Table. 3</tableCitation>
).
