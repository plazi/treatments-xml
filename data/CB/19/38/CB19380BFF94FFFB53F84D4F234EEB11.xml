<paragraph blockId="6.[151,1437,151,861]" box="[199,764,223,248]" pageId="6" pageNumber="193">
<emphasis bold="true" box="[199,492,223,248]" pageId="6" pageNumber="193">Alate viviparous females</emphasis>
(
<specimenCount box="[508,651,223,248]" pageId="6" pageNumber="193" type="generic">
<number box="[508,523,224,248]" pageId="6" pageNumber="193" value="2.0">2</number>
