<taxonomicName id="85BC4D03FFC39A53FD0FCC41FCC7FAA8" authorityName="Leach" authorityYear="1815" box="[748,864,1373,1402]" class="Insecta" family="Erebidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="16" phylum="Arthropoda" rank="subFamily" subFamily="Erebinae">Erebinae</taxonomicName>
