<taxonomicName id="85BC4D03FFC39A53FC8FCC41FC53FAA8" box="[876,1012,1373,1402]" class="Insecta" family="Erebidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="16" phylum="Arthropoda" rank="tribe" tribe="Melipotini">Melipotini</taxonomicName>
