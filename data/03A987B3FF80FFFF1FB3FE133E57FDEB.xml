<taxonomicName id="4C004D26FF82FFFF1C46FC973DBBFB27" authorityName="Chapuis" authorityYear="1869" box="[299,402,837,855]" class="Insecta" family="Curculionidae" genus="Ceratolepis" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="550" phylum="Arthropoda" rank="genus">
