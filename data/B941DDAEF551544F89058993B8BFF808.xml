<subSubSection id="A11FB4C78FB8DAC11A32B0E67DA5EC7E" pageId="0" pageNumber="71" type="type material">
<paragraph id="AA1376D6999D4435EAE1C700F4F80F00" pageId="0" pageNumber="71">Type material.</paragraph>
<paragraph id="3442ED0D9AB6D81C8B658C586C1DBA4B" pageId="0" pageNumber="71">
<emphasis id="477E06141F872125ECC1266DA4501EAE" bold="true" italics="true" pageId="0" pageNumber="71">Holotype</emphasis>
♂, 
<emphasis id="21EAB6B862BCD05584B7C80E85DE3E12" bold="true" pageId="0" pageNumber="71">Peru</emphasis>
