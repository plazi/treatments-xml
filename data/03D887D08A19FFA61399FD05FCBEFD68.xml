<emphasis id="B905EAD48A18FFA613C9FE06FE9CFE3C" bold="true" box="[199,352,439,464]" inLineHeading="true" pageId="43" pageNumber="44" reason="1">Distribution.</emphasis>
<collectingCountry id="F36676568A18FFA61269FE06FE32FE23" box="[359,462,439,463]" name="Australia" pageId="43" pageNumber="44">Australia</collectingCountry>
(
<collectingRegion id="49B5F8248A18FFA612D2FE06FDB0FE23" box="[476,588,439,463]" country="Australia" name="Tasmania" pageId="43" pageNumber="44">Tasmania</collectingRegion>
, 
<collectingRegion id="49B5F8248A18FFA61159FE06FD1DFE23" box="[599,737,439,463]" country="Australia" name="Queensland" pageId="43" pageNumber="44">Queensland</collectingRegion>
