<taxonomicName id="C3BA4D67034F9C2234F046E7A6011D6A" authorityName="OPITZ" authorityYear="2016" box="[923,1045,1530,1552]" class="Insecta" family="Cleridae" genus="Tarsostenus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="602" phylum="Arthropoda" rank="species" species="antehelvis">
