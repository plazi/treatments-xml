<taxonomicName id="C3BA4D67034F9C223669450DA3761D43" authorityName="OPITZ" authorityYear="2016" box="[258,354,1553,1574]" class="Insecta" family="Cleridae" genus="Tarsostenodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="602" phylum="Arthropoda" rank="species" species="bullatus">
