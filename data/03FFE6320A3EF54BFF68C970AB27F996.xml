<collectionCode id="ED47CFE10A3EF54BFD71C9E5A8CCFA73" LSID="urn:lsid:biocol.org:col:34665" box="[654,760,1483,1506]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="59" pageNumber="60">NHMUK</collectionCode>
1907.10. 
<number id="3D9E45030A3EF54BFC9AC9E4A982FA72" box="[869,950,1482,1507]" pageId="59" pageNumber="60" value="28225.0">28.225</number>
