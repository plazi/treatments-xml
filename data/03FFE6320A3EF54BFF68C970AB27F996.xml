<taxonomicName id="4C562CA70A3EF54BFBBBC9E2AE97FA72" box="[1092,1187,1484,1507]" class="Gastropoda" family="Ampullariidae" genus="Pomacea" higherTaxonomySource="CoL" kingdom="Animalia" order="Architaenioglossa" pageId="59" pageNumber="60" phylum="Mollusca" rank="species" species="cerasum">
