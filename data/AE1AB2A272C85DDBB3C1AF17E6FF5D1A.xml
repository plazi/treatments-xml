<paragraph id="C982852E5A501DAB088044A9A7825786" pageId="6" pageNumber="29">Type.</paragraph>
<paragraph id="2B0AEA83C3C76D2B1326E9B4421449B6" pageId="6" pageNumber="29">
<taxonomicName id="EA80BBA86D124215DFD8D2B0B22FACE7" authorityName="Schuettp., J. Prado &amp; A. T. Cochran" authorityYear="2014" baseAuthorityName="Schuettp., J. Prado &amp; A. T. Cochran" baseAuthorityYear="2014" class="Gastropoda" family="Cochliopidae" genus="Tryonia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Littorinimorpha" pageId="6" pageNumber="29" phylum="Mollusca" rank="species" species="myriophylla">
