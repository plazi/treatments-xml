<taxonomicName id="4C754D5EFFB9873AB241FA00FF69AFAE" authority="Linnavuori, 2010" authorityName="Linnavuori" authorityYear="2010" box="[368,544,1425,1448]" class="Insecta" family="Miridae" genus="Yotvata" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="408" phylum="Arthropoda" rank="species" species="farsiana" status="sp. nov.">
<emphasis id="B901EACFFFB9873AB241FA00FF69AFAE" box="[368,544,1425,1448]" italics="true" pageId="39" pageNumber="408">Yotvata farsiana</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23257B4FFB9873AB114FA00FF31AFAE" box="[549,632,1425,1448]" pageId="39" pageNumber="408" rank="species">sp. nov.</taxonomicNameLabel>
is related to 
<taxonomicName id="4C754D5EFFB9873AB1CCFA00F974AFAE" authority="(Horvath, 1913)" baseAuthorityName="Horvath" baseAuthorityYear="1913" box="[765,1085,1424,1448]" class="Insecta" family="Miridae" genus="Yotvata" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="408" phylum="Arthropoda" rank="species" species="picticornis">
