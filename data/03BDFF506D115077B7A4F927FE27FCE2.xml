<paragraph id="8BAB4E466D115076B7A4F8C8FC7BF8B5" blockId="8.[151,917,1837,1860]" box="[151,917,1837,1860]" pageId="8" pageNumber="229">
<taxonomicName id="4C1435C56D115076B7A4F8C8FE3FF8B5" authority="Say, 1823: 193" authorityName="Say" authorityPageNumber="193" authorityYear="1823" box="[151,465,1837,1859]" class="Insecta" family="Curculionidae" genus="Silpha" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="229" phylum="Arthropoda" rank="species" species="truncata">
