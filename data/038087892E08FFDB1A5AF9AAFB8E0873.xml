<paragraph id="8B96369F2E0DFFDB1A6AFD5AFD8D0E9B" blockId="4.[264,1323,284,1030]" box="[312,633,724,750]" pageId="4" pageNumber="37">
Distribution. 
<collectingCountry id="F33E760F2E0DFFDB1A86FD5AFDEE0E9B" box="[468,538,724,750]" name="China" pageId="4" pageNumber="37">China</collectingCountry>
