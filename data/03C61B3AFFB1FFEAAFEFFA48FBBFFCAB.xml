<taxonomicName id="4C6FD1AFFFB2FFEAAD91FD2DFCC6FD6E" authorityName=": DANKS" authorityYear="1939" box="[716,844,689,711]" class="Insecta" family="Staphylinidae" genus="Gyrophaena" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="439" phylum="Arthropoda" rank="species" species="bihamata">
