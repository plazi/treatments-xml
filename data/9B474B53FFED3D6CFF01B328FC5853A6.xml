<taxonomicName id="D4EE81C6FFED3D6CFCEDB11FFC695383" box="[891,953,891,914]" class="Magnoliopsida" family="Moraceae" genus="Ficus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="54" phylum="Tracheophyta" rank="genus">
