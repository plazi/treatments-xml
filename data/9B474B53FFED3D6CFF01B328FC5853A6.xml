<collectingCountry id="6BF9BAD5FFED3D6CFEF1B132FE7D537E" box="[359,429,854,879]" name="China" pageId="3" pageNumber="54">China</collectingCountry>
, 
<collectingCountry id="6BF9BAD5FFED3D6CFE2EB133FE3E537F" box="[440,494,855,878]" name="Laos" pageId="3" pageNumber="54">Laos</collectingCountry>
