<number id="723624B3E150FF9EFCD6FEAD257EFE84" box="[822,834,306,332]" pageId="4" pageNumber="24" value="1.0">1</number>
</emphasis>
<number id="723624B3E150FF9EFCAAFEAD255BFE84" box="[842,871,306,332]" pageId="4" pageNumber="24" value="18.0">18</number>
(
<number id="723624B3E150FF9EFC90FEAD2587FE84" box="[880,955,306,332]" pageId="4" pageNumber="24" value="18.0" valueMax="19.0" valueMin="17.0">17–19</number>
