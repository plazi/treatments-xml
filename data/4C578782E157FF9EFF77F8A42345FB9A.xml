h 
<number id="723624B3E150FF9EFD92FEC72443FEBA" box="[626,639,344,370]" pageId="4" pageNumber="24" value="1.0">1</number>
</emphasis>
<number id="723624B3E150FF9EFD66FEC6249FFEBB" box="[646,675,345,371]" pageId="4" pageNumber="24" value="21.0">21</number>
(
<number id="723624B3E150FF9EFD4CFEC624CBFEBB" box="[684,759,345,371]" pageId="4" pageNumber="24" value="20.5" valueMax="23.0" valueMin="18.0">18–23</number>
