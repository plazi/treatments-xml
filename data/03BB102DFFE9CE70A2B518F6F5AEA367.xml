<taxonomicName id="4C12DAB8FFE9CE4EA61E1B3EF2F1A203" authority="Walker, 1865" authorityName="Walker" authorityYear="1865" box="[1084,1432,1565,1587]" class="Insecta" family="Bombycidae" genus="Mustilia" kingdom="Animalia" order="Lepidoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="falcipennis">
<emphasis id="B9667D29FFE9CE4EA61E1B3EF26EA203" box="[1084,1287,1565,1587]" italics="true" pageId="95" pageNumber="96">Mustilia falcipennis</emphasis>
Walker, 
<number id="3DDAB31CFFE9CE4EA7411B3EF2F1A203" box="[1379,1432,1565,1587]" pageId="95" pageNumber="96" value="1865.0">1865</number>
