<taxonomicName id="B559E2F99889E99E74F7F9140FA5462A" LSID="http://zoobank.org/ED046074-9FC1-45E3-A2F1-2B453A67DECB" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus veintidos" order="Hymenoptera" pageId="421" pageNumber="422" phylum="Arthropoda" rank="species" species="veintidos">
<pageBreakToken id="3BF3EF648442005AA05D8F48859C5126" pageId="421" pageNumber="422" start="start">Heterospilus</pageBreakToken>
veintidos Marsh
</taxonomicName>
<taxonomicNameLabel id="5442E35A1A438326E198A9247049FAB5" pageId="421" pageNumber="422">sp. n.</taxonomicNameLabel>
