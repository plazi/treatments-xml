<location id="AF220AF1BF73FFCF594DFBC1366A4F2C" LSID="urn:lsid:plazi:treatment:2254ED3CBF70FFCF5A77FA4031FD4FD4:AF220AF1BF73FFCF594DFBC1366A4F2C" box="[562,772,1062,1088]" country="Canada" county="southern Appalachians" municipality="Great Plains" name="Rocky Mountains" pageId="30" pageNumber="31" stateProvince="Nova Scotia">Rocky Mountains</location>
