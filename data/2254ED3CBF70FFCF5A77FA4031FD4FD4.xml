<paragraph id="AA425C2ABF73FFCF5A47FC3237B34F04" blockId="30.[264,1322,941,1208]" pageId="30" pageNumber="31">
<emphasis id="98898038BF73FFCF5A47FC3237274883" bold="true" box="[312,585,981,1007]" pageId="30" pageNumber="31">General distribution:</emphasis>
<materialsCitation id="1A955677BF73FFCF5927FC3137B64F04" country="Canada" county="southern Appalachians" location="Rocky Mountains" municipality="Great Plains" pageId="30" pageNumber="31" specimenCount="1" stateProvince="Nova Scotia">
