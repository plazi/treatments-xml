<taxonomicName id="4C294D57FFBA550FCB2E5539C56FF8D8" box="[1117,1323,1885,1910]" class="Insecta" family="Coenagrionidae" genus="Ischnura" kingdom="Animalia" order="Odonata" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="elegans">
