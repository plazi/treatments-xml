<collectingCountry id="F3294AF4FFBE4633FE92F94AFDCEFDC4" box="[359,517,586,611]" name="Turkmenistan" pageId="37" pageNumber="236">Turkmenistan</collectingCountry>
, 
<collectingCountry id="F3294AF4FFBE4633FDE5F94AFD59FDC4" box="[528,658,586,611]" name="Uzbekistan" pageId="37" pageNumber="236">Uzbekistan</collectingCountry>
