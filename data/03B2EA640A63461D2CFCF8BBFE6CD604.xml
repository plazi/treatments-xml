<taxonomicName id="4C1B20F10A62461F2D4DF85EFD19D0A0" authority="Brain. Macropterous" authorityName="Brain. Macropterous" authorityYear="1918" box="[294,722,1999,2024]" class="Insecta" family="Eriococcidae" genus="Calycicoccus" kingdom="Animalia" order="Hemiptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="merwei">
