<taxonomicName id="4C0741F5AC17FFC96D6FFE88FC8AC23F" authority="eclecticus Lowry &amp; Stoddart, 1997" authorityName="eclecticus Lowry &amp; Stoddart" authorityYear="1997" box="[313,830,314,339]" class="Malacostraca" family="Lysianassidae" genus="Eclecticus" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="eclecticus">
