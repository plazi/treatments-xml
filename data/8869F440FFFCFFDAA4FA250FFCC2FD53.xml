<taxonomicName id="C7C03ED5FFFEFFDCA57C2564FFEAFBBF" box="[273,477,1083,1104]" class="Insecta" family="Gryllidae" genus="Xuanwua" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="261" phylum="Arthropoda" rank="species" species="motuoensis" status="sp. nov.">
