<taxonomicName id="C7C03ED5FFFDFFDFA6C426E3FA1CF83D" authority="Chopard, 1928" authorityName="Chopard" authorityYear="1928" box="[681,1067,1980,2002]" class="Insecta" family="Haglotettigoniidae" genus="Valiatrella" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="260" phylum="Arthropoda" rank="species" species="bimaculata">
