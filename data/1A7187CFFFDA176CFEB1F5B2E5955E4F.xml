<taxonomicName id="55D84D5AFFDA176DFEB1F5B2E0E35048" authorityName="Sahni" authorityYear="1972" box="[267,667,2572,2610]" class="Mammalia" family="Taeniolabidae" genus="Cimexomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Multituberculata" pageId="48" pageNumber="365" phylum="Chordata" rank="species" species="judithae" status="new species">Cimexomys judithae</taxonomicName>
, 
<taxonomicNameLabel id="BB9F57B0FFDA176DFD0CF5B1E1F85048" box="[694,896,2575,2610]" pageId="48" pageNumber="365" rank="species">new species</taxonomicNameLabel>
