<taxonomicName id="3ED91DDA6A15FFC0FB78FB2F8F27D8DE" authority="(Meigen)" baseAuthorityName="Meigen" baseAuthorityYear="1830" class="Insecta" family="Sciomyzidae" genus="Ditaeniella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="grisescens">
<emphasis id="CBADBA4B6A15FFC0FB78FB2F8BB8D8B8" box="[1184,1437,1159,1185]" italics="true" pageId="23" pageNumber="24">Ditaeniella grisescens</emphasis>
(Meigen)
</taxonomicName>
, 
<taxonomicName id="3ED91DDA6A15FFC0FEC8FB038C4AD8DE" authority="(Fabricius)" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[272,623,1195,1222]" class="Insecta" family="Sciomyzidae" genus="Hydromya" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="dorsalis">
<emphasis id="CBADBA4B6A15FFC0FEC8FB038FCDD8DD" box="[272,488,1195,1221]" italics="true" pageId="23" pageNumber="24">Hydromya dorsalis</emphasis>
