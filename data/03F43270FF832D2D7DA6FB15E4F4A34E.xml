</taxonomicName>
(
<taxonomicName id="4C5DF8E5FF822D2E7E19FF8EE4A8A290" box="[800,909,121,139]" class="Insecta" family="Carabidae" genus="Himalayodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
