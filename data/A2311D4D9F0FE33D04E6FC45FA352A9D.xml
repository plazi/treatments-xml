<subSubSection id="6282FFD09F0FE33304E6FB6FFD132C52" pageId="22" pageNumber="23" type="materials_examined">
<paragraph id="2A27AC5B9F0FE33304E6FB6FFD132C52" blockId="22.[151,1437,1226,1899]" pageId="22" pageNumber="23">
<emphasis id="18EC70499F0FE33304E6FB6FFEAE2DB6" bold="true" box="[151,262,1226,1251]" pageId="22" pageNumber="23">Material.</emphasis>
<specimenCount id="3C9E67D29F0FE333057FFB6FFE032DB6" box="[270,427,1226,1251]" pageId="22" pageNumber="23" type="generic">37 specimens</specimenCount>
