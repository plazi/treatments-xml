<collectingCountry id="F310A4C85002FFB1FEB5FA60C7B6F9D4" box="[359,426,1533,1558]" name="China" pageId="4" pageNumber="345">China</collectingCountry>
(
<collectingRegion id="49C32ABA5002FFB1FE6BFA60C406F9D4" box="[441,538,1533,1558]" country="China" name="Sichuan" pageId="4" pageNumber="345">Sichuan</collectingRegion>
