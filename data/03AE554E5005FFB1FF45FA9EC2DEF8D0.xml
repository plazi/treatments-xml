<taxonomicName id="4C079FDB5002FFB1FE8CFA01C417FA70" authorityName="Hu &amp; Tang" authorityYear="2018" box="[350,523,1436,1458]" class="Insecta" family="Staphylinidae" genus="Stenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="345" phylum="Arthropoda" rank="species" species="lizipingus">
<emphasis id="B973384A5002FFB1FE8CFA01C417FA70" box="[350,523,1436,1458]" italics="true" pageId="4" pageNumber="345">Stenus lizipingus</emphasis>
</taxonomicName>
. 
<emphasis id="B973384A5002FFB1FDCEFA01C428FA73" bold="true" box="[540,564,1436,1457]" pageId="4" pageNumber="345">11</emphasis>
male sternite VIII 
<emphasis id="B973384A5002FFB1FCD3FA01C506FA73" bold="true" box="[769,794,1436,1457]" pageId="4" pageNumber="345">12</emphasis>
male sternite IX 
<emphasis id="B973384A5002FFB1FC04FA01C204FA73" bold="true" box="[982,1048,1436,1457]" pageId="4" pageNumber="345">13, 14</emphasis>
