<taxonomicName id="4C176CDCCF33EB6EFE3EFE61FD2DB481" box="[500,659,484,509]" class="Arachnida" family="Phytoseiidae" genus="Euseius" kingdom="Animalia" order="Mesostigmata" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="rhusi">
