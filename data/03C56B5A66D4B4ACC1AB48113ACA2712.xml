<taxonomicName id="4C6CA1CF66D4B4ACC2974ECE3C332486" box="[939,1065,1470,1495]" class="Magnoliopsida" family="Lamiaceae" kingdom="Plantae" order="Lamiales" pageId="129" pageNumber="130" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
