<subSubSection id="C37689C766D4B4ACC1FB4ECE3ACA2712" pageId="129" pageNumber="130" type="biology_ecology">
<paragraph id="8BD3DA4C66D4B4ACC1FB4ECE3ACA2712" blockId="129.[151,1437,1002,1603]" pageId="129" pageNumber="130">
<emphasis id="B918065E66D4B4ACC1FB4ECE393D2486" bold="true" box="[199,295,1470,1495]" pageId="129" pageNumber="130">Biology.</emphasis>
