<subSubSection id="98F7651AA5133F3F8EF3FB61FBC15B75" box="[151,1061,1267,1289]" pageId="11" pageNumber="12" type="type_taxon">
<paragraph id="D0523691A5133F3F8EF3FB61FBC15B75" blockId="11.[151,1061,1236,1289]" box="[151,1061,1267,1289]" pageId="11" pageNumber="12">
<typeStatus id="0F568833A5133F3F8EF3FB61FF2D5B75" box="[151,201,1268,1289]" pageId="11" pageNumber="12">Type</typeStatus>
species: 
<taxonomicName id="17ED4D12A5133F3F8F42FB66FCCE5B75" authority="Skoracki et al., 2014" authorityName="Skoracki et al." authorityYear="2014" box="[294,810,1267,1289]" class="Arachnida" family="Syringophilidae" genus="Charadriineopicobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="calidris">
