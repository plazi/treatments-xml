<geoCoordinate id="EE60506BFF5A2E9D6623FE37D0C6CB91" box="[587,649,398,422]" degrees="39" direction="north" orientation="latitude" pageId="182" pageNumber="183" precision="55555" value="39.0">39°N</geoCoordinate>
