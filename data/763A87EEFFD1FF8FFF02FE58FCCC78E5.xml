<paragraph id="FE2C36F8FFD1FF8FFFDEFA54FCCC78E5" blockId="25.[106,1095,1416,1472]" pageId="25" pageNumber="62">
<taxonomicName id="39934D7BFFD1FF8FFFDEFA54FEA27885" authorityName="Lonsdale &amp; Marshall" authorityYear="2007" box="[106,329,1416,1440]" class="Insecta" family="Clusiidae" genus="Heteromeringia" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="62" phylum="Arthropoda" rank="species" species="decora">
