<taxonomicName id="39934D7BFFD1FF8FFCB7FA54FC777885" authorityName="CZERNY" authorityYear="1903" box="[771,924,1417,1440]" class="Insecta" family="Clusiidae" genus="Heteromeringia" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="62" phylum="Arthropoda" rank="genus">
