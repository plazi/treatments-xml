<taxonomicName id="C29F4D3FFFA70555FF3BFD59FEB69FEE" authority="Cavichioli, 2017" authorityName="Cavichioli" authorityYear="2017" box="[151,360,655,681]" class="Insecta" family="Cicadellidae" genus="Kalopona" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="589" phylum="Arthropoda" rank="species" species="sagytta" status="sp. nov.">
<emphasis id="37EBEAAEFFA70555FF3BFD59FEB69FEE" bold="true" box="[151,360,655,681]" italics="true" pageId="3" pageNumber="589">Kalopona sagytta</emphasis>
