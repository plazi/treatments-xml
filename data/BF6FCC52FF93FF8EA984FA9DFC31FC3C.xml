<subSubSection id="7FDC2ECFFF93FF8DA984FA9DFE37FADF" box="[151,472,1297,1323]" pageId="1" pageNumber="478" type="nomenclature">
<paragraph id="37797D44FF93FF8DA984FA9DFE37FADF" blockId="1.[151,472,1297,1323]" box="[151,472,1297,1323]" pageId="1" pageNumber="478">
<heading id="6C31CA28FF93FF8DA984FA9DFE37FADF" bold="true" box="[151,472,1297,1323]" fontSize="11" level="1" pageId="1" pageNumber="478" reason="1">
<emphasis id="05B2A156FF93FF8DA984FA9DFE37FADF" bold="true" box="[151,472,1297,1323]" pageId="1" pageNumber="478">
<taxonomicName id="F0C606C7FF93FF8DA984FA9DFE6FFADF" authority="Chambrier &amp; Beveridge &amp; Scholz, 2018" authorityName="Chambrier &amp; Beveridge &amp; Scholz" authorityYear="2018" box="[151,384,1297,1323]" class="Cestoda" family="Proteocephalidae" genus="Australophiotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Proteocephalidea" pageId="1" pageNumber="478" phylum="Platyhelminthes" rank="genus" status="n. gen.">
<emphasis id="05B2A156FF93FF8DA984FA9DFE6FFADF" bold="true" box="[151,384,1297,1323]" italics="true" pageId="1" pageNumber="478">Australophiotaenia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="1E811C2DFF93FF8DA895FA9DFE37FADF" box="[390,472,1297,1323]" pageId="1" pageNumber="478" rank="genus">n. gen.</taxonomicNameLabel>
