<collectingCountry id="C6DF760DFFABFFDEFF16AEC69BBAFD67" box="[151,217,655,679]" name="Spain" pageId="5" pageNumber="50">Spain</collectingCountry>
, 
<collectingCountry id="C6DF760DFFABFFDEFF65AEC69A20FD67" box="[228,323,655,679]" name="Portugal" pageId="5" pageNumber="50">Portugal</collectingCountry>
and 
<collectingCountry id="C6DF760DFFABFFDEFEF9AEC69AAFFD67" box="[376,460,655,679]" name="Algeria" pageId="5" pageNumber="50">Algeria</collectingCountry>
(
<bibRefCitation id="DA594B6CFFABFFDEFE5BAEC6993CFD67" author="Csiki" box="[474,607,655,679]" pageId="5" pageNumber="50" refString="Csiki, E. (1934) Coleopterorum Catalogus auspiciis et auxilio W. Junk editus a S. Schenkling. Pars 134. Curculionidae: Subfam. Cleoninae. Berlin: Junk. 152 p." type="book" year="1934">
Csiki, 
<number id="080024BAFFABFFDEFDA5AEC6993CFD67" box="[548,607,655,679]" pageId="5" pageNumber="50" value="1934.0">1934</number>
</bibRefCitation>
; 
<bibRefCitation id="DA594B6CFFABFFDEFDEBAEC69815FD67" author="Alonso-Zarazaga" box="[618,886,655,679]" pageId="5" pageNumber="50" refString="Alonso-Zarazaga, M. A. (2002) Lista preliminar des los Coleoptera Curculionoidea del area Ibero-Balear, con description de Melicius gen. nov. y nuevas citas. Boletin de la Sociedad Entomologica Aragonesa, 31, 9 - 93." type="journal article" year="2002">
Alonso-Zarazaga, 
<number id="080024BAFFABFFDEFCB8AEC69815FD67" box="[825,886,655,679]" pageId="5" pageNumber="50" value="2002.0">2002</number>
</bibRefCitation>
). Consequently: 
<taxonomicName id="79C84D1EFFABFFDEFBB1AED89EFFFD67" authority="Olivier, 1807" authorityName="Olivier" authorityYear="1807" box="[1072,1436,655,680]" class="Insecta" family="Curculionidae" genus="Larinus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="species" species="gravidus">
<emphasis id="8CBCEA8FFFABFFDEFBB1AED89F8CFD68" box="[1072,1263,656,680]" italics="true" pageId="5" pageNumber="50">Larinus gravidus</emphasis>
(
<bibRefCitation id="DA594B6CFFABFFDEFB7CAEC69EF0FD67" author="Olivier" box="[1277,1427,655,679]" pageId="5" pageNumber="50" refString="Olivier, A. G. (1807) Entomologie, ou Histoire naturelle des insectes, avec leurs caracteres generiques et specifiques, leur description, leur synonymie, et leur figure enluminee. Coleopteres. Tome cinquieme. Paris: Desray, 612 p." type="book" year="1807">
Olivier, 
<number id="080024BAFFABFFDEFADBAEC69EF0FD67" box="[1370,1427,655,679]" pageId="5" pageNumber="50" value="1807.0">1807</number>
</bibRefCitation>
)
</taxonomicName>
[= 
<taxonomicName id="79C84D1EFFABFFDEFF36AEFC9945FD0C" authority="Capiomont, 1874" authorityName="Capiomont" authorityYear="1874" box="[183,550,692,716]" class="Insecta" family="Curculionidae" genus="Larinus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="species" species="schoenherri" status="syn. nov.">
