<taxonomicName id="79C84D1EFFAAFFDFFCF8AB829FA8F821" authority="Olivier, 1807" authorityName="Olivier" authorityYear="1807" box="[889,1227,1994,2017]" class="Insecta" family="Curculionidae" genus="Larinus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="carthami">
