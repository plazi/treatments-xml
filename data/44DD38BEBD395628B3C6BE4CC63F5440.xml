<paragraph id="BB902F484D65635A15A5A5EA407F3707" pageId="0" pageNumber="1">
<taxonomicName id="52B20E3EAB41B47B7A8A44CF43E49D3A" authority="Sharkey, 2021" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Bioalfa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bioalfa rodrigogamezi" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="rodrigogamezi">
<emphasis id="D2DFC192499F81BD9F73860E788BCBC5" italics="true" pageId="0" pageNumber="1">Bioalfa rodrigogamezi</emphasis>
