<quantity id="4CC7F6DCFFB6FFB807B9F942FD7020C3" box="[676,731,1709,1731]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="21" pageNumber="2978" unit="mm" value="4.0">4 mm</quantity>
......... 13
</keyLead>
</paragraph>
</keyStep>
<keyStep id="30CB439CFFB5FFBB0582FF44FBF02714" pageId="22" pageNumber="2979">
