<quantity id="4CED9B22FA2AFF82BDF9FCD450820263" box="[669,714,867,887]" metricMagnitude="3" metricUnit="m" metricValue="5.0" pageId="3" pageNumber="930" unit="km" value="5.0">5 km</quantity>
