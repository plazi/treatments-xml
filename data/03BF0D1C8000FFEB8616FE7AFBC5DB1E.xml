<paragraph id="8BA9BC0A8000FFEB8616FD94FD21DE09" blockId="3.[264,891,542,662]" box="[264,735,606,630]" pageId="3" pageNumber="4">
<taxonomicName id="4C16C7898000FFEB8616FD94FD25DE09" authority="Tsacas, 1968: 301" authorityName="Tsacas" authorityPageNumber="301" authorityYear="1968" box="[264,731,606,630]" class="Insecta" family="Asilidae" genus="Neomochtherus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="sinensis">
