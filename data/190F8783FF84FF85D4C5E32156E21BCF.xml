<treatmentCitationGroup id="B1B611BBFF84FF85D4C5E3B6554619F1" box="[151,516,1505,1527]" pageId="88" pageNumber="89">
<taxonomicName id="56A64D16FF84FF85D4C5E3B6554219F1" authority="Hood, 1955: 143" authorityName="Hood" authorityPageNumber="143" authorityYear="1955" box="[151,512,1505,1527]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="species" species="tupi">
<emphasis id="A3D2EA87FF84FF85D4C5E3B6560919F1" box="[151,331,1505,1527]" italics="true" pageId="88" pageNumber="89">Holopothrips tupi</emphasis>
<treatmentCitation id="10071084FF84FF85D500E3B6554219F1" author="" box="[338,512,1505,1527]" page="143" pageId="88" pageNumber="89">
<bibRefCitation id="F5374B64FF84FF85D500E3B6568F19F1" box="[338,461,1505,1527]" pageId="88" pageNumber="89" refId="ref58815" refString="Hood, J. D. (1955) Brasilian Thysanoptera VI. Revista Brasileira de Entomologia, 4, 51 - 160." type="journal article">Hood, 1955</bibRefCitation>
