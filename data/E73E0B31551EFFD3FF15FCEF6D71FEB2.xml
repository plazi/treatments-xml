<quantity id="A86F17C2551EFFD7FD9AF91A6E37F8D4" box="[536,608,1770,1795]" metricMagnitude="4" metricUnit="m" metricValue="6.0" pageId="11" pageNumber="212" unit="km" value="60.0">60 km</quantity>
