<taxonomicName id="78BCA98D4F86292EC80DD0B5A9854DE8" lsidName="" pageId="3" pageNumber="4" rank="subFamily" subFamily="Selenopinae">Selenopinae</taxonomicName>
<bibRefCitation id="9F142E18248F2AC1E6AFF6B32C0A256C" author="Simon, E" journalOrPublisher="Encyclopedie Roret, Paris," pageId="68" pageNumber="69" pagination="1 - 192" title="Histoire naturelle des araignees." volume="2" year="1897">Simon 1897</bibRefCitation>
