<taxonomicName id="62C65FC07481CC7881C06E700F72DD1B" class="Arachnida" family="Selenopidae" genus="Selenops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Selenops" order="Araneae" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">Selenops</taxonomicName>
