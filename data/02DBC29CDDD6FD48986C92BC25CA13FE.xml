<taxonomicName id="A2CF6B82C869496B2EEACF3F0ECA053A" authority="Faldermann, 1837" authorityName="Faldermann" authorityYear="1837" class="Insecta" family="Tenebrionidae" genus="Cylindronotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cylindronotus" order="Coleoptera" pageId="180" pageNumber="181" phylum="Arthropoda" rank="genus">Cylindronotus</taxonomicName>
