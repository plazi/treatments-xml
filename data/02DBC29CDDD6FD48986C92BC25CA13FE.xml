<taxonomicName id="D4B354BCC8D946B7CFFCA07C317BD34F" authority="Faldermann, 1837" authorityName="Faldermann" authorityYear="1837" class="Insecta" family="Tenebrionidae" genus="Cylindrinotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cylindrinotus" order="Coleoptera" pageId="180" pageNumber="181" phylum="Arthropoda" rank="genus">Cylindrinotus</taxonomicName>
