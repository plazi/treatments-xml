<taxonomicName id="4C444D4CA912FF8AFE997D6BFDCC4567" baseAuthorityName="L. Koch" baseAuthorityYear="1876" box="[348,546,1044,1068]" class="Arachnida" family="Gnaphosidae" genus="Zelotes" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="508" phylum="Arthropoda" rank="species" species="apricorum">
