<taxonomicName id="4C444D4CA912FF8AFEDB7D0FFE4D45CD" authorityName="Senglet" authorityYear="2012" box="[286,419,1136,1159]" class="Arachnida" family="Gnaphosidae" genus="Civizelotes" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="508" phylum="Arthropoda" rank="genus">
