<taxonomicName id="4C444D4CA912FF8AFC517CEAFBC144E7" baseAuthorityName="Simon" baseAuthorityYear="1914" box="[916,1071,1428,1452]" class="Arachnida" family="Gnaphosidae" genus="Civizelotes" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="508" phylum="Arthropoda" rank="species" species="dentatidens">
<emphasis id="B930EADDA912FF8AFC517CEAFBC144E7" box="[916,1071,1428,1452]" italics="true" pageId="7" pageNumber="508">C. dentatidens</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8BFB36CFA912FF8AFF527CCBFBDF44A6" blockId="7.[151,1073,1172,1517]" pageId="7" pageNumber="508">
<keyLead id="30B595FAA912FF8AFF527CCBFBDF44A6" pageId="7" pageNumber="508">
