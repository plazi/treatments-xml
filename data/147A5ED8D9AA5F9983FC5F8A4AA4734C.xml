<geoCoordinate id="D6658448DAAF77E33C4E8FFEBB4F0170" degrees="67" direction="north" minutes="34" orientation="latitude" precision="1" seconds="3.5" value="67.567635">67°34'3.5&quot;N</geoCoordinate>
, 
<geoCoordinate id="AC9070C0F76935E9EC501822DB8C0208" degrees="62" direction="east" minutes="52" orientation="longitude" precision="1" seconds="17.9" value="62.871635">62°52'17.9&quot;E</geoCoordinate>
; 
<geoCoordinate id="F8DCC8DEB463C342464C6075A0F4B616" degrees="67" direction="north" minutes="31" orientation="latitude" precision="15" seconds="38" value="67.52723">67°31'38&quot;N</geoCoordinate>
, 
<geoCoordinate id="4498087681B986DD4EE3B6DF830C5F29" degrees="62" direction="east" minutes="53" orientation="longitude" precision="1" seconds="2.8" value="62.884113">62°53'2.8&quot;E</geoCoordinate>
