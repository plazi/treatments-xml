<geoCoordinate id="882A1457FFC7544A8D1F4B142F41BA45" degrees="67" direction="north" minutes="36" orientation="latitude" precision="1" seconds="21.2" value="67.60589">67°36'21.2&quot;N</geoCoordinate>
, 
<geoCoordinate id="CE847264C296F40A2E438D1CB7EC2C00" degrees="62" direction="east" minutes="56" orientation="longitude" precision="1" seconds="6.6" value="62.93517">62°56'6.6&quot;E</geoCoordinate>
