<paragraph id="A35036B69E3DFFF5FD0E7022FCC312D6" blockId="57.[701,1228,1024,1179]" pageId="57" pageNumber="57">
<treatmentCitationGroup id="83FF11989E3DFFF5FD0E7022FCC312D6" pageId="57" pageNumber="57">
Now 
<taxonomicName id="64EF4D359E3DFFF5FD45702DFB3E1206" authority="Dalmas, 1900" authorityName="Dalmas" authorityYear="1900" box="[758,1224,1079,1099]" class="Aves" family="Icteridae" genus="Molothrus" kingdom="Animalia" order="Passeriformes" pageId="57" pageNumber="57" phylum="Chordata" rank="subSpecies" species="bonariensis" subSpecies="minimus">
<emphasis id="919BEAA49E3DFFF5FD45702DFBC41207" box="[758,1074,1079,1099]" italics="true" pageId="57" pageNumber="57">Molothrus bonariensis minimus</emphasis>
<bibRefCitation id="C77E4B479E3DFFF5FB84702DFB3E1206" author="Dalmas, R." box="[1079,1224,1079,1099]" pageId="57" pageNumber="57" pagination="132 - 144" refId="ref115320" refString="Dalmas, R. 1900. Note sur une collection d'oiseaux de l'ile de Tobago (mer des Antilles). Memoires de la Societe Zoologique de France 13: 132 - 144." type="journal article" year="1900">Dalmas, 1900</bibRefCitation>
</taxonomicName>
