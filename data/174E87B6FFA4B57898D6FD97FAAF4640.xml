<number id="292F2487FFA4B57998B8F9DAFEFD4148" box="[249,300,1578,1604]" pageId="12" pageNumber="13" value="0.12">0.12</number>
long, 
<number id="292F2487FFA4B5799934F9DAFE794148" box="[373,424,1578,1604]" pageId="12" pageNumber="13" value="0.2">0.20</number>
wide. Sternum 
<number id="292F2487FFA4B5799A21F9DAFD424148" box="[608,659,1578,1604]" pageId="12" pageNumber="13" value="0.82">0.82</number>
long, 
<number id="292F2487FFA4B5799A9DF9DAFCDE4148" box="[732,783,1578,1604]" pageId="12" pageNumber="13" value="0.68">0.68</number>
wide. Leg measurements: I: femur 
<number id="292F2487FFA4B5799CEFF9DAFB044148" box="[1198,1237,1578,1604]" pageId="12" pageNumber="13" value="2.5">2.5</number>
, patella 
<number id="292F2487FFA4B5799D74F9DAFA8D4148" box="[1333,1372,1578,1604]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, tibia 
<number id="292F2487FFA4B57998D6F9A1FF6E4167" box="[151,191,1617,1643]" pageId="12" pageNumber="13" value="2.9">2.9</number>
, metatarsus 
<number id="292F2487FFA4B5799911F9A1FEA94167" box="[336,376,1617,1643]" pageId="12" pageNumber="13" value="3.6">3.6</number>
, tarsus 
<number id="292F2487FFA4B5799991F9A1FE294167" box="[464,504,1617,1643]" pageId="12" pageNumber="13" value="0.7">0.7</number>
, total 
