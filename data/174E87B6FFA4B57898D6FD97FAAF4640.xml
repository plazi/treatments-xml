<number id="292F2487FFA4B5799B7AF85DFCB340CB" box="[827,866,1965,1991]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, 
<number id="292F2487FFA4B5799B2EF85DFC4740CB" box="[879,918,1965,1991]" pageId="12" pageNumber="13" value="1.4">1.4</number>
, 
<number id="292F2487FFA4B5799BE2F85DFC1B40CB" box="[931,970,1965,1991]" pageId="12" pageNumber="13" value="1.8">1.8</number>
, 
<number id="292F2487FFA4B5799B97F85DFC2F40CB" box="[982,1022,1965,1991]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, 
<number id="292F2487FFA4B5799C4BF85DFBE340CB" box="[1034,1074,1965,1991]" pageId="12" pageNumber="13" value="5.5">5.5</number>
; III: 
<number id="292F2487FFA4B5799C2DF85DFB4240CB" box="[1132,1171,1965,1991]" pageId="12" pageNumber="13" value="1.1">1.1</number>
, 
<number id="292F2487FFA4B5799CE1F85DFB1640CB" box="[1184,1223,1965,1991]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, 1.0, 
<number id="292F2487FFA4B5799D49F85DFAFE40CB" box="[1288,1327,1965,1991]" pageId="12" pageNumber="13" value="1.1">1.1</number>
, 
<number id="292F2487FFA4B5799D7AF85DFAB240CB" box="[1339,1379,1965,1991]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, 4.0; IV: 
<number id="292F2487FFA4B5799882F824FF3B40E2" box="[195,234,2004,2030]" pageId="12" pageNumber="13" value="1.6">1.6</number>
