; II: 2.0, 
<number id="292F2487FFA4B5799A9BF9A1FCD04167" box="[730,769,1617,1643]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, 2.0, 
<number id="292F2487FFA4B5799B03F9A1FCB84167" box="[834,873,1617,1643]" pageId="12" pageNumber="13" value="2.4">2.4</number>
, 
<number id="292F2487FFA4B5799B37F9A1FC4C4167" box="[886,925,1617,1643]" pageId="12" pageNumber="13" value="0.5">0.5</number>
, 
<number id="292F2487FFA4B5799BEBF9A1FC034167" box="[938,978,1617,1643]" pageId="12" pageNumber="13" value="7.3">7.3</number>
; III: 
<number id="292F2487FFA4B5799C4DF9A1FBE24167" box="[1036,1075,1617,1643]" pageId="12" pageNumber="13" value="1.3">1.3</number>
, 
<number id="292F2487FFA4B5799C7EF9A1FBB74167" box="[1087,1126,1617,1643]" pageId="12" pageNumber="13" value="0.4">0.4</number>
, 
<number id="292F2487FFA4B5799C32F9A1FB4B4167" box="[1139,1178,1617,1643]" pageId="12" pageNumber="13" value="1.2">1.2</number>
