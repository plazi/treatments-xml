<taxonomicName id="51A94D4EFFD3D532FED5FA57F66417C3" baseAuthorityName="Gerstaecker" baseAuthorityYear="1859" box="[308,775,1411,1430]" class="Insecta" family="Formicidae" genus="Dorylus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="198" phylum="Arthropoda" rank="variety" species="nigricans" subGenus="Anomma" subSpecies="burmeisteri" variety="molestus">
<emphasis id="A4DDEADFFFD3D532FED5FA57F41A17C3" box="[308,377,1412,1430]" italics="true" pageId="5" pageNumber="198">Dorylus</emphasis>
(
<emphasis id="A4DDEADFFFD3D532FE64FA57F4B217C3" box="[389,465,1412,1430]" italics="true" pageId="5" pageNumber="198">Anomma</emphasis>
