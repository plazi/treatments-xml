<taxonomicName id="51A94D4EFFD3D532FC7DFA9DF1231734" authority="" box="[924,1088,1358,1377]" class="Insecta" family="Formicidae" genus="Dorylus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="198" phylum="Arthropoda" rank="subGenus" subGenus="Anomma">
