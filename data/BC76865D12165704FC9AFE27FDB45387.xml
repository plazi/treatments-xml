<taxonomicName id="F3DF4CC812175704FE85FBBBFE055276" box="[287,499,1093,1116]" class="Elasmobranchii" family="Orectolobidae" genus="Orectolobus" kingdom="Animalia" order="Orectolobiformes" pageId="55" pageNumber="55" phylum="Chordata" rank="species" species="parvimaculatus">
