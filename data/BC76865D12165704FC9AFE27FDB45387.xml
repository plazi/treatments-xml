<taxonomicName id="F3DF4CC812175704FDB5FBBBFD785276" box="[559,654,1093,1116]" class="Elasmobranchii" family="Orectolobidae" genus="Orectolobus" kingdom="Animalia" order="Orectolobiformes" pageId="55" pageNumber="55" phylum="Chordata" rank="species" species="halei">
<emphasis id="06ABEB5912175704FDB5FBBBFD785276" box="[559,654,1093,1116]" italics="true" pageId="55" pageNumber="55">O. halei</emphasis>
