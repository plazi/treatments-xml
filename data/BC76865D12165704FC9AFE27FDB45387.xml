<taxonomicName id="F3DF4CC812175704FF5AFB99FEAE5250" box="[192,344,1123,1146]" class="Elasmobranchii" family="Orectolobidae" genus="Orectolobus" kingdom="Animalia" order="Orectolobiformes" pageId="55" pageNumber="55" phylum="Chordata" rank="species" species="maculatus">
<emphasis id="06ABEB5912175704FF5AFB99FEAE5250" box="[192,344,1123,1146]" italics="true" pageId="55" pageNumber="55">O. maculatus</emphasis>
