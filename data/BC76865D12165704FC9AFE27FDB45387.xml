<taxonomicName id="F3DF4CC812175704FE58FB99FDCA5253" box="[450,572,1124,1145]" class="Elasmobranchii" family="Orectolobidae" genus="Orectolobus" kingdom="Animalia" order="Orectolobiformes" pageId="55" pageNumber="55" phylum="Chordata" rank="species" species="ornatus">
