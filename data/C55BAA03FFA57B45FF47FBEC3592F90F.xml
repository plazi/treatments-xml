<taxonomicName id="8AF26096FFA77B5AFB99F91230E0F907" authority="Coelho &amp; Engel, 2018" authorityName="Coelho &amp; Engel" authorityYear="2018" box="[1097,1248,1704,1726]" class="Insecta" family="Halictidae" genus="Ruizantheda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="528" phylum="Arthropoda" rank="species" species="colombiana" status="n. sp.">
