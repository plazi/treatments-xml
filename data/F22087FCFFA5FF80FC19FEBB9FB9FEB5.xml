<taxonomicName id="BD894D69FFA5FF8FFEA5F9099C00F923" authority="Pellmyr &amp; Balcázar-Lara &amp; Segraves &amp; Althoff &amp; Littlefield, 2008" authorityName="Pellmyr &amp; Balcázar-Lara &amp; Segraves &amp; Althoff &amp; Littlefield" authorityYear="2008" box="[300,379,1744,1763]" class="Insecta" family="Prodoxidae" genus="Tegeticula" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="304" phylum="Arthropoda" rank="species" species="baja" status="sp. nov.">
