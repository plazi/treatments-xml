<subSubSection id="07DF35BFB91DFFA6FF5EF9B7FC15139F" pageId="17" pageNumber="45" type="discussion">
<paragraph id="4F7A6634B91DFFA6FF5EF9B7FC15139F" blockId="17.[151,1436,1455,1668]" pageId="17" pageNumber="45">
<emphasis id="7DB1BA26B91DFFA6FF5EF9B7FED81344" bold="true" box="[151,263,1605,1631]" pageId="17" pageNumber="45">Remark.</emphasis>
<taxonomicName id="88C51DB7B91DFFA6FEC6F9B7FE391345" box="[271,486,1605,1630]" class="Insecta" family="Lycidae" genus="Bulenides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="45" phylum="Arthropoda" rank="species" species="longeareolatus">
