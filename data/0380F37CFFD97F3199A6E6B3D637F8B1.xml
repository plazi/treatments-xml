<quantity id="4CD1EF8FFFD87F319B26E086D566FBB9" box="[535,628,1017,1043]" metricMagnitude="3" metricUnit="m" metricValue="2.35" pageId="29" pageNumber="30" unit="m" value="2350.0">2350 m</quantity>
, 
<date id="FF9764AAFFD87F319BB0E086D41DFBB9" box="[641,783,1017,1043]" pageId="29" pageNumber="30" value="1996-07-07">7 July 1996</date>
, 
