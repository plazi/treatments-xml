<taxonomicName id="80B14D3DFFC1495FFF28FB479A0FFADB" authorityName="Schneider" authorityYear="1851" box="[151,280,1257,1283]" class="Insecta" family="Chrysopidae" genus="Apochrysa" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="201" phylum="Arthropoda" rank="genus">
