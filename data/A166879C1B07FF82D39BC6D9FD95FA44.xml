</taxonomicName>
currentlyincludestwovalid species, 
<taxonomicName id="EECF4D091B00FF82D09BC72DFD74FDF9" authority="GotoetOzaki, 1929" authorityName="Gotoet Ozaki" authorityYear="1929" box="[400,755,584,611]" class="Trematoda" family="Microscaphidiidae" genus="Hexangium" kingdom="Animalia" order="Plagiorchiida" pageId="4" pageNumber="7" phylum="Platyhelminthes" rank="species" species="sigani">
