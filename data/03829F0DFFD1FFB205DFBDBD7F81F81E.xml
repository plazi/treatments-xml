<taxonomicName id="4C2B5598FFEEFFB201D8BCDA7F2AF867" authorityName="Reygel, Willems &amp; Artois" authorityYear="2011" box="[1168,1322,1924,1950]" class="Rhabditophora" family="Koinocystididae" genus="Galapagetula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabdocoela" pageId="32" pageNumber="483" phylum="Platyhelminthes" rank="genus">
