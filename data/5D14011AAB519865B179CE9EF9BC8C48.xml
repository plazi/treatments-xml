<taxonomicName id="1FCEA68F2402FBB60F29F63C654D8BF0" LSID="5D14011A-AB51-9865-B179-CE9EF9BC8C48" authority="Dejean, 1826" authorityName="Dejean" authorityYear="1826" class="Insecta" family="Carabidae" genus="Scaphinotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Scaphinotus" order="Coleoptera" pageId="155" pageNumber="156" phylum="Arthropoda" rank="genus">Scaphinotus Dejean, 1826</taxonomicName>
