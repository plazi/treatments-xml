<taxonomicName id="4C7AE1FFEA33FFF7FB19FF444FB9FF2D" authorityName="Breuning" authorityYear="1965" class="Insecta" family="Cerambycidae" genus="Paracanthocinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="399" phylum="Arthropoda" rank="species" species="laosensis">
<emphasis id="B90E466EEA33FFF7FB19FF444FB9FF2D" italics="true" pageId="2" pageNumber="399">Paracanthocinus laosensis</emphasis>
</taxonomicName>
, 
<emphasis id="B90E466EEA33FFF7FEF7FF664E20FF2D" box="[264,352,186,209]" italics="true" pageId="2" pageNumber="399">mihi typ</emphasis>
, Breuning dét. (“ 
<taxonomicName id="4C7AE1FFEA33FFF7FDDFFF664C00FF2D" authorityName="Breuning" authorityYear="1965" box="[544,832,186,209]" class="Insecta" family="Cerambycidae" genus="Paracanthocinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="399" phylum="Arthropoda" rank="species" species="laosensis">
<emphasis id="B90E466EEA33FFF7FDDFFF664C00FF2D" box="[544,832,186,209]" italics="true" pageId="2" pageNumber="399">Paracanthocinus laosensis</emphasis>
