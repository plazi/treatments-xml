<taxonomicName id="4C7AE1FFEA30FFF4FDB9F88B4CAEF890" authority="Tippmann, 1955" authorityName="Tippmann" authorityYear="1955" box="[582,1006,1879,1900]" class="Insecta" family="Cerambycidae" genus="Sternacanista" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="398" phylum="Arthropoda" rank="species" species="retrospinosa">
