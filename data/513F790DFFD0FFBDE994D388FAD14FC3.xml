<taxonomicName id="1E96B398FFD0FFBDEB85D257FC824FE7" authority="Alvizu &amp; Xavier &amp; Rapp, 2019" authorityName="Alvizu &amp; Xavier &amp; Rapp" authorityYear="2019" box="[646,791,874,900]" class="Calcarea" family="Achramorphidae" genus="Achramorpha" kingdom="Animalia" order="Leucosolenida" pageId="5" pageNumber="206" phylum="Porifera" rank="species" species="antarctica" status="sp. nov.">
<emphasis id="EBE21409FFD0FFBDEB85D257FC824FE7" box="[646,791,874,900]" italics="true" pageId="5" pageNumber="206">A. antarctica</emphasis>
</taxonomicName>
<taxonomicNameLabel id="F0D1A972FFD0FFBDEA1DD257FCED4FE0" box="[798,888,874,900]" pageId="5" pageNumber="206" rank="species">
