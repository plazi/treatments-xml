<taxonomicName id="4C7C4D58472DFFC32546FBFB6601A822" authority="Zettel, 2004" authorityName="Zettel" authorityYear="2004" box="[1089,1404,1033,1056]" class="Insecta" family="Gerridae" genus="Neogerris" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="290" phylum="Arthropoda" rank="species" species="philippinensis">
