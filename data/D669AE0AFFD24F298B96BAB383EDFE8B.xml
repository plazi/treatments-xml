<typeStatus id="817BA1BEFFD24F2E8AD5BACF8029FE77" box="[468,516,465,486]" pageId="11" pageNumber="462">Type</typeStatus>
species (by original designation): 
<treatmentCitation id="DF61390DFFD24F2E8879BACF8388FE6E" author="PEARMAN, J. V." page="134" pageId="11" pageNumber="462" year="1935">
<taxonomicName id="99C0649FFFD24F2E8879BACF8388FE6E" authority="Pearman, 1935: 134" authorityName="Pearman" authorityPageNumber="134" authorityYear="1935" class="Insecta" family="Amphientomidae" genus="Nephax" kingdom="Animalia" order="Psocodea" pageId="11" pageNumber="462" phylum="Arthropoda" rank="species" species="sofadanus">
<emphasis id="6CB4C30EFFD24F2E8879BACF861CFE77" box="[888,1073,465,486]" italics="true" pageId="11" pageNumber="462">Nephax sofadanus</emphasis>
<bibRefCitation id="3A5162EDFFD24F2E8BE2BAF48388FE6E" author="PEARMAN, J. V." box="[227,421,490,512]" pageId="11" pageNumber="462" pagination="134 - 137" refId="ref7063" refString="PEARMAN, J. V. 1935. Two remarkable Amphientomids (Psocoptera). Stylops 4 (6): 134 - 137." type="journal article" year="1935">Pearman, 1935: 134</bibRefCitation>
