<taxonomicName id="A6C9A0CFD35FE3691275B5ABFF9EFA90" box="[847,949,1268,1291]" class="Arachnida" family="Agelenidae" genus="Coelotes" kingdom="Animalia" order="Araneae" pageId="83" pageNumber="84" phylum="Arthropoda" rank="genus">
