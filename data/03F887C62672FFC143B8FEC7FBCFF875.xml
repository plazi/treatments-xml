<subSubSection id="C34B655B2672FFC143B8FD9AFAC0FDAE" box="[151,1334,575,600]" pageId="4" pageNumber="33" type="materials_examined">
<paragraph id="8BEE36D02672FFC143B8FD9AFAC0FDAE" blockId="4.[151,1436,575,1932]" box="[151,1334,575,600]" pageId="4" pageNumber="33">
<emphasis id="B925EAC22672FFC143B8FD9AFEDDFDA1" bold="true" box="[151,299,575,600]" pageId="4" pageNumber="33">
<typeStatus id="54EA88722672FFC143B8FD9AFF24FDA1" box="[151,210,576,600]" pageId="4" pageNumber="33">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="4C514D532672FFC14216FD9AFD72FDAE" authority="Stimpson, 1859" authorityName="Stimpson" authorityYear="1859" box="[313,644,575,600]" class="Malacostraca" family="Xanthidae" genus="Actaea" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="erosa">
<emphasis id="B925EAC22672FFC14216FD9AFE3CFDA1" box="[313,458,576,600]" italics="true" pageId="4" pageNumber="33">Actaea erosa</emphasis>
<bibRefCitation id="EFC04B212672FFC142FDFDE5FD72FDAE" author="Stimpson" box="[466,644,575,599]" pageId="4" pageNumber="33" refString="Stimpson, W. (1859) Notes on North American Crustacea, No. 1. Annals of the Lyceum of Natural History of New York, 7, 49 - 93." type="journal article" year="1859">
Stimpson, 
<number id="3D9924F72672FFC14166FDE5FD72FDAE" box="[585,644,575,599]" pageId="4" pageNumber="33" value="1859.0">1859</number>
