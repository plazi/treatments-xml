<taxonomicName id="4C6F4D2846073243FB18F80CDA73D6B2" authorityName="Moser" authorityYear="1919" box="[1185,1351,1964,1990]" class="Insecta" family="Scarabaeidae" genus="Manodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
