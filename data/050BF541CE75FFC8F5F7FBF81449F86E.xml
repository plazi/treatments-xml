<taxonomicName id="4AA23FD4CE75FFC8F5F7FABB145DFAC9" box="[151,373,1340,1362]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="pumilio">
