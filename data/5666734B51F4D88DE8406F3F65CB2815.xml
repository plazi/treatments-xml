<taxonomicName id="96F862C9858B75E57176B2733926B9FA" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="24" pageNumber="25" phylum="Angiospermae" rank="species" species="shawii">
