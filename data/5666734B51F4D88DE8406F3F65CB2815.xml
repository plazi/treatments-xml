<taxonomicName id="6990494F5FE635E56EA3DF5D90FC7CD0" class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="nummularia">
