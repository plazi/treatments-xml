<taxonomicName id="64405FC5539826E16325032FEBA9B8BA" authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="24" pageNumber="25" phylum="Angiospermae" rank="species" species="stricta">
