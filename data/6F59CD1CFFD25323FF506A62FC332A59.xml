<quantity id="2008D1EFFFD25323FE81692EFE212A24" box="[326,404,1554,1580]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="5" pageNumber="394" unit="m" value="1800.0">
<elevation id="6CDD9B39FFD25323FE81692EFE212A24" box="[326,404,1554,1580]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="5" pageNumber="394" unit="m" value="1800.0">1800m</elevation>
</quantity>
in 
<collectingRegion id="2534B2E8FFD25323FE72692FFDBE2A25" box="[437,523,1555,1581]" country="China" name="Yunnan" pageId="5" pageNumber="394">Yunnan</collectingRegion>
and above about 
<quantity id="2008D1EFFFD25323FD09692EFCA82A24" box="[718,797,1554,1580]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="394" unit="m" value="1000.0">
<elevation id="6CDD9B39FFD25323FD09692EFCA82A24" box="[718,797,1554,1580]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="394" unit="m" value="1000.0">1000m</elevation>
</quantity>
in 
<collectingRegion id="2534B2E8FFD25323FCF8692EFC0A2A24" box="[831,959,1554,1580]" country="China" name="Chongqing" pageId="5" pageNumber="394">Chongqing</collectingRegion>
. 
<location id="E22F2AD1FFD25323FC0F692EFC4F2A24" LSID="urn:lsid:plazi:treatment:6F59CD1CFFD25323FF506A62FC332A59:E22F2AD1FFD25323FC0F692EFC4F2A24" box="[968,1018,1554,1580]" name="This" pageId="5" pageNumber="394" stateProvince="Yunnan">This</location>
