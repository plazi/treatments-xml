<taxonomicName id="06BCE1C3FFCEBA0EFC72FC11FB1FFC06" baseAuthorityName="Zehntner" baseAuthorityYear="1894" box="[943,1170,930,951]" class="Malacostraca" family="Sesarmidae" genus="Bresedium" kingdom="Animalia" order="Decapoda" pageId="47" pageNumber="938" phylum="Arthropoda" rank="species" species="laevimanum">
