Gen. 
<taxonomicName id="EB9B1E190E7560DC495305360CCDC560" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2418" class="Insecta" family="Formicidae" genus="Echinopla" kingdom="Animalia" lsidName-HNS="Echinopla Smith" order="Hymenoptera" pageNumber="79" phylum="Arthropoda" rank="genus">Echinopla</taxonomicName>
