<collectorName id="C749531BFF86FFCAFC67FA724F4CFA77" box="[993,1103,1470,1495]" pageId="30" pageNumber="31">V. Aellen</collectorName>
)
</materialsCitation>
. 
<specimenCount id="7CBAFD44FF86FFCAFBE6FA724F80FA77" box="[1120,1155,1471,1495]" pageId="30" pageNumber="31" type="female">1♀</specimenCount>
2 juvs, MHNG, Grot van Fontein [
<geoCoordinate id="0F88500AFF86FFCAFF7CFA2F4A68FA5B" box="[250,363,1506,1531]" direction="north" orientation="latitude" pageId="30" pageNumber="31" precision="55" value="12.495">12.495°N</geoCoordinate>
, 
<geoCoordinate id="0F88500AFF86FFCAFEF1FA2F4AF0FA5B" box="[375,499,1506,1531]" direction="west" orientation="longitude" pageId="30" pageNumber="31" precision="55" value="-69.907">69.907°W</geoCoordinate>
], 
<date id="1E02100DFF86FFCAFE78FA2F4968FA5B" box="[510,619,1506,1531]" pageId="30" pageNumber="31" value="1985-02-12">12.ii.1985</date>
