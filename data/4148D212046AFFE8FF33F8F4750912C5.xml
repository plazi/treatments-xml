<emphasis id="FB95BF16046BFFE8FCFBFEE475601175" box="[863,882,297,322]" italics="true" pageId="59" pageNumber="60">c.</emphasis>
<geoCoordinate id="ACD505C3046BFFE8FCDDFEE475BB1174" box="[889,937,297,323]" degrees="7" direction="north" orientation="latitude" pageId="59" pageNumber="60" precision="55555" value="7.0">7°N</geoCoordinate>
