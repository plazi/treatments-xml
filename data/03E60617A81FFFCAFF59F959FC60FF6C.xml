<taxonomicName id="4C4FCC82A81FFFCDFDF1F8C9FD34F8A1" box="[575,705,1822,1845]" class="Insecta" family="Cicadellidae" genus="Paramesodes" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="56" phylum="Arthropoda" rank="species" species="annamae">
