<taxonomicName id="4C5A3DEAFF81FF9DFA8ED4E9BA22FB10" authorityName="Gray" authorityYear="1842" box="[1295,1385,1178,1202]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="morio">
<emphasis id="B92E9A7BFF81FF9DFA8ED4E9BA54FB10" box="[1295,1311,1179,1202]" italics="true" pageId="31" pageNumber="32">R</emphasis>
. 
<emphasis id="B92E9A7BFF81FF9DFAAAD4E9BA22FB10" box="[1323,1385,1179,1202]" italics="true" pageId="31" pageNumber="32">morio</emphasis>
