<taxonomicName id="4C474D07C426380DD3D4F82385392DDD" authorityName="Fernandez-Triana &amp; Fiaboe" authorityYear="2017" box="[474,569,1930,1956]" class="Insecta" family="Braconidae" genus="Cotesia" kingdom="Animalia" order="Hymenoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="icipe">
