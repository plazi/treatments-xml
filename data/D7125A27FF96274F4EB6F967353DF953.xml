(
<figureCitation id="C780F7B4FF96274C4EBEF8823772F8AF" box="[159,288,1798,1822]" captionStart="FIGURES 23 – 35" captionStartId="5.[151,264,1870,1893]" captionTargetBox="[169,1411,486,1819]" captionTargetId="figure@5.[151,1436,481,1849]" captionTargetPageId="5" captionText="FIGURES 23 – 35. Discophorellus transspinus sp. nov. (23) Head and thorax, dorsal view; (24) Frons; (25) Forewing; (26) Male genitalia, lateral view; (27) Pygofer and genital styles, ventral view; (28) Anal segment, dorsal view; (29) Anal segment, caudal view; (30) Connective, ventrocephalad view; (31) Right genital style, ventral view; (32) Aedeagus, left side; (33) Aedeagus, right side; (34) Aedeagus, dorsal view; (35) Aedeagus, ventral view. Scale bars = 0.5 mm (Figs 27 – 35), 1 mm (Figs 23 – 25)." httpUri="https://zenodo.org/record/201244/files/figure.png" pageId="4" pageNumber="64">
Figs 
<number id="E973F916FF96274C4EF6F88236A6F8AF" box="[215,244,1798,1822]" pageId="4" pageNumber="64" value="23.0">23</number>
–35
</figureCitation>
, 
<figureCitation id="C780F7B4FF96274C4F0AF8823720F8AF" box="[299,370,1798,1822]" captionStart="FIGURES 36 – 39" captionStartId="6.[151,264,1976,1999]" captionTargetBox="[348,1227,195,1953]" captionTargetId="figure@6.[348,1227,193,1954]" captionTargetPageId="6" captionText="FIGURES 36 – 39. Body of adult of Discophorellus. D. cehengensis sp. nov.: 36. dorsal view, 37. lateral view; D. transspinus sp. nov.: 38. dorsal view, 39. lateral view." httpUri="https://zenodo.org/record/201245/files/figure.png" pageId="4" pageNumber="64">
38, 
<number id="E973F916FF96274C4F74F8823720F8AF" box="[341,370,1798,1822]" pageId="4" pageNumber="64" value="39.0">39</number>
