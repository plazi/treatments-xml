<emphasis id="B90B660C5807B574FE82EC7EFE01F9FE" box="[328,460,1589,1611]" italics="true" pageId="2" pageNumber="61">Heleodromia</emphasis>
</taxonomicName>
(
<emphasis id="B90B660C5807B574FE15EC7EFDA9F9FE" box="[479,612,1589,1611]" italics="true" pageId="2" pageNumber="61">Heleodromia</emphasis>
) 
<taxonomicName id="4C7FC19D5807B574FDBDEC7EFD0CF9FE" box="[631,705,1589,1611]" pageId="2" pageNumber="61" rank="species" species="didyma" status="sp. nov.">
<emphasis id="B90B660C5807B574FDBDEC7EFD0CF9FE" box="[631,705,1589,1611]" italics="true" pageId="2" pageNumber="61">didyma</emphasis>
</taxonomicName>
</taxonomicName>
<emphasis id="B90B660C5807B574FD06EC7DFCEDF9FE" bold="true" box="[716,800,1590,1611]" pageId="2" pageNumber="61">
<taxonomicNameLabel id="A238DB775807B574FD06EC7DFCEDF9FE" box="[716,800,1590,1611]" pageId="2" pageNumber="61" rank="species">sp. nov.</taxonomicNameLabel>
