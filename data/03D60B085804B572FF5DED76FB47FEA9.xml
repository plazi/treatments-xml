<taxonomicName id="4C7FC19D5807B574FE82EC7EFD0CF9FE" box="[328,705,1589,1611]" class="Insecta" family="Brachystomatidae" genus="Heleodromia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="didyma" status="sp. nov." subGenus="Heleodromia">
