<collectingRegion id="D070F823FF9C2030FE9A45CAADBCF5D1" box="[363,458,1249,1274]" country="Netherlands" name="Zeeland" pageId="4" pageNumber="560">Zealand</collectingRegion>
</collectingCountry>
: FD, NC (
<bibRefCitation id="76254B30FF9C2030FDCF45CAAF41F5D1" author="Ferris" box="[574,823,1249,1274]" pageId="4" pageNumber="560" refString="Ferris, G. F. &amp; Klyver, F. D. (1932) Report upon a Collection of Chermidae (Homoptera) from New Zealand. Transactions of the New Zealand Institute, 63, 34 - 61." type="journal article" year="1932">Ferris &amp; Klyver 1932</bibRefCitation>
, as 
<taxonomicName id="D5B44D42FF9C2030FC9345CAA800F5D1" authority="Crawford, 1925" authorityName="Crawford" authorityYear="1925" box="[866,1142,1249,1274]" class="Insecta" family="Psyllidae" genus="Metaphalara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="560" phylum="Arthropoda" rank="species" species="zealandica">
