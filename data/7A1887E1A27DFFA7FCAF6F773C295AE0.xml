<emphasis id="C0C5EAE5A27DFFA4FB406F2F3B035EF8" bold="true" box="[1221,1320,626,650]" pageId="17" pageNumber="198">
<collectingRegion id="3075F815A27DFFA4FB406F2F3B035EF8" box="[1221,1320,626,650]" country="Canada" name="Nunavut" pageId="17" pageNumber="198">Nunavut</collectingRegion>
