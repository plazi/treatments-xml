<taxonomicName id="4C429DEEFF96F04AFC37F956FB19F8FB" authority="(Engl.) Eyma" authorityName="Eyma" baseAuthorityName="Engl." box="[912,1272,1790,1817]" class="Magnoliopsida" family="Sapotaceae" genus="Pouteria" kingdom="Plantae" order="Ericales" pageId="32" pageNumber="31" phylum="Tracheophyta" rank="species" species="reticulata">
<emphasis id="B9363A7FFF96F04AFC37F956FBBCF8FA" box="[912,1117,1790,1816]" italics="true" pageId="32" pageNumber="31">Pouteria reticulata</emphasis>
(Engl.) Eyma
</taxonomicName>
(n = 1) (
<taxonomicName id="4C429DEEFF96F04AFAF9F957FEE6F8D9" class="Magnoliopsida" family="Sapotaceae" kingdom="Plantae" order="Ericales" pageId="32" pageNumber="31" phylum="Tracheophyta" rank="family">Sapotaceae</taxonomicName>
) (07-SRNP-35039) (
<tableCitation id="C6C0D3D6FF96F04AFE4DF889FDDAF8D8" box="[490,571,1824,1850]" captionStart="Table 1" captionStartId="8.[192,255,214,240]" captionText="Table 1. Chlamydastis from Costa Rica. Food plant summary. Number (n) of rearing records for each plant species and family." pageId="32" pageNumber="31">Table 1</tableCitation>
