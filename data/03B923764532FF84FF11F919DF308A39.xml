<subSubSection id="C30AC1EB4536FF84FF43FF0CDB0B8BA9" pageId="30" pageNumber="31" type="etymology">
<paragraph id="8BAF92604536FF84FF43FF0CDB0B8BA9" blockId="30.[151,1437,151,327]" pageId="30" pageNumber="31">
<emphasis id="B9644E724536FF84FF43FF0CDB4F8BCF" bold="true" box="[197,332,151,177]" pageId="30" pageNumber="31">Etymology</emphasis>
