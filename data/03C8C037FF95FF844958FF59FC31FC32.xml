<taxonomicName id="4C610AA2FF95FF854958FF59FBDCFE90" authority="(Fabricius, 1801)" baseAuthorityName="Fabricius" baseAuthorityYear="1801" box="[533,1047,234,258]" class="Insecta" family="Lycidae" genus="Metriorrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="1496" phylum="Arthropoda" rank="species" species="thoracicus">
