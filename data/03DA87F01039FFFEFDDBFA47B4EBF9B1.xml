<taxonomicName id="4C734D651038FFFEFD7BFEECB4A0FED8" authorityName="Andrews" authorityYear="1978" box="[644,746,305,330]" class="Insecta" family="Charipidae" genus="Phaenoglyphis" kingdom="Animalia" order="Hymenoptera" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="stenos">
