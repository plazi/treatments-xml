<collectingCountry id="59F9BAF22978FFD4CEC39478FB1CC040" box="[1175,1233,562,586]" name="Chile" pageId="4" pageNumber="906">Chile</collectingCountry>
and 
<collectingCountry id="59F9BAF22978FFD4CF519478FAA7C040" box="[1285,1386,562,586]" name="Argentina" pageId="4" pageNumber="906">Argentina</collectingCountry>
