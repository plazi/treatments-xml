: 
<taxonomicName id="3C844D19E4426E184D2BFBC9FD62FB8D" box="[548,724,1078,1103]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="103" pageNumber="104" phylum="Tracheophyta" rank="species" species="undetermined">
