Rchb.
</taxonomicName>
f.: 
<emphasis id="B904EACF5E1DFFE919F7541DFC5DFCB0" bold="true" box="[1008,1021,774,791]" pageId="37" pageNumber="192">A</emphasis>
, 
<emphasis id="B904EACF5E1DFFE91E0C541DFBB8FCB0" bold="true" box="[1035,1048,774,791]" pageId="37" pageNumber="192">B</emphasis>
, flower; 
<emphasis id="B904EACF5E1DFFE91E6F541DFBD6FCB0" bold="true" box="[1128,1142,774,791]" pageId="37" pageNumber="192">C</emphasis>
, lip; 
<emphasis id="B904EACF5E1DFFE91EA2541DFB13FCB0" bold="true" box="[1189,1203,774,791]" pageId="37" pageNumber="192">D</emphasis>
, 
<taxonomicName id="4C704D5E5E1DFFE91A765400FE86FC8B" authority="Harv." authorityName="Harv." box="[113,294,795,812]" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="37" pageNumber="192" phylum="Tracheophyta" rank="species" species="cardiophora">
