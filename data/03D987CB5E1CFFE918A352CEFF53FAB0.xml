<caption id="DF0F66555E1DFFE91A76541DFCC5FC8B" ID-DOI="http://doi.org/10.5281/zenodo.5186537" ID-Zenodo-Dep="5186537" httpUri="https://zenodo.org/record/5186537/files/figure.png" pageId="37" pageNumber="192" startId="37.[113,124,774,791]" targetBox="[150,1185,211,735]" targetPageId="37">
