<emphasis id="E5BEBB7CFFABFFAEFD18F8E1FD5BF8A0" box="[668,682,1826,1851]" italics="true" pageId="6" pageNumber="39">g</emphasis>
<number id="61027549FFABFFAEFD2FF8F1FD42F8DB" box="[683,691,1842,1856]" pageId="6" pageNumber="39" value="1.0">
<emphasis id="E5BEBB7CFFABFFAEFD2FF8F1FD42F8DB" box="[683,691,1842,1856]" italics="true" pageId="6" pageNumber="39">1</emphasis>
</number>
