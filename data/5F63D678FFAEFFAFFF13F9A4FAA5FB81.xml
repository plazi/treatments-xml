<taxonomicName id="10CA1CEDFFABFFAEFAB6F8E1FF2AF8FD" class="Arachnida" family="Oppiidae" genus="Rhinoppia" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="bipectinata">
<emphasis id="E5BEBB7CFFABFFAEFAB6F8E1FF2AF8FD" italics="true" pageId="6" pageNumber="39">R. bipectinata</emphasis>
</taxonomicName>
and 
<taxonomicName id="10CA1CEDFFABFFAEFE90F88EFE64F8FD" box="[276,405,1869,1894]" class="Arachnida" family="Oppiidae" genus="Rhinoppia" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="plumata">
<emphasis id="E5BEBB7CFFABFFAEFE90F88EFE64F8FD" box="[276,405,1869,1894]" italics="true" pageId="6" pageNumber="39">R. plumata</emphasis>
