<taxonomicName id="4C7469B87C4FFFC3FC51FCD052F6FCCD" baseAuthorityName="Bleeker" baseAuthorityYear="1854" box="[1007,1414,833,856]" class="Actinopterygii" family="Cyprinidae" genus="Schismatorhynchos" kingdom="Animalia" order="Cypriniformes" pageId="2" pageNumber="253" phylum="Chordata" rank="species" species="heterorhynchus">
