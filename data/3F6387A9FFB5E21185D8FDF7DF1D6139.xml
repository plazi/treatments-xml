<taxonomicName id="70CA4D3CFFB5E21184D9FDBEDF5567AC" box="[406,603,620,645]" class="Hydrozoa" family="Tiarannidae" genus="Tiaranna" higherTaxonomySource="CoL" kingdom="Animalia" order="Leptothecata" pageId="88" pageNumber="89" phylum="Cnidaria" rank="species" species="rotunda">
