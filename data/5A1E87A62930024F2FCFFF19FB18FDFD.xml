<collectionCode id="B4A6AE752930024F2EB0FEB9FDCCFEFD" box="[447,538,355,379]" country="USA" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="8" pageNumber="7" type="Museum">USNM</collectionCode>
