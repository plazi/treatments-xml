<number id="DF5624C3FFA2FFD2950DFD9EFC376BC1" box="[890,952,617,643]" pageId="24" pageNumber="25" value="1829.0">1829</number>
, pl. 
<number id="DF5624C3FFA2FFD2959AFD9EFB826BC1" box="[1005,1037,617,643]" pageId="24" pageNumber="25" value="58.0">58</number>
