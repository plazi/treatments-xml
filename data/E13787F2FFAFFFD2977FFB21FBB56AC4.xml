<taxonomicName id="AE9E4D67FFAFFFDF97E8FAD5FC8A6C7A" authority="Walker, 1851" authorityName="Walker" authorityYear="1851" box="[415,773,1313,1337]" class="Insecta" family="Achilidae" genus="Elidiptera" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="fimlivata">
