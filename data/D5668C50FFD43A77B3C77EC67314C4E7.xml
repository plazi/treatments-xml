<number id="EB072F61FFD73A77B63D7CF47375C773" box="[1389,1403,1268,1292]" pageId="18" pageNumber="299" value="8.0">8</number>
<collectionCode id="3BDEA583FFD73A77B62A7CF47381C773" box="[1402,1423,1268,1292]" pageId="18" pageNumber="299">E</collectionCode>
