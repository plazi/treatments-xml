<number id="EB072F61FFD73A77B7BB7E8072F7C4E7" box="[1259,1273,1664,1688]" pageId="18" pageNumber="299" value="8.0">8</number>
<collectionCode id="3BDEA583FFD73A77B7A87E807303C4E7" box="[1272,1293,1664,1688]" pageId="18" pageNumber="299">E</collectionCode>
