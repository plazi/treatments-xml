<taxonomicName id="9ACF46C5FFD73A77B1477E5D747EC40B" box="[535,624,1629,1652]" class="Insecta" family="Pentatomidae" genus="Xynocoris" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="299" phylum="Arthropoda" rank="species" species="egeri" status="sp. nov.">
<emphasis id="6FBBE154FFD73A77B1477E5D747EC40B" box="[535,624,1629,1652]" italics="true" pageId="18" pageNumber="299">X. egeri</emphasis>
</taxonomicName>
<taxonomicNameLabel id="74885C2FFFD73A77B1277E5C74DFC40A" box="[631,721,1628,1653]" pageId="18" pageNumber="299" rank="species">
