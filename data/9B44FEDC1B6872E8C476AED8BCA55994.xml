<taxonomicName id="B96C8D7BD473B4260871718731C9BB4B" class="Insecta" family="Melolonthidae" genus="Liogenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liogenys palmata" order="Coleoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="palmata">Liogenys palmata</taxonomicName>
