longa. Fructus drupaceus, depresse-ovoideus, 
<quantity id="335E9B0A1669FFE0FB69B5FD5D96D2B8" box="[1154,1247,982,1006]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.2" metricValueMin="0.9" pageId="34" pageNumber="185" unit="mm" value="10.5" valueMax="12.0" valueMin="9.0">9-12 mm</quantity>
