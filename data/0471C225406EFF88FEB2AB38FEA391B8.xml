<paragraph id="8C677333406EFF8BFEB2ABA0FAE4952D" blockId="1.[264,1309,1420,1547]" pageId="1" pageNumber="2">
<taxonomicName id="4BD808B0406EFF8BFEB2ABA0FDD596CE" authority="DeLeon, 1966: 86" authorityName="DeLeon" authorityPageNumber="86" authorityYear="1966" box="[264,556,1489,1512]" class="Arachnida" family="Phytoseiidae" genus="Euseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
