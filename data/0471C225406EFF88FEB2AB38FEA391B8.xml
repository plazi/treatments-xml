<taxonomicName id="4BD808B0406EFF8BFEB2ABDEFD3096E0" authority="DeLeon, 1965: 125" authorityName="DeLeon" authorityPageNumber="125" authorityYear="1965" box="[264,713,1455,1478]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="2" phylum="Arthropoda" rank="subGenus" subGenus="Euseius">
