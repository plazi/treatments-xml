Genus 
<taxonomicName id="BAC2FCE0FFC3FF9BFF01E71CFD6CFEF5" authority="Usinger &amp;Matsuda, 1959" authorityName="Usinger &amp;Matsuda" authorityYear="1959" box="[238,718,290,316]" class="Insecta" family="Aradidae" genus="Neoproxius" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="1" pageNumber="48" phylum="Arthropoda" rank="genus">
