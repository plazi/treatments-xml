<taxonomicName id="FFCF4D4DFFA7664B595D95D93F6FF94E" authority="Hassl." authorityName="Hassl." class="Magnoliopsida" family="Moraceae" form="subexcentrica" genus="Dorstenia" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="208" phylum="Tracheophyta" rank="form" species="tubicina">
