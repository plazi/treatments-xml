<taxonomicName id="22DE4D7EFFC32107FDF4FB1E059DFB3E" authorityName="Nair &amp; Janardhanan" authorityYear="1981" baseAuthorityName="Hutch." box="[595,723,1238,1264]" class="Magnoliopsida" family="Oleaceae" genus="Chionanthus" kingdom="Plantae" order="Lamiales" pageId="14" pageNumber="181" phylum="Tracheophyta" rank="species" species="parkinsonii">
