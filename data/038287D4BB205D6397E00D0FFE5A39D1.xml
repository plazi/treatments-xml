<taxonomicName id="4C2B4D41BB225D6395BF0BFBFC893B91" authorityName="Lopes Det. R. Tibana" authorityYear="1946" box="[712,852,151,177]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="edwardsi">
