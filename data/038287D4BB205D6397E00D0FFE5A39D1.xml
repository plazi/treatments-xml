<taxonomicName id="4C2B4D41BB225D63933E0BFBFB2D3B91" box="[1097,1264,151,177]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="genus">
