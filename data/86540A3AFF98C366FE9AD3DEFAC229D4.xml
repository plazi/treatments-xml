<taxonomicName id="C9FDC0AFFF9BC367FE34D02CFD9D2D47" box="[422,576,1798,1821]" class="Insecta" family="Bombyliidae" genus="Villa" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="67" phylum="Arthropoda" rank="species" species="sesivora" status="sp. nov.">
