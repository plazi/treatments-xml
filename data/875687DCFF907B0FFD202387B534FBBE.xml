TYPE SPECIES: 
<taxonomicName id="C8FF4D49FF907B0BFC4323CDB770FAD0" authority="Liu and Engel" authorityName="Liu and Engel" authorityYear="2007" class="Insecta" family="Figitidae" genus="Anteucoila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="delicia" status="new species">
<emphasis id="3D8BEAD8FF907B0BFC4323CDB02BFB35" box="[908,1114,1254,1278]" italics="true" pageId="16" pageNumber="17">Anteucoila delicia</emphasis>
Liu and Engel
</taxonomicName>
, 
<taxonomicNameLabel id="26B857A3FF907B0BFCC12228B7E4FAD0" box="[782,917,1283,1307]" pageId="16" pageNumber="17" rank="species">new species</taxonomicNameLabel>
