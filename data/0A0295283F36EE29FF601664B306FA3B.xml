<taxonomicName id="45AB5FBD3F36EE29FCA811D4B4F0FB3B" authorityName="Latreille" authorityYear="1809" box="[776,902,1181,1205]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="31" phylum="Arthropoda" rank="genus">
