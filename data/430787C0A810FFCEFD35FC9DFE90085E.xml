<location id="CE71600DA810FFCFFD0EFB07FD600DFD" LSID="urn:lsid:plazi:treatment:430787C0A810FFCEFD35FC9DFE90085E:CE71600DA810FFCFFD0EFB07FD600DFD" box="[667,733,1234,1255]" country="Denmark" municipality="Brehm Collection" name="From" pageId="8" pageNumber="8">From</location>
the 
<collectingMunicipality id="2B75ACACA810FFCFFC8BFB04FC4F0DFD" box="[798,1010,1233,1255]" pageId="8" pageNumber="8">Brehm Collection</collectingMunicipality>
