<paragraph id="8BB41D2159239568FFB9FD87A4FDD727" blockId="15.[113,646,523,572]" box="[113,451,549,572]" pageId="15" pageNumber="306">
<collectingRegion id="49CFD3C359239568FFB9FD87A426D727" box="[113,280,549,572]" country="Cambodia" name="Kampong Thom" pageId="15" pageNumber="306">Kompong Thom</collectingRegion>
