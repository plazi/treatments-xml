<taxonomicName id="DD909883411FEF2330294813982A0C6D" authorityName="LeConte" authorityYear="1859" class="Insecta" family="Carabidae" genus="Nomaretus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="159" pageNumber="160" phylum="Arthropoda" rank="species" species="cavicollis">Nomaretus cavicollis</taxonomicName>
