<taxonomicName id="4C744D5DFFF7737B1967FEF4FD72960A" authority="(J. Gray, 1867)" baseAuthorityName="J. Gray" baseAuthorityYear="1867" box="[341,679,286,310]" class="Mammalia" family="Muridae" genus="Rattus" kingdom="Animalia" order="Rodentia" pageId="78" pageNumber="355" phylum="Chordata" rank="species" species="leucopus">
