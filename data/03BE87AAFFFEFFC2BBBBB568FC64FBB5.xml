<taxonomicName id="4C174D3FFFFFFFC2BB4DB20AFE82FAB5" authorityName="Schuh &amp; P. Pedraza" authorityYear="2010" box="[93,272,1298,1319]" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="tasmanensis">
<emphasis id="B963EAAEFFFFFFC2BB4DB20AFE82FAB5" box="[93,272,1298,1319]" italics="true" pageId="81" pageNumber="82">W. tasmanensis</emphasis>
</taxonomicName>
