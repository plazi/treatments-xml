<taxonomicName id="4C03D7EA17644879FAB9FAFEFA69FADE" box="[1284,1436,1342,1365]" class="Insecta" family="Miridae" genus="Labops" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="65" phylum="Arthropoda" rank="species" species="burmeisteri">
