<taxonomicName id="4C03D7EA17644879FBA4FAFEFB6FFADE" box="[1049,1178,1342,1365]" class="Insecta" family="Miridae" genus="Labops" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="65" phylum="Arthropoda" rank="species" species="sahlbergi">
