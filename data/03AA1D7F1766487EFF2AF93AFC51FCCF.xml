<taxonomicName id="4C03D7EA1767487AFA3BF8B9FEE7F83B" class="Insecta" family="Miridae" genus="Labops" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="burmeisteri">
