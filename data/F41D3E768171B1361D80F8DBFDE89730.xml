<taxonomicName id="BBB4F4E38172B1361CB4FF15FDE59597" box="[419,566,152,176]" class="Malacostraca" family="Mysidae" genus="Corellamysis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mysida" pageId="2" pageNumber="325" phylum="Arthropoda" rank="genus">
