<taxonomicName id="864DF7223F7C3DA2FD49A20B1810DDCC" authorityName="Spruce ex Decne., Ann. Gen. Hort. 23: 52. 1881?" authorityYear="1880" class="Magnoliopsida" family="Bombacaceae" genus="Pachira" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pachira humilis" order="Malvales" pageId="0" pageNumber="53" phylum="Tracheophyta" rank="species" species="humilis">Pachira humilis</taxonomicName>
