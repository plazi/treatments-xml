<taxonomicName id="4C4F94A1FFC6FFD80939F975FC2FF94F" baseAuthorityName="Attems" baseAuthorityYear="1936" box="[849,930,1698,1721]" class="Diplopoda" family="Harpagophoridae" genus="Gonoplectus" kingdom="Animalia" order="Spirostreptida" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="cautus">
