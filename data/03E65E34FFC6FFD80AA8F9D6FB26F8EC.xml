<taxonomicName id="4C4F94A1FFC6FFD80EE3F975FEA3F92C" authority="Chamberlin, 1921" authorityName="Chamberlin" authorityYear="1921" class="Diplopoda" family="Harpagophoridae" genus="Gonoplectus" kingdom="Animalia" order="Spirostreptida" pageId="5" pageNumber="4" phylum="Arthropoda" rank="genus">
