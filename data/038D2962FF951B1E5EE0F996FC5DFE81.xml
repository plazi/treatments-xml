</emphasis>
<taxonomicName id="4C24E3F7FF941B1E5F57FF78FDCAFEF7" authorityName="Cassino" authorityYear="1918" box="[315,512,237,263]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="52" phylum="Arthropoda" rank="species" species="elizabeth">
