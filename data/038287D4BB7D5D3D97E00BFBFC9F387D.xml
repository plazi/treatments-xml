</taxonomicName>
(
<taxonomicName id="4C2B4D41BB7C5D3D96460AB7FE0D3AD6" box="[305,464,475,502]" class="Magnoliopsida" family="Polygonaceae" kingdom="Plantae" order="Polygonales" pageId="109" pageNumber="110" phylum="Tracheophyta" rank="family">Polygonaceae</taxonomicName>
