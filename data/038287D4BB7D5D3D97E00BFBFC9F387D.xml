<taxonomicName id="4C2B4D41BB7C5D3D924D0ADBFEFF3AD5" class="Magnoliopsida" family="Polygonaceae" genus="Triplaris" kingdom="Plantae" order="Polygonales" pageId="109" pageNumber="110" phylum="Tracheophyta" rank="species" species="gardneriana">
