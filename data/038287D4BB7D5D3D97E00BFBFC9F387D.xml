<subSubSection id="C3316549BB7C5D3D97B00ADAFD013AD5" pageId="109" pageNumber="110" type="biology_ecology">
<paragraph id="8B9436C2BB7C5D3D97B00ADAFD013AD5" blockId="109.[151,1437,151,862]" pageId="109" pageNumber="110">
<emphasis id="B95FEAD0BB7C5D3D97B00ADAFEFA3AF0" bold="true" box="[199,295,438,464]" pageId="109" pageNumber="110">Biology.</emphasis>
