<taxonomicName id="4C2B4D41BB7C5D3D95A90ADBFC483AF1" authorityName="Lopes &amp; Tibana" authorityYear="1987" box="[734,917,439,465]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="109" pageNumber="110" phylum="Arthropoda" rank="species" species="wygodzinskyi">
