<taxonomicName id="E28324AF3F4EFFC7F41DFD90EE522442" box="[774,868,566,587]" class="Insecta" family="Carabidae" genus="Typhlocharis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="248" phylum="Arthropoda" rank="species" species="gomezi">
