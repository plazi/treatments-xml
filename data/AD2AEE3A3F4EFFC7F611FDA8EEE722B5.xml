<taxonomicName id="E28324AF3F4EFFC7F78FFDF7EDDF246F" box="[148,233,593,614]" class="Insecta" family="Carabidae" genus="Typhlocharis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="248" phylum="Arthropoda" rank="species" species="hiekei">
<emphasis id="17F7833E3F4EFFC7F78FFDF7EDDF246F" box="[148,233,593,614]" italics="true" pageId="9" pageNumber="248">T. hiekei</emphasis>
</taxonomicName>
, 
<taxonomicName id="E28324AF3F4EFFC7F7E2FDF7EC6A246F" box="[249,348,593,614]" class="Insecta" family="Carabidae" genus="Typhlocharis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="248" phylum="Arthropoda" rank="species" species="passosi">
<emphasis id="17F7833E3F4EFFC7F7E2FDF7EC6A246F" box="[249,348,593,614]" italics="true" pageId="9" pageNumber="248">T. passosi</emphasis>
</taxonomicName>
