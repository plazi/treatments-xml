<taxonomicName id="E28324AF3F4EFFC7F689FDF7EF2D246F" box="[402,539,593,614]" class="Insecta" family="Carabidae" genus="Typhlocharis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="248" phylum="Arthropoda" rank="species" species="fozcoaensis">
