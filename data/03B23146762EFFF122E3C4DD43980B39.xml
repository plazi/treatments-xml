<taxonomicName id="4C1BFBD3762DFFF023CFC4E1410C0D88" box="[443,634,1172,1194]" class="Insecta" family="Staphylinidae" genus="Paraneseuthia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="568" phylum="Arthropoda" rank="species" species="angustifurculata">
<emphasis id="B96F5C42762DFFF023CFC4E1410C0D88" box="[443,634,1172,1194]" italics="true" pageId="2" pageNumber="568">P. angustifurculata</emphasis>
</taxonomicName>
(3, 4c); 
<taxonomicName id="4C1BFBD3762DFFF020BBC4E140870D8B" authority="Jaloszynski, 2011" authorityName="Jaloszynski" authorityYear="2011" box="[719,1009,1172,1194]" class="Insecta" family="Staphylinidae" genus="Paraneseuthia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="568" phylum="Arthropoda" rank="species" species="carltoni">
<emphasis id="B96F5C42762DFFF020BBC4E140410D88" box="[719,823,1172,1194]" italics="true" pageId="2" pageNumber="568">P. carltoni</emphasis>
Jałoszyński, 2011
</taxonomicName>
(4d); and 
<taxonomicName id="4C1BFBD3762DFFF0262CC4E146EA0D8B" authority="Jaloszynski, 2011" authorityName="Jaloszynski" authorityYear="2011" box="[1112,1436,1172,1194]" class="Insecta" family="Staphylinidae" genus="Paraneseuthia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="568" phylum="Arthropoda" rank="species" species="booloumba">
<emphasis id="B96F5C42762DFFF0262CC4E147950D88" box="[1112,1251,1172,1194]" italics="true" pageId="2" pageNumber="568">P. booloumba</emphasis>
Jałoszyński, 2011
