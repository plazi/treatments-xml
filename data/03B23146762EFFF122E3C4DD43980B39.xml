<taxonomicName id="4C1BFBD3762DFFF021C2C40147BF0DAB" box="[950,1225,1141,1162]" class="Insecta" family="Staphylinidae" genus="Paraneseuthia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="568" phylum="Arthropoda" rank="species" species="bernierana" status="sp. n.">
<emphasis id="B96F5C42762DFFF021C2C40147BF0DAB" box="[950,1225,1141,1162]" italics="true" pageId="2" pageNumber="568">Paraneseuthia bernierana</emphasis>
</taxonomicName>
