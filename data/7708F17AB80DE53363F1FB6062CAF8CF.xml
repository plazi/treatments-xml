<taxonomicName id="38A13BEFB80DE533624DF8B762F4F816" authorityName="C.Linnaeus" authorityYear="1753" box="[299,408,1839,1865]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="genus">
<emphasis id="CDD59C7EB80DE533624DF8B762F4F816" box="[299,408,1839,1865]" italics="true" pageId="47" pageNumber="48">Artemisia</emphasis>
</taxonomicName>
