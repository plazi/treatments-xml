<taxonomicName id="D89FBBBCFFC3FFF80136C1D1FAD7F87E" authority="Botero &amp; Heffern &amp; Santos-Silva, 2018" authorityName="Botero &amp; Heffern &amp; Santos-Silva" authorityYear="2018" box="[1065,1297,1946,1971]" class="Insecta" family="Cerambycidae" genus="Eurysthea" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="nogueirai" status="new species">
