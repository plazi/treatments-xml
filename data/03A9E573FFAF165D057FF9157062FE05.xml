<taxonomicName id="4C002FE6FFAF165A057FF91571F9F8EB" authority="Muller, 1907" authorityName="Muller" authorityYear="1907" box="[264,710,1782,1808]" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="spinicollis">
