<subSubSection id="C36F6546FFD7FFE470EBFB5CFAD5FEBB" box="[201,1396,1221,1246]" pageId="91" pageNumber="92" type="biology_ecology">
<paragraph id="8BCA36CDFFD7FFE470EBFB5CFAD5FEBB" blockId="91.[151,1437,1077,1569]" box="[201,1396,1221,1246]" pageId="91" pageNumber="92">
