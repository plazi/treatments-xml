<quantity id="4CA69B40D473FFFBFEAC1402FE496FB6" box="[328,388,1654,1676]" metricMagnitude="4" metricUnit="m" metricValue="4.0" pageId="63" pageNumber="64" unit="km" value="40.0">40 km</quantity>
