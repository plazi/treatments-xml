</emphasis>
<taxonomicName id="4C5E4D26D473FFFBFC0A1442FB806F76" authorityName="Gregor &amp; Povolny" authorityYear="1983" box="[1006,1101,1591,1612]" class="Insecta" family="Nepticulidae" genus="Ectoedemia" kingdom="Animalia" order="Lepidoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="spiraeae">
