<bibRefCitation id="EFB74B653E748E03FC1EFD84FBEAC13C" author="Hsiao" box="[1009,1144,583,608]" pageId="12" pageNumber="113" refString="Hsiao, Y. (2015) Description of two new species of the genus Stenothemus from Taiwan (Coleoptera: Cantharidae). Zootaxa, 3937 (2), 386 - 392. http: // dx. doi. org / 10.11646 / zootaxa. 3937.2.9" type="journal article" year="2015">
Hsiao, 
<number id="3DEE24B33E748E03FBAFFD84FBEAC13C" box="[1088,1144,583,608]" pageId="12" pageNumber="113" value="2015.0">2015</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="4C264D173E748E03FB42FD8BFAEDC13C" authority="Hsiao, 2015" authorityName="Hsiao" authorityYear="2015" box="[1197,1407,583,608]" class="Insecta" family="Cantharidae" genus="Stenothemus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="cou">
<emphasis id="B952EA863E748E03FB42FD8BFB63C13C" box="[1197,1265,584,608]" italics="true" pageId="12" pageNumber="113">S. cou</emphasis>
<bibRefCitation id="EFB74B653E748E03FB17FD84FAEDC13C" author="Hsiao" box="[1272,1407,583,608]" pageId="12" pageNumber="113" refString="Hsiao, Y. (2015) Description of two new species of the genus Stenothemus from Taiwan (Coleoptera: Cantharidae). Zootaxa, 3937 (2), 386 - 392. http: // dx. doi. org / 10.11646 / zootaxa. 3937.2.9" type="journal article" year="2015">
Hsiao, 
<quantity id="4CDE9B713E748E03FAA8FD84FAEDC13C" box="[1351,1407,583,608]" metricMagnitude="1" metricUnit="m" metricValue="5.1181" pageId="12" pageNumber="113" unit="in" value="2015.0">2015</quantity>
