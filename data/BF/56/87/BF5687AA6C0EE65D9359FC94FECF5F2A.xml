<taxonomicName authorityName="Gibson &amp; Skevington" authorityYear="2013" baseAuthorityName="Gibson &amp; Skevington" baseAuthorityYear="1926" box="[172,315,1376,1397]" class="Insecta" family="Conopidae" genus="Physoconops" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="64" phylum="Arthropoda" rank="genus">
