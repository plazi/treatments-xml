<location LSID="urn:lsid:plazi:treatment:BF46007EFFAFAD7EFDAE0B8C66A4FBCE:3230E7B3FFAFAD7CFE2C0D7C6742FA9A" box="[420,518,1347,1371]" country="Kenya" municipality="Nairobi" name="Muguga" pageId="25" pageNumber="761">Muguga</location>
, near 
<collectingMunicipality box="[589,681,1347,1371]" pageId="25" pageNumber="761">Nairobi</collectingMunicipality>
, 
<date box="[693,913,1347,1371]" pageId="25" pageNumber="761" value="2004-11-20">
<collectingDate box="[693,913,1347,1371]" pageId="25" pageNumber="761" value="2004-11-20">20 November 2004</collectingDate>
