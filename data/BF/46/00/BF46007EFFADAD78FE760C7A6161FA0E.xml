<typeStatus box="[213,316,1497,1521]" pageId="27" pageNumber="763">Holotype</typeStatus>
</emphasis>
<collectingCountry box="[324,402,1496,1520]" name="Kenya" pageId="27" pageNumber="763">Kenya</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:BF46007EFFADAD78FE760C7A6161FA0E:3230E7B3FFADAD7EFE170DE76745FA31" box="[415,513,1496,1520]" country="Kenya" municipality="Nairobi" name="Muguga" pageId="27" pageNumber="763">Muguga</location>
, near 
