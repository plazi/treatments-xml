<collectionCode box="[517,612,1560,1584]" country="Hungary" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" lsid="urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="27" pageNumber="763">HNHM</collectionCode>
, 
<specimenCount box="[626,791,1560,1584]" pageId="27" pageNumber="763" type="generic" typeStatus="paratypes">two paratypes</specimenCount>
