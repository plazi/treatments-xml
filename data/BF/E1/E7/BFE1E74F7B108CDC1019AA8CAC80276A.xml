<paragraph pageId="null" pageNumber="435">
<taxonomicName authority="DC." class="Polychaeta" family="Asteraceae" genus="Serratula" kingdom="Animalia" order="Asterales" pageId="null" pageNumber="435" phylum="Annelida" rank="species" species="nudicaulis">
