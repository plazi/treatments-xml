<taxonomicName authority="(Gunther, 1858)" authorityName="Gunther" authorityYear="1858" baseAuthorityName="Gunther" baseAuthorityYear="1858" box="[994,1429,469,494]" class="Reptilia" family="Elapidae" genus="Austrelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="18" pageNumber="495" phylum="Chordata" rank="species" species="superbus">
