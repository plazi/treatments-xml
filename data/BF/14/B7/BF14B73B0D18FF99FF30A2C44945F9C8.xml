<taxonomicName authority="Runyon, 2019" authorityName="Runyon" authorityYear="2019" box="[993,1171,1165,1187]" class="Insecta" family="Dolichopodidae" genus="Hurleyella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="550" phylum="Arthropoda" rank="species" species="salina" status="sp. nov.">
