<taxonomicName authorityName="Janka Oesterr. Bot. Z. 22: 177 - 178" authorityYear="1872" class="Aves" family="Apiaceae" genus="Oenanthe" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Oenanthe millefolia" order="Apiales" pageId="4" pageNumber="35" phylum="Chordata" rank="species" species="millefolia">
