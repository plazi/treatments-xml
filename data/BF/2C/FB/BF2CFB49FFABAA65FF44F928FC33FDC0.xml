<collectingRegion box="[819,1006,475,501]" country="India" name="Madhya Pradesh" pageId="10" pageNumber="502">Madhya Pradesh</collectingRegion>
and 
<collectingRegion box="[1058,1204,475,501]" country="India" name="Chhattisgarh" pageId="10" pageNumber="502">Chhattisgarh</collectingRegion>
, previously recorded from 
<collectingCountry box="[213,271,511,537]" name="India" pageId="10" pageNumber="502">India</collectingCountry>
(
<collectingRegion box="[286,427,511,537]" country="India" name="Uttarakhand" pageId="10" pageNumber="502">Uttarakhand</collectingRegion>
, 
<collectingRegion box="[438,582,511,538]" country="India" name="West Bengal" pageId="10" pageNumber="502">West Bengal</collectingRegion>
) and 
<collectingCountry box="[644,711,511,537]" name="Nepal" pageId="10" pageNumber="502">Nepal</collectingCountry>
(
<bibRefCitation author="Ahrens, D. &amp; Fabrizi, S." box="[725,991,511,538]" pageId="10" pageNumber="502" pagination="1 - 355" refId="ref15580" refString="Ahrens, D. &amp; Fabrizi, S. (2016) A monograph of the Sericini of India (Coleoptera: Scarabaeidae). Bonn Zoological Bulletin, 65, 1 - 355." type="journal article" year="2016">Ahrens &amp; Fabrizi 2016</bibRefCitation>
