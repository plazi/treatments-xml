<typeStatus box="[608,656,835,861]" pageId="6" pageNumber="498">type</typeStatus>
locality (
<figureCitation box="[763,846,835,861]" captionStart="FIGURE 21" captionStartId="8.[151,250,1889,1913]" captionTargetBox="[163,1424,985,1840]" captionTargetId="figure-279@8.[151,1436,957,1866]" captionTargetPageId="8" captionText="FIGURE 21. Map of India showing type localities of the newly described species." figureDoi="http://doi.org/10.5281/zenodo.4668225" httpUri="https://zenodo.org/record/4668225/files/figure.png" pageId="6" pageNumber="498">Fig. 21</figureCitation>
