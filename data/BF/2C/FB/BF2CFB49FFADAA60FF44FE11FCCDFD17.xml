<subSubSection pageId="15" pageNumber="507" type="discussion">
<paragraph blockId="15.[151,1437,510,717]" pageId="15" pageNumber="507">
<emphasis bold="true" box="[199,304,654,680]" pageId="15" pageNumber="507">Remark.</emphasis>
