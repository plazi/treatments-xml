] 
<number box="[503,533,907,933]" pageId="12" pageNumber="30" value="39.0">39</number>
–41, 40 μm from base. AR [
<number box="[857,913,907,933]" pageId="12" pageNumber="30" value="1.21">1.21</number>
] 
<number box="[927,1046,907,933]" pageId="12" pageNumber="30" value="1.065" valueMax="1.21" valueMin="0.92">0.92–1.21</number>
, 
<number box="[1058,1113,907,933]" pageId="12" pageNumber="30" value="1.01">1.01</number>
. Antennal blade [
<number box="[1315,1348,907,933]" pageId="12" pageNumber="30" value="39.0">39</number>
] 
<number box="[1363,1436,907,933]" pageId="12" pageNumber="30" value="40.0" valueMax="41.0" valueMin="39.0">39–41</number>
