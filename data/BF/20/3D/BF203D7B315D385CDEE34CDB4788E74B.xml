<paragraph blockId="2.[151,728,1961,1987]" box="[151,728,1961,1987]" pageId="2" pageNumber="193">
<heading bold="true" box="[151,728,1961,1987]" fontSize="11" level="1" pageId="2" pageNumber="193" reason="1">
<emphasis bold="true" box="[151,728,1961,1987]" pageId="2" pageNumber="193">
Genus 
<taxonomicName authority="Quatrefages, 1865" authorityName="Quatrefages" authorityYear="1865" box="[238,594,1961,1987]" class="Polychaeta" family="Eunicidae" genus="Marphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Eunicida" pageId="2" pageNumber="193" phylum="Annelida" rank="genus">
<emphasis bold="true" box="[238,360,1961,1987]" italics="true" pageId="2" pageNumber="193">Marphysa</emphasis>
Quatrefages, 1865
</taxonomicName>
, restricted
</emphasis>
