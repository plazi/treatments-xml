<subSubSection type="nomenclature">
<paragraph pageNumber="275">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181724" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole cielana Wilson" lsidName-HNS="Pheidole cielana Wilson" order="Hymenoptera" pageNumber="275" phylum="Arthropoda" rank="species" species="cielana">Pheidole cielana</taxonomicName>
