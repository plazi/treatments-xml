<subSubSection pageId="5" pageNumber="259" type="materials_examined">
<paragraph blockId="5.[151,1437,153,1727]" pageId="5" pageNumber="259">
<emphasis bold="true" box="[199,427,1088,1115]" pageId="5" pageNumber="259">Material examined</emphasis>
