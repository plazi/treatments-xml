(
<number box="[421,432,1543,1569]" pageId="5" pageNumber="6" value="1.0">1</number>
)­ 
<number box="[453,469,1543,1569]" pageId="5" pageNumber="6" value="5.0">5</number>
­ 
<number box="[479,495,1543,1569]" pageId="5" pageNumber="6" value="5.0">5</number>
, tibiae 
<number box="[582,595,1543,1569]" pageId="5" pageNumber="6" value="3.0">3</number>
