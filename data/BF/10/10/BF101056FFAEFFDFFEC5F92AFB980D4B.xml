<number box="[764,778,846,872]" pageId="8" pageNumber="9" value="6.0">6</number>
­ 
<number box="[802,817,846,872]" pageId="8" pageNumber="9" value="2.0">2</number>
­ 
<number box="[841,859,846,872]" pageId="8" pageNumber="9" value="2.0">2</number>
