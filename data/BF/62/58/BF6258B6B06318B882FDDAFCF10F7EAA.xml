<taxonomicName LSID="http://zoobank.org/5194EC74-6F45-4D90-8F3F-797267412A3D" class="Collembola" family="Tullbergiidae" genus="Mexicaphorura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mexicaphorura" order="Poduromorpha" pageId="1" pageNumber="92" phylum="Arthropoda" rank="genus">Mexicaphorura</taxonomicName>
