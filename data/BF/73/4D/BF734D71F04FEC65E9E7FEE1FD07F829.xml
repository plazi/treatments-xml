HL 
<number box="[929,1041,541,565]" pageId="11" pageNumber="512" value="0.725" valueMax="0.76" valueMin="0.69">0.69–0.76</number>
X HW, HW 
<number box="[1192,1303,541,565]" pageId="11" pageNumber="512" value="1.045" valueMax="1.06" valueMin="1.03">1.03–1.06</number>
X TW, FW 
<number box="[151,262,577,601]" pageId="11" pageNumber="512" value="1.255" valueMax="1.31" valueMin="1.2">1.20–1.31</number>
X FH, EL 
<number box="[389,502,577,601]" pageId="11" pageNumber="512" value="0.765" valueMax="0.8" valueMin="0.73">0.73–0.80</number>
