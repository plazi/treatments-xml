<collectingCountry box="[610,721,474,499]" name="Argentina" pageId="7" pageNumber="147">Argentina</collectingCountry>
) regions; 
<collectingCountry box="[842,965,474,499]" name="Azerbaijan" pageId="7" pageNumber="147">Azerbaijan</collectingCountry>
