<collectingCountry box="[994,1090,438,463]" name="Pakistan" pageId="7" pageNumber="147">Pakistan</collectingCountry>
, 
<collectingCountry box="[1107,1192,438,463]" name="Taiwan" pageId="7" pageNumber="147">Taiwan</collectingCountry>
