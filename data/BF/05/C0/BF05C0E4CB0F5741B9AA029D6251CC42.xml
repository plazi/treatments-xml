<subSubSection pageId="68" pageNumber="69" type="type locality">
<paragraph pageId="68" pageNumber="69">Type locality.</paragraph>
<paragraph pageId="68" pageNumber="69">[Bolivia] &quot;provincia Chiquitensi, republica Boliviana&quot;.</paragraph>
</subSubSection>
<subSubSection pageId="68" pageNumber="69" type="label">
<paragraph pageId="68" pageNumber="69">Label.</paragraph>
<paragraph pageId="68" pageNumber="69">
&quot;Sn Xavier de Chiquitos Bolivia&quot;, label in 
<normalizedToken originalValue="d’Orbigny’s">d'Orbigny's</normalizedToken>
handwriting.
</paragraph>
</subSubSection>
<subSubSection pageId="68" pageNumber="69" type="dimensions">
