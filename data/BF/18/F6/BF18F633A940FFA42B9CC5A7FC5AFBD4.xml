stratum and 
<typeStatus box="[419,470,1873,1899]" pageId="64" pageNumber="65">type</typeStatus>
locality.
</emphasis>
<location LSID="urn:lsid:plazi:treatment:BF18F633A940FFA42B9CC5A7FC5AFBD4:326E11FEA940FFA5294AC61CFC05F888" box="[577,940,1873,1899]" country="Denmark" municipality="Faxe Quarry" name="Middle Danian Coral Limestone" pageId="64" pageNumber="65">Middle Danian Coral Limestone</location>
from 
<collectingMunicipality box="[1008,1150,1873,1899]" pageId="64" pageNumber="65">Faxe Quarry</collectingMunicipality>
, 
<collectingCountry box="[1161,1269,1873,1899]" name="Denmark" pageId="64" pageNumber="65">Denmark</collectingCountry>
