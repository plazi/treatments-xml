<location LSID="urn:lsid:plazi:treatment:BF18F633A94AFFAE2B9CC591FC68FDDC:326E11FEA94AFFAF2E53C775FF48F995" municipality="Stevns Klint" name="Kulsti Rende" pageId="74" pageNumber="75">Kulsti Rende</location>
