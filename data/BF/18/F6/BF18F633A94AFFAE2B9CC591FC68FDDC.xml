</paragraph>
<paragraph blockId="74.[151,1437,1412,2015]" box="[199,1267,1700,1727]" pageId="74" pageNumber="75">
<emphasis bold="true" box="[199,570,1700,1727]" pageId="74" pageNumber="75">
<typeStatus box="[199,258,1701,1727]" pageId="74" pageNumber="75">Type</typeStatus>
stratum and 
<typeStatus box="[419,470,1701,1727]" pageId="74" pageNumber="75">type</typeStatus>
