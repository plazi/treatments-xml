<typeStatus box="[753,851,960,986]" pageId="104" pageNumber="105">holotype</typeStatus>
<collectionCode box="[861,947,960,986]" country="Denmark" httpUri="http://grbio.org/cool/c1ae-v64p" name="Museum Geologicum Universitatis Hafniensis" pageId="104" pageNumber="105">MGUH</collectionCode>
3148 and 
<typeStatus box="[1071,1167,961,987]" pageId="104" pageNumber="105">paratype</typeStatus>
<collectionCode box="[1177,1263,960,986]" country="Denmark" httpUri="http://grbio.org/cool/c1ae-v64p" name="Museum Geologicum Universitatis Hafniensis" pageId="104" pageNumber="105">MGUH</collectionCode>
