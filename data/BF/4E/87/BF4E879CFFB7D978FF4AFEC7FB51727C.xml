<quantity box="[693,835,474,499]" metricMagnitude="3" metricUnit="m" metricValue="1.043" metricValueMax="1.8" metricValueMin="0.286" pageId="10" pageNumber="456" unit="m" value="1043.0" valueMax="1800.0" valueMin="286.0">286–1800 m</quantity>
