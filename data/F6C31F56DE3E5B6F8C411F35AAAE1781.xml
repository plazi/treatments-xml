<taxonomicName id="CCC47ADE877CEADC80730033D8CC834C" authorityName="Baehr, Schule &amp; Lorenz" authorityYear="2009" class="Insecta" family="Carabidae" genus="Afrogehringia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="308" pageNumber="309" phylum="Arthropoda" rank="genus">
