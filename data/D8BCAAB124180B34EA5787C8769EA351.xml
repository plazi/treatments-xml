<taxonomicName id="D9A4D9A1DCAC416D6AF0A6E2494B457A" authority="LeConte, 1874" authorityName="LeConte" authorityYear="1874" lsidName="" pageId="250" pageNumber="251" rank="tribe" tribe="Rhinostomini">Rhinostomini LeConte, 1874</taxonomicName>
