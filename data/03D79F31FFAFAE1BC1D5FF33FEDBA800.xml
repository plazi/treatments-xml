<taxonomicName id="4C7E55A4FFAFAE1BC3A3FC44FB4CA91E" authority="Robineau-Desvoidy, 1830" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[737,1233,588,614]" class="Insecta" family="Sepsidae" genus="Themira" kingdom="Animalia" order="Diptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="pilosa">
