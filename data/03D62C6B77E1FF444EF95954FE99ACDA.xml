<collectingCountry id="F368DDED77E0FF444F955EE6FDFCAE8A" box="[507,595,223,249]" name="Mexico" pageId="131" pageNumber="132">Mexico</collectingCountry>
, 
<collectingCountry id="F368DDED77E0FF444C305EE6FD75AE8A" box="[606,730,223,249]" name="Guatemala" pageId="131" pageNumber="132">Guatemala</collectingCountry>
