. 
<taxonomicName id="4C7FE6FE77E0FF444F2C5F8EFDC9AFA3" authorityName="Glassell" authorityYear="1936" box="[322,614,439,465]" class="Malacostraca" family="Porcellanidae" genus="Porcellana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="paguriconviva">
