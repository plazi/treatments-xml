<paragraph id="D10D36F999082B39BD71FAC6FDAFFACF" blockId="15.[306,614,1336,1360]" box="[306,614,1336,1360]" pageId="15" pageNumber="407">
<heading id="8A45819599082B39BD71FAC6FDAFFACF" bold="true" box="[306,614,1336,1360]" centered="true" fontSize="10" level="2" pageId="15" pageNumber="407" reason="2">
