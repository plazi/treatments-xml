<taxonomicName id="4C03C086FF9EB66EFEC3F886FD231D5C" authority="Vervoort, 1993" authorityName="Vervoort" authorityYear="1993" box="[345,724,1832,1856]" class="Hydrozoa" family="Sertulariidae" genus="Gonaxia" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="409" phylum="Cnidaria" rank="species" species="crassicaulis">
