<taxonomicName id="4C03C086FF9EB66EFC5CF83AFAEA1DB0" authority="Vervoort, 1993" authorityName="Vervoort" authorityYear="1993" box="[966,1309,1940,1964]" class="Hydrozoa" family="Sertulariidae" genus="Gonaxia" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="409" phylum="Cnidaria" rank="species" species="crusgalli">
