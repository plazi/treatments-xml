<taxonomicName id="4C0A4D14FFF7FF9EFA65FA49FEC6C3E7" class="Cycadopsida" family="Cycadaceae" genus="Cycas" kingdom="Plantae" order="Cycadales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="debaoensis">
