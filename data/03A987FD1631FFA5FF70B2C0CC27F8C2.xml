<subSubSection id="C31A65601632FFA5FF20B1CECB5DF90A" box="[199,1053,1735,1760]" pageId="10" pageNumber="461" type="biology_ecology">
<paragraph id="8BBF36EB1632FFA5FF20B1CECB5DF90A" blockId="10.[151,1437,151,1833]" box="[199,1053,1735,1760]" pageId="10" pageNumber="461">
<emphasis id="B974EAF91632FFA5FF20B1CECE67F90A" bold="true" box="[199,295,1735,1760]" pageId="10" pageNumber="461">Biology.</emphasis>
