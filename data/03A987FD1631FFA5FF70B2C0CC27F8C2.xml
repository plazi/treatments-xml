<taxonomicName id="4C004D681632FFA5FDDBB1C0CDFCF90A" box="[572,700,1737,1760]" class="Agaricomycetes" family="Polyporaceae" genus="Nigroporus" kingdom="Fungi" order="Polyporales" pageId="10" pageNumber="461" phylum="Basidiomycota" rank="genus">
