<specimenCount id="9D24E8CBCF3AB73EFE9647BB03D62FDA" box="[369,391,1485,1511]" pageId="21" pageNumber="1296" type="female">1♀</specimenCount>
<date id="FF9C0582CF3AB73EFE7447BB03AB2FDE" box="[403,506,1485,1507]" pageId="21" pageNumber="1296" value="2002-09-03" valueMax="2002-09-08" valueMin="2002-09-03">3-8.ix.2002</date>
