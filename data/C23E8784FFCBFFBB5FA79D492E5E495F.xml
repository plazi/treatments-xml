<taxonomicName id="8D974D11FFCBFFBB5B2B9A792E924A14" box="[1100,1224,1361,1387]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="26" pageNumber="25" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
