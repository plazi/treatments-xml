<geoCoordinate id="36CD500FFFF2FFCC11C9FB5AFD9BFB2A" box="[452,603,1183,1207]" degrees="104.18990" direction="west" orientation="longitude" pageId="2" pageNumber="81" precision="1" value="-104.1899">104.18990째W</geoCoordinate>
