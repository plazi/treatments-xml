<geoCoordinate id="36CD500FFFF1FFCF1450F8C0FB1BF881" box="[1117,1243,1797,1821]" degrees="29.0294" direction="north" orientation="latitude" pageId="1" pageNumber="80" precision="5" value="29.0294">29.0294째 N</geoCoordinate>
