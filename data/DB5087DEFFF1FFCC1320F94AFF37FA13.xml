<geoCoordinate id="36CD500FFFF2FFCC113CFB5AFE77FB2A" box="[305,439,1183,1207]" degrees="29.05345" direction="north" orientation="latitude" pageId="2" pageNumber="81" precision="1" value="29.05345">29.05345째 N</geoCoordinate>
