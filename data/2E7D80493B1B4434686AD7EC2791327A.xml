<taxonomicName id="730C9F97B6E2992DCCC18D19F295F45E" class="Insecta" family="Coccidae" genus="Pulvinaria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pulvinaria polygonata" order="Hemiptera" pageId="0" pageNumber="5216" phylum="Arthropoda" rank="species" species="polygonata">Pulvinaria polygonata</taxonomicName>
Cockerell (
<taxonomicName id="9337BF0C1CE8243ED649B245D420B5D9" authority="" authorityName="" class="Sordariomycetes" family="Lasiosphaeriaceae" genus="Sternorrhyncha" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Sternorrhyncha" order="Sordariales" pageId="0" pageNumber="5216" phylum="Ascomycota" rank="suborder" suborder="Sternorrhyncha">Sternorrhyncha</taxonomicName>
