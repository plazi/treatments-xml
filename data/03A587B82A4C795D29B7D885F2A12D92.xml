<collectingCountry id="F31B763E2A4C795D2A23DF9EF55E2CE2" box="[771,842,1242,1267]" name="Brazil" pageId="19" pageNumber="20">Brazil</collectingCountry>
, 
<collectingCountry id="F31B763E2A4C795D2A76DF9EF5D32CE2" box="[854,967,1242,1267]" name="Colombia" pageId="19" pageNumber="20">Colombia</collectingCountry>
, 
<collectingCountry id="F31B763E2A4C795D2AF4DF9FF2462CE2" box="[980,1106,1242,1267]" name="Costa Rica" pageId="19" pageNumber="20">Costa Rica</collectingCountry>
, 
<collectingCountry id="F31B763E2A4C795D2D7EDF9EF2A92CE2" box="[1118,1213,1242,1267]" name="Ecuador" pageId="19" pageNumber="20">Ecuador</collectingCountry>
, 
<collectingRegion id="49C8F84C2A4C795D2DE9DF9FF3312CE3" box="[1225,1317,1243,1266]" country="Panama" name="Panama" pageId="19" pageNumber="20">
<collectingCountry id="F31B763E2A4C795D2DE9DF9FF3312CE3" box="[1225,1317,1243,1266]" name="Panama" pageId="19" pageNumber="20">Panama</collectingCountry>
</collectingRegion>
, 
<collectingCountry id="F31B763E2A4C795D2C11DF9FF3732CE3" box="[1329,1383,1243,1266]" name="Peru" pageId="19" pageNumber="20">Peru</collectingCountry>
, and 
<collectingCountry id="F31B763E2A4C795D29B7DFBAF71E2D06" box="[151,266,1278,1303]" name="Venezuela" pageId="19" pageNumber="20">Venezuela</collectingCountry>
(
<bibRefCitation id="EF9D4B5F2A4C795D283CDFBAF7D32D06" box="[284,455,1278,1303]" pageId="19" pageNumber="20" refId="ref62259" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii + 1 - 585." type="journal article">Metcalf 1963a</bibRefCitation>
; 
<bibRefCitation id="EF9D4B5F2A4C795D28F7DFBAF46F2D06" box="[471,635,1278,1303]" pageId="19" pageNumber="20" refId="ref63516" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book">Sanborn 2013</bibRefCitation>
