<number id="A22A24BEFFA1E2028DF1FA42FF4BDBC9" box="[151,163,1505,1527]" pageId="3" pageNumber="376" value="1.0">1</number>
(
<number id="A22A24BEFFA1E2028DCBFA42FF51DBC9" box="[173,185,1505,1527]" pageId="3" pageNumber="376" value="1.0">1</number>
