of 
<taxonomicName id="7FE54D58FFF8FFE0FE33B6DAFDD9C4DA" authorityName="Newstead" authorityYear="1917" box="[399,584,915,940]" class="Insecta" family="Margarodidae" genus="Icerya" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="nigroareolata">
