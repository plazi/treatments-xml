<taxonomicName id="7FE54D58FFF8FFE0FD7FB143FB91C352" authority="Newst" authorityName="Newstead" authorityYear="1917" box="[707,1024,1034,1060]" class="Insecta" family="Margarodidae" genus="Icerya" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="nigroareolata">Icerya nigroareolata Newst</taxonomicName>
