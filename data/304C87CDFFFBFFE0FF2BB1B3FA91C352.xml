<collectorName id="1510530DFFF8FFE0FD3BB6ABFCF5C48A" box="[647,868,994,1020]" pageId="62" pageNumber="63">C.C. Gowdey. The</collectorName>
slide with the 
<typeStatus id="675E8879FFF8FFE0FBA4B6ABFB12C48A" box="[1048,1155,994,1020]" pageId="62" pageNumber="63">lectotype</typeStatus>
<specimenCount id="AEE3FD52FFF8FFE0FB32B6ABFB33C480" box="[1166,1186,994,1014]" pageId="62" pageNumber="63" type="female">♀</specimenCount>
