<specimenCode id="90739ED5FFB7B413F83AFAA7FBFDFA43" box="[1099,1130,1388,1412]" collectionCode="R" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="30" pageNumber="288">R1</specimenCode>
