<taxonomicName id="4C67C48963086A0AE0FE2D806389DD81" box="[752,922,727,751]" class="Arachnida" family="Dictynidae" genus="Lathys" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="stigmatisata">
