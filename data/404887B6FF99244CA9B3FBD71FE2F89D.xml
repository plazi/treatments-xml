<taxonomicName id="0FE14D23FF99244CABBCF9EE1833F9C0" box="[664,802,1628,1653]" class="Secernentea" family="Hoplolaimidae" genus="Pratylenchus" kingdom="Animalia" order="Tylenchida" pageId="12" pageNumber="191" phylum="Nematoda" rank="species" species="scribneri">
