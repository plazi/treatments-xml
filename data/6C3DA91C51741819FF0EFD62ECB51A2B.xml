<taxonomicName id="2394638951741819FEB3FC1BED851FDB" authority="(Geisthardt 2004)" authorityName="Geisthardt" authorityYear="2004" baseAuthorityName="Geisthardt" baseAuthorityYear="2004" box="[298,701,899,924]" class="Insecta" family="Lampyridae" genus="Curtos" kingdom="Animalia" order="Coleoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="pseudosauteri">
