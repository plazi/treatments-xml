<taxonomicName id="66D0E6CE142532742D1A064C3C26D2D4" LSID="http://zoobank.org/EA2DA715-B22B-4B4F-B913-90ABD67D98DA" authority="Staines" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia ochra" order="Coleoptera" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="ochra">Cephaloleia ochra Staines</taxonomicName>
