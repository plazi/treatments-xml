<taxonomicName id="4C0B4D37486131109E7F6688FAA4F87B" authority="Beauvais, 1986" authorityName="Beauvais" authorityYear="1986" box="[1063,1381,1937,1963]" class="Anthozoa" family="Oppelismiliidae" genus="Phacelophyllia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="34" pageNumber="1219" phylum="Cnidaria" rank="genus">
