<taxonomicName id="E7864D007F0DFFFFFF3E58A4FE5CFE22" authority="(Stal, 1864)" authorityName="Stal" authorityYear="1864" baseAuthorityName="Stal" baseAuthorityYear="1864" box="[151,482,474,499]" class="Insecta" family="Cicadellidae" genus="Fieberiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="486" phylum="Arthropoda" rank="species" species="florii">
