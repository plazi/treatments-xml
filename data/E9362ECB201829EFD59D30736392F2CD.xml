<paragraph id="3235211A31F7818AAABAC9B0D9D2456B" pageId="4" pageNumber="129">
<taxonomicName id="A5BC81BC2F975E905339D46941AF2677" LSID="http://zoobank.org/363BF9EF-58B2-4C09-9067-9B4400939B41" authority="Brown" authorityName="Brown" authorityYear="2014" class="Insecta" family="Tortricidae" genus="Circanota" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Circanota undulata" order="Lepidoptera" pageId="4" pageNumber="129" phylum="Arthropoda" rank="species" species="undulata">Circanota undulata Brown</taxonomicName>
<taxonomicNameLabel id="D509A372FCD60B2F1F41767C03EEDE6C" pageId="4" pageNumber="129">sp. n.</taxonomicNameLabel>
