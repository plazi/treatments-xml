<taxonomicName id="73112C3EE2096DBC5A131F092FC021C2" authority="Brown, 2014" authorityName="Brown" authorityYear="2014" class="Insecta" family="Tortricidae" genus="Circanota" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Circanota" order="Lepidoptera" pageId="4" pageNumber="129" phylum="Arthropoda" rank="genus">Circanota</taxonomicName>
