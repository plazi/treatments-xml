<taxonomicName id="1BCA3B605B7EF8A6A5B5BF47645598E4" authority="Chaix" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="449" phylum="Tracheophyta" rank="species" species="montana">
