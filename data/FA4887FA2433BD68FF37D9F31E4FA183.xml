<emphasis id="4095EAFE2431BD68FB37DFB61E68A10F" box="[1175,1367,500,526]" italics="true" pageId="47" pageNumber="48">G. macrostachys</emphasis>
</taxonomicName>
clade (
<emphasis id="4095EAFE2431BD68FF01DC581946A135" box="[161,633,538,565]" italics="true" pageId="47" pageNumber="48">
<taxonomicName id="B5E14D6F2431BD68FF01DC581A1BA134" authorityName="Trail" authorityYear="1876" box="[161,292,538,565]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="camana">G. camana</taxonomicName>
, 
<taxonomicName id="B5E14D6F2431BD68FE90DC581AEFA135" authorityName="Wessels Boer" authorityYear="1968" box="[304,464,538,564]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="chococola">G. chococola</taxonomicName>
, 
<taxonomicName id="B5E14D6F2431BD68FE7CDC581946A135" authorityName="Henderson" authorityYear="2011" box="[476,633,538,564]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="deneversii">G. deneversii</taxonomicName>
</emphasis>
, and 
<taxonomicName id="B5E14D6F2431BD68FD1CDC581803A134" box="[700,828,538,565]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="maxima">
<emphasis id="4095EAFE2431BD68FD1CDC581803A134" box="[700,828,538,565]" italics="true" pageId="47" pageNumber="48">G. maxima</emphasis>
