<taxonomicName id="27CA4D35FF48FF2E5A96FD76FC37626D" authorityName="Rondani" authorityYear="1868" box="[648,900,763,789]" class="Insecta" family="Lauxaniidae" genus="Sapromyza" kingdom="Animalia" order="Diptera" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="rectinervis">
