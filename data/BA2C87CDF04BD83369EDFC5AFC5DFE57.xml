<taxonomicName id="F5854D58F04AD8336B91FEDCFBF8FEEF" authority="YANG &amp; LI (1989)" authorityName="YANG &amp; LI" authorityYear="1989" box="[735,1069,330,352]" class="Liliopsida" family="Poaceae" genus="Ammophila" kingdom="Plantae" order="Poales" pageId="83" pageNumber="466" phylum="Tracheophyta" rank="species" species="ganquana">
