<paragraph id="8BAF36B35275FFE6FF7D3F46FE04FE08" blockId="30.[151,1436,151,573]" pageId="30" pageNumber="469">
<emphasis id="B964EAA15275FFE6FF7D3F46FE67FEC0" bold="true" box="[199,437,295,320]" pageId="30" pageNumber="469">
<typeStatus id="54AB88115275FFE6FF7D3F46FEE6FEC0" box="[199,308,295,320]" pageId="30" pageNumber="469">Holotype</typeStatus>
condition.
</emphasis>
