<number id="3D83081D126B3D58FF2FFDD7DB858F25" box="[151,201,568,591]" pageId="3" pageNumber="498" value="0.85">0.85</number>
; cephalic index 
<number id="3D83081D126B3D58FE3FFDD7DAE98F25" box="[391,421,568,591]" pageId="3" pageNumber="498" value="77.0">77</number>
