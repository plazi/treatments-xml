<number id="3D83081D126B3D58FDD4FDD7D9D28F25" box="[620,670,568,591]" pageId="3" pageNumber="498" value="0.85">0.85</number>
; width 
<number id="3D83081D126B3D58FD40FDD7D8668F25" box="[760,810,568,591]" pageId="3" pageNumber="498" value="0.55">0.55</number>
