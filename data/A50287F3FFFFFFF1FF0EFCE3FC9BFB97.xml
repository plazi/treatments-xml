<taxonomicName id="EAAB4D66FFFFFFF1FF56FCE3FCE9FCD8" authority="Staudinger, 1888" authorityName="Staudinger" authorityYear="1888" box="[207,780,869,894]" class="Insecta" family="Lycaenidae" genus="Stempfferia" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="leonina" subGenus="Cercenia">
<emphasis id="1FDFEAF7FFFFFFF1FF56FCE3FDD1FCD8" bold="true" box="[207,564,869,894]" italics="true" pageId="18" pageNumber="19">Stempfferia (Cercenia) leonina</emphasis>
