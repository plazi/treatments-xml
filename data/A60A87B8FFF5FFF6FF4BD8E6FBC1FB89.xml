<treatmentCitationGroup id="0EB31180FFF5FFF5FF4BDF25FCFBFB9B" box="[192,782,1057,1082]" pageId="25" pageNumber="24">
<treatmentCitation id="AF0210BFFFF5FFF5FF4BDF25FD66FB9B" ID-DOI="http://doi.org/10.5281/zenodo.3915624" ID-Zenodo-Dep="3915624" LSID="urn:lsid:zoobank.org:act:95113A55-4C6D-4DC7-A0E5-620BACADFFE5" author="Linnaeus, C." box="[192,659,1057,1082]" httpUri="http://treatment.plazi.org/id/090A2DC26C77BB3AE9B9B28ED4C34189" page="424" pageId="25" pageNumber="24" year="1758">
<taxonomicName id="E9A34D2DFFF5FFF5FF4BDF25FD66FB9B" authority="Linnaeus 1758: 424" authorityName="Linnaeus" authorityPageNumber="424" authorityYear="1758" box="[192,659,1057,1082]" class="Insecta" family="Blattidae" genus="Blatta" kingdom="Animalia" order="Blattodea" pageId="25" pageNumber="24" phylum="Arthropoda" rank="species" species="americana">
<emphasis id="1CD7EABCFFF5FFF5FF4BDF25FE62FB98" box="[192,407,1058,1082]" italics="true" pageId="25" pageNumber="24">Blatta americana</emphasis>
<bibRefCitation id="4A324B5FFFF5FFF5FE14DF25FD66FB9B" author="Linnaeus, C." box="[415,659,1057,1082]" pageId="25" pageNumber="24" refId="ref23339" refString="Linnaeus, C. 1758. Systema Naturae per Regna tria Naturae secundum Classes, Ordines, Genera, Species, cum Characteribus, Differentiis, Synonymis, Locis (10 th edition). Laurentii Salvii; Stockholm Sweden. 1384 p." type="book" year="1758">Linnaeus 1758: 424</bibRefCitation>
