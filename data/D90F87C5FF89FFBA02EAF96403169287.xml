<taxonomicNameLabel id="78E157BAFF8AFFBA074CFF15072292CF" box="[1329,1395,151,176]" pageId="2" pageNumber="189" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<emphasis id="63D2EAC1FF8AFFBA07F9FF1B07CD92CF" box="[1412,1436,153,176]" italics="true" pageId="2" pageNumber="189">B.</emphasis>
<taxonomicName id="96A64D50FF8AFFBA02EAFF3E035592AA" box="[151,260,188,213]" class="Arachnida" family="Gnaphosidae" genus="Berlandina" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="189" phylum="Arthropoda" rank="species" species="mishenini" status="sp. n.">
