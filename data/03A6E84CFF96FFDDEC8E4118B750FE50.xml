<date id="FFB17F9AFF96FFDCEE524288B6AFF906" box="[587,748,1733,1758]" pageId="12" pageNumber="517" value="1927-06-11" valueMax="1927-06-20" valueMin="1927-06-11">
<number id="3DC74B7DFF96FFDCEE524288B6D1F906" box="[587,658,1733,1758]" pageId="12" pageNumber="517" value="15.5" valueMax="20.0" valueMin="11.0">11–20</number>
.vi. 
<number id="3DC74B7DFF96FFDCEEA84288B6AFF906" box="[689,748,1733,1758]" pageId="12" pageNumber="517" value="1927.0">1927</number>
</date>
with wings slightly worn.
</paragraph>
