<quantity id="4CB26CB50B15FF98FC9BFC9A20DBFCEC" box="[869,1022,826,851]" metricMagnitude="-3" metricUnit="m" metricValue="1.825" metricValueMax="1.85" metricValueMin="1.8" pageId="8" pageNumber="9" unit="mm" value="1.825" valueMax="1.85" valueMin="1.8">1.80-1.85 mm</quantity>
