<emphasis id="EF839500FFF3FF95FF3A6975FEDFFEEA" bold="true" box="[197,320,306,332]" pageId="6" pageNumber="44">Remarks:</emphasis>
<taxonomicName id="1AF73291FFF3FF95FEB76975FE77FEED" box="[328,488,306,331]" class="Insecta" family="Dolichopodidae" genus="Erebomyia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="44" phylum="Arthropoda" rank="species" species="akidoptera">
