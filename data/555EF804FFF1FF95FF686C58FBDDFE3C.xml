<taxonomicName id="1AF73291FFF0FF96FEB06FD3FE59F80D" box="[335,454,1940,1963]" class="Insecta" family="Dolichopodidae" genus="Erebomyia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="43" phylum="Arthropoda" rank="genus">
