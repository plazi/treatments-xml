<bibRefCitation id="CD534B15FFFA2B00FE383115FDB5EA0C" author="Hebard" box="[434,586,1111,1135]" pageId="12" pageNumber="13" refString="Hebard, M. (1922) Studies in Malayan, Melanesian and Australian Tettigoniidae (Orthoptera). Proceedings of the Academy of Natural Sciences of Philadelphia, 74, 121 - 299, pls 11 - 22; Philadelphia." type="journal article" year="1922">
Hebard 
<number id="1F0A24C3FFFA2B00FD9A3115FDB5EA0C" box="[528,586,1111,1135]" pageId="12" pageNumber="13" value="1922.0">1922</number>
</bibRefCitation>
, 
<bibRefCitation id="CD534B15FFFA2B00FDD0311AFD1AEA0C" author="Karny" box="[602,741,1111,1136]" pageId="12" pageNumber="13" refString="Karny, H. H. (1925) List of some katydids (Tettigoniidae) in the Sarawak Museum. Sarawak Museum Journal, 8, 35 - 53; Kuching." type="journal article" year="1925">Karny 1925</bibRefCitation>
