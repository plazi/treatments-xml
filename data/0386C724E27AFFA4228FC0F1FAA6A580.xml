<paragraph id="8B907632E27AFFA4228FC178FDB6A618" blockId="17.[151,594,1258,1311]" box="[151,594,1289,1311]" pageId="17" pageNumber="165">
<taxonomicName id="4C2F0DB1E27AFFA4228FC178FDB6A618" authority="Johnston and Tiegs, 1921" authorityName="Johnston and Tiegs" authorityYear="1921" box="[151,594,1289,1311]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="165" phylum="Arthropoda" rank="species" species="theta">
