<taxonomicName id="4C204D1FFF982164DA90FEE4FAC363ED" authority="Wei and Gu 2019" authorityName="Wei and Gu" authorityYear="2019" box="[935,1388,370,394]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="21" pageNumber="2803" phylum="Firmicutes" rank="species" species="baiquanensis">
