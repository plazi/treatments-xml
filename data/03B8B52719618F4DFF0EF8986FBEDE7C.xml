<specimenCount id="9D17CFB819628F42FCE2FF716DA9DBA5" box="[891,972,188,213]" pageId="7" pageNumber="428" type="male">
<number id="3DD9161619628F42FCE2FF716DECDBA4" box="[891,905,188,212]" pageId="7" pageNumber="428" value="1.0">1</number>
