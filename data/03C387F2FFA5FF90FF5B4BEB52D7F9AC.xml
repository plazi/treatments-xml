<taxonomicName id="4C6A4D67FFA5FF90FBE04AE652D9FA77" authorityName="Jaub. &amp; Spach" authorityYear="1843" box="[1068,1185,1476,1499]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="41" pageNumber="42" phylum="Tracheophyta" rank="species" species="persica">
