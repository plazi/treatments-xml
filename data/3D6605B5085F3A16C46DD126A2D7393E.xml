<taxonomicName id="6EDE042CE3E8B239CD987120AE0A75E7" class="Insecta" family="Carabidae" genus="Perigona" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Perigona" order="Coleoptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="genus">Perigona</taxonomicName>
