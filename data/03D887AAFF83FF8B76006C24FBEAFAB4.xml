<taxonomicName id="4C714D3FFF80FF8774576FC8FE39FA4F" authorityName="Ren" authorityYear="1997" box="[336,445,1445,1469]" class="Insecta" family="Bittacidae" genus="Megabittacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mecoptera" pageId="6" pageNumber="791" phylum="Arthropoda" rank="species" species="colosseus">
