<quantity id="4C899B59FF8FFF8871826CFEFB33F953" box="[1157,1207,1680,1697]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="9" pageNumber="794" unit="mm" value="1.0">1 mm</quantity>
