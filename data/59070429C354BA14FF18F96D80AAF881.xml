<emphasis id="E3DA692DC354BA14FF18F96D837EF924" bold="true" box="[151,610,1744,1770]" pageId="40" pageNumber="41">
<taxonomicName id="16AECEBCC354BA14FF18F96D80E0F924" authority="Signoret, 1869: 847" authorityName="Signoret" authorityPageNumber="847" authorityYear="1869" box="[151,508,1744,1770]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="rotundus">
<emphasis id="E3DA692DC354BA14FF18F96D8018F924" bold="true" box="[151,260,1744,1770]" italics="true" pageId="40" pageNumber="41">rotundus</emphasis>
Signoret, 1869: 847
</taxonomicName>
, 
<taxonomicName id="16AECEBCC354BA14FD85F96D837EF924" authorityName="Linnaeus" authorityYear="1758" box="[522,610,1744,1770]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus">
