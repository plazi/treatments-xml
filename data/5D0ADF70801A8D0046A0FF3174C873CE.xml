<taxonomicNameLabel id="FCE40F0F801A8D00425CFE4F75C873C9" box="[1131,1179,486,508]" pageId="8" pageNumber="595" rank="species">n.sp.</taxonomicNameLabel>
</emphasis>
(
<collectingRegion id="1767A084801A8D004290FE4C743273CE" box="[1191,1377,485,507]" country="India" name="Arunachal Pradesh" pageId="8" pageNumber="595">Arunachal Pradesh</collectingRegion>
, 
<collectingCountry id="ADB42EF6801A8D00435DFE4C74C873CE" box="[1386,1435,485,507]" name="India" pageId="8" pageNumber="595">India</collectingCountry>
