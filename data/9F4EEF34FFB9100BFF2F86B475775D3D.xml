<taxonomicName id="D0E725A1FFB91008FEAA829970225BE4" authority="Skoracki 2011:389" authorityName="Skoracki" authorityPageNumber="389" authorityYear="2011" box="[274,379,1967,1988]" class="Arachnida" family="Syringophilidae" genus="Picobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="58" phylum="Arthropoda" rank="species" species="locustella">locustella,</taxonomicName>
