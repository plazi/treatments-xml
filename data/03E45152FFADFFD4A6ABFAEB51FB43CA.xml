<taxonomicName id="4C4D9BC7FFADFFD4A6E1FAEB54D04020" authorityName="Kirk-Spriggs &amp; Wiegmann" authorityYear="2013" box="[221,495,1359,1384]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="uncinatum">
<emphasis id="B9393C56FFADFFD4A6E1FAEB54D04020" bold="true" box="[221,495,1359,1384]" italics="true" pageId="50" pageNumber="51">Curtonotum uncinatum</emphasis>
