<taxonomicName id="6DB14D0CFF97FF925A53FA8E951AFA99" box="[777,865,1305,1329]" class="Insecta" family="Drepanidae" genus="Cyclura" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis id="98C5EA9DFF97FF925A53FA8E951AFA99" box="[777,865,1305,1329]" italics="true" pageId="29" pageNumber="30">Cyclura</emphasis>
</taxonomicName>
and listed 
<taxonomicName id="6DB14D0CFF97FF925A87FA8D924FFA99" authority="Warren, 1897" authorityName="Warren" authorityYear="1897" box="[989,1076,1306,1329]" class="Insecta" family="Drepanidae" genus="Neoreta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis id="98C5EA9DFF97FF925A87FA8D924FFA99" box="[989,1076,1306,1329]" italics="true" pageId="29" pageNumber="30">Neoreta</emphasis>
</taxonomicName>
, 
<taxonomicName id="6DB14D0CFF97FF925D19FA8D92BFFA99" authority="Warren, 1923" authorityName="Warren" authorityYear="1923" box="[1091,1220,1306,1329]" class="Insecta" family="Drepanidae" genus="Procampsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis id="98C5EA9DFF97FF925D19FA8D92BFFA99" box="[1091,1220,1306,1329]" italics="true" pageId="29" pageNumber="30">Procampsis</emphasis>
</taxonomicName>
and 
