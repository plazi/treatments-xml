<paragraph id="0FD758BE29F4DFB88365DA747D0C0756" pageId="5" pageNumber="128">
<taxonomicName id="CDBA6F4B3EDDB2CF6DF91EB1F682E239" class="Insecta" family="Crambidae" genus="Udea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Udea stationalis" order="Lepidoptera" pageId="5" pageNumber="128" phylum="Arthropoda" rank="species" species="stationalis">Udea stationalis</taxonomicName>
