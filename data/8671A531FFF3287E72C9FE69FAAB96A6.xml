<taxonomicName id="C9D86FA4FFF3287D7758F902FA6F914B" authorityName="Wang &amp; Liu" authorityYear="2020" box="[1286,1430,1790,1814]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="4" phylum="Arthropoda" rank="species" species="trimaculata">
<emphasis id="3CACC835FFF3287D7758F902FA6F914B" box="[1286,1430,1790,1814]" italics="true" pageId="4">P. trimaculata</emphasis>
</taxonomicName>
