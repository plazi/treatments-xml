Adults of 
<taxonomicName id="C9D86FA4FFF3287D73F7F902FDDD914B" authorityName="Meyrick" authorityYear="1908" box="[425,548,1790,1814]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="4" phylum="Arthropoda" rank="genus">
<emphasis id="3CACC835FFF3287D73F7F902FDDD914B" box="[425,548,1790,1814]" italics="true" pageId="4">Promalactis</emphasis>
</taxonomicName>
spp. 1, 
<taxonomicName id="C9D86FA4FFF3287D7029F902FCE8914B" authorityName="Wang &amp; Liu" authorityYear="2020" box="[631,785,1790,1814]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="4" phylum="Arthropoda" rank="species" species="simingshana">
<emphasis id="3CACC835FFF3287D7029F902FCE8914B" box="[631,785,1790,1814]" italics="true" pageId="4">P. simingshana</emphasis>
</taxonomicName>
, holotype, ♂; 2, 
<taxonomicName id="C9D86FA4FFF3287D719FF902FBAC9148" authorityName="Wang &amp; Liu" authorityYear="2020" box="[961,1109,1790,1813]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="4" phylum="Arthropoda" rank="species" species="costispinata">
<emphasis id="3CACC835FFF3287D719FF902FBAC9148" box="[961,1109,1790,1813]" italics="true" pageId="4">P. costispinata</emphasis>
</taxonomicName>
