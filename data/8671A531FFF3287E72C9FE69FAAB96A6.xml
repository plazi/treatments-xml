<taxonomicName id="C9D86FA4FFF3287D7368F8DEFE4D9164" authorityName="Wang &amp; Liu" authorityYear="2020" box="[310,436,1826,1849]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="4" phylum="Arthropoda" rank="species" species="yongjiana">
