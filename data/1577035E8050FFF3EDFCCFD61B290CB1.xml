<collectingCountry id="E5C9F2D88050FFF3ECAFCCB91B5A0CDB" box="[452,519,656,679]" name="Japan" pageId="84" pageNumber="85">Japan</collectingCountry>
, south to 
<collectingRegion id="5F1A7CAA8050FFF3EF1BCCB91A660CD4" box="[624,827,655,680]" country="Australia" name="Western Australia" pageId="84" pageNumber="85">Western Australia</collectingRegion>
, 
<collectingRegion id="5F1A7CAA8050FFF3EE2DCCA61A970CD4" box="[838,970,655,680]" country="Australia" name="Queensland" pageId="84" pageNumber="85">Queensland</collectingRegion>
(
<collectingCountry id="E5C9F2D88050FFF3EEB3CCA61D1F0CD4" box="[984,1090,655,680]" name="Australia" pageId="84" pageNumber="85">Australia</collectingCountry>
) and 
<collectingCountry id="E5C9F2D88050FFF3E914CCB91C730CD4" box="[1151,1326,655,680]" name="New Caledonia" pageId="84" pageNumber="85">New Caledonia</collectingCountry>
