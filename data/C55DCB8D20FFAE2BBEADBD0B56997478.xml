<emphasis id="9663AA3AFF4BC51E8980D8865C8DD364" italics="true" pageId="16" pageNumber="61">Acacia</emphasis>
</taxonomicName>
[
<taxonomicName id="A075F8A2E8EB599C6D5AFF3A32276939" authorityName="Rafinesque" authorityYear="1838" class="Magnoliopsida" family="Fabaceae" genus="Senegalia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Senegalia" order="Fabales" pageId="16" pageNumber="61" phylum="Tracheophyta" rank="genus">
<emphasis id="01C4AE3414A56DCBFB622AE112A70F93" italics="true" pageId="16" pageNumber="61">Senegalia</emphasis>
