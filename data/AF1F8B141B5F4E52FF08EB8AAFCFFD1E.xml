<taxonomicName id="E0B641811B5F4E51FF20ED43AD68F9F9" box="[191,366,1630,1653]" class="Insecta" family="Asilidae" genus="Choerades" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="gilva">
