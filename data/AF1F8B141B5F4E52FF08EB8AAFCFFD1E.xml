<taxonomicName id="E0B641811B5F4E51FE13ED26AE61F9DE" box="[396,615,1595,1618]" class="Insecta" family="Asilidae" genus="Choerades" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="caudatus">
