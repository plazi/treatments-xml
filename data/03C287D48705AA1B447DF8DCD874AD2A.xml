<caption id="DF14664A8704AA1B46A0FB44DA24AF11" ID-DOI="http://doi.org/10.5281/zenodo.3722994" ID-Zenodo-Dep="3722994" httpUri="https://zenodo.org/record/3722994/files/figure.png" pageId="31" pageNumber="32" startId="31.[189,232,1206,1232]" targetBox="[189,1398,711,1166]" targetPageId="31">
