<paragraph id="757BB7917B49ED48C62C9526B4545F97" pageId="0" pageNumber="1">
<taxonomicName id="3CF5091E6ABF3D0969C525AFA2372247" authority="Hulcr &amp; Cognato, 2009" authorityName="Hulcr &amp; Cognato" authorityYear="2009" baseAuthorityName="Eichhoff" baseAuthorityYear="1876" class="Insecta" family="Curculionidae" genus="Ambrosiophilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ambrosiophilus atratus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="atratus">
<emphasis id="130B3C110A6BA6E840D8F54A4E65474F" italics="true" pageId="0" pageNumber="1">Ambrosiophilus atratus</emphasis>
</taxonomicName>
, 
<taxonomicName id="A73A541FC026203F4AEC976F4425676D" genus="A." lsidName="A. satoi" pageId="0" pageNumber="1" rank="species" species="satoi">
