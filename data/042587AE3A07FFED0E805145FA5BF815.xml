, 
<collectingCountry id="F49B76283A07FFED0EF65554FEDDF8E2" box="[225,282,1799,1821]" name="China" pageId="1" pageNumber="298">China</collectingCountry>
(
<collectingRegion id="4E48F85A3A07FFED0F305554FEA8F8E2" box="[295,367,1799,1821]" country="China" name="Xizang" pageId="1" pageNumber="298">Xizang</collectingRegion>
)
</materialsCitation>
. 
<materialsCitation id="3CE43CE53A07FFED0F9C555BFA5BF8E2" ID-GBIF-Occurrence="1439686193" box="[395,1436,1799,1821]" country="Guinea-Bissau" location="Darlington" pageId="1" pageNumber="298" specimenCount="1" stateProvince="Darlington">
