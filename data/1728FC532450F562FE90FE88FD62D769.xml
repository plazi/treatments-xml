<taxonomicName id="588136C62450F562FE90FE1BFD7ED5C5" authority="Hy. Edwards, 1890" authorityName="Hy. Edwards" authorityYear="1890" box="[264,669,398,422]" class="Insecta" family="Castniidae" genus="Herrichia" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="68" phylum="Arthropoda" rank="species" species="cervina">
