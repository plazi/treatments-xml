<taxonomicName id="4C60A7D4EA52FFC476DEF945FEBCF903" baseAuthorityName="Cresson" baseAuthorityYear="1865" box="[212,334,1672,1696]" class="Insecta" family="Ichneumonidae" genus="Hymenoepimecis" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="2695" phylum="Arthropoda" rank="species" species="atriceps">
