<taxonomicName id="4C60A7D4EA52FFC4750EF9C5FEE0F99C" authority="(Cresson, 1865)" baseAuthorityName="Cresson" baseAuthorityYear="1865" class="Insecta" family="Ichneumonidae" genus="Hymenoepimecis" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="2695" phylum="Arthropoda" rank="species" species="atriceps">
