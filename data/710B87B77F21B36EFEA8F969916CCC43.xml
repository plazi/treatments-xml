<paragraph id="F91D36A17F20B369FEA8FD6B916DCD68" blockId="3.[264,1323,284,1870]" pageId="3" pageNumber="4">
<keyLead id="425395947F20B369FEA8FD6B916DCD68" pageId="3" pageNumber="4">
<number id="4F6A24867F20B369FEA8FD6B955FCD18" box="[264,281,764,790]" pageId="3" pageNumber="4" value="3.0">3</number>
