<taxonomicName id="17BE82F3BD76B3480F86FCFC3DB45010" authority="Wang, Wang, Cao &amp; Cai" authorityName="Wang, Wang, Cao &amp; Cai" box="[151,718,799,826]" class="Insecta" family="Reduviidae" genus="Ocelliemesina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="430" phylum="Arthropoda" rank="species" species="sinica" status="sp. nov.">
<emphasis id="E2CA2562BD76B3480F86FCFC3EEB5013" bold="true" box="[151,401,799,825]" italics="true" pageId="1" pageNumber="430">Ocelliemesina sinica</emphasis>
Wang, Wang, Cao &amp; Cai
</taxonomicName>
, 
<taxonomicNameLabel id="F9F99819BD76B3480DCDFCC33C405010" box="[732,826,800,826]" pageId="1" pageNumber="430" rank="species">sp. nov.</taxonomicNameLabel>
