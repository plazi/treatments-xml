<quantity id="4C8C56FFC65DFFBECFDDFB36FB472792" box="[1160,1246,1156,1180]" metricMagnitude="3" metricUnit="m" metricValue="1.77" pageId="7" pageNumber="43" unit="m" value="1770.0">1770 m</quantity>
