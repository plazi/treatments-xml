<taxonomicName id="4C748099C65DFFBECA97FB7EFD4127EA" authority="Pursh" authorityName="Pursh" box="[450,728,1228,1252]" class="Magnoliopsida" family="Ericaceae" genus="Arbutus" kingdom="Plantae" order="Ericales" pageId="7" pageNumber="43" phylum="Tracheophyta" rank="species" species="menziesii">
