<taxonomicName id="3CF54D648A14FFEC9F7CFEA55EC58816" authority="Kuhl" authorityName="Kuhl" box="[1053,1225,320,342]" class="Chondrichthyes" family="Myliobatidae" genus="Rhinoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="66" pageNumber="67" phylum="Chordata" rank="genus">Rhinoptera Kuhl</taxonomicName>
