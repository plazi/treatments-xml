<taxonomicName id="3CF54D648A14FFEC9A7EFEA5587A8816" authority="van Hasselt, 1824: 90" authorityName="van Hasselt" authorityPageNumber="90" authorityYear="1824" box="[287,630,320,342]" class="Chondrichthyes" family="Myliobatidae" genus="Rhinoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="66" pageNumber="67" phylum="Chordata" rank="genus">
