<normalizedToken id="436BA74CF6EA8FE039F607DBE5C04F9D" originalValue="Gewöhnlicher" pageId="null" pageNumber="579">Gewoehnlicher</normalizedToken>
<normalizedToken id="2E8462FDF3D4AD7B4B10BBA73E43D081" originalValue="Beifuß" pageId="null" pageNumber="579">Beifuss</normalizedToken>
</paragraph>
