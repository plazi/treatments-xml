<taxonomicName id="BCBE4D1A7553FFA8A9A40051458DC96B" authorityName="Ima" authorityYear="2013" box="[417,651,1854,1876]" class="Anthozoa" family="Paralcyoniidae" genus="Nanalcyon" kingdom="Animalia" order="Alcyonacea" pageId="3" pageNumber="427" phylum="Cnidaria" rank="species" species="sagamiense">
