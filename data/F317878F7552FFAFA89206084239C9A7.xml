<taxonomicName id="BCBE4D1A7553FFA8ACCA01AC429DC8E6" authorityName="Ima" authorityYear="2013" box="[1231,1435,1731,1753]" class="Anthozoa" family="Paralcyoniidae" genus="Ceeceenus" kingdom="Animalia" order="Alcyonacea" pageId="3" pageNumber="427" phylum="Cnidaria" rank="species" species="retractus">
<emphasis id="49CAEA8B7553FFA8ACCA01AC429DC8E6" box="[1231,1435,1731,1753]" italics="true" pageId="3" pageNumber="427">Ceeceenus retractus</emphasis>
