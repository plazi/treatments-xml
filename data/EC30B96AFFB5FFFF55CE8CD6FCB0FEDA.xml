<taxonomicName id="A39973FFFFB5FFF850D48C95FDFAFB90" authority="(Munchh.) Fliche" authorityName="Fliche" authorityYear="1888" baseAuthorityName="Munchh." class="Magnoliopsida" family="Betulaceae" genus="Ostrya" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="variety" species="carpinifolia" variety="virginica">
<emphasis id="56EDD46EFFB5FFF850D48C95FAD6FC4C" box="[1225,1399,962,988]" italics="true" pageId="35" pageNumber="36">O. carpinifolia</emphasis>
