<taxonomicName id="4C184D37FFC8DF54FE9A80FFFDF853E0" authorityName="Bello &amp; Osella" authorityYear="2016" box="[280,576,943,969]" class="Insecta" family="Curculionidae" genus="Baldorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="pedersolii" status="sp. n.">
<emphasis id="B96CEAA6FFC8DF54FE9A80FFFE7153E0" box="[280,457,943,969]" italics="true" pageId="83" pageNumber="84">Baldorhynchus</emphasis>
