<taxonomicName id="F61C9FC01D5092B9C5F7F3CDC2CEBF37" LSID="1BF3796E-CB84-5360-AC28-A43C22D4D4B8" authority="Li &amp; He &amp; Chen, 2020" authorityName="Li &amp; He &amp; Chen" authorityYear="2020" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon (Lucobracon) quadratus" order="Hymenoptera" pageId="0" pageNumber="209" phylum="Arthropoda" rank="species" species="quadratus" status="sp. nov." subGenus="Lucobracon">Bracon (Lucobracon) quadratus</taxonomicName>
