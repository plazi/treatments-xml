<taxonomicName id="82F8AC8A0326FFCFFB4ACE51FAEAA6BA" authorityName="Rodríguez-Flores &amp; Macpherson &amp; Machordom" authorityYear="2019" box="[1249,1371,871,896]" class="Malacostraca" family="Galatheidae" genus="Leiogalathea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="24" pageNumber="225" phylum="Arthropoda" rank="species" species="evander">
<emphasis id="778C0B1B0326FFCFFB4ACE51FAEAA6BA" box="[1249,1371,871,896]" italics="true" pageId="24" pageNumber="225">L. evander</emphasis>
</taxonomicName>
