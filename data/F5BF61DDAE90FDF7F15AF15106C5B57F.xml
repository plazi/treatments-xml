<taxonomicName id="D2685C4743559CC6F6C2545A7D8264C9" authorityName="LeConte" authorityYear="1873" class="Hexapoda" family="Carabidae" genus="Pterostichus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="504" pageNumber="505" phylum="Arthropoda" rank="species" species="corrusculus">Pterostichus corrusculus</taxonomicName>
