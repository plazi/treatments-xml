<quantity id="5E639B5E9B54FFD1FEDAFC90FE52FC8D" box="[350,487,871,895]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" metricValueMax="2.9" metricValueMin="2.1" pageId="13" pageNumber="14" unit="mm" value="2.5" valueMax="2.9" valueMin="2.1">2.1–2.9 mm</quantity>
