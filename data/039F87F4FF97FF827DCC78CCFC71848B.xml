<emphasis id="B942EAF0FF95FF837DF978B4FE878064" bold="true" box="[189,313,230,256]" pageId="10" pageNumber="151">
<location id="8EE96039FF95FF837DF978B4FE878064" LSID="urn:lsid:plazi:treatment:039F87F4FF97FF827DCC78CCFC71848B:8EE96039FF95FF837DF978B4FE878064" box="[189,313,230,256]" country="China" county="Xiaojin County" municipality="The" name="Phenology" pageId="10" pageNumber="151" stateProvince="Sichuan">Phenology</location>
