<taxonomicName id="4C234D543D4DFF86FF689F61FD16FDDA" authority="Richters 1904" authorityName="Richters" authorityYear="1904" box="[151,659,551,577]" class="Eutardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="19" pageNumber="20" phylum="Tardigrada" rank="species" species="echinogenitus">
