<subSubSection id="C31965457616FF8F6883FD86900CFD6B" box="[151,611,626,648]" pageId="9" pageNumber="34" type="materials_examined">
<paragraph id="8BBC36CE7616FF8F6883FD86900CFD6B" blockId="9.[151,611,626,648]" box="[151,611,626,648]" pageId="9" pageNumber="34">
<typeStatus id="54B8886C7616FF8F6883FD8692A7FD64" box="[151,200,627,648]" pageId="9" pageNumber="34">Type</typeStatus>
species. 
<taxonomicName id="4C034D4D7616FF8F6931FD87900CFD6B" authority="Storm, 1888" authorityName="Storm" authorityYear="1888" box="[293,611,626,648]" class="Polychaeta" family="Polynoidae" genus="Polynoe" kingdom="Animalia" order="Phyllodocida" pageId="9" pageNumber="34" phylum="Annelida" rank="species" species="paradoxa">
