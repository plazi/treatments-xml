<taxonomicName id="4C664D67FFFEFFE62E9854A773184F80" authorityName="Tuff" authorityYear="1967" box="[977,1099,1198,1221]" class="Insecta" family="Philopteridae" genus="Ardeicola" kingdom="Animalia" order="Psocodea" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="nyctanassa">
