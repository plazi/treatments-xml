<taxonomicName id="4C2AE7E210BD939BFF67E539FD3E5A77" authority="(Chamberlin &amp; Ivie, 1938)" baseAuthorityName="Chamberlin &amp; Ivie" baseAuthorityYear="1938" box="[164,678,1090,1112]" class="Arachnida" family="Mysmenidae" genus="Maymena" kingdom="Animalia" order="Araneae" pageId="251" pageNumber="778" phylum="Arthropoda" rank="species" species="mayana">
