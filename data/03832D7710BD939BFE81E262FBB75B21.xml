by original designation, 
<typeStatus id="549122C310BD939BFEA2E51BFE095A59" box="[353,401,1121,1142]" pageId="251" pageNumber="778">type</typeStatus>
material in 
<collectionCode id="ED3B04A410BD939BFDE2E51BFDC35A59" box="[545,603,1121,1142]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="251" pageNumber="778" type="Museum">MCZ</collectionCode>
