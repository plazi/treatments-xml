<collectingCountry id="F33DDCF110BD939BFDC2E4C4FDD95BFC" box="[513,577,1470,1491]" name="Mexico" pageId="251" pageNumber="778">MEX</collectingCountry>
, and 
<taxonomicName id="4C2AE7E210BD939BFD4FE4C5FC995BFB" authorityName="Gertsch" authorityYear="1960" box="[652,769,1471,1492]" class="Arachnida" family="Mysmenidae" genus="Maymena" kingdom="Animalia" order="Araneae" pageId="251" pageNumber="778" phylum="Arthropoda" rank="genus">
<emphasis id="B95E407310BD939BFD4FE4C5FC995BFB" box="[652,769,1471,1492]" italics="true" pageId="251" pageNumber="778">Maymena</emphasis>
</taxonomicName>
- 
<collectionCode id="ED3B04A410BD939BFF67E4A7FF675BDC" box="[164,255,1501,1523]" pageId="251" pageNumber="778">MYSM-</collectionCode>
