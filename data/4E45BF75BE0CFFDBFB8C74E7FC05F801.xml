specimens.
</emphasis>
<typeStatus id="1957B0C1BE0BFFDBFA8973EDFE21FD67" box="[402,506,727,752]" pageId="4" pageNumber="267">Holotype</typeStatus>
female (
<collectionCode id="A0FD96A6BE0BFFDBF94473E2FD6AFD67" LSID="urn:lsid:biocol.org:col:34871" box="[607,689,728,752]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="4" pageNumber="267">USNM</collectionCode>
