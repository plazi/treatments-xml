<taxonomicName id="B99F4D4B3D2BFFD7FD28D156022CC644" box="[752,893,1071,1094]" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="310" phylum="Arthropoda" rank="genus">
