<collectionCode id="ED71AE24FFA84E00FDBEF87AFD8C39B5" box="[515,622,2006,2030]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="29" pageNumber="711" type="Museum">NHMUK</collectionCode>
