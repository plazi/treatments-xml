<taxonomicName id="6DFD27A9BF7CFFC05872FE5B36824AB9" box="[781,1004,444,469]" class="Magnoliopsida" family="Brassicaceae" genus="Brassica" kingdom="Plantae" order="Brassicales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="oleracea">
<emphasis id="98898038BF7CFFC05872FE5B36824AB9" box="[781,1004,444,469]" italics="true" pageId="17" pageNumber="18">Brassica oleracea</emphasis>
</taxonomicName>
L., 
<taxonomicName id="6DFD27A9BF7CFFC05F50FE5B31A54ABA" baseAuthorityName="Passoa and Hollingsworth" baseAuthorityYear="1996" box="[1071,1227,444,470]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
), globe artichoke (
<taxonomicName id="6DFD27A9BF7CFFC05AF3FE0337354A91" box="[396,603,484,509]" class="Magnoliopsida" family="Asteraceae" genus="Cynara" kingdom="Plantae" order="Asterales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="scolymus">
