<taxonomicName id="6DFD27A9BF7CFFC0599BFDEB36B24949" box="[740,988,524,549]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="tuberosum">
