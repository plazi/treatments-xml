<collectingCountry id="00C87643FF88D26EFAD5F963FAE78AD5" box="[1298,1366,1665,1691]" name="Brazil" pageId="23" pageNumber="324">Brazil</collectingCountry>
(from 
<collectingRegion id="BA1BF831FF88D26EFF50F947FF598AF1" box="[151,232,1701,1727]" country="Brazil" name="Amapa" pageId="23" pageNumber="324">Amapá</collectingRegion>
