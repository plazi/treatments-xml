<quantity id="BF279B36FF88D26EFC4BF9F7FBB38A61" box="[908,1026,1557,1583]" metricMagnitude="-2" metricUnit="m" metricValue="2.843" pageId="23" pageNumber="324" unit="mm" value="28.43">28.43 mm</quantity>
