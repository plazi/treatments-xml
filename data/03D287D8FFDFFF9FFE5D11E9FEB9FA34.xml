<emphasis id="B90FEADCFFDFFF9FFE5D11E9FCC0FB25" bold="true" box="[404,798,1176,1201]" pageId="31" pageNumber="618">
<taxonomicName id="4C7B4D4DFFDFFF9FFE5D11E9FDA1FB25" baseAuthorityName="Fabricius" baseAuthorityYear="1781" box="[404,639,1176,1201]" class="Insecta" family="Ripiphoridae" genus="Macrosiagon" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="618" phylum="Arthropoda" rank="species" species="limbata">
<emphasis id="B90FEADCFFDFFF9FFE5D11E9FDA1FB25" bold="true" box="[404,639,1176,1201]" italics="true" pageId="31" pageNumber="618">Macrosiagon limbata</emphasis>
</taxonomicName>
