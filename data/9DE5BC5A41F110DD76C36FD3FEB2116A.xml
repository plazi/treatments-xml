<taxonomicName id="462982AF3DF4093279D2F79CC9456EBA" authority="" authorityName="" class="Centricae" family="Chironomidae" genus="C." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="C. cunicularius" order="Diptera" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="cunicularius" subSpecies="celticus">C. cunicularius celticus</taxonomicName>
