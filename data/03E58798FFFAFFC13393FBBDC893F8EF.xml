<number id="3D8424A9FFFAFFC137D2FB24C932FB73" box="[1238,1296,1190,1214]" pageId="5" pageNumber="6" value="6401.0">6401</number>
), Harrison’s Point reef, from 
<taxonomicName id="4C4C4D0DFFFAFFC1325BFB49CE6DFB2E" box="[351,591,1227,1251]" class="Demospongiae" family="Spongiidae" genus="Hyattella" kingdom="Animalia" order="Dictyoceratida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="intestinalis">
<emphasis id="B938EA9CFFFAFFC1325BFB49CE6DFB2E" box="[351,591,1227,1251]" italics="true" pageId="5" pageNumber="6">Hyattella intestinalis</emphasis>
</taxonomicName>
. 
<number id="3D8424A9FFFAFFC13165FB48CE4DFB2F" box="[609,623,1226,1250]" pageId="5" pageNumber="6" value="1.0">1</number>
non-ovigerous individual, 
<number id="3D8424A9FFFAFFC130AAFB48CF9EFB2F" box="[942,956,1226,1250]" pageId="5" pageNumber="6" value="1.0">1</number>
ovigerous female (
<collectionCode id="ED5DAE4BFFFAFFC137A7FB48C8C9FB2F" box="[1187,1259,1226,1250]" httpUri="http://grbio.org/cool/a6xx-dd75" name="Virginia Institute of Marine Science" pageId="5" pageNumber="6">VIMS</collectionCode>
08BR 
<number id="3D8424A9FFFAFFC1363DFB48C951FB2F" box="[1337,1395,1226,1250]" pageId="5" pageNumber="6" value="9603.0">9603</number>
– 
<number id="3D8424A9FFFAFFC13686FB48C9B2FB2F" box="[1410,1424,1226,1250]" pageId="5" pageNumber="6" value="4.0">4</number>
), Thunder Bay, from 
<taxonomicName id="4C4C4D0DFFFAFFC1327DFB6CCE61FACB" box="[377,579,1262,1286]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="clathrodes">
