<taxonomicName id="4C4C4D0DFFFAFFC1377BFAFCC949FA5B" box="[1151,1387,1406,1430]" class="Demospongiae" family="Spongiidae" genus="Hyattella" kingdom="Animalia" order="Dictyoceratida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="intestinalis">
<emphasis id="B938EA9CFFFAFFC1377BFAFCC949FA5B" box="[1151,1387,1406,1430]" italics="true" pageId="5" pageNumber="6">Hyattella intestinalis</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C4C4D0DFFFAFFC13393FA21CD43FA76" box="[151,353,1443,1467]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="clathrodes">
<emphasis id="B938EA9CFFFAFFC13393FA21CD43FA76" box="[151,353,1443,1467]" italics="true" pageId="5" pageNumber="6">Agelas clathrodes</emphasis>
</taxonomicName>
; it also occurs in the sponges 
<emphasis id="B938EA9CFFFAFFC131BBFA21C89CFA76" box="[703,1214,1443,1467]" italics="true" pageId="5" pageNumber="6">
<taxonomicName id="4C4C4D0DFFFAFFC131BBFA21CF8CFA76" box="[703,942,1443,1467]" class="Demospongiae" family="Spongiidae" genus="Hyattella" kingdom="Animalia" order="Dictyoceratida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="intestinalis">Hyattella intestinalis</taxonomicName>
, 
<taxonomicName id="4C4C4D0DFFFAFFC130BFFA26C898FA76" box="[955,1210,1444,1467]" class="Demospongiae" family="Petrosiidae" genus="Xestospongia" kingdom="Animalia" order="Haplosclerida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="proxima">Xestospongia proxima</taxonomicName>
,
</emphasis>
and 
<taxonomicName id="4C4C4D0DFFFAFFC137FCFA26CCE9FA13" class="Demospongiae" family="Petrosiidae" genus="Xestospongia" kingdom="Animalia" order="Haplosclerida" pageId="5" pageNumber="6" phylum="Porifera" rank="species" species="subtriangularis">
<emphasis id="B938EA9CFFFAFFC137FCFA26CCE9FA13" italics="true" pageId="5" pageNumber="6">X. subtriangularis</emphasis>
