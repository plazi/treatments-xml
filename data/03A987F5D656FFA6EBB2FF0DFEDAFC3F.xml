hosts.
</emphasis>
Maple, 
<taxonomicName id="4C004D60D656FFA6EAE6FCE9FD81FCE3" authorityName="de Jussieu" authorityYear="1789" box="[472,588,835,861]" class="Magnoliopsida" family="Aceraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="2" pageNumber="25" phylum="Tracheophyta" rank="family">Aceraceae</taxonomicName>
(
<taxonomicName id="4C004D60D656FFA6E95AFCEEFCD4FCE3" authorityName="C.P.Thunberg ex A.Murray" authorityYear="1784" box="[612,793,835,862]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="2" pageNumber="25" phylum="Tracheophyta" rank="species" species="palmatum">
<emphasis id="B974EAF1D656FFA6E95AFCEEFCD4FCE3" box="[612,793,835,862]" italics="true" pageId="2" pageNumber="25">Acer palmatum</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C004D60D656FFA6E810FCEEFBC0FCE3" box="[814,1037,835,862]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="2" pageNumber="25" phylum="Tracheophyta" rank="species" species="buergerianum">
<emphasis id="B974EAF1D656FFA6E810FCEEFBC0FCE3" box="[814,1037,835,862]" italics="true" pageId="2" pageNumber="25">Acer buergerianum</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C004D60D656FFA6EF1CFCEEFECBFC3E" authorityName="Bunge" authorityYear="1833" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="2" pageNumber="25" phylum="Tracheophyta" rank="species" species="truncatum">
<emphasis id="B974EAF1D656FFA6EF1CFCEEFECBFC3E" italics="true" pageId="2" pageNumber="25">Acer truncatum</emphasis>
</taxonomicName>
).
</paragraph>
