</taxonomicName>
and 
<taxonomicName id="4C6E4D69FFD8FFE3FEEA75BD96C6F98E" authority="Rothschild, 1910" authorityName="Rothschild" authorityYear="1910" box="[348,673,1521,1546]" class="Insecta" family="Arctiidae" genus="Paracles" kingdom="Animalia" order="Lepidoptera" pageId="4" phylum="Arthropoda" rank="species" species="klagesi">
