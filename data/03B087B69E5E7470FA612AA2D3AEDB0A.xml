<taxonomicName id="4C194D239E5E7471FA612AA2D2BDDFAC" authority="Dohrn, 1860" authorityName="Dohrn" authorityYear="1860" box="[264,643,1742,1768]" class="Insecta" family="Reduviidae" genus="Emesa" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="annulata">
