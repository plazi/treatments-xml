<taxonomicName id="23957EA3E64EFFFB3D9DA2FDFD4762A9" authority="Badamdorj, 2003" authorityName="Badamdorj" authorityYear="2003" box="[417,708,1407,1429]" class="Arachnida" family="Eremaeidae" genus="Eueremaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="19" pageNumber="1589" phylum="Arthropoda" rank="species" species="tenuisensillatus" status="sp. nov.">
