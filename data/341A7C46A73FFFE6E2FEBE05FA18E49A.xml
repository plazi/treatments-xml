<heading id="E7447A3CA73FFFE5E2FEBE05FD4BE119" bold="true" box="[151,719,1842,1868]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<taxonomicName id="7BB3B6D3A73FFFE5E2FEBE05FD4BE119" authority="Aguiar et Ramos, 2011" authorityName="Aguiar et Ramos" authorityYear="2011" box="[151,719,1842,1868]" class="Insecta" family="Ichneumonidae" genus="Digonocryptus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="arcaeus">
