<emphasis id="79A0EAB1665EFF9AFF01F983FE88FF63" bold="true" box="[151,369,1637,1662]" italics="true" pageId="6" pageNumber="377">
<taxonomicName id="8CD44D20665EFF9AFF01F983FF12FF63" box="[151,235,1637,1662]" class="Insecta" family="Capniidae" genus="Capnia" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="377" phylum="Arthropoda" rank="genus" sensu="lato">Capnia</taxonomicName>
