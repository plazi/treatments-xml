<taxonomicName id="6015AB426D9E155E2BD184D196C9F2E8" LSID="http://zoobank.org/4CBFFD98-D953-43C8-B15B-8B7A32B5666F" class="Insecta" family="Melolonthidae" genus="Tetraserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetraserica changshouensis" order="Coleoptera" pageId="20" pageNumber="103" phylum="Arthropoda" rank="species" species="changshouensis">Tetraserica changshouensis</taxonomicName>
