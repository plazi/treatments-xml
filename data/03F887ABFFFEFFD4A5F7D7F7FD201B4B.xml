<taxonomicName id="4C514D3EFFFEFFD4A147D4CEFB0F1F8E" authorityName="Waterston" authorityYear="1915" box="[1063,1169,942,967]" class="Insecta" family="Menoponidae" genus="Myrsidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="22" pageNumber="423" phylum="Arthropoda" rank="genus">
