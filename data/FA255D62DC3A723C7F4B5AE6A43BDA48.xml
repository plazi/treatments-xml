<taxonomicName id="C743284560532D6E0E8F4C4E6186283D" authorityName="van Achterberg" authorityYear="1990" class="Insecta" family="Braconidae" genus="Plesiocoelus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Plesiocoelus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
