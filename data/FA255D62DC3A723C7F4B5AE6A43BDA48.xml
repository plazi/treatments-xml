<taxonomicName id="E579946B791411F81CC29AE60B7E5EA1" authority="Wesmael, 1837" authorityName="Wesmael" authorityYear="1837" class="Insecta" family="Braconidae" genus="Therophilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Therophilus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
