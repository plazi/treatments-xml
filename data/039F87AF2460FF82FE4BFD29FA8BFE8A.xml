<collectingCountry id="F32176292460FF8DFE47F810FD82F839" box="[496,610,1961,1987]" name="Indonesia" pageId="23" pageNumber="24">Indonesia</collectingCountry>
(
<collectingRegion id="49F2F85B2460FF8DFDC4F810FD39F839" box="[627,729,1961,1987]" country="Indonesia" name="Sumatera Utara" pageId="23" pageNumber="24">Sumatra</collectingRegion>
, 
