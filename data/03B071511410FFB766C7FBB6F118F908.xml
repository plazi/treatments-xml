<location id="8EC6969C1410FFB76755F9CCF38DF999" LSID="urn:lsid:plazi:treatment:03B071511410FFB766C7FBB6F118F908:8EC6969C1410FFB76755F9CCF38DF999" box="[261,392,1547,1571]" name="Mer Rouge" pageId="5" pageNumber="6">Mer Rouge</location>
</materialsCitation>
</emphasis>
.
