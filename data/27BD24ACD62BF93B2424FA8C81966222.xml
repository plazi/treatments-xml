<taxonomicName id="D798B6B4D2E206AFFD790A699F37B332" family="Staphylinidae" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="terminalis" order="Coleoptera" pageId="5" pageNumber="222" rank="species" species="terminalis">terminalis</taxonomicName>
