<taxonomicName id="126DEC5C04C048C31715B8F83B927127" class="Insecta" family="Staphylinidae" genus="Liogluta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liogluta terminalis" order="Coleoptera" pageId="5" pageNumber="222" phylum="Arthropoda" rank="species" species="terminalis">Liogluta terminalis</taxonomicName>
