<taxonomicName id="58F37717FDB4CA3878DF1BFDC393414F" class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta renominata" order="Coleoptera" pageId="5" pageNumber="222" phylum="Arthropoda" rank="species" species="renominata">Atheta renominata</taxonomicName>
