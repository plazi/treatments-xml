<taxonomicName id="4C534D7FFFFEFF9559880262FDB77116" authority="(Blandford)" authorityName="Blandford" baseAuthorityName="Blandford" box="[151,532,1092,1117]" class="Insecta" family="Curculionidae" genus="Dryocoetiops" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="247" phylum="Arthropoda" rank="species" species="flavicornis">
