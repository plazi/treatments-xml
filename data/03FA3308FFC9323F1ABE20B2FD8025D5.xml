<taxonomicName id="4C53F99DFFC8323F1B632165FDB62539" authority="Wang &amp; Zhou, 2020" authorityName="Wang &amp; Zhou" authorityYear="2020" box="[330,539,333,359]" class="Magnoliopsida" family="Amaranthaceae" genus="Exomis" kingdom="Plantae" order="Caryophyllales" pageId="3" pageNumber="354" phylum="Tracheophyta" rank="species" species="deqinensis" status="sp. nov.">
