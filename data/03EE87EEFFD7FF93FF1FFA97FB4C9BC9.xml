There are teeth at the basis of pereiopod 
<quantity id="4CBF9B1DFFEBFF93FE1DFEA7FE5898CA" box="[405,448,367,391]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="35" pageNumber="36" unit="in" value="1.0">1 in</quantity>
<taxonomicName id="4C474D7BFFEBFF93FE4FFEB9FDFE98C5" authority="Mühlenhardt-Siegel, 2011" authorityName="Mühlenhardt-Siegel" authorityYear="2011" box="[455,614,368,392]" class="Insecta" family="Curculionidae" genus="Leucon" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="longirhinus" status="sp. n.">
<emphasis id="B933EAEAFFEBFF93FE4FFEB9FDFE98C5" box="[455,614,368,392]" italics="true" pageId="35" pageNumber="36">L. longirhinus</emphasis>
</taxonomicName>
<emphasis id="B933EAEAFFEBFF93FDE5FEB8FD3398C5" bold="true" box="[621,683,368,392]" pageId="35" pageNumber="36">
<taxonomicNameLabel id="A2005791FFEBFF93FDE5FEB8FD3398C5" box="[621,683,368,392]" pageId="35" pageNumber="36" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
(0 in 
<taxonomicName id="4C474D7BFFEBFF93FD67FEB9FC2D98C5" authority="Mühlenhardt-Siegel, 2011" authorityName="Mühlenhardt-Siegel" authorityYear="2011" box="[751,949,368,392]" class="Insecta" family="Curculionidae" genus="Leucon" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="grandidentatus" status="sp. n.">
