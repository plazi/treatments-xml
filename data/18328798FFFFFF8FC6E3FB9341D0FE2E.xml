<number id="265324A9FFFFFF8EC3F5FACE41D7FAD6" box="[1409,1435,1312,1335]" pageId="8" pageNumber="53" value="40.0">40</number>
<number id="265324A9FFFFFF8EC6E3FAAE44FEFAB6" box="[151,178,1344,1367]" pageId="8" pageNumber="53" value="40.0">40</number>
