<emphasis id="0B6F62768245287FB943F9940CE96C83" bold="true" pageId="30" pageNumber="33">Figures 94.</emphasis>
<taxonomicName id="B8C0ABC0D795FA4195722CF3EF9B7932" authority="Ashmead, 1893" authorityName="Ashmead" authorityYear="1893" baseAuthorityName="Nixon" baseAuthorityYear="1943" class="Insecta" family="Platygastroidea" genus="Trissolcus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trissolcus larides" order="Hemiptera" pageId="30" pageNumber="33" phylum="Arthropoda" rank="species" species="larides">
