<taxonomicName id="4C334D56D55339561DFFFAF8FDA6454A" authorityName="Peters" authorityYear="1871" box="[502,601,1371,1395]" class="Mammalia" family="Mammalia (awaiting allocation)" genus="Doryrhina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mammalia (awaiting allocation)" pageId="19" pageNumber="500" phylum="Chordata" rank="genus">
