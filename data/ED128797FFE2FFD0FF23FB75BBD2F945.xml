<taxonomicName id="A2BB4D02FFE2FFD0FF23FB75BA2CFB61" authority="Mamaev, 1990" authorityName="Mamaev" authorityYear="1990" box="[151,512,1197,1224]" class="Insecta" family="Cecidomyiidae" genus="Dendrepidosis" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="157" phylum="Arthropoda" rank="genus">
