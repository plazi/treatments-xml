<subSubSection id="C33D698DD873FFE7BF96E7A62A7AFB91" pageId="103" pageNumber="102" type="discussion">
<paragraph id="8B983A06D873FFE7BF96E7A62A7AFB91" blockId="103.[192,1441,963,1115]" pageId="103" pageNumber="102">
<emphasis id="B953E614D873FFE7BF96E7A62952FC11" bold="true" box="[192,324,963,987]" pageId="103" pageNumber="102">Remarks.</emphasis>
<taxonomicName id="4C274185D873FFE7BE1DE7A62A00FC11" authorityName=": Korschefsky" authorityYear="1931" box="[331,534,963,987]" class="Insecta" family="Coccinellidae" genus="Prodilis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="103" pageNumber="102" phylum="Arthropoda" rank="species" species="cribrata">
<emphasis id="B953E614D873FFE7BE1DE7A62A00FC11" box="[331,534,963,987]" italics="true" pageId="103" pageNumber="102">Prodilis cribrata</emphasis>
</taxonomicName>
