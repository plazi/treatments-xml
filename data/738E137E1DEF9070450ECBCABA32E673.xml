in Philippines 1 
<taxonomicName id="17F48A6E061E1B356CEB764F64D365C7" class="Arachnida" family="Psilodercidae" genus="Luzonacera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Luzonacera francescoballarini" order="Araneae" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="francescoballarini">Luzonacera francescoballarini</taxonomicName>
