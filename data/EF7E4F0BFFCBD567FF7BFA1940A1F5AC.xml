<quantity id="A02F53F8FFCBD564FE17F9744226F6FC" box="[471,615,1696,1717]" metricMagnitude="-2" metricUnit="m" metricValue="2.04" metricValueMax="2.25" metricValueMin="1.83" pageId="1" pageNumber="126" unit="mm" value="20.4" valueMax="22.5" valueMin="18.3">18.3–22.5 mm</quantity>
