<specimenCount id="9D16FD661432FFEECCCAD297F16CB4B5" box="[490,615,1408,1432]" pageId="10" pageNumber="242" type="generic">
<number id="3DD824C81432FFEECCCAD297F2FDB4B5" box="[490,502,1408,1432]" pageId="10" pageNumber="242" value="1.0">1</number>
