<taxonomicName id="4C424D37FFE1FFB59542F8D6FD02F8AD" authorityName="Fahey &amp; Gosliner" authorityYear="1999" box="[581,693,1876,1901]" class="Gastropoda" family="Discodorididae" genus="Halgerda" kingdom="Animalia" order="Nudibranchia" pageId="49" pageNumber="50" phylum="Mollusca" rank="species" species="toliara">
