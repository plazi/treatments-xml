<taxonomicName id="79DB63FBFFDBFFAAD826FCA1FBB23286" authority="Ferris &amp; Lafontaine" authorityName="Ferris &amp; Lafontaine" authorityYear="2009" box="[617,1030,870,897]" class="Insecta" family="Noctuidae" genus="Acontia" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="29" phylum="Arthropoda" rank="species" species="geminocula" status="sp. n.">
