<taxonomicName id="4C61D195AE22FFB5FEB0F909DB0AF975" authority="Hennemann &amp; Conle, 2020" authorityName="Hennemann &amp; Conle" authorityYear="2020" box="[297,595,1666,1690]" class="Insecta" family="Diapheromeridae" genus="Andeocalynda" kingdom="Animalia" order="Phasmida" pageId="24" pageNumber="325" phylum="Arthropoda" rank="species" species="putumayoense" status="n. sp.">
<emphasis id="B9157604AE22FFB5FEB0F909DB0AF975" box="[297,595,1666,1690]" italics="true" pageId="24" pageNumber="325">Andeocalynda putumayoense</emphasis>
