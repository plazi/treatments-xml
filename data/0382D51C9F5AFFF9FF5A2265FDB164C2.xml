<quantity id="4CD3C9EF9F5AFFF9FCC022B2FC89643E" box="[781,834,1258,1282]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="7" pageNumber="26" unit="m" value="5.0">5 m</quantity>
