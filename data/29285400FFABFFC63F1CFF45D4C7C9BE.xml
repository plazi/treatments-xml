<taxonomicName id="66819E95FFABFFC53EFDFE10D68ACDD2" authority="(Smith)" baseAuthorityName="Smith" box="[704,1020,404,425]" class="Hexapoda" family="Liopteridae" genus="Mesocynips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="insignis">
