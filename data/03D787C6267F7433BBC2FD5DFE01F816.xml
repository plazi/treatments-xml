<taxonomicName id="4C7E4D53267F7433B82BF8BEFC22F8A2" box="[894,1021,1878,1903]" class="Cestoda" family="Rhinobothriidae" genus="Anthocephalum" kingdom="Animalia" order="Rhinebothriidea" pageId="19" pageNumber="70" phylum="Platyhelminthes" rank="species" species="micheali">
