<taxonomicName id="4C2C5BCBFFD3096D633CFEE5CA3EFEEA" authority="Olexa, 1984" authorityName="Olexa" authorityYear="1984" box="[619,1044,257,276]" class="Insecta" family="Histeridae" genus="Hypocacculus" kingdom="Animalia" order="Coleoptera" pageId="68" pageNumber="65" phylum="Arthropoda" rank="species" species="neftensis" subGenus="Nessus">
