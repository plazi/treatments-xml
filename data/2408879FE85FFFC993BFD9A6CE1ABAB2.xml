<taxonomicName id="6BA14D0AE85EFFC990E9DFECC9F7B9B6" authority="Brulle" authorityName="Brulle" box="[961,1176,1508,1533]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="500" phylum="Arthropoda" rank="species" species="fimbriata">
