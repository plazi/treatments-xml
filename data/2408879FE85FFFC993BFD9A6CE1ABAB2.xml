<taxonomicName id="6BA14D0AE85EFFC990ADDC00C960BA6A" box="[901,1039,1544,1569]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="500" phylum="Arthropoda" rank="species" species="variabilis">
<emphasis id="9ED5EA9BE85EFFC990ADDC00C960BA6A" box="[901,1039,1544,1569]" italics="true" pageId="15" pageNumber="500">A. variabilis</emphasis>
</taxonomicName>
and 
<taxonomicName id="6BA14D0AE85EFFC9976DDC00C9AABA6A" box="[1093,1221,1544,1569]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="500" phylum="Arthropoda" rank="species" species="wilhelmi">
