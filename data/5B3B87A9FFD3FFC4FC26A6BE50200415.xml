<collectionCode id="B583AE7AFFDCFFC4FE5BA664502307B5" box="[453,523,1388,1411]" collectionName="Brazil, Sao Paulo, Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" country="Brazil" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="40" pageNumber="205" type="Museum">MZSP</collectionCode>
