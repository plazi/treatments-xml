<collectionCode id="B583AE7AFFDCFFC4FE8EA664537F07B5" box="[272,343,1388,1411]" collectionName="Brazil, Rio Grande do Sul, Porto Alegre, Universidade Federal do Rio Grande do Sul, Instituto de Biologia" country="Brazil" httpUri="http://grbio.org/cool/nen5-04em" name="Instituto de Biologia" pageId="40" pageNumber="205">UFRG</collectionCode>
