<paragraph id="E00AE09331D0A1A4347F179AAB81D32D" pageNumber="139">
<taxonomicName id="BDA38480CBF5CEEE27EBDD964D2BCFEB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29231" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica truncorum Fabricius" order="Hymenoptera" pageNumber="139" phylum="Arthropoda" rank="species" species="truncorum">Formica truncorum Fabricius</taxonomicName>
