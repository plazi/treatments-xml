<collectingMunicipality id="6BE6D004FFCE0D15433DFD01490EFD63" box="[805,1128,645,670]" pageId="21" pageNumber="332">Moravian Museum in Brno</collectingMunicipality>
, 
<collectingCountry id="F32A0AEEFFCE0D154460FD014854FD60" box="[1144,1330,645,670]" name="Czech Republic" pageId="21" pageNumber="332">Czech Republic</collectingCountry>
