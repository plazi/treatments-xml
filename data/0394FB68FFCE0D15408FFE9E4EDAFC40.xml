locality: cave 
<location id="8EE21CA5FFCE0D15424CFD2D4FE8FD3C" LSID="urn:lsid:plazi:treatment:0394FB68FFCE0D15408FFE9E4EDAFC40:8EE21CA5FFCE0D15424CFD2D4FE8FD3C" box="[596,654,681,706]" country="Czech Republic" municipality="Trebinje in Hercegovina" name="Baba" pageId="21" pageNumber="332">Baba</location>
