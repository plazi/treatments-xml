<taxonomicName id="4C3395F3DD160477D6CFFE12F80CA7C6" box="[479,580,404,429]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="laesus">
