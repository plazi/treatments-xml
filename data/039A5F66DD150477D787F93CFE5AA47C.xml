<taxonomicName id="4C3395F3DD160477D294FEF6FB41A7C7" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="amurensis">
<emphasis id="B9473262DD160477D294FEF6FB41A7C7" italics="true" pageId="34" pageNumber="35">B. amurensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3395F3DD160477D606FE12FB9AA7C6" box="[278,466,404,429]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="distinguendus">
<emphasis id="B9473262DD160477D606FE12FB9AA7C6" box="[278,466,404,429]" italics="true" pageId="34" pageNumber="35">B. distinguendus</emphasis>
