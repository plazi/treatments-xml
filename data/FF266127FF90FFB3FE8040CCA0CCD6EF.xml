<taxonomicName id="B08FABB2FF91FFB4FE7644FCA022D1DA" box="[510,618,1716,1739]" class="Insecta" family="Reduviidae" genus="Bactrodes" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">Bactrodes</taxonomicName>
misionensis
