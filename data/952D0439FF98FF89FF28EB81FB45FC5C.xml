<taxonomicName id="DA84CEACFF98FF89FEDFEA26FD34FCE8" authority="Burmeister, 1839" authorityName="Burmeister" authorityYear="1839" box="[315,718,830,853]" class="Insecta" family="Calopterygidae" genus="Calopteryx" kingdom="Animalia" order="Odonata" pageId="18" pageNumber="249" phylum="Arthropoda" rank="species" species="iridipennis">
