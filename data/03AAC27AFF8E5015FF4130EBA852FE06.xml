<taxonomicName id="4C0308EFFF8E5015FE9730EBAE84F924" authority="K. O. Viets, 1978" authorityName="K. O. Viets" authorityYear="1978" box="[310,696,1761,1785]" class="Arachnida" family="Hygrobatidae" genus="Procorticacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="11" pageNumber="331" phylum="Arthropoda" rank="genus">
