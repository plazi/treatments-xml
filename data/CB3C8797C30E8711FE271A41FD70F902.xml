(
<figureCitation id="DBAE2A04C30E8711FDDF1AA1FD51FC0C" box="[558,754,899,923]" captionStart="Figures 147–164" captionStartId="56.[138,207,1530,1549]" captionTargetBox="[154,1168,133,1492]" captionTargetId="figure-1@56.[154,1170,133,1495]" captionTargetPageId="56" captionText="Figures 147–164. Columbicola harrisoni: (147) female ventral terminalia. C. reedi: (148) male dorsal head; (149) female dorsal head; (150) male genitalia; (151) female ventral terminalia. C. gourae: (152) male dorsal head; (153) male genitalia; (154) female dorsal head; (155) female ventral terminalia. C. paradoxus: (156) male dorsal head; (157) male genitalia. C. clayae: (158) male dorsal head; (159) female dorsal head; (160) male genitalia; (161) female ventral terminalia. C. davisae: (162) male dorsal head; (163) female dorsal head; (164) male genitalia." figureDoi="http://doi.org/10.5281/zenodo.5220822" httpUri="https://zenodo.org/record/5220822/files/figure.png" pageId="52" pageNumber="3595">Figures 152–155</figureCitation>
) 
<taxonomicName id="84954D02C30E8711FF541A80FDC7FC2D" authority="Tendeiro 1984: 97" authorityName="Tendeiro" authorityPageNumber="97" authorityYear="1984" box="[165,612,930,954]" class="Insecta" family="Philopteridae" genus="Columbicola" kingdom="Animalia" order="Psocodea" pageId="52" pageNumber="3595" phylum="Arthropoda" rank="species" species="gourae">
<emphasis id="71E1EA93C30E8711FF541A80FED9FC2D" box="[165,378,930,954]" italics="true" pageId="52" pageNumber="3595">Columbicola gourae</emphasis>
<bibRefCitation id="27044B70C30E8711FE751A80FDC7FC2D" author="Tendeiro J" box="[388,612,930,954]" pageId="52" pageNumber="3595" pagination="77 - 118" refId="ref39526" refString="Tendeiro J. 1984. Nouvelles etudes sur la systematique, la zoogeographie et l'ecologie du genre Columbicola Ewing, 1929 (Mallophaga, Ischnocera). Garcia de Orta, Serie Zoologia 11: 77 - 118." type="journal article" year="1984">Tendeiro 1984: 97</bibRefCitation>
