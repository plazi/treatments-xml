<subSubSection id="C37C6549097D0440FF2D7F55FA95FE5D" lastPageId="20" lastPageNumber="21" pageId="19" pageNumber="20" type="diagnosis">
<paragraph id="8BD936C2097D0447FF2D7F55FED4FBAE" blockId="19.[189,310,1067,1093]" box="[189,310,1067,1093]" pageId="19" pageNumber="20">
