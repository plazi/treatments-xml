<taxonomicName id="4C6844F76E12D214FE51FB30FDAAE7AD" baseAuthorityName="Kira" baseAuthorityYear="1959" box="[389,597,1169,1192]" class="Gastropoda" family="Muricidae" genus="Vokesimurex" kingdom="Animalia" order="Neogastropoda" pageId="5" pageNumber="442" phylum="Mollusca" rank="species" species="kiiensis">
