<number id="8B65248FFF950E68F974BAB87A69F903" box="[572,611,1740,1766]" pageId="2" pageNumber="51" value="6.5" valueMax="7.0" valueMin="6.0">6-7</number>
pocl black. Legs. I: 
<number id="8B65248FFF950E68F819BAB87B72F903" box="[849,888,1740,1766]" pageId="2" pageNumber="51" value="1.7">1.7</number>
, 
<number id="8B65248FFF950E68F8CCBAB87BA1F903" box="[900,939,1740,1766]" pageId="2" pageNumber="51" value="1.7">1.7</number>
