Mountain, 
<geoCoordinate id="E91EC03800F647105AB7A3924579CEA9" direction="north" orientation="latitude" precision="15" value="41.044445">41°2'40&quot;N</geoCoordinate>
, 
<geoCoordinate id="FB7383861AF8F121062542EAE18B41C5" direction="east" orientation="longitude" precision="15" value="122.62723">122°37'38&quot;E</geoCoordinate>
