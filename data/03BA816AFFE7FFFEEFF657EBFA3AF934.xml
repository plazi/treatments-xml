<taxonomicName id="4C134BFFFFE7FFFEEA405114FAC0FBF4" authority="Mound, 2019" authorityName="Mound" authorityYear="2019" box="[1313,1382,1062,1083]" class="Insecta" family="Phlaeothripidae" genus="Haplothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="42" phylum="Arthropoda" rank="species" species="aliceae" status="sp.n.">
<emphasis id="B967EC6EFFE7FFFEEA405114FAC0FBF4" box="[1313,1382,1062,1083]" italics="true" pageId="1" pageNumber="42">aliceae</emphasis>
