<materialsCitation id="E2DA3450E704FFC9FF01FE6BFE360ECF" ID-GBIF-Occurrence="1841140125" collectionCode="PABM" collectorName="S. Richards" location="Casey Station" pageId="15" pageNumber="216" specimenCount="5" specimenCount-male="5">
