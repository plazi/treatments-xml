<number id="D41AA141124BFFE3FF1F2B376ED4FD67" box="[205,255,637,660]" pageId="1" pageNumber="398" value="0.12">0.12</number>
; AME-ALE 
<number id="D41AA141124BFFE3FE5B2B376F91FD67" box="[393,442,637,660]" pageId="1" pageNumber="398" value="0.12">0.12</number>
, PME-PLE 
<number id="D41AA141124BFFE3FDE92B376C47FD67" box="[571,620,637,660]" pageId="1" pageNumber="398" value="0.24">0.24</number>
, AME-AME 
<number id="D41AA141124BFFE3FD2C2B376D04FD67" box="[766,815,637,660]" pageId="1" pageNumber="398" value="0.1">0.10</number>
