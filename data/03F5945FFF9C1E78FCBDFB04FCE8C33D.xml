<caption id="DF2375C1FF921E78FF79FC21FD56C257" ID-DOI="http://doi.org/10.5281/zenodo.5445470" ID-Zenodo-Dep="5445470" httpUri="https://zenodo.org/record/5445470/files/figure.png" pageId="57" pageNumber="431" startId="57.[144,223,988,1007]" targetBox="[148,756,197,959]" targetPageId="57">
