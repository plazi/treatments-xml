<taxonomicName id="4C5C5ECAFF921E78FE73FC20FDE6C5AA" box="[410,618,989,1008]" class="Amphibia" family="Rhacophoridae" genus="Philautus" kingdom="Animalia" order="Anura" pageId="57" pageNumber="431" phylum="Chordata" rank="species" species="ponmudi">
