<taxonomicName id="4C1FD9E2FFA8FF95EC62F980FDFCB26C" authorityName="Zhang, Deharveng, Greenslade &amp; Chen" authorityYear="2009" baseAuthorityName="Yosii" baseAuthorityYear="1965" box="[347,548,1587,1609]" class="Collembola" family="Entomobryidae" genus="Sinhomidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="571" phylum="Arthropoda" rank="species" species="bicolor">
<emphasis id="B96B7E73FFA8FF95EC62F980FDFCB26C" bold="true" box="[347,548,1587,1609]" italics="true" pageId="2" pageNumber="571">Sinhomidia bicolor</emphasis>
