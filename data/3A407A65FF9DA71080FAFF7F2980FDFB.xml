<paragraph id="B256CB73FF9DA71080AAFE3D2F61FD97" blockId="9.[151,1437,325,602]" pageId="9" pageNumber="210">
<emphasis id="809D1761FF9DA71080AAFE3D2ABBFE4F" bold="true" box="[199,472,469,494]" pageId="9" pageNumber="210">Diagnostic description.</emphasis>
Body-length 
<quantity id="75116696FF9DA710821FFE3D2998FE4C" box="[626,763,469,494]" metricMagnitude="-3" metricUnit="m" metricValue="5.55" metricValueMax="6.8" metricValueMin="4.3" pageId="9" pageNumber="210" unit="mm" value="5.55" valueMax="6.8" valueMin="4.3">4.3–6.8 mm</quantity>
