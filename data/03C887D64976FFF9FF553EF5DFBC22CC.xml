<number id="3DA924E74976FFFDFBE13FCADB1B20E9" box="[1059,1104,981,1007]" pageId="8" pageNumber="27" value="155.0">155</number>
º 
<number id="3DA924E74976FFFDFB983FCADB3320E9" box="[1114,1144,981,1007]" pageId="8" pageNumber="27" value="23.0">23</number>
'E
</geoCoordinate>
, depth 
<quantity id="4C999B254976FFFDFB213FCADA7420E9" box="[1251,1343,981,1007]" metricMagnitude="3" metricUnit="m" metricValue="4.945" pageId="8" pageNumber="27" unit="m" value="4945.0">4945 m</quantity>
, coll. in 
