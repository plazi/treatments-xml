<taxonomicName id="0E5E7423B5B5D2731F5BD7CCE42E96AF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:30643" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Meranoplus" kingdom="Animalia" lsidName-HNS="Meranoplus radamae Forel" order="Hymenoptera" pageNumber="148" phylum="Arthropoda" rank="species" species="radamae" status="n. sp.">MERANOPLUS RADAMAE</taxonomicName>
