, 
<emphasis id="B971EAD2FFBF59735B75A7B80135D697" bold="true" box="[797,874,1942,1964]" pageId="40" pageNumber="541">
<taxonomicNameLabel id="A24257A9FFBF59735B75A7B80108D697" box="[797,855,1942,1964]" pageId="40" pageNumber="541" rank="species">sp. n.</taxonomicNameLabel>
a
</emphasis>
, habitus, lateral view; 
<emphasis id="B971EAD2FFBF59735C3BA7B8063ED697" bold="true" box="[1107,1121,1942,1964]" pageId="40" pageNumber="541">b</emphasis>
