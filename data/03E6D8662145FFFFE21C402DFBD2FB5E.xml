<taxonomicName id="4C4F12F32145FFFFE2464786FE71FB2A" authorityName="Zorn" authorityYear="2007" box="[339,499,1154,1178]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="85" phylum="Arthropoda" rank="species" species="bifida">
