4. 
<taxonomicName id="4C6D4D78FFECA851FC30FDE08031FD1F" authority="Andersen, 1905" authorityName="Andersen" authorityYear="1905" box="[915,1817,605,700]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="7" pageNumber="82" phylum="Chordata" rank="species" species="nanus" status="sp. n.">
<emphasis id="B919EAE9FFECA851FC30FDE08031FD1F" bold="true" box="[915,1817,605,700]" pageId="7" pageNumber="82">
