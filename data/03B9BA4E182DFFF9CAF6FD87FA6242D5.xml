<quantity id="4CE8A6BD182DFFF9C99CFC7BFBA746C6" box="[1021,1113,958,982]" metricMagnitude="-1" metricUnit="m" metricValue="5.5" metricValueMax="10.0" metricValueMin="1.0" pageId="37" pageNumber="38" unit="m" value="0.55" valueMax="1.0" valueMin="0.1">
