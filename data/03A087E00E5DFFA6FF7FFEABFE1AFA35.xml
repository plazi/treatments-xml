<collectionCode id="ED18AE330E5DFFA6FCBBFBAFFC76FBF1" LSID="urn:lsid:biocol.org:col:34988" box="[851,940,1089,1114]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="20" pageNumber="121">MNHN</collectionCode>
