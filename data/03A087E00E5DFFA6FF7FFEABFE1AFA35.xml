spm 
<collectionCode id="ED18AE330E5DFFA6FB3FFBACFAE8FBF1" LSID="urn:lsid:biocol.org:col:34988" box="[1239,1330,1090,1114]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="20" pageNumber="121">MNHN</collectionCode>
