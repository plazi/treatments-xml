<pageBreakToken id="23684ECA1DE7F66BCECD914C9FE6BC18" pageId="27" pageNumber="28" start="start">12</pageBreakToken>
. 
<taxonomicName id="6C9CEE3F331747343D8E7A7CE7489DC8" LSID="http://species-id.net/wiki/Mecyclothorax_fosbergi" authority="Perrault, 1979: 9; 1988: 232" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax fosbergi" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="fosbergi">Mecyclothorax fosbergi Perrault, 1979: 9; 1988: 232</taxonomicName>
