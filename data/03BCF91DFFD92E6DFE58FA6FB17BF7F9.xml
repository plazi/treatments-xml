<taxonomicName id="4C153388FFD92E6DFE58FA6FB5C1F98A" authorityName="Cope" authorityYear="1876" box="[395,703,1526,1557]" class="Reptilia" family="Hadrosauridae" genus="Diclonius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="7" pageNumber="255" phylum="Chordata" rank="species" species="calamarius" status="sp. nov.">Diclonius calamarius</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A2522962FFD92E6DFD07FA6EB441F98D" box="[724,831,1524,1557]" pageId="7" pageNumber="255" rank="species">sp. nov.</taxonomicNameLabel>
