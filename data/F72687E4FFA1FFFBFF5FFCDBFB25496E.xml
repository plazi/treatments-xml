<taxonomicName id="B88F4D71FFA1FFFBFBD7FC46FAFE4FBE" authority="Pic, 1936" authorityName="Pic" authorityYear="1936" box="[1055,1288,900,925]" class="Insecta" family="Meloidae" genus="Ceroctis" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="seabrai">
<emphasis id="4DFBEAE0FFA1FFFBFBD7FC46FB624FBF" box="[1055,1172,900,924]" italics="true" pageId="54" pageNumber="55">C. seabrai</emphasis>
