<emphasis id="B9715E109C71FF857CFFFDDEFE73FDD0" bold="true" box="[213,445,546,572]" pageId="67" pageNumber="68">
<collectingRegion id="49C14CE09C71FF857CFFFDDEFECCFDD0" box="[213,258,546,572]" country="South Sudan" name="Lakes" pageId="67" pageNumber="68">Leg</collectingRegion>
