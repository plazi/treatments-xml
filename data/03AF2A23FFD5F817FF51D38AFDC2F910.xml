<typeStatus id="54BD2597FFD5F817FF51D207FF2EFC9C" box="[151,207,796,822]" pageId="47" pageNumber="48">Type</typeStatus>
host: 
<taxonomicName id="4C06E0B6FFD5F817FED5D207FDDBFC9C" authorityName="Blyth" authorityYear="1863" box="[275,570,796,822]" class="Reptilia" family="Agamidae" genus="Phrynocephalus" kingdom="Animalia" order="Squamata" pageId="47" pageNumber="48" phylum="Chordata" rank="species" species="theobaldi">
