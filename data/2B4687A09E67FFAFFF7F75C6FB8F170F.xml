<collectingRegion id="612BF8549E67FFAFFE60776BFF4515E9" country="Australia" name="Northern Territory" pageId="99" pageNumber="99">Northern Territory</collectingRegion>
, 
<collectingCountry id="DBF876269E67FFAFFF727794FEC615E9" box="[193,304,910,932]" name="Australia" pageId="99" pageNumber="99">Australia</collectingCountry>
, on 
<date id="D75110769E67FFAFFED47794FDD015EE" box="[359,550,910,931]" pageId="99" pageNumber="99" value="1913-08-30">30 August 1913</date>
