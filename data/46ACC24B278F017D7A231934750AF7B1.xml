<taxonomicName id="A52F73AE6639286384710741033CE33B" class="Insecta" family="Canacidae" genus="Paracanace" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracanace oliveirai" order="Diptera" pageId="13" pageNumber="72" phylum="Arthropoda" rank="species" species="oliveirai">Paracanace oliveirai</taxonomicName>
