<collectingCountry id="F37876651D2DD37AFE80420F67E59C14" box="[359,459,1807,1833]" name="Thailand" pageId="2" pageNumber="597">Thailand</collectingCountry>
(
<collectingRegion id="49ABF8171D2DD37AFE3E420F64A49C17" box="[473,650,1807,1834]" country="Thailand" name="Nakhon Nayok" pageId="2" pageNumber="597">Nakhon Nayok</collectingRegion>
