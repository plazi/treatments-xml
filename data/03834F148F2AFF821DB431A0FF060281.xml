<caption id="DF55AE8A8F28FF841DB433EAFE7C0430" ID-DOI="http://doi.org/10.5281/zenodo.5426508" ID-Zenodo-Dep="5426508" httpUri="https://zenodo.org/record/5426508/files/figure.png" pageId="10" pageNumber="61" startId="10.[136,229,1694,1716]" targetBox="[167,1420,208,1657]" targetPageId="10">
