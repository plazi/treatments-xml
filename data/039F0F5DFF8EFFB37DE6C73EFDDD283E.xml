(n = 
<number id="3DFEAC6CFF8FFFB37CC1C114FE202B42" box="[432,446,332,356]" pageId="21" pageNumber="22" value="1.0">1</number>
): HB = 
<number id="3DFEAC6CFF8FFFB37F6AC114FDA72B42" box="[539,569,332,356]" pageId="21" pageNumber="22" value="82.0">82</number>
, T = 
<number id="3DFEAC6CFF8FFFB37F05C114FD3E2B42" box="[628,672,332,356]" pageId="21" pageNumber="22" value="102.0">102</number>
, HF = 
<number id="3DFEAC6CFF8FFFB37F9CC114FC952B42" box="[749,779,332,356]" pageId="21" pageNumber="22" value="23.0">23</number>
