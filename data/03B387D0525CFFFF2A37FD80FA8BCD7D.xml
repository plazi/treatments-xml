<taxonomicName id="4C1A4D455259FFFF290EFC15FD58CE0F" box="[561,760,1019,1042]" class="Insecta" family="Dynastidae" genus="Ancognatha" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="67" phylum="Arthropoda" rank="species" species="veliae">
