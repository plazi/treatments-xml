<taxonomicName id="33CD4D1DD54BFFEFD497F96CFC978D61" box="[485,819,1791,1814]" class="Insecta" family="Coleophoridae" genus="Coleophora" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="45" phylum="Arthropoda" rank="species" species="innermongoliensis" status="sp. n.">
<emphasis id="C6B9EA8CD54BFFEFD497F96CFC978D61" box="[485,819,1791,1814]" italics="true" pageId="8" pageNumber="45">Coleophora innermongoliensis</emphasis>
</taxonomicName>
<emphasis id="C6B9EA8CD54BFFEFD64EF96DFCDE8D61" bold="true" box="[828,890,1790,1814]" pageId="8" pageNumber="45">
