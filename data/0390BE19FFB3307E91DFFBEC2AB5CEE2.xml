<taxonomicName id="4C39748CFFB03078903CFA472A2AC8C8" authority="(Lehrer)" authorityName="Rognes" authorityYear="2012" baseAuthorityName="Lehrer" baseAuthorityYear="2005" box="[372,626,1456,1478]" class="Insecta" family="Calliphoridae" genus="Bengalia" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="wyatti">
