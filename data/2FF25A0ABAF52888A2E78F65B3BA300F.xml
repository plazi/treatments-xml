[[ Genus 
<taxonomicName id="F0E80AADD11182FFB0C0C4CC6D8464BE" LSID-ZBK="urn:lsid:zoobank.org:act:1C6AC061-0134-487B-9102-EFF719A47063" class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pareuchiloglanis Pellegrin 1936:245" order="Siluriformes" pageNumber="1" phylum="Chordata" rank="genus">Pareuchiloglanis Pellegrin</taxonomicName>
