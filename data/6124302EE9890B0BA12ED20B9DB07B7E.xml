<taxonomicName id="41D75CEB00DDB6EDDCE6B95A41407CBB" LSID="http://zoobank.org/D5207705-FEF4-42CA-B28B-C2DB218B22AF" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus flavostigmus" order="Hymenoptera" pageId="350" pageNumber="351" phylum="Arthropoda" rank="species" species="flavostigmus">Heterospilus flavostigmus Marsh</taxonomicName>
<taxonomicNameLabel id="727663A7128808811AD0C0DDD49A74CA" pageId="350" pageNumber="351">sp. n.</taxonomicNameLabel>
