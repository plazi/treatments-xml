<taxonomicName id="8BA455B2606222451FDB7C04C006EAF3" class="Arachnida" family="Damaeidae" genus="Metabelba" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="7" pageNumber="148" phylum="Arthropoda" rank="species" species="pulverosa">Metabelba pulverosa Strenzke</taxonomicName>
