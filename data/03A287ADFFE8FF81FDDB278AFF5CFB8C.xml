<quantity id="4CF39B5EFFE8FF8FFE942291FE6AF935" box="[357,450,1687,1714]" metricMagnitude="-3" metricUnit="m" metricValue="5.2" pageId="6" pageNumber="7" unit="mm" value="5.2">5.2 mm</quantity>
