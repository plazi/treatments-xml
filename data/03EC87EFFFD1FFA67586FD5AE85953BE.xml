). 
<location id="8E9A6022FFD1FFA6740AF95DED0253C7" LSID="urn:lsid:plazi:treatment:03EC87EFFFD1FFA67586FD5AE85953BE:8E9A6022FFD1FFA6740AF95DED0253C7" box="[283,335,1681,1707]" name="This" pageId="22" pageNumber="23" stateProvince="Guangxi">This</location>
