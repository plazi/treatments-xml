at 
<location id="8E9A6022FFD1FFA67651F9D1EF88535B" LSID="urn:lsid:plazi:treatment:03EC87EFFFD1FFA67586FD5AE85953BE:8E9A6022FFD1FFA67651F9D1EF88535B" box="[832,965,1565,1591]" municipality="Vincent Kalkman" name="Longsheng" pageId="22" pageNumber="23" stateProvince="Guangxi">Longsheng</location>
, 
<collectingRegion id="4981F81BFFD1FFA676C1F9D1E875535B" box="[976,1080,1565,1591]" country="China" name="Guangxi" pageId="22" pageNumber="23">Guangxi</collectingRegion>
</materialsCitation>
, 
<materialsCitation id="3B2D3CA4FFD1FFA67155F9D1E96B5332" collectorName="Asiadragonfly" location="The" pageId="22" pageNumber="23" specimenCount="1" stateProvince="Guangdong">
