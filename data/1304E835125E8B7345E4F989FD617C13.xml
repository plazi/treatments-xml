<taxonomicName id="5CAD22A012588B71471BF8C1FC887946" authority="Eskov &amp; Marusik, 2021" authorityName="Eskov &amp; Marusik" authorityYear="2021" box="[616,838,1845,1869]" class="Arachnida" family="Theridiidae" genus="Glebych" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="minutissimus" status="sp. n.">
