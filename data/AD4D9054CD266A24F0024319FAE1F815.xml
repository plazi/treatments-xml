<taxonomicName id="E2E45AC1CD266A24F2DD438EFD9BFB00" authority="Strand, 1934" authorityName="Strand" authorityYear="1934" box="[189,574,1187,1213]" class="Cephalopoda" family="Tarphycerida (awaiting allocation)" genus="Tyrioceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tarphycerida" pageId="66" pageNumber="67" phylum="Mollusca" rank="species" species="kjaerulfi">
<emphasis id="1790FD50CD266A24F2DD438EFE07FB00" box="[189,418,1187,1213]" italics="true" pageId="66" pageNumber="67">Tyrioceras kjaerulfi</emphasis>
<bibRefCitation id="41755CB3CD266A24F3C74389FD9BFB00" author="Strand T." box="[423,574,1187,1213]" pageId="66" pageNumber="67" pagination="1 - 117" refId="ref49308" refString="Strand T. 1934. The Upper Ordovician Cephalopods of the Oslo Area. Norsk Geologiske Tidsskrift 14: 1 - 117." type="journal article" year="1934">Strand, 1934</bibRefCitation>
