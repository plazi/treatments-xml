<specimenCount id="9D43FD23FFDAD738FC51FA9DFBEFFADE" box="[1016,1082,1320,1346]" pageId="25" pageNumber="26" type="female">
2 
<femaleSymbol id="B95DABC0FFDAD738FBA7FA9DFBEFFADE" box="[1038,1082,1320,1346]" pageId="25" pageNumber="26">♀♀</femaleSymbol>
</specimenCount>
