<number id="5B17A023991AFF9DE85D989ADE29FC05" box="[320,328,934,948]" pageId="41" pageNumber="42" value="1.0">1</number>
about 
<number id="5B17A023991AFF9DE88998ABDEFFFC1E" box="[404,414,919,943]" pageId="41" pageNumber="42" value="1.0">1</number>
/ 
<number id="5B17A023991AFF9DE8B498ABDED9FC1E" box="[425,440,919,943]" pageId="41" pageNumber="42" value="7.0">7</number>
