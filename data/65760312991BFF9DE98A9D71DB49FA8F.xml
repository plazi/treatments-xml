<number id="5B17A023991AFF9DEC399831DA4DFCAA" box="[1316,1324,781,795]" pageId="41" pageNumber="42" value="4.0">4</number>
+ 
<number id="5B17A023991AFF9DEC289831DA5DFCAA" box="[1333,1340,781,795]" pageId="41" pageNumber="42" value="5.0">5</number>
and M 
<number id="5B17A023991AFF9DEC899831DAFDFCAA" box="[1428,1436,781,795]" pageId="41" pageNumber="42" value="1.0">1</number>
confluent; brown cloud on 
<emphasis id="DFAB6E16991AFF9DE8CB981ADE9FFC8C" box="[470,510,806,829]" italics="true" pageId="41" pageNumber="42">r-m</emphasis>
and 
<emphasis id="DFAB6E16991AFF9DEB21981ADD15FC8C" box="[572,628,806,829]" italics="true" pageId="41" pageNumber="42">m-cu</emphasis>
, constricted at middle; subcostal cell brown apically; costa with 
<number id="5B17A023991AFF9DEC6F9819DA1EFC8F" box="[1394,1407,805,830]" pageId="41" pageNumber="42" value="2.0">2</number>
nd (between R 
<number id="5B17A023991AFF9DE8019864DE45FCD7" box="[284,292,856,870]" pageId="41" pageNumber="42" value="1.0">1</number>
and R 
<number id="5B17A023991AFF9DE8659864DEE1FCD7" box="[376,384,856,870]" pageId="41" pageNumber="42" value="2.0">2</number>
+ 
<number id="5B17A023991AFF9DE8949864DEF1FCD7" box="[393,400,856,870]" pageId="41" pageNumber="42" value="3.0">3</number>
), 
<number id="5B17A023991AFF9DE8B09875DED8FCD3" box="[429,441,841,866]" pageId="41" pageNumber="42" value="3.0">3</number>
rd (between R 
<number id="5B17A023991AFF9DEB799864DD0DFCD7" box="[612,620,856,870]" pageId="41" pageNumber="42" value="2.0">2</number>
+ 
<number id="5B17A023991AFF9DEB689864DD1DFCD7" box="[629,636,856,870]" pageId="41" pageNumber="42" value="3.0">3</number>
and R 
<number id="5B17A023991AFF9DEBCC9864DDB8FCD7" box="[721,729,856,870]" pageId="41" pageNumber="42" value="4.0">4</number>
+ 
<number id="5B17A023991AFF9DEBFC9864DD89FCD7" box="[737,744,856,870]" pageId="41" pageNumber="42" value="5.0">5</number>
) and 
<number id="5B17A023991AFF9DEA2E9875DC20FCD3" box="[819,833,841,866]" pageId="41" pageNumber="42" value="4.0">4</number>
th (between R 
<number id="5B17A023991AFF9DEAF49864DC90FCD7" box="[1001,1009,856,870]" pageId="41" pageNumber="42" value="4.0">4</number>
