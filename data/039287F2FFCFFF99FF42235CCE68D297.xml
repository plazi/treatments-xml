<taxonomicName id="4C3B4D67FFC8FF99FE8F2150C9E1D225" authorityName="Ament &amp; Amorim" authorityYear="2013" box="[346,658,1840,1862]" class="Insecta" family="Phoridae" genus="Neopleurophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="carcharopyga">
