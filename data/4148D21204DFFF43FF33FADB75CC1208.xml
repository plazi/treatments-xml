<taxonomicName id="0EE1188704C0FF43FD6FFE77753D11E4" authorityName="Theel" authorityYear="1882" box="[715,815,442,467]" class="Holothuroidea" family="Psychropotidae" genus="Benthodytes" kingdom="Animalia" order="Elasipodida" pageId="144" pageNumber="145" phylum="Echinodermata" rank="species" species="typica">
