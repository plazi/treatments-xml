<taxonomicName id="0EE1188704C0FF43FCCCFDCF75D9122C" authorityName="Theel" authorityYear="1882" box="[872,971,513,539]" class="Holothuroidea" family="Psychropotidae" genus="Benthodytes" kingdom="Animalia" order="Elasipodida" pageId="144" pageNumber="145" phylum="Echinodermata" rank="species" species="typica">
<emphasis id="FB95BF1604C0FF43FCCCFDCF756B122C" box="[872,889,514,539]" italics="true" pageId="144" pageNumber="145">B</emphasis>
