<taxonomicName id="0EE1188704C0FF43FF33FE77770611E4" authorityName="$ Marenzeller" authorityYear="1892" box="[151,276,441,467]" class="Holothuroidea" family="Psychropotidae" genus="Benthodytes" kingdom="Animalia" order="Elasipodida" pageId="144" pageNumber="145" phylum="Echinodermata" rank="species" species="janthina">
