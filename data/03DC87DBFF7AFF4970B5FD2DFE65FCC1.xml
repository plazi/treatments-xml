<geoCoordinate id="EE41500AFF7AFF49726CFA21FD2FFFA9" box="[590,654,1463,1488]" degrees="2" direction="east" minutes="20" orientation="longitude" pageId="246" pageNumber="247" precision="925" value="2.3333333">
