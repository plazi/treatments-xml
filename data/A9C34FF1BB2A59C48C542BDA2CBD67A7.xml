<taxonomicName id="CF7A131E2888AD29C7F773B568ABAFB2" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Pinales" pageId="0" pageNumber="57" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
