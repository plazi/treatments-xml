<subSubSection id="C1949593FFCD3D23959771E9FE07A9D9" pageId="15" pageNumber="16" type="materials_examined">
<paragraph id="8931C618FFCD3D23959771E9FE07A9D9" blockId="15.[151,1436,258,1074]" pageId="15" pageNumber="16">
<emphasis id="BBFA1A0AFFCD3D23959771E9FED3A991" box="[199,373,906,931]" inLineHeading="true" pageId="15" pageNumber="16" reason="1">
<emphasis id="BBFA1A0AFFCD3D23959771E9FEC8A990" bold="true" box="[199,366,906,931]" pageId="15" pageNumber="16">Type material</emphasis>
