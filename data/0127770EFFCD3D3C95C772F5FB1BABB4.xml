</emphasis>
Female 
<typeStatus id="563578BAFFCD3D23948671E8FD9EA991" box="[470,568,906,930]" pageId="15" pageNumber="16">holotype</typeStatus>
and 
<specimenCount id="9F880D91FFCD3D23973E71E8FD69A991" box="[622,719,906,930]" pageId="15" pageNumber="16" type="female">8 female</specimenCount>
