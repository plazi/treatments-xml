<taxonomicName id="4C504D78CD3CFFB3FCA82AD51C94EAA3" authority="Spix and Agassiz, 1829" authorityName="Spix and Agassiz" authorityYear="1829" box="[888,1256,675,697]" class="Actinopterygii" family="Cetopsidae" genus="Cetopsis" kingdom="Animalia" order="Siluriformes" pageId="48" pageNumber="803" phylum="Chordata" rank="genus">
