<typeStatus id="54C708D6FFD5FF87DD2BF9E7BACDA936" box="[580,679,1593,1619]" pageId="2" pageNumber="3">Neotype</typeStatus>
(
<collectionCode id="ED6D2EB1FFD5FF87DDD8F9E7BB64A936" box="[695,782,1593,1619]" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="2" pageNumber="3">SMNH</collectionCode>
