<emphasis id="68EB58674068FF8BFF60FAE5FEF7FAE9" bold="true" box="[199,353,1340,1367]" pageId="4" pageNumber="290">Distribution.</emphasis>
<taxonomicName id="9D9FFFF64068FF8BFECFFAE4FDE0FAE9" authorityName="Natarajan" authorityYear="2019" box="[360,630,1341,1367]" class="Insecta" family="Diptera (awaiting allocation)" genus="Paraedes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="290" phylum="Arthropoda" rank="species" species="jambulingami">
<emphasis id="68EB58674068FF8BFECFFAE4FDE0FAE9" box="[360,630,1341,1367]" italics="true" pageId="4" pageNumber="290">Paraedes jambulingami</emphasis>
</taxonomicName>
