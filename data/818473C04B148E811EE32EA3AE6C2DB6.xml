<taxonomicName id="8CC2441D3FA3E00CACAD9CC1425E2325" authorityName="Soula" authorityYear="2009" class="Insecta" family="Scarabaeidae" genus="Pelidnota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pelidnota acconciai" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="acconciai">
