<taxonomicName id="933D018D9B1CBFEC14822E4C16C3340A" authorityName="Andrewes" authorityYear="1929" class="Insecta" family="Carabidae" genus="Aeolodermus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">Aeolodermus</taxonomicName>
