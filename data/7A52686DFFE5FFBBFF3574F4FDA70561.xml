<collectionCode id="94EA41BEFFE5FFB8FF3D76A3FF1803A8" LSID="urn:lsid:biocol.org:col:33074" box="[159,224,1981,2005]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33074" name="China Agricultural University" pageId="5" pageNumber="63">CAU</collectionCode>
