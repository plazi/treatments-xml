<typeStatus id="2D4067D9FFE5FFB8FE6B7687FDCD03CC" box="[457,565,1945,1969]" pageId="5" pageNumber="63">Holotype</typeStatus>
: 
<number id="4433CB5CFFE5FFB8FDE57684FDA403CF" box="[583,604,1946,1970]" pageId="5" pageNumber="63" value="3.0">3</number>
, 
<collectingCountry id="8AEC99EBFFE5FFB8FDD27687FD3403CC" box="[624,716,1945,1969]" name="China" pageId="5" pageNumber="63">CHINA</collectingCountry>
, Yunnan Prov., Ruili, Mengxiu, 
<date id="8645FFBBFFE5FFB8FBF57687FB3803CC" box="[1111,1216,1945,1969]" pageId="5" pageNumber="63" value="1981-05-02">
