<typeStatus id="54CE50A5A73C891DFF09FA7BFF3AF9DA" box="[151,212,1535,1561]" pageId="10" pageNumber="11">Type</typeStatus>
locality:
</emphasis>
<collectingCountry id="F362AE97A73C891DFED6F984FE4DF9D9" box="[328,419,1536,1562]" name="Algeria" pageId="10" pageNumber="11">Algeria</collectingCountry>
