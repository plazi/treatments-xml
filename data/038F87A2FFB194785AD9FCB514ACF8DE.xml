<materialsCitation id="3B4E3CE9FFB194785BE3FB8B1409FBED" ID-GBIF-Occurrence="1316556645" box="[429,854,1113,1138]" collectionCode="MSNV" pageId="10" pageNumber="152" specimenCount="1" specimenCount-female="1" typeStatus="paratype">
