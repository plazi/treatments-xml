<subSubSection id="338B6FBA51323C3BE6BBFF044E426FE8" box="[199,1064,151,176]" pageId="30" pageNumber="181" type="etymology">
<paragraph id="7B2E3C3151323C3BE6BBFF044E426FE8" blockId="30.[151,1437,151,573]" box="[199,1064,151,176]" pageId="30" pageNumber="181">
<emphasis id="49E5E02351323C3BE6BBFF044B2C6FE8" bold="true" box="[199,326,151,176]" pageId="30" pageNumber="181">Etymology</emphasis>
