. 
<collectingCountry id="F364764AFFFCFFEB03A4FCEEB8C9FC5F" box="[408,471,872,898]" name="Malta" pageId="24" pageNumber="25">Male</collectingCountry>
, 
<collectingCountry id="F364764AFFFCFFEB03DEFCEEBBD6FC5F" box="[482,712,872,898]" name="Papua New Guinea" pageId="24" pageNumber="25">Papua New Guinea</collectingCountry>
, Madang Prov., Hapurpi village, nr. Halopa mission, 
<quantity id="4C8B9B3FFFFCFFEB070AFCEEBC9EFC5F" box="[1334,1408,872,898]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="24" pageNumber="25" unit="m" value="700.0">700 m</quantity>
