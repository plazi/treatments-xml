<taxonomicName id="4C4C4D0DFFF2FFC93761FECEC90DFEA9" box="[1125,1327,332,356]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="13" pageNumber="14" phylum="Porifera" rank="species" species="clathrodes">
