<paragraph id="AE0DE731FFDEFFCEC3F3E6F3FD930BCE" blockId="28.[151,557,1158,1184]" box="[151,557,1158,1184]" pageId="28" pageNumber="29">
<heading id="F545505DFFDEFFCEC3F3E6F3FD930BCE" bold="true" box="[151,557,1158,1184]" fontSize="11" level="1" pageId="28" pageNumber="29" reason="1">
<taxonomicName id="69B29CB2FFDEFFCEC3F3E6F3FD930BCE" authority="Calman 1907" authorityName="Calman" authorityYear="1907" box="[151,557,1158,1184]" class="Malacostraca" family="Bodotriidae" genus="Cyclaspis" kingdom="Animalia" order="Cumacea" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="thomsoni">
