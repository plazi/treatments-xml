<emphasis id="B916EAB01E3BFFEAFC19FCDCA43B827A" box="[987,1085,852,874]" italics="true" pageId="20" pageNumber="573">O. bidens</emphasis>
(FÖRSTER, 1853)
</taxonomicName>
, 
<taxonomicName id="4C624D211E3BFFEAFEB6FCFBA2C28297" authority="(FORSTER, 1853)" baseAuthorityName="FORSTER" baseAuthorityYear="1853" box="[372,708,881,903]" class="Insecta" family="Hymenoptera" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="573" phylum="Arthropoda" rank="species" species="constrictus">
<emphasis id="B916EAB01E3BFFEAFEB6FCFBA2018297" box="[372,519,882,903]" italics="true" pageId="20" pageNumber="573">O. constrictus</emphasis>
(FÖRSTER, 1853)
</taxonomicName>
, 
<taxonomicName id="4C624D211E3BFFEAFD11FCFBA4138297" authority="(MOCSARY, 1914)" baseAuthorityName="MOCSARY" baseAuthorityYear="1914" box="[723,1045,881,903]" class="Insecta" family="Hymenoptera" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="573" phylum="Arthropoda" rank="species" species="foveatus">
<emphasis id="B916EAB01E3BFFEAFD11FCFBA34D8297" box="[723,843,881,903]" italics="true" pageId="20" pageNumber="573">O. foveatus</emphasis>
(MOCSÁRY, 1914)
</taxonomicName>
, 
<taxonomicName id="4C624D211E3BFFEAFBE6FCFBA1C682B5" authority="(FABRICIUS, 1804)" baseAuthorityName="FABRICIUS" baseAuthorityYear="1804" class="Insecta" family="Hymenoptera" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="573" phylum="Arthropoda" rank="species" species="panzeri">
<emphasis id="B916EAB01E3BFFEAFBE6FCFBA0F882B5" italics="true" pageId="20" pageNumber="573">O. panzeri</emphasis>
(FABRICIUS, 1804)
</taxonomicName>
und 
<emphasis id="B916EAB01E3BFFEAFE30FC06A27882B5" box="[498,638,911,933]" italics="true" pageId="20" pageNumber="573">O. pyrosomus</emphasis>
(FÖRSTER, 1853).
</paragraph>
