<collectingRegion id="3F49F823FF800679FAA7FBA7FE9B317F" country="Colombia" name="Boyaca" pageId="9" pageNumber="76">Boyacá Department</collectingRegion>
of 
<collectingCountry id="859A7651FF800679FED7FB84FE3F317C" box="[367,483,1087,1113]" name="Colombia" pageId="9" pageNumber="76">Colombia</collectingCountry>
