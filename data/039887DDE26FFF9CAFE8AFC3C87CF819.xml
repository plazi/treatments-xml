<taxonomicNameLabel id="A27657A2E26FFF9CAA6AAEC6C909FDBD" box="[1301,1394,402,427]" pageId="5" pageNumber="6" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C314D48E26FFF9CAAFDAEC7CC96FDD8" class="Arachnida" family="Salticidae" genus="Corusca" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="gracilis" status="sp. nov.">
