<taxonomicName id="4C314D48E26FFF9CAAFDAE8FCD6BFE00" class="Arachnida" family="Salticidae" genus="Corusca" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="sanyaensis" status="sp. nov.">
