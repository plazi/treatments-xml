<taxonomicName id="F386FD528F29282DBC2DB5A2B814E065" LSID="FA7878F1-8BB1-5DB4-D75A-17F658325BC1" authority="L." authorityName="L." class="Magnoliopsida" family="Myrtaceae" genus="Psidium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Psidium guajava" order="Myrtales" pageId="126" pageNumber="127" phylum="Tracheophyta" rank="species" species="guajava">Psidium guajava L.</taxonomicName>
