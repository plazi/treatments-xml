<taxonomicName id="3CF1AD3EF50000D62C2EE2D70E30E00B" authorityName="Soula" authorityYear="2008" class="Insecta" family="Scarabaeidae" genus="Mesomerodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mesomerodon gilletti" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gilletti">
