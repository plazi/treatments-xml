<taxonomicName id="A7F015FEEF33770F5143AB0BDA166CA6" authorityName="Soula" authorityYear="2008" class="Insecta" family="Scarabaeidae" genus="Mesomerodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mesomerodon gilletti" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gilletti">
