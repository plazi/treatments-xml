<taxonomicName id="79C4A9F3027491080046F37293BE363F" authorityName="Soula" authorityYear="2008" class="Insecta" family="Scarabaeidae" genus="Mesomerodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mesomerodon gilletti" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gilletti">
