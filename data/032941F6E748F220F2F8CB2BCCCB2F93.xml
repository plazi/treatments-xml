<taxonomicName id="63AD3D61E7F8A52F9B7F7AD1692367A9" authorityName="Soula" authorityYear="2008" class="Insecta" family="Scarabaeidae" genus="Mesomerodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mesomerodon gilletti" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gilletti">
