<taxonomicName id="6FFD0DD9B27AE4A682255C88ADB91397" authorityName="Soula" authorityYear="2008" class="Insecta" family="Scarabaeidae" genus="Mesomerodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mesomerodon gilletti" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gilletti">
