<taxonomicName id="4C364D7DFFC4FFFA901F3200D8DB8858" baseAuthorityName="Leach" baseAuthorityYear="1815" box="[856,947,1286,1306]" class="Insecta" family="Braconidae" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="716" phylum="Arthropoda" rank="subFamily" subFamily="Alysiinae">Alysiinae</taxonomicName>
