117. 
<taxonomicName id="4C4A9AB999348546E8DEFA0ED656F662" authority="(Fallen, 1823) (Sciapodinae)" authorityName="Sciapodinae" baseAuthorityName="Fallen" baseAuthorityYear="1823" box="[206,775,1520,1548]" class="Insecta" family="Dolichopodidae" genus="Sciapus" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="wiedemanni">
