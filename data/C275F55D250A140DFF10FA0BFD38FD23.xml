<quantity id="8D24E9AE250D140DFF10FF06FEE6FF79" box="[151,307,190,216]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" metricValueMax="4.2" metricValueMin="4.0" pageId="4" pageNumber="59" unit="mm" value="0.41" valueMax="0.42" valueMin="0.4">0.4–0.42 mm</quantity>
