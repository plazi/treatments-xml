<subSubSection id="30B765142E0FFF926E0AFB9E331BEEF8" pageId="10" pageNumber="11" type="diagnosis">
<paragraph id="7812369F2E0FFF926E0AFB9E331BEEF8" blockId="10.[264,1323,1094,1401]" pageId="10" pageNumber="11">
<emphasis id="4AD9EA8D2E0FFF926E0AFB9E30DCEE41" bold="true" box="[264,392,1094,1120]" pageId="10" pageNumber="11">Diagnosis.</emphasis>
<taxonomicName id="BFAD4D1C2E0FFF926E96FB9F3324EE41" box="[404,624,1095,1120]" class="Arachnida" family="Selenopidae" genus="Selenops" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="camerun">
