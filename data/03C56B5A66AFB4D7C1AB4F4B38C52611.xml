<taxonomicName id="4C6CA1CF66AFB4D7C58D4DAF3D0427A9" box="[1201,1310,1759,1784]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="250" pageNumber="251" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
