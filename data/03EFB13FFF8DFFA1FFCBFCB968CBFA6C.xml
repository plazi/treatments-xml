<taxonomicName id="4C467BAAFF8DFFA1FCC9FA086995FA00" authorityName="Grazia-Vieira" authorityYear="1973" box="[864,977,1434,1461]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="rolstoni">
