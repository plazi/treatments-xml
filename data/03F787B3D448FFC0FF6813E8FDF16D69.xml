. 
<typeStatus id="54E58807D448FFC0FE061391FDD468C4" box="[482,537,484,510]" pageId="4" pageNumber="5">Type</typeStatus>
species 
<treatmentCitation id="0AFF10B4D448FFC0FD9F1396FBE068C4" author="Busck A" box="[635,1069,483,510]" page="98" pageId="4" pageNumber="5" year="1907">
