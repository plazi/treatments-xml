<taxonomicName id="268E4D51FFC23B4CFDC8F97DA46A873C" authority="Braet, 2016" authorityName="Braet" authorityYear="2016" box="[603,838,1773,1793]" class="Insecta" family="Braconidae" genus="Austroascogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="444" phylum="Arthropoda" rank="species" species="wilhelmensis" status="n.sp.">
<emphasis id="D3FAEAC0FFC23B4CFDC8F97DA46A873C" box="[603,838,1773,1793]" italics="true" pageId="37" pageNumber="444">Austroascogaster wilhelmensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="C8C957BBFFC23B4CFCD9F97DA459873C" box="[842,885,1773,1793]" pageId="37" pageNumber="444" rank="species">n.sp.</taxonomicNameLabel>
, 
<typeStatus id="3E358870FFC23B4CFCEAF97DA4EF873C" box="[889,963,1773,1793]" pageId="37" pageNumber="444">holotype</typeStatus>
