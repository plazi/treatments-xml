<subSubSection id="A9946559FFC33B4DFF3DFA8FA57A8564" pageId="36" pageNumber="443" type="diagnosis">
<paragraph id="E13136D2FFC33B4DFF3DFA8FA57A8564" blockId="36.[174,1406,1310,1369]" pageId="36" pageNumber="443">
<emphasis id="D3FAEAC0FFC33B4DFF3DFA8FA60E850B" bold="true" box="[174,290,1311,1334]" pageId="36" pageNumber="443">DIAGNOSIS</emphasis>
