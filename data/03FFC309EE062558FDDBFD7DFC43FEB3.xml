<taxonomicName id="4C56099CEE06255EFDDBFD7DFC72FD6B" authority="HANSSON &amp; LASALLE, 2003" authorityName="HANSSON &amp; LASALLE" authorityYear="2003" box="[573,937,674,698]" class="Insecta" family="Eulophidae" genus="Entedononecremnus" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="771" phylum="Arthropoda" rank="species" species="unicarinatus" status="sp. n.">
<emphasis id="B922AE0DEE06255EFDDBFD7DFC72FD6B" bold="true" box="[573,937,674,698]" italics="true" pageId="75" pageNumber="771">Entedononecremnus unicarinatus</emphasis>
</taxonomicName>
