<paragraph id="8BF03690FFD8FF91F5A3CCEFDD003999" blockId="24.[151,1437,327,712]" pageId="24" pageNumber="65">
<materialsCitation id="3B273CCDFFD8FF91F5A3CCEFDD003999" ID-GBIF-Occurrence="1314597110" collectorName="A Mediterranean" country="Ukraine" county="southern Arabian Peninsula" location="western Iran. Living" municipality="northern Iran" pageId="24" pageNumber="65" specimenCount="1" stateProvince="Ben">
<emphasis id="B93BEA82FFD8FF91F5A3CCEFDB6A3AA1" bold="true" box="[199,628,471,496]" pageId="24" pageNumber="65">General distribution and habitats.</emphasis>
<collectorName id="26BA5346FFD8FF91F7E6CCE0DA593AA1" box="[642,839,471,496]" pageId="24" pageNumber="65">A Mediterranean</collectorName>
