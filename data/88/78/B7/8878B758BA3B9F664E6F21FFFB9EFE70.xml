, 
<collectingCountry box="[189,298,1150,1176]" name="Germany" pageId="77" pageNumber="78">Germany</collectingCountry>
(
<specimenCode box="[314,499,1150,1176]" pageId="77" pageNumber="78">SMF XXX 873</specimenCode>
, including the 
<typeStatus box="[672,778,1150,1176]" pageId="77" pageNumber="78">holotype</typeStatus>
, and 
<specimenCode box="[839,1023,1150,1176]" pageId="77" pageNumber="78">SMF XXX 874</specimenCode>
, including the 
<typeStatus box="[1196,1303,1150,1176]" pageId="77" pageNumber="78">paratype</typeStatus>
).
