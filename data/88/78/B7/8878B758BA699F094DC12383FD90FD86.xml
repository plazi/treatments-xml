, 
<location LSID="urn:lsid:plazi:treatment:8878B758BA699F094DC12383FD90FD86:050E5095BA569F084ECD2341FCE9FCA5" box="[670,784,844,870]" country="United States of America" county="Seneca County" municipality="It" name="Frankfurt" pageId="32" pageNumber="33" stateProvince="New York">Frankfurt</location>
, 
