<quantity box="[1315,1398,774,800]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="32" pageNumber="33" unit="km" value="0.2">
<locationDeviation box="[1315,1398,774,800]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="32" pageNumber="33" unit="km" value="0.2">0.2 km</locationDeviation>
