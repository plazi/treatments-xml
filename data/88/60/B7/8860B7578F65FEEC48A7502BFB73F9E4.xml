<taxonomicName authority="(Olmi)" baseAuthorityName="Olmi" box="[1243,1436,1261,1282]" class="Insecta" family="Dryinidae" genus="Aphelopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="300" pageNumber="301" phylum="Arthropoda" rank="species" species="orientalis">
