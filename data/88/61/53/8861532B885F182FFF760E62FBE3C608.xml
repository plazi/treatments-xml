<taxonomicName baseAuthorityName="Fabricius" baseAuthorityYear="1781" box="[324,575,225,251]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="furcatus">
