<specimenCount box="[428,448,1634,1657]" pageId="17" pageNumber="67" type="female">♀</specimenCount>
<quantity box="[455,540,1634,1657]" metricMagnitude="-3" metricUnit="m" metricValue="5.7" pageId="17" pageNumber="67" unit="mm" value="5.7">5.7 mm</quantity>
; length of thorax, 
<specimenCount box="[734,754,1634,1657]" pageId="17" pageNumber="67" type="male">♂</specimenCount>
<quantity metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="17" pageNumber="67" unit="mm" value="2.6">2.6 mm</quantity>
