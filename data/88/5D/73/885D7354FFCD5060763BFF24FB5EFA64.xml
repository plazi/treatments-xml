</paragraph>
<paragraph blockId="19.[113,646,417,566]" pageId="19" pageNumber="108">
<materialsCitation collectorName="Rakotonasolo &amp; Ravelonarivo" country="Madagascar" elevation="976" latitude="-14.751389" location="Anjanaharibe-Sud Reserve" longLatPrecision="21" longitude="49.495834" pageId="19" pageNumber="108" specimenCount="1" stateProvince="Antsiranana">
