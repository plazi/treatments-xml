<subSubSection pageId="null" pageNumber="478" type="nomenclature">
<paragraph pageId="null" pageNumber="478">
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="478" phylum="Tracheophyta" rank="genus">Gnaphalium</taxonomicName>
