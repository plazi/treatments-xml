<location LSID="urn:lsid:plazi:treatment:886CAA78FFE8FFDCFD6C0805FB8CFCD0:051A4DB5FFF7FFDCFD7A0B26FCECFEAF" box="[753,818,343,370]" country="Madagascar" county="Massif d'Andingitra" latitude="-37.866665" longLatPrecision="1178" longitude="77.583336" municipality="Eastern Cape. The" name="Africa" pageId="18" pageNumber="25">Africa</location>
