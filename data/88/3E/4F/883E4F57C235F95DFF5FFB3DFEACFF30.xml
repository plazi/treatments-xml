<subSubSection box="[199,1114,1842,1867]" pageId="3" pageNumber="432" type="etymology">
<paragraph blockId="3.[151,1436,1266,2011]" box="[199,1114,1842,1867]" pageId="3" pageNumber="432">
<emphasis bold="true" box="[199,326,1842,1867]" pageId="3" pageNumber="432">Etymology</emphasis>
