<date box="[580,706,1191,1216]" pageId="8" pageNumber="277" value="2005" valueMax="2009">
<number box="[580,706,1191,1216]" pageId="8" pageNumber="277" value="2007.0" valueMax="2009.0" valueMin="2005.0">2005â€“2009</number>
</date>
