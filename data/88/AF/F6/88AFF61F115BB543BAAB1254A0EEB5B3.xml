<paragraph pageId="34" pageNumber="35">
<taxonomicName class="Insecta" family="Tenthredinidae" genus="Nematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nematus albilabris" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="albilabris">Nematus albilabris</taxonomicName>
