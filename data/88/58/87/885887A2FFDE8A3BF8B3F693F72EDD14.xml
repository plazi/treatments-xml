<emphasis bold="true" box="[2956,3096,4322,4361]" pageId="17" pageNumber="296">
<collectingCountry box="[2956,3091,4322,4361]" name="Yemen" pageId="17" pageNumber="296">Yemen</collectingCountry>
.
</emphasis>
</paragraph>
<paragraph blockId="17.[1916,3539,3163,4627]" pageId="17" pageNumber="296">
<taxonomicName box="[1921,2586,4377,4416]" pageId="17" pageNumber="296">
<emphasis box="[1921,2225,4377,4416]" italics="true" pageId="17" pageNumber="296">
