<subSubSection pageId="49" pageNumber="328" type="discussion">
<paragraph blockId="49.[1012,1817,2383,2951]" pageId="49" pageNumber="328">
<taxonomicName authorityName="K. Andersen" authorityYear="1905" box="[1019,1383,2489,2528]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="49" pageNumber="328" phylum="Chordata" rank="species" species="sedulus">
<emphasis box="[1019,1383,2489,2528]" italics="true" pageId="49" pageNumber="328">Rhinolophus sedulus</emphasis>
