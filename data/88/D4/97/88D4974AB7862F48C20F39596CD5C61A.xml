<taxonomicName authorityName="Troscherl" authorityYear="1847" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="171" pageNumber="172" phylum="Mollusca" rank="genus">
