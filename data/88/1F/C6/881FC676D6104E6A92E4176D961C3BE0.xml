<subSubSection pageId="null" pageNumber="923" type="vernacular_name">
<paragraph pageId="null" pageNumber="923">
Graufilzige 
<normalizedToken originalValue="Schlüsselblume" pageId="null" pageNumber="923">Schluesselblume</normalizedToken>
</paragraph>
</subSubSection>
