<paragraph id="2E33551D4F1630FADF24EA6F8BA8C561" pageId="0" pageNumber="77">
<taxonomicName id="DFB0EA7F8D2BC11D60B4707212136533" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon franklinpaniaguai" order="Hymenoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="franklinpaniaguai">
