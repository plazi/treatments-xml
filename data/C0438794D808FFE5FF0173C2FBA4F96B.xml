. 
<taxonomicName id="8FEA4D01D808FFE5FDBE70F7FD0EF913" baseAuthorityName="Broun" baseAuthorityYear="1881" box="[552,682,1791,1817]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="ignotus" subGenus="Byraxis">
