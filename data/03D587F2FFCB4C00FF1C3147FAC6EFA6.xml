<paragraph id="8BC336E4FFCB4C00FC54300AF93EFA30" blockId="4.[1004,1799,1299,1865]" pageId="4" pageNumber="353">
<taxonomicName id="4C7C4D67FFCB4C00FC54300AFA7AFA4D" authorityName="Hardwicke" authorityYear="1825" box="[1009,1410,1405,1444]" class="Mammalia" family="Emballonuridae" genus="Taphozous" kingdom="Animalia" order="Chiroptera" pageId="4" pageNumber="353" phylum="Chordata" rank="species" species="longimanus">
