<taxonomicName id="1A8F4D75FFA80D024D16CE0BFCCCF8C5" box="[636,802,1880,1905]" class="Catenulida" family="Stenostomidae" genus="Stenostomum" kingdom="Animalia" order="Gentianales" pageId="14" pageNumber="15" phylum="Platyhelminthes" rank="species" species="constrictum">
