<geoCoordinate id="EE60506BFF712EB66122FACBD7C3CFBC" box="[1354,1420,1394,1419]" degrees="61" direction="west" orientation="longitude" pageId="157" pageNumber="158" precision="55555" value="-61.0">61°W</geoCoordinate>
