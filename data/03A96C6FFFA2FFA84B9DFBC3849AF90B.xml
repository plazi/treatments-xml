sp. near 
<taxonomicName id="4C00A6FAFFA2FFA84977FB1287F5FAD6" authorityName="Burckhardt &amp; Sharma &amp; Raman" authorityYear="2018" baseAuthorityName="Crawford" baseAuthorityYear="1912" box="[637,710,1262,1287]" class="Insecta" family="Psyllidae" genus="Psylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="simlae">
