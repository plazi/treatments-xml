<taxonomicName id="4C00A6FAFFA2FFA84AD8FB12872BFAD6" box="[466,536,1262,1287]" class="Insecta" family="Psyllidae" genus="Psylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
