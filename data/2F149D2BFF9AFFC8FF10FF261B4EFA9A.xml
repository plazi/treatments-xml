<paragraph id="A7022C3DFF9DFFC8FF40FB421B4EFA9A" blockId="20.[151,1436,151,1292]" box="[199,1045,1267,1292]" pageId="20" pageNumber="21">
<emphasis id="95C9F02FFF9DFFC8FF40FB421E3BFA9A" bold="true" box="[199,352,1267,1292]" pageId="20" pageNumber="21">Distribution.</emphasis>
South-western Pacific, Kermadec Trench, 
<number id="11753E1AFF9DFFC8FCC5FB451C20FA9A" box="[834,891,1268,1292]" pageId="20" pageNumber="21" value="5340.0">5340</number>
- 
<number id="11753E1AFF9DFFC8FC05FB451CE0FA9A" box="[898,955,1268,1292]" pageId="20" pageNumber="21" value="5230.0">5230</number>
