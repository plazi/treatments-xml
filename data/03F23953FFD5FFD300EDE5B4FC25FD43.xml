<collectingRegion id="499F46A7FFD6FFD303A3E594FBF9FD73" box="[1023,1081,698,718]" country="Belarus" name="Minsk" pageId="66" pageNumber="1079">Minsk</collectingRegion>
, München). - 1, 
<collectingCountry id="F34CC8D5FFD6FFD301DDE5FCFE3BFD5B" box="[385,507,722,742]" name="Belarus" pageId="66" pageNumber="1079">Weissrussland</collectingCountry>
