<collectionCode id="ED39AE28FFF83A73DB83BF8EFCAB08F3" LSID="urn:lsid:biocol.org:col:32981" box="[737,812,907,931]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="8" pageNumber="245">ANIC</collectionCode>
