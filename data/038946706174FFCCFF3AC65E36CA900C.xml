<taxonomicName id="4C208CE56175FFCCFB59C1FD316393E1" box="[1268,1365,476,501]" class="Insecta" family="Thripidae" genus="Hydatothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="sternalis">
