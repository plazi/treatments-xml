<taxonomicName id="02E44D7BFFA3FFDF79F7FC71FDF63499" authority="McClelland, 1838" authorityName="McClelland" authorityYear="1838" box="[467,637,979,1002]" class="Actinopterygii" family="Cyprinidae" genus="Gonorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="18" pageNumber="489" phylum="Chordata" rank="species" species="periyarensis">
