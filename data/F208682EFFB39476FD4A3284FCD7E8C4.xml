<taxonomicName id="BDA1A2BBFFBE9476FC3E340AFB65EB3A" box="[958,1172,1387,1404]" class="Polypodiopsida" family="Dryopteridaceae" genus="Polystichum" kingdom="Plantae" order="Polypodiales" pageId="10" pageNumber="275" phylum="Tracheophyta" rank="species" species="crinulosum">
<emphasis id="48D5052AFFBE9476FC3E340AFB65EB3A" bold="true" box="[958,1172,1387,1404]" italics="true" pageId="10" pageNumber="275">Polystichum crinulosum</emphasis>
