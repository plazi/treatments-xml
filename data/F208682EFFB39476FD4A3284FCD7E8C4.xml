<taxonomicName id="BDA1A2BBFFBE9476FE533434FD76EB20" box="[467,647,1365,1382]" class="Polypodiopsida" family="Dryopteridaceae" genus="Polystichum" kingdom="Plantae" order="Polypodiales" pageId="10" pageNumber="275" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="48D5052AFFBE9476FE533434FDC8EB20" box="[467,569,1365,1382]" italics="true" pageId="10" pageNumber="275">Polystichum</emphasis>
