np, 
<number box="[515,530,832,858]" pageId="33" pageNumber="34" value="2.0">2</number>
sa. 
<emphasis box="[576,644,832,857]" italics="true" pageId="33" pageNumber="34">Wing.</emphasis>
Length 
<quantity box="[742,835,832,858]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="33" pageNumber="34" unit="mm" value="5.3">5.3 mm</quantity>
