<number box="[1340,1385,912,938]" pageId="33" pageNumber="34" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
and 
<number box="[151,195,952,978]" pageId="33" pageNumber="34" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
