<emphasis box="[161,192,1256,1281]" italics="true" pageId="1" pageNumber="63">
<number box="[161,176,1256,1281]" pageId="1" pageNumber="63" value="2.0">2</number>
a
</emphasis>
) 
<number box="[211,242,1256,1282]" pageId="1" pageNumber="63" value="12.0">12</number>
(
<number box="[260,339,1256,1282]" pageId="1" pageNumber="63" value="13.0" valueMax="15.0" valueMin="11.0">11–15</number>
) long, 
<number box="[429,460,1256,1282]" pageId="1" pageNumber="63" value="17.0">17</number>
(
<number box="[477,557,1256,1282]" pageId="1" pageNumber="63" value="18.5" valueMax="20.0" valueMin="17.0">17–20</number>
) apart. 
<emphasis bold="true" box="[654,745,1255,1281]" pageId="1" pageNumber="63">Legs—</emphasis>
Segments normal. Leg 
<date box="[1036,1086,1256,1282]" pageId="1" pageNumber="63" value="1928-01">
