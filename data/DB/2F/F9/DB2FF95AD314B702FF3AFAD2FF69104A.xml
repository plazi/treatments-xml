. 
<taxonomicName box="[352,427,413,438]" class="Arachnida" family="Diptilomiopidae" genus="Trimeroptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="67" phylum="Arthropoda" rank="species" species="kakius">
