<quantity box="[659,797,493,517]" metricMagnitude="2" metricUnit="m" metricValue="2.6399999999999997" metricValueMax="2.67" metricValueMin="2.61" pageId="14" pageNumber="807" unit="m" value="264.0" valueMax="267.0" valueMin="261.0">261–267 m</quantity>
