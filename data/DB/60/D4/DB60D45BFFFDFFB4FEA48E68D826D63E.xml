high (
<collectionCode box="[869,958,1499,1523]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="805" type="Museum">USNM</collectionCode>
