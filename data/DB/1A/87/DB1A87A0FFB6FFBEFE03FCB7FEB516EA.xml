<quantity box="[423,556,185,209]" metricMagnitude="2" metricUnit="m" metricValue="2.335" metricValueMax="2.9" metricValueMin="1.77" pageId="10" pageNumber="2859" unit="m" value="233.5" valueMax="290.0" valueMin="177.0">177–290 m</quantity>
