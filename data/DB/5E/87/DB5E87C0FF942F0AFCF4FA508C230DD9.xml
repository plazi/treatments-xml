<subSubSection lastPageId="14" lastPageNumber="315" pageId="12" pageNumber="313" type="etymology">
<paragraph blockId="12.[151,1436,151,429]" box="[199,1208,404,429]" pageId="12" pageNumber="313">
<emphasis bold="true" box="[199,334,404,429]" pageId="12" pageNumber="313">Etymology.</emphasis>
The name 
<taxonomicName box="[459,557,404,428]" class="Reptilia" family="Liolaemidae" genus="Liolaemus" kingdom="Animalia" order="Squamata" pageId="12" pageNumber="313" phylum="Chordata" rank="species" species="chilensis">
