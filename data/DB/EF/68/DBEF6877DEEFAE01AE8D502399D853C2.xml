<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180347" class="Insecta" family="Formicidae" genus="Messor" kingdom="Animalia" lsidName="Messor barbarus capensis var. tropicorum Forel" lsidName-HNS="Messor barbarus capensis var. tropicorum Forel" order="Hymenoptera" pageNumber="444" phylum="Arthropoda" rank="variety" species="barbarus" subSpecies="capensis" variety="tropicorum">Messor barbarus L., subsp, capensis Mayr., var. tropicorum</taxonomicName>
<taxonomicNameLabel rank="variety">n. var.</taxonomicNameLabel>
