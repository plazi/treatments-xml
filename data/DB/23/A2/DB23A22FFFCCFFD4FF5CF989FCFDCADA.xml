<taxonomicName authority="(Thomson, 1895)" authorityName="Thomson" authorityYear="1895" baseAuthorityName="Thomson" baseAuthorityYear="1895" box="[1111,1432,476,501]" class="Insecta" family="Braconidae" genus="Choeras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="458" phylum="Arthropoda" rank="species" species="validus">
