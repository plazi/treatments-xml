<subSubSection box="[151,572,1454,1478]" pageId="0" pageNumber="65" type="nomenclature">
<paragraph blockId="0.[151,572,1454,1478]" box="[151,572,1454,1478]" pageId="0" pageNumber="65">
<heading bold="true" box="[151,572,1454,1478]" fontSize="10" level="2" pageId="0" pageNumber="65" reason="2">
<emphasis bold="true" box="[151,572,1454,1478]" pageId="0" pageNumber="65">
<taxonomicName box="[151,323,1454,1478]" class="Insecta" family="Chrysomelidae" genus="Australotymnes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="65" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[151,323,1454,1478]" italics="true" pageId="0" pageNumber="65">Australotymnes</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[329,447,1454,1478]" pageId="0" pageNumber="65" rank="genus">new genus</taxonomicNameLabel>
(
<figureCitation box="[461,564,1454,1478]" captionStart-0="FIGURES 1 – 3" captionStart-1="FIGURES 4 – 8" captionStartId-0="1.[151,269,1564,1588]" captionStartId-1="2.[151,273,1034,1058]" captionTargetBox-0="[255,1320,964,1537]" captionTargetBox-1="[246,1392,204,966]" captionTargetId-0="figure@1.[247,1340,949,1538]" captionTargetId-1="figure@2.[194,1392,194,1030]" captionTargetPageId-0="1" captionTargetPageId-1="2" captionText-0="FIGURES 1 – 3. Australotymnes jipijapa 1, dorsal view; 2. lateral view; 3, view of apices of elytra." captionText-1="FIGURES 4 – 8. Australotymnes jipijapa 4, median lobe; 5, apical sclerite; 6, apex of median lobe; 7, ovipositor; 8, spermatheca." httpUri-0="https://zenodo.org/record/188403/files/figure.png" httpUri-1="https://zenodo.org/record/188404/files/figure.png" pageId="0" pageNumber="65">
Figs. 
<number box="[523,564,1454,1478]" pageId="0" pageNumber="65" value="4.5" valueMax="8.0" valueMin="1.0">1–8</number>
</figureCitation>
)
</emphasis>
</heading>
</paragraph>
