<taxonomicName authority="Mountains" authorityName="Mountains" box="[947,1150,1451,1477]" class="Insecta" family="Staphylinidae" genus="Dayao" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="580" phylum="Arthropoda" rank="genus">Dayao Mountains</taxonomicName>
