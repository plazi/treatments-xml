<number box="[151,167,1184,1207]" pageId="20" pageNumber="21" value="3.0">3</number>
. Western southern Africa; 
<number box="[474,501,1184,1207]" pageId="20" pageNumber="21" value="17.0">17</number>
midbody dorsal scale rows, 
<number box="[806,833,1184,1207]" pageId="20" pageNumber="21" value="15.0">15</number>
