<taxonomicName class="Sordariomycetes" family="Diatrypaceae" genus="Eutypa" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Eutypa" order="Xylariales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="genus">
