<subSubSection pageId="29" pageNumber="30" type="etymology">
<paragraph blockId="29.[264,1323,724,830]" box="[264,389,724,749]" pageId="29" pageNumber="30">
<emphasis box="[264,389,724,749]" italics="true" pageId="29" pageNumber="30">Etymology</emphasis>
