<paragraph pageId="140" pageNumber="140">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24864" class="Insecta" family="Formicidae" genus="Myrmicaria" kingdom="Animalia" lsidName-HNS="Myrmicaria Saunders" order="Hymenoptera" pageId="140" pageNumber="140" phylum="Arthropoda" rank="genus">Myrmicaria</taxonomicName>
