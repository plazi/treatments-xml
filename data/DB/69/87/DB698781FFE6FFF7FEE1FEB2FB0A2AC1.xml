<subSubSection box="[268,639,342,369]" pageId="16" pageNumber="251" type="nomenclature">
<paragraph blockId="16.[268,639,342,369]" box="[268,639,342,369]" pageId="16" pageNumber="251">
<heading box="[268,639,342,369]" centered="true" fontSize="11" level="2" pageId="16" pageNumber="251" reason="2">
<taxonomicName authority="Pomel, 1892" authorityName="Pomel" authorityYear="1892" box="[268,639,342,369]" class="Mammalia" family="Cricetidae" genus="Ellobius" isUncertain="true" kingdom="Animalia" order="Rodentia" pageId="16" pageNumber="251" phylum="Chordata" rank="species" species="barbarus">
<emphasis bold="true" box="[268,356,342,369]" italics="true" pageId="16" pageNumber="251">Ellobius</emphasis>
cf. 
<emphasis bold="true" box="[396,493,342,369]" italics="true" pageId="16" pageNumber="251">barbarus</emphasis>
Pomel, 1892
</taxonomicName>
</heading>
</paragraph>
