<taxonomicName authority="Wagler, 1832" authorityName="Wagler" authorityYear="1832" box="[360,621,1873,1900]" class="Mammalia" family="Soricidae" genus="Crocidura" kingdom="Animalia" order="Soricomorpha" pageId="18" pageNumber="253" phylum="Chordata" rank="genus">
