(A, B) and 
<emphasis id="B95BD62EED69FF98FAE6F9BDFEF1F940" bold="true" pageId="5" pageNumber="728">
<taxonomicName id="4C2F71BFED69FF98FAE6F9BDFF50F940" authority="Jr &amp; Ricci &amp; Melone &amp; Fontaneto, 2011" authorityName="Jr &amp; Ricci &amp; Melone &amp; Fontaneto" authorityYear="2011" class="Eurotatoria" family="Philodinavidae" genus="Abrochtha" kingdom="Animalia" order="Bdelloidea" pageId="5" pageNumber="728" phylum="Rotifera" rank="species" species="kingi" status="sp. nov.">
<emphasis id="B95BD62EED69FF98FAE6F9BDFF50F940" bold="true" italics="true" pageId="5" pageNumber="728">Abrochtha kingi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2686B55ED69FF98FF2AF99FFEF1F940" box="[234,323,1645,1664]" pageId="5" pageNumber="728" rank="species">sp. nov.</taxonomicNameLabel>
