<taxonomicName id="7784F85F1F828382920A33A9B0D3C3E1" baseAuthorityName="Reinhardt" baseAuthorityYear="1877" class="Gastropoda" family="Alycaeidae" genus="Chamalycaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chamalycaeus nipponensis" order="Architaenioglossa" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="nipponensis">
