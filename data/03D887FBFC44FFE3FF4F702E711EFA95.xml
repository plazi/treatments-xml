<collectingCountry id="F366767DFC45FFE3FC80757B70C5FDDA" box="[856,931,512,536]" name="Republic of the Congo" pageId="19" pageNumber="420">Congo</collectingCountry>
(Werner 
<number id="3DB924CAFC45FFE3FBD47684772FFDDA" box="[1036,1097,511,536]" pageId="19" pageNumber="420" value="2000.0">2000</number>
