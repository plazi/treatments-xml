<number id="7D216D41FFFD045C710CFDBFFDEEFD93" box="[553,562,583,607]" pageId="5" pageNumber="33" value="1.0">1</number>
LW 
<number id="7D216D41FFFD045C7142FDBFFD47FD93" box="[615,667,583,607]" pageId="5" pageNumber="33" value="3.48">3.48</number>
, T 
<number id="7D216D41FFFD045C719DFDBFFD1DFD93" box="[696,705,583,607]" pageId="5" pageNumber="33" value="1.0">1</number>
WW 
