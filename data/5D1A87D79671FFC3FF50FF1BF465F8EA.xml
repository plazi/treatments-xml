<emphasis id="E7C7EAD39671FFC2FF00FDA2F638FD83" bold="true" box="[199,357,545,570]" pageId="6" pageNumber="81">
<typeStatus id="0A0888639671FFC2FF00FDA2F65FFD83" box="[199,258,545,570]" pageId="6" pageNumber="81">Type</typeStatus>
locality.
</emphasis>
<location id="D06C601A9671FFC2FEABFDA2F54AFD83" LSID="urn:lsid:plazi:treatment:5D1A87D79671FFC3FF50FF1BF465F8EA:D06C601A9671FFC2FEABFDA2F54AFD83" box="[364,535,545,570]" name="Sullivan Island" pageId="6" pageNumber="81" stateProvince="South Carolina">Sullivan Island</location>
, 
<collectingRegion id="1777F8239671FFC2FDE4FDA2F591FD83" box="[547,716,545,570]" country="United States of America" name="South Carolina" pageId="6" pageNumber="81">South Carolina</collectingRegion>
(
<bibRefCitation id="B1224B309671FFC2FD1DFDA2F4D3FD83" author="Pourtales, L. F." box="[730,910,545,570]" pageId="6" pageNumber="81" pagination="8 - 16" refId="ref5162" refString="Pourtales, L. F. (1851) On the Holothuriae of the Atlantic Coast of the United States. Proceedings American Association Advancement Science, 1851, 8 - 16." type="journal article" year="1851">Pourtalès, 1851</bibRefCitation>
