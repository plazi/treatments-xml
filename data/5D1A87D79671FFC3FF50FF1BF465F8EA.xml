<taxonomicName id="12B34D429670FFC3FEDDFABAF5C5FAF6" authority="(Pourtales, 1851)" authorityName="Pourtales" authorityYear="1851" baseAuthorityName="Pourtales" baseAuthorityYear="1851" box="[282,664,1337,1359]" class="Holothuroidea" family="Cucumariidae" genus="Thyonella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="82" phylum="Echinodermata" rank="species" species="gemmata">
