<subSubSection id="48D7DDBAB08795B27ABDC5E4261AB298" pageId="2" pageNumber="27" type="type material">
<paragraph id="946DDF72991FAA62E8596FCC7C0EAFDC" pageId="2" pageNumber="27">Type material.</paragraph>
<paragraph id="A44274265309652423E5B348CE97BF75" pageId="2" pageNumber="27">
