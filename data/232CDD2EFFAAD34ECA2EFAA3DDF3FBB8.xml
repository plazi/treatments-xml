<heading id="F072DB54FFAAD341CA2EFAA3DFA1FAEA" box="[364,982,1302,1326]" centered="true" fontSize="10" level="2" pageId="5" pageNumber="1690" reason="4">
<taxonomicName id="6C8517BBFFAAD341CA2EFAA3DFA1FAEA" authority="anatoliae (Leidenfrost, 1912)" authorityName="anatoliae (Leidenfrost" authorityYear="1912" baseAuthorityName="Leidenfrost" baseAuthorityYear="1912" box="[364,982,1302,1326]" class="Actinopterygii" family="Cyprinodontidae" genus="Aphanius" kingdom="Animalia" order="Cyprinodontiformes" pageId="5" pageNumber="1690" phylum="Chordata" rank="subSpecies" species="anatoliae" subSpecies="anatoliae">
<emphasis id="99F1B02AFFAAD341CA2EFAA3DE9EFAEA" bold="true" box="[364,745,1302,1326]" italics="true" pageId="5" pageNumber="1690">Aphanius anatoliae anatoliae</emphasis>
(Leidenfrost, 1912)
</taxonomicName>
