<paragraph id="8BDB1C73575E3C02FF55F9D6BBD1AE44" blockId="38.[151,1437,1492,2021]" pageId="38" pageNumber="139">
<emphasis id="B910C061575E3C02FF55F9D6BBC1AF6C" box="[199,255,1636,1661]" italics="true" pageId="38" pageNumber="139">Shell</emphasis>
. Minute (
<quantity id="4C9CB196575E3C02FEE7F9D6BAEEAF6D" box="[373,464,1636,1661]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="38" pageNumber="139" unit="mm" value="1.5">1.5 mm</quantity>
