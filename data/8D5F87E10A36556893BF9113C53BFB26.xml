<taxonomicName id="C2F64D740A36556993BF9113C289F8D0" authority="Tikyani &amp; Khera, 1970" authorityName="Tikyani &amp; Khera" authorityYear="1970" box="[151,721,1816,1842]" class="Secernentea" family="Tylenchidae" genus="Telotylenchus" kingdom="Animalia" order="Tylenchida" pageId="12" pageNumber="583" phylum="Nematoda" rank="species" species="paaloofi">
