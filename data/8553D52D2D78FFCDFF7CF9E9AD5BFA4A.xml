; width, 
<quantity id="CA02C9DE2D78FFCDFC56FEBDAA21FE07" box="[999,1134,374,401]" metricMagnitude="-3" metricUnit="m" metricValue="6.7" metricValueMax="7.2" metricValueMin="6.2" pageId="35" pageNumber="82" unit="mm" value="6.7" valueMax="7.2" valueMin="6.2">6.2-7.2 mm</quantity>
; thickness, 
<quantity id="CA02C9DE2D78FFCDFD6FFE5EAD2BFE26" box="[734,868,405,432]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="5.4" metricValueMin="2.6" pageId="35" pageNumber="82" unit="mm" value="4.0" valueMax="5.4" valueMin="2.6">2.6-5.4 mm</quantity>
.
</paragraph>
