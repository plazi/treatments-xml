</taxonomicName>
from 
<collectingCountry id="F3396EEBFFF4FF92FECC154BFEE1AE57" box="[283,378,259,283]" name="Thailand" pageId="2" pageNumber="397">Thailand</collectingCountry>
and 
<taxonomicName id="4C2E55F8FFF4FF92FE61154CFD03AE57" authority="Assing" authorityName="Assing" authorityYear="2013" box="[438,664,259,283]" class="Insecta" family="Staphylinidae" genus="Pseudolathra" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="397" phylum="Arthropoda" rank="species" species="separanda">
<emphasis id="B95AF269FFF4FF92FE61154CFDDEAE57" box="[438,581,259,283]" italics="true" pageId="2" pageNumber="397">P. separanda</emphasis>
