<taxonomicName id="4C644D203007C046FDDAFBF6D73EFABE" box="[622,776,1136,1157]" class="Insecta" family="Chironomidae" genus="Kiefferulus" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="241" phylum="Arthropoda" rank="species" species="barbatitarsis">K. barbatitarsis</taxonomicName>
</emphasis>
