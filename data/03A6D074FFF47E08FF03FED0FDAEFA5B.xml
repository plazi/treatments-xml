21.313- 
<collectionCode id="ED1EF9A7FFF47E08FE69FEB7FE21FE16" box="[388,439,368,392]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="27" pageNumber="720">SMF</collectionCode>
