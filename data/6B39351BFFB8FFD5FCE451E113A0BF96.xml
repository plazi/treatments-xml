<paragraph id="E32F840DFFBFFFD4FF3057C417C5BE9B" blockId="4.[163,779,197,407]" pageId="4">
<materialsCitation id="53F88E50FFBFFFD4FF3057C417C5BE9B" collectionCode="IGR" country="France" county="University of Rennes" location="Chalcidoidea" municipality="Museum" pageId="4" specimenCount="1" typeStatus="holotype">
