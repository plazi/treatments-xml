<taxonomicName id="4C5D4D66FF9BFFB3FF478EBFC6A0EADA" authority="Signoret, 1858" authorityName="Signoret" authorityYear="1858" box="[151,640,681,707]" class="Insecta" family="Scutelleridae" genus="Anoplogonius" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="9" pageNumber="110" phylum="Arthropoda" rank="species" species="nigricollis">
<emphasis id="B929EAF7FF9BFFB3FF478EBFC59AEADB" box="[151,442,681,706]" italics="true" pageId="9" pageNumber="110">Anoplogonius nigricollis</emphasis>
