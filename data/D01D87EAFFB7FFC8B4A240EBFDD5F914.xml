<taxonomicName id="9FB44D7FFFB7FFC8B00941EFFB0BFE50" box="[1084,1188,403,427]" class="Insecta" family="Ptiliidae" genus="Bambara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="160" phylum="Arthropoda" rank="genus">
