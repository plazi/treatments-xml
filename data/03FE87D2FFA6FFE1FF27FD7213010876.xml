<taxonomicName id="4C574D47FFA6FFE1FDF9FC1212B80CD5" box="[585,738,977,1001]" class="Arachnida" family="Pholcidae" genus="Khorata" kingdom="Animalia" order="Araneae" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="luojinensis">
<emphasis id="B923EAD6FFA6FFE1FDF9FC1212B80CD5" box="[585,738,977,1001]" italics="true" pageId="27" pageNumber="28">K. luojinensis</emphasis>
</taxonomicName>
(
<bibRefCitation id="EFC64B35FFA6FFE1FD41FC1214D50CD6" author="Yao" box="[753,1167,977,1002]" pageId="27" pageNumber="28" refString="Yao, Z. &amp; Li, S. (2010) Pholcid spiders of the genus Khorata Huber, 2005 (Araneae: Pholcidae) from Guangxi, China. Zootaxa, 2594, 1 - 79." type="journal article" year="2010">
Yao &amp; Li 
<number id="3D9F24E3FFA6FFE1FCD2FC1213C50CD6" box="[866,927,978,1002]" pageId="27" pageNumber="28" value="2010.0">2010</number>
: 
<number id="3D9F24E3FFA6FFE1FC19FC1213E30CD6" box="[937,953,978,1002]" pageId="27" pageNumber="28" value="9.0">9</number>
, figs 
<number id="3D9F24E3FFA6FFE1FC45FC1114660CD6" box="[1013,1084,977,1002]" pageId="27" pageNumber="28" value="26.5" valueMax="28.0" valueMin="25.0">25–28</number>
and 
<number id="3D9F24E3FFA6FFE1FBC2FC1114D50CD6" box="[1138,1167,977,1002]" pageId="27" pageNumber="28" value="61.0">61</number>
</bibRefCitation>
) and 
<taxonomicName id="4C574D47FFA6FFE1FB7DFC1215190CD5" box="[1229,1347,978,1001]" class="Arachnida" family="Pholcidae" genus="Khorata" kingdom="Animalia" order="Araneae" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="paquini">
<emphasis id="B923EAD6FFA6FFE1FB7DFC1215190CD5" box="[1229,1347,978,1001]" italics="true" pageId="27" pageNumber="28">K. paquini</emphasis>
</taxonomicName>
(
<bibRefCitation id="EFC64B35FFA6FFE1FAE1FC1211AD0B32" author="Yao" pageId="27" pageNumber="28" refString="Yao, Z. &amp; Li, S. (2010) Pholcid spiders of the genus Khorata Huber, 2005 (Araneae: Pholcidae) from Guangxi, China. Zootaxa, 2594, 1 - 79." type="journal article" year="2010">
Yao &amp; Li 
<number id="3D9F24E3FFA6FFE1FF07FC3610A90B32" box="[183,243,1014,1038]" pageId="27" pageNumber="28" value="2010.0">2010</number>
: 
<number id="3D9F24E3FFA6FFE1FF4FFC3611470B32" box="[255,285,1014,1038]" pageId="27" pageNumber="28" value="13.0">13</number>
, figs 
<number id="3D9F24E3FFA6FFE1FEEBFC3611FB0B32" box="[347,417,1014,1038]" pageId="27" pageNumber="28" value="42.5" valueMax="44.0" valueMin="41.0">41–44</number>
