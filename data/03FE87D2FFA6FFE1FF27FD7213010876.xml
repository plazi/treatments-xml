<number id="3D9F24E3FFA6FFE1FDCBFB6A12F60BFE" box="[635,684,1194,1218]" pageId="27" pageNumber="28" value="0.42">0.42</number>
+ 
<number id="3D9F24E3FFA6FFE1FD7DFB6A12A40BFE" box="[717,766,1194,1218]" pageId="27" pageNumber="28" value="3.73">3.73</number>
+ 
<number id="3D9F24E3FFA6FFE1FCAEFB6913150BFE" box="[798,847,1193,1218]" pageId="27" pageNumber="28" value="5.77">5.77</number>
+ 
<number id="3D9F24E3FFA6FFE1FCDFFB6913F90BFE" box="[879,931,1193,1218]" pageId="27" pageNumber="28" value="1.25">1.25</number>
), leg IV: 
<number id="3D9F24E3FFA6FFE1FBBEFB6914170BFE" box="[1038,1101,1193,1218]" pageId="27" pageNumber="28" value="19.63">19.63</number>
