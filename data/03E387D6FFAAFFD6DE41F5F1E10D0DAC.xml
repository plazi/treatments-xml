, 
<taxonomicName id="4C4A4D43FFAAFFD6D835F1A0E1090DAC" authority="(Moricand, 1838)" baseAuthorityName="Moricand" baseAuthorityYear="1838" class="Gastropoda" family="Bulimulidae" genus="Cochlorina" kingdom="Animalia" order="Stylommatophora" pageId="6" pageNumber="63" phylum="Mollusca" rank="species" species="aurismuris">
