<typeStatus id="0F568833A55C3F708EF3FB21FF2D5AB5" box="[151,201,1204,1225]" pageId="68" pageNumber="69">Type</typeStatus>
species: 
<taxonomicName id="17ED4D12A55C3F708F42FB26FC985AB5" authority="Skoracki, Scibek and Sikora, 2012" authorityName="Skoracki, Scibek and Sikora" authorityYear="2012" box="[294,892,1203,1225]" class="Arachnida" family="Syringophilidae" genus="Pseudopicobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="nonnula">
