<quantity id="9B17ECB0FFDDFFF58F4EFD00FC5B02C8" box="[843,978,686,713]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" metricValueMax="6.5" metricValueMin="4.7" pageId="40" pageNumber="41" unit="mm" value="5.6" valueMax="6.5" valueMin="4.7">4.7–6.5 mm</quantity>
