<taxonomicName id="DFC54D56FFEC671BFD8346B4ED1F628B" box="[627,780,943,967]" class="Insecta" family="Curculionidae" genus="Aulacus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="279" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="2AB1EAC7FFEC671BFD8346B4ECAC628B" box="[627,703,943,967]" italics="true" pageId="12" pageNumber="279">Aulacus</emphasis>
