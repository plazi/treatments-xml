<taxonomicName id="A2DEA8D0FFA1D337D3572CE1FD6B7193" authority="Mohrig &amp; Kauschke" authorityName="Mohrig &amp; Kauschke &amp; Broadley" authorityYear="2017" box="[293,725,1972,1994]" class="Insecta" family="Sciaridae" genus="Scatopsciara" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="476" phylum="Arthropoda" rank="species" species="dubiosa" status="sp. n.">
