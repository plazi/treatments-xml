<taxonomicName id="2963AC26CD007ED569D8BAB07F8CE1B7" authorityName="Lepeletier" authorityYear="1845" lsidName="" pageId="0" pageNumber="1" rank="tribe" tribe="Gorytini">Gorytini</taxonomicName>
