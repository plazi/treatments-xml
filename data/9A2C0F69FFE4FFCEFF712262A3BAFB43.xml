<taxonomicName id="D585C5FCFFE5FFCEFE8325A6A269FD60" authority="Matile" authorityName="Matile" box="[357,617,693,719]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="ctenophora">
<emphasis id="20F1626DFFE5FFCEFE8325A6A214FD61" box="[357,532,693,718]" italics="true" pageId="13" pageNumber="14">M. ctenophora</emphasis>
Matile
</taxonomicName>
(
<collectingCountry id="6A92FEEFFFE5FFCEFD9F25A6A338FD60" box="[633,824,693,719]" name="New Caledonia" pageId="13" pageNumber="14">New Caledonia</collectingCountry>
