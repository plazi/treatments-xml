<taxonomicName id="D585C5FCFFE5FFCEFA9D243AA113FCC6" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="unifurcata">
<emphasis id="20F1626DFFE5FFCEFA9D243AA113FCC6" italics="true" pageId="13" pageNumber="14">M. unifurcata</emphasis>
