<taxonomicName id="D585C5FCFFE5FFCEFCD6259CA38BFD07" box="[816,907,655,680]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis id="20F1626DFFE5FFCEFCD6259CA38BFD07" box="[816,907,655,680]" italics="true" pageId="13" pageNumber="14">Manota</emphasis>
</taxonomicName>
