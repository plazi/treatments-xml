<subSubSection id="D6F92CA2BE3AFF8EFF0BFD28FB75B0D8" pageId="29" pageNumber="30" type="materials_examined">
<paragraph id="9E5C7F29BE3AFF8EFF0BFD28FDD6B640" blockId="29.[151,1437,757,1754]" box="[151,529,757,781]" pageId="29" pageNumber="30">
<emphasis id="AC97A33BBE3AFF8EFF0BFD28FEE3B640" bold="true" box="[151,292,757,781]" pageId="29" pageNumber="30">
<typeStatus id="4158C18BBE3AFF8EFF0BFD28FF15B640" box="[151,210,757,781]" pageId="29" pageNumber="30">Type</typeStatus>
