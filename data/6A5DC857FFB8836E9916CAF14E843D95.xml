<tableNote id="1F1278CFFFB8836E9A56CB4E4A6A3985" pageId="2" pageNumber="98" targetBox="[130,719,477,951]" targetPageId="2">
<paragraph id="E24B7941FFB8836E9A56CB4E49C4392C" blockId="2.[113,748,259,437]" box="[113,748,259,284]" pageId="2" pageNumber="98">
Strains: 1, 311T; 2, 
<taxonomicName id="25F402C2FFB8836E9B15CB4E4934392C" box="[306,540,265,284]" class="Cytophagia" family="Flammeovirgaceae" genus="Cesiribacter" higherTaxonomySource="GBIF,CoL" kingdom="Bacteria" order="Cytophagales" pageId="2" pageNumber="98" phylum="Bacteroidetes" rank="species" species="andamanensis">
