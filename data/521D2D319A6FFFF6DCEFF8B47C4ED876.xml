<taxonomicName id="1DB4E7A49A6FFFF7DFA3F8217C26DEE6" authorityName="Forel" authorityYear="1890" box="[987,1069,1944,1967]" class="Insecta" family="Formicidae" genus="Messor" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="536" phylum="Arthropoda" rank="genus">
<emphasis id="E8C040359A6FFFF7DFA3F8217C26DEE6" box="[987,1069,1944,1967]" italics="true" pageId="4" pageNumber="536">Messor</emphasis>
</taxonomicName>
