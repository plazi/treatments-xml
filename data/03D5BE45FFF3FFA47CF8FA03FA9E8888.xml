3. 
<taxonomicName id="4C7C74D0FFF3FFAA7CDAFA03FDD18C99" authority="(Chang, 1938)" baseAuthorityName="Chang" baseAuthorityYear="1938" box="[181,555,1482,1509]" class="Insecta" family="Cicadellidae" genus="Neoaliturus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="207" phylum="Arthropoda" rank="species" species="hui">
