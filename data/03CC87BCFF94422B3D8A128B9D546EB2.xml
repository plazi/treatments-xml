<quantity id="4C9D9B4FFF96422B3F6214689F756FCB" box="[639,787,1652,1677]" metricMagnitude="-1" metricUnit="m" metricValue="3.825" metricValueMax="3.97" metricValueMin="3.6799999999999997" pageId="7" pageNumber="308" unit="mm" value="382.5" valueMax="397.0" valueMin="368.0">368–397 µm</quantity>
. Setae 
<emphasis id="B911EAB8FF96422B3E7A14689FF66FCA" box="[871,912,1652,1676]" italics="true" pageId="7" pageNumber="308">ad1</emphasis>
