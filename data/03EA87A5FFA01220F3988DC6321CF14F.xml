genus 
<taxonomicName id="4C434D30FFA01220F13B8D813095F3AA" authority="Dumeril and Bibron, 1841:506" authorityName="Dumeril and Bibron" authorityPageNumber="506" authorityYear="1841" box="[564,939,222,244]" class="Amphibia" family="Hylidae" genus="Acris" kingdom="Animalia" order="Anura" pageId="9" pageNumber="10" phylum="Chordata" rank="genus">
