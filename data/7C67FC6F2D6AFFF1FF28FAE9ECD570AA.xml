<taxonomicName id="33CE36FA2D6AFFF1FED1FE02ED7870A5" authorityName="Fibiger" authorityYear="2010" box="[366,544,1918,1943]" class="Insecta" family="Erebidae" genus="Sinochrostia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="sichuanensis">
<emphasis id="C6BA916B2D6AFFF1FED1FE02ED7870A5" box="[366,544,1918,1943]" italics="true" pageId="89" pageNumber="90">S. sichuanensis</emphasis>
</taxonomicName>
