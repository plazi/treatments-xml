<taxonomicName id="BF974D3FF91AFFFBFBF8FB9EF94F1A14" authority="(Mulsant)" baseAuthorityName="Mulsant" baseAuthorityYear="1850" box="[1146,1675,1024,1066]" class="Insecta" family="Coccinellidae" genus="Rodolia" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="157" phylum="Arthropoda" rank="species" species="cardinalis">
