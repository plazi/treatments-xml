<taxonomicName id="BF974D3FF91AFFFBFE21FC53FDD41DC6" box="[419,528,973,1016]" class="Magnoliopsida" family="Rutaceae" genus="Citrus" kingdom="Plantae" order="Sapindales" pageId="0" pageNumber="157" phylum="Tracheophyta" rank="genus">
