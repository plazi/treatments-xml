<subSubSection id="4CF5E3F3FFB1FFF7FEA0F983FEBD0672" pageId="31" pageNumber="232" type="etymology">
<paragraph id="0450B078FFB1FFF7FEA0F983FEBD0672" blockId="31.[151,1436,1468,1637]" pageId="31" pageNumber="232">
<emphasis id="369B6C6AFFB1FFF7FEA0F983FEB60656" bold="true" box="[199,334,1576,1601]" pageId="31" pageNumber="232">Etymology.</emphasis>
