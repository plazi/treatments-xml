<taxonomicName id="62514279748B4964D7E42A164E300F42" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230295" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName="Polyrhachis militaris bruta Santschi" lsidName-HNS="Polyrhachis militaris bruta Santschi" order="Hymenoptera" pageNumber="223" phylum="Arthropoda" rank="subSpecies" species="militaris" subGenus="Myrma" subSpecies="bruta">Polyrhachis (Myrma) militaris F. st. bruta Sants</taxonomicName>
