<quantity id="6F0DC883FFFDFF92D3A42DC31DB2FE33" box="[388,447,408,428]" metricMagnitude="2" metricUnit="m" metricValue="5.05" pageId="13" pageNumber="1690" unit="m" value="505.0">505 m</quantity>
