<quantity id="6F0DC883FFFDFF92D1892D331FE7FEE3" box="[937,1002,360,380]" metricMagnitude="2" metricUnit="m" metricValue="5.05" pageId="13" pageNumber="1690" unit="m" value="505.0">505 m</quantity>
