<taxonomicName id="4C744D51FFF9FFD61EFCDC16FE7CFE57" authority="Kohls, 1950" authorityName="Kohls" authorityYear="1950" box="[151,470,367,392]" class="Arachnida" family="Ixodidae" genus="Ixodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="werneri">
