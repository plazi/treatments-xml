<taxonomicName id="E6B19FF29B11FFB2935BFAF64DE21818" authority="Scopoli, 1763" authorityName="Scopoli" authorityYear="1763" box="[306,656,1298,1323]" class="Insecta" family="Vespidae" genus="Sphex" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="nigerrima">
