<taxonomicName id="708EEC94506BFFDCC044FAD7FDF1FA75" authority="Hanley &amp; Ashe." authorityName="Hanley &amp; Ashe." box="[271,625,1404,1423]" class="Insecta" family="Staphylinidae" genus="Ligulata" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="121" phylum="Arthropoda" rank="species" species="hansoni">
