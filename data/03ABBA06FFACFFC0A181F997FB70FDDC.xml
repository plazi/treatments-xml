<subSubSection id="C318589BFFACFFCCA181F997FD6CF9DB" box="[151,679,1567,1594]" pageId="7" pageNumber="502" type="nomenclature">
<paragraph id="8BBD0B10FFACFFCCA181F997FD6CF9DB" blockId="7.[151,679,1567,1594]" box="[151,679,1567,1594]" pageId="7" pageNumber="502">
<heading id="D0F5BC7CFFACFFCCA181F997FD6CF9DB" bold="true" box="[151,679,1567,1594]" fontSize="11" level="1" pageId="7" pageNumber="502" reason="1">
<emphasis id="B976D702FFACFFCCA181F997FD6CF9DB" bold="true" box="[151,679,1567,1594]" pageId="7" pageNumber="502">
Subgenus 
<taxonomicName id="4C027093FFACFFCCA001F9A8FD6CF9DB" authority="Boucomont, 1909" authorityName="Boucomont" authorityYear="1909" box="[279,679,1567,1594]" class="Insecta" family="Bolboceratidae" genus="Bolbochromus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="502" phylum="Arthropoda" rank="genus">
