sp. and 
<taxonomicName id="9073C4260D962788EB16B03D08F50BD1" authority="Emery, 1906" authorityName="Emery" authorityYear="1906" class="Insecta" family="Formicidae" genus="Nylanderia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Nylanderia" order="Hymenoptera" pageId="17" pageNumber="28" phylum="Arthropoda" rank="genus">Nylanderia</taxonomicName>
