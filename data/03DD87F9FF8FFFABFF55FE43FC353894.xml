Known from 
<collectingRegion id="49B0F80DFF8EFFABFE3CFB50FD1F3894" box="[510,742,1231,1256]" country="Jamaica" name="Saint Thomas" pageId="31" pageNumber="332">
<collectingCountry id="F363767FFF8EFFABFE3CFB50FD613894" box="[510,664,1231,1256]" name="Virgin Islands" pageId="31" pageNumber="332">Saint Thomas</collectingCountry>
