<taxonomicName id="4C4385E3680CFFD5FBFBFE58FA3D4CCC" box="[1116,1436,440,464]" class="Insecta" family="Cecidomyiidae" genus="Neolasioptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="444" phylum="Arthropoda" rank="species" species="cephalandrae">
<emphasis id="B9372272680CFFD5FBFBFE58FA3D4CCC" box="[1116,1436,440,464]" italics="true" pageId="5" pageNumber="444">Neolasioptera cephalandrae</emphasis>
