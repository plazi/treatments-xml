<paragraph id="CC2E01178E8A87208DCF733699E8D181" pageId="28" pageNumber="29">
<taxonomicName id="900B745820F19DA84D5629A13E289569" authority="Capa, Nygren, Parapar, Bakken, Meissner &amp; Moreira, 2019" authorityName="Capa, Nygren, Parapar, Bakken, Meissner &amp; Moreira" authorityYear="2019" class="Polychaeta" family="Sphaerodoridae" genus="Geminofilum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Geminofilum fauchaldi" order="Phyllodocida" pageId="28" pageNumber="29" phylum="Annelida" rank="species" species="fauchaldi">Geminofilum fauchaldi</taxonomicName>
