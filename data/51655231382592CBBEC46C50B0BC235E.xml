<paragraph id="38BA68BCC7EE6E56BCC8166C2ABACEEE" pageId="28" pageNumber="29">
<taxonomicName id="4236CDA382A981DA0CBDA1B6B0CA7648" authority="Capa, Nygren, Parapar, Bakken, Meissner &amp; Moreira, 2019" authorityName="Capa, Nygren, Parapar, Bakken, Meissner &amp; Moreira" authorityYear="2019" class="Polychaeta" family="Sphaerodoridae" genus="Geminofilum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Geminofilum garciaalvarezi" order="Phyllodocida" pageId="28" pageNumber="29" phylum="Annelida" rank="species" species="garciaalvarezi">Geminofilum garciaalvarezi</taxonomicName>
