<paragraph id="B4FE0D773FCCBA443E27A47F32E26594" pageId="28" pageNumber="29">
<taxonomicName id="7DCF1EAA108EB5F06839BF2AE4C58B52" authority="Capa, Nygren, Parapar, Bakken, Meissner &amp; Moreira, 2019" authorityName="Capa, Nygren, Parapar, Bakken, Meissner &amp; Moreira" authorityYear="2019" class="Polychaeta" family="Sphaerodoridae" genus="Geminofilum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Geminofilum halldori" order="Phyllodocida" pageId="28" pageNumber="29" phylum="Annelida" rank="species" species="halldori">Geminofilum halldori</taxonomicName>
