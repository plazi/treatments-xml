<taxonomicName id="C44CD37A0DEA9D4044BD43B16732628E" class="Polychaeta" family="Sphaerodoridae" genus="Sphaerodorum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphaerodorum distichum" order="Phyllodocida" pageId="27" pageNumber="28" phylum="Annelida" rank="species" species="distichum">Sphaerodorum distichum</taxonomicName>
