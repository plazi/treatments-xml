<collectionCode id="6A9ADFCAC507BD46FB43FF3B547FFF04" box="[1217,1303,151,177]" country="Hungary" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" lsid="urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="23" pageNumber="254">HNHM</collectionCode>
