<taxonomicName id="0350B404F1EB462CA1FC916E664013F7" LSID="178F1E4A-D9F7-5C12-859D-626D293D6F4C" authority="Blackwall, 1850" authorityName="Blackwall" authorityYear="1850" class="Arachnida" family="Linyphiidae" genus="Oedothorax" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Oedothorax apicatus" order="Araneae" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="apicatus">Oedothorax apicatus Blackwall, 1850</taxonomicName>
