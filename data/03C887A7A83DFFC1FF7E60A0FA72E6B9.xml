<emphasis id="B915EAA3A83DFFC7FF7E670AFEE1E719" bold="true" box="[151,273,1038,1063]" pageId="10" pageNumber="311">Diagnosis.</emphasis>
<taxonomicName id="4C614D32A83DFFC7FEF2670BFE78E718" authorityName="Klautau" authorityYear="2017" box="[283,392,1039,1062]" class="Calcarea" family="Clathrinidae" genus="Borojevia" kingdom="Animalia" order="Clathrinida" pageId="10" pageNumber="311" phylum="Porifera" rank="genus">
