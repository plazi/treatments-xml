<taxonomicName id="9FB840E95B07001FFADEFEF5FBAC79BC" authority="Xu &amp; Huang, 2004" authorityName="Xu &amp; Huang" authorityYear="2004" class="Hydrozoa" family="Pandeidae" genus="Leuckartiara" kingdom="Animalia" order="Anthoathecata" pageId="11" pageNumber="247" phylum="Cnidaria" rank="species" species="neustona">
