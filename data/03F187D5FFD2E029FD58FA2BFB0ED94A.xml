<taxonomicName id="4C584D40FFD0E029FF72FA81FEE0DD72" authorityName="Wollaston" authorityYear="1854" box="[233,352,1336,1358]" class="Insecta" family="Curculionidae" genus="Caulophilus" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="genus">
