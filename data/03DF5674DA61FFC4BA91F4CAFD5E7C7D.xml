<paragraph id="8BC9E762DA61FFC4BAEDFB32F8777F32" blockId="0.[108,2105,72,3030]" pageId="0" pageNumber="4">
<emphasis id="B9023B70DA61FFC4BAEDFB32FE9B7F62" box="[192,359,2326,2360]" italics="true" pageId="0" pageNumber="4">Sie gleicht</emphasis>
<taxonomicName id="4C769CE1DA61FFC4BBACFB32FD847F62" box="[385,632,2326,2360]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="4" phylum="Arthropoda" rank="genus" species="diversus">
