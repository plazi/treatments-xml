<taxonomicName id="4C769CE1DA61FFC4BDF8FB81F7D37F9D" box="[2005,2095,2469,2503]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="4" phylum="Arthropoda" rank="genus">
