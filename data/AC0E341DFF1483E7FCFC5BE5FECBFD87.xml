<taxonomicName id="E3A7FE88FF1483E4FADD5EF5FA02F937" authority="(DC.) Thell. (GREUTER et al., 1986: 150)" class="Magnoliopsida" family="Brassicaceae" genus="Raphanus" kingdom="Plantae" order="Brassicales" pageId="169" pageNumber="169" phylum="Tracheophyta" rank="subSpecies" species="raphanistrum" subSpecies="rostratus">
