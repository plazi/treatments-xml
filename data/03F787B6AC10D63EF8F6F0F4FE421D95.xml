<taxonomicName id="4C5E4D23AC11D63EF8BBF7DCFD951881" authority="MacCallum, 1918" authorityName="MacCallum" authorityYear="1918" box="[218,620,187,213]" class="Insecta" family="Pieridae" genus="Eurema" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="234" phylum="Arthropoda" rank="species" species="keksooni">
