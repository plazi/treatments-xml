<taxonomicName id="4C014C9DFFA8FFCD1013F31CE6E7FE82" class="Magnoliopsida" family="Oleaceae" genus="Notelaea" kingdom="Plantae" order="Lamiales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="species" species="microcarpa">
<emphasis id="B975EB0CFFA8FFCD1013F31CE6E7FE82" italics="true" pageId="53" pageNumber="54">Notelaea microcarpa</emphasis>
</taxonomicName>
(
<taxonomicName id="4C014C9DFFA8FFCD12F5F37BE77DFE83" authorityName="von Hoffmannsegg &amp; Link" authorityYear="1809" box="[228,335,330,354]" class="Magnoliopsida" family="Oleaceae" kingdom="Plantae" order="Lamiales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="family">Oleaceae</taxonomicName>
). 
<taxonomicName id="4C014C9DFFA8FFCD1372F37AE47EFE82" box="[355,588,331,355]" class="Magnoliopsida" family="Oleaceae" genus="Notelaea" kingdom="Plantae" order="Lamiales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="species" species="microcarpa">
