<quantity id="4CF99AFBFFA8FFCD12B2F3CDE6D9FDF5" box="[163,235,508,532]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="53" pageNumber="54" unit="km" value="15.0">15 km</quantity>
