<subSubSection id="01EF6A9AE55AF474BC8F1D927FCAE405" pageId="0" pageNumber="49" type="description">
<paragraph id="CA09686A7DCC48890100CD6C30CC1742" pageId="0" pageNumber="49">Measurements.</paragraph>
<paragraph id="87CA1B1190EB89822045EEED66E7EA0C" pageId="0" pageNumber="49">
