<subSubSection id="9256B40296F41078778B6299504590E5" pageId="0" pageNumber="49" type="etymology">
<paragraph id="C149C2F3963B21F7FF8BE9999278FC68" pageId="0" pageNumber="49">Etymology.</paragraph>
<paragraph id="1DCCE3356B1FA8D19100C48C254127B4" pageId="0" pageNumber="49">
<emphasis id="A26596CA5F43AAB71DC45E82312045A5" italics="true" pageId="0" pageNumber="49">Recurvata</emphasis>
