<materialsCitation id="6DCA3C88C162FFB9FD617366FD770433" collectionCode="SAM" pageId="28" pageNumber="589" specimenCode="PQ 69675" specimenCount="1" typeStatus="holotype">
<typeStatus id="02198877C162FFB9FD617366FCB20449" box="[677,780,273,296]" pageId="28" pageNumber="589">HOLOTYPE</typeStatus>
. — 
<collectionCode id="BBB3AE10C162FFB9FCF27366FCD70447" box="[822,873,273,297]" country="South Africa" httpUri="http://grbio.org/cool/mhew-9czz" name="South African Museum" pageId="28" pageNumber="589">SAM</collectionCode>
