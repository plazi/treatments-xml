<taxonomicName id="35F4FEDC88F325C6F128B2D1020232F7" genus="Chalcidoidea" lsidName="Chalcidoidea" order="Lepidoptera" pageId="91" pageNumber="92" rank="order">Lepidoptera</taxonomicName>
(
<taxonomicName id="37258FDDF422EAB9D3D7CF0FF3CA0248" family="Bucculatricidae" lsidName="" pageId="91" pageNumber="92" rank="family">Bucculatricidae</taxonomicName>
; 
<taxonomicName id="77F6EE87611C1CB795D6A63A62E39895" family="Gelechiidae" lsidName="" pageId="91" pageNumber="92" rank="family">Gelechiidae</taxonomicName>
; 
<taxonomicName id="1F16F28FDC2E65550CA5E7528C10F972" family="Momphidae" lsidName="" pageId="91" pageNumber="92" rank="family">Momphidae</taxonomicName>
; 
<taxonomicName id="F9E880BC13C18C40F45B31AD1D750B78" family="Notodontidae" lsidName="" pageId="91" pageNumber="92" rank="family">Notodontidae</taxonomicName>
; 
<taxonomicName id="33E342D3EB5383CE783F20246ED5EC10" family="Oecophoridae" lsidName="" pageId="91" pageNumber="92" rank="family">Oecophoridae</taxonomicName>
; 
<taxonomicName id="C2E99C8013778280106131375ECAF03D" family="Pyralidae" lsidName="" pageId="91" pageNumber="92" rank="family">Pyralidae</taxonomicName>
; 
<taxonomicName id="08A25D4226C5A99434370C3C54FDD263" family="Tortricidae" lsidName="" pageId="91" pageNumber="92" rank="family">Tortricidae</taxonomicName>
) and 
<taxonomicName id="28D4AF53CBE0B8886234A386E9678D16" genus="Chalcidoidea" lsidName="Chalcidoidea" order="Hymenoptera" pageId="91" pageNumber="92" rank="order">Hymenoptera</taxonomicName>
(
<taxonomicName id="94C0C3945C664DA7FC62B392B08262E1" family="Braconidae" lsidName="" pageId="91" pageNumber="92" rank="family">Braconidae</taxonomicName>
; 
<taxonomicName id="4B7BBA7390EE339579FF1F2CAC8A9310" family="Ichneumonidae" lsidName="" pageId="91" pageNumber="92" rank="family">Ichneumonidae</taxonomicName>
).
</paragraph>
</subSubSection>
<subSubSection id="4F4549DAB2033BD46E15A8A1E3B23B7A" pageId="91" pageNumber="92" type="distribution">
<paragraph id="C2495D8038A99894B15688BCA91AAF3A" pageId="91" pageNumber="92">Distribution.</paragraph>
