<taxonomicName id="61DC50F14424DD5E478546F8A7D1DC84" class="Insecta" family="Chalcididae" genus="Haltichella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haltichella" order="Hymenoptera" pageId="91" pageNumber="92" phylum="Arthropoda" rank="genus">Haltichella</taxonomicName>
