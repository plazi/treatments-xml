<typeStatus id="54F28840FFB3FFC5FDB940C3E742AF6D" box="[562,617,237,258]" pageId="40" pageNumber="413">Type</typeStatus>
species (by monotypy): 
<taxonomicName id="4C494D61FFB3FFC5FEBA4125E7CAAF4F" authority="Draparnaud, 1801" authorityName="Draparnaud" authorityYear="1801" box="[305,737,267,289]" class="Gastropoda" family="Helicidae" genus="Helix" kingdom="Animalia" order="Stylommatophora" pageId="40" pageNumber="413" phylum="Mollusca" rank="species" species="conspurcata">
