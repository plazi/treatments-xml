<materialsCitation id="B73F2065E051F8E97303C3C8B617FA5F" ID-GBIF-Occurrence="923888754" biotopType="Kiefernforste" collectingDate="1961" collectingDateMax="1963" collectingDateMin="1961" collectingMethod="Stechzylinder oder Stechrahmen" collectorName="M. Moritz" country="Germany" determinationType="Wiederbeschreibung" elevation="10 m - 45 m" extractionMethod="Berlese-Tullgren-Trichter, modifiziert" location="bei Greifswald" microHabitat="4.3 Pflanzenabfälle - Streu" municipality="Greifswald" plant="Wald-Kiefer" plantTaxonName="Pinus sylvestris" plantTaxonType="plant" soilLayer="Organischer Horizont (&gt;30 Masse-% org. Substanz)" stateProvince="Mecklenburg-Vorpommern">
Bodenauflage aus 
<normalizedToken id="895E06B214DB4A2CE866E21B87977ECD" originalValue="Kiefernmischwäldern" pageId="4" pageNumber="138">Kiefernmischwaeldern</normalizedToken>
Norddeutschlands 
<location id="C836412FC1040FEC1423B9D46F6B6400" LSID="urn:lsid:plazi:treatment:98F51739D98CDA0301B215A1910245FA:C836412FC1040FEC1423B9D46F6B6400" country="Germany" latitude="54.0874" longLatPrecision="7000" longitude="13.3895" municipality="Greifswald" name="bei Greifswald" stateProvince="Mecklenburg-Vorpommern">bei Greifswald</location>
</materialsCitation>
und aus 
<materialsCitation id="F95F65757E2A03E5CFEEAA4537A221F7" ID-GBIF-Occurrence="923888752" biotopType="LAUB(MISCH)WÄLDER UND -FORSTE (Laubbaumanteil &gt; 50 %)" biotopTypeCode="43" collectingDate="ca. 1960" collectingMethod="Substratprobe (Gewinnung der Tiere im Labor)" collectorName="M. Moritz" country="Germany" determinationType="Wiederbeschreibung" latitude="51.7560" location="bei Ruebeland" longLatPrecision="3000" longitude="10.8500" municipality="Ruebeland" stateProvince="Sachsen-Anhalt">
<normalizedToken id="4ED8855D7AD5DC71ADF71B3F11EC3878" originalValue="Laubmischwäldern" pageId="4" pageNumber="138">Laubmischwaeldern</normalizedToken>
des Harzes 
<location id="2623368D7DC995586BFDE7254445B268" LSID="urn:lsid:plazi:treatment:98F51739D98CDA0301B215A1910245FA:2623368D7DC995586BFDE7254445B268" country="Germany" latitude="51.7560" longLatPrecision="3000" longitude="10.8500" municipality="Ruebeland" name="bei Ruebeland" stateProvince="Sachsen-Anhalt">
bei 
<normalizedToken id="19A1AE71357D97A44EA0A24308533F6D" originalValue="Rübeland" pageId="4" pageNumber="138">Ruebeland</normalizedToken>
</location>
(
<collectorName id="8B2DC8280BC3FD7D9DFC078223FF27A1">Moritz</collectorName>
