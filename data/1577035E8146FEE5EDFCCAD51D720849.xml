<collectingCountry id="E5C9F2D88146FEE5EF56CBD11BF9086D" box="[573,676,1528,1553]" name="Australia" pageId="322" pageNumber="323">Australia</collectingCountry>
and 
<collectingCountry id="E5C9F2D88146FEE5EFB1CBD01AD7086D" box="[730,906,1528,1553]" name="New Caledonia" pageId="322" pageNumber="323">New Caledonia</collectingCountry>
