<quantity id="5A261FAD8146FEE5EF5BC8351BD80848" box="[560,645,1564,1589]" metricMagnitude="1" metricUnit="m" metricValue="2.6" metricValueMax="5.0" metricValueMin="0.2" pageId="322" pageNumber="323" unit="m" value="26.0" valueMax="50.0" valueMin="2.0">2–50 m</quantity>
