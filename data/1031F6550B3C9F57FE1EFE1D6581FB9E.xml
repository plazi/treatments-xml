<specimenCount id="8E9E8CCA0B3C9F55FBA0FADA60A6FA80" box="[1144,1228,1315,1341]" pageId="5" pageNumber="6" type="male">14 ♂♂</specimenCount>
, 
<specimenCount id="8E9E8CCA0B3C9F55FB01FADA6146FA80" box="[1241,1324,1315,1341]" pageId="5" pageNumber="6" type="female">12 ♀♀</specimenCount>
, same locality, site “4”, 
<date id="EC2661830B3C9F55FE51FABF6638FADD" box="[393,594,1350,1376]" pageId="5" pageNumber="6" value="2001-05-10" valueMax="2001-05-26" valueMin="2001-05-10">10–26 May 2001</date>
(
<collectionCode id="FE89DF860B3C9F55FDB9FABF6769FADD" box="[609,771,1350,1376]" pageId="5" pageNumber="6">NHRS-GULI</collectionCode>
000022471, -472, -474–479 and 
<collectionCode id="FE89DF860B3C9F55FB59FABF60ADFADD" box="[1153,1223,1350,1376]" pageId="5" pageNumber="6">SDEI</collectionCode>
