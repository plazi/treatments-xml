<taxonomicName id="AADD6EE6FFC67E1B7ED484DBFF0FFE84" class="Malacostraca" family="Ampithoidae" genus="Paragrubia" kingdom="Animalia" order="Amphipoda" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="vorax">
