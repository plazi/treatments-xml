<taxonomicName id="4C2B4D41BB365D7794B20A6FFADB3A3E" authority="Martius" authorityName="Martius" authorityYear="1841" box="[965,1286,259,286]" class="Magnoliopsida" family="Sapindaceae" genus="Orbignya" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="species" species="phalerata">
