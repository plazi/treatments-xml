<subSubSection id="C3316549BB365D7797B00A6EFC703A61" pageId="39" pageNumber="40" type="biology_ecology">
<paragraph id="8B9436C2BB365D7797B00A6EFC703A61" blockId="39.[151,1436,151,393]" pageId="39" pageNumber="40">
<emphasis id="B95FEAD0BB365D7797B00A6EFEFA3A3C" bold="true" box="[199,295,258,284]" pageId="39" pageNumber="40">Biology.</emphasis>
