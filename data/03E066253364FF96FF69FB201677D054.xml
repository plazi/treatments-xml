<taxonomicName id="4C49ACB03364FF96FF69FB20113BD293" authority="Berenger, 2007" authorityName="Berenger" authorityYear="2007" box="[151,547,1189,1215]" class="Insecta" family="Reduviidae" genus="Heza" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="gilsantanai">
