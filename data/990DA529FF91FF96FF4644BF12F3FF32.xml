<taxonomicName id="D6A46FBCFF8EFF97FE8A44441415F9D7" box="[347,588,1563,1585]" class="Insecta" family="Dynastidae" genus="Oxyligyrus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="467" phylum="Arthropoda" rank="species" species="cayennensis">
