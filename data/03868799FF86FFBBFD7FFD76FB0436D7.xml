<taxonomicName id="4C2F4D0CFF86FFBBFCB1FC1AFC5B36F0" box="[781,894,969,990]" class="Magnoliopsida" family="Fabaceae" genus="Desmodium" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="365" phylum="Tracheophyta" rank="genus">
