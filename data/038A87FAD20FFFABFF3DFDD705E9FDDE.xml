<paragraph id="8B9C36ECD20FFFA8FF3DFDD700F2FD82" blockId="1.[151,390,546,573]" box="[151,390,546,573]" pageId="1" pageNumber="272">
<heading id="D0D48180D20FFFA8FF3DFDD700F2FD82" bold="true" box="[151,390,546,573]" fontSize="11" level="1" pageId="1" pageNumber="272" reason="1">
<emphasis id="B957EAFED20FFFA8FF3DFDD700F2FD82" bold="true" box="[151,390,546,573]" pageId="1" pageNumber="272">
<taxonomicName id="4C234D6FD20FFFA8FF3DFDD7007DFD82" authority="Rédei &amp; Tsai, 2021" authorityName="Rédei &amp; Tsai" authorityYear="2021" box="[151,265,546,573]" class="Insecta" family="Pentatomidae" genus="Zhengica" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="272" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B957EAFED20FFFA8FF3DFDD7007DFD82" bold="true" box="[151,265,546,573]" italics="true" pageId="1" pageNumber="272">Zhengica</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A2645785D20FFFA8FEB2FDD600F2FD82" box="[280,390,547,573]" pageId="1" pageNumber="272" rank="genus">gen. nov.</taxonomicNameLabel>
