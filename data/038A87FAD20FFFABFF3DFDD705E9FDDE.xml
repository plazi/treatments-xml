<subSubSection id="C3396567D20CFFABFF6DFE0A05E9FDDE" pageId="2" pageNumber="273" type="etymology">
<paragraph id="8B9C36ECD20CFFABFF6DFE0A0556FD82" blockId="2.[151,1436,151,609]" pageId="2" pageNumber="273">
<emphasis id="B957EAFED20CFFABFF6DFE0A003AFDA6" bold="true" box="[199,334,511,537]" pageId="2" pageNumber="273">Etymology.</emphasis>
