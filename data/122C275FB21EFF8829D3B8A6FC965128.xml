<quantity id="5D7D3BACB21EFF892846BEEDFE4051BB" box="[295,405,1193,1215]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" metricValueMax="5.3" metricValueMin="4.5" pageId="20" pageNumber="1621" unit="mm" value="4.9" valueMax="5.3" valueMin="4.5">4,5-5,3mm</quantity>
