<emphasis id="B903EA8AFFE6C45EFF1CFA00CC9285C1" box="[236,253,1447,1473]" italics="true" pageId="16" pageNumber="57">A</emphasis>
. 
<emphasis id="B903EA8AFFE6C45EFEFBFA00CDE985C1" box="[267,390,1447,1473]" italics="true" pageId="16" pageNumber="57">oligoneura</emphasis>
</taxonomicName>
; 
<quantity id="4C8F9B7DFFE6C45EFE65FA01CE4D85C1" box="[405,546,1446,1473]" metricMagnitude="-2" metricUnit="m" metricValue="9.8806" metricValueMax="10.541" metricValueMin="9.2202" pageId="16" pageNumber="57" unit="in" value="3.89" valueMax="4.15" valueMin="3.63">3.63–4.15 in</quantity>
<taxonomicName id="4C774D1BFFE6C45EFDD9FA00CE8385C1" authorityName="Archibald &amp; Makarkin" authorityYear="2021" baseAuthorityName="Engel" baseAuthorityYear="2011" box="[553,748,1447,1473]" class="Insecta" family="Raphidiidae" genus="Archiinocellia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="16" pageNumber="57" phylum="Arthropoda" rank="species" species="protomaculata">
