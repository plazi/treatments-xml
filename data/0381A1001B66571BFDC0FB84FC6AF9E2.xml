Junior synonyms: 
<taxonomicName id="4C286B951B66571BFF22FB11FC3AFB52" authority="(Walker, 1869)" authorityName="Walker" authorityYear="1869" baseAuthorityName="Walker" baseAuthorityYear="1869" box="[629,1014,1191,1216]" class="Insecta" family="Gryllacrididae" genus="Eremus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="581" phylum="Arthropoda" rank="species" species="incerta">
