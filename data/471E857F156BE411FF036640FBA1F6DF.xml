<collectionCode id="A9A6ACAC1568E411FCB96334FC93F64C" box="[813,875,512,535]" country="Germany" httpUri="http://grbio.org/cool/kdnf-0379" name="Zoologisches Museum Hamburg" pageId="26" pageNumber="27">ZMH</collectionCode>
