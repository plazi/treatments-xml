<taxonomicName id="34C04D4EFFB6FFFBFD9EBE6DCDE5F98D" box="[572,650,1591,1614]" class="Magnoliopsida" family="Juncaceae" genus="Juncus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="13" pageNumber="464" phylum="Tracheophyta" rank="genus">
