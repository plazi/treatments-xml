<taxonomicName id="8592DAD9FF8BFF85FD4EF940FB34E79F" authority="Huber &amp; Benjamin, 2005" authorityName="Huber &amp; Benjamin" authorityYear="2005" box="[740,1154,1759,1782]" class="Arachnida" family="Pholcidae" genus="Wanniyala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="agrabopath">
