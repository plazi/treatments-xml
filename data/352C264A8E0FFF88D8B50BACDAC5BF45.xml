REMARKS. — 
<taxonomicName id="7A85ECDF8E0FFF89D92C085ADB39B93A" baseAuthorityName="Azuma" baseAuthorityYear="1961" box="[363,526,1827,1850]" class="Gastropoda" family="Muricidae" genus="Mipus" kingdom="Animalia" order="Neogastropoda" pageId="86" pageNumber="567" phylum="Mollusca" rank="species" species="tortuosus">
