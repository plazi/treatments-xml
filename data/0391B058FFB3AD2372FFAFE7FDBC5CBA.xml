<date id="FF86278EFFB3AD1C7292AE66FE895A18" box="[223,343,1284,1304]" pageId="31" pageNumber="2056" value="2007-06-02" valueMax="2007-06-10" valueMin="2007-06-02">2-10.VI.2007</date>
