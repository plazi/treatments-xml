<quantity id="4CC0ACABFFB3AD1C719AAF8EFBE75A00" box="[983,1081,1260,1280]" metricMagnitude="2" metricUnit="m" metricValue="8.2" metricValueMax="9.0" metricValueMin="7.4" pageId="31" pageNumber="2056" unit="m" value="820.0" valueMax="900.0" valueMin="740.0">740-900 m</quantity>
