<taxonomicName id="4C444D22CB7F1B25F8D5FDCEFECBFDBB" authorityName="Quentin et Villiers" authorityYear="1981" box="[144,290,616,637]" class="Insecta" family="Cerambycidae" genus="Anomophysis" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="44" phylum="Arthropoda" rank="species" species="australis">
