</emphasis>
<taxonomicName id="4C444D22CB7F1B25F90FFDEFFE39FD98" authorityName=": Quentin and Villiers" authorityYear="1981" box="[330,464,585,606]" class="Insecta" family="Cerambycidae" genus="Anomophysis" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="44" phylum="Arthropoda" rank="species" species="coxalis">
