<taxonomicName id="1FC3D4B1E047BC7FFEB7FB61F4A5537B" authorityName=": Loew" authorityYear="1858" box="[352,461,2341,2373]" class="Insecta" family="Asilidae" genus="Daspletis" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="293" phylum="Arthropoda" rank="genus">
