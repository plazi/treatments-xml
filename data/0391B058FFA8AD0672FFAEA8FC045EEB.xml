<collectingCountry id="F32F41DEFFA9AD067067AAB7FD515EEA" box="[554,655,469,490]" name="Myanmar" pageId="5" pageNumber="2030">Myanmar</collectingCountry>
, new for 
<collectingCountry id="F32F41DEFFA9AD0670A0AAB7FC945EEB" box="[749,842,469,491]" name="Malaysia" pageId="5" pageNumber="2030">Malaysia</collectingCountry>
