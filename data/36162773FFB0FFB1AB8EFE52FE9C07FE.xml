<taxonomicName id="79BFEDE6FFB0FFB1AA73FDC8FD9E0146" box="[362,539,581,606]" class="Actinopterygii" family="Cichlidae" genus="Herotilapia" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageId="19" pageNumber="230" phylum="Chordata" rank="species" species="multispinosa">
