and 
<taxonomicName id="4C5B4D08FFEDFFE8A342FC1CFB000296" authority="Grobler, Bayram &amp; Cobanoglu, 2004" authorityName="Grobler, Bayram &amp; Cobanoglu" authorityYear="2004" box="[587,1164,907,933]" class="Arachnida" family="Galumnidae" genus="Allogalumna" kingdom="Animalia" order="Sarcoptiformes" pageId="11" pageNumber="288" phylum="Arthropoda" rank="species" species="turkeyensis">
