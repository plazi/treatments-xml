<taxonomicName id="4C5D4D00DC4CFFA2A718A255FCBFFAE7" authorityName="Gonzalez-Alvarado &amp; Vaz-de-Mello" authorityYear="2021" box="[834,882,1286,1312]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="103" phylum="Arthropoda" rank="species" species="gilli">
