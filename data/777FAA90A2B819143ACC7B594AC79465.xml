species: black star 
<taxonomicName id="4DBF02FA6548596B97FE4DA73091848A" genus="C." lsidName="C. watanabei" pageId="0" pageNumber="45" rank="species" species="watanabei">C. watanabei</taxonomicName>
sp. n., black circle 
<taxonomicName id="C879E53C20C2CD3A22F54E546362D1E8" genus="C." lsidName="C. malayanum" pageId="0" pageNumber="45" rank="species" species="malayanum">C. malayanum</taxonomicName>
