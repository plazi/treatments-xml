<quantity id="2A271FE19922FFA5EC6E9ABDDAFDFE3A" box="[1395,1436,370,399]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="17" pageNumber="18" unit="in" value="1.0">1 in</quantity>
proportion of 
<number id="5B17A0239922FFA5E82F9AA6DE5CFE03" box="[306,317,410,434]" pageId="17" pageNumber="18" value="1.0">1</number>
