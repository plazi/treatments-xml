<taxonomicName id="4C05E1BA5024C94FFDD171A1FD33B5C3" authorityName="vulpes (Linnaeus" authorityYear="1758" box="[554,651,1186,1210]" class="Mammalia" family="Canidae" genus="Vulpes" kingdom="Animalia" order="Carnivora" pageId="5" pageNumber="292" phylum="Chordata" rank="species" species="vulpes">
