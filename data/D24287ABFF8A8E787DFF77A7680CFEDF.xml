d (min)= 
<quantity id="9D139B58FF8A8E797CF6718E6E1DFB61" box="[414,546,1174,1200]" metricMagnitude="-2" metricUnit="m" metricValue="3.75" metricValueMax="4.0" metricValueMin="3.5" pageId="82" pageNumber="83" unit="mm" value="37.5" valueMax="40.0" valueMin="35.0">35–40 mm</quantity>
; d (max)= 
<quantity id="9D139B58FF8A8E797FF3718E6F21FB61" box="[667,798,1174,1200]" metricMagnitude="-2" metricUnit="m" metricValue="4.15" metricValueMax="4.5" metricValueMin="3.8" pageId="82" pageNumber="83" unit="mm" value="41.5" valueMax="45.0" valueMin="38.0">38–45 mm</quantity>
