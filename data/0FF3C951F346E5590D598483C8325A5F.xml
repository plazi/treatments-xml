<paragraph id="23DE65D5F1361FD06FA0B6072974285B">
<taxonomicName id="7CD455B4AA294D9EFBBF336866693BB1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145151" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Uploaded - Tetramorium (Xiphomyrmex) andrei subsp. robustior" lsidName-HNS="Tetramorium (Xiphomyrmex) andrei r. robustior Forel" order="Hymenoptera" phylum="Arthropoda" rank="subSpecies" species="andrei" subGenus="Xiphomyrmex" subSpecies="robustior">Tetramorium (Xiphomyrmex) andrei st. robustior Forel</taxonomicName>
