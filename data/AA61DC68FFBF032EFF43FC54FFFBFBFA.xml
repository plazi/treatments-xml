<paragraph id="22776D7EFFBF032EFF43FC6FF805FC4E" blockId="5.[151,1436,907,1149]" box="[151,632,943,968]" pageId="5" pageNumber="506">
<treatmentCitationGroup id="02D84A50FFBF032EFF43FC6FF805FC4E" box="[151,632,943,968]" pageId="5" pageNumber="506">
<emphasis id="10BCB16CFFBF032EFF43FC6FFBE6FC4E" box="[151,411,944,968]" italics="true" pageId="5" pageNumber="506">
<taxonomicName id="E5C816FDFFBF032EFF43FC6FFB5EFC4E" box="[151,291,944,968]" class="Gymnostomatea" family="Amphileptidae" genus="Amphileptus" kingdom="Protozoa" order="Pleurostomatida" pageId="5" pageNumber="506" phylum="Ciliophora" rank="genus">Amphileptus</taxonomicName>
