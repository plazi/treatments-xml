(Lamto, 
<collectingCountry id="15C9AFDFFFDAFFB5FC716AB8FEBD348B" box="[1123,1266,1953,1978]" name="Ivory Coast" pageId="23" pageNumber="324">Ivory Coast</collectingCountry>
); ZMB 
<number id="DB16FD68FFDAFFB5FD446ABBFFD3348B" box="[1366,1436,1953,1977]" pageId="23" pageNumber="324" value="77415.0">77415</number>
(Kérouane, 
<collectingCountry id="15C9AFDFFFDAFFB5F9096ADFFB3E34EF" box="[283,369,1989,2013]" name="Guinea-Bissau" pageId="23" pageNumber="324">Guinea</collectingCountry>
); ZMB 
<number id="DB16FD68FFDAFFB5F9DA6ADFF84134EF" box="[456,526,1989,2013]" pageId="23" pageNumber="324" value="77416.0">77416</number>
(Konsankoro, 
<collectingCountry id="15C9AFDFFFDAFFB5FAA76ADFF94434EF" box="[693,779,1989,2013]" name="Guinea-Bissau" pageId="23" pageNumber="324">Guinea</collectingCountry>
); ZMB 
<number id="DB16FD68FFDAFFB5FB736ADFF9E834EF" box="[865,935,1989,2013]" pageId="23" pageNumber="324" value="77410.0">77410</number>
(Dantilla, 
<collectingCountry id="15C9AFDFFFDAFFB5FC326ADFFE3C34EF" box="[1056,1139,1989,2013]" name="Guinea-Bissau" pageId="23" pageNumber="324">Guinea</collectingCountry>
) (
<figureCitation id="F5E5F3CAFFDAFFB5FC9B6ADFFE8434EF" box="[1161,1227,1989,2013]" captionStart="FIGURE 1" captionStartId="4.[151,250,1838,1861]" captionTargetBox="[170,1406,1021,1787]" captionTargetId="figure@4.[151,1436,985,1817]" captionTargetPageId="4" captionText="FIGURE 1. Map showing the distribution of the sequenced specimens of recognised species in the Hyperolius nasutus group plus H. lamottei. Species numbers represent the following: 1 — H. acuticeps, 2 — H. adspersus, 3 — H. benguellensis, 4 — H. friedemanni sp. nov., 5 — H. howelli sp. nov., 6 — H. igbettensis, 7 — H. inyangae sp. nov., 8 — H. jacobseni sp. nov., 9 — H. lamottei, 10 — H. lupiroensis sp. nov., 11 — H. nasutus, 12 — H. poweri, 13 — H. rwandae sp. nov., 14 — H. dartevellei, 15 — H. viridis, 16 — H. nasicus." httpUri="https://zenodo.org/record/215984/files/figure.png" pageId="23" pageNumber="324">
Fig. 
