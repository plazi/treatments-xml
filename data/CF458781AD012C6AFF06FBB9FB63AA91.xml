<quantity id="80149B72AD012C6AFF44FA12FE97AA91" box="[213,369,1438,1464]" metricMagnitude="3" metricUnit="m" metricValue="3.0" metricValueMax="4.0" metricValueMin="2.0" pageId="3" pageNumber="529" unit="m" value="3000.0" valueMax="4000.0" valueMin="2000.0">2000−4000 m</quantity>
