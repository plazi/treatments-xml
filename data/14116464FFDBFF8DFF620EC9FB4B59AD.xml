<collectingCountry id="E4AF95E2FFDBFF8DFEC80A3CFED15B62" box="[317,355,1122,1148]" name="Chile" pageId="5" pageNumber="50">TR</collectingCountry>
<number id="2A70C755FFDBFF8DFE9F0A3CFE535B62" box="[362,481,1122,1148]" pageId="5" pageNumber="50" value="2.4299999999999997" valueMax="2.53" valueMin="2.33">2.33–2.53</number>
(
<number id="2A70C755FFDBFF8DFE070A3CFD985B62" box="[498,554,1122,1148]" pageId="5" pageNumber="50" value="2.39">2.39</number>
