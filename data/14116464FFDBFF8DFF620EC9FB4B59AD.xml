<number id="2A70C755FFDBFF8DFC940A63FCC25B49" box="[865,880,1085,1111]" pageId="5" pageNumber="50" value="7.0">7</number>
spines; prothoracic 
<collectingCountry id="E4AF95E2FFDBFF8DFBAB0A63FB365B49" box="[1118,1156,1085,1111]" name="Chile" pageId="5" pageNumber="50">TR</collectingCountry>
