<number id="E76824EDFF9E911BFE09482CFE74FDAC" box="[394,423,599,625]" pageId="9" pageNumber="10" value="18.0">18</number>
(
<number id="E76824EDFF9E911BFE3E482CFDDBFDAC" box="[445,520,599,625]" pageId="9" pageNumber="10" value="18.0" valueMax="19.0" valueMin="17.0">17–19</number>
