<number id="E76824EDFF9E911BFD0A4BA4FD75FE24" box="[649,678,479,505]" pageId="9" pageNumber="10" value="18.0">18</number>
(
<number id="E76824EDFF9E911BFD344BA4FCD6FE24" box="[695,773,479,505]" pageId="9" pageNumber="10" value="18.5" valueMax="20.0" valueMin="17.0">17–20</number>
