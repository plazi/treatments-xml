<taxonomicName id="C3EFCBFBFFBCFFFAFF4DF9B7FD5B0625" authority="Duda, 1926" authorityName="Duda" authorityYear="1926" box="[298,675,1564,1586]" class="Insecta" family="Drosophilidae" genus="Leucophenga" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="flavicosta">
