<taxonomicName id="071E8BD589DD555C2156219E4E7245EC" class="Trematoda" family="Hemiuridae" genus="Robinia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Robinia neomexicana" order="Plagiorchiida" pageId="29" pageNumber="58" phylum="Platyhelminthes" rank="species" species="neomexicana">Robinia neomexicana</taxonomicName>
