<specimenCount id="9D1DFD5F545DFFCFFCD0F943FC07F97B" box="[782,899,1676,1701]" pageId="1" pageNumber="597" type="subadult">1 subadult</specimenCount>
, 
<quantity id="4CE39B33545DFFCFFC50F943FC57F97B" box="[910,979,1676,1701]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="597" unit="mm" value="1.0">1 mm</quantity>
