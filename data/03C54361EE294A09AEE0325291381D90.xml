<paragraph id="8BD3F277EE294A08AEE0331194DA1A22" blockId="2.[151,1436,1838,2045]" box="[151,1154,1838,1866]" pageId="2" pageNumber="405">
<emphasis id="B9182E65EE294A08AEE03311916C1A22" bold="true" box="[151,308,1838,1865]" pageId="2" pageNumber="405">
<typeStatus id="54D74CD5EE294A08AEE03311908A1A22" box="[151,210,1839,1865]" pageId="2" pageNumber="405">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C6C89F4EE294A08AF4B331193291A21" authority="(Tinkham, 1936)" baseAuthorityName="Tinkham" baseAuthorityYear="1936" box="[316,881,1839,1866]" class="Insecta" family="Tetrigidae" genus="Austrohancockia" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="405" phylum="Arthropoda" rank="species" species="kuangtungensis">
<emphasis id="B9182E65EE294A08AF4B331192F51A22" box="[316,685,1839,1865]" italics="true" pageId="2" pageNumber="405">Austrohancockia kuangtungensis</emphasis>
(
<bibRefCitation id="EFFD8F86EE294A08ACCB331193311A21" author="Tinkham, E. R." box="[700,873,1839,1866]" pageId="2" pageNumber="405" pagination="401 - 413" refId="ref24407" refString="Tinkham, E. R. (1936) Four new species of Orthoptera from Loh Fau Shan, Kwangtung, South China. Lingnan Science Journal, 15 (3), 401 - 413." type="journal article" year="1936">Tinkham, 1936</bibRefCitation>
