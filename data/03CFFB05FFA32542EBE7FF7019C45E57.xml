<subSubSection id="C37C1998FFA32542EBE7FF701E8E5D76" pageId="18" pageNumber="519" type="nomenclature">
<paragraph id="8BD94A13FFA32542EBE7FF701FE55CDB" blockId="18.[151,1436,151,573]" box="[151,762,151,176]" pageId="18" pageNumber="519">
<heading id="D091FD7FFFA32542EBE7FF701FE55CDB" bold="true" box="[151,762,151,176]" fontSize="10" level="2" pageId="18" pageNumber="519" reason="2">
<taxonomicName id="4C663190FFA32542EBE7FF701FE55CDB" authority="Saussure and Zehntner, 1894" authorityName="Saussure and Zehntner" authorityYear="1894" box="[151,762,151,176]" class="Insecta" family="Mantidae" genus="Stagmomantis" kingdom="Animalia" order="Mantodea" pageId="18" pageNumber="519" phylum="Arthropoda" rank="species" species="venusta">
