<collectionCode id="ED28E8E6C5029672A4EBF9A3FD957614" LSID="urn:lsid:biocol.org:col:34871" box="[472,557,1586,1612]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="26" pageNumber="27">USNM</collectionCode>
