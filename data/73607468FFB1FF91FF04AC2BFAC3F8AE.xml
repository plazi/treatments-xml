<taxonomicName id="3CC9BEFDFFB1FF91FF04ACD5FDA0FB4A" authority=": Osten-Sacken (1865)" authorityName=": Osten-Sacken" authorityYear="1865" box="[151,559,1216,1240]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="mendax">
<emphasis id="C9BD196CFFB1FF91FF04ACD5FEC7FB4A" box="[151,328,1216,1240]" italics="true" pageId="22" pageNumber="23">Synergus mendax</emphasis>
