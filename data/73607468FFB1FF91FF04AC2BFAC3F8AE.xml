<subSubSection id="B3D396F5FFB1FF91FF54AEEBFAC3F8AE" pageId="22" pageNumber="23" type="discussion">
<paragraph id="FB76C57EFFB1FF91FF54AEEBFAC3F8AE" blockId="22.[151,1437,1286,1853]" pageId="22" pageNumber="23">
<emphasis id="C9BD196CFFB1FF91FF54AEEBFEBBF88A" bold="true" box="[199,308,1790,1816]" pageId="22" pageNumber="23">Remarks</emphasis>
. 
<taxonomicName id="3CC9BEFDFFB1FF91FED7AEEAFD86F88A" authorityName=": Osten-Sacken" authorityYear="1865" box="[324,521,1790,1817]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="mendax">
