<heading id="D83F601CFFA5060F17EEF985FC98F9E8" bold="true" box="[151,786,1618,1645]" fontSize="11" level="1" pageId="1" pageNumber="190" reason="1">
<emphasis id="B1BC0B62FFA5060F17EEF985FC98F9E8" bold="true" box="[151,786,1618,1645]" pageId="1" pageNumber="190">
<taxonomicName id="44C8ACF3FFA5060F17EEF985FD80F9E8" authority="Reshchikov" authorityName="Reshchikov" box="[151,522,1618,1645]" class="Insecta" family="Ichneumonidae" genus="Allophrys" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="190" phylum="Arthropoda" rank="species" species="falcatus" status="sp. nov.">
<emphasis id="B1BC0B62FFA5060F17EEF985FEF9F9E9" bold="true" box="[151,371,1618,1644]" italics="true" pageId="1" pageNumber="190">Allophrys falcatus</emphasis>
Reshchikov
</taxonomicName>
, 
