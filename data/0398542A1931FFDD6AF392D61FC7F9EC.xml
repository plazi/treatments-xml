<typeStatus id="548A5B9E1931FFDD6A2196141FEFF9A9" box="[93,206,1562,1583]" pageId="14" pageNumber="14">paratypes</typeStatus>
from 
<collectorName id="26C480EA1931FFDD6B6396171EB7F9A9" box="[287,406,1561,1583]" pageId="14" pageNumber="14">Turkestan</collectorName>
