<subSubSection id="9F90657A710AFEABFF5FFC2CFAC11B9F" box="[151,1321,905,930]" pageId="265" pageNumber="266" type="type_taxon">
<paragraph id="D73536F1710AFEABFF5FFC2CFAC11B9F" blockId="265.[151,1321,905,930]" box="[151,1321,905,930]" pageId="265" pageNumber="266">
<emphasis id="E5FEEAE3710AFEABFF5FFC2CFECB1B9F" bold="true" box="[151,291,905,930]" pageId="265" pageNumber="266">
<typeStatus id="08318853710AFEABFF5FFC2CFF241B9C" box="[151,204,905,929]" pageId="265" pageNumber="266">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="108A4D72710AFEABFEE2FC2FFDB21B9C" authorityName="New" authorityYear="1985" box="[298,602,906,929]" class="Insecta" family="Myrmeleontidae" genus="Austrogymnocnemia" kingdom="Animalia" order="Neuroptera" pageId="265" pageNumber="266" phylum="Arthropoda" rank="species" species="pygmaea">
