<taxonomicName id="4C432FBFFF6BA69582D821B5D98C4ECF" authority="PLATNICK, 2002" authorityName="PLATNICK" authorityYear="2002" box="[428,685,976,997]" class="Arachnida" family="Trochanteriidae" genus="Hemicloeina" kingdom="Animalia" order="Araneae" pageId="205" phylum="Arthropoda" rank="species" species="humptydoo" status="new species">
