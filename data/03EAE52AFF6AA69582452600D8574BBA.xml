<caption id="DF3C04B4FF6BA695838121B5D8C1492A" ID-DOI="http://doi.org/10.5281/zenodo.5396591" ID-Zenodo-Dep="5396591" httpUri="https://zenodo.org/record/5396591/files/figure.png" pageId="205" startId="205.[245,293,976,997]" targetBox="[225,1318,284,955]" targetPageId="205">
<paragraph id="8BFC543CFF6BA695838121B5D8C1492A" blockId="205.[219,1323,976,1024]" pageId="205">
