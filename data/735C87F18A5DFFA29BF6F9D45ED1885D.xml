<quantity id="3C0D9B028A5DFFA59FC5F8575EE18E89" box="[1188,1261,1970,1994]" metricMagnitude="2" metricUnit="m" metricValue="1.83" pageId="11" pageNumber="12" unit="m" value="183.0">183 m</quantity>
</elevation>
depth, 
<collectingDate id="9F0FE9CF8A5DFFA59E25F8575ADE8EAE" pageId="11" pageNumber="12" value="1987-05-10">
