<taxonomicName id="4C654D613D66FF8AFAAAFDB1FE15FD73" authority="Vygeboom Dam" authorityName="Vygeboom Dam" class="Magnoliopsida" family="Verbenaceae" genus="Lippia" kingdom="Plantae" order="Lamiales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="rehmannii">
