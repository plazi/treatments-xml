<taxonomicName id="3ED91DDA6A1BFFCEFF4FFBB28C80D82C" authority=", Knutson &amp; Orth, 1990" authorityName=", Knutson &amp; Orth" authorityYear="1990" box="[151,677,1050,1077]" class="Insecta" family="Sciomyzidae" genus="Colobaea" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="montana">
