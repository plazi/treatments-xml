<taxonomicName id="4C1BCF921D49FFF7FF1F914AD938C340" authority="Ragge, 1956" authorityName="Ragge" authorityYear="1956" box="[238,526,1615,1642]" class="Insecta" family="Phaneropteridae" genus="Eulioptera" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="128" phylum="Arthropoda" rank="genus">
