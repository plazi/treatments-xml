<paragraph id="BD14FDE8C83AA2D33018AC69FE1ED38C" pageId="0" pageNumber="77">
<taxonomicName id="4EDA4E1A2E5F9E4CCE123075E06CDB8A" authority="Szépligeti, 1902" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Aerophilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aerophilus davidwagneri" order="Hymenoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="davidwagneri">
<emphasis id="9867B9A339114DB4A963F36E8075F7FE" italics="true" pageId="0" pageNumber="77">Aerophilus davidwagneri</emphasis>
</taxonomicName>
