, IV 
<quantity id="397757BBFF9AFFB51E00773F0F24FBBC" box="[1304,1403,1122,1145]" metricMagnitude="-5" metricUnit="m" metricValue="5.0" pageId="3" pageNumber="150" unit="mm" value="0.05">0.05 mm</quantity>
, V 
<quantity id="397757BBFF9AFFB51B8F77D80AA7FB59" box="[151,248,1157,1180]" metricMagnitude="-5" metricUnit="m" metricValue="7" pageId="3" pageNumber="150" unit="mm" value="0.07">0.07 mm</quantity>
, VI 
<quantity id="397757BBFF9AFFB51A3E77D80BD8FB59" box="[294,391,1157,1180]" metricMagnitude="-5" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="150" unit="mm" value="0.06">0.06 mm</quantity>
, VII 
