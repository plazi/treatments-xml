material
</emphasis>
. 
<typeStatus id="003A55A9FF9A32199DA9F96F5DF16F47" box="[388,497,1778,1804]" pageId="1" pageNumber="30">Holotype</typeStatus>
male, 
<collectingCountry id="A796AB9BFF9A32199E6DF96F5E8A6F47" box="[576,650,1778,1804]" name="Nepal" pageId="1" pageNumber="30">Nepal</collectingCountry>
: Gupa Pokali (
<quantity id="187946EEFF9A32199F15F96F5F946F47" box="[824,916,1778,1804]" metricMagnitude="3" metricUnit="m" metricValue="2.9" pageId="1" pageNumber="30" unit="m" value="2900.0">2900 m</quantity>
) - Chouki (
<quantity id="187946EEFF9A32199831F96F587A6F47" box="[1052,1146,1778,1804]" metricMagnitude="3" metricUnit="m" metricValue="2.7" pageId="1" pageNumber="30" unit="m" value="2700.0">2700 m</quantity>
), 
<date id="AB3FCDCBFF9A321998BDF96F591F6F47" box="[1168,1311,1778,1804]" pageId="1" pageNumber="30" value="1972-06-16">
