<collectingCountry id="82EE7635FFF58D3ADA5F8047FE6AE231" box="[353,418,1051,1077]" name="China" pageId="25" pageNumber="318">China</collectingCountry>
(
<collectingRegion id="383DF847FFF58D3ADA8E8047FDC5E232" box="[432,525,1051,1078]" country="China" name="Sichuan" pageId="25" pageNumber="318">Sichuan</collectingRegion>
