<collectionCode id="ED095389F718FC57FD6EF4BF6898F8E5" box="[673,765,1774,1800]" country="Hungary" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" lsid="urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="10" pageNumber="11">HNHM</collectionCode>
103415, n= 5): H 
<quantity id="4CE066A9F718FC57FC19F4BF6E10F8E4" box="[982,1141,1774,1801]" metricMagnitude="-3" metricUnit="m" metricValue="1.375" metricValueMax="1.5" metricValueMin="1.25" pageId="10" pageNumber="11" unit="mm" value="1.375" valueMax="1.5" valueMin="1.25">1.25–1.5 mm</quantity>
; W 
<quantity id="4CE066A9F718FC57FB6CF4BF6F56F8E4" box="[1187,1331,1774,1801]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" metricValueMax="2.4" metricValueMin="2.2" pageId="10" pageNumber="11" unit="mm" value="2.3" valueMax="2.4" valueMin="2.2">2.2–2.4 mm</quantity>
