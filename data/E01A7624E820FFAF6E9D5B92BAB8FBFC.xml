<taxonomicName id="AFB3BCB1E820FFAF6C3458AABA52FC3E" authority="BORDONI 2012 d" authorityName="BORDONI" authorityYear="2012" box="[539,876,991,1013]" class="Insecta" family="Staphylinidae" genus="Indolinus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="1757" phylum="Arthropoda" rank="species" species="vietnamiticus">
