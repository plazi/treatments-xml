<taxonomicName id="AFB3BCB1E820FFAF6F9C58B7BB17FBD9" authorityName="Bordoni" authorityYear="2013" box="[435,553,1020,1042]" class="Insecta" family="Staphylinidae" genus="Indolinus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="1757" phylum="Arthropoda" rank="species" species="laosianus" status="nov.sp.">
