<collectingCountry id="10A487A2E820FFAF6D5D588BBA81FC1C" box="[882,959,961,983]" name="Taiwan" pageId="12" pageNumber="1757">Taiwan</collectingCountry>
; 
<taxonomicName id="AFB3BCB1E820FFAF6DE45888B873FC3E" authority="BORDONI 2002" authorityName="BORDONI" authorityYear="2002" class="Insecta" family="Staphylinidae" genus="Indolinus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="1757" phylum="Arthropoda" rank="species" species="sculptilis">
<emphasis id="5AC71B20E820FFAF6DE45888BD03FC1C" box="[971,1085,961,983]" italics="true" pageId="12" pageNumber="1757">I. sculptilis</emphasis>
<bibRefCitation id="0C22BAC3E820FFAF6E9D5895B873FC3E" author="BORDONI A" box="[178,333,991,1013]" pageId="12" pageNumber="1757" refId="ref19404" refString="BORDONI A. (2002): Xantholinini della Regione Orientale (Coleoptera: Staphylinidae). Classificazione, filogenesi e revisione tassonomica. - Monografie del Museo Regionale di Scienze Naturali, Torino 33: 998 pp." type="book" year="2002">BORDONI 2002</bibRefCitation>
