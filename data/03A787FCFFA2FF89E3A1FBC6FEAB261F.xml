<taxonomicName id="4C0E4D69FFA2FF89E7AEFB76FB6427EB" box="[1176,1270,1197,1223]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="75" pageNumber="76" phylum="Tracheophyta" rank="genus">
<emphasis id="B97AEAF8FFA2FF89E7AEFB76FB6427EB" box="[1176,1270,1197,1223]" italics="true" pageId="75" pageNumber="76">Quercus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C0E4D69FFA2FF89E604FB76FA0E27EB" box="[1330,1436,1197,1223]" class="Magnoliopsida" family="Fagaceae" genus="Castanea" kingdom="Plantae" order="Fagales" pageId="75" pageNumber="76" phylum="Tracheophyta" rank="genus">
<emphasis id="B97AEAF8FFA2FF89E604FB76FA0E27EB" box="[1330,1436,1197,1223]" italics="true" pageId="75" pageNumber="76">Castanea</emphasis>
</taxonomicName>
(
<taxonomicName id="4C0E4D69FFA2FF89E3F8FB0AFEAE27C7" authorityName="Dumortier" authorityYear="1829" box="[206,316,1233,1259]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="75" pageNumber="76" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
