<taxonomicName id="4C392FF2FFC98914FE11FCE1985AFCD0" authorityName="Wesolowska" authorityYear="1986" box="[421,553,779,805]" class="Arachnida" family="Salticidae" genus="Heliophanus" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="296" phylum="Arthropoda" rank="subGenus" subGenus="Helafricanus">
<emphasis id="B94D8863FFC98914FE11FCE1985AFCD0" box="[421,553,779,805]" italics="true" pageId="13" pageNumber="296">Helafricanus</emphasis>
</taxonomicName>
