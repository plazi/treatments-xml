<taxonomicName id="3EAE5DF41071FFD6AA607D059BC51A37" authorityName="HULL" authorityYear="1946" box="[295,431,1040,1062]" class="Insecta" family="Syrphidae" genus="Quichuana" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="97" phylum="Arthropoda" rank="species" species="brevicera">
