<subSubSection id="469E653EFFA0FF8BFF0CA900FE3B7831" box="[136,418,1111,1135]" pageId="21" pageNumber="685" type="distribution">
<paragraph id="0E3B36B5FFA0FF8BFF0CA900FE3B7831" blockId="21.[136,418,1111,1135]" box="[136,418,1111,1135]" pageId="21" pageNumber="685">
<emphasis id="3CF0EAA7FFA0FF8BFF0CA900FE837831" bold="true" box="[136,282,1111,1135]" pageId="21" pageNumber="685">Distribution.</emphasis>
<collectingCountry id="76937625FFA0FF8BFEA7A900FE067831" box="[291,415,1111,1135]" name="Philippines" pageId="21" pageNumber="685">Philippines</collectingCountry>
