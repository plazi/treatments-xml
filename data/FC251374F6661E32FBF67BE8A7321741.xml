<taxonomicName id="58C03FFF131F456424C5D355B9992C02" authority="Ehrh." class="Magnoliopsida" family="Geraniaceae" genus="Geranium" kingdom="Plantae" order="Geraniales" pageId="null" pageNumber="623" phylum="Tracheophyta" rank="species" species="divaricatum">
