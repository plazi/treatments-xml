<taxonomicName id="ACC9E2B2FFFAFB759764FC2DFBBC88C6" authority="Waldock 1995" authorityName="Waldock" authorityYear="1995" box="[571,1061,947,978]" class="Arachnida" family="Salticidae" genus="Maratus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="mungaich">
