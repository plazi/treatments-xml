<subSubSection id="C34379D9FFC4FFD0FF59FD6C09F09BE7" box="[199,1436,661,686]" pageId="10" pageNumber="594" type="etymology">
<paragraph id="8BE62A52FFC4FFD0FF59FD6C09F09BE7" blockId="10.[151,1436,151,758]" box="[199,1436,661,686]" pageId="10" pageNumber="594">
<emphasis id="B92DF640FFC4FFD0FF59FD6C0D3D9BE4" bold="true" box="[199,337,661,686]" pageId="10" pageNumber="594">Etymology:</emphasis>
