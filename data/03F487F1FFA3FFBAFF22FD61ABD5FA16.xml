<taxonomicName id="4C5D4D64FFA3FFBAFF22F9F7AFD1F9A1" baseAuthorityName="Brundin" baseAuthorityYear="1949" box="[151,408,1600,1626]" class="Insecta" family="Chironomidae" genus="Zalutschia" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="246" phylum="Arthropoda" rank="species" species="mucronata">
