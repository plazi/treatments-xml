<taxonomicName id="3BE32344D9C946A8D07DEDA5ABB21448" family="Combretaceae" genus="C." kingdom="Animalia" lsidName="C. coartatus" order="Coleoptera" pageId="0" pageNumber="609" rank="species" species="coartatus">
<emphasis id="7FCDD6D01D6CE14B8C667F8AA7531346" italics="true" pageId="0" pageNumber="609">C. coartatus</emphasis>
