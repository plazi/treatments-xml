<quantity id="4B285A9EFFB05351D43A44F2DF40FD75" box="[1293,1374,661,686]" metricMagnitude="3" metricUnit="m" metricValue="2.026" pageId="10" pageNumber="411" unit="m" value="2026.0">2026m</quantity>
, 
<date id="F86ED1BBFFB05351D45C44F2DB18FD09" pageId="10" pageNumber="411" value="2011-07-05" valueMax="2011-07-08" valueMin="2011-07-05">
<number id="3A18E55CFFB05351D45C44F2DF87FD75" box="[1387,1433,661,686]" pageId="10" pageNumber="411" value="6.5" valueMax="8.0" valueMin="5.0">5–8</number>
. July 
<number id="3A18E55CFFB05351D1FB44DDDB18FD09" box="[204,262,698,722]" pageId="10" pageNumber="411" value="2011.0">2011</number>
