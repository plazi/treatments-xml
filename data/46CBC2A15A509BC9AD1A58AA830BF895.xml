<paragraph id="339EC6C3BB3F68C3C27FC2035883353F" pageId="0" pageNumber="147">
<taxonomicName id="921276E1A7233E321EE4FD0017BF5FCB" class="Insecta" family="Diaspididae" genus="Parlatoria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parlatoria orbicularis" order="Hemiptera" pageId="0" pageNumber="147" phylum="Arthropoda" rank="species" species="orbicularis">Parlatoria orbicularis</taxonomicName>
