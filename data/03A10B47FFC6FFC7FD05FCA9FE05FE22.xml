<taxonomicName id="4C08C1D2FFC7FFB9FE9BF8E2FE1AF83A" authority="Enghoff, 2018" authorityName="Enghoff" authorityYear="2018" box="[286,488,1896,1922]" class="Diplopoda" family="Paradoxosomatidae" genus="Eviulisoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="63" pageNumber="63" phylum="Arthropoda" rank="species" species="zebra" status="sp. nov.">
