</emphasis>
: 
<taxonomicName id="8FFE4D54BE746235FEE84844FD37A13E" authority="Dietrich" authorityName="Dietrich" box="[377,765,546,570]" class="Insecta" family="Cicadellidae" genus="Krisna" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="garciamarquezi">
