<subSubSection id="00E4655CBE746235FF514845FC01A13E" box="[192,971,546,570]" pageId="3" pageNumber="2" type="type_taxon">
<paragraph id="484136D7BE746235FF514845FC01A13E" blockId="3.[192,971,546,570]" box="[192,971,546,570]" pageId="3" pageNumber="2">
<emphasis id="7A8AEAC5BE746235FF514845FEA6A13E" bold="true" box="[192,364,546,570]" pageId="3" pageNumber="2">
<typeStatus id="97458875BE746235FF514845FEC9A13E" box="[192,259,546,570]" pageId="3" pageNumber="2">Type</typeStatus>
