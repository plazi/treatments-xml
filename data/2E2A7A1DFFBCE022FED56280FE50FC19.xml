<quantity id="617B66EEFFBDE022FB70638EFAECFD01" box="[1197,1317,684,710]" metricMagnitude="1" metricUnit="m" metricValue="5.0" metricValueMax="10.0" metricValueMin="0.0" pageId="109" pageNumber="110" unit="m" value="50.0" valueMax="100.0" valueMin="0.0">0–100 m</quantity>
, Prov[incia] Punt[arenas], 
<collectingCountry id="DE948B9BFFBDE022FDE063F6FD27FD29" box="[573,750,724,750]" name="Costa Rica" pageId="109" pageNumber="110">
<collectionCode id="C09253CEFFBDE022FDE063F6FD52FD29" box="[573,667,724,750]" pageId="109" pageNumber="110">COSTA</collectionCode>
