<emphasis id="B94FEAA3D353A0692DABFB0AFD104CF1" bold="true" box="[198,326,1237,1263]" pageId="6" pageNumber="40">Diagnosis.</emphasis>
<taxonomicName id="4C3B4D32D353A0692C3AFB0AFEF24CF0" box="[343,676,1237,1262]" class="Actinopterygii" family="Sternoptychidae" genus="Argyropelecus" kingdom="Animalia" order="Stomiiformes" pageId="6" pageNumber="40" phylum="Chordata" rank="species" species="hemigymnus">
