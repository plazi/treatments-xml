<taxonomicName id="9BC14D0EA12DFFCE37F8F8A852C8F860" box="[658,922,1898,1920]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="palaichthus">
<emphasis id="6EB5EA9FA12DFFCE37F8F8A852C8F860" box="[658,922,1898,1920]" italics="true" pageId="4" pageNumber="5">Hemidactylus palaichthus</emphasis>
</taxonomicName>
