<taxonomicName id="4C3733FFFFFD2251EF1FF8FEFD23F89A" authority="von Oken" authorityName="von Oken" box="[523,666,1801,1826]" class="Anthozoa" family="Isididae" genus="Isis" kingdom="Animalia" order="Alcyonacea" pageId="6" pageNumber="7" phylum="Cnidaria" rank="genus">Isis von Oken</taxonomicName>
