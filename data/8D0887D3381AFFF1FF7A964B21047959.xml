<emphasis id="37D5EAD7381AFFFCFB769404252C7F54" bold="true" box="[1179,1344,1885,1911]" pageId="5" pageNumber="23">
<collectingCountry id="7DB67655381AFFFCFB769404252C7F54" box="[1179,1344,1885,1911]" name="South Africa" pageId="5" pageNumber="23">South Africa</collectingCountry>
</emphasis>
(
<collectingCountry id="7DB67655381AFFFCFABE940425FB7F54" box="[1363,1431,1885,1911]" name="South Africa" pageId="5" pageNumber="23">Natal</collectingCountry>
, Transvaal), 
<collectingCountry id="7DB67655381AFFFCFEC894DC21197FBC" box="[293,373,1925,1951]" name="Sudan" pageId="5" pageNumber="23">
<emphasis id="37D5EAD7381AFFFCFEC894DC21197FBC" bold="true" box="[293,373,1925,1951]" pageId="5" pageNumber="23">Sudan</emphasis>
</collectingCountry>
