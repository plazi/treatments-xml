<collectingCountry id="7DB67655381AFFFCFCF9940423157F54" box="[788,889,1885,1911]" name="Somalia" pageId="5" pageNumber="23">
<emphasis id="37D5EAD7381AFFFCFCF9940423157F54" bold="true" box="[788,889,1885,1911]" pageId="5" pageNumber="23">Somalia</emphasis>
</collectingCountry>
