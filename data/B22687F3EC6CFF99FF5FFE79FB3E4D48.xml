<taxonomicName id="FD8F4D66EC6CFF9EFF5FFDB6FD0B4A4C" authority="Kingsolver &amp; Silva 1991: 412" authorityName="Kingsolver &amp; Silva" authorityPageNumber="412" authorityYear="1991" box="[151,647,602,624]" class="Insecta" family="Chrysomelidae" genus="Sennius" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="520" phylum="Arthropoda" rank="species" species="lojaensis">
