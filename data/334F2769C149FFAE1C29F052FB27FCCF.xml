</taxonomicName>
<taxonomicNameLabel id="92A1F716C14BFFAE1B42F34EFACFFD30" box="[1301,1391,633,659]" pageId="38" pageNumber="39" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName id="7CE6EDFCC14BFFAE1EEAF3ABFEF0FD16" authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" box="[189,336,667,694]" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="malleolus" status="sp. nov.">
<emphasis id="89924A6DC14BFFAE1EEAF3ABFEF0FD16" box="[189,336,667,694]" italics="true" pageId="38" pageNumber="39">C. malleolus</emphasis>
</taxonomicName>
