<emphasis id="7FCA1C4577728B21FF5290ADFE3FE276" bold="true" box="[151,394,1517,1543]" pageId="11" pageNumber="12">
<taxonomicName id="8ABEBBD477728B21FF5290ADFF6EE276" box="[151,219,1517,1543]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="serini">
<emphasis id="7FCA1C4577728B21FF5290ADFF6EE276" bold="true" box="[151,219,1517,1543]" italics="true" pageId="11" pageNumber="12">serini</emphasis>
</taxonomicName>
