<collectingRegion id="B66E3BFFFFE6FF86E8F6487EF77CF4FD" box="[2029,2223,2841,2870]" country="Indonesia" name="Sumatera Barat" pageId="4" pageNumber="211">West Sumatra</collectingRegion>
