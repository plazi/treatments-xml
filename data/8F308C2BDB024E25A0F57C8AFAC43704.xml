<quantity id="C06190D8DB024E25A1DE7F84FDF83090" box="[444,552,921,947]" metricMagnitude="-3" metricUnit="m" metricValue="6.56" pageId="5" pageNumber="56" unit="mm" value="6.56">6.56 mm</quantity>
, HW 
<quantity id="C06190D8DB024E25A2127F84FD1E3090" box="[624,718,921,947]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="5" pageNumber="56" unit="mm" value="1.5">1.5 mm</quantity>
, PL 
<quantity id="C06190D8DB024E25A3647F84FCA23090" box="[774,882,921,947]" metricMagnitude="-4" metricUnit="m" metricValue="8.1" pageId="5" pageNumber="56" unit="mm" value="0.81">0.81 mm</quantity>
, PW 
<quantity id="C06190D8DB024E25A3D77F84FBF23090" box="[949,1058,921,947]" metricMagnitude="-3" metricUnit="m" metricValue="1.16" pageId="5" pageNumber="56" unit="mm" value="1.16">1.16 mm</quantity>
, EL 
<quantity id="C06190D8DB024E25A43E7F84FB183090" box="[1116,1224,921,947]" metricMagnitude="-3" metricUnit="m" metricValue="5.35" pageId="5" pageNumber="56" unit="mm" value="5.35">5.35 mm</quantity>
, Ediam 
<quantity id="C06190D8DB024E25A5497F84FA473090" box="[1323,1431,921,947]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" pageId="5" pageNumber="56" unit="mm" value="0.41">0.41 mm</quantity>
, Edist 
<quantity id="C06190D8DB024E25A0B87FDDFE9630F9" box="[218,326,960,986]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="5" pageNumber="56" unit="mm" value="0.35">0.35 mm</quantity>
