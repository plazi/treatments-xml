<taxonomicName id="52BC1EE08C969F88CF03AEB3203008E3" LSID="http://zoobank.org/A252B7DB-459E-44BB-97A8-72AC6EC76161" authority="Chen &amp; Johnson" class="Insecta" family="Scelionidae" genus="Chromoteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chromoteleia feng" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="feng">Chromoteleia feng Chen &amp; Johnson</taxonomicName>
