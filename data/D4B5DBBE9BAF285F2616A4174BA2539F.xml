<taxonomicName id="1DED9A3D650FFAC8CC0631F855405651" LSID="http://zoobank.org/53BD9379-0092-4255-A872-311C8795407F" authority="Kang" authorityName="Förster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lytopylus angelagonzalezae" order="Hymenoptera" pageId="7" pageNumber="100" phylum="Arthropoda" rank="species" species="angelagonzalezae">Lytopylus angelagonzalezae Kang</taxonomicName>
