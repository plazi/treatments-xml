and 
<taxonomicName id="4C0E99F25209CE217F4B8A01EFD39F75" authorityName="Akkari &amp; Enghoff" authorityYear="2008" box="[878,1005,1072,1095]" class="Diplopoda" family="Julidae" genus="Afropachyiulus" kingdom="Animalia" order="Julida" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="mauriesi">A. mauriesi</taxonomicName>
