<td id="76EF894F520931D87D9F8BF4EEFF9EE8" box="[442,705,1477,1498]" gridcol="1" gridrow="8" pageId="6" pageNumber="35">4 – 6</td>
<td id="76EF894F520931D87EC18BF4EFCF9EE8" box="[740,1009,1477,1498]" gridcol="2" gridrow="8" pageId="6" pageNumber="35">2</td>
<td id="76EF894F520931D8783D8BF4E9B99EE8" box="[1048,1415,1477,1498]" gridcol="3" gridrow="8" pageId="6" pageNumber="35">2</td>
</tr>
