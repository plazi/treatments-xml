<td id="76EF894F520931D87EC18BDCEFCF9D2D" box="[740,1009,1517,1567]" gridcol="2" gridrow="9" pageId="6" pageNumber="35">ca. 30</td>
<td id="76EF894F520931D8783D8BDCE9B99D2D" box="[1048,1415,1517,1567]" gridcol="3" gridrow="9" pageId="6" pageNumber="35">ca. 12</td>
</tr>
