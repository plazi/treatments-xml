<taxonomicName id="ED74DEADCB6DB83102AE5DC069831027" class="Insecta" family="Staphylinidae" genus="Imirus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Imirus" order="Coleoptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="genus">Imirus</taxonomicName>
