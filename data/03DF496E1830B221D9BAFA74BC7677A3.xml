and southwestern 
<collectingCountry id="F361B8E81837B221DC4AFEB0BBEC776F" box="[1383,1432,261,284]" name="Iran" pageId="12" pageNumber="173">Iran</collectingCountry>
; Levant (
<collectingCountry id="F361B8E81837B221D9D9FE92BF417733" box="[244,309,295,320]" name="Syria" pageId="12" pageNumber="173">Syria</collectingCountry>
