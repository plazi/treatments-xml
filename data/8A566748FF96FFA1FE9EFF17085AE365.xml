<taxonomicName id="C5FFADDDFF96FFA1FDBBFAC70E86E779" box="[534,717,1302,1323]" class="Amphibia" family="Ceratobatrachidae" genus="Tirahanulap" kingdom="Animalia" order="Anura" pageId="16" pageNumber="146" phylum="Chordata" rank="species" species="polillensis">
