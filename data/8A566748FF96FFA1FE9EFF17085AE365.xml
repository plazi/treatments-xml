<taxonomicName id="C5FFADDDFF96FFA1FDE5FB290D55E77E" class="Amphibia" family="Ceratobatrachidae" genus="Tirahanulap" kingdom="Animalia" order="Anura" pageId="16" pageNumber="146" phylum="Chordata" rank="variety" species="lawtoni" variety="montanus">
