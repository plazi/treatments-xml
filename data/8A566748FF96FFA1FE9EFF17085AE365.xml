<taxonomicName id="C5FFADDDFF96FFA1FE15FB290E79E75F" box="[440,562,1272,1293]" class="Amphibia" family="Ceratobatrachidae" genus="Tirahanulap" kingdom="Animalia" order="Anura" pageId="16" pageNumber="146" phylum="Chordata" rank="species" species="isarog">
