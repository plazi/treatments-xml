<taxonomicName class="Insecta" family="Aphididae" genus="Atheroides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheroides serrulatus" order="Hemiptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="serrulatus">Atheroides serrulatus</taxonomicName>
