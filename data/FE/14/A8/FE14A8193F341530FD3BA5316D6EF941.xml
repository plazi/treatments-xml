species 
<taxonomicName authority="Sowerby I, 1825" authorityName="Sowerby I" authorityYear="1825" box="[861,1298,994,1024]" class="Gastropoda" family="Turbinidae" genus="Turbo" higherTaxonomySource="CoL" kingdom="Animalia" order="Trochida" pageId="3" pageNumber="16" phylum="Mollusca" rank="species" species="bicarinata">
