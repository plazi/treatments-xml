<subSubSection pageId="0" pageNumber="15" type="composition">
<paragraph pageId="0" pageNumber="15">Composition.</paragraph>
<paragraph pageId="0" pageNumber="15">
<taxonomicName authority="Zhao &amp; Li, 2020" authorityName="Zhao &amp; Li" authorityYear="2020" class="Arachnida" family="Telemidae" genus="Mekonglema" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Mekonglema bailang" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="bailang">
<emphasis italics="true" pageId="0" pageNumber="15">Mekonglema bailang</emphasis>
</taxonomicName>
sp. nov., 
<taxonomicName genus="M." lsidName="M. kaorao" pageId="0" pageNumber="15" rank="species" species="kaorao">
<emphasis italics="true" pageId="0" pageNumber="15">M. kaorao</emphasis>
</taxonomicName>
sp. nov., 
<taxonomicName genus="M." lsidName="M. walayaku" pageId="0" pageNumber="15" rank="species" species="walayaku">
<emphasis italics="true" pageId="0" pageNumber="15">M. walayaku</emphasis>
</taxonomicName>
sp. nov., 
<taxonomicName genus="M." lsidName="M. xinpingi" pageId="0" pageNumber="15" rank="species" species="xinpingi">
<emphasis italics="true" pageId="0" pageNumber="15">M. xinpingi</emphasis>
</taxonomicName>
comb. nov., and 
<taxonomicName genus="M." lsidName="M. yan" pageId="0" pageNumber="15" rank="species" species="yan">
<emphasis italics="true" pageId="0" pageNumber="15">M. yan</emphasis>
