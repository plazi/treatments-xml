<paragraph blockId="5.[151,1437,151,789]" box="[199,983,366,393]" pageId="5" pageNumber="534">
<emphasis bold="true" box="[199,347,366,393]" pageId="5" pageNumber="534">
<typeStatus box="[199,258,367,393]" pageId="5" pageNumber="534">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="(Carter, 1943) Freze, 1963" authorityName="Freze" authorityYear="1963" baseAuthorityName="Carter" baseAuthorityYear="1943" box="[361,979,367,393]" class="Cestoda" family="Proteocephalidae" genus="Kapsulotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Proteocephalidea" pageId="5" pageNumber="534" phylum="Platyhelminthes" rank="species" species="sandgroundi">
