<taxonomicName authority="Freze, 1963" authorityName="Freze" authorityYear="1963" box="[500,774,1691,1715]" class="Cestoda" family="Proteocephalidae" genus="Capsulotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Proteocephalidea" pageId="4" pageNumber="533" phylum="Platyhelminthes" rank="genus">
