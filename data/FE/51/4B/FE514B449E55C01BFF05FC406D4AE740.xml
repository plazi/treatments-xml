<subSubSection box="[381,626,1000,1027]" pageId="17" pageNumber="356" type="vernacular_name">
<paragraph blockId="17.[381,626,1000,1027]" box="[381,626,1000,1027]" pageId="17" pageNumber="356">
<heading box="[381,626,1000,1027]" fontSize="12" level="2" pageId="17" pageNumber="356" reason="1">
<taxonomicName authority="Knabenkraut" authorityName="Knabenkraut" box="[381,626,1000,1027]" class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="17" pageNumber="356" phylum="Tracheophyta" rank="genus">Orchis, Knabenkraut</taxonomicName>
</heading>
</paragraph>
