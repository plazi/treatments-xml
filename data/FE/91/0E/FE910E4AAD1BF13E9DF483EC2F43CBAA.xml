<taxonomicName class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina skipetarica" order="Stylommatophora" pageId="74" pageNumber="75" phylum="Mollusca" rank="species" species="skipetarica">Montenegrina skipetarica</taxonomicName>
