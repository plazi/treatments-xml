<location LSID="urn:lsid:plazi:treatment:FE0387F2FF9FFFF66ACAF8D774B3FD0E:7375603FFF83FFF66F3BFF777426FF16" country="Australia" county="Plettenberg Bay" municipality="Canberra" name="Australian National Insect Collection" pageId="16" pageNumber="481">Australian National Insect Collection</location>
, 
<collectingMunicipality box="[550,653,187,213]" pageId="16" pageNumber="481">Canberra</collectingMunicipality>
(
<collectingCountry box="[670,779,187,214]" name="Australia" pageId="16" pageNumber="481">Australia</collectingCountry>
), and each microslide containing one female labelled as: 
<taxonomicName authority="Halliday" authorityName="Halliday" authorityYear="2005" box="[151,492,223,249]" class="Arachnida" family="Laelapidae" genus="Hypoaspis" kingdom="Animalia" order="Mesostigmata" pageId="16" pageNumber="481" phylum="Arthropoda" rank="species" species="muellerae">
<emphasis box="[151,388,223,249]" italics="true" pageId="16" pageNumber="481">Hypoaspis muellerae</emphasis>
Halliday
</taxonomicName>
, 
<collectingCountry box="[502,646,223,249]" name="South Africa" pageId="16" pageNumber="481">South Africa</collectingCountry>
, 
<collectingCounty box="[657,836,223,249]" pageId="16" pageNumber="481">Plettenberg Bay</collectingCounty>
, 
<date box="[847,1026,223,249]" pageId="16" pageNumber="481" value="1994-08-18">
<collectingDate box="[847,1026,223,249]" pageId="16" pageNumber="481" value="1994-08-18">18 August 1994</collectingDate>
</date>
, 
<collectorName box="[1036,1140,223,249]" pageId="16" pageNumber="481">Roadside</collectorName>
picnic area, TK. 
<collectorName box="[1332,1374,223,249]" pageId="16" pageNumber="481">Qin</collectorName>
