<paragraph blockId="16.[151,1437,150,717]" pageId="16" pageNumber="481">
<emphasis bold="true" box="[151,406,150,176]" pageId="16" pageNumber="481">Specimens examined.</emphasis>
Five 
<typeStatus box="[469,576,151,177]" pageId="16" pageNumber="481">paratypes</typeStatus>
(
<specimenCode box="[591,789,150,177]" collectionCode="ANIC" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" lsid="urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="16" pageNumber="481">ANIC 51-002833</specimenCode>
– 
<specimenCode box="[804,925,150,177]" collectionCode="ANIC" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" lsid="urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="16" pageNumber="481">51-002837</specimenCode>
