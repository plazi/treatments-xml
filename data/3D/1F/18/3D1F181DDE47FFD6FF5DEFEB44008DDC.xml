<location LSID="urn:lsid:plazi:treatment:3D1F181DDE47FFD6FF5DEFEB44008DDC:B069FFD0DE47FFD1FE83EE7B44C48C3C" box="[364,471,1381,1405]" municipality="Patria" name="Piemonte" pageId="3" pageNumber="4">Piemonte</location>
(
<location LSID="urn:lsid:plazi:treatment:3D1F181DDE47FFD6FF5DEFEB44008DDC:B069FFD0DE47FFD1FE0BEE7B46038C3C" box="[484,784,1381,1405]" municipality="Patria" name="Champoluc in Valle Aosta" pageId="3" pageNumber="4">Champoluc in Valle Aosta</location>
