<location LSID="urn:lsid:plazi:treatment:3D1F181DDE47FFD6FF5DEFEB44008DDC:B069FFD0DE47FFD1FC82EEAE46918C89" box="[877,898,1456,1480]" country="Switzerland" county="St. Bernhard Sud" municipality="Gr. St." name="In" pageId="3" pageNumber="4">In</location>
