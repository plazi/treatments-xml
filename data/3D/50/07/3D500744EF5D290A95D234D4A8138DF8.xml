<taxonomicName authorityName="Eggers" authorityYear="1923" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus morio" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="morio">
<emphasis italics="true" pageId="0" pageNumber="1">Xyleborus morio</emphasis>
