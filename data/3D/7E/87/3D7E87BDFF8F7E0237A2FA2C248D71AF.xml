, 
<taxonomicName box="[875,1004,1933,1958]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="subFamily" subFamily="Idiocerinae">Idiocerinae</taxonomicName>
, 
<taxonomicName box="[1016,1165,1933,1958]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="subFamily" subFamily="Macropsinae">Macropsinae</taxonomicName>
), 
<taxonomicName box="[1184,1327,1933,1958]" class="Insecta" family="Delphacidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="family">Delphacidae</taxonomicName>
, 
<taxonomicName box="[1339,1432,1933,1958]" class="Insecta" family="Flatidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="family">Flatidae</taxonomicName>
, 
<taxonomicName box="[151,232,1969,1994]" class="Insecta" family="Issidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="family">Issidae</taxonomicName>
, 
<taxonomicName box="[243,380,1969,1994]" class="Insecta" family="Lophopidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="family">Lophopidae</taxonomicName>
, 
<taxonomicName box="[390,552,1969,1994]" class="Insecta" family="Tropiduchidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="526" phylum="Arthropoda" rank="family">Tropiduchidae</taxonomicName>
(
<bibRefCitation author="Guglielmino" box="[567,869,1969,1994]" pageId="1" pageNumber="526" refString="Guglielmino, A. &amp; Olmi, M. (1997) A host-parasite catalog of world Dryinidae (Hymenoptera: Chrysidoidea). Contributions on Entomology, International, 2 (2), 165 - 298." type="journal article" year="1997">
