(Bedel, 
<number box="[744,803,1182,1206]" pageId="1" pageNumber="287" value="1901.0">1901</number>
)
</taxonomicName>
, 
<taxonomicName authority="Parry, 1986" authorityName="Parry" authorityYear="1986" box="[825,1125,1181,1206]" class="Insecta" family="Chrysomelidae" genus="Crepidodera" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="287" phylum="Arthropoda" rank="species" species="populivora">
