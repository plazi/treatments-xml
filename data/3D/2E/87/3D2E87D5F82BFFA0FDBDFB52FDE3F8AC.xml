<materialsCitation ID-GBIF-Occurrence="2625229302" location="NORFOLK" pageId="37" pageNumber="38" specimenCount="1" specimenCount-female="1" stateProvince="NORFOLK" typeStatus="holotype">
<paragraph blockId="37.[189,1399,1744,1876]" box="[234,348,1744,1770]" pageId="37" pageNumber="38">
<emphasis bold="true" box="[234,348,1744,1770]" pageId="37" pageNumber="38">
<typeStatus box="[234,348,1744,1770]" pageId="37" pageNumber="38">Holotype</typeStatus>
</emphasis>
</paragraph>
<paragraph blockId="37.[189,1399,1744,1876]" pageId="37" pageNumber="38">
<collectingRegion box="[189,328,1779,1805]" country="United Kingdom" name="Norfolk" pageId="37" pageNumber="38">NORFOLK</collectingRegion>
1: Stn. DW1704, single, unbranched colony, 
<quantity box="[898,961,1779,1805]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="37" pageNumber="38" unit="cm" value="2.0">2 cm</quantity>
