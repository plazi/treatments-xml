. 
<taxonomicName authority="Vervoort, 1993" authorityName="Vervoort" authorityYear="1993" box="[402,857,1649,1675]" class="Hydrozoa" family="Sertulariidae" genus="Gonaxia" kingdom="Animalia" order="Leptothecata" pageId="9" pageNumber="10" phylum="Cnidaria" rank="species" species="amphorifera">
