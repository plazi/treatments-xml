<subSubSection pageId="8" pageNumber="9" type="materials_examined">
<paragraph blockId="8.[189,426,727,753]" box="[189,426,727,753]" pageId="8" pageNumber="9">
<heading bold="true" box="[189,426,727,753]" fontSize="11" level="3" pageId="8" pageNumber="9" reason="3">
