and 
<taxonomicName class="Insecta" family="Staphylinidae" genus="Papuapsenius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papuapsenius magnus" order="Coleoptera" pageId="17" pageNumber="72" phylum="Arthropoda" rank="species" species="magnus">Papuapsenius magnus</taxonomicName>
