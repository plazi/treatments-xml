<paragraph pageId="null" pageNumber="392">
<pageBreakToken pageId="null" pageNumber="392" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName authority="L." class="Magnoliopsida" family="Campanulaceae" genus="Campanula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="392" phylum="Tracheophyta" rank="species" species="rotundifolia">
Campanula 
<normalizedToken originalValue="rotundifólia" pageId="null" pageNumber="392">rotundifolia</normalizedToken>
<authorityName pageId="null" pageNumber="392">L.</authorityName>
</taxonomicName>
</paragraph>
