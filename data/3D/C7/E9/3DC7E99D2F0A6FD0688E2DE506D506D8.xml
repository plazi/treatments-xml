<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33731" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole exarata Emery" lsidName-HNS="Pheidole exarata Emery" order="Hymenoptera" pageNumber="700" phylum="Arthropoda" rank="species" species="exarata">exarata</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182030" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole excubitor Wilson" lsidName-HNS="Pheidole excubitor Wilson" order="Hymenoptera" pageNumber="700" phylum="Arthropoda" rank="species" species="excubitor">excubitor</taxonomicName>
