, BS= 
<number box="[1051,1106,781,807]" pageId="15" pageNumber="44" value="0.95">0.95</number>
; 
<emphasis box="[1117,1149,781,806]" italics="true" pageId="15" pageNumber="44">
t- 
<number box="[1134,1149,781,806]" pageId="15" pageNumber="44" value="2.0">2</number>
</emphasis>
slim and long, 
<emphasis box="[1330,1363,781,806]" italics="true" pageId="15" pageNumber="44">
