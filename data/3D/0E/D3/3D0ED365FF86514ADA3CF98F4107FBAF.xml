<paragraph blockId="10.[835,1443,1615,1923]" box="[835,1086,1615,1639]" pageId="10" pageNumber="41">
<taxonomicName authority="Stark. This" authorityName="Stark. This" box="[835,1086,1615,1639]" class="Insecta" family="Perlidae" genus="Perlesta" kingdom="Animalia" order="Plecoptera" pageId="10" pageNumber="41" phylum="Arthropoda" rank="species" species="shubuta">
