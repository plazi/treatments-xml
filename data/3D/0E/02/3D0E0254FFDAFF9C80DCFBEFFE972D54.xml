<taxonomicName authority="Meigen, 1824" authorityName="Meigen" authorityYear="1824" box="[1001,1399,376,402]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="picipes">
