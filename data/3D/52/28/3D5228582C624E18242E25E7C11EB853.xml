<subSubSection pageId="1" pageNumber="67" type="type species">
<paragraph pageId="1" pageNumber="67">Type species.</paragraph>
<paragraph pageId="1" pageNumber="67">
<taxonomicName class="Insecta" family="Dictyopharidae" genus="Dictyophara" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dictyophara nigrosuturalis" order="Hemiptera" pageId="1" pageNumber="67" phylum="Arthropoda" rank="species" species="nigrosuturalis">Dictyophara nigrosuturalis</taxonomicName>
