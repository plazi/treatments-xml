<subSubSection pageId="null" pageNumber="156" type="vernacular_name">
<paragraph pageId="null" pageNumber="156">
<normalizedToken originalValue="Rundblättrige" pageId="null" pageNumber="156">Rundblaettrige</normalizedToken>
Minze
</paragraph>
