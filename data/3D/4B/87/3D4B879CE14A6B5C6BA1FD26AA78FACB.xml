<taxonomicName authorityName="Larivière &amp; Larochelle" authorityYear="2016" baseAuthorityName="White" baseAuthorityYear="1878" box="[473,696,2004,2028]" class="Insecta" family="Saldidae" genus="Kiwisaldula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="474" phylum="Arthropoda" rank="species" species="laelaps">
