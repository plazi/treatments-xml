<taxonomicName authorityName="Larivière &amp; Larochelle" authorityYear="2016" box="[850,985,1968,1992]" class="Insecta" family="Saldidae" genus="Kiwisaldula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="474" phylum="Arthropoda" rank="genus">
