<subSubSection pageId="1" pageNumber="142" type="multiple">
<paragraph pageId="1" pageNumber="142">Taxon classification Animalia Polydesmida Dalodesmidae</paragraph>
</subSubSection>
<subSubSection pageId="1" pageNumber="142" type="nomenclature">
<paragraph pageId="1" pageNumber="142">
Genus 
<taxonomicName LSID="http://zoobank.org/09602805-528E-411A-9C96-B2EB11037259" authority="Mesibov" class="Diplopoda" family="Dalodesmidae" genus="Cernethia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cernethia" order="Polydesmida" pageId="1" pageNumber="142" phylum="Arthropoda" rank="genus">Cernethia Mesibov</taxonomicName>
<taxonomicNameLabel pageId="1" pageNumber="142">gen. n.</taxonomicNameLabel>
</paragraph>
</subSubSection>
<subSubSection pageId="1" pageNumber="142" type="type species">
<paragraph pageId="1" pageNumber="142">Type species.</paragraph>
<paragraph pageId="1" pageNumber="142">
<taxonomicName class="Diplopoda" family="Dalodesmidae" genus="Cernethia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cernethia inopinata" order="Polydesmida" pageId="1" pageNumber="142" phylum="Arthropoda" rank="species" species="inopinata">Cernethia inopinata</taxonomicName>
sp. n., by present designation.
</paragraph>
</subSubSection>
<subSubSection pageId="1" pageNumber="142" type="other assigned species">
