<paragraph blockId="33.[151,461,152,211]" lastBlockId="33.[240,775,267,289]" pageId="33" pageNumber="34">
<emphasis bold="true" box="[151,461,186,211]" pageId="33" pageNumber="34">Broad-winged Snob-noses</emphasis>
urn:lsid: 
<taxonomicName box="[318,434,267,289]" class="Insecta" kingdom="Animalia" order="Orthoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
