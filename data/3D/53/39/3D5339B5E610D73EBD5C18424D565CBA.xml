<taxonomicName class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta renominata" order="Coleoptera" pageId="5" pageNumber="222" phylum="Arthropoda" rank="species" species="renominata">Atheta renominata</taxonomicName>
