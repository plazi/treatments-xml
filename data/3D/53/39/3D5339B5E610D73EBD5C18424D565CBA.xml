<taxonomicName class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta" order="Coleoptera" pageId="5" pageNumber="222" phylum="Arthropoda" rank="genus">Atheta</taxonomicName>
, not 
<taxonomicName class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta terminalis" order="Coleoptera" pageId="5" pageNumber="222" phylum="Arthropoda" rank="species" species="terminalis">Atheta terminalis</taxonomicName>
