<paragraph blockId="69.[151,1436,1050,1147]" pageId="69" pageNumber="70">
<materialsCitation ID-GBIF-Occurrence="1455384298" location="A" pageId="69" pageNumber="70" specimenCount="1" stateProvince="A">
