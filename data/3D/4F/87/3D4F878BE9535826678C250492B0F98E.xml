<collectingRegion box="[537,678,1334,1359]" country="Iran" name="Kermanshah" pageId="7" pageNumber="318">Kermanshah</collectingRegion>
(
<bibRefCitation author="Jalilvand" box="[696,938,1334,1359]" pageId="7" pageNumber="318" refString="Jalilvand, K., Shirazi, M., Fallahzadeh, M., Vahedi, M. H. A., Samih, M. A. &amp; Moeini Naghadeh, N. (2014) Survey of natural enemies of mealybug species (Hemiptera, Pseudococcidae) in Kermanshah Province, western Iran to inform biological control research. Journal of the Entomological Research Society, 16 (3), 1 - 10." type="journal article" year="2014">
Jalilvand 
<emphasis box="[809,861,1334,1359]" italics="true" pageId="7" pageNumber="318">et al</emphasis>
. 2014
</bibRefCitation>
), 
<collectingRegion box="[960,1053,1335,1358]" country="Iran" name="Kerman" pageId="7" pageNumber="318">Kerman</collectingRegion>
, 
<collectingRegion box="[1068,1189,1334,1359]" country="Iran" name="Khuzestan" pageId="7" pageNumber="318">Khuzestan</collectingRegion>
, 
<collectingRegion box="[1203,1344,1334,1359]" country="Iran" name="Mazandaran" pageId="7" pageNumber="318">Mazandaran</collectingRegion>
, 
<collectingRegion box="[1358,1436,1334,1359]" country="Iran" name="Zanjan" pageId="7" pageNumber="318">Zanjan</collectingRegion>
