<taxonomicName baseAuthorityName="Dunker" baseAuthorityYear="1847" class="Gastropoda" family="Odontostomidae" genus="Bahiensis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="41" pageNumber="42" phylum="Mollusca" rank="species" species="ringens">
