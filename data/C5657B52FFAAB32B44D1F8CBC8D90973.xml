locality Concepcion Bay, 
<collectingRegion id="8F0804A6FFA4B32B40C4FE7ECD340AE3" box="[1154,1330,439,463]" country="Mexico" name="Baja California" pageId="56" pageNumber="57">Baja California</collectingRegion>
