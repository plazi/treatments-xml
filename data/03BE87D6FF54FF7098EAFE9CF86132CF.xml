<quantity id="4CEF9B25FF54FF709B47FE9CF9C6318C" box="[826,903,366,393]" metricMagnitude="-2" metricUnit="m" metricValue="8.5" pageId="197" pageNumber="198" unit="cm" value="8.5">8.5 cm</quantity>
