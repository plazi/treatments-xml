<taxonomicName baseAuthorityName="Lucas" baseAuthorityYear="1871" box="[449,661,889,914]" class="Insecta" family="Chalcididae" genus="Euchalcis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="259" phylum="Arthropoda" rank="species" species="unicolor">
