<subSubSection lastPageId="4" lastPageNumber="59" pageId="3" pageNumber="58" type="materials_examined">
<paragraph blockId="3.[151,1437,1146,2012]" lastBlockId="4.[151,1436,152,378]" lastPageId="4" lastPageNumber="59" pageId="3" pageNumber="58">
<emphasis bold="true" box="[198,314,1906,1932]" pageId="3" pageNumber="58">Material.</emphasis>
