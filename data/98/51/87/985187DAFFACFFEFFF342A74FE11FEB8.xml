<collectingCountry box="[1354,1436,1936,1961]" name="Taiwan" pageId="26" pageNumber="327">Taiwan</collectingCountry>
(
<quantity box="[158,291,1973,1997]" metricMagnitude="2" metricUnit="m" metricValue="4.1049999999999995" metricValueMax="4.4" metricValueMin="3.81" pageId="26" pageNumber="327" unit="m" value="410.5" valueMax="440.0" valueMin="381.0">381–440 m</quantity>
