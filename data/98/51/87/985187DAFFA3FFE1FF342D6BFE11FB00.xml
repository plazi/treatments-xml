<bibRefCitation author="Saito" box="[294,422,788,813]" pageId="21" pageNumber="322" refId="ref16415" refString="Saito, H. (2001) Shallow-water chitons (Мollusca: Polyplacophora) of Hachijo Island, Izu Islands, Japan. Memoir of the National Sсienсe Museum, Tokyo, 37, 193 - 202." type="journal article" year="2001">Saito 2001</bibRefCitation>
), Amami-Oshima 
<collectingCountry box="[631,702,788,813]" name="Iceland" pageId="21" pageNumber="322">Island</collectingCountry>
, 
<quantity box="[715,842,788,812]" metricMagnitude="2" metricUnit="m" metricValue="2.995" metricValueMax="3.11" metricValueMin="2.88" pageId="21" pageNumber="322" unit="m" value="299.5" valueMax="311.0" valueMin="288.0">288–311 m</quantity>
(
<bibRefCitation author="Saito" box="[858,985,788,813]" pageId="21" pageNumber="322" refId="ref16453" refString="Saito, H. (2005) Shelf and bathyl chitons (Мollusca: Polyplacophora) from the Nansei Islands, Southwestern Japan. National Sсienсe Museum Monograph, 29, 101 - 113." type="journal article" year="2005">Saito 2005</bibRefCitation>
