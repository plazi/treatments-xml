<taxonomicName authority=", Ipomoea" authorityName="Ipomoea" box="[647,906,692,717]" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Solanales" pageId="14" pageNumber="315" phylum="Tracheophyta" rank="genus">
