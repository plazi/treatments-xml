<subSubSection pageId="2" pageNumber="43" type="nomenclature">
<paragraph pageId="2" pageNumber="43">
<taxonomicName LSID="http://zoobank.org/0785483E-E218-48BB-8DD9-19BAEE8E6D0A" authority="Smolis &amp; Kuznetsova, 2018" authorityName="Smolis &amp; Kuznetsova" authorityYear="2018" class="Entognatha" family="Neanuridae" genus="Paravietnura" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paravietnura" order="Collembola" pageId="2" pageNumber="43" phylum="Arthropoda" rank="genus">Paravietnura</taxonomicName>
