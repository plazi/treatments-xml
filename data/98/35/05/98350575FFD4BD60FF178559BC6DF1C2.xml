<collectingRegion box="[1081,1174,1572,1598]" country="Indonesia" name="Sumatera Utara" pageId="1" pageNumber="68">Sumatra</collectingRegion>
(Simeulue Island), 
<collectingRegion box="[1395,1447,1573,1599]" country="Indonesia" name="Jawa Timur" pageId="1" pageNumber="68">Java</collectingRegion>
, Borneo, 
<collectingRegion box="[232,336,1608,1634]" country="Indonesia" name="Sulawesi Utara" pageId="1" pageNumber="68">Sulawesi</collectingRegion>
, the 
<collectingCountry box="[389,565,1608,1634]" name="Indonesia" pageId="1" pageNumber="68">Maluku Islands</collectingCountry>
