<taxonomicName authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[464,652,297,320]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="agnae">
