, L-N- 
<number box="[742,828,943,967]" pageId="108" pageNumber="109" value="253250.0">253250</number>
, 
<number box="[845,932,943,967]" pageId="108" pageNumber="109" value="449700.0">449700</number>
, “ 
<collectionCode box="[960,1036,943,967]" country="Costa Rica" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="108" pageNumber="109">INBio</collectionCode>
: 
<collectingCountry box="[1052,1226,943,967]" name="Costa Rica" pageId="108" pageNumber="109">
<collectionCode box="[1052,1142,943,967]" pageId="108" pageNumber="109">COSTA</collectionCode>
