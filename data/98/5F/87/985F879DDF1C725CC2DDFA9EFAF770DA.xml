<taxonomicName authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[563,624,1969,1993]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="127" pageNumber="128" phylum="Arthropoda" rank="species" species="dicax">
