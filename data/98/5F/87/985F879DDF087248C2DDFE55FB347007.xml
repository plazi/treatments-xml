<taxonomicName authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[469,671,1716,1740]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="phoebe">
