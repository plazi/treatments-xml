<taxonomicName authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[463,654,1907,1931]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="94" pageNumber="95" phylum="Arthropoda" rank="species" species="caedis">
