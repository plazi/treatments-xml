<paragraph blockId="38.[151,1437,151,2017]" pageId="38" pageNumber="39">
<typeStatus box="[199,308,1670,1694]" pageId="38" pageNumber="39">Paratypes</typeStatus>
(
<number box="[325,354,1669,1693]" pageId="38" pageNumber="39" value="12.0">12</number>
