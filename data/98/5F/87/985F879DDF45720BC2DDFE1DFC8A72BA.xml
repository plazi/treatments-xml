<number box="[410,441,1842,1866]" pageId="39" pageNumber="40" value="10.0">10</number>
). 
<taxonomicName box="[462,681,1843,1867]" class="Insecta" family="Blastobasidae" genus="Blastobasis" kingdom="Animalia" order="Lepidoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="xiphiae">
<emphasis box="[462,681,1843,1867]" italics="true" pageId="39" pageNumber="40">Blastobasis xiphiae</emphasis>
</taxonomicName>
