<collectionCode box="[550,631,871,895]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="146" pageNumber="147">USNM</collectionCode>
<number box="[637,710,871,895]" pageId="146" pageNumber="147" value="83502.0">83502</number>
