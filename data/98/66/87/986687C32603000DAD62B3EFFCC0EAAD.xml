<paragraph blockId="3.[93,1049,586,1662]" pageId="3" pageNumber="174">
<emphasis box="[134,238,586,610]" italics="true" pageId="3" pageNumber="174">Material.</emphasis>
<materialsCitation collectingDate="1997-12" collectionCode="S, E, J" country="Mozambique" location="More" municipality="Tofo" pageId="3" pageNumber="174" specimenCount="1" stateProvince="Inhambane">
<location LSID="urn:lsid:plazi:treatment:986687C32603000DAD62B3EFFCC0EAAD:1510600E2603000BAD14B07DFEB2EC63" box="[265,328,586,609]" country="Mozambique" municipality="Tofo" name="More" pageId="3" pageNumber="174" stateProvince="Inhambane">More</location>
