<taxonomicName authority="Larochelle and Lariviere" authorityName="Larochelle and Lariviere" authorityYear="2015" box="[192,796,1186,1210]" class="Insecta" family="Carabidae" genus="Bembidion" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="14" phylum="Arthropoda" rank="species" species="waimarama" status="new species">
