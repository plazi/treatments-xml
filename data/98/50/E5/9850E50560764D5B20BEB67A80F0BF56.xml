[[ male ]] (= 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136387" class="Insecta" family="Formicidae" genus="Carebara" kingdom="Animalia" lsidName="Carebara dux Smith" lsidName-HNS="Carebara dux Smith" order="Hymenoptera" pageNumber="5" phylum="Arthropoda" rank="species" species="dux">Carebara dux Sm.</taxonomicName>
