</taxonomicName>
on safflower, 
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Carthamus" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="39" phylum="Tracheophyta" rank="species" species="oxycantha">
