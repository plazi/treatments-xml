<taxonomicName authority="martinae" authorityName="martinae Heller" authorityYear="2004" box="[437,784,1897,1922]" class="Insecta" family="Tettigoniidae" genus="Poecilimon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="463" phylum="Arthropoda" rank="subSpecies" species="martinae" subSpecies="martinae">
