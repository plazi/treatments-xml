<taxonomicName id="BBEB5FAE2E76EF69FE59FCADFD317D74" baseAuthorityName="Nardo" baseAuthorityYear="1827" box="[511,644,837,861]" class="Elasmobranchii" family="Carcharhinidae" genus="Carcharhinus" kingdom="Animalia" order="Carcharhiniformes" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="plumbeus">
