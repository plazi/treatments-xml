<taxonomicName id="B718C933D884106E6D209D6B72AFA1D0" authority="Ashmead, 1902" authorityName="Ashmead" authorityYear="1902" class="Insecta" family="Chrysididae" genus="Pseudomalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pseudomalus" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
