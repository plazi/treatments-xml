<taxonomicName id="11774EEEECA3E789C16C73591141E238" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="214" phylum="Arthropoda" rank="species" species="agamus">
<pageBreakToken id="697B9018D54E7A7E05755D5E6D208C71" pageId="0" pageNumber="214" start="start">Eupolybothrus</pageBreakToken>
agamus
</taxonomicName>
