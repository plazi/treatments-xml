<taxonomicName id="4C0F4D457B1E0360EFE7FDC6FD89FD7E" authorityName="C. Linnaeus" authorityYear="1753" box="[472,591,618,643]" class="Polypodiopsida" family="Aspleniaceae" genus="Asplenium" higherTaxonomySource="GBIF" kingdom="Plantae" order="Polypodiales" pageId="102" pageNumber="103" phylum="Tracheophyta" rank="genus">
