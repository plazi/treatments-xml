<subSubSection id="C30D653DFFE4FFA1FF45FD69E2DDFD75" box="[198,1005,711,738]" pageId="4" pageNumber="67" type="etymology">
<paragraph id="8BA836B6FFE4FFA1FF45FD69E2DDFD75" blockId="4.[151,1436,152,738]" box="[198,1005,711,738]" pageId="4" pageNumber="67">
<emphasis id="B963EAA4FFE4FFA1FF45FD69E063FD76" bold="true" box="[198,339,711,737]" pageId="4" pageNumber="67">Etymology.</emphasis>
