<paragraph id="5F603691FFA70D281ADBF9311028F929" blockId="26.[151,1437,1709,2024]" box="[151,1079,1709,1736]" pageId="26" pageNumber="477">
<emphasis id="6DABEA83FFA70D281ADBF931158CF926" bold="true" box="[151,403,1709,1735]" pageId="26" pageNumber="477">General distribution.</emphasis>
<collectingCountry id="27C87601FFA70D281BD6F93115FEF926" box="[410,481,1709,1735]" name="Brazil" pageId="26" pageNumber="477">Brazil</collectingCountry>
, 
<collectingCountry id="27C87601FFA70D281BA1F931165DF926" box="[493,578,1709,1735]" name="Bolivia" pageId="26" pageNumber="477">Bolivia</collectingCountry>
, 
