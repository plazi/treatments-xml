<taxonomicName id="859684E8813B0B30FEFEC42EFDD3D427" authority="Fowler &amp; Bean 1930" authorityName="Fowler &amp; Bean" authorityYear="1930" box="[273,614,582,604]" class="Actinopterygii" family="Apogonidae" genus="Sphaeramia" kingdom="Animalia" order="Perciformes" pageId="35" pageNumber="186" phylum="Chordata" rank="genus">
