</taxonomicName>
, subgenus 
<taxonomicName id="4C784D7CFFA54078B9113B78179FFCB4" authorityName="Macquart" authorityYear="1835" box="[1281,1423,801,822]" class="Insecta" family="Sciomyzidae" genus="Graphomyzina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="69" phylum="Arthropoda" rank="genus">
