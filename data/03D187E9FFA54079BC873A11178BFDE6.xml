<taxonomicName id="4C784D7CFFA54078B8233B781688FCB4" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[1075,1176,801,822]" class="Insecta" family="Sciomyzidae" genus="Pherbellia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="69" phylum="Arthropoda" rank="genus">
