<paragraph blockId="1.[151,1071,476,572]" pageId="1" pageNumber="60">
<taxonomicName authority="Smith &amp; Radcliffe, 1913" authorityName="Smith &amp; Radcliffe" authorityYear="1913" box="[151,631,511,536]" class="Actinopterygii" family="Bythitidae" genus="Pseudonus" kingdom="Animalia" order="Ophidiiformes" pageId="1" pageNumber="60" phylum="Chordata" rank="species" species="platycephalus">
<emphasis box="[151,333,512,536]" italics="true" pageId="1" pageNumber="60">P. platycephalus</emphasis>
(Smith &amp; Radcliffe, 
<number box="[571,624,511,535]" pageId="1" pageNumber="60" value="1913.0">1913</number>
