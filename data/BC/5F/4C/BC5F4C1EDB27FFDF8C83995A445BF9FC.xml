<subSubSection pageId="9" pageNumber="530" type="description">
<paragraph blockId="9.[151,1437,152,327]" box="[151,972,305,327]" pageId="9" pageNumber="530">
<taxonomicName box="[151,552,305,327]" class="Insecta" family="Colletidae" genus="Leioproctus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="530" phylum="Arthropoda" rank="species" species="stigmaticus" subGenus="Tetraglossula">
