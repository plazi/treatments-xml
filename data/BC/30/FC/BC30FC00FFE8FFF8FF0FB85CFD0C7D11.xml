<taxonomicName box="[705,1039,1507,1531]" class="Clitellata" family="Lumbricidae" genus="Dendrodrilus" kingdom="Animalia" order="Crassiclitellata" pageId="11" pageNumber="12" phylum="Annelida" rank="subSpecies" species="rubidus" subSpecies="rubidus">
