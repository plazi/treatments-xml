Figs. 
<number box="[213,227,799,823]" pageId="6" pageNumber="7" value="9.0">9</number>
–11
</figureCitation>
, 
<figureCitation box="[282,313,798,823]" captionStart="FIGURES 64 – 72" captionStartId="31.[151,264,1954,1977]" captionTargetBox="[174,1424,219,1933]" captionTargetId="figure@31.[157,1429,193,1933]" captionTargetPageId="31" captionText="FIGURES 64 – 72. Billaea spp., heads of male in lateral view. 64. B. atkinsoni (Baranov). 65. B. brevicauda sp. nov. 66. B. carinata sp. nov. 67. B. chinensis sp. nov. 68. B. ficorum (Townsend). 69. B. flava sp. nov. 70. B. fortis (Rondani). 71. B. kolomyetzi Mesnil. 72. B. kurahashii sp. nov. Scale bars = 0.5 mm." httpUri="https://zenodo.org/record/288392/files/figure.png" pageId="6" pageNumber="7">66</figureCitation>
, 
<figureCitation box="[324,352,798,823]" captionStart="FIGURES 73 – 81" captionStartId="32.[151,264,1919,1942]" captionTargetBox="[184,1393,193,1879]" captionTargetId="figure@32.[184,1393,193,1880]" captionTargetPageId="32" captionText="FIGURES 73 – 81. Billaea spp., heads of male in anterior view. 73. B. atkinsoni (Baranov). 74. B. brevicauda sp. nov. 75. B. carinata sp. nov. 76. B. chinensis sp. nov. 77. B. ficorum (Townsend). 78. B. flava sp. nov. 79. B. fortis (Rondani). 80. B. kolomyetzi Mesnil. 81. B. kurahashii sp. nov. Scale bars = 0.5 mm." httpUri="https://zenodo.org/record/288393/files/figure.png" pageId="6" pageNumber="7">
<number box="[324,352,798,823]" pageId="6" pageNumber="7" value="75.0">75</number>
