(
<taxonomicName box="[982,1086,1493,1519]" class="Arachnida" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="50" phylum="Arthropoda" rank="order">Araneae</taxonomicName>
: 
<taxonomicName authorityName="Sundevall" authorityYear="1833" box="[1103,1240,1493,1519]" class="Arachnida" family="Theridiidae" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="50" phylum="Arthropoda" rank="family">Theridiidae</taxonomicName>
