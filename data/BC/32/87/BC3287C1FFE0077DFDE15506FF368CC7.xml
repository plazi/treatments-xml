, 
<collectingRegion box="[891,1012,1086,1115]" country="Colombia" name="Cordoba" pageId="1" pageNumber="2">Córdoba</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:BC3287C1FFE0077DFDE15506FF368CC7:3144600CFFE0077EFBB05272FB5E896A" box="[1027,1157,1086,1115]" country="Colombia" name="Monteria" pageId="1" pageNumber="2" stateProvince="Cordoba">Montería</location>
, 
