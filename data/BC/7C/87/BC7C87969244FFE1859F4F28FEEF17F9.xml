<quantity box="[947,1085,1695,1719]" metricMagnitude="-3" metricUnit="m" metricValue="3.8499999999999996" metricValueMax="4.1" metricValueMin="3.6" pageId="16" pageNumber="517" unit="mm" value="3.8499999999999996" valueMax="4.1" valueMin="3.6">3.6–4.1 mm</quantity>
