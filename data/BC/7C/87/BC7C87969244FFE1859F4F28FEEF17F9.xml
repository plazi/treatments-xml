<quantity box="[707,845,1695,1719]" metricMagnitude="-3" metricUnit="m" metricValue="3.5999999999999996" metricValueMax="3.9" metricValueMin="3.3" pageId="16" pageNumber="517" unit="mm" value="3.5999999999999996" valueMax="3.9" valueMin="3.3">3.3–3.9 mm</quantity>
