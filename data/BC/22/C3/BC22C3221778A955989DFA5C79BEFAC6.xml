<heading bold="true" box="[151,817,1431,1457]" fontSize="11" level="1" pageId="73" pageNumber="74" reason="1">
<emphasis bold="true" box="[151,817,1431,1457]" pageId="73" pageNumber="74">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,660,1431,1457]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="narayana" status="new species">
<emphasis bold="true" box="[151,432,1431,1457]" italics="true" pageId="73" pageNumber="74">Hydropsyche narayana</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[673,817,1431,1457]" pageId="73" pageNumber="74" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
