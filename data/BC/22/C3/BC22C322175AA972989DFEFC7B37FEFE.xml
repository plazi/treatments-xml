<paragraph blockId="107.[151,776,311,378]" box="[151,776,311,337]" pageId="107" pageNumber="108">
<heading bold="true" box="[151,776,311,337]" fontSize="11" level="1" pageId="107" pageNumber="108" reason="1">
<emphasis bold="true" box="[151,776,311,337]" pageId="107" pageNumber="108">
<taxonomicName authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,620,311,337]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="kangra" status="new species">
<emphasis bold="true" box="[151,402,311,337]" italics="true" pageId="107" pageNumber="108">Hydropsyche kangra</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel box="[633,776,311,337]" pageId="107" pageNumber="108" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
