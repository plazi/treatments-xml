<subSubSection box="[151,791,1442,1468]" pageId="42" pageNumber="43" type="nomenclature">
<paragraph blockId="42.[151,791,1442,1508]" box="[151,791,1442,1468]" pageId="42" pageNumber="43">
<heading bold="true" box="[151,791,1442,1468]" fontSize="11" level="1" pageId="42" pageNumber="43" reason="1">
<emphasis bold="true" box="[151,791,1442,1468]" pageId="42" pageNumber="43">
<taxonomicName authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,634,1442,1468]" class="Insecta" family="Hydropsychidae" genus="Hydromanicus" kingdom="Animalia" order="Trichoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="naraik" status="new species">
<emphasis bold="true" box="[151,416,1442,1468]" italics="true" pageId="42" pageNumber="43">Hydromanicus naraik</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel box="[647,791,1442,1468]" pageId="42" pageNumber="43" rank="species">new species</taxonomicNameLabel>
</emphasis>
