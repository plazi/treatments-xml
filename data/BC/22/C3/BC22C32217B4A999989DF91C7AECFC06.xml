<subSubSection box="[151,803,1751,1777]" pageId="133" pageNumber="134" type="nomenclature">
<paragraph blockId="133.[151,803,1751,1818]" box="[151,803,1751,1777]" pageId="133" pageNumber="134">
<heading bold="true" box="[151,803,1751,1777]" fontSize="11" level="1" pageId="133" pageNumber="134" reason="1">
<emphasis bold="true" box="[151,803,1751,1777]" pageId="133" pageNumber="134">
<taxonomicName authority="Olah &amp; Schefter" authorityName="Olah &amp; Schefter" authorityYear="2008" box="[151,646,1751,1777]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="yathetima" status="new species">
<emphasis bold="true" box="[151,435,1751,1777]" italics="true" pageId="133" pageNumber="134">Hydropsyche yathetima</emphasis>
Oláh &amp; Schefter
</taxonomicName>
, 
<taxonomicNameLabel box="[659,803,1751,1777]" pageId="133" pageNumber="134" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
