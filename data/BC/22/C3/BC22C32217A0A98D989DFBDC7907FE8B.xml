<paragraph blockId="145.[151,773,1047,1114]" box="[151,773,1047,1073]" pageId="145" pageNumber="146">
<heading bold="true" box="[151,773,1047,1073]" fontSize="11" level="1" pageId="145" pageNumber="146" reason="1">
<emphasis bold="true" box="[151,773,1047,1073]" pageId="145" pageNumber="146">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,617,1047,1073]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="keoda" status="new species">
<emphasis bold="true" box="[151,387,1047,1073]" italics="true" pageId="145" pageNumber="146">Hydropsyche keoda</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[629,773,1047,1073]" pageId="145" pageNumber="146" rank="species">new species</taxonomicNameLabel>
