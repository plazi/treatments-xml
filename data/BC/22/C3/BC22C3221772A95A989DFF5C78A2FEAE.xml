<paragraph blockId="67.[151,760,151,218]" box="[151,760,151,177]" pageId="67" pageNumber="68">
<heading bold="true" box="[151,760,151,177]" fontSize="11" level="1" pageId="67" pageNumber="68" reason="1">
<emphasis bold="true" box="[151,760,151,177]" pageId="67" pageNumber="68">
