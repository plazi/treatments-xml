<subSubSection pageId="148" pageNumber="149" type="etymology">
<paragraph blockId="148.[151,1436,152,658]" pageId="148" pageNumber="149">
<emphasis bold="true" box="[197,337,591,617]" pageId="148" pageNumber="149">Etymology.</emphasis>
<taxonomicName authorityName="Olah &amp; Barnard" authorityYear="2008" box="[344,422,592,617]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="148" pageNumber="149" phylum="Arthropoda" rank="species" species="namea">
