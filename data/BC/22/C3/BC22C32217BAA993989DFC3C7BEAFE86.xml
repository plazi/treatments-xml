<heading bold="true" box="[151,801,1015,1041]" fontSize="11" level="1" pageId="139" pageNumber="140" reason="1">
<emphasis bold="true" box="[151,801,1015,1041]" pageId="139" pageNumber="140">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,643,1015,1041]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="139" pageNumber="140" phylum="Arthropoda" rank="species" species="semkala" status="new species">
<emphasis bold="true" box="[151,415,1015,1041]" italics="true" pageId="139" pageNumber="140">Hydropsyche semkala</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[657,801,1015,1041]" pageId="139" pageNumber="140" rank="species">new species</taxonomicNameLabel>
</emphasis>
