<paragraph blockId="63.[151,1437,152,1658]" pageId="63" pageNumber="64">
<materialsCitation collectingDate="1994-01" collectionCode="NHML" collectorName="M. Erle" country="Malaysia" location="Belum Expedition" municipality="North" pageId="63" pageNumber="64" specimenCount="1" stateProvince="Perak" typeStatus="holotype">
<emphasis bold="true" box="[197,553,1511,1537]" pageId="63" pageNumber="64">
<typeStatus box="[197,311,1511,1537]" pageId="63" pageNumber="64">Holotype</typeStatus>
male: 
<collectingCountry box="[396,553,1511,1537]" name="Malaysia" pageId="63" pageNumber="64">MALAYSIA</collectingCountry>
</emphasis>
, 
<collectingMunicipality box="[567,635,1512,1538]" pageId="63" pageNumber="64">North</collectingMunicipality>
