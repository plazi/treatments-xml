<subSubSection pageId="38" pageNumber="39" type="etymology">
<paragraph blockId="38.[151,1436,152,898]" pageId="38" pageNumber="39">
<emphasis bold="true" box="[197,337,751,777]" pageId="38" pageNumber="39">Etymology.</emphasis>
<taxonomicName authorityName="Olah &amp; Johanson" authorityYear="2008" box="[345,410,752,777]" class="Insecta" family="Hydropsychidae" genus="Hydromanicus" kingdom="Animalia" order="Trichoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="sacso">
