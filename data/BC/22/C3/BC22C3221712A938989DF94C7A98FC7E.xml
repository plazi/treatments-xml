<subSubSection pageId="38" pageNumber="39" type="discussion">
<paragraph blockId="38.[151,1436,152,898]" pageId="38" pageNumber="39">
<emphasis bold="true" box="[197,318,831,857]" pageId="38" pageNumber="39">Remarks.</emphasis>
