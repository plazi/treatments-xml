<heading bold="true" box="[151,784,671,697]" fontSize="11" level="1" pageId="45" pageNumber="46" reason="1">
<emphasis bold="true" box="[151,784,671,697]" pageId="45" pageNumber="46">
<taxonomicName authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,627,671,697]" class="Insecta" family="Hydropsychidae" genus="Hydromanicus" kingdom="Animalia" order="Trichoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="palnis" status="new species">
<emphasis bold="true" box="[151,410,671,697]" italics="true" pageId="45" pageNumber="46">Hydromanicus palnis</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel box="[640,784,671,697]" pageId="45" pageNumber="46" rank="species">new species</taxonomicNameLabel>
</emphasis>
