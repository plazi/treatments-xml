<subSubSection box="[198,1299,191,218]" pageId="151" pageNumber="152" type="etymology">
<paragraph blockId="151.[197,1299,151,218]" box="[198,1299,191,218]" pageId="151" pageNumber="152">
<emphasis bold="true" box="[198,339,191,217]" pageId="151" pageNumber="152">Etymology.</emphasis>
<taxonomicName authorityName="Olah &amp; Barnard" authorityYear="2008" box="[346,455,192,217]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="shillonga">
<emphasis box="[346,455,192,217]" italics="true" pageId="151" pageNumber="152">shillonga</emphasis>
