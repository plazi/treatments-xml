<paragraph blockId="151.[151,775,309,375]" box="[151,775,309,335]" pageId="151" pageNumber="152">
<heading bold="true" box="[151,775,309,335]" fontSize="11" level="1" pageId="151" pageNumber="152" reason="1">
<emphasis bold="true" box="[151,775,309,335]" pageId="151" pageNumber="152">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,619,309,335]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="xenga" status="new species">
<emphasis bold="true" box="[151,389,309,335]" italics="true" pageId="151" pageNumber="152">Hydropsyche xenga</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[631,775,309,335]" pageId="151" pageNumber="152" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
