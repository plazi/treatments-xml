<subSubSection pageId="172" pageNumber="173" type="etymology">
<paragraph blockId="172.[151,1436,152,658]" pageId="172" pageNumber="173">
<emphasis bold="true" box="[197,337,551,577]" pageId="172" pageNumber="173">Etymology.</emphasis>
<taxonomicName authorityName="Olah &amp; Schefter" authorityYear="2008" box="[346,402,552,577]" class="Insecta" family="Hydropsychidae" genus="Schmidopsyche" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="172" pageNumber="173" phylum="Arthropoda" rank="species" species="rossi">
