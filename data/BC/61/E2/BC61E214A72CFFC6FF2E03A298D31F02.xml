<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[581,665,1904,1928]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="4" pageNumber="53">USNM</collectionCode>
