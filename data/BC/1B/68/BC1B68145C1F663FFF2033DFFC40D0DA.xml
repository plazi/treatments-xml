, 
<taxonomicName box="[584,651,1683,1705]" class="Insecta" family="Staphylinidae" genus="Aenictobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="thoi">
