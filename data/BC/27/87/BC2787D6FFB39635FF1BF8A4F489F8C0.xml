<number box="[983,1042,943,967]" pageId="2" pageNumber="60" value="1700.0">1700</number>
(
<number box="[1050,1095,943,967]" pageId="2" pageNumber="60" value="295.0">295</number>
); head 
<number box="[1177,1221,943,967]" pageId="2" pageNumber="60" value="125.0">125</number>
(
