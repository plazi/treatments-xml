<paragraph pageId="0" pageNumber="77">
<taxonomicName authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Triraphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Triraphis doncombi" order="Hymenoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="doncombi">
