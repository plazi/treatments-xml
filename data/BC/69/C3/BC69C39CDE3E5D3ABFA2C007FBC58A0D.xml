<taxonomicName authorityName="Schick" authorityYear="1965" class="Arachnida" family="Philodromidae" genus="Rhysodromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhysodromus" order="Araneae" pageId="0" pageNumber="95" phylum="Arthropoda" rank="genus">
