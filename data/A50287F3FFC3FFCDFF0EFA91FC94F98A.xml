<quantity id="EA539B00FFC3FFCDFE98FABAFEA9FAF3" box="[257,332,1340,1365]" metricMagnitude="-2" metricUnit="m" metricValue="3.6" pageId="46" pageNumber="47" unit="mm" value="36.0">36mm</quantity>
. 
<taxonomicName id="EAAB4D66FFC3FFCDFECCFABBFE2DFAF2" box="[341,456,1341,1364]" class="Insecta" family="Nymphalidae" genus="Euphaedra" kingdom="Animalia" order="Lepidoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="inanum">
