<taxonomicName id="16AECEBCC373BA33FBD9FA1F809EFA2C" authority="(Fabricius)" baseAuthorityName="Fabricius" class="Insecta" family="Coccidae" genus="Parthenolecanium" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="subSpecies" species="persicae" status="syn. n." subSpecies="persicae">
<emphasis id="E3DA692DC373BA33FBD9FA1F81E6FA2F" italics="true" pageId="15" pageNumber="16">Parthenolecanium persicae persicae</emphasis>
(Fabricius)
</taxonomicName>
<taxonomicNameLabel id="F8E9D456C373BA33FE06FA7580C6FA2C" box="[393,474,1480,1506]" pageId="15" pageNumber="16">
<emphasis id="E3DA692DC373BA33FE06FA7580CEFA2C" bold="true" box="[393,466,1480,1506]" pageId="15" pageNumber="16">syn. n</emphasis>
.
</taxonomicNameLabel>
, following the synonymy already proposed for 
<taxonomicName id="16AECEBCC373BA33FB87FA758421FA2C" authority="Gmelin" authorityName="Gmelin, 1790:" authorityYear="2220" box="[1032,1341,1480,1506]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="clematidis">
<emphasis id="E3DA692DC373BA33FB87FA7585C2FA2F" box="[1032,1246,1480,1505]" italics="true" pageId="15" pageNumber="16">Coccus clematidis</emphasis>
