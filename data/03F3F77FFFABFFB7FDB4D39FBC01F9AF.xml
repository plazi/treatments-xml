<taxonomicName id="4C5A3DEAFFABFFB7FDB4D5C2BDD0FA65" baseAuthorityName="Thomas" baseAuthorityYear="1898" box="[565,667,1455,1480]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="53" pageNumber="54" phylum="Chordata" rank="species" species="ridleyi">
<emphasis id="B92E9A7BFFABFFB7FDB4D5C2BD07FA65" box="[565,588,1456,1479]" italics="true" pageId="53" pageNumber="54">M</emphasis>
