<collectingCountry id="B1DF766BEB5CFFFAFDAA49CB8A2BFFA5" box="[538,606,668,712]" name="Fiji" pageId="5" pageNumber="110">Fiji</collectingCountry>
, 
<collectingCountry id="B1DF766BEB5CFFFAFDC749CB8BBFFFA5" box="[631,970,668,712]" name="Philippines" pageId="5" pageNumber="110">Philippine Islands</collectingCountry>
, Queensland, 
<taxonomicName id="0EC84D78EB5CFFFAFB6349CB8D09FFA5" authority="Kirkaldy, 1913" authorityName="Kirkaldy" authorityYear="1913" box="[1235,1404,668,712]" class="Insecta" family="Dictyopharidae" genus="Amboina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="110" phylum="Arthropoda" rank="genus">Amboina</taxonomicName>
, 
<collectingCountry id="B1DF766BEB5CFFFAFA2449CB8E49FFA5" box="[1428,1596,668,712]" name="Malaysia" pageId="5" pageNumber="110">Malaysia</collectingCountry>
, Java, 
<collectingCountry id="B1DF766BEB5CFFFAF97349CB8F69FFA5" box="[1731,1820,668,712]" name="Niue" pageId="5" pageNumber="110">Niue</collectingCountry>
, 
<collectingCountry id="B1DF766BEB5CFFFAFE1F49878AC8FF96" box="[431,701,720,763]" name="Vanuatu" pageId="5" pageNumber="110">New Hebrides</collectingCountry>
