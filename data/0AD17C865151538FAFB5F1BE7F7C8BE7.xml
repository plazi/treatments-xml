<taxonomicName id="BDD7BA1C9635FBBCDDE910CE5A7A5621" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" lsidName="Pinus densiflora" order="Pinales" pageId="0" pageNumber="181" phylum="Tracheophyta" rank="species" species="densiflora">
