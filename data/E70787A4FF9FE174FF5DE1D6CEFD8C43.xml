<emphasis id="5DDAEAA0FF9FE174FF5DE1D6CE808888" bold="true" box="[151,511,151,177]" pageId="18" pageNumber="19">
<taxonomicName id="A8AE4D31FF9FE174FF5DE1D6CEEC8888" authority="Nielson, 2011" authorityName="Nielson" authorityYear="2011" box="[151,403,151,177]" class="Insecta" family="Cicadellidae" genus="Docalidia" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="acuminata" status="sp. nov.">
<emphasis id="5DDAEAA0FF9FE174FF5DE1D6CEEC8888" bold="true" box="[151,403,151,177]" italics="true" pageId="18" pageNumber="19">Docalidia acuminata</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="46E957DBFF9FE174FE68E1D6CE808888" box="[418,511,151,177]" pageId="18" pageNumber="19" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
