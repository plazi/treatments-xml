<subSubSection id="32EE96CEFFE6FF98FF74FE98E46CFE64" pageId="5" pageNumber="185" type="etymology">
<paragraph id="7A4BC545FFE6FF98FF74FE98E24CFE41" blockId="5.[151,1437,151,464]" box="[199,1067,367,392]" pageId="5" pageNumber="185">
<emphasis id="48801957FFE6FF98FF74FE98E729FE41" bold="true" box="[199,334,367,392]" pageId="5" pageNumber="185">Etymology.</emphasis>
