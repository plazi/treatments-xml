<taxonomicName id="F6FA4D0EFFA8FFE5FE92B9104923F808" authority="Vlasak &amp; Santos-Silva, 2018" authorityName="Vlasak &amp; Santos-Silva" authorityYear="2018" box="[298,564,1912,1934]" class="Insecta" family="Disteniidae" genus="Katerinaella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="386" phylum="Arthropoda" rank="species" species="costaricensis" status="sp. nov.">
