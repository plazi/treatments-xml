<taxonomicName id="18814D554B55FFA4FE40FC9D6955FC2A" box="[421,577,876,901]" class="Malacostraca" family="Galatheidae" genus="Babamunida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="23" phylum="Arthropoda" rank="genus">
