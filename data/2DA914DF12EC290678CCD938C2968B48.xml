<taxonomicName id="2A3410B725822F4CE96029A45C64E81D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145082" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium (Xiphomyrmex) marginatum Forel" lsidName-HNS="Tetramorium (Xiphomyrmex) marginatum Forel" order="Hymenoptera" pageNumber="485" phylum="Arthropoda" rank="species" species="marginatum" subGenus="Xiphomyrmex">Tetramorium (Xiphomyrmex) marginatum</taxonomicName>
