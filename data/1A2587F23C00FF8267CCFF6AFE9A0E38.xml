<taxonomicName id="558C4D673C03FF84651CF85DFDBC0FAC" authorityName="Lowry &amp; Stoddart" authorityYear="2002" box="[375,613,1967,1988]" class="Malacostraca" family="Tryphosidae" genus="Lepidepecreum" kingdom="Animalia" order="Amphipoda" pageId="26" pageNumber="361" phylum="Arthropoda" rank="species" species="tourville" status="n.sp.">
