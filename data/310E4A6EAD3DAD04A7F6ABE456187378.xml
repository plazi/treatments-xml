<taxonomicName id="7EA780FBAD3CAD02A6DAA89A562176B9" box="[443,591,1932,1953]" class="Insecta" family="Flatidae" genus="Hilavrita" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="268" phylum="Arthropoda" rank="species" species="xizangensis">
