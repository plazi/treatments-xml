<taxonomicName id="898AC68335D38F0E698AE099C297A936" authority="Townsend, 1935" authorityName="Townsend" authorityYear="1935" class="Insecta" family="Tachinidae" genus="Myiodoriops" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Myiodoriops marginalis" order="Diptera" pageId="18" pageNumber="81" phylum="Arthropoda" rank="species" species="marginalis">Myiodoriops marginalis</taxonomicName>
