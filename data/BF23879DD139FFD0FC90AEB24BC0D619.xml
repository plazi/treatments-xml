<taxonomicName id="F08A4D08D139FFD0FC77AF594B30D16A" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[983,1126,318,339]" class="Reptilia" family="Viperidae" genus="Vipera" kingdom="Animalia" order="Squamata" pageId="106" pageNumber="107" phylum="Chordata" rank="species" species="aspis">
