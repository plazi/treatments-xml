<taxonomicName id="F08A4D08D139FFD0FD1DAF5A4C92D16A" authorityName="Jan" authorityYear="1860" box="[701,964,317,340]" class="Reptilia" family="Anomalepididae" genus="Anomalepis" kingdom="Animalia" order="Squamata" pageId="106" pageNumber="107" phylum="Chordata" rank="species" species="mexicanus">
<emphasis id="05FEEA99D139FFD0FD1DAF5A4C92D16A" box="[701,964,317,340]" italics="true" pageId="106" pageNumber="107">Anomalepis mexicanus</emphasis>
</taxonomicName>
