<taxonomicName id="4C0C13E3FFC8FFD46DA19159B054F8D2" authority="Akingbohungbe 2003: 2850" authorityName="Akingbohungbe 2003:" authorityPageNumber="2850" authorityYear="2850" box="[151,629,1866,1888]" class="Insecta" family="Miridae" genus="Myiomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="505" phylum="Arthropoda" rank="species" species="cobbeni">
