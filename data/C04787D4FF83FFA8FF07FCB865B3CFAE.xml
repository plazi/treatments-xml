<taxonomicName id="8FEE4D41FF83FFA8FF57FCB9672DC87D" authority="(Koch, 1867)" baseAuthorityName="Koch" baseAuthorityYear="1867" box="[199,622,798,826]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="moreliae">
