(
<figureCitation id="13492A43FFC7FFB702BEFBE7499AFBE9" box="[159,245,1122,1147]" captionStart="FIGURES 23–30" captionStartId="15.[151,264,1766,1789]" captionTargetBox="[166,1418,209,1720]" captionTargetId="figure-87@15.[153,1433,209,1723]" captionTargetPageId="15" captionText="FIGURES 23–30. Adults of new Stigmella spp. 23, S. pseudoconcreta Remeikis &amp; Stonis, sp. nov., holotype; 24, same; 25, S. quadrata Remeikis &amp; Stonis, sp. nov., holotype; 26, same; 27, S. semilactea, Remeikis &amp; Stonis, sp. nov., holotype; 28, same, paratype; 29, S. brutea Remeikis &amp; Stonis, sp. nov., holotype; 30, S. pseudodigitata Remeikis &amp; Stonis, sp. nov., holotype." figureDoi="http://doi.org/10.5281/zenodo.4952127" httpUri="https://zenodo.org/record/4952127/files/figure.png" pageId="10" pageNumber="331">Figs 29</figureCitation>
, 
<figureCitation id="13492A43FFC7FFB70320FBE74825FBE9" box="[257,330,1122,1147]" captionStart="FIGURES 79–82" captionStartId="26.[151,264,1776,1799]" captionTargetBox="[204,1383,220,1728]" captionTargetId="figure-69@26.[194,1394,215,1729]" captionTargetPageId="26" captionText="FIGURES 79–82. Male genitalia of holotypes of new Stigmella species. 79, S. brutea Remeikis &amp; Stonis, sp. nov., genitalia slide no. RA332, genital capsule; 80, same, phallus; 81, S. pseudodigitata Remeikis &amp; Stonis, sp. nov., genitalia slide no. RA516, genital capsule; 82, same, phallus." figureDoi="http://doi.org/10.5281/zenodo.4952157" httpUri="https://zenodo.org/record/4952157/files/figure.png" pageId="10" pageNumber="331">79, 80</figureCitation>
, 
<figureCitation id="13492A43FFC7FFB70374FBE748EFFBE9" box="[341,384,1122,1147]" captionStart="FIGURES 101, 102" captionStartId="30.[151,264,968,991]" captionTargetBox="[202,1386,197,943]" captionTargetId="figure-380@30.[196,1392,193,948]" captionTargetPageId="30" captionText="FIGURES 101, 102. Female genitalia of new Stigmella species. 101, S. semilactea Remeikis &amp; Stonis, sp. nov., odd, non typeseries specimen, genitalia slide no. RA511; 102, S. brutea Remeikis &amp; Stonis, sp. nov., paratype, genitalia slide no. RA372." figureDoi="http://doi.org/10.5281/zenodo.4952165" httpUri="https://zenodo.org/record/4952165/files/figure.png" pageId="10" pageNumber="331">102</figureCitation>
