<taxonomicName id="76B903A6FFDCFFCBCCE3FEFFE1F2FE82" authority="PEREZ, 1913" authorityName="PEREZ" authorityYear="1913" box="[178,473,327,350]" class="Insecta" family="Apidae" genus="Nomada" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="1483" phylum="Arthropoda" rank="species" species="fallax">
