<keyLead id="A52C95C1FF97FF94FF4D9FD2F88BFDD2" pageId="7" pageNumber="8">
<number id="A81524D3FF97FF94FF4D9FD2FDFDFE32" box="[151,237,474,495]" pageId="7" pageNumber="8" value="6.1">6. 1</number>
