<taxonomicName id="2A9A68E1FFA2D82D708FFD88FC78FDD8" authority="Martinez" authorityName="Martinez" authorityYear="1949" box="[652,913,617,638]" class="Insecta" family="Cetoniidae" genus="Hologymnetis" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="213" phylum="Arthropoda" rank="genus">
