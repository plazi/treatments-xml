<taxonomicName id="F8FCE8C98E339C3E272522E6B90226EE" baseAuthorityName="Broderip" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="66" pageNumber="67" phylum="Mollusca" rank="species" species="guttatus">
