<taxonomicName id="E3A7FE88FFDB832BFF2D59E6FEA7FE66" box="[233,311,390,413]" class="Magnoliopsida" family="Brassicaceae" genus="Koniga" higherTaxonomySource="GBIF" kingdom="Plantae" order="Brassicales" pageId="102" pageNumber="102" phylum="Tracheophyta" rank="genus">Koniga</taxonomicName>
