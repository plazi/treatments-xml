<taxonomicName id="E3A7FE88FFDB832BFFB559C5FE87FE46" authority="Desv." authorityName="Desv." box="[113,279,421,445]" class="Leotiomycetes" family="Rhytismataceae" genus="Lobularia" kingdom="Fungi" order="Rhytismatales" pageId="102" pageNumber="102" phylum="Ascomycota" rank="genus">
