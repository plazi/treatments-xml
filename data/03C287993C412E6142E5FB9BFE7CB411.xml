<taxonomicName id="4C6B4D0C3C412E624143F9E0FC57B536" authorityName="Linnaeus" authorityYear="1758" box="[817,944,1541,1568]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="238" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
