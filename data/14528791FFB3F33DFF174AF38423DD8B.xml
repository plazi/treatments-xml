<taxonomicName id="5BFB4D04FFB3F33DFB174F5E82E3DF4B" box="[1175,1256,1086,1111]" class="Insecta" family="Cerophytidae" genus="Baissophytum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="181" phylum="Arthropoda" rank="species" species="amplus">
