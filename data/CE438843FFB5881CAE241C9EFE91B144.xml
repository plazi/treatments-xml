<collectionCode id="20FBA190FFB68802ADF818CEFCD1B5DC" LSID="urn:lsid:biocol.org:col:34871" box="[724,811,1748,1774]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="14" pageNumber="15">USNM</collectionCode>
No. 
<number id="F0222B72FFB68802AC4818CEFC55B5DC" box="[868,943,1748,1774]" pageId="14" pageNumber="15" value="26540.0">26540</number>
) from 
<collectingCountry id="3EFD79C5FFB68802ACD318C9FB1EB5DF" box="[1023,1252,1747,1773]" name="United States of America" pageId="14" pageNumber="15">
