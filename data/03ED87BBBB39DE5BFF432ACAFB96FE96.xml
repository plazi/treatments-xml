<collectionCode id="ED55AE68BB39DE5CFD2C291CFC8AF99B" box="[730,821,1561,1582]" country="0" httpUri="http://grbio.org/cool/bmqw-2x87" name="Museo Nacional de Historia Natural del Paraguay" pageId="11" pageNumber="466">MNHNP</collectionCode>
