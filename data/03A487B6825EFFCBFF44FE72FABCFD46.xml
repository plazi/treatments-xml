<taxonomicName id="4C0D4D23825CFFC4FEC9FA6CFD15FA16" authority="Navas 1923: 6" authorityName="Navas" authorityPageNumber="6" authorityYear="1923" box="[282,654,1418,1440]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="210" phylum="Arthropoda" rank="species" species="turbida">
<taxonomicName id="4C0D4D23825CFFC4FEC9FA6CFDECFA16" authority="Navas 1923" authorityName="Navas" authorityYear="1923" box="[282,631,1418,1440]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="210" phylum="Arthropoda" rank="species" species="turbida">
<taxonomicName id="4C0D4D23825CFFC4FEC9FA6CFDA2FA16" authority="Navas" authorityName="Navas" box="[282,569,1418,1440]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="210" phylum="Arthropoda" rank="species" species="turbida">
<taxonomicName id="4C0D4D23825CFFC4FEC9FA6CFE6FFA16" box="[282,500,1418,1440]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="210" phylum="Arthropoda" rank="species" species="turbida">
<emphasis id="B979EAB2825CFFC4FEC9FA6CFE6FFA16" box="[282,500,1418,1440]" italics="true" pageId="7" pageNumber="210">
Helicopsyche 
<taxonomicName id="4C0D4D23825CFFC4FE7AFA6CFE6FFA16" authority="Navas 1923" authorityName="Navas" authorityPageNumber="6" authorityYear="1923" box="[425,500,1418,1440]" pageId="7" pageNumber="210" rank="species" species="turbida">turbida</taxonomicName>
