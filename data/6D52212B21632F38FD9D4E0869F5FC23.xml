(
<specimenCount id="F3FD5BB421632F3BFCD349236943FA7D" box="[769,793,1395,1421]" pageId="72" pageNumber="73" type="male">♂</specimenCount>
), 
<quantity id="22033DD821632F3BFCFD492369D2FA7E" box="[815,904,1395,1422]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="72" pageNumber="73" unit="mm" value="4.2">4.2 mm</quantity>
(
<specimenCount id="F3FD5BB421632F3BFC48492369F7FA7D" box="[922,941,1395,1421]" pageId="72" pageNumber="73" type="female">♀</specimenCount>
), 
<quantity id="22033DD821632F3BFC1449236E74FA7E" box="[966,1070,1395,1422]" metricMagnitude="-3" metricUnit="m" metricValue="4.35" pageId="72" pageNumber="73" unit="mm" value="4.35">4.35 mm</quantity>
