<quantity id="22033DD821632F3BFBBC49006E8CFA9B" box="[1134,1238,1360,1387]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" pageId="72" pageNumber="73" unit="mm" value="2.15">2.15 mm</quantity>
long, 
<quantity id="22033DD821632F3BFACC49006F2DFA9B" box="[1310,1399,1360,1387]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="72" pageNumber="73" unit="mm" value="1.7">1.7 mm</quantity>
combined wide. Selected 
<typeStatus id="3A402E9F21632F3BFE354924680DFA7E" box="[487,599,1396,1422]" pageId="72" pageNumber="73">paratypes</typeStatus>
are 
