<elevation id="56B1D1AAAB71FFF1FDB5A67FAD9BF988" box="[633,763,1554,1581]" metricMagnitude="2" metricUnit="m" metricValue="8.54" pageId="10" pageNumber="196" unit="m" value="854.0">
<quantity id="1A649B7CAB71FFF1FDB5A67FADA0F988" box="[633,704,1554,1581]" metricMagnitude="2" metricUnit="m" metricValue="8.54" pageId="10" pageNumber="196" unit="m" value="854.0">854 m</quantity>
