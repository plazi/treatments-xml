</elevation>
<location id="D8436042AB71FFF1FCCDA67EAC53F988" LSID="urn:lsid:plazi:treatment:5535878FAB71FFF1FF5BA5EFAB1DF988:D8436042AB71FFF1FCCDA67EAC53F988" box="[769,819,1555,1581]" country="Algeria" name="This" pageId="10" pageNumber="196">This</location>
