<number id="1B23249A3A1FAB17FAE1FAD046020056" box="[1311,1319,1306,1320]" pageId="5" pageNumber="161" value="1.0">1</number>
<number id="1B23249A3A1FAB17FAD1FAC64668005B" box="[1327,1357,1292,1317]" pageId="5" pageNumber="161" value="56.0">56</number>
, 
