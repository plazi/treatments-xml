<taxonomicName id="4C0A4D58FFDEFFAD7508F9E6FF7F7979" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" class="Insecta" family="Psychodidae" genus="Psychoda" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="818" phylum="Arthropoda" rank="genus">
