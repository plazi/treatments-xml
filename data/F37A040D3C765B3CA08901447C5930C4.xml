, SIZK, no. K-9948, 
<specimenCount id="7F18C53FFF0D83E83914F84BE3C41E9E" type="female">1 ♀</specimenCount>
, 
<location id="045FC9084BBE75872EBE90666A56674B" LSID="urn:lsid:plazi:treatment:F37A040D3C765B3CA08901447C5930C4:045FC9084BBE75872EBE90666A56674B" country="Ukraine" county="All" municipality="Sarny District" name="Klesov" stateProvince="Rovno Region">Klesov</location>
, 
<collectingMunicipality id="AEFBF1700DAD7AFFF8B19FE6529FC5FC">Sarny District</collectingMunicipality>
, 
<collectingRegion id="BA2FB598918D8747A2AAC558BC1A32E8" country="Ukraine" name="Rivne">Rovno Region</collectingRegion>
