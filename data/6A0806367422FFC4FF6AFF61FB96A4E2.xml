<taxonomicName id="25A1CCA37422FFC4FC99FBB2FBBDA71F" box="[868,1079,1092,1114]" class="Insecta" family="Thripidae" genus="Paithrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="pornthipae">
<emphasis id="D0D56B327422FFC4FC99FBB2FBBDA71F" box="[868,1079,1092,1114]" italics="true" pageId="2" pageNumber="3">Paithrips pornthipae</emphasis>
</taxonomicName>
