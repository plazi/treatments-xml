<paragraph id="C52B368CFFA9FFA0FF46C244FBE0FAD6" blockId="15.[151,1437,151,1832]" box="[199,717,1375,1400]" pageId="15" pageNumber="16">
<emphasis id="F7E0EA9EFFA9FFA0FF46C244F84DFAD6" bold="true" box="[199,352,1375,1400]" pageId="15" pageNumber="16">Distribution.</emphasis>
<collectingCountry id="BD83761CFFA9FFA0FEE6C244FBE7FAD6" box="[359,714,1375,1400]" name="Democratic Republic of the Congo" pageId="15" pageNumber="16">
Democratic Republic of 
<collectingRegion id="0750F86EFFA9FFA0FDFAC27BFBE7FAD6" box="[635,714,1376,1400]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="15" pageNumber="16">Congo</collectingRegion>
