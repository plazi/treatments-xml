<quantity id="004C348A63352749069CBD89C17CF8E8" box="[458,542,1769,1793]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="1" pageNumber="162" unit="mm" value="3.6">3.6 mm</quantity>
