<collectingCountry id="2ADBEFEAFFEEFB41FD1B3471FCFFFAF5" box="[705,804,1312,1336]" name="Namibia" pageId="67" pageNumber="1078">Namibia</collectingCountry>
], Abachaus [
<geoCoordinate id="37F8C9BDFFEEFB41FC163471FBCDFAF5" box="[972,1046,1312,1336]" degrees="19.47" direction="south" orientation="latitude" pageId="67" pageNumber="1078" precision="555" value="-19.47">19.47S</geoCoordinate>
