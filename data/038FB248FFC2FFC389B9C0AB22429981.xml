<taxonomicName id="4C2678DDFFC2FFC5898BC0AB230398A2" authority="(Von Linstow, 1883) Gilbert, 1927" authorityName="(Von Linstow, 1883) Gilbert" authorityYear="1927" box="[165,841,1720,1746]" class="Nematoda" family="Nematoda" genus="Syncuaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nematoda" pageId="115" pageNumber="116" phylum="Nematoda" rank="species" species="squamata">
<emphasis id="B952DF4CFFC2FFC5898BC0AB21D498A2" bold="true" box="[165,414,1720,1746]" italics="true" pageId="115" pageNumber="116">Syncuaria squamata</emphasis>
