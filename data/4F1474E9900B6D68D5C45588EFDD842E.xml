<pageBreakToken id="8087785B06F3514794C2213207935634" pageId="0" pageNumber="9" start="start">2</pageBreakToken>
. 
<taxonomicName id="B72AAFCE7F8AF05AC7E60F39F1C64989" authority="L." authorityName="L." class="Magnoliopsida" family="Amaranthaceae" genus="Chenopodium" higherTaxonomySource="CoL" kingdom="Plantae" order="Caryophyllales" pageId="0" pageNumber="9" phylum="Tracheophyta" rank="genus">Chenopodium L.</taxonomicName>
