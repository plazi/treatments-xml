<taxonomicName id="4C3F4D3DD7116449C5B7E4502811C73D" authorityName="Stephens" authorityYear="1829" box="[407,581,1026,1048]" class="Insecta" family="Philopotamidae" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="484" phylum="Arthropoda" rank="family">Philopotamidae</taxonomicName>
