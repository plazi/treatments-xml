= 
<taxonomicName id="00C9D0DCFFB9FFF0FF7B2152FDC1D740" authority="Kuoh, 1992" authorityName="Kuoh" authorityYear="1992" box="[172,521,824,846]" class="Insecta" family="Cicadellidae" genus="Macropsis" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="55" phylum="Arthropoda" rank="species" species="pallidinota" status="syn. nov.">
