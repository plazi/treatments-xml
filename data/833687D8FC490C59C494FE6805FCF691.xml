<taxonomicName id="CC9F4D4DFC490C59C5E5FE680215F58E" authority="Thery, 1934" authorityName="Thery" authorityYear="1934" box="[486,626,462,488]" class="Insecta" family="Buprestidae" genus="Anilaroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
<emphasis id="39EBEADCFC490C59C5E5FE680215F58E" bold="true" box="[486,626,462,488]" italics="true" pageId="2" pageNumber="3">Anilaroides</emphasis>
</taxonomicName>
and 
<emphasis id="39EBEADCFC490C59C6B3FE6803E1F58E" bold="true" box="[688,902,462,488]" italics="true" pageId="2" pageNumber="3">Tetragonoschema</emphasis>
</emphasis>
</heading>
