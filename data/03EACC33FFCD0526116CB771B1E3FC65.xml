and 
<taxonomicName id="4C4306A6FFCB052611D9B532B227FCA5" baseAuthorityName="Thai &amp; Huynh" baseAuthorityYear="1992" box="[402,578,829,853]" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="5" pageNumber="225" phylum="Annelida" rank="species" species="dorsobitheca">
