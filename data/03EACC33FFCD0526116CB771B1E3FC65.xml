<quantity id="4CBBD0C0FFCB0526113CB4F1B26EFCE5" box="[375,523,765,789]" metricMagnitude="-1" metricUnit="m" metricValue="1.3599999999999999" metricValueMax="1.65" metricValueMin="1.0699999999999998" pageId="5" pageNumber="225" unit="mm" value="136.0" valueMax="165.0" valueMin="107.0">107–165 mm</quantity>
