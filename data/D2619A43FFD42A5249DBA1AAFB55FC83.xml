<taxonomicName id="9DC850D6FFD42A4F4854A28AFE55F8DB" authorityName="Rafinesque" authorityYear="1820" box="[280,424,1771,1797]" family="Viburnaceae" pageId="45" pageNumber="46" rank="family">Viburnaceae</taxonomicName>
: 
<taxonomicName id="9DC850D6FFD42A4F48FFA28AFDA4F8DA" box="[435,601,1771,1797]" class="Magnoliopsida" family="Adoxaceae" genus="Sambucus" kingdom="Plantae" order="Dipsacales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="undetermined">
