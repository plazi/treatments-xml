<taxonomicName id="49D9B9FB145D3B73F6A4954FA04E7C4F" authority="Macquart" class="Insecta" family="Bombyliidae" genus="Ogcodocera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ogcodocera" order="Diptera" pageId="9" pageNumber="58" phylum="Arthropoda" rank="genus">Ogcodocera Macquart</taxonomicName>
