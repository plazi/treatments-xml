<taxonomicName id="4C224D28896A7827F8A83782420CD1A7" authority="Taczanowski, 1874" authorityName="Taczanowski" authorityYear="1874" box="[1014,1398,1407,1434]" class="Arachnida" family="Pholcidae" genus="Physocyclus" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="14" phylum="Arthropoda" rank="species" species="globosus">
