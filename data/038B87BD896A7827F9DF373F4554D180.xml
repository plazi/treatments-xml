<paragraph id="8B9D36AB896A7827FBE337824554D180" blockId="14.[189,1398,1407,1470]" pageId="14" pageNumber="14">
<taxonomicName id="4C224D28896A7827FBE337824634D1A7" box="[189,334,1408,1434]" class="Arachnida" family="Pholcidae" genus="Physocyclus" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="14" phylum="Arthropoda" rank="genus">
