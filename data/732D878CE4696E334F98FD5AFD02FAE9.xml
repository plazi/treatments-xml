<taxonomicName id="3C844D19E4696E334EAEFC0EFDBBFBC9" box="[417,525,1009,1034]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
: 
