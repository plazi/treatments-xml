<quantity id="A75E9B703F74FFFC324FBF5AA37A786B" box="[552,609,1365,1385]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="1" pageNumber="1862" unit="m" value="500.0">500 m</quantity>
