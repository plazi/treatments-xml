<taxonomicName id="4D0FDE844EB3A810B00137BE72037F91" authority="Wildish, 2014" authorityName="Wildish" authorityYear="2014" class="Malacostraca" family="Talitridae" genus="Neotenorchestia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neotenorchestia" order="Amphipoda" pageId="4" pageNumber="137" phylum="Arthropoda" rank="genus">Neotenorchestia</taxonomicName>
: 
