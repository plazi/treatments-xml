<taxonomicName id="70FA8EB5FF9A4A4A0DE4FA1BFD28014E" box="[509,722,1515,1539]" class="Insecta" family="Gryllidae" genus="Lebinthus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="460" phylum="Arthropoda" rank="species" species="saussurei">
