<taxonomicName id="4C2B4D3BC144174DFE555B1E0B430E76" authority="Vayssiere, 1893" authorityName="Vayssiere" authorityYear="1893" box="[445,588,259,285]" class="Gastropoda" family="Omalogyridae" genus="Ammonicera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="6" pageNumber="475" phylum="Mollusca" rank="genus">
