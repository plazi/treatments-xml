</emphasis>
<taxonomicName id="4C7FA98535561754FE9BFA1EFBF4FA35" authority="Heinicke, Duellman, Trueb, Means, MacCulloch" authorityName="Heinicke, Duellman, Trueb, Means, MacCulloch" box="[305,1128,1413,1436]" class="Amphibia" family="Ceuthomantidae" genus="Ceuthomantis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="smaragdinus">
