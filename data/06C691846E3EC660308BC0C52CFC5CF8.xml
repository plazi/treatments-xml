<taxonomicName id="FB0EBCF9EC046ACA98EF56CC9E88C710" baseAuthorityName="Pilsbry" baseAuthorityYear="1897" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="136" pageNumber="137" phylum="Mollusca" rank="species" species="angrandianus">
