<paragraph id="E50C312871B606F603CF6E2637C84C72" pageId="0" pageNumber="1">
<emphasis id="3BE7D11E1DADD48289500B4861ABF28E" bold="true" italics="true" pageId="0" pageNumber="1">Holotype</emphasis>
<taxonomicName id="10A62FA6E1215DC9DDD6F2163DC6B66C" authorityName="Eggers" authorityYear="1933" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus orientalis" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="orientalis">
<emphasis id="684863D12F11D8D762E52216A9DC4CFC" italics="true" pageId="0" pageNumber="1">Xyleborus orientalis</emphasis>
</taxonomicName>
(NMNH). 
<emphasis id="E69699A0E39E51E760FE04828F2A1C8B" bold="true" italics="true" pageId="0" pageNumber="1">Syntypes</emphasis>
<taxonomicName id="E31AA16A1D2D7B9341BB71C1BE888E96" authorityName="Blandford" authorityYear="1894" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus seriatus" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="seriatus">
