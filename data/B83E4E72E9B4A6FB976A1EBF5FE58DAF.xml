<paragraph id="AAB29EE1A528E5E00723E35B45B57B04" pageId="0" pageNumber="19938">
<taxonomicName id="C36E628CDBAD3465B926748916F55954" class="Insecta" family="Dytiscidae" genus="Microdytes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microdytes elgae" order="Coleoptera" pageId="0" pageNumber="19938" phylum="Arthropoda" rank="species" species="elgae">Microdytes elgae</taxonomicName>
