<taxonomicName id="8B854D60EF50FF8BFF78767FFD2F5112" authority="(LINNAEUS 1758)" baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[226,663,1028,1056]" class="Insecta" family="Noctuidae" genus="Tiliacea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="952" phylum="Arthropoda" rank="species" species="citrago">
<emphasis id="7EF1EAF1EF50FF8BFF78767FFD2F5112" bold="true" box="[226,663,1028,1056]" pageId="7" pageNumber="952">
<emphasis id="7EF1EAF1EF50FF8BFF78767FFE175112" bold="true" box="[226,431,1028,1056]" italics="true" pageId="7" pageNumber="952">Tiliacea citrago</emphasis>
