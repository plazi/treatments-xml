<taxonomicName id="258E4D14FF8FFFD7FC49F980FAAC227F" authority="Cranston" authorityName="Cranston" box="[1000,1322,1588,1611]" class="Insecta" family="Chironomidae" genus="Orthocladius" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="subletteorum" subGenus="Eudactylocladius">
