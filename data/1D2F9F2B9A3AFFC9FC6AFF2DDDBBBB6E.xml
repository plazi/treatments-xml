<quantity id="527E83D89A3AFFCEFC18FC1ADB0EB9E6" box="[963,1075,992,1017]" metricMagnitude="-3" metricUnit="m" metricValue="5.75" metricValueMax="6.5" metricValueMin="5.0" pageId="43" pageNumber="352" unit="mm" value="5.75" valueMax="6.5" valueMin="5.0">5 to 6.5 µm</quantity>
