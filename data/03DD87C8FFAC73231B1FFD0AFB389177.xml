<collectingRegion id="49B0F83CFFAF73231ADEF85DFCCD90F1" box="[748,792,1974,1997]" country="Papua New Guinea" name="National Capital" pageId="22" pageNumber="299">Port</collectingRegion>
</paragraph>
<paragraph id="8BCB36DEFFAF73231B77FA30FB389177" blockId="22.[837,1467,1499,1611]" pageId="22" pageNumber="299">
<materialsCitation id="3B1C3C83FFAF73231B77FA30FB3F9177" collectorName="Moresby &amp; Rev. Lawes &amp; The &amp; Palmer &amp; P Register" country="Guinea-Bissau" location="New" pageId="22" pageNumber="299" specimenCount="1">
<collectorName id="26815308FFAF73231B77FA30FC7192CF" box="[837,932,1499,1523]" pageId="22" pageNumber="299">Moresby</collectorName>
