<quantity id="4CAC5FB3FFEE180CFCF10D70FC38FDB4" box="[833,904,582,609]" metricMagnitude="4" metricUnit="m" metricValue="5.6" pageId="84" pageNumber="85" unit="km" value="56.0">56 km</quantity>
