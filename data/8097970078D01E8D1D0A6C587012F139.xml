<taxonomicName id="B71F945D8DFD56145708EA70063C8518" class="Agaricostilbomycetes" family="Chrysididae" genus="Stilbum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stilbum cyanurum subsp. auratum" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Basidiomycota" rank="subSpecies" species="cyanurum" subSpecies="auratum">
