<taxonomicName id="4C674D0BFFEEFFB4D7685929F8BAFABF" box="[327,435,1378,1401]" class="Insecta" family="Curculionidae" genus="Xyleborus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="33" phylum="Arthropoda" rank="genus">
