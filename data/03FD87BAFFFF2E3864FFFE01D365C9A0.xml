<taxonomicName id="4C544D2FFFFF2E3867B9FCE4D60DC943" authorityName="sensu Sars. Gender" authorityYear="1863" box="[977,1090,861,884]" class="Malacostraca" family="Lampropidae" genus="Lamprops" kingdom="Animalia" order="Cumacea" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
