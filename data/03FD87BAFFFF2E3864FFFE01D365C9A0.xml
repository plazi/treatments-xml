. 
<taxonomicName id="4C544D2FFFFF2E386529FCE5D077C943" authorityName="Gerken" authorityYear="2018" baseAuthorityName="Gladfelter" baseAuthorityYear="1975" box="[321,568,860,885]" class="Malacostraca" family="Lampropidae" genus="Alamprops" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Cumacea" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="triserratus">
