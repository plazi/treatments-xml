<taxonomicName id="4C30CED5DE13F0021B7B8627FD41FD87" box="[584,686,512,537]" class="Arachnida" family="Phytoseiidae" genus="Kuzinellus" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="blairi">
<emphasis id="B9446944DE13F0021B7B8627FD41FD87" box="[584,686,512,537]" italics="true" pageId="7" pageNumber="8">K. blairi</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C30CED5DE13F0021BD88627FC12FD84" authority="El-Badry" authorityName="El-Badry" box="[747,1021,512,538]" class="Arachnida" family="Phytoseiidae" genus="Kuzinellus" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="niloticus">
