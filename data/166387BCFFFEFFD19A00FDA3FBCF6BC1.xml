(
<bibRefCitation id="FA5B4B5BFFFEFFD09891FB9CFBD06E9A" author="KIMOTO S." box="[967,1119,1143,1167]" pageId="8" pageNumber="723" pagination="1 - 241" refId="ref15334" refString="KIMOTO S. 1989: Chrysomelidae (Coleoptera) of Thailand, Cambodia, Laos and Vietnam. IV. Galerucinae. Esakia 27: 1 - 241." type="journal article" year="1989">Kimoto, 1989</bibRefCitation>
)
