<number id="9C462481FFD0C830380DF9655656176D" box="[1310,1357,1785,1811]" pageId="9" pageNumber="66" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
; distal paired spines on P 
<number id="9C462481FFD0C8303C7CF88252641746" box="[367,383,1822,1848]" pageId="9" pageNumber="66" value="4.0">4</number>
