<number id="9C462481FFD0C8303DB9F96553A1176D" box="[170,186,1785,1811]" pageId="9" pageNumber="66" value="4.0">4</number>
; flexor margin with 
<number id="9C462481FFD0C8303CA7F96552D8176D" box="[436,451,1785,1811]" pageId="9" pageNumber="66" value="5.0">5</number>
or 
<number id="9C462481FFD0C8303CFFF96552E0176D" box="[492,507,1785,1811]" pageId="9" pageNumber="66" value="6.0">6</number>
