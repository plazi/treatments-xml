<subSubSection id="C334653CFFDFFF87FF42214094DCFE09" lastPageId="17" lastPageNumber="418" pageId="16" pageNumber="417" type="description">
<paragraph id="8B9136B7FFDFFF86FF4221409652FE09" blockId="16.[151,1437,152,827]" pageId="16" pageNumber="417">
<emphasis id="B95AEAA5FFDFFF86FF4221409491FE2D" bold="true" box="[199,485,368,395]" pageId="16" pageNumber="417">
Description of 
<typeStatus id="54958815FFDFFF86FEF221419494FE2D" box="[375,480,369,395]" pageId="16" pageNumber="417">holotype</typeStatus>
.
</emphasis>
Adult male; SVL = 
<quantity id="4CD69B52FFDFFF86FD4F2140964BFE2D" box="[714,831,368,395]" metricMagnitude="-2" metricUnit="m" metricValue="2.735" pageId="16" pageNumber="417" unit="mm" value="27.35">27.35 mm</quantity>
; HL = 
<quantity id="4CD69B52FFDFFF86FC0821409680FE2D" box="[909,1012,368,395]" metricMagnitude="-3" metricUnit="m" metricValue="7.05" pageId="16" pageNumber="417" unit="mm" value="7.05">7.05 mm</quantity>
; HW = 
<quantity id="4CD69B52FFDFFF86FBCE214191C6FE2D" box="[1099,1202,369,395]" metricMagnitude="-3" metricUnit="m" metricValue="4.94" pageId="16" pageNumber="417" unit="mm" value="4.94">4.94 mm</quantity>
; HD = 
<quantity id="4CD69B52FFDFFF86FA812141901FFE2D" box="[1284,1387,369,395]" metricMagnitude="-3" metricUnit="m" metricValue="3.48" pageId="16" pageNumber="417" unit="mm" value="3.48">3.48 mm</quantity>
; ED = 
<quantity id="4CD69B52FFDFFF86FF2B21A59461FE09" box="[174,277,405,431]" metricMagnitude="-3" metricUnit="m" metricValue="1.24" pageId="16" pageNumber="417" unit="mm" value="1.24">1.24 mm</quantity>
; FL = 7.36; HdL = 
<quantity id="4CD69B52FFDFFF86FE6B21A59717FE09" box="[494,611,405,431]" metricMagnitude="-2" metricUnit="m" metricValue="1.077" pageId="16" pageNumber="417" unit="mm" value="10.77">10.77 mm</quantity>
; TL = 
<quantity id="4CD69B52FFDFFF86FD2921A59655FE09" box="[684,801,405,431]" metricMagnitude="-2" metricUnit="m" metricValue="2.822" pageId="16" pageNumber="417" unit="mm" value="28.22">28.22 mm</quantity>
