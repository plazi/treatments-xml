<taxonomicName id="4C6FC7F5FFAE1421FF1D2DA3A49D369F" authorityName="Boisduval" authorityYear="1828" box="[151,244,223,249]" class="Insecta" family="Sesiidae" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="122" phylum="Arthropoda" rank="family">Sesiidae</taxonomicName>
