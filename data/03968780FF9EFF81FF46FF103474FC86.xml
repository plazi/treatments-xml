<taxonomicName id="4C3F4D15FF9EFF87FE37FBA137EEFB8D" authorityName="Thorell" authorityYear="1897" box="[509,676,1047,1073]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="25" phylum="Arthropoda" rank="species" species="scrobiculatus">
<emphasis id="B94BEA84FF9EFF87FE37FBA137EEFB8D" box="[509,676,1047,1073]" italics="true" pageId="2" pageNumber="25">S. scrobiculatus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3F4D15FF9EFF87FD7CFBA13641FB8C" authorityName="Deeleman-Reinhold" authorityYear="2001" box="[694,779,1047,1073]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="25" phylum="Arthropoda" rank="species" species="songi">
<emphasis id="B94BEA84FF9EFF87FD7CFBA13641FB8C" box="[694,779,1047,1073]" italics="true" pageId="2" pageNumber="25">S. songi</emphasis>
