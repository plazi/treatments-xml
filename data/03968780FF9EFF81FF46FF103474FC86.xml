<collectingCountry id="F3287606FF9EFF87FDB4FB5C37ADFAB8" box="[638,743,1258,1284]" name="Thailand" pageId="2" pageNumber="25">Thailand</collectingCountry>
, 
<collectingCountry id="F3287606FF9EFF87FD39FB5C3629FAB9" box="[755,867,1258,1285]" name="Indonesia" pageId="2" pageNumber="25">Indonesia</collectingCountry>
