<collectingCountry id="F3287606FF9EFF87FE47FB3634B5FB26" box="[397,511,1152,1178]" name="Myanmar" pageId="2" pageNumber="25">Myanmar</collectingCountry>
) and 
<taxonomicName id="4C3F4D15FF9EFF87FD89FB3737B3FB26" authorityName="Deeleman-Reinhold" authorityYear="2001" box="[579,761,1152,1179]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="25" phylum="Arthropoda" rank="species" species="octomaculatus">
