</taxonomicName>
(
<collectingCountry id="F3287606FF9EFF87FDFCFC6637D4FC57" box="[566,670,976,1003]" name="Thailand" pageId="2" pageNumber="25">Thailand</collectingCountry>
) and 
<taxonomicName id="4C3F4D15FF9EFF87FD29FC673612FC56" authorityName="Deeleman-Reinhold" authorityYear="2001" box="[739,856,976,1003]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="25" phylum="Arthropoda" rank="species" species="gothicus">
<emphasis id="B94BEA84FF9EFF87FD29FC673612FC56" box="[739,856,976,1003]" italics="true" pageId="2" pageNumber="25">S. gothicus</emphasis>
