<taxonomicName id="4C3F4D15FF9EFF87FCFAFB1236AFFB01" authorityName="Deeleman-Reinhold" authorityYear="2001" box="[816,997,1187,1214]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="25" phylum="Arthropoda" rank="species" species="octomaculatus">
