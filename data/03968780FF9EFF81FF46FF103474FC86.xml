, 
<taxonomicName id="4C3F4D15FF9EFF87FD47FB1237BEFB01" baseAuthorityName="Thorell" baseAuthorityYear="1895" box="[653,756,1187,1214]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="25" phylum="Arthropoda" rank="species" species="gracilis">
