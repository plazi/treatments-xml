<taxonomicName id="4C604D48AE5C1C0DFE01F9B37AAA2E52" authority="Berl." authorityName="Berl." authorityYear="2018" box="[491,683,1603,1627]" pageId="3" pageNumber="685" rank="variety" status="n. var." variety="gallicus">
var. 
<emphasis id="B914EAD9AE5C1C0DFDF1F9B47A702E52" box="[539,625,1603,1627]" italics="true" pageId="3" pageNumber="685">gallicus</emphasis>
