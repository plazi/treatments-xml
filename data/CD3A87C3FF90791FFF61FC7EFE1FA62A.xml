<quantity id="826B9B30FF907919FB16FAD7FA98A24C" box="[1248,1334,1294,1319]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="2" pageNumber="461" unit="mm" value="2.5">2.5 mm</quantity>
