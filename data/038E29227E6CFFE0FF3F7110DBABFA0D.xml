<taxonomicName id="4C27E3B77E6CFFE0FE817186DDCAFD3F" authority="Schaus, 1902" authorityName="Schaus" authorityYear="1902" box="[382,724,684,708]" class="Insecta" family="Cerambycidae" genus="Phanis" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="29" phylum="Arthropoda" rank="species" species="tavola">
