<paragraph id="FA3E2A29FFEDFFFAFECEF8A9FCC7F882" blockId="9.[264,882,1809,1835]" box="[264,882,1809,1835]" pageId="9" pageNumber="50">
<heading id="A1769D45FFEDFFFAFECEF8A9FCC7F882" bold="true" box="[264,882,1809,1835]" fontSize="11" level="1" pageId="9" pageNumber="50" reason="1">
<emphasis id="C8F5F63BFFEDFFFAFECEF8A9FCC7F882" bold="true" box="[264,882,1809,1835]" pageId="9" pageNumber="50">
Genus 
<taxonomicName id="3D8151AAFFEDFFFAFE99F8A9FCADF882" authority="Gheerardyn and Fiers" authorityName="Gheerardyn and Fiers" box="[351,792,1809,1835]" class="Maxillopoda" family="Laophontidae" genus="Propephonte" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="9" pageNumber="50" phylum="Arthropoda" rank="genus">
