<taxonomicName id="4C524D76FF81FFA23B86FF5AFE037C84" box="[254,488,224,248]" class="Insecta" family="Stratiomyidae" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="family">STRATIOMYIDAE</taxonomicName>
