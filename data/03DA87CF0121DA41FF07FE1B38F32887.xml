<quantity id="4C8B9B3C0121DA41FDC9FDE43EE72817" box="[601,657,583,608]" metricMagnitude="3" metricUnit="m" metricValue="6.437376" pageId="42" pageNumber="43" unit="mi" value="4.0">4 mi</quantity>
