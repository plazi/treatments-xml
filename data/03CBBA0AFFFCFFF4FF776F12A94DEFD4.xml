<taxonomicName id="4C62709FFFFDFFF4FF776BDEA956EFD5" baseAuthorityName="Gertsch et Soleglad" baseAuthorityYear="1972" box="[192,334,395,419]" class="Arachnida" family="Vaejovidae" genus="Catalinia" kingdom="Animalia" order="Scorpiones" pageId="65" pageNumber="62" phylum="Arthropoda" rank="species" species="thompsoni">
