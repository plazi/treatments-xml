<quantity id="89359B63FFD32C11FE8D0AD7FE06FCAA" box="[380,463,744,768]" metricMagnitude="-2" metricUnit="m" metricValue="3.6" pageId="7" pageNumber="8" unit="mm" value="36.0">36 mm</quantity>
; total length to trochlea metatarsal IV— 
