<taxonomicName id="4C504D20FFDB8808308D32267D65F9D8" authority="Dwyer" authorityName="Dwyer" authorityYear="1945" box="[199,517,1588,1614]" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" kingdom="Plantae" order="Fabales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="aromatica">
