<taxonomicName id="4C504D20FFDB8808338A321D7B4FF9BC" authorityName="Lindley &amp; John" authorityYear="1830" box="[960,1071,1551,1578]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
