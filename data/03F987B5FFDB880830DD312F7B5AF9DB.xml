<taxonomicName id="4C504D20FFDB8808327632267CC2F9DB" authority="(Britton) Standl." authorityName="Standl." baseAuthorityName="Britton" box="[572,930,1587,1614]" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" kingdom="Plantae" order="Fabales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="panamensis">
<emphasis id="B924EAB1FFDB8808327632267D80F9DB" box="[572,736,1588,1614]" italics="true" pageId="47" pageNumber="48">C. panamensis</emphasis>
