<taxonomicName id="42BFB3893E1AFFE7FB31F8C8A510B3F2" box="[1197,1407,1917,1942]" class="Insecta" family="Zopheridae" genus="Syncalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="oblongus">
