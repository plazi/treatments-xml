(
<quantity id="794C58F34D01FFD3ED1970E3FEF0F7F6" box="[256,350,1186,1210]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="19" pageNumber="18" unit="m" value="1000.0">
<elevation id="359912254D01FFD3ED1970E3FEF0F7F6" box="[256,350,1186,1210]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="19" pageNumber="18" unit="m" value="1000.0">1000 m</elevation>
