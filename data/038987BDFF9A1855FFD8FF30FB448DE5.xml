<taxonomicName id="4C204D28FF9A1855FA44FE01FED88FB1" authority="Lucas" authorityName="Lucas" class="Insecta" family="Arctiidae" genus="Poecilosoma" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="359" phylum="Arthropoda" rank="species" species="haemoptera">
