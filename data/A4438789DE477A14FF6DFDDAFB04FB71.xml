<taxonomicName id="EBEA4D1CDE477A14FEDCFD94FDD9FD4C" authority="Horvath 1913" authorityName="Horvath" authorityYear="1913" box="[294,632,618,640]" class="Insecta" family="Aradidae" genus="Calisius" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="372" phylum="Arthropoda" rank="species" species="lativentris">
