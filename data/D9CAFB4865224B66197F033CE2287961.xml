<taxonomicName id="88C3B03111AAD18461B7B94E8745AD12" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:232159" authority="" authorityName="" class="Insecta" family="Formicidae" genus="Ocodoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ocodoma diversa" lsidName-HNS="Ocodoma diversa" order="Hymenoptera" pageNumber="109" phylum="Arthropoda" rank="species" species="diversa">Ocodoma diversa</taxonomicName>
