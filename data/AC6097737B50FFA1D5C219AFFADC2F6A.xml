<collectingCountry id="5CDE66F57B50FFA1D4411B8EFE392AC4" box="[335,435,1996,2019]" name="Germany" pageId="5" pageNumber="534">Germany</collectingCountry>
, 
<collectingCountry id="5CDE66F57B50FFA1D4B21B8EFD992AC4" box="[444,531,1996,2019]" name="Norway" pageId="5" pageNumber="534">Norway</collectingCountry>
