<taxonomicName id="4C3A7DAE5E29FFEAFDB9FD6AFC91FD3B" authority="Nascimento &amp; Heffern, 2018" authorityName="Nascimento &amp; Heffern" authorityYear="2018" box="[612,836,656,680]" class="Insecta" family="Cerambycidae" genus="Urgleptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="299" phylum="Arthropoda" rank="species" species="curvatum" status="sp. nov.">
