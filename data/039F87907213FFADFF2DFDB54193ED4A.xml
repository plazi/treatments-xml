<taxonomicName id="4C364D057213FFADFE2AFD2B42BFED12" baseAuthorityName="LACORDAIRE" baseAuthorityYear="1835" box="[491,662,755,782]" class="Insecta" family="Staphylinidae" genus="Amarochara" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1141" phylum="Arthropoda" rank="species" species="forticornis">
