<number id="3DBE771728534B07FE4EFD07FDC5E5DA" box="[492,530,720,742]" pageId="26" pageNumber="227" value="417.0">417</number>
</treatmentCitation>
(in key); 
<treatmentCitation id="0AD7432128534B07FDD7FD07FCFCE5DA" author="Parnell" box="[629,811,720,742]" page="283" pageId="26" pageNumber="227" year="1971">
