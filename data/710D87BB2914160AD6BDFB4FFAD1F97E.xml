<typeStatus id="261F880F2914160AD6BDFAD6FF03FA5E" box="[151,210,1397,1422]" pageId="2" pageNumber="303">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="3EA44D2E2914160AD713FAD6FCD2FA5E" authority="Rathbun, 1896" authorityName="Rathbun" authorityYear="1896" box="[313,771,1397,1422]" class="Malacostraca" family="Pseudothelphusidae" genus="Pseudothelphusa" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="303" phylum="Arthropoda" rank="species" species="agrestis">
<emphasis id="CBD0EABF2914160AD713FAD6FD86FA5D" box="[313,599,1397,1422]" italics="true" pageId="2" pageNumber="303">Pseudothelphusa agrestis</emphasis>
<bibRefCitation id="9D354B5C2914160AD474FAD6FCD2FA5E" author="Rathbun" box="[606,771,1397,1422]" pageId="2" pageNumber="303" refString="Rathbun, M. J. (1896) Descriptions of two new species of fresh-water crabs from Costa Rica. Proceedings of the United States National Museum, 18 (1071), 377 - 379, pls. XIX - XXX. http: // dx. doi. org / 10.5479 / si. 00963801.18 - 1071.377" type="journal article" year="1896">
Rathbun, 
<number id="4F6C248A2914160AD4E1FAD6FCD2FA5E" box="[715,771,1397,1422]" pageId="2" pageNumber="303" value="1896.0">1896</number>
</bibRefCitation>
