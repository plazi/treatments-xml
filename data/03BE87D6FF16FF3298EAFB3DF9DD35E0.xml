<quantity id="4CEF9B25FF16FF329B47FB3DF9D434EC" box="[826,917,1231,1257]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="135" pageNumber="136" unit="cm" value="12.7">12.7 cm</quantity>
