<taxonomicName id="D9B1CB29BFAFCE164240D04BB2693BE8" class="Actinopterygii" family="Loricariidae" genus="Harttia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Harttia" order="Siluriformes" pageId="0" pageNumber="25" phylum="Chordata" rank="genus">
