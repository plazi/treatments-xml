<taxonomicName id="84AB58B318C27F2DDC00F0DFDBB3A5AB" authorityName="Oliveira &amp; Oyakawa" authorityYear="2019" class="Actinopterygii" family="Loricariidae" genus="Harttia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Harttia intermontana" order="Siluriformes" pageId="0" pageNumber="25" phylum="Chordata" rank="species" species="intermontana">
