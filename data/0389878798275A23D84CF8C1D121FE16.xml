<taxonomicName id="4C204D1298215A23D93DFED8D1ACFE8C" authorityName="J. A. Allen" authorityYear="1917" box="[860,953,339,364]" class="Mammalia" family="Molossidae" genus="Tadarida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="61" pageNumber="62" phylum="Chordata" rank="species" species="congica">
