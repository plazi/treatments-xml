<location id="8E806044C5447B19FEF0FEFDFDD8884F" LSID="urn:lsid:plazi:treatment:03F68789C5447B19FEE6FF61FE798BDF:8E806044C5447B19FEF0FEFDFDD8884F" box="[335,518,294,318]" county="Kuril Islands" latitude="44.004665" longLatPrecision="11" longitude="145.65633" municipality="Kunashir Island" name="Severjanka River" pageId="8" pageNumber="107">Severjanka River</location>
