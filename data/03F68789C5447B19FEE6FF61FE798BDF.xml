<quantity id="4CA79B7AC5447B19FBA2FEDCFBB8886E" box="[1053,1126,263,287]" metricMagnitude="3" metricUnit="m" metricValue="3.5" pageId="8" pageNumber="107" unit="km" value="3.5">3.5 km</quantity>
