<geoCoordinate id="EE6B5058C5447B19FE75FE9EFD9C882C" box="[458,578,325,349]" degrees="44" direction="north" minutes="00.28" orientation="latitude" pageId="8" pageNumber="107" precision="9" value="44.004665">44°00.28'N</geoCoordinate>
