<taxonomicName id="13D5912CBBB0C5C1BE95F220A6FEAA0D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139841" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName="Myrmica sallei Guérin-Méneville" lsidName-HNS="Myrmica sallei Guérin-Méneville" order="Hymenoptera" pageNumber="742" phylum="Arthropoda" rank="species" species="sallei">M. Sallei</taxonomicName>
