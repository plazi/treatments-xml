<taxonomicName id="962C277F3CCFB1CBA7C1652418B9E9F3" authority="Scopoli, 1777" authorityName="Scopoli" authorityYear="1777" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bulimus semipictus" order="Stylommatophora" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="semipictus">Bulimus semipictus</taxonomicName>
