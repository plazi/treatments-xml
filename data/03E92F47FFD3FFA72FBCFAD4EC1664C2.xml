<caption id="DF3FCED9FFD6FFA62FBCF9BFEC276311" ID-DOI="http://doi.org/10.5281/zenodo.4443772" ID-Zenodo-Dep="4443772" box="[151,1064,1638,1663]" httpUri="https://zenodo.org/record/4443772/files/figure.png" pageId="6" pageNumber="118" startId="6.[151,250,1638,1662]" targetBox="[151,1436,200,1601]" targetPageId="6">
<paragraph id="8BFF9E51FFD6FFA62FBCF9BFEC276311" blockId="6.[151,1064,1638,1663]" box="[151,1064,1638,1663]" pageId="6" pageNumber="118">
