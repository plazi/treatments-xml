<taxonomicName id="629D25C45F4AFFC1FCE2E40E588EFC88" baseAuthorityName="Pic" baseAuthorityYear="1938" box="[772,938,794,819]" class="Insecta" family="Cantharidae" genus="Lycocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="nigripennis">
