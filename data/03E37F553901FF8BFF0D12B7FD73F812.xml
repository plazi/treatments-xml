<number id="3D82DC643901FF8BFCF1155DFB9FFABE" box="[875,935,1374,1398]" pageId="6" pageNumber="245" value="5900.0">5900</number>
. Head, length 
<number id="3D82DC643901FF8BFBCB155DFC46FABE" box="[1105,1150,1374,1398]" pageId="6" pageNumber="245" value="620.0">620</number>
; median width 
<number id="3D82DC643901FF8BFAA9155DFD58FABE" box="[1331,1376,1374,1398]" pageId="6" pageNumber="245" value="370.0">370</number>
; preocellar setae 
<number id="3D82DC643901FF8BFEAF1581F95AFA52" box="[309,354,1410,1434]" pageId="6" pageNumber="245" value="100.0">100</number>
