<paragraph id="116E26756522850CD18D201DFBF09A35" blockId="119.[189,1032,1331,1358]" box="[189,1032,1331,1358]" pageId="119" pageNumber="120">
<taxonomicName id="D6D15DF66522850CD18D201DFD1D9A36" authority="Thuy &amp; Meyer, 2013" authorityName="Thuy &amp; Meyer" authorityYear="2013" box="[189,741,1331,1358]" class="Ophiuroidea" family="Ophiacanthidae" genus="Hanshessia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ophiurida" pageId="119" pageNumber="120" phylum="Echinodermata" rank="species" species="trochitophila">
