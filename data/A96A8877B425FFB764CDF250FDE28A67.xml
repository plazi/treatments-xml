<taxonomicName id="E6C342E2B426FFB764CDF034FEDD8A3A" box="[151,282,258,284]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="splendidula">
