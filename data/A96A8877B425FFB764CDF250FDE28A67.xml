<taxonomicName id="E6C342E2B426FFB765CEF011FE128A66" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[404,469,295,320]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="ignita">
