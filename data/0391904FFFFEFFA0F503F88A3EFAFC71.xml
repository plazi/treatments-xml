<subSection id="E2B738B2FFFCFFA0F6AAFD983EFAFC71" pageId="6" pageNumber="259" type="multiple">
<subSubSection id="C32272D2FFFCFFA0F6AAFD983DE1FDF7" box="[144,440,513,535]" pageId="6" pageNumber="259" type="materials_examined">
