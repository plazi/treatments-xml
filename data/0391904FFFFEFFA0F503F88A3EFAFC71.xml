<caption id="DF4771D1FFFCFFA0F6AAFF5F3EDFFED2" ID-DOI="http://doi.org/10.5281/zenodo.5747404" ID-Zenodo-Dep="5747404" httpUri="https://zenodo.org/record/5747404/files/figure.png" pageId="6" pageNumber="259" startId="6.[144,223,198,217]" targetBox="[337,1273,195,1808]" targetPageId="5">
