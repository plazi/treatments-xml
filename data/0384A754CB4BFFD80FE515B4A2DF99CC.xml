<taxonomicName id="4C2D6DC1CB4BFFD80CE81059A25399F7" box="[922,1099,1904,1930]" class="Arachnida" family="Eriophyidae" genus="Acaphyllisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cryptomeria">
<emphasis id="B959CA50CB4BFFD80CE81059A5B499F7" box="[922,940,1904,1929]" italics="true" pageId="14" pageNumber="15">A</emphasis>
. 
<emphasis id="B959CA50CB4BFFD80CCE1059A25399F7" box="[956,1099,1904,1929]" italics="true" pageId="14" pageNumber="15">cryptomeria</emphasis>
</taxonomicName>
