<taxonomicName id="4C2D6DC1CB4BFFD80EE910B1A45399CF" box="[411,587,1944,1970]" class="Arachnida" family="Eriophyidae" genus="Acaphyllisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cryptomeria">
