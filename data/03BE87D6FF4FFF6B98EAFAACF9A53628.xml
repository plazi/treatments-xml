<quantity id="4CEF9B25FF4FFF6B9993FA1DF875360C" box="[494,564,1519,1545]" metricMagnitude="2" metricUnit="m" metricValue="1.1" pageId="222" pageNumber="223" unit="m" value="110.0">110 m</quantity>
