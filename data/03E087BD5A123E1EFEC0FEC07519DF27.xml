<taxonomicName id="4C494D285A123E1EFB83FD45779DD9E6" class="Insecta" family="Mycetophilidae" genus="Allodiopsis" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="tuomikoskii" subGenus="Notolopha">
