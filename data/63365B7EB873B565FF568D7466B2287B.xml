: 
<collectingCountry id="9388AAF8B873B57AFEA18C9C64DC2CA2" box="[352,413,1453,1477]" name="Malta" pageId="15">
<emphasis id="D9EB367AB873B57AFEA18C9C64DC2CA2" bold="true" box="[352,413,1453,1477]" pageId="15">Male</emphasis>
</collectingCountry>
(
<figureCitation id="73A4F6EDB873B57AFE6D8C9D64B12CA3" box="[428,496,1452,1476]" captionStart="FIGURE 6" captionStartId="12.[151,249,1383,1406]" captionTargetBox="[175,1402,223,1342]" captionTargetId="figure@12.[151,1436,193,1362]" captionTargetPageId="12" captionText="FIGURE 6. Phrudoneura maculata Meuffels &amp; Grootaert; a. male habitus, left lateral; b. male habitus, left lateral (Mt Mandjélia colour variant). P. adusta sp. nov.; c. male habitus, left lateral. P. hibernalis sp. nov.; d. male habitus, left lateral." httpUri="https://zenodo.org/record/218314/files/figure.png" pageId="15">
Fig. 
<number id="5D57F84FB873B57AFE218C9D64B12CA3" box="[480,496,1452,1476]" pageId="15" value="6.0">6</number>
</figureCitation>
c): length: 
<number id="5D57F84FB873B57AFDA98C9D67CF2CA3" box="[616,654,1452,1476]" pageId="15" value="2.1">2.1</number>
; wing: 
<number id="5D57F84FB873B57AFD218C9D66422CA3" box="[736,771,1452,1476]" pageId="15" value="2.2">2.2</number>
x 
<number id="5D57F84FB873B57AFCDE8C9D66042CA3" box="[799,837,1452,1476]" pageId="15" value="0.9">0.9</number>
; similar to 
<taxonomicName id="2C9F91EBB873B57AFC028C9C610E2CA3" box="[963,1103,1452,1476]" class="Insecta" family="Dolichopodidae" genus="Phrudoneura" kingdom="Animalia" order="Diptera" pageId="15" phylum="Arthropoda" rank="species" species="hibernalis">
