<taxonomicName id="4C2AFF934D7AFFBA4AB69023BA8EFBA2" authority="Hartmann-Schroder, 1960" authorityName="Hartmann-Schroder" authorityYear="1960" box="[490,953,1077,1103]" class="Polychaeta" family="Syllidae" genus="Alcyonosyllis" kingdom="Animalia" order="Phyllodocida" pageId="1" pageNumber="36" phylum="Annelida" rank="species" species="bisetosa">
<emphasis id="B95E58024D7AFFBA4AB69023BB5CFBA3" box="[490,619,1077,1102]" italics="true" pageId="1" pageNumber="36">A. bisetosa</emphasis>
(Hartmann-Schröder, 
<number id="3DE296374D7AFFBA482E9023BA87FBA2" box="[882,944,1077,1103]" pageId="1" pageNumber="36" value="1960.0">1960</number>
)
</taxonomicName>
, and 
<taxonomicName id="4C2AFF934D7AFFBA48AA9023B9E2FB99" authority="Glasby &amp; Aguado, 2009" authorityName="Glasby &amp; Aguado" authorityYear="2009" class="Polychaeta" family="Syllidae" genus="Alcyonosyllis" kingdom="Animalia" order="Phyllodocida" pageId="1" pageNumber="36" phylum="Annelida" rank="species" species="hinterkircheri">
<emphasis id="B95E58024D7AFFBA48AA9023BD8EFBA3" box="[1014,1209,1077,1102]" italics="true" pageId="1" pageNumber="36">A. hinterkircheri</emphasis>
<bibRefCitation id="EFBBF9E14D7AFFBA4F9C9023B9E2FB99" author="Glasby" pageId="1" pageNumber="46" refString="Glasby, C. J &amp; Aguado, M. T. (2009) A new species and new records of the anthozoan commensal genus Alcyonosyllis (Polychaeta: Syllidae). The Beagle, Records of the Museums and Art Galleries of the Northern Territory, 25: 55 - 63." type="journal article" year="2009">
Glasby &amp; Aguado, 
<number id="3DE296374D7AFFBA4BCB904CB9E2FB99" box="[151,213,1114,1140]" pageId="1" pageNumber="36" value="2009.0">2009</number>
</bibRefCitation>
</taxonomicName>
. Three other species, 
<taxonomicName id="4C2AFF934D7AFFBA4A8D904DBAD9FB99" authority="Hartmann-Schroder, 1991" authorityName="Hartmann-Schroder" authorityYear="1991" box="[465,1006,1114,1140]" class="Polychaeta" family="Syllidae" genus="Syllis" kingdom="Animalia" order="Phyllodocida" pageId="1" pageNumber="36" phylum="Annelida" rank="species" species="onkylochaeta">
<emphasis id="B95E58024D7AFFBA4A8D904DBB83FB99" box="[465,692,1115,1140]" italics="true" pageId="1" pageNumber="36">Syllis onkylochaeta</emphasis>
Hartmann-Schröder, 
<number id="3DE296374D7AFFBA48ED904CBAD9FB99" box="[945,1006,1114,1140]" pageId="1" pageNumber="36" value="1991.0">1991</number>
</taxonomicName>
