<taxonomicName id="4C194A89F27DFF838AB7F8DFFDE4F888" authority="Needham &amp; Murphy 1924" authorityName="Needham &amp; Murphy" authorityYear="1924" box="[151,595,1831,1857]" class="Insecta" family="Baetidae" genus="Baetodes" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
<emphasis id="B96DED18F27DFF838AB7F8DFFDE4F888" bold="true" box="[151,595,1831,1857]" pageId="2" pageNumber="3">
<emphasis id="B96DED18F27DFF838AB7F8DFFEB3F888" bold="true" box="[151,260,1831,1857]" italics="true" pageId="2" pageNumber="3">Baetodes</emphasis>
<bibRefCitation id="EF884CFBF27DFF838B2BF8DFFDE4F888" author="Needham" box="[267,595,1831,1857]" pageId="2" pageNumber="3" refString="Needham, J. G. &amp; Murphy, H. E. (1924) Neotropical mayflies. Bulletin of the Lloyd Library, Number 24, Entomological Series, 4, 1 - 79." type="journal article" year="1924">
Needham &amp; Murphy 
<number id="3DD1232DF27DFF838839F8DFFDE4F888" box="[537,595,1831,1857]" pageId="2" pageNumber="3" value="1924.0">1924</number>
</bibRefCitation>
