<paragraph id="F9DE846664A07FBE3AF55F0D96DBB735" pageId="0" pageNumber="95">Type species.</paragraph>
<paragraph id="393C1E9169C0F7E5D2392D7049358788" pageId="0" pageNumber="95">
<taxonomicName id="675B74EC52EF2DB5FA9E130084B31871" authority="Lin &amp; Li, 2020" authorityName="Lin &amp; Li" authorityYear="2020" class="Arachnida" family="Salticidae" genus="Dendroicius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Dendroicius hotaruae" order="Araneae" pageId="0" pageNumber="95" phylum="Arthropoda" rank="species" species="hotaruae">
<emphasis id="8EFFB0C3618A5A6A9D1544F13E6B6739" italics="true" pageId="0" pageNumber="95">Dendroicius hotaruae</emphasis>
</taxonomicName>
