<taxonomicName id="969EBEC59A8C41AA4888F5C582E4DF0C" authority="formosus" authorityName="Labillardière" authorityYear="1806" class="Magnoliopsida" family="Myrtaceae" genus="Calothamnus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Calothamnus formosus" order="Myrtales" pageId="0" pageNumber="45" phylum="Tracheophyta" rank="species" species="formosus">
