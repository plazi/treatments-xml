<taxonomicName id="7FA01E81FFECFFA739E2FCACFCEDA135" authority="Blanchard, 1851" authorityName="Blanchard" authorityYear="1851" box="[195,892,861,887]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="122" phylum="Arthropoda" rank="species" species="menetriesi" subGenus="Phyllophaga">
