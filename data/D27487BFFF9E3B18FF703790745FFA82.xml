<taxonomicName id="9DDD4D2AFF9E3B18FE31308F77F3FAE6" box="[437,742,1261,1299]" class="Arachnida" family="Sparassidae" genus="Rhitymna" kingdom="Animalia" order="Araneae" pageId="28" pageNumber="57" phylum="Arthropoda" rank="species" species="verruca">
