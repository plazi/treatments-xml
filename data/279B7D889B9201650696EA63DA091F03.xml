<taxonomicName id="4B65043C2804BCA5676170E2A66B1AC6" LSID="urn:lsid:zoobank.org:act:53B538F7-D7F3-47C9-9A95-5BC50A01AC31" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus ixodiformis" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="ixodiformis">Trigonopterus ixodiformis Riedel</taxonomicName>
