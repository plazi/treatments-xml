<taxonomicName id="6DD84D1EDD4CFFB44902FC44FBBAFC74" authority="Sieboldii Tschudi, 1838" authorityName="Sieboldii Tschudi" authorityYear="1838" box="[576,1071,908,934]" class="Amphibia" family="Cryptobranchidae" genus="Megalobatrachus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Caudata" pageId="34" pageNumber="59" phylum="Chordata" rank="genus">
