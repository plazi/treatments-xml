Size: ds= 
<number id="3DEEC340B85E1C3BED67F935FED5F954" box="[266,316,1696,1722]" pageId="2" pageNumber="57" value="0.71">0.71</number>
± 
<quantity id="4CDE7C82B85E1C3BED21F935FE50F954" box="[332,441,1696,1722]" metricMagnitude="-5" metricUnit="m" metricValue="2.0" pageId="2" pageNumber="57" unit="mm" value="0.02">0.02 mm</quantity>
