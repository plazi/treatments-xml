<number id="3DEEC340B85E1C3BED9FF935FDCFF954" box="[498,550,1696,1722]" pageId="2" pageNumber="57" value="0.62">0.62</number>
± 
<quantity id="4CDE7C82B85E1C3BEE5BF935FD4BF954" box="[566,674,1696,1722]" metricMagnitude="-5" metricUnit="m" metricValue="3.0" pageId="2" pageNumber="57" unit="mm" value="0.03">0.03 mm</quantity>
