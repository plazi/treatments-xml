<taxonomicName id="4C3AD2DC6646E848C2855999FB3C1CF9" box="[948,1173,461,486]" class="Insecta" family="Cicadellidae" genus="Aphrodes" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="bicinctus">
