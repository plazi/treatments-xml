<typeStatus id="54B1883AD113FFF1269BF972EE210591" box="[197,256,1746,1772]" pageId="2" pageNumber="21">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54B1883AD113FFF127DAF972EED00591" box="[388,497,1746,1772]" pageId="2" pageNumber="21">Holotype</typeStatus>
<number id="3DC224BFD113FFF127A7F972ED2E0591" box="[505,527,1746,1772]" pageId="2" pageNumber="21" value="3.0">3</number>
, Guizhou province, Ziyun, 
<quantity id="4CF29B7DD113FFF12509F972EC950591" box="[855,948,1746,1772]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="2" pageNumber="21" unit="m" value="1300.0">1300 m</quantity>
, 
<date id="FFB41058D113FFF1259EF972EB780591" box="[960,1113,1746,1772]" pageId="2" pageNumber="21" value="2008-04-06">
<number id="3DC224BFD113FFF1259EF972ECEE0591" box="[960,975,1746,1772]" pageId="2" pageNumber="21" value="6.0">6</number>
April 
<number id="3DC224BFD113FFF12242F972EB780591" box="[1052,1113,1746,1772]" pageId="2" pageNumber="21" value="2008.0">2008</number>
</date>
, L. M. Wei leg.; 
