<paragraph id="82822C0A023B6CC8991D07189D79FEEB" pageId="0" pageNumber="1">
Israel • 1 spcm; Palmachim; 
<geoCoordinate id="0E92C029303143EACB2DE1ED43A2F020" degrees="31.9574" direction="north" orientation="latitude" precision="5" value="31.9574">31.9574°N</geoCoordinate>
, 
<geoCoordinate id="6CDA29EB8D55A8E014C6FDF300EB55AB" degrees="34.6645" direction="east" orientation="longitude" precision="5" value="34.6645">34.6645°E</geoCoordinate>
