<taxonomicName id="4C180ABDFF9A2314684D25A7D5B6628E" box="[327,423,1989,2013]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="42" phylum="Arthropoda" rank="genus">
<emphasis id="B96CAD2CFF9A2314684D25A7D5B6628E" box="[327,423,1989,2013]" italics="true" pageId="13" pageNumber="42">Aenictus</emphasis>
