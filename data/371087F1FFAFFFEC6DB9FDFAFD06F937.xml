<number id="097124C0FFAFFFEC6C39FBA8FED4FBB6" box="[279,331,1091,1115]" pageId="1" pageNumber="58" value="0.05">0.05</number>
; ALE-PME 
<number id="097124C0FFAFFFEC6CF9FBA8FD94FBB6" box="[471,523,1091,1115]" pageId="1" pageNumber="58" value="0.05">0.05</number>
, PLE-PLE 
<number id="097124C0FFAFFFEC6FA4FBA8FD21FBB6" box="[650,702,1091,1115]" pageId="1" pageNumber="58" value="0.05">0.05</number>
, PLE-PME 
<number id="097124C0FFAFFFEC6E6AFBA8FCE7FBB6" box="[836,888,1091,1115]" pageId="1" pageNumber="58" value="0.08">0.08</number>
; AER 
<number id="097124C0FFAFFFEC6EEDFBA8FC68FBB6" box="[963,1015,1091,1115]" pageId="1" pageNumber="58" value="0.1">0.10</number>
