<number id="097124C0FFAFFFEC6C69FB3AFEE7FB04" box="[327,376,1233,1257]" pageId="1" pageNumber="58" value="4.1">4.10</number>
(
<number id="097124C0FFAFFFEC6CA4FB3AFE21FB04" box="[394,446,1233,1257]" pageId="1" pageNumber="58" value="1.25">1.25</number>
, 
<number id="097124C0FFAFFFEC6CE5FB3AFE60FB04" box="[459,511,1233,1257]" pageId="1" pageNumber="58" value="0.2">0.20</number>
