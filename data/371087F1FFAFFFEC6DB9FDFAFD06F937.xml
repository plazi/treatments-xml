<number id="097124C0FFAFFFEC6EEDFB3AFC6BFB04" box="[963,1012,1233,1257]" pageId="1" pageNumber="58" value="1.5">1.50</number>
<number id="097124C0FFAFFFEC6ED3FB3AFBAEFB04" box="[1021,1073,1233,1257]" pageId="1" pageNumber="58" value="1.3">1.30</number>
