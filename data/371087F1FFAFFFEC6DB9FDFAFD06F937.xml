, 
<number id="097124C0FFAFFFEC6E90FB45FC6EFB2B" box="[958,1009,1198,1222]" pageId="1" pageNumber="58" value="1.85">1.85</number>
, 
<number id="097124C0FFAFFFEC6ED0FB45FBADFB2B" box="[1022,1074,1198,1222]" pageId="1" pageNumber="58" value="1.4">1.40</number>
, 
<number id="097124C0FFAFFFEC6910FB45FBECFB2B" box="[1086,1139,1198,1222]" pageId="1" pageNumber="58" value="0.9">0.90</number>
); II 
<number id="097124C0FFAFFFEC698CFB45FB4CFB2B" box="[1186,1235,1198,1222]" pageId="1" pageNumber="58" value="5.08">5.08</number>
(
<number id="097124C0FFAFFFEC69CDFB45FA88FB2B" box="[1251,1303,1198,1222]" pageId="1" pageNumber="58" value="1.5">1.50</number>
, 
<number id="097124C0FFAFFFEC680AFB45FAC8FB2B" box="[1316,1367,1198,1222]" pageId="1" pageNumber="58" value="0.2">0.20</number>
, 
<number id="097124C0FFAFFFEC684AFB45FA07FB2B" box="[1380,1432,1198,1222]" pageId="1" pageNumber="58" value="1.42">1.42</number>
