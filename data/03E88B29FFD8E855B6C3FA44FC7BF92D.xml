<taxonomicName id="4C4141BCFFDCE855B748FB68FE76FB75" box="[284,448,1178,1200]" class="Insecta" family="Formicidae" genus="Protanilla" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="123" phylum="Arthropoda" rank="species" species="jongi" status="sp. nov.">
