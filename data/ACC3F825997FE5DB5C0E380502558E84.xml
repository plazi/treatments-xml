<taxonomicName id="84AF75C7EECAAF925FED729137E8F9DC" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24842" class="Insecta" family="Formicidae" genus="Pachycondyla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pachycondyla Smith" order="Hymenoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="genus">Pachycondyla</taxonomicName>
