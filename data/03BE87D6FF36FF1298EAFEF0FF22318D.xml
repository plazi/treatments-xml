<quantity id="4CEF9B25FF36FF129BB0FEF0FE543118" box="[973,1045,258,285]" metricMagnitude="-1" metricUnit="m" metricValue="4.0" pageId="167" pageNumber="168" unit="cm" value="40.0">40 cm</quantity>
