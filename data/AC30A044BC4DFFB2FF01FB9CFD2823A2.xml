<taxonomicName id="E3996AD1BC4EFFB2FCD5FDD2FC1B248F" authorityName="Assing" authorityYear="2011" box="[870,971,624,645]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="1149" phylum="Arthropoda" rank="species" species="akiana">
