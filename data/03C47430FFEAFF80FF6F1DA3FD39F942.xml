17. 
<taxonomicName id="4C6DBEA5FFEAFF80FF371DA3FD35FE5F" authority="Chevrolat, 1862" authorityName="Chevrolat" authorityYear="1862" box="[227,644,455,476]" class="Insecta" family="Cerambycidae" genus="Elateropsis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="363" phylum="Arthropoda" rank="species" species="sericeiventris">
<emphasis id="B9191934FFEAFF80FF371DA3FE69FE5F" bold="true" box="[227,472,455,476]" italics="true" pageId="10" pageNumber="363">Elateropsis sericeiventris</emphasis>
Chevrolat, 1862
</taxonomicName>
</emphasis>
(
<figureCitation id="1356D9A3FFEAFF80FE8B1D86FE00FE74" box="[351,433,482,503]" captionStart="Fig" captionStartId="9.[175,207,1631,1650]" captionTargetBox="[166,1154,199,1608]" captionTargetId="figure-1@9.[151,1171,183,1636]" captionTargetPageId="9" captionText="Fig. 3. Dorsal habitus of HispaniolanPrioninae.a) Elateropsis femoratus, male,b) Elateropsis lineatus, female, c) Elateropsis quinquenotatus, female, d) Elateropsis sericeiventris, male, e) Elateropsis trimarginatus, female, f) Elateropsis dichroma, female holotype, g) Elateropsis woodleyi, male holotype, h) Solenoptera dominicensis, male, i) S. dominicensis, female." pageId="10" pageNumber="363">Figs. 3d</figureCitation>
, 
<figureCitation id="1356D9A3FFEAFF80FE6A1D86FE51FE74" box="[446,480,482,503]" captionStart="Fig" captionStartId="31.[175,207,1151,1170]" captionTargetBox="[151,1171,183,1127]" captionTargetId="figure-231@31.[151,1171,183,1127]" captionTargetPageId="31" captionText="Fig. 16. Distribution maps of Hispaniolan Prioninae. a) Elateropsis sericeiventris, Elateropsis trimarginatus, Elateropsis dichroma, and Elateropsis woodleyi, b) Solenoptera dominicensis, Solenoptera scutellata, and Solenoptera rugosa." pageId="10" pageNumber="363">16a</figureCitation>
