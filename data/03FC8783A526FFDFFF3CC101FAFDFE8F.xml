</taxonomicName>
, and 
<taxonomicName id="4C554D16A527FFDFFB55C5FEFA57FF37" authorityName="Zheng, Bai &amp; Xu" authorityYear="2011" box="[1278,1436,223,249]" class="Insecta" family="Tetrigidae" genus="Flatocerus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="508" phylum="Arthropoda" rank="species" species="nigritibialis">
<emphasis id="B921EA87A527FFDFFB55C5FEFA57FF37" box="[1278,1436,223,249]" italics="true" pageId="7" pageNumber="508">F. nigritibialis</emphasis>
</taxonomicName>
