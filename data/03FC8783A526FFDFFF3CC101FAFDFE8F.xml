<taxonomicName id="4C554D16A527FFDFFB89C5FEFB09FF37" authorityName="Zheng, Zhang &amp; Zeng" authorityYear="2011" box="[1058,1218,223,249]" class="Insecta" family="Tetrigidae" genus="Flatocerus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="508" phylum="Arthropoda" rank="species" species="nigrifemura">
