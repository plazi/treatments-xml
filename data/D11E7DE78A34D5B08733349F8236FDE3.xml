See 
<taxonomicName id="5BF39353E0C0872117716D8C7364688E" class="Insecta" family="Gracillariidae" genus="Liocrobyla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liocrobyla desmodiella" order="Lepidoptera" pageId="0" pageNumber="305" phylum="Arthropoda" rank="species" species="desmodiella">Liocrobyla desmodiella</taxonomicName>
