<taxonomicName id="4C0F4D04F437FF8DFBF5FC034491FC51" box="[1024,1155,957,981]" class="Insecta" family="Noctuidae" genus="Lophoptera" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
