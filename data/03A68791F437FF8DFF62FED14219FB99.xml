<taxonomicName id="4C0F4D04F437FF8DFBC3FC5C4466FC7D" box="[1078,1140,994,1017]" class="Insecta" family="Noctuidae" genus="Lophoptera" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="nama">
