<taxonomicName id="4C0F4D04F437FF8DFF62FBBB4108FB99" box="[151,282,1029,1053]" class="Insecta" family="Noctuidae" genus="Lophoptera" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
