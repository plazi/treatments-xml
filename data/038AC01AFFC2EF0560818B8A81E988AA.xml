(
<collectionCode id="ED32E9C9FFC1EF0563428E3886AC88D6" box="[852,918,368,392]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34854" name="University of Colorado Museum of Natural History" pageId="2" pageNumber="583">UCM</collectionCode>
