<taxonomicName id="516BFD81E971691774A7A90EA1E6DD36" authority="L." class="Magnoliopsida" family="Polygalaceae" genus="Polygala" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="645" phylum="Tracheophyta" rank="species" species="vulgaris">
