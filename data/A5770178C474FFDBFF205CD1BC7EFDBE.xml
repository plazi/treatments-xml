<paragraph id="2D61B06EC474FFDBFF205CD1B88BFF51" blockId="19.[151,418,151,177]" box="[151,418,151,177]" pageId="19" pageNumber="220">
<heading id="76290702C474FFDBFF205CD1B88BFF51" bold="true" box="[151,418,151,177]" fontSize="11" level="1" pageId="19" pageNumber="220" reason="1">
<taxonomicName id="EADECBEDC474FFDBFF205CD1B88BFF51" authority="Priesner" authorityName="Priesner" box="[151,418,151,177]" class="Insecta" family="Phlaeothripidae" genus="Streptothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="genus">
