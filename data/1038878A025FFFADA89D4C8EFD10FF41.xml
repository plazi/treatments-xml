<paragraph id="982E369C025FFFABA8044D9CFBBFFA22" blockId="27.[809,1424,1418,1470]" pageId="27">
<emphasis id="AAE5EA8E025FFFABA8044D9CFC70FA03" box="[809,958,1418,1439]" italics="true" pageId="27">
<typeStatus id="472A883E025FFFABA8044D9CFC91FA03" box="[809,863,1418,1439]" pageId="27">Type</typeStatus>
