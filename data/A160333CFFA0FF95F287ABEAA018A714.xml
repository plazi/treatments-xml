<taxonomicName id="EEC9F9A9FFA0FF95F651A98DA683A06C" authorityName="in Canberra. In" authorityYear="1976" box="[1089,1235,751,776]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="baileyana">
