<taxonomicName id="EEC9F9A9FFA0FF95F12DA869A1BBA049" box="[829,1003,788,813]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="melanoxylon">
