<taxonomicName id="EEC9F9A9FFA0FF95F1CCA9D4A60BA1A4" authorityName="Lea" authorityYear="1899" box="[988,1115,681,704]" class="Insecta" family="Curculionidae" genus="Melanterius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="acaciae">
