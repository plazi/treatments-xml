<taxonomicName id="C4FDCFFEED5D4651E0E0FDADD4264A75" authority="HAITLINGER 1991" authorityName="HAITLINGER" authorityYear="1991" box="[178,629,584,606]" class="Insecta" family="Laemophloeidae" genus="Leptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="1198" phylum="Arthropoda" rank="species" species="mariani" subGenus="Leptus">
<emphasis id="3189686FED5D4651E0E0FDADD4264A75" bold="true" box="[178,629,584,606]" pageId="1" pageNumber="1198">
<emphasis id="3189686FED5D4651E0E0FDADD6AB4A75" bold="true" box="[178,248,585,606]" italics="true" pageId="1" pageNumber="1198">Leptus</emphasis>
(
<emphasis id="3189686FED5D4651E155FDADD71E4A75" bold="true" box="[263,333,585,606]" italics="true" pageId="1" pageNumber="1198">Leptus</emphasis>
) 
<emphasis id="3189686FED5D4651E10EFDADD7FC4A75" bold="true" box="[348,431,585,606]" italics="true" pageId="1" pageNumber="1198">mariani</emphasis>
<bibRefCitation id="676CC98CED5D4651E1E4FDADD4684A76" author="HAITLINGER R" box="[438,571,585,606]" pageId="1" pageNumber="1198" pagination="265 - 272" refId="ref3407" refString="HAITLINGER R. (1991): Six new species of Leptus Latreille, 1796 (Acari, Prostigmata, Erythraeidae) from Neotropical Region - Zesz. Nauk. Akad. Roln. Wroc., Zootechnika - 35 (206): 265 - 272." type="journal article" year="1991">HAITLINGER</bibRefCitation>
