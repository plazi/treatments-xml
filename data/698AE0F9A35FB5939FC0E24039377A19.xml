<taxonomicName id="7A114FD060AB329DA83BEAA8C50DC317" class="Insecta" family="Perlidae" genus="Neoperla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neoperla duratubulata" order="Plecoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="duratubulata">Neoperla duratubulata</taxonomicName>
