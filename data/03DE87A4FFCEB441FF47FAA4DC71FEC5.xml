<quantity id="4C8F9B57FFCEB44FFD74FA35DCF3FA50" box="[676,754,1496,1521]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="6" pageNumber="557" unit="m" value="2500.0">2500m</quantity>
