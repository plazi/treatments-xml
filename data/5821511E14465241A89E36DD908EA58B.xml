<taxonomicName id="F871135B8593931D3A110F2B6D64C110" class="Insecta" genus="T." kingdom="Animalia" lsidName="T. (V.) semivittata" order="Diptera" pageId="0" pageNumber="23" phylum="Arthropoda" rank="species" species="semivittata" subGenus="V.">T. (V.) semivittata</taxonomicName>
