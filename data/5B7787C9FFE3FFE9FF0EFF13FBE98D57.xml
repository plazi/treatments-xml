<paragraph id="D36136DFFFE3FFE9FF5DFBD1FDCA8C12" blockId="7.[151,1437,1034,1322]" box="[196,602,1109,1135]" pageId="7" pageNumber="8">
<emphasis id="E1AAEACDFFE3FFE9FF5DFBD1FE038C12" bold="true" box="[196,403,1109,1135]" pageId="7" pageNumber="8">Elevation range:</emphasis>
<quantity id="14269B3AFFE3FFE9FE02FBD1FE628C12" box="[411,498,1109,1135]" metricMagnitude="2" metricUnit="m" metricValue="5.36448" pageId="7" pageNumber="8" unit="ft" value="1760.0">1760 ft</quantity>
. (
<quantity id="14269B3AFFE3FFE9FD9CFBD1FDC28C12" box="[517,594,1109,1135]" metricMagnitude="2" metricUnit="m" metricValue="5.36" pageId="7" pageNumber="8" unit="m" value="536.0">536 m</quantity>
