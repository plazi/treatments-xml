<typeStatus id="54CD8866FF95FFEBFF37390F22B8F89A" box="[151,201,1829,1850]" pageId="0" pageNumber="428">Type</typeStatus>
species: 
<taxonomicName id="4C764D47FF95FFEBFE86390F23BEF89A" box="[294,463,1829,1850]" class="Insecta" family="Sesiidae" genus="Microsesia" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="428" phylum="Arthropoda" rank="species" species="nana">
