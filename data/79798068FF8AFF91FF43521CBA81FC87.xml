<taxonomicName id="36D04AFDFF8BFF96FB825297BA97F966" authorityName="Shear" authorityYear="1971" box="[1110,1208,1724,1747]" class="Diplopoda" family="Trichopetalidae" genus="Scoterpes" kingdom="Animalia" order="Chordeumatida" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="ventus">S. ventus</taxonomicName>
