spp. 
<emphasis id="B934F46814434E71B4046B7787F5DBB5" bold="true" box="[884,939,1894,1920]" pageId="12" pageNumber="13">A–B</emphasis>
. 
<taxonomicName id="4C4053F914434E71B4B16B768012DBB5" authority="Zlatkov &amp; Huemer, 2017" authorityName="Zlatkov &amp; Huemer" authorityYear="2017" box="[961,1100,1894,1920]" class="Insecta" family="Tortricidae" genus="Phtheochroa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="alpinana" status="sp. nov.">
