<taxonomicName id="4C4053F914434E71B57F6B9B86EBDB96" authority="Zlatkov &amp; Huemer, 2017" authorityName="Zlatkov &amp; Huemer" authorityYear="2017" box="[527,693,1930,1955]" class="Insecta" family="Tortricidae" genus="Phtheochroa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="apenninana" status="sp. nov.">
