<taxonomicName id="6DFD27A9BF57FFEB5A82F90D37314C68" box="[509,607,1770,1796]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="98898038BF57FFEB5A82F90D375A4C6F" box="[509,564,1770,1795]" italics="true" pageId="58" pageNumber="59">Acer</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="6DFD27A9BF57FFEB5913F90D37864C68" box="[620,744,1770,1796]" class="Magnoliopsida" family="Aceraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="family">Aceraceae</taxonomicName>
