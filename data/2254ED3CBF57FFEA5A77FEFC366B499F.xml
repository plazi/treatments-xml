<taxonomicName id="6DFD27A9BF57FFEB5A92F92537FE4DB7" authorityName="Franclemont. Based" authorityYear="1980" box="[493,656,1730,1755]" class="Insecta" family="Noctuidae" genus="Xestia" kingdom="Animalia" order="Lepidoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="dolosa">
