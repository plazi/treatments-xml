<taxonomicName id="CDEE4D1F2B34FFB6FDB8FF3C68D3F7F2" box="[606,720,192,217]" class="Rhabditophora" family="Dalyelliidae" genus="Gieysztoria" kingdom="Animalia" order="Rhabdocoela" pageId="5" pageNumber="6" phylum="Platyhelminthes" rank="species" species="ornata">
