<taxonomicName id="AA569D7EAFBBD8F30ED23597E80DD0BD" baseAuthorityName="LeConte" baseAuthorityYear="1847" class="Hexapoda" family="Carabidae" genus="Bradycellus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="670" pageNumber="671" phylum="Arthropoda" rank="species" species="suturalis">
