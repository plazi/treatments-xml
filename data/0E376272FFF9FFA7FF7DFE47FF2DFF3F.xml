<number id="3056C143FFF9FFA1FBADFA0CFB8EFA3E" box="[1095,1141,1468,1493]" pageId="3" pageNumber="82" value="250.0">250</number>
; width across cheeks 
<number id="3056C143FFF9FFA1FA80FA0DFA6CFA3E" box="[1386,1431,1469,1493]" pageId="3" pageNumber="82" value="180.0">180</number>
