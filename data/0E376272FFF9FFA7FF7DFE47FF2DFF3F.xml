<number id="3056C143FFF9FFA1FC11FA6FFBE2FA13" box="[1019,1049,1503,1528]" pageId="3" pageNumber="82" value="45.0">45</number>
, aa 
<number id="3056C143FFF9FFA1FBA4FA50FB97FA13" box="[1102,1132,1504,1528]" pageId="3" pageNumber="82" value="40.0">40</number>
