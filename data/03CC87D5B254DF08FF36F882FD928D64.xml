<emphasis id="B911EAD1B255DF08FF66FEB0FEAF8D1C" bold="true" box="[199,315,259,285]" pageId="7" pageNumber="308">Remarks.</emphasis>
<taxonomicName id="4C654D40B255DF08FEE0FEB0FDA28D1C" authorityName="Nel &amp; Varenne" authorityYear="2019" box="[321,566,259,285]" class="Insecta" family="Psychidae" genus="Penestoglossa" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="308" phylum="Arthropoda" rank="species" species="gallica">
<emphasis id="B911EAD1B255DF08FEE0FEB0FDA28D1C" box="[321,566,259,285]" italics="true" pageId="7" pageNumber="308">Penestoglossa gallica</emphasis>
</taxonomicName>
