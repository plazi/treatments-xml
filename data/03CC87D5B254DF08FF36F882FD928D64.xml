<taxonomicName id="4C654D40B255DF08FAA1FEB0FEDA8D43" authority="(Milliere, 1863)" baseAuthorityName="Milliere" baseAuthorityYear="1863" class="Insecta" family="Psychidae" genus="Penestoglossa" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="308" phylum="Arthropoda" rank="species" species="dardoinella">
