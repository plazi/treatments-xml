<paragraph id="8B9136ABEF51FFF2FEC9FE5500E50EE7" blockId="15.[264,1323,284,510]" pageId="15" pageNumber="16">
<taxonomicName id="4C2E4D28EF51FFF2FEC9FE5506190EB4" box="[312,562,404,429]" class="Insecta" family="Bombyliidae" genus="Bombylius" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="canescens">
