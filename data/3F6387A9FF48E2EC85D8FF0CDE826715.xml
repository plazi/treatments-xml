<taxonomicName id="70CA4D3CFF48E2EC87DAFDD3DE526731" box="[661,860,511,536]" class="Hydrozoa" family="Sertulariidae" genus="Symplectoscyphus" kingdom="Animalia" order="Leptothecata" pageId="165" pageNumber="166" phylum="Cnidaria" rank="species" species="subdichotomus">
