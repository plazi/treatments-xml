, at 
<geoCoordinate id="D2FE5078FF48E2EC862BFEFBDEB56469" box="[868,955,296,320]" direction="south" orientation="latitude" pageId="165" pageNumber="166" precision="555" value="-43.88">43.88ºS</geoCoordinate>
