<quantity id="70329B5AFF48E2EC8716FE0FDF8464DD" box="[601,650,476,500]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="165" pageNumber="166" unit="m" value="30.0">30m</quantity>
