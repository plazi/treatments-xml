<geoCoordinate id="D2FE5078FF48E2EC842FFEA3DCA364A1" box="[352,429,368,392]" direction="south" orientation="latitude" pageId="165" pageNumber="166" precision="555" value="-8.71">8.71°S</geoCoordinate>
35°W, from 
<geoCoordinate id="D2FE5078FF48E2EC870BFEA3DF9E64A1" box="[580,656,368,392]" direction="south" orientation="latitude" pageId="165" pageNumber="166" precision="555" value="-9.7">9.70°S</geoCoordinate>
to 
<geoCoordinate id="D2FE5078FF48E2EC87F7FEBCDE0864A1" box="[696,774,367,392]" direction="south" orientation="latitude" pageId="165" pageNumber="166" precision="555" value="-9.95">9.95°S</geoCoordinate>
, at 
<geoCoordinate id="D2FE5078FF48E2EC867EFEA3DE8564A1" box="[817,907,368,392]" direction="south" orientation="latitude" pageId="165" pageNumber="166" precision="555" value="-20.3">20.30°S</geoCoordinate>
