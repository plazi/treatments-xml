<geoCoordinate id="D2FE5078FF48E2EC8105FEFBD9AF6469" box="[1098,1185,296,320]" direction="south" orientation="latitude" pageId="165" pageNumber="166" precision="555" value="-43.93">43.93ºS</geoCoordinate>
