<heading id="B36D81A2905AFF92E0A1A6FC14C6FD45" bold="true" box="[151,463,726,752]" fontSize="11" level="1" pageId="13" pageNumber="588" reason="1">
<taxonomicName id="2F9A4D4D905AFF92E0A1A6FC14C6FD45" authority="Chace, 1985" authorityName="Chace" authorityYear="1985" box="[151,463,726,752]" class="Malacostraca" family="Pandalidae" genus="Parapandalus" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="588" phylum="Arthropoda" rank="species" species="simulatrix">
