<taxonomicName id="7EB27B174A486C32F96D80DDFED1C89D" LSID="http://zoobank.org/7C3A602B-EA0F-46D5-8CEE-15BAA2875D00" authority="Brothers &amp; Lelej" family="Mutillidae" genus="Euspinoliina" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Euspinoliina" order="Hymenoptera" pageId="10" pageNumber="11" rank="subTribe" subTribe="Euspinoliina">Euspinoliina Brothers &amp; Lelej</taxonomicName>
