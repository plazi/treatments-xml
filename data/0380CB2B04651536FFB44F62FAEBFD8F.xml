<taxonomicName id="4C2901BE04651536FB134C49FB12FD8E" authorityName="P. J. Fischer" authorityYear="1921" box="[1072,1273,608,634]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="108" pageNumber="109" phylum="Mollusca" rank="species" species="malayana">
