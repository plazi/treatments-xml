</taxonomicName>
and 
<taxonomicName id="4C674D7EFFC5FFCEFBBBA375FAE3AAEE" box="[1053,1319,1916,1940]" class="Anthozoa" family="Plexauridae" genus="Plexaurella" kingdom="Animalia" order="Alcyonacea" pageId="18" pageNumber="19" phylum="Cnidaria" rank="species" species="grandiflora">
<emphasis id="B913EAEFFFC5FFCEFBBBA375FAE3AAEE" box="[1053,1319,1916,1940]" italics="true" pageId="18" pageNumber="19">Plexaurella grandiflora</emphasis>
</taxonomicName>
) at depths between 
<number id="3DAF24DAFFC5FFCEFF5DA3A9FED3AAC2" box="[251,279,1952,1976]" pageId="18" pageNumber="19" value="10.0">10</number>
and 
<quantity id="4C9F9B18FFC5FFCEFEEBA3A9FE42AAC2" box="[333,390,1952,1976]" metricMagnitude="1" metricUnit="m" metricValue="1.7" pageId="18" pageNumber="19" unit="m" value="17.0">17 m</quantity>
(
<bibRefCitation id="EFF64B0CFFC5FFCEFE33A3A9FD88AAC2" author="De" box="[405,588,1952,1976]" pageId="18" pageNumber="19" refString="De Grave, S. (2008) A new species of Periclimenes (Crustacea: Decapoda; Palaemonidae) from Guarapari, Espirito Santo, Brazil. Zootaxa, 1925, 14 - 22." type="journal article" year="2008">
