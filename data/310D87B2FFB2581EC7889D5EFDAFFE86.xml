<taxonomicName id="7EA44D27FFAE5819C5699D46FC18FAAF" box="[817,1003,1378,1402]" class="Insecta" family="Chrysomelidae" genus="Mimastra" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="838" phylum="Arthropoda" rank="species" species="undetermined">
