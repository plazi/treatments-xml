and 
<taxonomicName id="7EA44D27FFAE5819C75C9E66FE34F98F" authorityName="Jacoby" authorityYear="1886" box="[260,455,1603,1626]" class="Insecta" family="Chrysomelidae" genus="Mimastra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="838" phylum="Arthropoda" rank="species" species="semimarginata">
