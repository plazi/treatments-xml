<typeStatus id="548D88266D1DF14DFB37FAA4A015FAA3" box="[1201,1318,1359,1385]" pageId="30" pageNumber="31">Paratypes</typeStatus>
, 
<collectionCode id="ED27AE416D1DF14DFE8EFA9CA46EFA5B" LSID="urn:lsid:biocol.org:col:34871" box="[264,349,1399,1425]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="30" pageNumber="31">USNM</collectionCode>
<number id="3DFE24A36D1DF14DFEE3FA9CA480FA5B" box="[357,435,1399,1425]" pageId="30" pageNumber="31" value="32908.0">32908</number>
