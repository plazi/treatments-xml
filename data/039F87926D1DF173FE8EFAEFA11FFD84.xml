<date id="FF8810446D1DF14DFB99FAA4A1ABFAA3" box="[1055,1176,1359,1385]" pageId="30" pageNumber="31" value="1916-06-01">
<number id="3DFE24A36D1DF14DFB99FAA4A11AFAA3" box="[1055,1065,1359,1385]" pageId="30" pageNumber="31" value="1.0">1</number>
