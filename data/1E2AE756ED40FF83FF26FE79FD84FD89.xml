<quantity id="517BFBA5ED40FF83FE51FD9FFDE0FDF1" box="[469,528,519,541]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="1" pageNumber="1158" unit="m" value="900.0">900 m</quantity>
