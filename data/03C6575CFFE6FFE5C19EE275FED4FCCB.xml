<taxonomicName id="4C6F9DC9FFE7FFF8C148E2DAFC11F861" authority="(A. J. Wagner, 1906)" authorityName="A.J. Wagner" authorityYear="1906" baseAuthorityName="A. J. Wagner" baseAuthorityYear="1906" box="[282,907,1927,1954]" class="Gastropoda" family="Diplommatinidae" genus="Cochlostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="13" pageNumber="14" phylum="Mollusca" rank="species" species="erika" subGenus="Lovcenia">
