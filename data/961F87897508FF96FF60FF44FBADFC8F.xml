<taxonomicName id="D9B64D1C7508FF96FF60FEDFFE74FEA2" authority="Walker, 1859" authorityName="Walker" authorityYear="1859" box="[151,461,257,282]" class="Insecta" family="Crambidae" genus="Botys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="394" phylum="Arthropoda" rank="species" species="caldusalis">
