<taxonomicName id="C8D25CD32A06FFC9AF6FF8F5F4775C3B" authority="Tong &amp; Li, 2010" authorityName="Tong &amp; Li" authorityYear="2010" box="[363,526,1869,1892]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="46" phylum="Arthropoda" rank="species" species="jinniu" status="sp. nov.">
<emphasis id="3DA6FB422A06FFC9AF6FF8F5F4775C3B" box="[363,526,1869,1892]" italics="true" pageId="11" pageNumber="46">Pholcus jinniu</emphasis>
</taxonomicName>
<emphasis id="3DA6FB422A06FFC9AC13F8F5F4085C3A" bold="true" box="[535,625,1869,1893]" pageId="11" pageNumber="46">
