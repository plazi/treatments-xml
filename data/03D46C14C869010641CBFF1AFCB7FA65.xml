Subfamily 
<taxonomicName id="4C7DA681C86901064043FF1AFE04FF7F" authorityName="Brunner von Wattenwyl" authorityYear="1865" box="[287,476,222,248]" class="Insecta" family="Blaberidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="3" pageNumber="254" phylum="Arthropoda" rank="subFamily" subFamily="Perisphaerinae">Perisphaerinae</taxonomicName>
