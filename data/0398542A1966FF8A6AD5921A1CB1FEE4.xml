<taxonomicName id="4C319EBF1966FF8A6AFC92531E49FDF7" baseAuthorityName="Gould" baseAuthorityYear="1837" box="[128,360,605,625]" class="Aves" family="Ptilonorhynchidae" genus="Chlamydera" kingdom="Animalia" order="Passeriformes" pageId="89" pageNumber="89" phylum="Chordata" rank="species" species="maculata">Chlamydera maculata</taxonomicName>
