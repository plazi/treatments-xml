<materialsCitation id="3B733C81AB538E1FFF17FB83FA00FBDC" ID-GBIF-Occurrence="1795862888" box="[199,1075,1103,1128]" collectionCode="NHMUK" pageId="3" pageNumber="413" specimenCount="6" typeStatus="paratype">
