<quantity id="4CDFBDF0FFA8FFFFFEF6FBD006D0FBD5" box="[363,445,1117,1142]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" pageId="11" pageNumber="12" unit="mm" value="5.6">5.6mm</quantity>
. Female 
<quantity id="4CDFBDF0FFA8FFFFFDBCFBD0051EFBD5" box="[545,627,1117,1142]" metricMagnitude="-3" metricUnit="m" metricValue="5.4" pageId="11" pageNumber="12" unit="mm" value="5.4">5.4mm</quantity>
