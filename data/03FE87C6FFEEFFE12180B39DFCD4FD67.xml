<taxonomicName id="4C574D53FFEEFFE1228DB040FB51FD4B" authority="Distant, 1908" authorityName="Distant" authorityYear="1908" box="[922,1231,619,645]" class="Insecta" family="Cicadellidae" genus="Ujna" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="531" phylum="Arthropoda" rank="species" species="consors">
