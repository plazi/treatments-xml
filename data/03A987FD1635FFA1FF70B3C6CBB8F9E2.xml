<taxonomicName id="4C004D681636FFA1FD17B2C4CC17FA0E" box="[752,855,1485,1508]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="14" pageNumber="465" phylum="Basidiomycota" rank="genus">
