<collectingRegion id="B11EBAB95509FFF08FA5F105FF099D69" country="Costa Rica" name="Puntarenas" pageId="33" pageNumber="255">Puntarenas</collectingRegion>
and 
<collectingRegion id="B11EBAB95509FFF08C40F123FE409D76" box="[293,430,1850,1871]" country="Costa Rica" name="Guanacaste" pageId="33" pageNumber="255">Guanacaste</collectingRegion>
, 
<collectingCountry id="0BCD34CB5509FFF08CD9F123FDD29D69" box="[444,572,1850,1872]" name="Costa Rica" pageId="33" pageNumber="255">Costa Rica</collectingCountry>
, and 
<collectingRegion id="B11EBAB95509FFF08F1AF123FD1C9D69" box="[639,754,1850,1872]" country="Ecuador" name="Pichincha" pageId="33" pageNumber="255">Pichincha</collectingRegion>
, 
<collectingCountry id="0BCD34CB5509FFF08DF5F140FF1F9D56" box="[144,241,1881,1903]" name="Ecuador" pageId="33" pageNumber="255">Ecuador</collectingCountry>
