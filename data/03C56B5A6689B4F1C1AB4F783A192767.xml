<taxonomicName id="4C6CA1CF6689B4F1C5D04EA53D8324BF" box="[1260,1433,1493,1518]" class="Magnoliopsida" family="Plantaginaceae" kingdom="Plantae" order="Lamiales" pageId="220" pageNumber="221" phylum="Tracheophyta" rank="family">Plantaginaceae</taxonomicName>
