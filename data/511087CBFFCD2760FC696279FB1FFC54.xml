<paragraph id="D90636DDFFCD2767FC696279FAD8FB75" blockId="67.[940,1322,1115,1156]" box="[940,1317,1120,1156]" pageId="67" pageNumber="106">
<emphasis id="EBCDEACFFFCD2767FC696279FB7BFB72" bold="true" box="[940,1158,1120,1156]" pageId="67" pageNumber="106">
<taxonomicName id="1EB94D5EFFCD2767FC696279FB83FB72" box="[940,1150,1120,1156]" class="Insecta" family="Mydidae" genus="Plyomydas" kingdom="Animalia" order="Diptera" pageId="67" pageNumber="106" phylum="Arthropoda" rank="genus" status="gen. n.">Plyomydas</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="F0FE57B4FFCD2767FB64627AFAD8FB75" box="[1185,1317,1123,1155]" pageId="67" pageNumber="106" rank="genus">gen. n.</taxonomicNameLabel>
</paragraph>
