<collectionCode id="ED5CAE40FFFFFF90FCE9FC4BBA99BBBC" box="[849,911,917,943]" country="Bolivia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15753" name="Museo de Historia Natural Noel Kempff Mercado -- Universidad Aut�noma Gabriel Ren� Moreno" pageId="18" pageNumber="219">
<potCollectionCode id="5919688BFFFFFF90FCE9FC4BBA99BBBC" box="[849,911,917,943]" pageId="18" pageNumber="219">USZ</potCollectionCode>
</collectionCode>
!); 
<emphasis id="B939EA97FFFFFF90FC1EFC4BBDB8BBBD" box="[934,1198,916,942]" italics="true" pageId="18" pageNumber="219">
<collectionCode id="ED5CAE40FFFFFF90FC1EFC4BBAA0BBBD" box="[934,950,917,942]" country="South Africa" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14647" name="University of the Witwatersrand" pageId="18" pageNumber="219">J</collectionCode>
