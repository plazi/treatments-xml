<collectionCode id="ED5CAE40FFFFFF90FC02FC4BBAD8BBBD" box="[954,974,917,942]" country="Chile" httpUri="http://grbio.org/cool/az7k-p4g9" name="Departamento de Geologia, Universidad de Chile" pageId="18" pageNumber="219">R</collectionCode>
. 
<collectionCode id="ED5CAE40FFFFFF90FC69FC4BBAC8BBBD" box="[977,990,917,942]" country="Romania" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14415" name="&amp;quot;Alexandru Ioan Cuza&amp;quot; University" pageId="18" pageNumber="219">I</collectionCode>
