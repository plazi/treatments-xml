<potCollectionCode id="5919688BFFFFFF90FAE4FCAFBC8CBB98" box="[1372,1434,881,907]" pageId="18" pageNumber="219">USZ</potCollectionCode>
</collectionCode>
!); 
<emphasis id="B939EA97FFFFFF90FF30FC4AB857BBBD" box="[136,321,916,942]" italics="true" pageId="18" pageNumber="219">Mostacedo 1766</emphasis>
(
<potCollectionCode id="5919688BFFFFFF90FEF6FC4BB869BBBC" box="[334,383,917,943]" pageId="18" pageNumber="219">
<collectionCode id="ED5CAE40FFFFFF90FEF6FC4BB869BBBC" box="[334,383,917,943]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15859" name="Missouri Botanical Garden" pageId="18" pageNumber="219">MO</collectionCode>
