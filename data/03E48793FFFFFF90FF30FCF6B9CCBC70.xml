<potCollectionCode id="5919688BFFFFFF90FE31FC4BB8A7BBBC" box="[393,433,917,943]" pageId="18" pageNumber="219">NY</potCollectionCode>
</collectionCode>
, 
<collectionCode id="ED5CAE40FFFFFF90FE03FC4BB8EFBBBC" box="[443,505,917,943]" country="Bolivia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15753" name="Museo de Historia Natural Noel Kempff Mercado -- Universidad Aut�noma Gabriel Ren� Moreno" pageId="18" pageNumber="219">
<potCollectionCode id="5919688BFFFFFF90FE03FC4BB8EFBBBC" box="[443,505,917,943]" pageId="18" pageNumber="219">USZ</potCollectionCode>
</collectionCode>
!); 
