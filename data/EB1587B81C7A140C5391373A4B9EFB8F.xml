<taxonomicName id="A4BC4D2D1C74140F521F37334A0BF8AA" authority="Mendes &amp; Rafael, 2020" authorityName="Mendes &amp; Rafael" authorityYear="2020" box="[281,514,1858,1882]" class="Insecta" family="Phaneropteridae" genus="Acropsis" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="239" phylum="Arthropoda" rank="species" species="solimoesensis" status="sp. nov.">
