and 
<figureCitation id="FB872A2B1C7A14015255371E4966F878" box="[339,367,1902,1928]" captionStart="FIGURE 13" captionStartId="13.[151,250,1444,1469]" captionTargetBox="[151,1436,561,1419]" captionTargetId="figure@13.[151,1436,561,1419]" captionTargetPageId="13" captionText="FIGURE 13. Map of Acropsis species geographical records." figureDoi="http://doi.org/10.5281/zenodo.3833529" httpUri="https://zenodo.org/record/3833529/files/figure.png" pageId="7" pageNumber="237">13</figureCitation>
