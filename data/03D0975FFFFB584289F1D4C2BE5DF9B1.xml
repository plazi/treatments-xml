<taxonomicName id="4C795DCAFFFB58428DF4D617BF06F909" authorityName="J. Agardh" authorityYear="1873" baseAuthorityName="Forsskal" baseAuthorityYear="1775" box="[1170,1314,863,889]" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Bryopsidales" pageId="9" pageNumber="60" phylum="Chlorophyta" rank="species" species="racemosa">
<emphasis id="B90DFA5BFFFB58428DF4D617BF06F909" box="[1170,1314,863,889]" italics="true" pageId="9" pageNumber="60">C. racemosa</emphasis>
</taxonomicName>
, 
<emphasis id="B90DFA5BFFFB58428C57D617BAF2F9ED" italics="true" pageId="9" pageNumber="60">
<taxonomicName id="4C795DCAFFFB58428C57D617BAF6F9ED" class="Phaeophyceae" family="Dictyotaceae" genus="Dictyota" higherTaxonomySource="CoL" kingdom="Animalia" order="Dictyotales" pageId="9" pageNumber="60" phylum="Ochrophyta" rank="species" species="dichotoma">D. dichotoma</taxonomicName>
,
</emphasis>
and 
<taxonomicName id="4C795DCAFFFB5842886BD6CBBBA5F9ED" authorityName="Linnaeus" authorityYear="1753" box="[269,385,898,924]" class="Ulvophyceae" family="Ulvaceae" genus="Ulva" higherTaxonomySource="CoL" kingdom="Fungi" order="Ulvales" pageId="9" pageNumber="60" phylum="Chlorophyta" rank="species" species="lactuca">
<emphasis id="B90DFA5BFFFB5842886BD6CBBBA5F9ED" box="[269,385,898,924]" italics="true" pageId="9" pageNumber="60">U. lactuca</emphasis>
</taxonomicName>
