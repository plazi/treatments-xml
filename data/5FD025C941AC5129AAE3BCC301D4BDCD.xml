<bibRefCitation id="4B79114161782F963D9E57CEDEA07AC7" author="Meeuse, ADL" journalOrPublisher="Bothalia" pageId="207" pageNumber="208" pagination="641 - 792" refId="B225" refString="Meeuse, ADL, 1958 1957. The South African Convolvulaceae. Bothalia 6: 641 - 792" title="The South African Convolvulaceae." volume="6" year="1958">Meeuse 1957</bibRefCitation>
: 678 as 
<taxonomicName id="58301D4CC232E841B0AC7554E5C0C712" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus ulosepalus" order="Solanales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="species" species="ulosepalus">
<emphasis id="BD0ABFA1E54D9C0687548C76042AF7F1" italics="true" pageId="76" pageNumber="77">Convolvulus ulosepalus</emphasis>
</taxonomicName>
; 
<bibRefCitation id="8B8AA59DCF335C17B0AFDE8B7467F060" author="Meeuse, ADL" journalOrPublisher="Flora of Southern Africa" pageId="207" pageNumber="208" pagination="1 - 138" refId="B226" refString="Meeuse, ADL, Welman, WG, 2000. Convolvulaceae. Flora of Southern Africa volume 28 (part 1): 1 - 138" title="Convolvulaceae." volume="volume 28" year="2000">Meuse and Welman 2000</bibRefCitation>
