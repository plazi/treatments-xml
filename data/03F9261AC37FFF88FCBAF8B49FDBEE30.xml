<taxonomicName id="4C50EC8FC37CFF88FF1AFA4B9C55EAFF" authority="Balynsky 1950" authorityName="Balynsky" authorityYear="1950" box="[227,637,1468,1492]" class="Insecta" family="Chloroperlidae" genus="Pontoperla" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="176" phylum="Arthropoda" rank="species" species="katherinae">
