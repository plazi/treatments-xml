<taxonomicName id="2EF05C87947FCE2F7AECA53AFC5F92D9" authority="Cuvier, 1829" authorityName="Cuvier" authorityYear="1829" box="[675,995,365,392]" class="Actinopterygii" family="Trachichthyidae" genus="Hoplostethus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Beryciformes" pageId="25" pageNumber="26" phylum="Chordata" rank="genus">
