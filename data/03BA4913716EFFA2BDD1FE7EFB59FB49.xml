<taxonomicName id="4C138386716EFFA2BDD1FDC1FD14FDC7" authority=", Valerio, Whitfield &amp; Kole, 2005" authorityName=", Valerio, Whitfield &amp; Kole" authorityYear="2005" box="[151,738,537,560]" class="Insecta" family="Braconidae" genus="Parapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="rooibos">
