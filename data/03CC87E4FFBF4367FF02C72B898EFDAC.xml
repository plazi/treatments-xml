<taxonomicName id="4C654D71FFBF4360FCD9C6278BC8FCCB" box="[844,1009,834,859]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="pipturicolus">
