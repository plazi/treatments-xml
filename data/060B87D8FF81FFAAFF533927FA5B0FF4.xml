</keyStep>
<keyStep id="35562E6BFF81FFADFF533818FA580BF1" pageId="3" pageNumber="4">
<paragraph id="8E1D36CEFF81FFADFF533818FA5B0B41" blockId="3.[151,1436,1101,2007]" pageId="3" pageNumber="4">
<keyLead id="355395FBFF81FFADFF533818FA5B0B41" pageId="3" pageNumber="4">
<number id="386A24E9FF81FFADFF533818FF6B0B69" box="[151,169,1341,1367]" pageId="3" pageNumber="4" value="2.0">2</number>
