<taxonomicName id="49A24D4DFF81FFADFAD839E0FA520AE0" box="[1308,1424,1221,1247]" class="Insecta" family="Anthomyiidae" genus="Botanophila" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="nupera">
