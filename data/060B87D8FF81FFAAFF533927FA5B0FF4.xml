Figs. 
<number id="386A24E9FF81FFADFBBC3950FB670AB1" box="[1144,1189,1141,1167]" pageId="3" pageNumber="4" value="7.5" valueMax="9.0" valueMin="6.0">6–9</number>
</figureCitation>
) .............................. 
<emphasis id="BCD6EADCFF81FFADFF0839B8FC070A88" box="[204,965,1181,1206]" italics="true" pageId="3" pageNumber="4">
.......................................................................................... 
<taxonomicName id="49A24D4DFF81FFADFCA739B8FC070A88" box="[867,965,1181,1206]" class="Insecta" family="Anthomyiidae" genus="Botanophila" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="latifrons">latifrons</taxonomicName>
</emphasis>
, 
<emphasis id="BCD6EADCFF81FFADFC1039B8FB7E0A88" box="[980,1212,1181,1206]" italics="true" pageId="3" pageNumber="4">
<taxonomicName id="49A24D4DFF81FFADFC1039B8FB830A88" box="[980,1089,1181,1206]" class="Insecta" family="Anthomyiidae" genus="Anthomyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="varicolor">varicolor</taxonomicName>
