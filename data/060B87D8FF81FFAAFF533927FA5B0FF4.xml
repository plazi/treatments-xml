<emphasis id="BCD6EADCFF81FFADFCF23B20FCBF0820" box="[822,893,1541,1566]" italics="true" pageId="3" pageNumber="4">gnava</emphasis>
</taxonomicName>
, 
<taxonomicName id="49A24D4DFF81FFADFC483B20FBC10820" box="[908,1027,1541,1566]" class="Insecta" family="Anthomyiidae" genus="Botanophila" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="cordifrons">
