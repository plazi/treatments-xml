<subSubSection id="C34A8789FFF0FFFC4AF5847C42644C20" box="[199,1072,1626,1651]" pageId="8" pageNumber="191" type="etymology">
<paragraph id="8BEFD402FFF0FFFC4AF5847C42644C20" blockId="8.[151,1436,1410,1651]" box="[199,1072,1626,1651]" pageId="8" pageNumber="191">
<emphasis id="B9240810FFF0FFFC4AF5847C471A4C20" bold="true" box="[199,334,1626,1651]" pageId="8" pageNumber="191">Etymology.</emphasis>
