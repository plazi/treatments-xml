</taxonomicName>
<taxonomicNameLabel id="A25BD9606620FFDF4903F9C4D1139002" box="[827,887,1565,1589]" pageId="38" pageNumber="850" rank="species">sp. n.</taxonomicNameLabel>
and 
<taxonomicName id="4C1CC38A6620FFDF498BF9C4D6449002" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[947,1056,1565,1589]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="850" phylum="Arthropoda" rank="species" species="vichitra" status="sp. n.">
<emphasis id="B968641B6620FFDF498BF9C4D6449002" box="[947,1056,1565,1589]" italics="true" pageId="38" pageNumber="850">B. vichitra</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25BD9606620FFDF4E11F9C4D6019002" box="[1065,1125,1565,1589]" pageId="38" pageNumber="850" rank="species">sp. n.</taxonomicNameLabel>
In 
<taxonomicName id="4C1CC38A6620FFDF4A98F9E6D3549060" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[160,304,1599,1623]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="850" phylum="Arthropoda" rank="species" species="flaviscapus" status="sp. n.">
<emphasis id="B968641B6620FFDF4A98F9E6D3549060" box="[160,304,1599,1623]" italics="true" pageId="38" pageNumber="850">B. flaviscapus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25BD9606620FFDF4B0FF9E6D3149060" box="[311,368,1599,1623]" pageId="38" pageNumber="850" rank="species">sp. n.</taxonomicNameLabel>
