<taxonomicName id="4C1CC38A6620FFDF4B42F9BBD078904D" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[378,540,1634,1658]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="850" phylum="Arthropoda" rank="species" species="nicobarensis" status="sp. n.">
<emphasis id="B968641B6620FFDF4B42F9BBD078904D" box="[378,540,1634,1658]" italics="true" pageId="38" pageNumber="850">B. nicobarensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25BD9606620FFDF481EF9BBD006904D" box="[550,610,1634,1658]" pageId="38" pageNumber="850" rank="species">sp. n.</taxonomicNameLabel>
pilosity dense, mesoscutellum semicircular and metascutellum visible; 
<taxonomicName id="4C1CC38A6620FFDF4B90F95CD05990AA" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[424,573,1668,1693]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="850" phylum="Arthropoda" rank="species" species="flaviscapus" status="sp. n.">
<emphasis id="B968641B6620FFDF4B90F95CD05990AA" box="[424,573,1668,1693]" italics="true" pageId="38" pageNumber="850">B. flaviscapus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25BD9606620FFDF4870F95CD0E390AA" box="[584,647,1669,1693]" pageId="38" pageNumber="850" rank="species">sp. n.</taxonomicNameLabel>
