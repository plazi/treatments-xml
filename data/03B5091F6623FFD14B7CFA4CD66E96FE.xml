<taxonomicName id="4C1CC38A6620FFDF4B12F9C4D3D99002" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[298,445,1564,1589]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="850" phylum="Arthropoda" rank="species" species="flaviscapus" status="sp. n.">
<emphasis id="B968641B6620FFDF4B12F9C4D3D99002" box="[298,445,1564,1589]" italics="true" pageId="38" pageNumber="850">B. flaviscapus</emphasis>
