<taxonomicName id="4C1CC38A6620FFDF48B7F9C4D1559002" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[655,817,1565,1589]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="850" phylum="Arthropoda" rank="species" species="nicobarensis" status="sp. n.">
