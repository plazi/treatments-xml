15. 
<taxonomicName id="4FEEA6AF0453ADD22B31ECEA0EB4D239" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:132448" class="Insecta" family="Formicidae" genus="Oecodoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Oecodoma pallida Smith" order="Hymenoptera" pageId="187" pageNumber="187" phylum="Arthropoda" rank="species" species="pallida">Oecodoma pallida</taxonomicName>
