<taxonomicName id="5276E0EDD9AE4FE0288F29017E63BA52" class="Insecta" family="Ichneumonidae" genus="Glypta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glypta" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Glypta</taxonomicName>
