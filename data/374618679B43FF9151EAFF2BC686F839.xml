<emphasis id="8D9B75639B43FF9152A5FCA9C7AEFCF1" box="[984,1010,789,813]" italics="true" pageId="6" pageNumber="34">bv</emphasis>
<number id="0927BB569B43FF915284FCA8C040FCF0" box="[1017,1052,788,812]" pageId="6" pageNumber="34" value="9.3">9.3</number>
