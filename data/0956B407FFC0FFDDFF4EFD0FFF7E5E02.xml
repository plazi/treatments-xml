<caption id="D5805599FFC1FFDAFF4EF841FBF858E5" httpUri="https://zenodo.org/record/3375770/files/figure.png" pageId="3" pageNumber="386" startId="3.[151,264,1979,2004]" targetBox="[171,1418,814,1954]" targetPageId="3">
<paragraph id="81400511FFC1FFDAFF4EF841FBF858E5" blockId="3.[151,1436,1979,2040]" pageId="3" pageNumber="386">
<emphasis id="B38BD903FFC1FFDAFF4EF841FDA458C9" bold="true" box="[151,546,1979,2004]" pageId="3" pageNumber="386">
FIGURES 5–10 
<taxonomicName id="46FF7E92FFC1FFDAFE93F841FE4F58C9" authority="Zhang &amp; Xing, 2019" authorityName="Zhang &amp; Xing" authorityYear="2019" box="[330,457,1979,2004]" class="Insecta" family="Cicadellidae" genus="Cyrta" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="386" phylum="Arthropoda" rank="species" species="webbi" status="sp. nov.">
<emphasis id="B38BD903FFC1FFDAFE93F841FE4F58C9" bold="true" box="[330,457,1979,2004]" italics="true" pageId="3" pageNumber="386">Cyrta webbi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A8B86478FFC1FFDAFE09F846FDA458C9" box="[464,546,1980,2004]" pageId="3" pageNumber="386" rank="species">sp. nov.</taxonomicNameLabel>
