<taxonomicName id="66D94D1DFFD2FFC1FC56F95DFBD4F8E7" authority="Fitzinger, 1833" authorityName="Fitzinger" authorityYear="1833" box="[907,1146,1788,1808]" class="Gastropoda" family="Vitrinidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="20" pageNumber="23" phylum="Mollusca" rank="family">Vitrinidae Fitzinger, 1833</taxonomicName>
