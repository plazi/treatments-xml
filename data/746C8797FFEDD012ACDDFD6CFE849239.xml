<taxonomicName id="3BC54D02FFEDD012AD7FFC89FD659501" authority="Pascoe, 1860: 124" authorityName="Pascoe" authorityPageNumber="124" authorityYear="1860" box="[309,750,798,823]" class="Insecta" family="Tenebrionidae" genus="Dipsaconia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="468" phylum="Arthropoda" rank="species" species="pyritosa">
