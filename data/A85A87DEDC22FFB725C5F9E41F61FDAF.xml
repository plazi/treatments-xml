<heading id="7B0481A4DC22FFB025C5F9E41EC3F99C" bold="true" box="[151,920,1554,1579]" fontSize="10" level="2" pageId="11" pageNumber="68" reason="2">
<emphasis id="1287EADADC22FFB025C5F9E41EC3F99C" bold="true" box="[151,920,1554,1579]" pageId="11" pageNumber="68">
<taxonomicName id="E7F34D4BDC22FFB025C5F9E41F46F99C" authority="Bremer, 1864" authorityName="Bremer" authorityYear="1864" box="[151,541,1554,1579]" class="Insecta" family="Papilionidae" genus="Parnassius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="68" phylum="Arthropoda" rank="species" species="bremeri">
