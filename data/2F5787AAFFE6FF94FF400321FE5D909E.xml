<taxonomicName id="60FE4D3FFFE7FF91FE8E027FFE51947A" box="[345,472,1808,1832]" class="Insecta" family="Noctuidae" genus="Cidariplura" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="82" phylum="Arthropoda" rank="genus">
