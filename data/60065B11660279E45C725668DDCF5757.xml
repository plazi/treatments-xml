<taxonomicName id="E278B1CA7BF63E038530A4CCEA5B59E0" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia" order="Fabales" pageId="4" pageNumber="97" phylum="Tracheophyta" rank="genus">Acacia</taxonomicName>
