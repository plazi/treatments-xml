<taxonomicName id="4C764CA6FFA3FFC340AFFBB6FCD7E069" box="[655,821,1030,1054]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="greggii">
