<subSubSection id="C31D6555EF02FFE8FF1B8E3A01CFFF69" lastPageId="31" lastPageNumber="535" pageId="28" pageNumber="532" type="description">
<paragraph id="8BB836DEEF02FFEBFF1B8E3A0410FCA9" blockId="28.[151,1437,402,824]" box="[199,1069,798,824]" pageId="28" pageNumber="532">
<emphasis id="B973EACCEF02FFEBFF1B8E3A0164FCA9" bold="true" box="[199,345,798,824]" pageId="28" pageNumber="532">Description.</emphasis>
Female 
<typeStatus id="54BC887CEF02FFEBFE658E3A0222FCA9" box="[441,543,798,824]" pageId="28" pageNumber="532">holotype</typeStatus>
. Body length 
<quantity id="4CFF9B3BEF02FFEBFD668E3A0336FCA9" box="[698,779,798,824]" metricMagnitude="-3" metricUnit="m" metricValue="7.2" pageId="28" pageNumber="532" unit="mm" value="7.2">7.2mm</quantity>
, fore wing length 
<quantity id="4CFF9B3BEF02FFEBFC0A8E3A0415FCA9" box="[982,1064,798,824]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="28" pageNumber="532" unit="mm" value="4.1">4.1mm</quantity>
.
</paragraph>
<caption id="DF786656EF02FFEBFF4B8ABF028AF822" ID-DOI="http://doi.org/10.5281/zenodo.4778014" ID-Zenodo-Dep="4778014" box="[151,695,1947,1971]" httpUri="https://zenodo.org/record/4778014/files/figure.png" pageId="28" pageNumber="532" startId="28.[151,250,1947,1971]" targetBox="[167,1420,848,1923]" targetPageId="28">
