<paragraph id="C813748A5A5FA9FC0A79F2CABD5F7CA5" pageId="23" pageNumber="144">
<taxonomicName id="3921E05477E140AE62C0A617ADADAB79" authority="Motschoulsky, 1861" authorityName="Motschoulsky" authorityYear="1861" class="Insecta" family="Chrysomelidae" genus="Gallerucida" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gallerucida singularis" order="Coleoptera" pageId="23" pageNumber="144" phylum="Arthropoda" rank="species" species="singularis">Gallerucida singularis</taxonomicName>
