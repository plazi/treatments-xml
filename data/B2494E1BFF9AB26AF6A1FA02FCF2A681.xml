<taxonomicName id="FDE0848EFF99B26BF557F9E7FE20A349" authorityName="Hartman" authorityYear="1958" box="[286,452,1579,1603]" class="Calcarea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosoleniida" pageId="34" pageNumber="237" phylum="Porifera" rank="order">Leucosoleniida</taxonomicName>
