<taxonomicName id="7C73D3BC6E961572E55A7EFBC9310449" authority="L." class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="null" pageNumber="173" phylum="Tracheophyta" rank="species" species="strobus">
