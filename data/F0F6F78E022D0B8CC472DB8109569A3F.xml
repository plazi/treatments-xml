<taxonomicName id="C23CEA6D78D86B221E47C59C552C581D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24785" class="Insecta" family="Formicidae" genus="Myrmelachista" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Myrmelachista Roger" order="Hymenoptera" pageNumber="10" phylum="Arthropoda" rank="genus">Myrmelachista</taxonomicName>
</paragraph>
