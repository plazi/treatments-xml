<collectingCountry id="35DB767FB056FF9CFE80E9B91778FAAB" box="[314,374,1276,1297]" name="China" pageId="5" pageNumber="316">China</collectingCountry>
: 
<collectingRegion id="8F08F80DB056FF9CFE30E9B917DEFAAB" box="[394,464,1276,1297]" country="China" name="Beijing" pageId="5" pageNumber="316">Beijing</collectingRegion>
City, Miyun; 
<collectingRegion id="8F08F80DB056FF9CFDCEE9B9150AFAAB" box="[628,772,1276,1297]" country="China" name="Jilin" pageId="5" pageNumber="316">Jilin Province</collectingRegion>
, Hunchun; 
<collectingRegion id="8F08F80DB056FF9CFC3FE9B916E5FA91" country="China" name="Yunnan" pageId="5" pageNumber="316">Yunnan Province</collectingRegion>
; 
<collectingRegion id="8F08F80DB056FF9CFF4CE85317ACFA91" box="[246,418,1302,1323]" country="China" name="Shaanxi" pageId="5" pageNumber="316">Shaanxi Province</collectingRegion>
. 
<collectingCountry id="35DB767FB056FF9CFE17E85317E8FA91" box="[429,486,1302,1323]" name="Japan" pageId="5" pageNumber="316">Japan</collectingCountry>
. 
<collectingCountry id="35DB767FB056FF9CFE4AE853142DFA91" box="[496,547,1302,1323]" name="India" pageId="5" pageNumber="316">India</collectingCountry>
, 
<collectingRegion id="8F08F80DB056FF9CFD95E85314DCFA91" box="[559,722,1302,1323]" country="India" name="Sikkim" pageId="5" pageNumber="316">Sikkim Province</collectingRegion>
