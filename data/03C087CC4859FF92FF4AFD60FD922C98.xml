<collectorName id="269C530C4859FF92FD83FC97FC6F2B14" box="[606,916,825,850]" pageId="5" pageNumber="406">
Villa 
<collectingRegion id="49ADF8384859FF92FD46FC97FC6F2B14" box="[667,916,825,850]" inferredBySuffix="true" pageId="5" pageNumber="406">
Bella de 
<collectingRegion id="49ADF8384859FF92FD22FC94FC6F2B14" box="[767,916,826,850]" country="Brazil" name="Mato Grosso" pageId="5" pageNumber="406">Mato Grosso</collectingRegion>
