<quantity id="4CF79B33FFD6FFFFC158FABEFCC1FA97" box="[801,855,1383,1403]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="57" pageNumber="212" unit="m" value="800.0">800m</quantity>
