<quantity id="4CF79B33FFD6FFFFC39CFA44FD8DFA5D" box="[485,539,1437,1457]" metricMagnitude="2" metricUnit="m" metricValue="7.35" pageId="57" pageNumber="212" unit="m" value="735.0">735m</quantity>
