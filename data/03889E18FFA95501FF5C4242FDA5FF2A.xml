<collectingRegion id="49E5E1ECFFA95500FE714702FD9CF9AE" box="[442,538,1635,1660]" country="Spain" name="Asturias" pageId="22" pageNumber="23">Asturias</collectingRegion>
, 
<collectingRegion id="49E5E1ECFFA95500FDEE4702FD13F9AE" box="[549,661,1635,1660]" country="Spain" name="Cantabria" pageId="22" pageNumber="23">Cantabria</collectingRegion>
