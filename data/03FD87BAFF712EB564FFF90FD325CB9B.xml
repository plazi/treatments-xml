<collectingRegion id="4990F84EFF712EB66713F825D656CD82" box="[891,1049,1948,1973]" country="Ireland" name="Mayo" pageId="157" pageNumber="158">County Mayo</collectingRegion>
, 
<collectingCountry id="F343763CFF712EB6604CF825D638CD82" box="[1060,1143,1948,1973]" name="Ireland" pageId="157" pageNumber="158">Ireland</collectingCountry>
