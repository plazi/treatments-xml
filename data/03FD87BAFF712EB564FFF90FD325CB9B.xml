<location id="8E8B6077FF712EB660E9F824D6E4CD83" LSID="urn:lsid:plazi:treatment:03FD87BAFF712EB564FFF90FD325CB9B:8E8B6077FF712EB660E9F824D6E4CD83" box="[1153,1195,1949,1972]" country="Ireland" county="Adult" municipality="Uropod" name="Not" pageId="157" pageNumber="158" stateProvince="Mayo">Not</location>
