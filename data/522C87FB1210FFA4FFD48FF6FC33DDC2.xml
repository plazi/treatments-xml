<quantity id="1D7D9B081210FFA4FDBF8E65FD10D85C" box="[538,647,804,828]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" metricValueMax="1.2" metricValueMin="1.1" pageId="3" pageNumber="170" unit="mm" value="11.5" valueMax="12.0" valueMin="11.0">11-12 mm</quantity>
longis sepalis, 
<quantity id="1D7D9B081210FFA4FF5A8E7CFEADD832" box="[255,314,829,853]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="3" pageNumber="170" unit="mm" value="1.0">1 mm</quantity>
longis petalis, 
