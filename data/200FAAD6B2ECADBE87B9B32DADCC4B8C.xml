<taxonomicName id="31AB3F1AD78EE0DB0BB9857271333EEB" authority="Chevrolat, 1837" authorityName="Chevrolat" authorityYear="1837" class="Insecta" family="Coccinellidae" genus="Chnoodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chnoodes" order="Coleoptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="genus">Chnoodes</taxonomicName>
