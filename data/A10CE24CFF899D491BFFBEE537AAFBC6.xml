</emphasis>
. 
<taxonomicName id="EEA528D9FF899D491A01BD8C3617FD79" box="[361,848,688,712]" class="Arachnida" family="Parakalummidae" genus="Neoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="225" phylum="Arthropoda" rank="species" species="negrosensis" status="sp. nov." subGenus="Pseudoneoribates">
