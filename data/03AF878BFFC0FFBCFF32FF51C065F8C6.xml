<specimenCount id="9D00FD14FFC0FFBDFDABFE5AC67DFD85" box="[526,604,402,427]" pageId="2" pageNumber="189" type="male">
<number id="3DCE24BAFFC0FFBDFDABFE5AC63DFD85" box="[526,540,403,427]" pageId="2" pageNumber="189" value="1.0">1</number>
male
</specimenCount>
