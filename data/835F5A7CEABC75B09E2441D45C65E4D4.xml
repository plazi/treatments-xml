<taxonomicName id="4A1369DBCA0C3B8508BD59B65BA7E0C0" authority="Stephens, 1835" authorityName="Benson" authorityYear="1968" class="Insecta" family="Tenthredinidae" genus="Sciapteryx" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sciapteryx byzantina" order="Hymenoptera" pageId="15" pageNumber="100" phylum="Arthropoda" rank="species" species="byzantina">
