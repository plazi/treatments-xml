<taxonomicName id="61698E0A1B8DD5BD16E14AE4582B1C65" authority="Stephens, 1835" authorityName="Benson" authorityYear="1968" class="Insecta" family="Tenthredinidae" genus="Sciapteryx" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sciapteryx byzantina" order="Hymenoptera" pageId="14" pageNumber="99" phylum="Arthropoda" rank="species" species="byzantina">
<emphasis id="9E6DC00868C608387868ECC8A0A92261" italics="true" pageId="14" pageNumber="99">Sciapteryx byzantina</emphasis>
</taxonomicName>
. 
<emphasis id="6100B36BBA5C05D25B79A6A1EF31A04D" bold="true" pageId="14" pageNumber="99">48, 
