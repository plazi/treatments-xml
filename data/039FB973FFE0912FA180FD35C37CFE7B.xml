<subSubSection id="C32C5BEEFFE09122A180FC60C519FC17" pageId="23" pageNumber="424" type="etymology">
<paragraph id="8B890865FFE09122A180FC60C519FC17" blockId="23.[151,1436,937,1976]" pageId="23" pageNumber="424">
<emphasis id="B942D477FFE09122A180FC60C01CFC33" bold="true" box="[151,286,937,962]" pageId="23" pageNumber="424">Etymology.</emphasis>
