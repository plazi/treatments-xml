<taxonomicName id="BCD14EC5F41FE60244A10AA4E91D69A6" class="Insecta" family="Issidae" genus="Scantinius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scantinius" order="Hemiptera" pageId="0" pageNumber="69" phylum="Arthropoda" rank="genus">
