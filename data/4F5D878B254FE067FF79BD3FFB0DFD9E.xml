<taxonomicName id="00F44D1E254EE067FC6EBBFBFBFEFE51" box="[896,1043,441,467]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="palmatum">A. palmatum</taxonomicName>
