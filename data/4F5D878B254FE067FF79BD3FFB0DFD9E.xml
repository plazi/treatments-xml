<taxonomicName id="00F44D1E254EE067FBF3BBFBFB65FE51" box="[1053,1160,441,467]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="pictum">A. pictum</taxonomicName>
