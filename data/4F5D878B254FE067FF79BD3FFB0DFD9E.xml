<taxonomicName id="00F44D1E254EE067FB79BBFBFA71FE51" box="[1175,1436,441,467]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="pseudosieboldianum">
