<taxonomicName id="5DA54D09FFE1FFB5FC478461FBD2CB06" box="[1021,1141,1629,1650]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="38" pageNumber="46" phylum="Tracheophyta" rank="species" species="undetermined">
