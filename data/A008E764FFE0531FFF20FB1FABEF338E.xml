<quantity id="EF59FB97FFE0531CFF5AF8D5A84634D9" box="[237,341,1896,1920]" metricMagnitude="-4" metricUnit="m" metricValue="1.6" pageId="1" pageNumber="41" unit="mm" value="0.16">0.16 mm</quantity>
