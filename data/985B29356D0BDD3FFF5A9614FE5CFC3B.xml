<taxonomicName id="D7F2E3A06D08DD3EFEFE94A2FD20FA81" authority="Casey" authorityName="Casey" box="[356,666,1259,1281]" class="Insecta" family="Laemophloeidae" genus="Lathropus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="13" phylum="Arthropoda" rank="species" species="robustulus">
