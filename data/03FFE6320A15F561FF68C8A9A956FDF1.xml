<taxonomicName id="4C562CA70A14F561FE53CCF1A8B8FF69" authority="Linnaeus" authorityName="Linnaeus" box="[428,652,223,248]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="18" phylum="Mollusca" rank="species" species="lusitanica">
