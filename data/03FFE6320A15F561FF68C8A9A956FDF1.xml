<taxonomicName id="4C562CA70A14F561FD2FCC92A985FF44" authority="Linnaeus" authorityName="Linnaeus" box="[720,945,188,213]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="18" phylum="Mollusca" rank="species" species="lusitanica">
