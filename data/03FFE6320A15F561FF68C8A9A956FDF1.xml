</taxonomicName>
, 
<taxonomicName id="4C562CA70A14F561FC41CC93AE6EFF44" authority="Muller" authorityName="Muller" box="[958,1114,188,213]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="18" phylum="Mollusca" rank="species" species="varica">
