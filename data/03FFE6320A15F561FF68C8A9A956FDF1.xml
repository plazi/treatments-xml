<taxonomicName id="4C562CA70A14F561FB6ACC92AFA8FF44" authority="Lamarck. The" authorityName="Lamarck. The" box="[1173,1436,188,213]" class="Gastropoda" family="Hydrobiidae" genus="Paludina" kingdom="Animalia" order="Littorinimorpha" pageId="17" pageNumber="18" phylum="Mollusca" rank="species" species="olivacea">
