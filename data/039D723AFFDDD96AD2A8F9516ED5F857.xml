<specimenCount id="9D3208A5FFDDD96AD05CF8B66EC2F8C7" box="[611,664,1890,1916]" pageId="2" pageNumber="62" type="generic">
<quantity id="4CCC6EC9FFDDD96AD05CF8B66EC9F8C7" box="[611,659,1890,1916]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="2" pageNumber="62" unit="m" value="3.0">3 m</quantity>
