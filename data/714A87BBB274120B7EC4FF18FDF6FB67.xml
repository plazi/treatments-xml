<paragraph id="F95C36ADB274120B7E94FDA6FF19FC8C" blockId="27.[151,1437,258,1200]" pageId="27" pageNumber="528">
<emphasis id="CB97EABFB274120B7E94FDA6FE9EFD88" box="[199,326,553,576]" italics="true" pageId="27" pageNumber="528">
<taxonomicName id="3EE34D2EB274120B7E94FDA6FE9AFD88" box="[199,322,553,576]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="27" pageNumber="528" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
