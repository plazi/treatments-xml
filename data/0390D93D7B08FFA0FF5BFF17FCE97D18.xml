<taxonomicName id="4C3913A87B08FFA1FC50F9A9FBCF7A3E" authorityName="Balint &amp; Moser" authorityYear="2007" box="[924,1143,1574,1595]" class="Insecta" family="Lycaenidae" genus="Denivia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="127" phylum="Arthropoda" rank="species" species="espiritosanto">
