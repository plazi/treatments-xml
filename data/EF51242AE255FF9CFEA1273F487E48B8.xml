<taxonomicName id="A0F8EEBFE255FF9CFDBA21B448804AB0" box="[531,669,1239,1264]" class="Gastropoda" family="Pyramidellidae" genus="Turbonilla" kingdom="Animalia" pageId="7" pageNumber="8" phylum="Mollusca" rank="species" species="aquilonaria">
