<quantity id="2C689B72F059FFF1BEC2F8DFFBE2C057" box="[953,1056,1812,1837]" metricMagnitude="-3" metricUnit="m" metricValue="3.45" pageId="5" pageNumber="312" unit="mm" value="3.45">3.45 mm</quantity>
