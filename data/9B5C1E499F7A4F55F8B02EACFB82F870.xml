: 
<taxonomicName id="D4F5D4DC9F7A4F55F9652F78FDF6F8B8" authorityName="Silveira &amp; Mendonca" authorityYear="2018" box="[322,583,1868,1894]" class="Entognatha" family="Neanuridae" genus="Pseudachorutes" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="213" phylum="Arthropoda" rank="species" species="solaris">
