<taxonomicName id="DCA94D51FF9CFFA5FD911A0E5E2FFDBA" authority="Kirkaldy" authorityName="Kirkaldy" box="[597,848,514,540]" class="Insecta" family="Membracidae" genus="Sundarion" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="genus">
