<keyLead id="30DA3139FFC2FFB81398FE16FA71F9BA" pageId="2" pageNumber="3">
<number id="3DE3802BFFC2FFB81398FE16FF46F999" box="[151,162,1546,1567]" pageId="2" pageNumber="3" value="8.0">8</number>
a (
<number id="3DE3802BFFC2FFB813B4FE16FF2CF999" box="[187,200,1546,1567]" pageId="2" pageNumber="3" value="7.0">7</number>
