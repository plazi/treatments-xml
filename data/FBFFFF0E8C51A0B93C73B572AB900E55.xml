<taxonomicName id="F71BFA05EB4E516DA1604645AD1E2C95" authority="Lam." class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="476" phylum="Tracheophyta" rank="species" species="magellanica">
