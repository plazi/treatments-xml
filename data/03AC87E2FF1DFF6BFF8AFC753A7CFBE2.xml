<paragraph id="8BBA36F4FF1DFF6BFFA2FBD03B76FB1E" blockId="166.[93,620,1055,1210]" pageId="166" pageNumber="166">
<treatmentCitationGroup id="AB1511DAFF1DFF6BFFA2FBD03B76FB1E" pageId="166" pageNumber="166">
Now 
<taxonomicName id="4C054D77FF1DFF6BFF5BFBD13B56FBCE" authority="Gould, 1838" authorityName="Gould" authorityYear="1838" box="[164,561,1110,1130]" class="Aves" family="Meliphagidae" genus="Anthochaera" kingdom="Animalia" order="Passeriformes" pageId="166" pageNumber="166" phylum="Chordata" rank="species" species="lunulata">
<emphasis id="B971EAE6FF1DFF6BFF5BFBD138ECFBCE" box="[164,395,1110,1130]" italics="true" pageId="166" pageNumber="166">Anthochaera lunulata</emphasis>
Gould, 1838
</taxonomicName>
