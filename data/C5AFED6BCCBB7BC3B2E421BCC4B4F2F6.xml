<taxonomicName id="DCF443162D56AC925C6B9FC3A6020024" LSID="http://zoobank.org/01E0E081-9F1E-4E43-8B31-B617B20780A3" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus chilamatensis" order="Hymenoptera" pageId="216" pageNumber="217" phylum="Arthropoda" rank="species" species="chilamatensis">Heterospilus chilamatensis Marsh</taxonomicName>
