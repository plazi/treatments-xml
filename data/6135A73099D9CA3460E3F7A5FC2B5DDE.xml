</taxonomicName>
(
<taxonomicName id="C5888B1E00F02CFF586ED8BF69D6A32C" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
