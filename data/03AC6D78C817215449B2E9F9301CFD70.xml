<number id="3DCDCE49C81621544D80E8B2307BFE49" box="[1189,1301,476,500]" pageId="23" pageNumber="474" value="0.355" valueMax="0.38" valueMin="0.33">0.33–0.38</number>
(
<number id="3DCDCE49C81621544C00E8B23037FE48" box="[1317,1369,476,501]" pageId="23" pageNumber="474" value="0.35">0.35</number>
) mm; EW 
<number id="3DCDCE49C816215449E9EB6E3453FDA5" box="[204,317,512,536]" pageId="23" pageNumber="474" value="1.15" valueMax="1.2" valueMin="1.1">1.10–1.20</number>
(
<number id="3DCDCE49C8162154486AE89134ECFDA5" box="[335,386,511,536]" pageId="23" pageNumber="474" value="1.15">1.15</number>
) mm; EL 
<number id="3DCDCE49C816215448DAEB6E3700FDA5" box="[511,622,512,536]" pageId="23" pageNumber="474" value="1.43" valueMax="1.48" valueMin="1.38">1.38–1.48</number>
(
<number id="3DCDCE49C81621544BA4EB6E37DAFDA5" box="[641,692,512,536]" pageId="23" pageNumber="474" value="1.42">1.42</number>
) mm. Female (n = 
<number id="3DCDCE49C81621544AB9EB6E36C3FDA5" box="[924,941,512,536]" pageId="23" pageNumber="474" value="9.0">9</number>
): TL 
<number id="3DCDCE49C81621544AD7E891310CFDA5" box="[1010,1122,511,536]" pageId="23" pageNumber="474" value="1.73" valueMax="1.88" valueMin="1.58">1.58–1.88</number>
(
