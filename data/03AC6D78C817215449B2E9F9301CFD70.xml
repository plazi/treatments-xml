) mm; PW 
<number id="3DCDCE49C81621544C09E89130F2FDA5" box="[1324,1436,511,536]" pageId="23" pageNumber="474" value="0.7250000000000001" valueMax="0.8" valueMin="0.65">0.65–0.80</number>
(
<number id="3DCDCE49C816215449BAEB4A35BDFD81" box="[159,211,548,572]" pageId="23" pageNumber="474" value="0.74">0.74</number>
) mm; PL 
<number id="3DCDCE49C81621544861EB4A34DAFD81" box="[324,436,548,572]" pageId="23" pageNumber="474" value="0.33" valueMax="0.38" valueMin="0.28">0.28–0.38</number>
(
<number id="3DCDCE49C816215448E7EB4A3498FD81" box="[450,502,548,572]" pageId="23" pageNumber="474" value="0.34">0.34</number>
