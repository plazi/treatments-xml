<subSubSection id="C31F8FE5C816215449E2EB293751FDDD" box="[199,575,583,608]" pageId="23" pageNumber="474" type="distribution">
<paragraph id="8BBADC6EC816215449E2EB293751FDDD" blockId="23.[151,1436,151,717]" box="[199,575,583,608]" pageId="23" pageNumber="474">
<emphasis id="B971007CC816215449E2EB29340EFDDD" bold="true" box="[199,352,583,608]" pageId="23" pageNumber="474">Distribution.</emphasis>
<collectingCountry id="F3129CFEC81621544842EB2934A3FDDD" box="[359,461,583,608]" name="Thailand" pageId="23" pageNumber="474">Thailand</collectingCountry>
, 
<collectingCountry id="F3129CFEC816215448FCEB293752FDDD" box="[473,572,583,608]" name="Vietnam" pageId="23" pageNumber="474">Vietnam</collectingCountry>
