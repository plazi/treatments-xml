<number id="3DCDCE49C81621544B57EB4A378CFD80" box="[626,738,548,573]" pageId="23" pageNumber="474" value="1.06" valueMax="1.15" valueMin="0.97">0.97–1.15</number>
(
<number id="3DCDCE49C81621544BD4EB4A364AFD81" box="[753,804,548,572]" pageId="23" pageNumber="474" value="1.1">1.10</number>
) mm; EL 
<number id="3DCDCE49C81621544AB2EB4A3169FD80" box="[919,1031,548,573]" pageId="23" pageNumber="474" value="1.3900000000000001" valueMax="1.53" valueMin="1.25">1.25–1.53</number>
(
<number id="3DCDCE49C81621544D33EB4A3127FD81" box="[1046,1097,548,572]" pageId="23" pageNumber="474" value="1.43">1.43</number>
