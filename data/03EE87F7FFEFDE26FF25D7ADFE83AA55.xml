(223, circle), 
<taxonomicName id="4C474D62FFEEDE24FB9DD611FAB1AA4B" authority="Jałoszyński, 2017" authorityName="Jałoszyński" authorityYear="2017" box="[1071,1308,1791,1813]" class="Insecta" family="Staphylinidae" genus="Cephennodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="myagdiensis" status="sp. n.">
