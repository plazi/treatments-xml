<taxonomicName id="0EE118870470FFF3FC38F93B729E1727" authorityName="Koehler" authorityYear="1895" box="[924,1164,1782,1808]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="32" pageNumber="33" phylum="Echinodermata" rank="species" species="aristata">
