<quantity id="4CCC9B12C4221F7748B8F9CEDA5EF90F" box="[748,849,1643,1666]" metricMagnitude="-2" metricUnit="m" metricValue="1.22" pageId="49" pageNumber="50" unit="mm" value="12.2">12.2 mm</quantity>
