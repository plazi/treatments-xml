<taxonomicName id="4C714D228170FFDCFBAAFD9BFB7AFD96" box="[1062,1225,554,580]" class="Liliopsida" family="Orchidaceae" genus="Dendrobium" kingdom="Plantae" order="Asparagales" pageId="95" pageNumber="96" phylum="Tracheophyta" rank="species" species="calothyrsos">
<emphasis id="B905EAB38170FFDCFBAAFD9BFB7AFD96" box="[1062,1225,554,580]" italics="true" pageId="95" pageNumber="96">D. calothyrsos</emphasis>
</taxonomicName>
