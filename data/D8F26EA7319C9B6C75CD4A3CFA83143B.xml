<geoCoordinate id="09A5CD64145491C502BD34FD8C149DE3" direction="east" orientation="longitude" precision="925" value="143.33333">143°20'E</geoCoordinate>
<normalizedToken id="40B25690AA4FC395D4F1D5C53C8C51BA" originalValue="±">+/-</normalizedToken>
