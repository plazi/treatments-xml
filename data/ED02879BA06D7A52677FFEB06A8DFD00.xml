<taxonomicName id="A2AB4D0EA06E7A526511FDA66BCDFDA7" authorityName="Chen &amp; Toda" authorityYear="2001" box="[770,940,548,572]" class="Insecta" family="Drosophilidae" genus="Amiota" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="282" phylum="Arthropoda" rank="species" species="aristata" subGenus="Amiota">
