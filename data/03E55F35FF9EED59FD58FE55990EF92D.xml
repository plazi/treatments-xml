<subSubSection id="C356BDA8FF9EED59FD6EFD2E9E5FFD63" pageId="34" type="materials_examined">
<paragraph id="8BF3EE23FF9EED59FD6EFD2E9E5FFD63" blockId="34.[696,1225,640,1717]" pageId="34">
