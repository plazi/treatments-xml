<collectionCode id="ED5D76E6FF9EED59FBDAFD6C99F2FD42" box="[1132,1219,706,730]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="34">AMNH</collectionCode>
