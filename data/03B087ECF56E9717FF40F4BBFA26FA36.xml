<keyLead id="30E895CFF56E9717FF40F610FA26FBAB" pageId="2" pageNumber="492">
<number id="3DD124DDF56E9717FF40F610FF18FB1C" box="[151,162,587,609]" pageId="2" pageNumber="492" value="3.0">3</number>
