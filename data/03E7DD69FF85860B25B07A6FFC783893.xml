cf. 
<taxonomicName id="4C4E17FCFF858608245F7A6FFC363D1C" authorityName="Gray" authorityYear="1868" box="[792,941,1546,1572]" pageId="85" pageNumber="900" rank="superFamily" superFamily="Physeteroidea">Physeteroidea</taxonomicName>
