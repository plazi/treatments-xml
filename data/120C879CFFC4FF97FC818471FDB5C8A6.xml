<taxonomicName id="5DA54D09FFC3FF97FDA58751FDD0C8F6" box="[543,631,1389,1410]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
