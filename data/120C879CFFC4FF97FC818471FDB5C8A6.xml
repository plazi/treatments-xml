</taxonomicName>
(
<taxonomicName id="5DA54D09FFC3FF97FF768751FEE9C8F6" box="[204,334,1389,1410]" class="Magnoliopsida" family="Hypericaceae" kingdom="Plantae" order="Malpighiales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="family">Hypericaceae</taxonomicName>
