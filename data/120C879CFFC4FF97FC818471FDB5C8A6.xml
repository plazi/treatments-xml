</taxonomicName>
species-group. 
<taxonomicName id="5DA54D09FFC3FF97FDF38704FF18C813" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="A8D1EA98FFC3FF97FDF38704FDD9C839" box="[585,638,1336,1357]" italics="true" pageId="4" pageNumber="12">Pinus</emphasis>
spp.
</taxonomicName>
(
<taxonomicName id="5DA54D09FFC3FF97FF72876EFEB9C813" box="[200,286,1362,1383]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
), 
<taxonomicName id="5DA54D09FFC3FF97FE96876EFE02C813" box="[300,421,1362,1383]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="A8D1EA98FFC3FF97FE96876EFEDDC813" box="[300,378,1362,1383]" italics="true" pageId="4" pageNumber="12">Quercus</emphasis>
spp.
</taxonomicName>
(
<taxonomicName id="5DA54D09FFC3FF97FE15876EFDAEC813" authorityName="Dumortier" authorityYear="1829" box="[431,521,1362,1383]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
), 
<taxonomicName id="5DA54D09FFC3FF97FDAD876EFF18C8F6" class="Magnoliopsida" family="Hypericaceae" genus="Hypericum" kingdom="Plantae" order="Malpighiales" pageId="4" pageNumber="12" phylum="Tracheophyta" rank="species" species="undetermined">
