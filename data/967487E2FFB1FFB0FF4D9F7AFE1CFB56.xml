<taxonomicName id="D9DD4D77FFB2FFB1FCFD9C2CF920FDE1" box="[807,1072,548,573]" class="Insecta" family="Ephydridae" genus="Neoephydra" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="neotropica">
