<taxonomicName id="4C2EC7A5FF8D631276730494FB1E41DE" authorityName="Brown" authorityYear="2006" box="[1128,1275,444,469]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="oligoseta">
<emphasis id="B95A6034FF8D631276730494FB1E41DE" box="[1128,1275,444,469]" italics="true" pageId="26" pageNumber="27">M. oligoseta</emphasis>
</taxonomicName>
