<subSubSection id="C334EFADFF8E6311731303A6FDAE46A3" box="[264,587,1678,1704]" pageId="25" pageNumber="26" type="nomenclature">
<paragraph id="8B91BC26FF8E6311731303A6FDAE46A3" blockId="25.[264,587,1678,1704]" box="[264,587,1678,1704]" pageId="25" pageNumber="26">
<heading id="D0D90B4AFF8E6311731303A6FDAE46A3" bold="true" box="[264,587,1678,1704]" fontSize="11" level="1" pageId="25" pageNumber="26" reason="1">
<emphasis id="B95A6034FF8E6311731303A6FDAE46A3" bold="true" box="[264,587,1678,1704]" pageId="25" pageNumber="26">
<taxonomicName id="4C2EC7A5FF8E6311731303A6FE1C46A3" authorityName="Borgmeier" authorityYear="1971" box="[264,505,1678,1704]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="sinistra">
<emphasis id="B95A6034FF8E6311731303A6FE1C46A3" bold="true" box="[264,505,1678,1704]" italics="true" pageId="25" pageNumber="26">Melaloncha sinistra</emphasis>
</taxonomicName>
