<taxonomicName id="4C2EC7A5FF8D6312702D04CCFD5041F6" authorityName="Borgmeier" authorityYear="1971" box="[566,693,484,509]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="sinistra">
