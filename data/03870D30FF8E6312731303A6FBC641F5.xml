<taxonomicName id="4C2EC7A5FF8D6312716B04CCFBFE41F6" authorityName="Brown" authorityYear="2006" box="[880,1051,484,510]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="zurquiensis">
