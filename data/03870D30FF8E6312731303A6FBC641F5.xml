<taxonomicName id="4C2EC7A5FF8D631271CA0494FBBC41DE" authorityName="Brown" authorityYear="2006" box="[977,1113,444,469]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="laselvae">
<emphasis id="B95A6034FF8D631271CA0494FBBC41DE" box="[977,1113,444,469]" italics="true" pageId="26" pageNumber="27">M. laselvae</emphasis>
</taxonomicName>
