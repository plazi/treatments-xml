<taxonomicName id="4C2EC7A5FF8D631277100494FE6341F6" authorityName="Brown" authorityYear="2006" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="rodeoensis">
<emphasis id="B95A6034FF8D631277100494FE6341F6" italics="true" pageId="26" pageNumber="27">M. rodeoensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C2EC7A5FF8D6312738F04CCFDC241F6" authorityName="Brown" authorityYear="2006" box="[404,551,484,509]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="ruinensis">
