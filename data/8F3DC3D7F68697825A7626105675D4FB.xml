<taxonomicName id="8A5D738B299AFFF76071D904E79AC59D" class="Insecta" family="Aphididae" genus="Aphidura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphidura bozhkoae" order="Hemiptera" pageId="4" pageNumber="117" phylum="Arthropoda" rank="species" species="bozhkoae">Aphidura bozhkoae</taxonomicName>
