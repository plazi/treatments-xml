<taxonomicName id="AF77AF6737D32CC080C183BE3AC8E309" class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ranunculus" order="Ranunculales" pageId="0" pageNumber="107" phylum="Tracheophyta" rank="genus">Ranunculus</taxonomicName>
