<quantity id="B4559B632B223C373DF1E8B8FB4BF92A" box="[1169,1271,1691,1715]" metricMagnitude="-3" metricUnit="m" metricValue="2.38" pageId="6" pageNumber="277" unit="mm" value="2.38">2.38 mm</quantity>
