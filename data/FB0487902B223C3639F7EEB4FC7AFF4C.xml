<quantity id="B4559B632B223C37380AE89DFE6DF94E" box="[362,465,1726,1751]" metricMagnitude="-4" metricUnit="m" metricValue="6.8" pageId="6" pageNumber="277" unit="mm" value="0.68">0.68 mm</quantity>
