<quantity id="B4559B632B223C373B7EE89DFD39F94E" box="[542,645,1726,1751]" metricMagnitude="-4" metricUnit="m" metricValue="5.1" pageId="6" pageNumber="277" unit="mm" value="0.51">0.51 mm</quantity>
