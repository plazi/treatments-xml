(
<figureCitation id="919BFA9DFFAC3E66ED5D4B53FC03393A" box="[850,917,1613,1634]" captionStart="Fig" captionStartId="2.[175,206,1357,1376]" captionTargetBox="[156,1165,206,1339]" captionTargetId="figure-147@2.[154,1169,194,1341]" captionTargetPageId="2" captionText="Fig. 1. Brounhister vividulus, habitus. A) Dorsal view, B) Lateral view, C) Oblique lateral view." figureDoi="http://doi.org/10.5281/zenodo.5377183" httpUri="https://zenodo.org/record/5377183/files/figure.png" pageId="10" pageNumber="546">Figs. 1</figureCitation>
, 
<figureCitation id="919BFA9DFFAC3E66EDAC4B53FC5F393A" box="[931,969,1613,1635]" captionStart-0="Fig" captionStart-1="Fig" captionStart-2="Fig" captionStart-3="Fig" captionStart-4="Fig" captionStartId-0="5.[197,228,1647,1666]" captionStartId-1="7.[197,228,972,991]" captionStartId-2="8.[175,206,1165,1184]" captionStartId-3="9.[197,228,1469,1488]" captionStartId-4="10.[175,206,1002,1021]" captionTargetBox-0="[204,1162,194,1631]" captionTargetBox-1="[175,1190,194,956]" captionTargetBox-2="[155,630,196,1148]" captionTargetBox-3="[177,1188,196,1451]" captionTargetBox-4="[156,1167,197,984]" captionTargetId-0="figure-0@5.[204,1162,194,1631]" captionTargetId-1="figure-370@7.[175,1190,194,956]" captionTargetId-2="figure-560@8.[153,635,194,1150]" captionTargetId-3="figure-80@9.[175,1190,194,1453]" captionTargetId-4="figure-342@10.[154,1169,194,986]" captionTargetPageId-0="5" captionTargetPageId-1="7" captionTargetPageId-2="8" captionTargetPageId-3="9" captionTargetPageId-4="10" captionText-0="Fig. 3. Brounhister vividulus. A) Dorsal habitus, B) Ventral habitus, C) Head and prothorax, ventral view, D) Pygidium, dorsal view, E) Pro- and mesosterna, F) Prosternum, G) Mesosternum, H) Mesoventrite, lateral view." captionText-1="Fig. 4. Brounhister vividulus, antennomere 9. A) Antennal club, B) Apex, C) Subapex." captionText-2="Fig. 5. Brounhister vividulus. A) Antenna, B) Antennal club showing internal vesicles, C) Protibia, anterior view, D) Protibia, posterior view." captionText-3="Fig. 6. Brounhister vividulus, male. A) Tergite and sternite VIII, dorsal view, B) Tergite and sternite VIII, lateral view, C) Sternite VIII of a different specimen, dorsal view, D) Tergites IX and X, dorsal view, E) Tergites IX and X, lateral view, F) Aedeagus, dorsal view, G) Aedeagus, lateral view." captionText-4="Fig. 7. Brounhister vividulus, ovipositor. A) Dorsal view, B) Ventral view, C) Lateral view, D) Gonocoxite, lateral view, E) Gonostylus, lateral view." figureDoi-0="http://doi.org/10.5281/zenodo.5377187" figureDoi-1="http://doi.org/10.5281/zenodo.5377189" figureDoi-2="http://doi.org/10.5281/zenodo.5377191" figureDoi-3="http://doi.org/10.5281/zenodo.5377193" figureDoi-4="http://doi.org/10.5281/zenodo.5377195" httpUri-0="https://zenodo.org/record/5377187/files/figure.png" httpUri-1="https://zenodo.org/record/5377189/files/figure.png" httpUri-2="https://zenodo.org/record/5377191/files/figure.png" httpUri-3="https://zenodo.org/record/5377193/files/figure.png" httpUri-4="https://zenodo.org/record/5377195/files/figure.png" pageId="10" pageNumber="546">
