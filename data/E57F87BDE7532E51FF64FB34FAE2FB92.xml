<quantity id="AA2E9B4EE7522E56FB1CFD44FA44FD58" box="[1263,1433,692,716]" metricMagnitude="-3" metricUnit="m" metricValue="1.24" metricValueMax="1.28" metricValueMin="1.2" pageId="11" pageNumber="42" unit="mm" value="1.24" valueMax="1.28" valueMin="1.2">1.20–1.28 mm</quantity>
