<collectionCode id="7EB6AE24FFA59601FE7BF9FEB4F636B8" box="[398,475,1648,1672]" country="0" httpUri="http://grbio.org/cool/42td-7yhx" name="Universidad Nacional de San Agustin, Museo de Historia Natural (Peru)" pageId="6" pageNumber="779">MUSA</collectionCode>
