<geoCoordinate id="7D935026FFA59601FD40F9E0B5DD3699" degrees="069" direction="west" minutes="36" orientation="longitude" pageId="6" pageNumber="779" precision="15" seconds="36" value="-69.61">069°36′ 36″ W</geoCoordinate>
