<subSubSection id="C363652FFFE2BE28FF277480F2BBFCE4" box="[212,1426,751,775]" pageId="6" pageNumber="593" type="distribution">
<paragraph id="8BC636A4FFE2BE28FF277480F2BBFCE4" blockId="6.[212,1426,751,775]" box="[212,1426,751,775]" pageId="6" pageNumber="593">
<materialsCitation id="3B113CF9FFE2BE28FF277480F2BBFCE4" box="[212,1426,751,775]" collectionCode="DISTRIBUTION" country="Philippines, Indonesia" elevation="430" location="Philippines, Indonesia" pageId="6" pageNumber="593" specimenCount="1">
<collectionCode id="ED68AE61FFE2BE28FF277480F6AFFCE4" box="[212,390,751,775]" pageId="6" pageNumber="593">DISTRIBUTION</collectionCode>
