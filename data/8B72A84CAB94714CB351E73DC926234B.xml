<geoCoordinate id="4B10B4B4C5DAA40B89668A26D9574FC6" direction="north" orientation="latitude" precision="15" value="54.01139">54°00'41&quot;N</geoCoordinate>
, 
<geoCoordinate id="7670132014F339D2CBC8B34985716D2F" direction="east" orientation="longitude" precision="15" value="179.78111">179°46'52&quot;E</geoCoordinate>
