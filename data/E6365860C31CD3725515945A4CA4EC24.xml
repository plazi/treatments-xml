<taxonomicName id="D36170399E8EEF2109821CE8D0825362" class="Insecta" family="Braconidae" genus="Braunsia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Braunsia bipunctata" order="Hymenoptera" pageId="1" pageNumber="96" phylum="Arthropoda" rank="species" species="bipunctata">Braunsia bipunctata</taxonomicName>
