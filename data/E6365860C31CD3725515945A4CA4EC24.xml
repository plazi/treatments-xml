<bibRefCitation id="B7349E346E5D41177E3E7D22E450D555" author="Shenefelt, RD" journalOrPublisher="Hymenopterorum Catalogus (nova editio) Pars" pageId="7" pageNumber="102" pagination="307 - 428" title="Braconidae 3 Agathidinae." volume="6" year="1970">Shenefelt 1970</bibRefCitation>
: 371; 
<bibRefCitation id="FAB2EC58B6D0514919248222F5B9DB9E" author="Bhat, S" journalOrPublisher="Oriental Insects Monograph" pageId="7" pageNumber="102" pagination="1 - 353" title="The subfamily Agathidinae (Hymenoptera, Braconidae). Ichneumonologia Orientalis 6." volume="6" year="1977">Bhat and Gupta 1977</bibRefCitation>
: 64; 
<bibRefCitation id="2F14F0221BB26AA4DDAC36DDBC60C685" author="Chou, LY" journalOrPublisher="Journal of Taiwan Museum" pageId="7" pageNumber="102" pagination="147 - 223" title="The Braconidae (Hymenoptera) of Taiwan. 1. Agathidinae." volume="42" year="1989">Chou and Sharkey 1989</bibRefCitation>
: 175; 
<bibRefCitation id="1D977B1D1ED926695C5847EE81F3DD72" author="Chen, JH" journalOrPublisher="Science Press, Beijing" pageId="7" pageNumber="102" title="HymenopteraBraconidae (IV) Agathidinae. Fauna Sinica. Insecta 46." year="2006">Chen and Yang 2006</bibRefCitation>
