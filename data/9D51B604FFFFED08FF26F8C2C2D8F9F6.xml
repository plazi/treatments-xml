<paragraph id="15470712FFFEED0BFF26F84AC380F86D" blockId="1.[151,1436,1922,1975]" pageId="1" pageNumber="26">
<emphasis id="278CDB00FFFEED0BFF26F84AC67EF842" bold="true" box="[151,314,1922,1945]" pageId="1" pageNumber="26">FIGURES 1–4.</emphasis>
<taxonomicName id="D2F87C91FFFEED0BFE8EF84CC5E6F843" box="[319,674,1923,1945]" class="Arachnida" family="Polyaspididae" genus="Uroseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="26" phylum="Arthropoda" rank="species" species="durjadhanai" status="sp. nov." subGenus="Apionseius">
<emphasis id="278CDB00FFFEED0BFE8EF84CC6DDF843" box="[319,409,1924,1945]" italics="true" pageId="1" pageNumber="26">Uroseius</emphasis>
(
<emphasis id="278CDB00FFFEED0BFE19F84BC553F842" box="[424,535,1923,1944]" italics="true" pageId="1" pageNumber="26">Apionseius</emphasis>
) 
<emphasis id="278CDB00FFFEED0BFD94F84BC5E6F843" box="[549,674,1923,1945]" italics="true" pageId="1" pageNumber="26">durjadhanai</emphasis>
</taxonomicName>
<taxonomicNameLabel id="3CBF667BFFFEED0BFD19F84CC5BCF843" box="[680,760,1924,1945]" pageId="1" pageNumber="26" rank="species">sp. nov.</taxonomicNameLabel>
