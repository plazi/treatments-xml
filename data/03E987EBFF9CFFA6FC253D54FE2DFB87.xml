<taxonomicName id="4C404D7EFF9CFFA7FCB83A3BFAA5FB88" authority="Schicha, 1981: 210" authorityName="Schicha" authorityPageNumber="210" authorityYear="1981" box="[826,1315,1033,1057]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="14" pageNumber="298" phylum="Arthropoda" rank="species" species="paspalivorus">
