<paragraph id="ED141B75E050FFB11BB1F9A7FEB54E10" blockId="20.[677,1209,1610,1700]" lastBlockId="21.[113,648,216,559]" lastPageId="21" lastPageNumber="668" pageId="20" pageNumber="667">
The species name 
<taxonomicName id="2AAB60F6E050FFB01A68F9A7FC264ABC" baseAuthorityName="Peters" baseAuthorityYear="1876" box="[892,979,1641,1667]" class="Mammalia" family="Muridae" genus="Grammomys" kingdom="Animalia" order="Rodentia" pageId="20" pageNumber="667" phylum="Chordata" rank="species" species="rutilans">
