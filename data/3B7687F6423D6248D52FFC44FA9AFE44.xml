<taxonomicName id="74DF4D63423D6277D4EAFBB4FE78FBE7" authority="Devaney, 1970" authorityName="Devaney" authorityYear="1970" box="[189,506,1085,1112]" class="Ophiuroidea" family="Clarkcomidae" genus="Clarkcoma" kingdom="Animalia" order="Ophiacanthida" pageId="31" pageNumber="32" phylum="Echinodermata" rank="genus">
