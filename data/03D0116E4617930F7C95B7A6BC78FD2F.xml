<taxonomicName id="4C79DBFB4617930F7CB6B4DABF83FD2F" authority="(Meyrick, 1913)" baseAuthorityName="Meyrick" baseAuthorityYear="1913" box="[180,507,680,707]" class="Insecta" family="Gelechiidae" genus="Gelechia" kingdom="Animalia" order="Lepidoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="sematica">
