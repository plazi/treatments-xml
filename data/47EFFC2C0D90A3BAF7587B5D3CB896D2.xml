<taxonomicName id="67EB7058ED01FBABEE1356BD5BBEA20E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:135656" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus maculatus st. fairmairei Santschi" order="Hymenoptera" phylum="Arthropoda" rank="subSpecies" species="maculatus" subSpecies="fairmairei">Camponotus maculatus st. Fairmairei</taxonomicName>
<taxonomicNameLabel id="CEEEC1569C6909CFE94BAB2BA85B19DB" rank="subSpecies">n. st.</taxonomicNameLabel>
