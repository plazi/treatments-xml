<quantity id="C4029B23254F9A3611DDE738FE09FE31" box="[400,456,367,391]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="34" pageNumber="1644" unit="m" value="20.0">20 m</quantity>
