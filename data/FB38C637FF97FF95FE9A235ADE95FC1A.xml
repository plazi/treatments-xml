<taxonomicName id="B4910CA2FF97FF94FE9A235AD89AFDFF" authority="Hall &amp; Evenhuis" authorityName="Hall &amp; Evenhuis" box="[264,660,593,619]" class="Insecta" family="Bombyliidae" genus="Geron" kingdom="Animalia" order="Diptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="weemsi" status="sp. nov.">Geron weemsi Hall &amp; Evenhuis</taxonomicName>
, 
<taxonomicNameLabel id="5AD61648FF97FF94FD32235AD8F0FDFF" box="[672,766,593,619]" pageId="64" pageNumber="65" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
