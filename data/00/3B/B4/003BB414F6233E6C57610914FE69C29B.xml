<taxonomicName authority="W. Sm. var. diaphana" box="[334,825,1861,1887]" class="Bacillariophyceae" family="Cocconeidaceae" genus="Cocconeis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Achnanthales" pageId="8" pageNumber="9" phylum="Ochrophyta" rank="variety" species="diaphana" variety="diaphana">
