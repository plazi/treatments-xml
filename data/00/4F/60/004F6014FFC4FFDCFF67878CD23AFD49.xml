<collectingCountry box="[299,385,421,445]" name="Malaysia" pageId="8" pageNumber="769">Malaysia</collectingCountry>
(
<collectingCountry box="[397,484,421,445]" name="Malaysia" pageId="8" pageNumber="769">Sarawak</collectingCountry>
