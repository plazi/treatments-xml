<emphasis bold="true" box="[151,275,1248,1270]" pageId="3" pageNumber="304">FIGURE 2.</emphasis>
<taxonomicName box="[282,508,1248,1270]" class="Arachnida" family="Pyroglyphidae" genus="Tuccioglyphus" kingdom="Animalia" order="Astigmata" pageId="3" pageNumber="304" phylum="Arthropoda" rank="species" species="setosus" status="gen. et sp. nov.">
<emphasis box="[282,508,1248,1270]" italics="true" pageId="3" pageNumber="304">Tuccioglyphus setosus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[515,673,1248,1269]" pageId="3" pageNumber="304" rank="species">
