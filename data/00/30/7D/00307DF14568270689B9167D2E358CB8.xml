and 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:0A97801F-8B97-481C-B59C-4F8C97FCD0E9" authority="Swainson, 1839" authorityName="Swainson" authorityYear="1839" class="Actinopterygii" family="Cobitidae" genus="Diacantha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-ZBK="Canthrophrys Diacantha flavicauda Swainson 1839:310" order="Cypriniformes" pageNumber="11" phylum="Chordata" rank="species" species="flavicauda">D. flavicauda</taxonomicName>
