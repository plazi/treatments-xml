<number box="[1347,1389,1268,1292]" pageId="5" pageNumber="576" value="300.0">300</number>
and 
<number box="[151,194,1304,1328]" pageId="5" pageNumber="576" value="380.0">380</number>
-m depth off 
<collectingCountry box="[343,439,1303,1328]" name="Namibia" pageId="5" pageNumber="576">Namibia</collectingCountry>
(
<bibRefCitation author="Macpherson" box="[455,916,1303,1328]" pageId="5" pageNumber="576" refString="Macpherson, E. (1983) Crustaceos decapodos capturados en las costas de Namibia. Resultados Expediciones Cientificas, 11, 3 - 80." type="journal article" year="1983">
