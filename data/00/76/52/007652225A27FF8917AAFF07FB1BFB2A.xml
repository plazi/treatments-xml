<taxonomicName authority="Meyrick" authorityName="Meyrick" authorityYear="1915" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="species" species="dorcas">
<emphasis italics="true" pageId="6" pageNumber="307">P. dorcas</emphasis>
Meyrick
</taxonomicName>
, 
<taxonomicName authority="Davis" authorityName="Davis" authorityYear="1994" box="[343,583,696,721]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="species" species="puyehuensis">
