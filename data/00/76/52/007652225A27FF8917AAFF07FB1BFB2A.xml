<taxonomicName authorityName="Hering" authorityYear="1958" box="[1315,1429,1170,1195]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="species" species="abatiae">
