<taxonomicName authority="Kawahara, Nishida &amp; Davis, 2009" authorityName="Kawahara, Nishida &amp; Davis" authorityYear="2009" box="[334,1037,1725,1750]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="327" phylum="Arthropoda" rank="species" species="tropaeolicola">
