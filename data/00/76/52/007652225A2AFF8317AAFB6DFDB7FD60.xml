<subSubSection pageId="12" pageNumber="313" type="etymology">
<paragraph blockId="12.[151,1436,151,645]" pageId="12" pageNumber="313">
<emphasis bold="true" box="[199,326,583,608]" pageId="12" pageNumber="313">Etymology</emphasis>
