<taxonomicName authority="Meyrick, 1915" authorityName="Meyrick" authorityYear="1915" box="[330,727,721,746]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="311" phylum="Arthropoda" rank="species" species="dorcas">
