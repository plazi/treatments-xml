<taxonomicName authorityName="Meyrick" authorityYear="1915" box="[790,1012,1051,1076]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="rotans">
