<subSubSection pageId="11" pageNumber="60" type="etymology">
<paragraph blockId="11.[151,1437,1271,1767]" pageId="11" pageNumber="60">
<emphasis bold="true" box="[199,334,1706,1731]" pageId="11" pageNumber="60">Etymology.</emphasis>
