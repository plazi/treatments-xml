<taxonomicName authority="Murray" authorityName="Murray" box="[585,883,967,989]" class="Heterotardigrada" family="Echiniscidae" genus="Echiniscus" higherTaxonomySource="CoL" kingdom="Animalia" order="Echiniscoidea" pageId="12" pageNumber="89" phylum="Tardigrada" rank="species" species="militaris">
<taxonomicName box="[585,793,967,989]" class="Heterotardigrada" family="Echiniscidae" genus="Echiniscus" higherTaxonomySource="CoL" kingdom="Animalia" order="Echiniscoidea" pageId="12" pageNumber="89" phylum="Tardigrada" rank="species" species="militaris">
<taxonomicName box="[585,694,967,989]" class="Heterotardigrada" family="Echiniscidae" genus="Echiniscus" higherTaxonomySource="CoL" kingdom="Animalia" order="Echiniscoidea" pageId="12" pageNumber="89" phylum="Tardigrada" rank="genus">
