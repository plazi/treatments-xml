<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Ramaliella" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ramaliella polyclada" order="Asterales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="polyclada">
