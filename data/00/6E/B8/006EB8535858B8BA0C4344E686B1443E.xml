and 
<taxonomicName class="Arachnida" family="Gymnodamaeidae" genus="Gymnodamaeus" genus.bestMatchDistance="3885" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="263" phylum="Arthropoda" rank="species" species="bicostatus">G. bicostatus</taxonomicName>
