<collectingCountry box="[365,460,620,645]" name="Vietnam" pageId="26" pageNumber="177">Vietnam</collectingCountry>
(Phu Quoc Island), 
<collectingCountry box="[712,812,620,645]" name="Thailand" pageId="26" pageNumber="177">Thailand</collectingCountry>
(
<collectingRegion box="[832,924,620,644]" country="Thailand" name="Ranong" pageId="26" pageNumber="177">Ranong</collectingRegion>
, 
<collectingRegion box="[941,1008,620,645]" country="Thailand" name="Krabi" pageId="26" pageNumber="177">Krabi</collectingRegion>
, and 
<collectingRegion box="[1079,1145,621,645]" country="Thailand" name="Trang" pageId="26" pageNumber="177">Trang</collectingRegion>
Provinces) (
<bibRefCitation author="Hamalainen" pageId="26" pageNumber="177" refString="Hamalainen, M. (2017) The Caloptera damselflies of Thailand - Distribution maps by provinces (Odonata: Calopterygoidea). Faunistic studies in South-East Asian and Pacific Island Odonata, 19, 1 - 28." type="journal article" year="2017">Hämäläinen 2017</bibRefCitation>
), 
<collectingCountry box="[230,345,655,680]" name="Cambodia" pageId="26" pageNumber="177">Cambodia</collectingCountry>
(Preah Sihanouk Province) (
<bibRefCitation author="Kosterin" box="[668,895,655,680]" pageId="26" pageNumber="177" refString="Kosterin, O. E., Chartier, G., Holden, J. &amp; Mey, F. S. (2012) New records of Odonata from Cambodia, based mostly on photographs. Cambodian Journal of Natural History, 2012 (2), 150 - 163." type="journal article" year="2012">
Kosterin 
<emphasis box="[773,829,655,680]" italics="true" pageId="26" pageNumber="177">et al.</emphasis>
2012
</bibRefCitation>
, and Kosterin 
<emphasis box="[1057,1194,657,680]" italics="true" pageId="26" pageNumber="177">pers. comm.</emphasis>
).
</paragraph>
