<taxonomicName baseAuthorityName="Froelich" baseAuthorityYear="1789" class="Trematoda" family="Brachycoeliidae" genus="Brachycoelium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="4" pageNumber="5" phylum="Platyhelminthes" rank="species" species="salamandrae">
