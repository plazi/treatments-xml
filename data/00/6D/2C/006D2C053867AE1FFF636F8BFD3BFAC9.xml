<taxonomicName authority="Rudolphi" authorityName="Rudolphi" box="[757,1152,1184,1210]" class="Trematoda" family="Fasciolidae" genus="Distomum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="12" pageNumber="13" phylum="Platyhelminthes" rank="species" species="variegatum">
