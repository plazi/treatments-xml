<taxonomicName authority="(Looss)" baseAuthorityName="Looss" box="[459,762,1225,1251]" class="Trematoda" family="Haematoloechidae" genus="Haematoloechus" kingdom="Animalia" order="Plagiorchiida" pageId="12" pageNumber="13" phylum="Platyhelminthes" rank="genus">
