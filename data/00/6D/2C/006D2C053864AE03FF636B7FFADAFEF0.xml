<taxonomicName baseAuthorityName="Stafford" baseAuthorityYear="1905" box="[1164,1334,280,306]" class="Trematoda" family="Cladorchiidae" genus="Megalodiscus" kingdom="Animalia" order="Plagiorchiida" pageId="16" pageNumber="17" phylum="Platyhelminthes" rank="species" species="temperatus">
