<taxonomicName authorityName="Chandler" authorityYear="1923" box="[1119,1282,239,264]" class="Trematoda" family="Cladorchiidae" genus="Megalodiscus" kingdom="Animalia" order="Plagiorchiida" pageId="16" pageNumber="17" phylum="Platyhelminthes" rank="genus">
