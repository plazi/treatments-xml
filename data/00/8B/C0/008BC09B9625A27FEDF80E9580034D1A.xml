<taxonomicName authorityName="Eggers" authorityYear="1930" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus dalbergiae" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="dalbergiae">
