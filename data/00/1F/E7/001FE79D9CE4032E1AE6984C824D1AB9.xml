(
<taxonomicName authority="Haenke" authorityName="Haenke" class="Magnoliopsida" family="Campanulaceae" genus="Campanula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="391" phylum="Tracheophyta" rank="species" species="pusilla">
