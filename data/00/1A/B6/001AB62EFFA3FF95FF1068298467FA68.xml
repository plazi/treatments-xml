<collectingRegion country="United States of America" name="Wyoming" pageId="57" pageNumber="58">Wyoming</collectingRegion>
, 
<collectingRegion box="[785,891,428,451]" country="United States of America" name="Montana" pageId="57" pageNumber="58">Montana</collectingRegion>
, 
<collectingRegion box="[915,1026,428,451]" country="United States of America" name="Colorado" pageId="57" pageNumber="58">Colorado</collectingRegion>
, 
<collectingRegion box="[1049,1223,428,451]" country="United States of America" name="North Dakota" pageId="57" pageNumber="58">North Dakota</collectingRegion>
, 
<collectingRegion box="[699,861,457,480]" country="United States of America" name="South Dakota" pageId="57" pageNumber="58">South Dakota</collectingRegion>
, 
<collectingRegion box="[873,1041,457,480]" country="Canada" name="Saskatchewan" pageId="57" pageNumber="58">Saskatchewan</collectingRegion>
, 
<collectingRegion box="[1052,1173,457,480]" country="United States of America" name="California" pageId="57" pageNumber="58">California</collectingRegion>
, 
<collectingRegion country="United States of America" name="Nevada" pageId="57" pageNumber="58">Nevada</collectingRegion>
, 
<collectingRegion box="[775,834,486,509]" country="United States of America" name="Utah" pageId="57" pageNumber="58">Utah</collectingRegion>
, 
<collectingRegion box="[854,924,486,509]" country="United States of America" name="Texas" pageId="57" pageNumber="58">Texas</collectingRegion>
