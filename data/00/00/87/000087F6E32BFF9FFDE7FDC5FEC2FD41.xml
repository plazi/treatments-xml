<subSubSection box="[189,1020,468,542]" pageId="15" pageNumber="16" type="distribution">
<paragraph blockId="15.[189,342,468,494]" box="[189,342,468,494]" pageId="15" pageNumber="16">
<heading bold="true" box="[189,342,468,494]" fontSize="11" level="3" pageId="15" pageNumber="16" reason="3">
<emphasis bold="true" box="[189,342,468,494]" pageId="15" pageNumber="16">Distribution</emphasis>
</heading>
</paragraph>
<paragraph blockId="15.[189,1020,515,542]" box="[189,1020,515,542]" pageId="15" pageNumber="16">
