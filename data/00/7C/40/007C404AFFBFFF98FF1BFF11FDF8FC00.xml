<subSubSection pageId="4" pageNumber="232" type="discussion">
<paragraph blockId="4.[151,1437,389,992]" pageId="4" pageNumber="232">
<emphasis bold="true" box="[199,308,894,920]" pageId="4" pageNumber="232">Remarks</emphasis>
. Specimens from 
<collectingRegion box="[512,575,894,920]" country="Ecuador" name="Napo" pageId="4" pageNumber="232">Napo</collectingRegion>
, 
<collectingCountry box="[587,680,893,919]" name="Ecuador" pageId="4" pageNumber="232">Ecuador</collectingCountry>
