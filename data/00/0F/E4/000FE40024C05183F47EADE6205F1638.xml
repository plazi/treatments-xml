<paragraph pageNumber="122">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147232" authority="Emery, 1921" authorityName="Emery" authorityYear="1921" class="Insecta" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sima Roger" order="Hymenoptera" pageNumber="122" phylum="Arthropoda" rank="genus">Sima</taxonomicName>
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:187983" authority="Emery, 1921" authorityName="Emery" authorityYear="1921" class="Insecta" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sima allaborans (Walker)" order="Hymenoptera" pageNumber="122" phylum="Arthropoda" rank="species" species="allaborans">allaborans</taxonomicName>
var
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145304" authority="Emery, 1921" authorityName="Emery" authorityYear="1921" class="Insecta" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sima allaborans var. longinoda Forel" order="Hymenoptera" pageNumber="122" phylum="Arthropoda" rank="subSpecies" species="allaborans" subSpecies="longinoda">. longinoda</taxonomicName>
