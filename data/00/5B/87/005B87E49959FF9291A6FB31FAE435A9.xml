<subSubSection pageId="2" pageNumber="191" type="etymology">
<paragraph blockId="2.[151,1437,1384,1771]" pageId="2" pageNumber="191">
<emphasis bold="true" box="[199,334,1493,1519]" pageId="2" pageNumber="191">Etymology.</emphasis>
