<taxonomicName class="Insecta" family="Carabidae" genus="Trechus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trechus" order="Coleoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="genus">Trechus</taxonomicName>
