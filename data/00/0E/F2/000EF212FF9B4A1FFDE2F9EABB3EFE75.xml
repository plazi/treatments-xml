<paragraph blockId="17.[564,1024,1581,1608]" box="[564,1024,1581,1608]" pageId="17" pageNumber="18">
<heading box="[564,1024,1581,1608]" centered="true" fontSize="11" level="2" pageId="17" pageNumber="18" reason="2">
