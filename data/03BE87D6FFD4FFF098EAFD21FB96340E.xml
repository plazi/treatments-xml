<quantity id="4CEF9B25FFD4FFF09A2FFC74F8EE33A4" box="[594,687,902,929]" metricMagnitude="3" metricUnit="m" metricValue="1.5364968" pageId="69" pageNumber="70" unit="ft" value="5041.0">5,041 ft</quantity>
