<quantity id="4CEF9B25FFD4FFF09D54FC74FFD433A4" box="[1321,1429,902,929]" metricMagnitude="3" metricUnit="m" metricValue="4.2668952" pageId="69" pageNumber="70" unit="ft" value="13999.0">13,999 ft</quantity>
