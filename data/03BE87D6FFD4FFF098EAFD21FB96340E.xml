<quantity id="4CEF9B25FFD4FFF0999FFC74F80033A5" box="[482,577,902,929]" metricMagnitude="3" metricUnit="m" metricValue="1.537" pageId="69" pageNumber="70" unit="m" value="1537.0">1,537 m</quantity>
