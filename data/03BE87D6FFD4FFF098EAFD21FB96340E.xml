<quantity id="4CEF9B25FFD4FFF09CC3FC74FF5933A5" box="[1214,1304,902,928]" metricMagnitude="3" metricUnit="m" metricValue="1.3008864" pageId="69" pageNumber="70" unit="ft" value="4268.0">4,268 ft</quantity>
