<bibRefCitation id="506595F3FFE2521EFF40F8668545F81A" author="Koehler, R." box="[159,330,1930,1956]" pageId="84" pageNumber="85" pagination="1 - 142" refId="ref88606" refString="Koehler, R. (1905) Ophiures littorales. Siboga-Expeditie Monograph, 45, 1 - 142," type="journal article" year="1905">Koehler, 1905</bibRefCitation>
)
</taxonomicName>
, 
<collectionCode id="52E570C7FFE2521EFE80F86685B6F81A" box="[351,441,1930,1956]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="84" pageNumber="85">MNHN</collectionCode>
EcOs 22478, 
<specimenCount id="22F2238BFFE2521EFD80F86686FCF81A" box="[607,755,1930,1956]" pageId="84" pageNumber="85" type="generic">4 specimens</specimenCount>
