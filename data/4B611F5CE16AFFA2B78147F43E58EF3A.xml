<quantity id="043003AFE16AFFA2B62343213C73EE6D" box="[309,323,1090,1114]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="405" unit="mm" value="9.0">9</quantity>
