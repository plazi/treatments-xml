= 
<taxonomicName id="AEF4F96DF9D39AEDFD1B0F21AB0FA9EE" class="Bivalvia" family="Unionidae" genus="Unio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Unio rectangularis" order="Unionida" pageId="1" pageNumber="24" phylum="Mollusca" rank="species" species="rectangularis">Unio rectangularis</taxonomicName>
