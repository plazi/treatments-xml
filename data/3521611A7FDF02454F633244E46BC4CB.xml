([crosses]]), 
<taxonomicName id="3960C6F03E2C030FF2D5EF62573F4096" LSID-ZBK="urn:lsid:zoobank.org:act:21DBAAB2-7F89-4855-A753-00B67D92E428" class="Actinopterygii" family="Liparidae" genus="Allocareproctus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Allocareproctus tanix Orr &amp; Busby 2006" order="Scorpaeniformes" pageNumber="35" phylum="Chordata" rank="species" species="tanix">A. tanix</taxonomicName>
