<emphasis id="D8CCEA92FF91FFE0FED3E5C2FD89F90F" bold="true" box="[264,518,1750,1776]" pageId="2" pageNumber="19">
<emphasis id="D8CCEA92FF91FFE0FED3E5C2FEEAF90F" bold="true" box="[264,357,1750,1776]" italics="true" pageId="2" pageNumber="19">Lapsias</emphasis>
<bibRefCitation id="8E294B71FF91FFE0FEB7E5C2FD89F90F" author="Simon" box="[364,518,1750,1776]" pageId="2" pageNumber="28" refString="Simon, E. (1900) Descriptions d'arachnides nouveaux de la famille des Attidae. Annales de la Societe entomologique de Belgique, 44, 381 - 407" type="journal article" year="1900">
Simon, 
<number id="5C7024A7FF91FFE0FE10E5C2FD89F90F" box="[459,518,1750,1776]" pageId="2" pageNumber="19" value="1900.0">1900</number>
