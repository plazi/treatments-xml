(
<figureCitation id="EBE168DE5519FFE08C7DF12BFE4E9D7E" box="[280,416,1842,1864]" captionStart="Figures 44–52" captionStartId="18.[164,254,1100,1119]" captionTargetBox="[325,1283,199,1068]" captionTargetId="figure-275@18.[323,1283,197,1071]" captionTargetPageId="18" captionText="Figures 44–52. Adults: 44, male, Cossula gaudeator; 45, female, Cossula gaudeator; 46, male, Cossula longirostrum; 47, female, Cossula longirostrum; 48, male, Cossula minutiloba; 49, female, Cossula minutiloba; 50, unidentified female 1; 51, unidentified female 2; 52, unidentified female 3. Scale bar = 10.0 mm." figureDoi="http://doi.org/10.5281/zenodo.5446393" httpUri="https://zenodo.org/record/5446393/files/figure.png" pageId="49" pageNumber="271">FIGS 46, 47</figureCitation>
, 
<figureCitation id="EBE168DE5519FFE08CCBF12BFE149D71" box="[430,506,1842,1864]" captionStart="Figures 77–88" captionStartId="21.[144,234,1387,1406]" captionTargetBox="[303,1261,196,1355]" captionTargetId="figure-145@21.[303,1263,196,1358]" captionTargetPageId="21" captionText="Figures 77–88. Male genitalia. Cossula cossuloides: 77, genital capsule; 78, aedoeagus. Cossula duplex: 79, genital capsule; 80, aedoeagus. Cossula duplexata: 81, genital capsule; 82, aedoeagus. Cossula gaudeator: 83, genital capsule; 84, aedoeagus. Cossula longirostrum: 85, genital capsule; 86, aedoeagus. Cossula minutiloba: 87, genital capsule; 88, aedoeagus." figureDoi="http://doi.org/10.5281/zenodo.5446400" httpUri="https://zenodo.org/record/5446400/files/figure.png" pageId="49" pageNumber="271">85, 86</figureCitation>
, 
<figureCitation id="EBE168DE5519FFE08F6CF12BFDD89D71" box="[521,566,1842,1864]" captionStart="Figures 101–106" captionStartId="24.[164,254,1348,1367]" captionTargetBox="[387,1221,196,1317]" captionTargetId="figure-164@24.[387,1221,196,1317]" captionTargetPageId="24" captionText="Figures 101–106. Male genitalia, lateral view of genital capsule: 101, Cossula cossuloides; 102, Cossula duplex; 103, Cossula duplexata; 104, Cossula gaudeator; 105, Cossula longirostrum; 106, Cossula minutiloba. Scale bar = 1.0 mm." figureDoi="http://doi.org/10.5281/zenodo.5446407" httpUri="https://zenodo.org/record/5446407/files/figure.png" pageId="49" pageNumber="271">105</figureCitation>
, 
<figureCitation id="EBE168DE5519FFE08F21F12BFD819D71" box="[580,623,1842,1864]" captionStart="Figures 141–144" captionStartId="53.[144,234,1047,1066]" captionText="Figures 141–144. Species distribution maps in Costa Rica." pageId="49" pageNumber="271">142</figureCitation>
