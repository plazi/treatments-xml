<taxonomicName id="4C334D56D55239571894F812FAFF47F0" authorityName="Peters" authorityYear="1871" box="[1181,1280,1969,1993]" class="Mammalia" family="Mammalia (awaiting allocation)" genus="Doryrhina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mammalia (awaiting allocation)" pageId="18" pageNumber="499" phylum="Chordata" rank="genus">
