<heading id="653E7570FFFB934F4AF91C75FD090853" box="[178,746,1297,1319]" fontSize="8" level="2" pageId="10" pageNumber="591" reason="2">
<taxonomicName id="F9C9B99FFFFB934F4AF91C75FD090853" authority="(NYLANDER 1848)" baseAuthorityName="NYLANDER" baseAuthorityYear="1848" box="[178,746,1297,1319]" class="Lecanoromycetes" family="Teloschistaceae" genus="Dufourea" kingdom="Fungi" order="Teloschistales" pageId="10" pageNumber="591" phylum="Ascomycota" rank="species" species="dentiventris" subGenus="Halictoides">
