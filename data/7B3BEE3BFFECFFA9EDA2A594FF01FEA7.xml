<taxonomicName id="349224AEFFECFFAFEDA2A400FD4FFAFA" authority="Baker 1924: 63" authorityName="Baker" authorityPageNumber="63" authorityYear="1924" box="[151,673,1322,1344]" class="Insecta" family="Cicadellidae" genus="Makilingia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="491" phylum="Arthropoda" rank="variety" species="variabilis" variety="simillima">
