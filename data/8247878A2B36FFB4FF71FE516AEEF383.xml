<number id="BC2624BB2B36FFB4FD7FFDBC68CDF57C" box="[665,718,576,599]" pageId="7" pageNumber="8" value="1917.0">1917</number>
</taxonomicName>
<taxonomicName id="CDEE4D1F2B36FFB4FEC9FD9C68B5F55C" authority="Corde, 1923" authorityName="Corde" authorityYear="1923" box="[303,694,608,631]" class="Rhabditophora" family="Typhloplanidae" genus="Typhloplanella" kingdom="Animalia" order="Rhabdocoela" pageId="7" pageNumber="8" phylum="Platyhelminthes" rank="species" species="limicola">
