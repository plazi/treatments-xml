. 
<emphasis id="B9708E3CFF90FFBECB31FADDFD49B15C" bold="true" box="[633,759,1310,1336]" pageId="5" pageNumber="198">
<collectingRegion id="49C09CCCFF90FFBECB31FADDFD4FB15C" box="[633,753,1310,1336]" country="Brazil" name="Sao Paulo" pageId="5" pageNumber="198">São Paulo</collectingRegion>
:
</emphasis>
<location id="8EDB04F5FF90FFBECBB5FADDFCF9B15C" LSID="urn:lsid:plazi:treatment:03ADE338FF90FFBEC9C0FC3DFC4EB251:8EDB04F5FF90FFBECBB5FADDFCF9B15C" box="[765,839,1310,1336]" country="Brazil" name="Itarare" pageId="5" pageNumber="198" stateProvince="Sao Paulo">Itararé</location>
, 
