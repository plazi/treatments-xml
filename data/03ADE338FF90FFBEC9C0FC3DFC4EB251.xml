<collectingDate id="EFFE8D06FF90FFBECA1AFADDFC52B15C" box="[850,1004,1310,1336]" pageId="5" pageNumber="198" value="1993-04-22">22 April 1993</collectingDate>
</date>
, 
<emphasis id="B9708E3CFF90FFBECABFFADCFA80B15C" box="[1015,1342,1310,1337]" italics="true" pageId="5" pageNumber="198">
