<td id="9C6A0DE423A88DF8F547C0D897ACDF68" align="left" pageId="null" pageNumber="166" valign="top">
1*. 
<normalizedToken id="2E4DC12B41A162BA16C6BEF402492D51" originalValue="Blätter" pageId="null" pageNumber="166">Blaetter</normalizedToken>
