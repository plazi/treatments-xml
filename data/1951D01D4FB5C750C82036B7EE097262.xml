<taxonomicName id="801E79263A70AB88B919DD460AF44D49" baseAuthorityName="H.Adams" baseAuthorityYear="1867" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="23" pageNumber="24" phylum="Mollusca" rank="species" species="bartletti" subGenus="Drymaeus">
