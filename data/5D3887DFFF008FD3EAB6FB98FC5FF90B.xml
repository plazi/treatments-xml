<taxonomicName id="12914D4AFF038FD3EBA7F9D3FE86F9CB" baseAuthorityName="Silvestri" baseAuthorityYear="1927" box="[217,344,1597,1621]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="138" pageNumber="2235" phylum="Arthropoda" rank="species" species="opulenta">
