<paragraph id="D52E36C9FF038FD3EBF4F9F0FC5FF90B" blockId="138.[138,1185,1565,1685]" pageId="138" pageNumber="2235">
<taxonomicName id="12914D4AFF038FD3EBF4F9F0FEE1F9AB" baseAuthorityName="Girault" baseAuthorityYear="1931" box="[138,319,1565,1589]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="138" pageNumber="2235" phylum="Arthropoda" rank="species" species="socratis">
<emphasis id="E7E5EADBFF038FD3EBF4F9F0FEE1F9AB" box="[138,319,1565,1589]" italics="true" pageId="138" pageNumber="2235">Encarsia socratis</emphasis>
</taxonomicName>
