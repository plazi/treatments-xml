<emphasis id="F4C07F1BFE0CD875FEFB889BFE78FBB9" box="[328,470,1004,1028]" italics="true" pageId="1" pageNumber="546">S. peruviana</emphasis>
(
<bibRefCitation id="A225DEF8FE0CD875FE57889BFC8FFBB9" author="Brailovsky, H. &amp; Barrera, E." box="[484,801,1004,1029]" pageId="1" pageNumber="546" pagination="261 - 273" refId="ref5580" refString="Brailovsky, H. &amp; Barrera, E. (1997) Especies nuevas del genero Stenoscelidea (Hemiptera: Heteroptera: Coreidae: Acanthocephalini). Anales del Instituto de Biologia, Universidad Nacional Autonoma de Mexico, Serie Zoologia, 68 (2), 261 - 273." type="journal article" year="1997">Brailovsky &amp; Barrera, 1997</bibRefCitation>
)
</taxonomicName>
, and 
<taxonomicName id="01B4D88AFE0CD875FCD6889BFB03FBB9" authority="(Brailovsky, 1983)" authorityName="Brailovsky" authorityYear="1983" baseAuthorityName="Brailovsky" baseAuthorityYear="1983" box="[869,1197,1004,1029]" class="Insecta" family="Coreidae" genus="Stenoscelidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="546" phylum="Arthropoda" rank="species" species="prolixa">
<emphasis id="F4C07F1BFE0CD875FCD6889BFC7FFBB9" box="[869,977,1004,1028]" italics="true" pageId="1" pageNumber="546">S. prolixa</emphasis>
(
