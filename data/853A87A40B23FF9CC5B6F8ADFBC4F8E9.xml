''E
</geoCoordinate>
), 
<quantity id="CA6B9B570B23FF82C758F830FD48F801" box="[633,723,2010,2036]" metricMagnitude="3" metricUnit="m" metricValue="2.311" pageId="14" pageNumber="15" unit="m" value="2311.0">2311 m</quantity>
, 
<date id="792D10720B23FF82C7FFF830FC07F801" box="[734,924,2010,2036]" pageId="14" pageNumber="15" value="2009-04-30" valueMax="2009-05-01" valueMin="2009-04-30">
