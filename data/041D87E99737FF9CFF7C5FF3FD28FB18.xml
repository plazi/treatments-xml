<collectingCountry id="F4A3766F9737FF9CFD305C32FCE2FCC4" box="[731,801,854,878]" name="Belize" pageId="32" pageNumber="33">Belize</collectingCountry>
[Carrie Bow Cay], 
<collectingCountry id="F4A3766F9737FF9CFC165C32FB29FCC4" box="[1021,1258,854,878]" name="British Virgin Islands" pageId="32" pageNumber="33">British Virgin Islands</collectingCountry>
