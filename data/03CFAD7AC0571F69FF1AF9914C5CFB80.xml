<paragraph id="8BD91C6CC0571F68FF1AF9914A4CF9FE" blockId="0.[151,573,1586,1612]" box="[151,573,1586,1612]" pageId="0" pageNumber="55">
<heading id="D091AB00C0571F68FF1AF9914A4CF9FE" bold="true" box="[151,573,1586,1612]" fontSize="11" level="1" pageId="0" pageNumber="55" reason="1">
<emphasis id="B912C07EC0571F68FF1AF9914A4CF9FE" bold="true" box="[151,573,1586,1612]" pageId="0" pageNumber="55">
Genus 
<taxonomicName id="4C6667EFC0571F68FF63F9914A4CF9FE" authority="Deshayes, 1824" authorityName="Deshayes" authorityYear="1824" box="[238,573,1586,1612]" class="Gastropoda" family="Neritidae" genus="Tomostoma" higherTaxonomySource="CoL" kingdom="Animalia" order="Archaeogastropoda" pageId="0" pageNumber="55" phylum="Mollusca" rank="genus">
<emphasis id="B912C07EC0571F68FF63F9914908F9FE" bold="true" box="[238,377,1586,1612]" italics="true" pageId="0" pageNumber="55">Tomostoma</emphasis>
Deshayes, 
<number id="3DAE0E4BC0571F68FD8FF9914A4CF9FE" box="[514,573,1586,1612]" pageId="0" pageNumber="55" value="1824.0">1824</number>
</taxonomicName>
