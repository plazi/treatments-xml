<taxonomicName id="82F23DD6FFFEFF9EFE4AFDE7FDC8545C" authorityName="Semenov" authorityYear="1903" box="[429,613,590,614]" class="Insecta" family="Aphodiidae" genus="Cnemargulus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="genus">
