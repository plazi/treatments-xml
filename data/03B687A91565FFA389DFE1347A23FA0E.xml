<caption id="DF6066371567FFA389DFE2777AADFB3E" box="[151,1220,1150,1172]" httpUri="https://zenodo.org/record/271141/files/figure.png" pageId="6" pageNumber="93" targetBox="[226,1364,191,1112]" targetPageId="6">
<paragraph id="8BA036BF1567FFA389DFE2777AADFB3E" blockId="6.[151,1220,1150,1172]" box="[151,1220,1150,1172]" pageId="6" pageNumber="93">
