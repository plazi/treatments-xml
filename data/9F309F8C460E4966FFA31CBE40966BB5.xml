<taxonomicName id="7720A7878D736F2A1DE84EC60E46A005" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" phylum="Arthropoda" rank="variety" species="auropubens" variety="jacob">var. Jacob</taxonomicName>
, 
