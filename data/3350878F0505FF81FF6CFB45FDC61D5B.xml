<taxonomicName id="7CF94D1A0505FF81FCBDFAC2FBBA1F5D" authorityName="Gould" authorityYear="1847" box="[838,1037,1316,1339]" class="Aves" family="Trochilidae" genus="Trochilus" kingdom="Animalia" order="Apodiformes" pageId="22" pageNumber="423" phylum="Chordata" rank="species" species="coeruleogularis">
