<collectionCode id="768BAE0DFFEAED2DCA7BF9F8301CF9A8" LSID="urn:lsid:biocol.org:col:34871" box="[1046,1138,1599,1625]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="6" pageNumber="7">USNM</collectionCode>
