<collectionCode id="768BAE0DFFEAED2DCC4EFA283611F9F8" LSID="urn:lsid:biocol.org:col:34871" box="[547,639,1519,1545]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="6" pageNumber="7">USNM</collectionCode>
