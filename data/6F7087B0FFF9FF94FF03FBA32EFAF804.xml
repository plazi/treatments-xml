<paragraph id="E76636A6FFF9FF94FF53FB56290AFB65" blockId="1.[151,1437,1172,1940]" box="[199,1282,1244,1269]" pageId="1" pageNumber="543">
<emphasis id="D5ADEAB4FFF9FF94FF53FB562D4EFB65" bold="true" box="[199,326,1244,1269]" pageId="1" pageNumber="543">Etymology</emphasis>
