<paragraph id="CD377B2118DF194586FAB9C05150D637" pageId="0" pageNumber="75">
<taxonomicName id="B9C2229261285AC0BCA66EA6A2191DAC" LSID="AA421CCF-0FB9-5AA5-9714-9E68D4C0F30D" authority="Narakusumo &amp; Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus triradiatus" order="Coleoptera" pageId="0" pageNumber="75" phylum="Arthropoda" rank="species" species="triradiatus">Trigonopterus triradiatus Narakusumo &amp; Riedel</taxonomicName>
<taxonomicNameLabel id="3B49A564B9417A29C3802EA622107222" pageId="0" pageNumber="75">sp. nov.</taxonomicNameLabel>
