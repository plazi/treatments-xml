<taxonomicName id="4C669FCCFFDFEE3097439C428F7F076C" authority="LeConte, 1858: 69" authorityName="LeConte" authorityPageNumber="69" authorityYear="1858" box="[264,806,1065,1091]" class="Insecta" family="Buprestidae" genus="Acmaeodera" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="haemorrhoa">
