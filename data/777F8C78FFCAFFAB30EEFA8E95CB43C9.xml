<number id="491E2F49FFCAFFA4309CF8FA9704454D" box="[229,280,1834,1860]" pageId="7" pageNumber="24" value="0.31">0.31</number>
+ 
<number id="491E2F49FFCAFFA43151F8FA9742454D" box="[296,350,1834,1860]" pageId="7" pageNumber="24" value="2.16">2.16</number>
