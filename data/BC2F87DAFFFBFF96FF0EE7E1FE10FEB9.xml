<taxonomicName id="F3864D4FFFFBFF94FB31E456FA46FC53" authority="Otto, 2020" authorityName="Otto" authorityYear="2020" box="[1279,1442,1554,1580]" class="Insecta" family="Eucnemidae" genus="Fornax" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="12" phylum="Arthropoda" rank="species" species="convexicollis" status="new species">
