<taxonomicName id="F3864D4FFFFBFF94FBADE7B4FAECFC74" box="[1123,1288,1521,1547]" class="Insecta" family="Eucnemidae" genus="Fornax" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="12" phylum="Arthropoda" rank="species" species="undetermined">
