<quantity id="062CC698FF98F11D42F6B5014F66C679" box="[458,517,573,599]" metricMagnitude="3" metricUnit="m" metricValue="5.0" pageId="11" pageNumber="12" unit="km" value="5.0">5 km</quantity>
