<collectionCode id="A7C5F3B8FF98F11D4787B52A4873C61E" LSID="urn:lsid:biocol.org:col:34795" box="[1211,1296,534,560]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="11" pageNumber="12">FMNH</collectionCode>
