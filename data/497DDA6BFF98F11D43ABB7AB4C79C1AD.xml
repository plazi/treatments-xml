<collectionCode id="A7C5F3B8FF98F11D43ABB5014D91C679" box="[151,242,573,599]" httpUri="http://grbio.org/cool/ra64-3eva" name="University of Michigan, Museum of Zoology" pageId="11" pageNumber="12">UMMZ</collectionCode>
<number id="771C795AFF98F11D43C6B5014C31C679" box="[250,338,573,599]" pageId="11" pageNumber="12" value="199672.0">199672</number>
