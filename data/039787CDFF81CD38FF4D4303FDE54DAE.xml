<quantity id="4CC69B3EFF81CD38FC19479AFBBB4C07" box="[963,1056,1510,1536]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="15" pageNumber="16" unit="mm" value="2.2">2.2 mm</quantity>
; LP: 
<number id="3DF624FCFF81CD38FB84479AFB184C07" box="[1118,1155,1510,1536]" pageId="15" pageNumber="16" value="2.1">2.1</number>
