<quantity id="4CC69B3EFF81CD38FCC1479AFCE34C07" box="[795,888,1510,1536]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="15" pageNumber="16" unit="mm" value="1.6">1.6 mm</quantity>
