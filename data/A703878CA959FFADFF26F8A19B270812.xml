<taxonomicName id="E8AA4D19A958FFADFC2BFC1A99960FA6" authority="Vilkamaa &amp; Menzel, 2019" authorityName="Vilkamaa &amp; Menzel" authorityYear="2019" box="[922,1018,909,935]" class="Insecta" family="Sciaridae" genus="Trichocoelina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="jukkai" status="sp. n.">
