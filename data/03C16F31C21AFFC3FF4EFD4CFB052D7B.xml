<taxonomicName id="4C68A5A4C21AFFC3FB0BFBE1FAA22D33" authorityName="Elberg" authorityYear="1965" box="[1234,1368,1048,1074]" class="Insecta" family="Sciomyzidae" genus="Neodictya" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="88" phylum="Arthropoda" rank="species" species="jakovlevi">
