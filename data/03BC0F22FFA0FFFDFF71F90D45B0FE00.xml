<taxonomicName id="4C15C5B7FFA0FFFCFDD1F8B44302F887" authority="Szepligeti" authorityName="Szepligeti" box="[567,814,1855,1877]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Isurgus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="292" phylum="Arthropoda" rank="species" species="rufipes">
