<taxonomicName id="B0C04D7BFF82FFFBFA4244BE7DE9FC1D" authority="Verrill, 1915" authorityName="Verrill" authorityYear="1915" class="Asteroidea" family="Ophidiasteridae" genus="Ophidiaster" kingdom="Animalia" order="Valvatida" pageId="47" pageNumber="48" phylum="Echinodermata" rank="species" species="alexandri">
