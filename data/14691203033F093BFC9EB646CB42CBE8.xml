<paragraph id="9C7FA3150339093BFF56B324C9F5CBBC" blockId="29.[155,436,885,908]" box="[155,436,885,908]" pageId="29" pageNumber="56">
<materialsCitation id="2CA8A9480339093BFF56B324C9F5CBBC" box="[155,436,885,908]" pageId="29" pageNumber="56" specimenCount="1" specimenCount-female="1" typeStatus="allotype">
