<quantity id="4C979B2C3978FFADDD7AFA190BBAEDEB" box="[1084,1165,1472,1496]" metricMagnitude="3" metricUnit="m" metricValue="1.6149999999999998" pageId="0" pageNumber="588" unit="m" value="1615.0">1615m</quantity>
, 
<date id="FFD110093978FFADDDDFFA190A6AEDEB" box="[1177,1373,1472,1497]" pageId="0" pageNumber="588" value="2004-05-22" valueMax="2004-05-29" valueMin="2004-05-22">
