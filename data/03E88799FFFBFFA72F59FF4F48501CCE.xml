<quantity id="4CB99B6AFFFBFFA72841FE5A49501FDE" box="[1304,1389,386,412]" metricMagnitude="3" metricUnit="m" metricValue="1.525" pageId="71" pageNumber="72" unit="m" value="1525.0">1525m</quantity>
