<collectingCountry id="F356761FFFFBFFA72F43FE824E4E1F36" box="[538,627,346,372]" name="Mexico" pageId="71" pageNumber="72">México</collectingCountry>
(
<collectingRegion id="4985F86DFFFBFFA72FDAFE824EE41F36" box="[643,729,346,372]" country="Mexico" name="Puebla" pageId="71" pageNumber="72">Puebla</collectingRegion>
