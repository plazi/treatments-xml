<taxonomicName id="3A8C97E0FF8BCF28FEBCEE6EFDF6FEFA" authority="Bellardi, 1872" authorityName="Bellardi" authorityYear="1872" box="[301,596,272,296]" class="Gastropoda" family="Muricidae" genus="Murex" kingdom="Animalia" order="Neogastropoda" pageId="5" pageNumber="486" phylum="Mollusca" rank="species" species="latifolius">
