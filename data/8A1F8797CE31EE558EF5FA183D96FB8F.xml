<emphasis id="30C2EA93CE37EE568EF5F8ED3869F83E" bold="true" box="[151,278,1903,1925]" pageId="5" pageNumber="398">FIGURES 4</emphasis>
. 
<taxonomicName id="C5B64D02CE37EE568F46F8ED3899F83F" authority="Zhang &amp; Yu &amp; Zhong, 2018" authorityName="Zhang &amp; Yu &amp; Zhong" authorityYear="2018" box="[292,486,1903,1924]" class="Arachnida" family="Clubionidae" genus="Clubiona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="398" phylum="Arthropoda" rank="species" species="lamellaris" status="sp. nov.">
<emphasis id="30C2EA93CE37EE568F46F8ED3899F83F" box="[292,486,1903,1924]" italics="true" pageId="5" pageNumber="398">Clubiona lamellaris</emphasis>
</taxonomicName>
<emphasis id="30C2EA93CE37EE568F8DF8ED3B1FF83E" bold="true" box="[495,608,1903,1925]" pageId="5" pageNumber="398">
