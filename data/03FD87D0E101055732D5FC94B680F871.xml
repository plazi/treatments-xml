</taxonomicName>
<taxonomicNameLabel id="A21357AFE1010557319EF998B12BF9ED" box="[988,1077,1555,1578]" pageId="18" pageNumber="19" rank="species">sp. nov.</taxonomicNameLabel>
, 
<emphasis id="B920EAD4E1010557367DF99FB1C3F9EC" box="[1087,1245,1555,1579]" italics="true" pageId="18" pageNumber="19">
<taxonomicName id="4C544D45E1010557367DF99FB1C7F9EC" box="[1087,1241,1555,1579]" class="Adenophorea" family="Tripylidae" genus="Trischistoma" kingdom="Animalia" order="Enoplida" pageId="18" pageNumber="19" phylum="Nematoda" rank="species" species="pellucidum">T. pellucidum</taxonomicName>
,
</emphasis>
and 
<taxonomicName id="4C544D45E10105573751F99FB085F9EC" box="[1299,1435,1556,1579]" class="Adenophorea" family="Tripylidae" genus="Trischistoma" kingdom="Animalia" order="Enoplida" pageId="18" pageNumber="19" phylum="Nematoda" rank="species" species="waiotama" status="sp. nov.">
