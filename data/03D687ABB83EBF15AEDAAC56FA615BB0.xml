<collectionCode id="ED6EAE78B83EBF15AB1CADA1FA605F80" box="[1361,1436,979,1002]" httpUri="http://grbio.org/cool/zi1i-a0b5" name="Museu Nacional/Universidade Federal de Rio de Janeiro" pageId="6" pageNumber="31">MNRJ</collectionCode>
<number id="3DB7249AB83EBF15AEDAAD84FF1D5864" box="[151,225,1014,1038]" pageId="6" pageNumber="31" value="21966.0">21966</number>
