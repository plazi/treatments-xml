<taxonomicName id="4C222083FFAA7809FE10FA01FDBDFA55" baseAuthorityName="Drury" baseAuthorityYear="1770" box="[454,633,1448,1466]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="508" phylum="Arthropoda" rank="species" species="triangularis">
