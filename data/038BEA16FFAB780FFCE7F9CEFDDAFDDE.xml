<taxonomicName id="4C222083FFAA7809FD5AFAC9FCABFA9D" authorityName="Drapiez" authorityYear="1819" box="[652,879,1376,1394]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="508" phylum="Arthropoda" rank="species" species="bidentatus">
