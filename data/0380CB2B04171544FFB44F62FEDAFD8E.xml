<taxonomicName id="4C2901BE04171544FB6B4C30FF24FDA2" authority="Angas, 1877" authorityName="Angas" authorityYear="1877" class="Gastropoda" family="Cerithiidae" genus="Bittium" kingdom="Animalia" pageId="30" pageNumber="31" phylum="Mollusca" rank="species" species="turritelliformis">
