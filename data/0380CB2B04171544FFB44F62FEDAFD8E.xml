<taxonomicName id="4C2901BE04171544FC174C15FC51FDA2" baseAuthorityName="Hedley" baseAuthorityYear="1900" box="[820,954,572,598]" class="Gastropoda" family="Triphoridae" genus="Seilarex" kingdom="Animalia" pageId="30" pageNumber="31" phylum="Mollusca" rank="species" species="attenuata">
