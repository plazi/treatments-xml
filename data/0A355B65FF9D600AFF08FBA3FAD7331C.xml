<taxonomicName id="459C91F0FF9F600FFC25F9F8FBE5347B" authorityName="Mayr. Body" authorityYear="1864" box="[954,1135,1634,1658]" class="Insecta" family="Pentatomidae" genus="Placocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="598" phylum="Arthropoda" rank="species" species="viridis">
