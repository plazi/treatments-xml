<emphasis id="B0E83661FF9E600EFCECF8E0FC0C3593" bold="true" box="[883,902,1914,1938]" pageId="2" pageNumber="597">2.</emphasis>
<taxonomicName id="459C91F0FF9E600EFC10F8E0FBF73593" authorityName="Kormilev" authorityYear="1956" box="[911,1149,1914,1938]" class="Insecta" family="Pentatomidae" genus="Neoadoxoplatys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="597" phylum="Arthropoda" rank="species" species="saileri">
