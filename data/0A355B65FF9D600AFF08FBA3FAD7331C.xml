<emphasis id="B0E83661FF996009FC6BF831FB9935C2" bold="true" box="[1012,1043,1963,1987]" pageId="5" pageNumber="600">10.</emphasis>
<taxonomicName id="459C91F0FF996009FB83F831FB5E35C2" authorityName="Mayr. Body" authorityYear="1864" box="[1052,1236,1963,1987]" class="Insecta" family="Pentatomidae" genus="Placocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="600" phylum="Arthropoda" rank="species" species="viridis">
