<taxonomicName id="DAF24D5CA147DD15FB52FE5DF7042EAB" box="[1255,1351,476,500]" class="Hydrozoa" family="Stylasteridae" genus="Stylaster" kingdom="Animalia" order="Anthoathecata" pageId="50" pageNumber="51" phylum="Cnidaria" rank="genus">
<emphasis id="2F86EACDA147DD15FB52FE5DF7042EAB" box="[1255,1351,476,500]" italics="true" pageId="50" pageNumber="51">Stylaster</emphasis>
