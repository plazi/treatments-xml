</taxonomicName>
, 
<taxonomicName id="DAF24D5CA147DD15FB84FE5CF63F2EAB" box="[1073,1148,477,500]" class="Hydrozoa" family="Stylasteridae" genus="Errina" kingdom="Animalia" order="Anthoathecata" pageId="50" pageNumber="51" phylum="Cnidaria" rank="genus">
<emphasis id="2F86EACDA147DD15FB84FE5CF63F2EAB" box="[1073,1148,477,500]" italics="true" pageId="50" pageNumber="51">Errina</emphasis>
