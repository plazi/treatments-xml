<taxonomicName id="DAF24D5CA147DD15FCD9FE5CF6632EAB" box="[876,1056,477,500]" class="Hydrozoa" family="Stylasteridae" genus="Errina" kingdom="Animalia" order="Anthoathecata" pageId="50" pageNumber="51" phylum="Cnidaria" rank="species" species="capensis">
