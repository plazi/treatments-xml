<taxonomicName id="DAF24D5CA147DD15FF22FD58F30F2DAF" box="[151,332,729,752]" class="Hydrozoa" family="Stylasteridae" genus="Errina" kingdom="Animalia" order="Anthoathecata" pageId="50" pageNumber="51" phylum="Cnidaria" rank="species" species="capensis">
