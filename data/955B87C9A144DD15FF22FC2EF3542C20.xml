</taxonomicName>
on a pink 
<taxonomicName id="DAF24D5CA147DD15FD6EFD81F1782D47" box="[731,827,512,536]" class="Hydrozoa" family="Stylasteridae" genus="Stylaster" kingdom="Animalia" order="Anthoathecata" pageId="50" pageNumber="51" phylum="Cnidaria" rank="genus">
