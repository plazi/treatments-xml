<taxonomicName id="DAF24D5CA147DD15FE6AFD81F0202D47" box="[479,611,512,536]" class="Gastropoda" family="Pediculariidae" genus="Pedicularia" kingdom="Animalia" order="Littorinimorpha" pageId="50" pageNumber="51" phylum="Mollusca" rank="genus">
