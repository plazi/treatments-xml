<typeStatus id="8664886CFFEAFFB86B4CFCE0FE471E5D" box="[843,891,848,872]" pageId="5" pageNumber="31">type</typeStatus>
locality (HU 
<number id="EF1724E9FFEAFFB86C12FCFFF9411E52" box="[1045,1149,847,871]" pageId="5" pageNumber="31" value="525.0" valueMax="526.0" valueMin="524.0">524–526</number>
