<date id="2D61100EFFEAFFB86D41FCBBFDC61E70" pageId="5" pageNumber="31" value="1967-10-07">
<number id="EF1724E9FFEAFFB86D41FCBBF8681E17" box="[1350,1364,779,802]" pageId="5" pageNumber="31" value="7.0">7</number>
October 
<number id="EF1724E9FFEAFFB868C5FC9DFDC61E70" box="[194,250,813,837]" pageId="5" pageNumber="31" value="1967.0">1967</number>
</date>
(
<collectionCode id="3FCEAE0BFFEAFFB8690FFC9DFC671E70" box="[264,347,813,837]" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="5" pageNumber="31">SMNH</collectionCode>
