<emphasis id="367D962ADEBA434387D218CFACF4D7BA" italics="true" pageId="0" pageNumber="1">Arixyleborus hirsutulus</emphasis>
</taxonomicName>
, 
<taxonomicName id="0D56F69C971045A10790E27A3A81459F" genus="A." lsidName="A. sittichayai" pageId="0" pageNumber="1" rank="species" species="sittichayai">
<emphasis id="D6DEABED8FDA28388C8F76514A40257F" italics="true" pageId="0" pageNumber="1">A. sittichayai</emphasis>
