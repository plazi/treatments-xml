<taxonomicName id="2A9DC261CEEA8859FCFCD1D5083A5C97" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25034" class="Insecta" family="Formicidae" genus="Aenictus" genus.completionEvidence="missing" genus.completionRound="1" kingdom="Animalia" lsidName="Aenictus eugenii caroli Forel" lsidName-HNS="Aenictus eugenii caroli Forel" order="Hymenoptera" pageNumber="204" phylum="Arthropoda" rank="variety" species="eugeni" variety="caroli">caroli</taxonomicName>
.
</paragraph>
