<location id="65177ABC2D21424DFC00F9B3FBAFF962" LSID="urn:lsid:plazi:treatment:E8619D712D21424CFE3EFB72FDCBFE80:65177ABC2D21424DFC00F9B3FBAFF962" box="[946,1065,1650,1674]" county="The" municipality="Cockburn Sound" name="Previously" pageId="96" pageNumber="1198" stateProvince="Queensland">Previously</location>
its greatest recorded depth was 
<specimenCount id="76CEE7EE2D21424DFE6FF953FD9FF942" box="[477,537,1682,1706]" pageId="96" pageNumber="1198" type="generic">
<quantity id="A73081822D21424DFE6FF953FD93F942" box="[477,533,1682,1706]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="96" pageNumber="1198" unit="m" value="50.0">
<elevation id="EBE5CB542D21424DFE6FF953FD93F942" box="[477,533,1682,1706]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="96" pageNumber="1198" unit="m" value="50.0">50 m</elevation>
