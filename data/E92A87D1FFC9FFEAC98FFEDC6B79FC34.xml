<taxonomicName id="A6834D44FFC9FFEACC43FC3068E8FC34" authorityName="Dumortier" authorityYear="1829" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="17" pageNumber="418" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
