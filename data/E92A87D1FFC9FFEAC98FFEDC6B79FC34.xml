<taxonomicName id="A6834D44FFC9FFEACBA2FC316B0DFC50" box="[698,815,928,955]" class="Insecta" family="Cynipidae" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="418" phylum="Arthropoda" rank="family">Cynipidae</taxonomicName>
