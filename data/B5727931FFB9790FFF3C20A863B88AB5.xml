</emphasis>
<taxonomicName id="FADBB3A4FFB9790FFE92233560A98AC9" authority="Perrier, 1872" authorityName="Perrier" authorityYear="1872" box="[313,686,1650,1675]" class="Clitellata" family="Megascolecidae" genus="Perionyx" kingdom="Animalia" order="Opisthopora" pageId="8" pageNumber="9" phylum="Annelida" rank="species" species="excavatus">
