</taxonomicName>
. 
<taxonomicName id="4C6B57B9FF9F1737FCB6F88EFB16F8B0" authority="Linstow, 1906" authorityName="Linstow" authorityYear="1906" box="[803,1196,1864,1890]" class="Gordioida" family="Gordiidae" genus="Gordius" kingdom="Animalia" order="Gordioidea" pageId="25" pageNumber="26" phylum="Nematomorpha" rank="species" species="pallidus">
