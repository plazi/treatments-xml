<taxonomicName id="4C6B57B9FF9F1737FDF8F88EFCA8F8B3" box="[621,786,1864,1889]" class="Gordioida" family="Gordiidae" genus="Acutogordius" kingdom="Animalia" order="Gordioidea" pageId="25" pageNumber="26" phylum="Nematomorpha" rank="genus">
