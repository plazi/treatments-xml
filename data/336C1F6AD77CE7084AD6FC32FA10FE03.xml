<keyLead id="00340D49D77CE7094AD6F90FFA10F8DB" pageId="32" pageNumber="433">
<number id="0D0DBC5BD77CE7094AD6F90FFF28F93D" box="[151,164,1751,1772]" pageId="32" pageNumber="433" value="7.0">7</number>
