<taxonomicName id="5BFFA3ECFFCE3A728431FC0DFE0566AB" authorityName="Prinsloo &amp; LaSallei" authorityYear="1995" box="[257,504,916,939]" class="Insecta" family="Tanaostigmatidae" genus="Tanaostigmodes" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="720" phylum="Arthropoda" rank="species" species="tambotis">
