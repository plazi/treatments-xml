<taxonomicName id="59E304AABE39FF8DFEB7F8D0FD48B268" authority="K.H. Barnard, 1916" authorityName="K.H. Barnard" authorityYear="1916" box="[299,655,1805,1829]" class="Malacostraca" family="Plioplateidae" genus="Plioplateia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
