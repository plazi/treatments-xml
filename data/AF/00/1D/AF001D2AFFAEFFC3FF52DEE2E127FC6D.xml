peduncular article 
<number box="[233,248,272,298]" pageId="17" pageNumber="18" value="1.0">1</number>
longer than article 
<number box="[482,496,272,298]" pageId="17" pageNumber="18" value="2.0">2</number>
(
<number box="[515,567,272,298]" pageId="17" pageNumber="18" value="1.18">1.18</number>
x), article 
<number box="[699,714,272,298]" pageId="17" pageNumber="18" value="2.0">2</number>
longer than article 
<number box="[948,963,272,298]" pageId="17" pageNumber="18" value="3.0">3</number>
