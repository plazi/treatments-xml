<emphasis box="[305,515,1821,1844]" italics="true" pageId="28" pageNumber="29">
<taxonomicName box="[305,418,1821,1844]" class="Malacostraca" family="Ampithoidae" genus="Cymadusa" higherTaxonomySource="CoL" kingdom="Animalia" order="Amphipoda" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">Cymadusa</taxonomicName>
pemptos
