<emphasis box="[1235,1355,1771,1796]" italics="true" pageId="46" pageNumber="47">
Antenna 
<number box="[1340,1355,1771,1796]" pageId="46" pageNumber="47" value="1.0">1</number>
</emphasis>
longer than antenna 
<number box="[307,325,1811,1837]" pageId="46" pageNumber="47" value="2.0">2</number>
; peduncular article 
<number box="[554,569,1811,1837]" pageId="46" pageNumber="47" value="1.0">1</number>
subequal to article 
<number box="[798,812,1811,1837]" pageId="46" pageNumber="47" value="2.0">2</number>
(
