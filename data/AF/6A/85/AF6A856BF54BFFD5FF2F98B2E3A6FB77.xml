<taxonomicName authority="Urso-Guimarães &amp; Cruz &amp; Martinelli &amp; Peronti, 2020" authorityName="Urso-Guimarães &amp; Cruz &amp; Martinelli &amp; Peronti" authorityYear="2020" box="[206,465,1274,1299]" class="Insecta" family="Cecidomyiidae" genus="Diadiplosis" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="saccharum" status="sp. nov.">
