<taxonomicName box="[1295,1566,335,374]" class="Insecta" family="Formicidae" genus="Calyptomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="410" phylum="Arthropoda" rank="genus">Calyptomyrmex</taxonomicName>
