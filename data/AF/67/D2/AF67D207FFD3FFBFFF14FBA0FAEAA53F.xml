</taxonomicName>
, 
<taxonomicName authority="Treadwell, 1921" authorityName="Treadwell" authorityYear="1921" box="[758,1166,1548,1573]" class="Polychaeta" family="Eunicidae" genus="Nicidion" kingdom="Animalia" order="Eunicida" pageId="17" pageNumber="168" phylum="Annelida" rank="species" species="notata">
<emphasis box="[758,941,1548,1572]" italics="true" pageId="17" pageNumber="168">Nicidion notata</emphasis>
(Treadwell, 
<number box="[1098,1157,1548,1572]" pageId="17" pageNumber="168" value="1921.0">1921</number>
)
</taxonomicName>
, and 
<taxonomicName authority="(Hartmann-Schroder, 1965) (Zanol et al. 2014)" authorityName="(Hartmann-Schroder, 1965) (Zanol et al." authorityYear="2014" class="Polychaeta" family="Eunicidae" genus="Nicidion" kingdom="Animalia" order="Eunicida" pageId="17" pageNumber="168" phylum="Annelida" rank="species" species="samoae">
<emphasis box="[1239,1435,1548,1572]" italics="true" pageId="17" pageNumber="168">Nicidion samoae</emphasis>
(Hartmann-Schr√∂der, 
<number box="[395,453,1582,1607]" pageId="17" pageNumber="168" value="1965.0">1965</number>
) (
<bibRefCitation author="Zanol" box="[475,673,1582,1607]" pageId="17" pageNumber="168" refString="Zanol, J., Halanych, K. M. &amp; Fauchald, K. (2014) Reconciling taxonomy and phylogeny in the bristleworm family Eunicidae (Polychaete, Annelida). Zoologica Scripta, 42, 79 - 100. http: // dx. doi. org / 10.1111 / zsc. 12034" type="journal article" year="2014">
Zanol 
<emphasis box="[549,605,1582,1606]" italics="true" pageId="17" pageNumber="168">et al.</emphasis>
