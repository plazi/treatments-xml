</taxonomicName>
, 
<taxonomicName box="[960,1134,1513,1537]" class="Polychaeta" family="Eunicidae" genus="Nicidion" kingdom="Animalia" order="Eunicida" pageId="17" pageNumber="168" phylum="Annelida" rank="species" species="mikeli">
<emphasis box="[960,1134,1513,1537]" italics="true" pageId="17" pageNumber="168">Nicidion mikeli</emphasis>
</taxonomicName>
(Carrera-Parra &amp; Salazar- Vallejo, 
<number box="[252,311,1548,1572]" pageId="17" pageNumber="168" value="1998.0">1998</number>
), 
<taxonomicName authority="Webster 1884" authorityName="Webster" authorityYear="1884" box="[338,739,1548,1573]" class="Polychaeta" family="Eunicidae" genus="Nicidion" kingdom="Animalia" order="Eunicida" pageId="17" pageNumber="168" phylum="Annelida" rank="species" species="mutilata">
<emphasis box="[338,542,1548,1572]" italics="true" pageId="17" pageNumber="168">Nicidion mutilata</emphasis>
(Webster 
<number box="[671,731,1548,1572]" pageId="17" pageNumber="168" value="1884.0">1884</number>
