<taxonomicName baseAuthorityName="Peckham and Peckham" baseAuthorityYear="1892" box="[249,473,1290,1311]" class="Arachnida" family="Salticidae" genus="Myrmarachne" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="2624" phylum="Arthropoda" rank="species" species="imbellis">
