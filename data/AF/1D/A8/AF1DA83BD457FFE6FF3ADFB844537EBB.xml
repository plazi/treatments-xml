of 
<taxonomicName authority="Kraus &amp; Allison, 2009" authorityName="Kraus &amp; Allison" authorityYear="2009" box="[529,692,864,885]" class="Amphibia" family="Microhylidae" genus="Albericus" kingdom="Animalia" order="Anura" pageId="3" pageNumber="56" phylum="Chordata" rank="species" species="murritus" status="sp. n.">
