<emphasis bold="true" box="[1374,1605,4502,4583]" pageId="3" pageNumber="300">Genus</emphasis>
</heading>
<heading bold="true" box="[1639,2167,4510,4580]" fontSize="44" level="8" pageId="3" pageNumber="300" reason="2">
<emphasis bold="true" box="[1639,2167,4510,4580]" pageId="3" pageNumber="300">
<taxonomicName authorityName="Kirby" authorityYear="1889" box="[1639,2152,4510,4580]" class="Insecta" family="Megapodagrionidae" genus="Tatocnemis" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="300" phylum="Arthropoda" rank="genus">Tatocnemis</taxonomicName>
