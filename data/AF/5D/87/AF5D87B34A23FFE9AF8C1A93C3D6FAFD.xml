<typeStatus box="[199,258,260,285]" pageId="44" pageNumber="345">Type</typeStatus>
locality.
</emphasis>
<location LSID="urn:lsid:plazi:treatment:AF5D87B34A23FFE9AF8C1A93C3D6FAFD:222B607E4A24FFE9AE771C17C392FEB5" box="[364,474,260,285]" country="Bonaire, Sint Eustatius and Saba" name="Pos Shiki" pageId="44" pageNumber="345">Pos Shiki</location>
