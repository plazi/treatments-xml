<subSubSection pageId="19" pageNumber="20" type="description">
<paragraph blockId="19.[151,512,888,911]" box="[151,512,888,911]" pageId="19" pageNumber="20">
<taxonomicName box="[151,272,888,911]" class="Insecta" family="Asilidae" genus="Orthogonis" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
