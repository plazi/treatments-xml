, at 
<quantity box="[373,509,601,628]" metricMagnitude="2" metricUnit="m" metricValue="4.8549999999999995" metricValueMax="5.76" metricValueMin="3.95" pageId="27" pageNumber="28" unit="m" value="485.5" valueMax="576.0" valueMin="395.0">
