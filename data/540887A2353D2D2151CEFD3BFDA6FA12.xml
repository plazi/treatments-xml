<collectingCountry id="A4B67624353A2D2150FAFA50FDFFFA13" box="[419,512,1447,1473]" name="China" pageId="13" pageNumber="54">CHINA</collectingCountry>
: 
<collectingRegion id="1E65F856353A2D215353FA51FDAAFA12" box="[522,597,1446,1472]" country="China" name="Fujian" pageId="13" pageNumber="54">Fujian</collectingRegion>
