<taxonomicName id="9C5A9E14B5A60BF81C56635FEFE7A863" class="Insecta" family="Ichneumonidae" genus="Trychosis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trychosis" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Trychosis</taxonomicName>
