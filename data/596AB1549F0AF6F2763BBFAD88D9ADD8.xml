<taxonomicName id="8106200A9459C66FD041E2EF83190115" class="Insecta" family="Ichneumonidae" genus="Cryptus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cryptus" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Cryptus</taxonomicName>
