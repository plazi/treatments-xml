</taxonomicName>
from 
<taxonomicName id="65D387EC352AFFA81694FE37FC8BFE4B" authorityName="Raf." box="[710,832,405,431]" class="Magnoliopsida" family="Apiaceae" genus="Osmorhiza" kingdom="Plantae" order="Apiales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="genus">
<emphasis id="90A7207D352AFFA81694FE37FC8BFE4B" box="[710,832,405,431]" italics="true" pageId="7" pageNumber="8">Osmorhiza</emphasis>
