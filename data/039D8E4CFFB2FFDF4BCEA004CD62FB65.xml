<caption id="DF4B6FD2FFB4FFDE4B20A6FCCB01FC06" ID-DOI="http://doi.org/10.5281/zenodo.5601028" ID-Zenodo-Dep="5601028" httpUri="https://zenodo.org/record/5601028/files/figure.png" pageId="4" pageNumber="5" startId="4.[874,912,904,925]" targetBox="[852,1374,176,880]" targetPageId="4">
<paragraph id="8B8B3F5AFFB4FFDE4B20A6FCCB01FC06" blockId="4.[848,1376,904,952]" pageId="4" pageNumber="5">
