<emphasis id="65EF0EA3E0B87933857D073698D5AEC5" bold="true" pageId="0" pageNumber="1">Figure 10.</emphasis>
<taxonomicName id="87CF27FEF718D58123B3CFE331AE3B0A" authorityName="Kaltenbach &amp; Garces &amp; Gattolliat" authorityYear="2020" class="Insecta" family="Baetidae" genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis camiguinensis" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="camiguinensis">
