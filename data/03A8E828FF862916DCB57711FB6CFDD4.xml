. 
<emphasis id="B975852CFF872916DF6570FDFC41FE7F" box="[839,926,477,500]" italics="true" pageId="31" pageNumber="32">serrana</emphasis>
</taxonomicName>
<emphasis id="B975852CFF872916DF8570FCFBDCFE7F" bold="true" box="[935,1027,476,500]" pageId="31" pageNumber="32">
<taxonomicNameLabel id="A2463857FF872916DF8570FCFBDCFE7F" box="[935,1027,476,500]" pageId="31" pageNumber="32" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C0122BDFF872916D83170FDFB4FFE7F" box="[1043,1168,476,500]" class="Insecta" family="Psychodidae" genus="Psychoda" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="silvensis" status="sp. nov.">
<emphasis id="B975852CFF872916D83170FDFBFEFE7F" box="[1043,1057,477,500]" italics="true" pageId="31" pageNumber="32">P</emphasis>
. 
<emphasis id="B975852CFF872916D81370FCFB4FFE7F" box="[1073,1168,476,500]" italics="true" pageId="31" pageNumber="32">silvensis</emphasis>
</taxonomicName>
<emphasis id="B975852CFF872916D8BB70FCFB2AFE7F" bold="true" box="[1177,1269,476,500]" pageId="31" pageNumber="32">
<taxonomicNameLabel id="A2463857FF872916D8BB70FCFB2AFE7F" box="[1177,1269,476,500]" pageId="31" pageNumber="32" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C0122BDFF872916D90D70FCFE84FD9C" authority="Tonnoir" authorityName="Tonnoir" class="Insecta" family="Psychodidae" genus="Psychoda" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="undulata">
<emphasis id="B975852CFF872916D90D70FCFF24FD93" italics="true" pageId="31" pageNumber="32">Psychoda undulata</emphasis>
Tonnoir
</taxonomicName>
