<taxonomicName id="4C1020E77F1EFF91FCE9FEF73911FE95" baseAuthorityName="Seguenza" baseAuthorityYear="1876" box="[771,1128,260,285]" class="Maxillopoda" family="Scalpellidae" genus="Arcoscalpellum" kingdom="Animalia" order="Pedunculata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="michelottianum">
<emphasis id="B96487767F1EFF91FCE9FEF73911FE95" box="[771,1128,260,285]" italics="true" pageId="41" pageNumber="42">Arcoscalpellum michelottianum</emphasis>
</taxonomicName>
