, width 
<quantity id="4CA19B4D3A12FFD4F964F7BDDC793AEA" box="[1713,1870,2171,2209]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="9" pageNumber="270" unit="mm" value="1.7">1.70 mm</quantity>
, height 
<quantity id="4CA19B4D3A12FFD4FE4CF768D9083A9F" box="[409,575,2222,2260]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" pageId="9" pageNumber="270" unit="mm" value="0.75">0.75 mm</quantity>
. head width 
<quantity id="4CA19B4D3A12FFD4FCE4F768D8E63A9F" box="[817,977,2222,2260]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="9" pageNumber="270" unit="mm" value="1.2">1.20 mm</quantity>
, eye group width 
<quantity id="4CA19B4D3A12FFD4FACAF768DEF33A9F" box="[1311,1476,2222,2260]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="9" pageNumber="270" unit="mm" value="0.9">0.90 mm</quantity>
, abdomen length 2.00 mm, width 
<quantity id="4CA19B4D3A12FFD4FE39F725D9B03B42" box="[492,647,2275,2313]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="9" pageNumber="270" unit="mm" value="1.6">1.60 mm</quantity>
