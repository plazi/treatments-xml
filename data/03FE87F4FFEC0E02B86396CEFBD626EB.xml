<quantity id="4CAF9B07FFEC0E00B89B9319FD342056" box="[496,640,1825,1851]" metricMagnitude="-3" metricUnit="m" metricValue="5.85" metricValueMax="6.4" metricValueMin="5.3" pageId="76" pageNumber="77" unit="mm" value="5.85" valueMax="6.4" valueMin="5.3">5.3–6.4 mm</quantity>
, 
<specimenCount id="9D51FD6BFFEC0E00BBE79319FD142058" box="[652,672,1825,1845]" pageId="76" pageNumber="77" type="female">♀</specimenCount>
<quantity id="4CAF9B07FFEC0E00BBCD9319FC832056" box="[678,823,1825,1851]" metricMagnitude="-3" metricUnit="m" metricValue="7.35" metricValueMax="7.9" metricValueMin="6.8" pageId="76" pageNumber="77" unit="mm" value="7.35" valueMax="7.9" valueMin="6.8">6.8–7.9 mm</quantity>
