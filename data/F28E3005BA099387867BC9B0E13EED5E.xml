<subSubSection id="EEA52E805ADF7139E5BB78610EBBF326" pageId="3" pageNumber="4" type="reference_group">
<paragraph id="0073F5DCA3AB2E4BD6684DFFE5F06337" pageId="3" pageNumber="4">
<taxonomicName id="EAAD87528685ACA2755D2B477AD145B9" class="Insecta" family="Apidae" genus="Xylocopa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xylocopa (Ctenoxylocopa) sulcatipes" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="sulcatipes" subGenus="Ctenoxylocopa">Xylocopa (Ctenoxylocopa) sulcatipes</taxonomicName>
