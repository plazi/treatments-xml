<collectionCode id="ED6AAE71FD1C3711FA99FBB5FAA5FBDD" box="[1273,1337,1024,1048]" httpUri="http://grbio.org/cool/47nz-wbm0" name="National Museum of Natural History, Naturalis" pageId="21" pageNumber="22">RGM</collectionCode>
<number id="3DB32493FD1C3711FB21FC4AFA00FBDD" box="[1345,1436,1023,1048]" pageId="21" pageNumber="22" value="261.097">261.097</number>
(
<typeStatus id="54C08816FD1C3711FEFEFB91FF44FBF9" box="[158,216,1060,1084]" pageId="21" pageNumber="22">Type</typeStatus>
III), 
<collectionCode id="ED6AAE71FD1C3711FF72FB91FECEFBF9" box="[274,338,1060,1084]" httpUri="http://grbio.org/cool/47nz-wbm0" name="National Museum of Natural History, Naturalis" pageId="21" pageNumber="22">RGM</collectionCode>
