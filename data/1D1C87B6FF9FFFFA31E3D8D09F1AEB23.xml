<subSubSection id="DDAF652BFF9FFFFA31BDDD279F1AEB23" box="[201,1077,1122,1147]" pageId="5" pageNumber="600" type="etymology">
<paragraph id="950A36A0FF9FFFFA31BDDD279F1AEB23" blockId="5.[151,1436,510,1147]" box="[201,1077,1122,1147]" pageId="5" pageNumber="600">
<emphasis id="A7C1EAB2FF9FFFFA31BDDD279A60EB22" bold="true" box="[201,335,1123,1147]" pageId="5" pageNumber="600">Etymology.</emphasis>
