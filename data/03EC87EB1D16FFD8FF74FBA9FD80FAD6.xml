<paragraph id="8BFA36FD1D16FFD8FF74FACDFD80FAD6" blockId="35.[178,1085,1293,1403]" pageId="35" pageNumber="942">
<taxonomicName id="4C454D7E1D16FFD8FF74FACDFE23FA8E" authorityName="Spinola" authorityYear="1839" box="[178,384,1294,1315]" class="Insecta" family="Crabronidae" genus="Cerceris" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="942" phylum="Arthropoda" rank="species" species="tricolorata">
