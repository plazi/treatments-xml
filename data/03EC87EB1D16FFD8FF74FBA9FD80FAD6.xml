<collectingCountry id="F352766D1D16FFD8FE4BFAE8FDB4FAEC" box="[397,535,1323,1345]" name="Saudi Arabia" pageId="35" pageNumber="942">Saudi Arabia</collectingCountry>
, 
<collectingCountry id="F352766D1D16FFD8FDE2FAE8FCA9FAEC" box="[548,778,1323,1345]" name="United Arab Emirates" pageId="35" pageNumber="942">United Arab Emirates</collectingCountry>
