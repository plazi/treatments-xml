<number id="3D8024A2FF86FF95FF4EFE13727F7180" box="[207,294,384,410]" pageId="19" pageNumber="20" value="7.5" valueMax="7.8" valueMin="7.2">7.2–7.8</number>
– 
<number id="3D8024A2FF86FF95FEB6FE1372047180" box="[311,349,384,410]" pageId="19" pageNumber="20" value="8.6">8.6</number>
, h/diam. maj. 
<number id="3D8024A2FF86FF95FD82FE1371217180" box="[515,632,384,410]" pageId="19" pageNumber="20" value="1.375" valueMax="1.4" valueMin="1.35">1.35–1.40</number>
– 
<number id="3D8024A2FF86FF95FD08FE1371E47180" box="[649,701,384,410]" pageId="19" pageNumber="20" value="1.49">1.49</number>
, h/aph 
<number id="3D8024A2FF86FF95FC93FE1370DE7180" box="[786,903,384,410]" pageId="19" pageNumber="20" value="1.725" valueMax="1.77" valueMin="1.68">1.68–1.77</number>
– 
