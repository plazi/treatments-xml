and 
<emphasis id="CEBD1E78B05B7640FD6956750A165F0D" box="[760,772,1126,1151]" italics="true" pageId="1" pageNumber="227">c</emphasis>
<subScript id="604DC02FB05B7640FC9456650A1F5FF6" attach="left" box="[773,781,1142,1156]" fontSize="6" pageId="1" pageNumber="227">3</subScript>
