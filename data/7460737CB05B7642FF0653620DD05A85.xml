<emphasis id="CEBD1E78B05B7640FBDA572E0D755E25" box="[1099,1127,1341,1367]" italics="true" pageId="1" pageNumber="227">ad</emphasis>
<subScript id="604DC02FB05B7640FBF6575D0D7D5E2E" attach="left" box="[1127,1135,1358,1372]" fontSize="6" pageId="1" pageNumber="227">2</subScript>
&gt; 
<emphasis id="CEBD1E78B05B7640FBEE572E0D895E25" box="[1151,1179,1341,1367]" italics="true" pageId="1" pageNumber="227">ad</emphasis>
<subScript id="604DC02FB05B7640FB0A575D0DB15E2E" attach="left" box="[1179,1187,1358,1372]" fontSize="6" pageId="1" pageNumber="227">1</subScript>
&gt; 
<emphasis id="CEBD1E78B05B7640FB22572D0DDD5E25" box="[1203,1231,1342,1367]" italics="true" pageId="1" pageNumber="227">an</emphasis>
<subScript id="604DC02FB05B7640FB5E575D0DC55E2E" attach="left" box="[1231,1239,1358,1372]" fontSize="6" pageId="1" pageNumber="227">1</subScript>
= 
<emphasis id="CEBD1E78B05B7640FB76572D0C115E25" box="[1255,1283,1342,1367]" italics="true" pageId="1" pageNumber="227">an</emphasis>
<subScript id="604DC02FB05B7640FA92575D0C195E2E" attach="left" box="[1283,1291,1358,1372]" fontSize="6" pageId="1" pageNumber="227">2</subScript>
&gt; 
<emphasis id="CEBD1E78B05B7640FA8B572E0C245E25" box="[1306,1334,1341,1367]" italics="true" pageId="1" pageNumber="227">ad</emphasis>
<subScript id="604DC02FB05B7640FAA7575D0C2C5E2E" attach="left" box="[1334,1342,1358,1372]" fontSize="6" pageId="1" pageNumber="227">3</subScript>
.
</paragraph>
