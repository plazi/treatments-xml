<paragraph id="F3A90D43A9C1CFBB1D992322820A1910" pageId="26" pageNumber="83">
<taxonomicName id="8DA3D754EAA9C470EFD50EA720C70D8E" class="Insecta" family="Noctuidae" genus="Hadena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hadena marina" order="Lepidoptera" pageId="26" pageNumber="83" phylum="Arthropoda" rank="species" species="marina">Hadena marina</taxonomicName>
