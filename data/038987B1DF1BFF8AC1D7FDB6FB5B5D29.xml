Genus 
<taxonomicName id="4C204D24DF1BFF8AC1ADFDB6FE4B5C5A" authority="Baird, 1845" authorityName="Baird" authorityYear="1845" box="[237,497,546,572]" class="Branchiopoda" family="Bosminidae" genus="Bosmina" kingdom="Animalia" order="Diplostraca" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
