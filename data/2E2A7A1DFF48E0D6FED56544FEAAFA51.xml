<taxonomicName id="6183B088FF49E0D6FD896426FC93FADA" box="[596,858,1284,1309]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="spathae">
