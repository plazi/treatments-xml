<taxonomicName id="978D4D19FFF3FFC2FEAC2D80FDA89C91" authority="Chen, 2020" authorityName="Chen" authorityYear="2020" box="[297,574,1946,1970]" class="Insecta" family="Perlodidae" genus="Sinoperlodes" kingdom="Animalia" order="Plecoptera" pageId="8" pageNumber="592" phylum="Arthropoda" rank="species" species="zhouchangfai" status="gen. et sp. nov.">
<emphasis id="62F9EA88FFF3FFC2FEAC2D80FDA89C91" box="[297,574,1946,1970]" italics="true" pageId="8" pageNumber="592">Sinoperlodes zhouchangfai</emphasis>
</taxonomicName>
<emphasis id="62F9EA88FFF3FFC2FDC02D80FD709C91" bold="true" box="[581,742,1946,1970]" pageId="8" pageNumber="592">
<taxonomicNameLabel id="79CA57F3FFF3FFC2FDC02D80FD709C91" box="[581,742,1946,1970]" pageId="8" pageNumber="592" rank="species">gen. et sp. nov.</taxonomicNameLabel>
