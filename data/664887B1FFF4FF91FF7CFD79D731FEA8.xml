<taxonomicName id="29E14D24FFF4FF92FF7CFD13D1E4FCF8" authority="Duellman and Schulte 1993" authorityName="Duellman and Schulte" authorityYear="1993" box="[151,658,762,784]" class="Amphibia" family="Centrolenidae" genus="Cochranella" kingdom="Animalia" order="Anura" pageId="45" pageNumber="46" phylum="Chordata" rank="species" species="chancas">
<emphasis id="DC95EAB5FFF4FF92FF7CFD13D219FCF8" box="[151,367,762,784]" italics="true" pageId="45" pageNumber="46">Cochranella chancas</emphasis>
<bibRefCitation id="8A704B56FFF4FF92FE9EFD13D1E4FCF8" author="Duellman" box="[373,658,762,784]" pageId="45" pageNumber="46" refString="Duellman, W. E. &amp; Schulte, R. (1993) New species of centrolenid frogs from northern Peru. The University of Kansas Museum of Natural History Occasional Papers, 155, 1 - 33." type="journal article" year="1993">
Duellman and Schulte 
<number id="58292480FFF4FF92FDB7FD13D1E4FCF8" box="[604,658,762,784]" pageId="45" pageNumber="46" value="1993.0">1993</number>
</bibRefCitation>
</taxonomicName>
. 
<typeStatus id="315A8805FFF4FF92FD70FD13D18FFCF8" box="[667,761,762,784]" pageId="45" pageNumber="46">Holotype</typeStatus>
<accessionNumber id="F1B2AB44FFF4FF92FCEBFD12D00FFCF8" box="[768,889,762,784]" httpUri="http://www.uniprot.org/uniprot/KU211778" pageId="45" pageNumber="46">
KU 
<number id="58292480FFF4FF92FCC0FD13D00FFCF8" box="[811,889,762,784]" pageId="45" pageNumber="46" value="211778.0">211778</number>
</accessionNumber>
