<taxonomicName id="4C7D4D4FFFC0D25F0FC374F588E490DF" box="[428,490,1188,1214]" class="Insecta" family="Chironomidae" genus="Litocladius" kingdom="Animalia" order="Diptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="setae">setae</taxonomicName>
