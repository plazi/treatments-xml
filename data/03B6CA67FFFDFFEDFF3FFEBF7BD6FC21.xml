<collectionCode id="ED0EE3B4FFFDFFEDFF6FFE2F7E5FFE39" box="[199,280,441,467]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="10" pageNumber="121" type="Museum">USNM</collectionCode>
