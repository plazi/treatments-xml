<taxonomicName id="33F24D0CFF965A3D6F4584FAFD3E0EA7" box="[584,694,151,176]" class="Insecta" family="Aphalaridae" genus="Vicinilura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="537" phylum="Arthropoda" rank="genus">
