<taxonomicName id="4C1E4D58C529FFB1FAB4F9DDFEEFF97A" authority="Dufay, 1958" authorityName="Dufay" authorityYear="1958" class="Insecta" family="Noctuidae" genus="Abrostola" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="144" phylum="Arthropoda" rank="species" species="korbi">
