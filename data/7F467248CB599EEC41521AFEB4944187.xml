<taxonomicName id="0E200588891A38567460D8305075C832" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium semilaeve" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="semilaeve">Tetramorium semilaeve</taxonomicName>
