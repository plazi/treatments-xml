<taxonomicName id="B868572042A382DD9AFF5BD23F839329" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium semilaeve" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="semilaeve">Tetramorium semilaeve</taxonomicName>
