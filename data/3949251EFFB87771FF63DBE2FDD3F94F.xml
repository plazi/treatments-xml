(
<typeStatus id="6E5B2AAAFFBB7771FDB5D980FD30FB6A" box="[577,681,1196,1222]" pageId="22" pageNumber="23">holotype</typeStatus>
<quantity id="761839EDFFBB7771FD45D980FC89FB6A" box="[689,784,1196,1222]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="22" pageNumber="23" unit="mm" value="2.9">2.9 mm</quantity>
).
</paragraph>
