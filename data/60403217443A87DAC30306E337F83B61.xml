<taxonomicName id="8CDAF5303EB1A6B3D6E3A0D9D22F00AA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:158872" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus (Myrmotrema) foraminosus Forel" order="Hymenoptera" pageNumber="249" phylum="Arthropoda" rank="species" species="foraminosus" subGenus="Myrmotrema">Camponotus (Myrmotrema) foraminosus Forel</taxonomicName>
