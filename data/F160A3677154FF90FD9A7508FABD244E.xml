<paragraph id="797612717154FF90FF4A735AFABD244E" blockId="34.[189,1389,1116,1143]" box="[189,1389,1116,1143]" pageId="34" pageNumber="183">
<taxonomicName id="BEC969F27154FF90FF4A735AFEF0244E" authorityName="Blake &amp; Reid" authorityYear="1998" box="[189,288,1116,1142]" class="Asteroidea" family="Benthopectinidae" genus="Alkaidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Paxillosida" pageId="34" pageNumber="183" phylum="Echinodermata" rank="genus">
