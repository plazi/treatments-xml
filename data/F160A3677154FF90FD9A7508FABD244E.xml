<taxonomicName id="BEC969F27154FF90FDDC74EAFC1B243D" authority="Ewin &amp; Gale, 2020" authorityName="Ewin &amp; Gale" authorityYear="2020" box="[555,971,1003,1030]" class="Asteroidea" family="Benthopectinidae" genus="Alkaidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Paxillosida" pageId="34" pageNumber="183" phylum="Echinodermata" rank="species" species="megaungula">
