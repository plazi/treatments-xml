<taxonomicName id="01F6AC2ACD82F63449AD8837E12A72D1" class="Insecta" family="Mordellidae" genus="Falsomordellistena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Falsomordellistena pubescens" order="Coleoptera" pageId="2" pageNumber="245" phylum="Arthropoda" rank="species" species="pubescens">Falsomordellistena pubescens</taxonomicName>
