<taxonomicName id="4C63D4DE9B42C3676373FA9F4F58FE1C" authority="(Blanchard, 1850)" baseAuthorityName="Blanchard" baseAuthorityYear="1850" box="[271,786,470,497]" class="Insecta" family="Cetoniidae" genus="Oxythyrea" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="166" phylum="Arthropoda" rank="species" species="testaceogutttata">
