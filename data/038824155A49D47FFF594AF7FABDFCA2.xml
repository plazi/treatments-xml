<taxonomicName id="4C21EE805A49D47FFDEE4B68FDE5FEE2" authorityName="Norton" authorityYear="1872" box="[544,620,262,287]" class="Insecta" family="Argidae" genus="Ptenos" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="genus">
