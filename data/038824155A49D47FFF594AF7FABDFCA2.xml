<taxonomicName id="4C21EE805A49D47FFD0E4B6BFCEBFEE2" authorityName="Kirby" authorityYear="1882" box="[704,866,261,287]" class="Insecta" family="Argidae" genus="Hemidianeura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="genus">
