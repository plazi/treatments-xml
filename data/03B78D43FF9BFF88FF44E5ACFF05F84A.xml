<taxonomicName id="4C1E47D6FF9BFF88FD6FE652FD9FFDF5" authority="Karny" authorityName="Karny" authorityYear="1925" box="[700,1017,546,568]" class="Insecta" family="Thripidae" genus="Physothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="51" phylum="Arthropoda" rank="species" species="hargreavesi">
