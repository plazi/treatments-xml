<quantity id="2A271FE1991FFF98EA269E99DC3DFA1E" box="[827,860,1430,1459]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="44" pageNumber="45" unit="in" value="1.0">1 in</quantity>
