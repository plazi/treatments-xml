<number id="3D818C7EB740FF95FF3CC1B8F806F84B" box="[254,282,1973,1998]" pageId="6" pageNumber="396" value="16.0">16</number>
(n= 
<number id="3D818C7EB740FF95FE8FC1BBF846F84B" box="[333,346,1974,1998]" pageId="6" pageNumber="396" value="3.0">3</number>
