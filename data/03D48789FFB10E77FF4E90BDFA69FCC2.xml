<materialsCitation id="3B153CC2FFB20E73FE8097BDFD14FECA" ID-GBIF-Occurrence="2234218678" collectionCode="CCHH" pageId="2" pageNumber="567" specimenCount="1" typeStatus="holotype">
<typeStatus id="54C6883DFFB20E73FE8097BDFE33FF67" box="[345,454,151,176]" pageId="2" pageNumber="567">Holotype</typeStatus>
