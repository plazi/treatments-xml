<taxonomicName id="4C7D4D1CFFB20E73FAED97CAFA69FF2F" authorityName="" authorityYear="1829" box="[1332,1436,224,248]" class="Magnoliopsida" family="Fagaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="2" pageNumber="567" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
