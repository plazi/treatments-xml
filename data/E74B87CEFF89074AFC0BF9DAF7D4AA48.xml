<materialsCitation id="DF8A3C85FF8A074AFF0DFF55F5D4A8F6" collectionCode="NGMJ" pageId="2" pageNumber="321" specimenCode="NGMJ 004546" specimenCount="1" typeStatus="holotype">
<emphasis id="5D96EACAFF8A074AFF0DFF55F4AEA80F" box="[142,248,242,263]" italics="true" pageId="2" pageNumber="321">
