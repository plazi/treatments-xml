<taxonomicName id="41EC4597FF85B259FC53FB60FBEEFA8D" box="[956,1103,1269,1288]" class="Insecta" family="Trichoceridae" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="723" phylum="Arthropoda" rank="family">Trichoceridae</taxonomicName>
