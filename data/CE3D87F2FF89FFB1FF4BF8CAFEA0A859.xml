<collectingCountry id="3E837674FF88FFB1FEBBFAEFFE2FA811" box="[359,474,1303,1328]" name="Cambodia" pageId="7" pageNumber="111">Cambodia</collectingCountry>
(
<collectingRegion id="8450F806FF88FFB1FE35FAE0FDBDA811" box="[489,584,1304,1328]" country="Cambodia" name="Kampot" pageId="7" pageNumber="111">Kampot</collectingRegion>
, Kohkong, 
