<taxonomicName id="EE824D33F45DBA2CFE04FBF8271FFBCC" box="[399,540,1121,1144]" class="Gastropoda" family="Orthalicidae" genus="Plekocheilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="2" pageNumber="53" phylum="Mollusca" rank="genus">
