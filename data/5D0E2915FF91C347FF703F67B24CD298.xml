<subSubSection id="9DBDCB88FF91C347FF203C36B24CD298" pageId="45" pageNumber="46" type="etymology">
<paragraph id="D5189803FF91C347FF203C36B7D0D240" blockId="45.[151,1437,1124,1943]" box="[199,1227,1844,1871]" pageId="45" pageNumber="46">
<emphasis id="E7D34411FF91C347FF203C36B255D240" bold="true" box="[199,334,1845,1871]" pageId="45" pageNumber="46">Etymology.</emphasis>
