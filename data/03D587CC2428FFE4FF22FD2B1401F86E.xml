<taxonomicName id="4C7C4D592428FFE4FDCCF917150FF8F6" authority="Keifer" authorityName="Keifer" box="[633,1058,1786,1812]" class="Arachnida" family="Eriophyidae" genus="Shevtchenkella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="48" phylum="Arthropoda" rank="species" species="philadelphi">
