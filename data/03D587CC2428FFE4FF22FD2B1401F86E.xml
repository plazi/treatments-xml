<taxonomicName id="4C7C4D592428FFE4FEBAF89F109CF86E" box="[271,433,1906,1932]" class="Arachnida" family="Eriophyidae" genus="Shevtchenkella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="48" phylum="Arthropoda" rank="species" species="philadelphi">
<emphasis id="B908EAC82428FFE4FEBAF89F1033F86E" box="[271,286,1907,1932]" italics="true" pageId="6" pageNumber="48">S</emphasis>
. 
<emphasis id="B908EAC82428FFE4FE99F89F109CF86E" box="[300,433,1907,1932]" italics="true" pageId="6" pageNumber="48">philadelphi</emphasis>
