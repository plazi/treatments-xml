<taxonomicName id="4C7C4D592428FFE4FE94F8A710EEF886" box="[289,451,1866,1892]" class="Arachnida" family="Eriophyidae" genus="Shevtchenkella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="48" phylum="Arthropoda" rank="species" species="philadelphi">
<emphasis id="B908EAC82428FFE4FE94F8A7101DF886" box="[289,304,1867,1892]" italics="true" pageId="6" pageNumber="48">S</emphasis>
. 
<emphasis id="B908EAC82428FFE4FE8BF8A710EEF886" box="[318,451,1867,1892]" italics="true" pageId="6" pageNumber="48">philadelphi</emphasis>
</taxonomicName>
