<emphasis id="379AEB7EFFFDBD4BFD133B331377D5CD" box="[704,839,1397,1422]" italics="true" pageId="24" pageNumber="25">
<taxonomicName id="C2EE4CEFFFFDBD4BFD133B331373D5CD" baseAuthorityName="Kulijev" baseAuthorityYear="1979" box="[704,835,1397,1422]" class="Arachnida" family="Gymnodamaeidae" genus="Gymnodamaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="frondeus">G. frondeus</taxonomicName>
