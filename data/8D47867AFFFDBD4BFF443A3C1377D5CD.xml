<taxonomicName id="C2EE4CEFFFFDBD4BFC643B141566D529" authority="(Perez-Inigo, 1972)" authorityName="Perez-Inigo" authorityYear="1972" baseAuthorityName="Perez-Inigo" baseAuthorityYear="1972" box="[951,1366,1361,1386]" class="Arachnida" family="Gymnodamaeidae" genus="Jacotella" kingdom="Animalia" order="Sarcoptiformes" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="ornata">
