<taxonomicName id="BAF69D975F41D3CA72E6B3C1B802620E" authorityName="Leger &amp; Nuss" authorityYear="2020" class="Insecta" family="Crambidae" genus="Hoploscopa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hoploscopa isarogensis" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="isarogensis">
<emphasis id="206B35842D336AAD86FCD7E411575720" italics="true" pageId="0" pageNumber="1">Hoploscopa isarogensis</emphasis>
</taxonomicName>
sp. nov. (q.v.), 
