4. 
<taxonomicName id="078E4D5DFFDD4B6DF6E587A3FB13E4CC" box="[1022,1253,1174,1198]" class="Cestoda" family="Hymenolepididae" genus="Microsomacanthus" kingdom="Animalia" order="Cyclophyllidea" pageId="7" pageNumber="262" phylum="Platyhelminthes" rank="species" species="undetermined">
