<subSubSection id="F482D3F7FFABFF9DFF50C728FB8EFEF9" box="[151,1142,263,287]" pageId="26" pageNumber="27" type="materials_examined">
<paragraph id="BC27807CFFABFF9DFF50C728FB8EFEF9" blockId="26.[151,1142,263,287]" box="[151,1142,263,287]" pageId="26" pageNumber="27">
