, females 
<quantity id="4CA0F7C2FFACFFD1FDE2F943FD67F92D" box="[619,758,1719,1746]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="2.9" metricValueMin="2.6" pageId="1" pageNumber="149" unit="mm" value="2.75" valueMax="2.9" valueMin="2.6">2.6−2.9 mm</quantity>
