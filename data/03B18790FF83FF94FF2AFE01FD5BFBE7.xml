<taxonomicName id="4C184D05FF83FF94FCF3FD06FC1DFD62" box="[846,909,731,752]" class="Insecta" family="Culicidae" genus="Lutzia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="genus">
