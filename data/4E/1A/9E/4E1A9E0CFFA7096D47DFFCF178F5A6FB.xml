<taxonomicName box="[368,598,1920,1945]" class="Insecta" family="Scelionidae" genus="Archaeoteleia" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="novaezealandiae">
<emphasis box="[368,598,1920,1945]" italics="true" pageId="11" pageNumber="12">A. novaezealandiae</emphasis>
</taxonomicName>
and 
<taxonomicName box="[654,790,1920,1945]" class="Insecta" family="Scelionidae" genus="Archaeoteleia" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="onamata" status="n.sp.">
<emphasis box="[654,790,1920,1945]" italics="true" pageId="11" pageNumber="12">A. onamata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[797,852,1920,1946]" pageId="11" pageNumber="12" rank="species">n.sp.</taxonomicNameLabel>
</paragraph>
