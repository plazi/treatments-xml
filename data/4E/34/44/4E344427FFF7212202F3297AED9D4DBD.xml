</taxonomicName>
(
<taxonomicName box="[1245,1381,223,248]" class="Anthozoa" kingdom="Animalia" order="Scleractinia" pageId="4" pageNumber="460" phylum="Cnidaria" rank="order">Scleractinia</taxonomicName>
) and 
<taxonomicName box="[151,346,260,285]" class="Anthozoa" family="Actiniidae" genus="Dofleinia" kingdom="Animalia" order="Actiniaria" pageId="4" pageNumber="460" phylum="Cnidaria" rank="species" species="armata">
<emphasis box="[151,346,260,285]" italics="true" pageId="4" pageNumber="460">Dofleinia armata</emphasis>
</taxonomicName>
(
<taxonomicName box="[361,477,260,285]" class="Anthozoa" kingdom="Animalia" order="Actiniaria" pageId="4" pageNumber="460" phylum="Cnidaria" rank="order">Actiniaria</taxonomicName>
) (
<bibRefCitation author="Bruce" box="[500,644,260,284]" pageId="4" pageNumber="460" refString="Bruce A. J. (1979) Notes on some Indo-Pacific Pontoniinae, XXXI. Periclimenes magnificus sp. nov., a coelenterate associate from the Capricorn Islands (Decapoda, Palaemonidae). Crustaceana, 5 (Supplement), 195 - 208, pl. 1." type="journal article" year="1979">
Bruce, 
<number box="[584,644,260,284]" pageId="4" pageNumber="460" value="1979.0">1979</number>
</bibRefCitation>
). Also associated with hydroid 
<taxonomicName authority="Octocorallia Lobophyton" authorityName="Octocorallia Lobophyton" class="Hydrozoa" family="Aglaopheniidae" genus="Aglaophenia" kingdom="Animalia" order="Leptothecata" pageId="4" pageNumber="460" phylum="Cnidaria" rank="species" species="whiteleggei">
<emphasis box="[1002,1282,260,285]" italics="true" pageId="4" pageNumber="460">Aglaophenia whiteleggei</emphasis>
, Octocorallia 
<emphasis box="[151,285,295,320]" italics="true" pageId="4" pageNumber="460">Lobophyton</emphasis>
</taxonomicName>
sp., 
