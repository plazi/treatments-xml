<taxonomicName authority="(Linnaeus) Britton" authorityName="Britton" baseAuthorityName="Linnaeus" box="[272,763,1163,1187]" class="Pinopsida" family="Cupressaceae" genus="Chamaecyparis" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="thyoides">
