Dimensions 
<quantity box="[471,614,1226,1250]" metricMagnitude="-2" metricUnit="m" metricValue="8.425" metricValueMax="9.15" metricValueMin="7.7" pageId="2" pageNumber="385" unit="mm" value="84.25" valueMax="91.5" valueMin="77.0">77–91.5 mm</quantity>
by 
<quantity box="[657,793,1226,1250]" metricMagnitude="-3" metricUnit="m" metricValue="3.95" metricValueMax="4.4" metricValueMin="3.5" pageId="2" pageNumber="385" unit="mm" value="3.95" valueMax="4.4" valueMin="3.5">3.5–4.4 mm</quantity>
at clitellum, segments 
<number box="[1056,1157,1226,1250]" pageId="2" pageNumber="385" value="122.5" valueMax="138.0" valueMin="107.0">107–138</number>
. Setae 
<number box="[1237,1309,1226,1250]" pageId="2" pageNumber="385" value="48.0" valueMax="56.0" valueMin="40.0">
40– 
<date box="[1281,1309,1226,1250]" bridgedPair="/" pageId="2" pageNumber="385" value="1956-03">56</date>
</number>
/III, 
<number box="[1358,1427,1226,1250]" pageId="2" pageNumber="385" value="51.0" valueMax="56.0" valueMin="46.0">
46– 
<date box="[1400,1427,1226,1250]" bridgedPair="/" pageId="2" pageNumber="385" value="1956-05">56</date>
</number>
/ V, 
<number box="[192,263,1262,1286]" pageId="2" pageNumber="385" value="54.0" valueMax="60.0" valueMin="48.0">
48– 
<date box="[235,263,1262,1286]" bridgedPair="/" pageId="2" pageNumber="385" value="1960-08">60</date>
</number>
/VIII, 
<number box="[343,414,1262,1286]" pageId="2" pageNumber="385" value="44.0" valueMax="48.0" valueMin="40.0">40–48</number>
/XX, 
<number box="[486,556,1262,1286]" pageId="2" pageNumber="385" value="43.0" valueMax="44.0" valueMin="42.0">42–44</number>
/XXV; 
<number box="[647,717,1262,1286]" pageId="2" pageNumber="385" value="11.5" valueMax="13.0" valueMin="10.0">10–13</number>
between male pores; 
<number box="[1004,1075,1262,1286]" pageId="2" pageNumber="385" value="31.0" valueMax="32.0" valueMin="30.0">30–32</number>
between spermathecal pores. Spermathecal pores in 
<number box="[414,427,1298,1322]" pageId="2" pageNumber="385" value="5.0">5</number>
/ 
<number box="[436,450,1298,1322]" pageId="2" pageNumber="385" value="6.0">6</number>
– 
<number box="[465,479,1298,1322]" pageId="2" pageNumber="385" value="6.0">6</number>
/ 
<number box="[488,501,1298,1322]" pageId="2" pageNumber="385" value="7.0">7</number>
, less than 
<number box="[625,661,1298,1322]" pageId="2" pageNumber="385" value="0.5">0.5</number>
