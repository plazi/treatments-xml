Dimensions 
<quantity box="[470,563,2010,2034]" metricMagnitude="-1" metricUnit="m" metricValue="1.63" pageId="4" pageNumber="387" unit="mm" value="163.0">163 mm</quantity>
by 
<quantity box="[606,692,2010,2034]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="4" pageNumber="387" unit="mm" value="5.5">5.5 mm</quantity>
at clitellum, segments 
