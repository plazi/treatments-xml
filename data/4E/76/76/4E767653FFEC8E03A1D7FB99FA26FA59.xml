<number box="[1211,1240,2010,2034]" pageId="4" pageNumber="387" value="40.0">40</number>
/VIII
</date>
, 
<number box="[1309,1337,2010,2034]" pageId="4" pageNumber="387" value="56.0">56</number>
/XX, 
<number box="[1400,1428,2010,2034]" pageId="4" pageNumber="387" value="40.0">40</number>
/ XXV; 
<number box="[227,255,151,175]" pageId="5" pageNumber="388" value="13.0">13</number>
