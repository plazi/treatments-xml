<taxonomicName box="[1359,1436,339,360]" class="Insecta" family="Leiodidae" genus="Adelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">
