<collectionCode box="[1002,1088,225,251]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="23" pageNumber="24">MNHN</collectionCode>
