(= 
<taxonomicName authority="Linnaeus [1758]" authorityName="Linnaeus" authorityYear="1758" class="Insecta" family="Curculionidae" genus="Silpha" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="seminulum">
