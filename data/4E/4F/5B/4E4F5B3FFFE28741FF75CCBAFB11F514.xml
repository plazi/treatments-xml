[in 
<collectionCode box="[1082,1170,1701,1727]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="25" pageNumber="26">MNHN</collectionCode>
