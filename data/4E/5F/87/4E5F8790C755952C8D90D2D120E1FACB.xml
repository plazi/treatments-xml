<taxonomicName authorityName="Mulsant" authorityYear="1850" box="[1236,1404,1289,1313]" class="Insecta" family="Coccinellidae" genus="Cryptognatha" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="10" phylum="Arthropoda" rank="genus">
