<paragraph pageId="0" pageNumber="57">Type species:</paragraph>
<paragraph pageId="0" pageNumber="57">
<taxonomicName authority="Stretch, 1872" authorityName="Stretch" authorityYear="1872" class="Insecta" family="Erebidae" genus="Seiarctia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Seiarctia lewisii" order="Lepidoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="species" species="lewisii">Seiarctia lewisii</taxonomicName>
