<taxonomicName authorityName="Ueno" authorityYear="1952" class="Insecta" family="Carabidae" genus="Jujiroa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Jujiroa" order="Coleoptera" pageId="0" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="33">Jujiroa</emphasis>
</taxonomicName>
<normalizedToken originalValue="Uéno">Ueno</normalizedToken>
