<taxonomicName authorityName="Rondani" authorityYear="1856" box="[736,882,1342,1364]" class="Insecta" family="Drosophilidae" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="1459" phylum="Arthropoda" rank="family">Drosophilidae</taxonomicName>
, 
<taxonomicName box="[900,1001,1342,1364]" class="Insecta" family="Muscidae" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="1459" phylum="Arthropoda" rank="family">Muscidae</taxonomicName>
, 
<taxonomicName class="Insecta" family="Sarcophagidae" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="1459" phylum="Arthropoda" rank="family">Sarcophagidae</taxonomicName>
