1*. 
<normalizedToken originalValue="Spreublätter" pageId="null" pageNumber="550">Spreublaetter</normalizedToken>
fast 
<normalizedToken originalValue="herzförmig" pageId="null" pageNumber="550">herzfoermig</normalizedToken>
