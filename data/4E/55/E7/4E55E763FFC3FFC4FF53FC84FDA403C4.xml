<quantity box="[857,986,1498,1524]" metricMagnitude="-2" metricUnit="m" metricValue="1.75" metricValueMax="1.8" metricValueMin="1.7" pageId="21" pageNumber="22" unit="mm" value="17.5" valueMax="18.0" valueMin="17.0">17–18 mm</quantity>
; forewing length 
