<subSubSection pageId="0" pageNumber="30" type="materials_examined">
<paragraph blockId="0.[136,778,2068,2092]" lastBlockId="0.[810,1452,741,2092]" pageId="0" pageNumber="30">
<emphasis bold="true" box="[136,247,2068,2092]" pageId="0" pageNumber="30">
<typeStatus box="[136,242,2068,2092]" pageId="0" pageNumber="30">Holotype</typeStatus>
.
</emphasis>
<typeStatus box="[264,363,2068,2092]" pageId="0" pageNumber="30">Holotype</typeStatus>
male in a 20×24× 
<quantity box="[602,657,2068,2092]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="0" pageNumber="30" unit="mm" value="5.0">5mm</quantity>
