, 
<collectingCountry box="[1085,1152,949,974]" name="China" pageId="35" pageNumber="2797">China</collectingCountry>
, 
<collectingCountry box="[160,218,983,1008]" name="India" pageId="35" pageNumber="2797">India</collectingCountry>
, 
<collectingCountry box="[232,343,983,1008]" name="Indonesia" pageId="35" pageNumber="2797">Indonesia</collectingCountry>
, 
<collectingCountry box="[356,424,983,1008]" name="Japan" pageId="35" pageNumber="2797">Japan</collectingCountry>
, 
<collectingCountry box="[438,536,983,1008]" name="Malaysia" pageId="35" pageNumber="2797">Malaysia</collectingCountry>
, 
<collectingCountry box="[550,618,983,1008]" name="Nepal" pageId="35" pageNumber="2797">Nepal</collectingCountry>
, 
<collectingCountry box="[632,758,983,1008]" name="Philippines" pageId="35" pageNumber="2797">Philippines</collectingCountry>
, 
<collectingCountry box="[771,888,983,1008]" name="Singapore" pageId="35" pageNumber="2797">Singapore</collectingCountry>
, 
<collectingCountry box="[901,1008,983,1008]" name="Sri Lanka" pageId="35" pageNumber="2797">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[1022,1102,983,1008]" name="Taiwan" pageId="35" pageNumber="2797">Taiwan</collectingCountry>
and 
<collectingCountry box="[160,256,1018,1043]" name="Vietnam" pageId="35" pageNumber="2797">Vietnam</collectingCountry>
