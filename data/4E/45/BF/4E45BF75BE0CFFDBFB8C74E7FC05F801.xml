<number box="[1238,1269,908,932]" pageId="4" pageNumber="267" value="27.0">27</number>
° 
<number box="[1289,1354,908,933]" pageId="4" pageNumber="267" value="28.9">28.90</number>
′N
</geoCoordinate>
, 
<geoCoordinate direction="west" orientation="longitude" pageId="4" pageNumber="267" precision="9" value="-80.22867">
<number box="[1397,1427,908,932]" pageId="4" pageNumber="267" value="80.0">80</number>
° 
<number box="[151,217,943,968]" pageId="4" pageNumber="267" value="13.72">13.72</number>
′W
</geoCoordinate>
). Depth of 
<quantity box="[375,504,943,968]" metricMagnitude="1" metricUnit="m" metricValue="1.03" metricValueMax="1.15" metricValueMin="0.9099999999999999" pageId="4" pageNumber="267" unit="m" value="10.3" valueMax="11.5" valueMin="9.1">9.1–11.5 m</quantity>
