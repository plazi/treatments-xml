, 
<collectingCountry id="76DF763A5F2AFFF8FB52FB8DFB2E34FE" box="[1165,1236,1138,1164]" name="Brazil" pageId="10" pageNumber="545">Brazil</collectingCountry>
, 
<collectingCountry id="76DF763A5F2AFFF8FB3FFB8DFAAB34FE" box="[1248,1361,1138,1164]" name="Colombia" pageId="10" pageNumber="545">Colombia</collectingCountry>
, 
<collectingCountry id="76DF763A5F2AFFF8FA83FB8DFF3734C2" name="Costa Rica" pageId="10" pageNumber="545">Costa Rica</collectingCountry>
, 
<collectingCountry id="76DF763A5F2AFFF8FF05FB69FEB034C2" box="[218,330,1174,1200]" name="Honduras" pageId="10" pageNumber="545">Honduras</collectingCountry>
, 
<collectingCountry id="76DF763A5F2AFFF8FE88FB69FE3734C2" box="[343,461,1174,1200]" name="Nicaragua" pageId="10" pageNumber="545">Nicaragua</collectingCountry>
, 
<collectingCountry id="76DF763A5F2AFFF8FE06FB69FDF534C2" box="[473,527,1174,1200]" name="Peru" pageId="10" pageNumber="545">Peru</collectingCountry>
, and 
<collectingCountry id="76DF763A5F2AFFF8FD92FB69FD3A34C2" box="[589,704,1174,1200]" name="Venezuela" pageId="10" pageNumber="545">Venezuela</collectingCountry>
(
<bibRefCitation id="6A594B5B5F2AFFF8FD0FFB69FC8134C2" author="Metcalf, Z. P." box="[720,891,1174,1200]" pageId="10" pageNumber="545" pagination="1 - 492" refId="ref26749" refString="Metcalf, Z. P. (1963 b) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i-vi + 1 - 492." type="journal article" year="1963">Metcalf 1963b</bibRefCitation>
; 
<bibRefCitation id="6A594B5B5F2AFFF8FC57FB69FBB134C2" author="Maes, J. - M. &amp; Moulds, M. &amp; Sanborn, A. F." box="[904,1099,1174,1200]" pageId="10" pageNumber="545" pagination="1 - 138" refId="ref26457" refString="Maes, J. - M., Moulds, M. &amp; Sanborn, A. F. (2012) Cicadidae (Homoptera) de Nicaragua: catalogo ilustrado, incluyendo especies exoticas del Museo Entomologico de Leon. Revista Nicaraguense Entomologia, 72 (Suplemento 2), 1 - 138." type="journal article" year="2012">
Maes 
<emphasis id="3CBCEAB85F2AFFF8FC12FB68FBFD34C2" box="[973,1031,1174,1200]" italics="true" pageId="10" pageNumber="545">et al.</emphasis>
2012
</bibRefCitation>
; Sanborn 2013; 2014; 2018b; 2019a; 2020a, b, c, d; 
<bibRefCitation id="6A594B5B5F2AFFF8FE51FB45FD7434A6" author="Sanborn, A. F. &amp; Maes, J. - M." box="[398,654,1210,1236]" pageId="10" pageNumber="545" pagination="437 - 442" refId="ref29061" refString="Sanborn, A. F. &amp; Maes, J. - M. (2012) Checklist of the cicadas (Insecta: Hemiptera: Cicadidae) of Nicaragua including new records for seventeen species. Check List, 8, 437 - 442. https: // doi. org / 10.15560 / 8.3.437" type="journal article" year="2012">Sanborn &amp; Maes 2012</bibRefCitation>
; 
<bibRefCitation id="6A594B5B5F2AFFF8FD46FB45FC5A34A6" author="Sanborn, A. F. &amp; Heath, M. S." box="[665,928,1210,1236]" pageId="10" pageNumber="545" pagination="1 - 94" refId="ref28842" refString="Sanborn, A. F. &amp; Heath, M. S. (2014) The cicadas of Argentina with new records, a new genus and fifteen new species (Hemiptera: Cicadoidea: Cicadidae). Zootaxa, 3883 (1), 1 - 94. https: // doi. org / 10.11646 / zootaxa. 3883.1.1" type="journal article" year="2014">Sanborn &amp; Heath 2014</bibRefCitation>
). The range was recently expanded to include 
<collectingCountry id="76DF763A5F2AFFF8FF48FB21FF0E348A" box="[151,244,1246,1272]" name="Ecuador" pageId="10" pageNumber="545">Ecuador</collectingCountry>
