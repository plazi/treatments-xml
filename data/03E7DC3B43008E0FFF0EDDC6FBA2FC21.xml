<taxonomicName id="4C4E16AE43008E0FFE31DFA8FD16FC2E" authority="(Osborn &amp; Ball)" baseAuthorityName="Osborn &amp; Ball" box="[435,736,941,968]" class="Insecta" family="Cicadellidae" genus="Pendarus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="26" phylum="Arthropoda" rank="species" species="magnus">
