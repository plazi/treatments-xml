<paragraph id="321238A3F1907180238D51EA80DAD382" pageId="0" pageNumber="1">
<taxonomicName id="53EFE5B77BD9E9F0A8FD7FAC912DDB59" baseAuthorityName="Swainson" baseAuthorityYear="1822" class="Gastropoda" family="Ampullariidae" genus="Pila" kingdom="Animalia" lsidName="Pila globosa" order="Architaenioglossa" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="globosa">
<emphasis id="6D892AC6A0ABB330C08EF0E6D5772380" italics="true" pageId="0" pageNumber="1">Pila globosa</emphasis>
