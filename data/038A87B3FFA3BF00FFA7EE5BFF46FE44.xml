(
<collectionCode id="ED32AE60FFA3BF01FD74E9EFFD0BFD6E" box="[684,767,696,719]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="4" pageNumber="729">MNHN</collectionCode>
