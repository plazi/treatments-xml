<paragraph id="7D050365E0398E88A3F44EC778D12222" pageId="9" pageNumber="10">
<taxonomicName id="42B2661F61F4870E158617A21EC3DF12" class="Arachnida" family="Linyphiidae" genus="Capsulia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Capsulia tianmushana" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="tianmushana">Capsulia tianmushana</taxonomicName>
