<taxonomicName id="B5E14D6F245CBD05FBB2D9FE1FA5A4D2" authorityName="Moore" authorityYear="1980" box="[1042,1178,1980,2003]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="66" pageNumber="67" phylum="Tracheophyta" rank="species" species="epetiolata">
<emphasis id="4095EAFE245CBD05FBB2D9FE1F1DA4D2" box="[1042,1058,1980,2003]" italics="true" pageId="66" pageNumber="67">G</emphasis>
. 
<emphasis id="4095EAFE245CBD05FB8FD9FE1FA5A4D2" box="[1071,1178,1980,2003]" italics="true" pageId="66" pageNumber="67">epetiolata</emphasis>
