, 
<taxonomicName id="B5E14D6F245CBD05FD5ED9FE1862A4D2" authorityName="Moore" authorityYear="1980" box="[766,861,1980,2003]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="66" pageNumber="67" phylum="Tracheophyta" rank="species" species="divisa">
<emphasis id="4095EAFE245CBD05FD5ED9FE1831A4D2" box="[766,782,1980,2003]" italics="true" pageId="66" pageNumber="67">G</emphasis>
