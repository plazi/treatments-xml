</emphasis>
<taxonomicName id="4C614D70FFEEFF97FEBCA308FCC9FBB6" authority="Angelier, 1954" authorityName="Angelier" authorityYear="1954" box="[313,801,1047,1072]" class="Arachnida" family="Halacaridae" genus="Acarochelopodia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="5" pageNumber="206" phylum="Arthropoda" rank="species" species="delamarei">
