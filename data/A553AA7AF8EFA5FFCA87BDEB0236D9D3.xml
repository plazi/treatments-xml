<taxonomicName id="AA5DFB88A38397CBAEB397ADAC5DDE9E" class="Magnoliopsida" family="Meliaceae" genus="Xylocarpus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Xylocarpus moluccensis" order="Sapindales" pageId="0" pageNumber="93" phylum="Tracheophyta" rank="species" species="moluccensis">Xylocarpus moluccensis</taxonomicName>
