<taxonomicName id="4C354D47CA5FFFC2FD3BA9327E90835C" box="[643,906,593,618]" class="Insecta" family="Rhynchitidae" genus="Hemilypus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="obliteratus">
