<taxonomicName id="6F0F5895DEF5A29FF58F701688113F28" authority="Loew, 1862" authorityName="Loew" authorityYear="1862" class="Insecta" family="Canacidae" genus="Rhicnoessa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhicnoessa milichioides" order="Diptera" pageId="19" pageNumber="78" phylum="Arthropoda" rank="species" species="milichioides">Rhicnoessa milichioides</taxonomicName>
