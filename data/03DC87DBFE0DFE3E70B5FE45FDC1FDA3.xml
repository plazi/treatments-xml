<taxonomicName id="4C754D4EFE0DFE3E7457F8FCFAF1FD1B" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[1141,1360,1893,1917]" class="Magnoliopsida" family="Lamiaceae" genus="Prunella" kingdom="Plantae" order="Lamiales" pageId="385" pageNumber="386" phylum="Tracheophyta" rank="species" species="modularis">
