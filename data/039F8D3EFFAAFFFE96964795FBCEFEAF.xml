<quantity id="4CCE91CDFFAAFFF1974B4648FE23FEA8" box="[330,421,330,355]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="23" pageNumber="324" unit="mm" value="2.6">2.6 mm</quantity>
