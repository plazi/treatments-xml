<heading id="D0F6EE52FF802911DCB575EDFDE9FB6C" bold="true" box="[151,566,1229,1255]" fontSize="11" level="1" pageId="24" pageNumber="25" reason="1">
<emphasis id="B975852CFF802911DCB575EDFDE9FB6C" bold="true" box="[151,566,1229,1255]" pageId="24" pageNumber="25">
<taxonomicName id="4C0122BDFF802911DCB575EDFE15FB6C" box="[151,458,1229,1255]" class="Insecta" family="Psychodidae" genus="Psychoda" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="matogrossensis" status="sp. nov.">
<emphasis id="B975852CFF802911DCB575EDFE15FB6C" bold="true" box="[151,458,1229,1255]" italics="true" pageId="24" pageNumber="25">Psychoda matogrossensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A2463857FF802911DDFA75EDFDE9FB6C" box="[472,566,1229,1255]" pageId="24" pageNumber="25" rank="species">sp. nov.</taxonomicNameLabel>
