<taxonomicName id="3ED7E985FFAEFFEBFEF79F191F70C46F" authority="Walker, 1862" authorityName="Walker" authorityYear="1862" box="[294,619,1123,1148]" class="Insecta" family="Arctiidae" genus="Lithosia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="40" phylum="Arthropoda" rank="species" species="nebulosa">
