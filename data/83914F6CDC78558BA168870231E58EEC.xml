a running individual of 
<taxonomicName id="DFA3D3D66CDCBF05CBA527A432F326F7" genus="Z." lsidName="Z. jingliae" pageId="0" pageNumber="175" rank="species" species="jingliae">
<emphasis id="7BF3EC1C04B4D2117CBC15EAA4F0A3A2" italics="true" pageId="0" pageNumber="175">Z. jingliae</emphasis>
</taxonomicName>
<emphasis id="07BDEE9700BC4A76767B52EE4C4E732A" bold="true" pageId="0" pageNumber="175">F</emphasis>
a 
<taxonomicName id="698545936C420B19F4FA57F3EBAE2746" authorityName="Adelung" authorityYear="1902" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" kingdom="Animalia" lsidName="Tachycines" order="Orthoptera" pageId="0" pageNumber="175" phylum="Arthropoda" rank="genus">
