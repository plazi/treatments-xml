<location id="865760118E166F36FD8CEB55110B6AB3" LSID="urn:lsid:plazi:treatment:0B2187DC8E166F36FF6DEAE410BD6ADF:865760118E166F36FD8CEB55110B6AB3" box="[630,800,889,914]" country="China" name="West Malaysia" pageId="10" pageNumber="551">West Malaysia</location>
