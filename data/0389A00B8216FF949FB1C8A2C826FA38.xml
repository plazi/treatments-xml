., 
<taxonomicName id="4C206A9E8211FF949E7BCC6DC900FBC0" authority="Vilkamaa" authorityName="Vilkamaa" box="[349,589,1112,1138]" class="Insecta" family="Sciaridae" genus="Keilbachia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="scutica">
<emphasis id="B954CD0F8211FF949E7BCC6DCA99FBC3" box="[349,468,1112,1137]" italics="true" pageId="4" pageNumber="61">K. scutica</emphasis>
