., 
<taxonomicName id="4C206A9E8211FF949B12CC05CE75FBF8" authority="Mohrig" authorityName="Mohrig" box="[1076,1336,1072,1098]" class="Insecta" family="Sciaridae" genus="Keilbachia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="flagrispina">
<emphasis id="B954CD0F8211FF949B12CC05CF9BFBFB" box="[1076,1238,1072,1097]" italics="true" pageId="4" pageNumber="61">K. flagrispina</emphasis>
Mohrig
</taxonomicName>
, 
<taxonomicName id="4C206A9E8211FF949A65CC05CA4AFBC0" authority="Vilkamaa" authorityName="Vilkamaa" class="Insecta" family="Sciaridae" genus="Keilbachia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="mira">
<emphasis id="B954CD0F8211FF949A65CC05CED1FBFB" box="[1347,1436,1072,1097]" italics="true" pageId="4" pageNumber="61">K. mira</emphasis>
Vilkamaa
</taxonomicName>
