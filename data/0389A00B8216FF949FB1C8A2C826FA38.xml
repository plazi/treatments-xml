., 
<taxonomicName id="4C206A9E8211FF949B46CC3DCE1FFB90" authority="Vilkamaa" authorityName="Vilkamaa" box="[1120,1362,1032,1058]" class="Insecta" family="Sciaridae" genus="Camptochaeta" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="arcuata">
<emphasis id="B954CD0F8211FF949B46CC3DCF91FB93" box="[1120,1244,1032,1057]" italics="true" pageId="4" pageNumber="61">C. arcuata</emphasis>
Vilkamaa
</taxonomicName>
&amp; 
<emphasis id="B954CD0F8211FF949A50CC3DCEC0FB93" box="[1398,1421,1032,1057]" italics="true" pageId="4" pageNumber="61">al</emphasis>
., 
<taxonomicName id="4C206A9E8211FF949FB1CC05CAC9FBF8" authority="Vilkamaa" authorityName="Vilkamaa" box="[151,388,1072,1098]" class="Insecta" family="Sciaridae" genus="Keilbachia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="arrecta">
<emphasis id="B954CD0F8211FF949FB1CC05CA40FBFB" box="[151,269,1072,1097]" italics="true" pageId="4" pageNumber="61">K. arrecta</emphasis>
Vilkamaa
</taxonomicName>
&amp; 
<emphasis id="B954CD0F8211FF949E8ECC05CAF3FBFB" box="[424,446,1072,1097]" italics="true" pageId="4" pageNumber="61">al</emphasis>
., 
<taxonomicName id="4C206A9E8211FF949EF2CC05C9AFFBF8" authority="Edwards" authorityName="Edwards" box="[468,738,1072,1098]" class="Insecta" family="Sciaridae" genus="Keilbachia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="curvispina">
<emphasis id="B954CD0F8211FF949EF2CC05C93FFBFB" box="[468,626,1072,1097]" italics="true" pageId="4" pageNumber="61">K. curvispina</emphasis>
Edwards
</taxonomicName>
, 
<taxonomicName id="4C206A9E8211FF949DCBCC05C8A9FBF8" authority="Vilkamaa" authorityName="Vilkamaa" box="[749,996,1072,1098]" class="Insecta" family="Sciaridae" genus="Keilbachia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="demissa">
<emphasis id="B954CD0F8211FF949DCBCC05C820FBFB" box="[749,877,1072,1097]" italics="true" pageId="4" pageNumber="61">K. demissa</emphasis>
Vilkamaa
</taxonomicName>
&amp; 
