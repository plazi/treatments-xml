<subSubSection id="C329A688FFCD3A0CFF25BA2214E1FDA3" box="[197,993,559,586]" pageId="11" pageNumber="12" type="etymology">
<paragraph id="8B8CF503FFCD3A0CFF25BA2214E1FDA3" blockId="11.[151,1437,447,1594]" box="[197,993,559,586]" pageId="11" pageNumber="12">
<emphasis id="B9472911FFCD3A0CFF25BA221655FDA0" bold="true" box="[197,341,559,585]" pageId="11" pageNumber="12">Etymology:</emphasis>
