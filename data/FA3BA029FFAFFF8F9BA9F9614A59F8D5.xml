<taxonomicName id="B5926ABCFFA1FF8C9A6DF87A498EF867" box="[339,494,1973,1996]" class="Insecta" family="Hymenoptera" genus="Chapmanella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="negrosensis">
