<taxonomicName id="4C304DEDD05EFFEAFC98FC4A33F500A9" box="[856,907,944,967]" class="Magnoliopsida" family="Amaranthaceae" genus="Beta" kingdom="Plantae" order="Caryophyllales" pageId="11" pageNumber="347" phylum="Tracheophyta" rank="genus">
