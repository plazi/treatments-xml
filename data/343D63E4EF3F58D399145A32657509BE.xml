, 
<taxonomicName id="AE854422B4EA85A4D0416A20C0E88A07" baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="7" pageNumber="8" phylum="Mollusca" rank="species" species="abyssorum" subGenus="Drymaeus">
