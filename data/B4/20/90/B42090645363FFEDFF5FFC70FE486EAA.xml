<subSubSection pageId="30" pageNumber="181" type="discussion">
<paragraph blockId="30.[151,1436,1338,1655]" pageId="30" pageNumber="181">
<emphasis bold="true" box="[199,315,1594,1619]" pageId="30" pageNumber="181">Remarks.</emphasis>
Resembling 
<taxonomicName box="[465,597,1594,1619]" class="Insecta" family="Aphrophoridae" genus="Tremapterus" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="181" phylum="Arthropoda" rank="species" species="brunneus">
