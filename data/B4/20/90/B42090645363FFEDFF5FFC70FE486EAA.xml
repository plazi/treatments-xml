(
<emphasis bold="true" box="[321,446,1740,1766]" italics="true" pageId="30" pageNumber="181">Patriziana</emphasis>
) 
<emphasis bold="true" box="[464,549,1740,1766]" italics="true" pageId="30" pageNumber="181">turneri</emphasis>
