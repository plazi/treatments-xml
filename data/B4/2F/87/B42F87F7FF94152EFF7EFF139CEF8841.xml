</taxonomicName>
[= 
<taxonomicName baseAuthorityName="Ramsay" baseAuthorityYear="1877" box="[151,428,1173,1197]" class="Mammalia" family="Peramelidae" genus="Isoodon" kingdom="Animalia" order="Peramelemorphia" pageId="15" pageNumber="216" phylum="Chordata" rank="subSpecies" species="macrourus" subSpecies="torosus">
