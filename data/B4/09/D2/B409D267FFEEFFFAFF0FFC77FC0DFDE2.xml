<paragraph blockId="18.[151,1436,1721,1804]" pageId="18" pageNumber="119">
<emphasis bold="true" box="[151,276,1721,1743]" pageId="18" pageNumber="119">FIGURE 7.</emphasis>
<taxonomicName box="[285,556,1721,1743]" class="Arachnida" family="Gonyleptidae" genus="Nanophareus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="18" pageNumber="119" phylum="Arthropoda" rank="species" species="polyhastatus" status="sp. nov.">
<emphasis box="[285,556,1721,1743]" italics="true" pageId="18" pageNumber="119">
<taxonomicName box="[285,420,1721,1743]" class="Arachnida" family="Gonyleptidae" genus="Nanophareus" higherTaxonomySource="CoL" kingdom="Animalia" order="Opiliones" pageId="18" pageNumber="119" phylum="Arthropoda" rank="genus">Nanophareus</taxonomicName>
<taxonomicName box="[428,556,1721,1743]" pageId="18" pageNumber="119" rank="species" species="polyhastatus" status="sp. nov.">polyhastatus</taxonomicName>
