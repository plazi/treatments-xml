<paragraph pageId="0" pageNumber="21">
<taxonomicName authorityName="X. Luo &amp; Q. Y. Yang" authorityYear="2021" class="Magnoliopsida" family="Caryophyllaceae" genus="Pseudostellaria" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pseudostellaria wuyishanensis" order="Caryophyllales" pageId="0" pageNumber="21" phylum="Tracheophyta" rank="species" species="wuyishanensis">
