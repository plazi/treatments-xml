<taxonomicName authority="Nomura &amp; Nomura &amp; Nomura Jaloszynski" authorityName="Nomura &amp; Nomura &amp; Nomura Jaloszynski" class="Insecta" family="Scydmaenidae" genus="Cephennodes" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="228" phylum="Arthropoda" rank="species" species="pseudobos">
