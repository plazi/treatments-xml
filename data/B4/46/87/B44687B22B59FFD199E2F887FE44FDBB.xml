<collectingCountry box="[359,426,223,248]" name="China" pageId="17" pageNumber="18">China</collectingCountry>
(
<collectingRegion box="[441,538,223,248]" country="China" name="Sichuan" pageId="17" pageNumber="18">Sichuan</collectingRegion>
