<taxonomicName authorityName="Dillon &amp; Dillon" authorityYear="1948" class="Insecta" family="Cerambycidae" genus="Olenecamptus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Olenecamptus bilobus subsp. tonkinus" order="Coleoptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="subSpecies" species="bilobus" subSpecies="tonkinus">
