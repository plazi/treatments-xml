<collectingCountry box="[459,558,1938,1962]" name="United States of America" pageId="2" pageNumber="39">America</collectingCountry>
, Caribbean Islands, Canary Islands, Iberian Peninsula, 
<collectingCountry box="[1185,1270,1938,1962]" name="Angola" pageId="2" pageNumber="39">Angola</collectingCountry>
, 
<collectingCountry box="[1282,1358,1939,1963]" name="Kenya" pageId="2" pageNumber="39">Kenya</collectingCountry>
, 
<collectingCountry name="South Africa" pageId="2" pageNumber="39">South Africa</collectingCountry>
, 
<collectingCountry box="[237,285,1975,1998]" name="Iran" pageId="2" pageNumber="39">Iran</collectingCountry>
, 
<collectingCountry box="[297,357,1974,1998]" name="India" pageId="2" pageNumber="39">India</collectingCountry>
, Southeastern Asia, 
<collectingCountry box="[586,656,1974,1998]" name="China" pageId="2" pageNumber="39">China</collectingCountry>
, 
<collectingCountry box="[667,887,1974,1998]" name="Papua New Guinea" pageId="2" pageNumber="39">Papua New Guinea</collectingCountry>
, Queensland (
