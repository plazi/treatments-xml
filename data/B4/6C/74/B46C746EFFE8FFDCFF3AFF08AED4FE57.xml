<number box="[1097,1173,790,814]" pageId="6" pageNumber="43" value="21.0" valueMax="22.0" valueMin="20.0">20–22</number>
), j 
<number box="[1203,1218,790,814]" pageId="6" pageNumber="43" value="3.0">3</number>
: 
<number box="[1231,1259,790,814]" pageId="6" pageNumber="43" value="23.0">23</number>
(
<number box="[1275,1350,790,814]" pageId="6" pageNumber="43" value="24.5" valueMax="26.0" valueMin="23.0">23–26</number>
), j 
<number box="[1380,1396,790,814]" pageId="6" pageNumber="43" value="4.0">4</number>
