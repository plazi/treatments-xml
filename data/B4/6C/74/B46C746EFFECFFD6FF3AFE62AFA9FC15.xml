<collectingCountry box="[861,948,777,801]" name="Algeria" pageId="2" pageNumber="39">Algeria</collectingCountry>
, 
<collectingCountry box="[959,1065,777,801]" name="Bermuda" pageId="2" pageNumber="39">Bermuda</collectingCountry>
, 
<collectingCountry box="[1076,1147,777,801]" name="Brazil" pageId="2" pageNumber="39">Brazil</collectingCountry>
, 
<collectingCountry box="[1158,1271,777,801]" name="Colombia" pageId="2" pageNumber="39">Colombia</collectingCountry>
, 
<collectingCountry box="[1282,1344,777,801]" name="Cuba" pageId="2" pageNumber="39">Cuba</collectingCountry>
, Florida (
<collectingCountry box="[159,220,813,837]" name="United States of America" pageId="2" pageNumber="39">USA</collectingCountry>
), Galápagos, 
<collectingCountry box="[376,514,813,837]" name="Guadeloupe" pageId="2" pageNumber="39">Guadeloupe</collectingCountry>
, 
<collectingCountry box="[527,617,813,837]" name="Guyana" pageId="2" pageNumber="39">Guyana</collectingCountry>
, 
<collectingCountry box="[629,741,813,837]" name="Honduras" pageId="2" pageNumber="39">Honduras</collectingCountry>
, 
<collectingCountry box="[754,814,813,837]" name="India" pageId="2" pageNumber="39">India</collectingCountry>
, 
<collectingCountry box="[827,920,813,837]" name="Jamaica" pageId="2" pageNumber="39">Jamaica</collectingCountry>
, Louisiana (
<collectingCountry box="[1061,1122,813,837]" name="United States of America" pageId="2" pageNumber="39">USA</collectingCountry>
), 
<collectingCountry box="[1143,1270,813,837]" name="Martinique" pageId="2" pageNumber="39">Martinique</collectingCountry>
, 
<collectingCountry box="[1283,1371,813,837]" name="Mexico" pageId="2" pageNumber="39">Mexico</collectingCountry>
, 
<collectingCountry box="[1385,1436,813,837]" name="Peru" pageId="2" pageNumber="39">Perú</collectingCountry>
and 
<collectingCountry box="[198,317,849,873]" name="Venezuela" pageId="2" pageNumber="39">Venezuela</collectingCountry>
