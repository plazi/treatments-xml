<taxonomicName class="Actinopterygii" family="Belonidae" genus="Strongylura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Strongylura cylindrata" order="Beloniformes" pageId="1" pageNumber="20" phylum="Chordata" rank="species" species="cylindrata">Strongylura cylindrata</taxonomicName>
