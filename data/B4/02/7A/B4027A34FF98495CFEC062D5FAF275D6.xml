<number box="[1310,1325,1873,1899]" pageId="1" pageNumber="32" value="3.0">3</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="2" pageNumber="33">
<paragraph blockId="2.[264,1323,284,990]" pageId="2" pageNumber="33">
<keyLead pageId="2" pageNumber="33">
<number box="[264,281,284,310]" pageId="2" pageNumber="33" value="2.0">2</number>
