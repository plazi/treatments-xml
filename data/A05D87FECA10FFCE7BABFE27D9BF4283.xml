<taxonomicName id="EFF44D6BCA10FFCE7BB2FE26D93241D6" authorityName="Dlussky" authorityYear="1996" box="[187,322,393,414]" class="Insecta" family="Formicidae" genus="Haidomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="838" phylum="Arthropoda" rank="genus">Haidomyrmex</taxonomicName>
