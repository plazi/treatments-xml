<paragraph id="4E6C368F484DFFA9E6BCFC3A41C3E305" blockId="2.[151,756,258,917]" pageId="2" pageNumber="271">
<treatmentCitationGroup id="6EC311A1484DFFA9E6BCFC3A41C3E305" pageId="2" pageNumber="271">
Now 
<taxonomicName id="89D34D0C484DFFA9E6F2FC3A42FAE325" authority="(de Saussure, 1859)" baseAuthorityName="de Saussure" baseAuthorityYear="1859" box="[217,752,258,281]" class="Aves" family="Apodidae" genus="Streptoprocne" kingdom="Animalia" order="Apodiformes" pageId="2" pageNumber="271" phylum="Chordata" rank="species" species="semicollaris">
