<typeStatus id="54FB8824FFFE771AB5F6FEF68F9CDD69" box="[672,767,272,292]" pageId="5" pageNumber="776">Holotypus</typeStatus>
(): &quot;N. Bogsán Hungaria&quot; (= Németbogsán = Bocsa Vasiove/ 
<collectingCountry id="F3577616FFFE771AB54CFECE8F19DD71" box="[538,634,296,316]" name="Romania" pageId="5" pageNumber="776">Rumänien</collectingCountry>
