</emphasis>
<collectingRegion id="8A2AF820FF00FF2BFE4BFEC86740CACE" box="[475,515,367,393]" country="South Sudan" name="Lakes" pageId="233" pageNumber="234">See</collectingRegion>
<taxonomicName id="8FEE4D41FF00FF2BFD9CFEC86601CACE" authorityName="Koch" authorityYear="1844" box="[524,834,367,393]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="233" pageNumber="234" phylum="Arthropoda" rank="species" species="senegalensis">
