<collectingCountry id="3CC9760AFF868B318109DAA8FB1D35CB" box="[1051,1192,1175,1200]" name="Netherlands" pageId="25" pageNumber="226">Netherlands</collectingCountry>
, 
<collectingCountry id="3CC9760AFF868B3181A8DAA8FAA835CB" box="[1210,1309,1175,1200]" name="Belgium" pageId="25" pageNumber="226">Belgium</collectingCountry>
