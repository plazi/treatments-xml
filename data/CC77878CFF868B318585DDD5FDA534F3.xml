<collectingCountry id="3CC9760AFF868B318411DAA7FEE635D4" box="[259,339,1176,1199]" name="France" pageId="25" pageNumber="226">France</collectingCountry>
; from 
<collectingCountry id="3CC9760AFF868B3184BBDAA8FDB335CB" box="[425,518,1175,1200]" name="United Kingdom" pageId="25" pageNumber="226">England</collectingCountry>
