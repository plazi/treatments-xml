<taxonomicName id="4C424D7BFFE8FFB03FB54313D84F432F" box="[415,634,1340,1364]" class="Clitellata" family="Glossoscolecidae" genus="Glossoscolex" kingdom="Animalia" order="Haplotaxida" pageId="14" pageNumber="73" phylum="Annelida" rank="species" species="terraopimus" subGenus="Glossoscolex">
