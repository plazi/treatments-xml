<number id="3D8A24DFFFE8FFB03AD34371DF37430C" box="[1273,1282,1375,1399]" pageId="14" pageNumber="73" value="1.0">1</number>
/ 
<number id="3D8A24DFFFE8FFB03B264371DF2F430C" box="[1292,1306,1375,1399]" pageId="14" pageNumber="73" value="2.0">2</number>
