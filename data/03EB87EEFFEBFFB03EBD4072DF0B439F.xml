<taxonomicName id="4C424D7BFFE8FFB03B394386DFA943BB" box="[1299,1436,1448,1472]" class="Clitellata" family="Glossoscolecidae" genus="Glossocolex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="14" pageNumber="73" phylum="Annelida" rank="genus">
<emphasis id="B936EAEAFFE8FFB03B394386DFA943BB" box="[1299,1436,1448,1472]" italics="true" pageId="14" pageNumber="73">Glossocolex</emphasis>
</taxonomicName>
defined by male pore location, 
<taxonomicName id="4C424D7BFFE8FFB03FD943E3D886439F" box="[499,691,1484,1508]" class="Clitellata" family="Glossoscolecidae" genus="Glossoscolex" kingdom="Animalia" order="Haplotaxida" pageId="14" pageNumber="73" phylum="Annelida" rank="species" species="mariarum" subGenus="Glossoscolex">
<emphasis id="B936EAEAFFE8FFB03FD943E3D83D439F" box="[499,520,1485,1508]" italics="true" pageId="14" pageNumber="73">G.</emphasis>
(
<emphasis id="B936EAEAFFE8FFB03C3143E3D805439F" box="[539,560,1485,1508]" italics="true" pageId="14" pageNumber="73">G.</emphasis>
) 
