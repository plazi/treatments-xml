<taxonomicName id="4C424D7BFFE8FFB03F6F426EDBC34223" box="[325,502,1087,1112]" class="Clitellata" family="Glossoscolecidae" genus="Glossoscolex" kingdom="Animalia" order="Haplotaxida" pageId="14" pageNumber="73" phylum="Annelida" rank="species" species="bondari" subGenus="Glossoscolex">
