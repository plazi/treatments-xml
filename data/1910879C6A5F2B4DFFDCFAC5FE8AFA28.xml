<taxonomicName id="56B94D096A5F2B4EFEB4FA5BFD05F9A5" box="[506,668,1523,1546]" class="Insecta" family="Cicadellidae" genus="Prescottia" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="60" phylum="Arthropoda" rank="subSpecies" species="lojana" subSpecies="folio">P. lojana folio</taxonomicName>
