<taxonomicName id="56B94D096A5F2B4EFC3EFA7BFBC2FA45" box="[880,1115,1491,1514]" class="Insecta" family="Cicadellidae" genus="Prescottia" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="60" phylum="Arthropoda" rank="subSpecies" species="stachyodes" subSpecies="labello">P. stachyodes labello</taxonomicName>
