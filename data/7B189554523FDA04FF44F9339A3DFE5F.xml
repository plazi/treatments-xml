<taxonomicName id="34B15FC1523CDA04FB73FF649B21FF74" box="[1184,1288,192,217]" class="Magnoliopsida" family="Asteraceae" genus="Solidago" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="genus">
<emphasis id="C1C5F850523CDA04FB73FF649B21FF74" box="[1184,1288,192,217]" italics="true" pageId="7" pageNumber="46">Solidago</emphasis>
</taxonomicName>
(goldenrod, 
<taxonomicName id="34B15FC1523CDA04FF44FF4C9F34FEAF" box="[151,285,232,258]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
); from 
<taxonomicName id="34B15FC1523CDA04FEA3FF4C9C98FEAF" authority="Burgess" authorityName="Burgess" box="[368,689,232,258]" class="Insecta" family="Agromyzidae" genus="Liriomyza" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="trifolii">
