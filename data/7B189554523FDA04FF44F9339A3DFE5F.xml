<taxonomicName id="34B15FC1523CDA04FE93FF649FC1FF77" box="[320,488,192,218]" class="Magnoliopsida" family="Aquifoliaceae" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
