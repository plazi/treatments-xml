. [first label] 
<collectingCountry id="8BA664D2523EDA06FD1AF8389D66F81B" box="[713,847,1948,1974]" name="Saint Vincent and the Grenadines" pageId="5" pageNumber="44">St. Vincent</collectingCountry>
, W[est] I[ndies], H.H. Smith, [second label] 
<typeStatus id="2C0A9AE0523EDA06FAB1F8389BB5F81B" box="[1378,1436,1948,1974]" pageId="5" pageNumber="44">Type</typeStatus>
No. 
<number id="45793665523EDA06FF19F8609F2CF873" box="[202,261,1988,2014]" pageId="5" pageNumber="44" value="2336.0">2336</number>
<collectionCode id="95A0BC87523EDA06FEDEF8609F4BF873" LSID="urn:lsid:biocol.org:col:34871" box="[269,354,1988,2014]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="5" pageNumber="44">USNM</collectionCode>
[third label] 
<taxonomicName id="34B15FC1523EDA06FE2EF8609D14F873" authority="Ashm." authorityName="Ashm." box="[509,829,1988,2014]" class="Liliopsida" family="Poaceae" genus="Chrestosem" kingdom="Plantae" order="Poales" pageId="5" pageNumber="44" phylum="Tracheophyta" rank="species" species="robusta">
