Fig. 
<number id="45793665523CDA04FCE2FF649D69FF77" box="[817,832,192,218]" pageId="7" pageNumber="46" value="2.0">2</number>
</figureCitation>
F); from 
<taxonomicName id="34B15FC1523CDA04FC6DFF649A16FF74" box="[958,1087,192,217]" class="Insecta" family="Agromyzidae" genus="Phytomyza" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="C1C5F850523CDA04FC6DFF649A16FF74" box="[958,1087,192,217]" italics="true" pageId="7" pageNumber="46">Phytomyza</emphasis>
</taxonomicName>
