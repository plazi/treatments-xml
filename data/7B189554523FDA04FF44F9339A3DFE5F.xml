<taxonomicName id="34B15FC1523CDA04FA9FFF3C9EF4FF74" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="species" species="myrtifolia">
<emphasis id="C1C5F850523CDA04FA9FFF3C9EF4FF74" italics="true" pageId="7" pageNumber="46">I. myrtifolia</emphasis>
