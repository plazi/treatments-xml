<taxonomicName id="34B15FC1523CDA04FB5AFF3C9B3BFF1C" box="[1161,1298,152,177]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="species" species="cassine">
