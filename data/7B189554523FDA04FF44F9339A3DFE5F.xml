<taxonomicName id="34B15FC1523CDA04FEB4FEB49FFFFE84" box="[359,470,272,297]" class="Insecta" family="Agromyzidae" genus="Liriomyza" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="species" species="trifolii">
<emphasis id="C1C5F850523CDA04FEB4FEB49FFFFE84" box="[359,470,272,297]" italics="true" pageId="7" pageNumber="46">L. trifolii</emphasis>
</taxonomicName>
