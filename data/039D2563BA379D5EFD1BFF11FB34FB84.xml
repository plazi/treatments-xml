<specimenCount id="9D325FFCBA379D5EFC48FCD6FBA9FCD5" box="[935,1046,774,797]" pageId="66" pageNumber="67" type="male">12 males</specimenCount>
and 
<specimenCount id="9D325FFCBA379D5EFBB3FCD6FCBFFCF2" pageId="66" pageNumber="67" type="female">seven females</specimenCount>
