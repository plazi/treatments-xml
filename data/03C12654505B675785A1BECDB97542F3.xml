<taxonomicName id="4C68ECC15058675785A1B94EBAD543BF" authority="Obenberger, 1924" authorityName="Obenberger" authorityYear="1924" box="[151,500,620,645]" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="121" phylum="Arthropoda" rank="species" species="protenor">
