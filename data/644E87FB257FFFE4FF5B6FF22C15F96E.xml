<taxonomicName id="2BE74D6E257FFFE4FE25681B2CCAFBFA" box="[489,650,1050,1076]" class="Insecta" family="Stratiomyidae" genus="Ptecticus" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="assamensis">
