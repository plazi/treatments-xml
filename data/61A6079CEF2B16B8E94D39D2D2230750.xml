New Zealand 
<taxonomicName id="7A1EC010BC83EEEA883BB329FB82213A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:453" class="Insecta" family="Scelionidae" genus="Archaeoteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Archaeoteleia Masner" order="Hymenoptera" pageNumber="22" phylum="Arthropoda" rank="genus">Archaeoteleia</taxonomicName>
