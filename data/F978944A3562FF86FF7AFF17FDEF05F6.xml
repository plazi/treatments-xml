<taxonomicName id="B6D15EDF3562FF86FF7AFF17FDA904EF" authority="Leach, 1815" authorityName="Leach" authorityYear="1815" box="[151,607,151,178]" class="Chilopoda" family="Scolopendridae" genus="Scolopendra" kingdom="Animalia" order="Scolopendromorpha" pageId="6" pageNumber="139" phylum="Arthropoda" rank="species" species="subspinipes">
