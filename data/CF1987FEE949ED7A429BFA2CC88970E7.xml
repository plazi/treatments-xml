<taxonomicName id="80B04D6BE94AED7B4296F978C8BC72C5" box="[753,993,1693,1717]" class="Insecta" family="Athericidae" genus="Sinocretomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="34" phylum="Arthropoda" rank="species" species="miniscule">
