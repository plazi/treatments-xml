<paragraph id="14CCA2810C5931E727CAD579E64A5B97" pageNumber="171">
<taxonomicName id="AD2C4C64EADC038382593FE1F4CA7DD2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:140561" class="Insecta" family="Pterophoridae" genus="Wheeleria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Wheeleria santschii Forel" lsidName-HNS="Wheeleria santschii Forel" order="Lepidoptera" pageNumber="171" phylum="Arthropoda" rank="species" species="santschii">Wheeleria Santschii</taxonomicName>
