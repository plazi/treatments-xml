<subSubSection id="F82549B606EBE88BC57FA814686F9954" pageId="0" pageNumber="89" type="comments">
<paragraph id="DDDAADD2F5B59DA19113AC92CAAD5EED" pageId="0" pageNumber="89">Comments.</paragraph>
<paragraph id="39AE6B49B39534BFDD8815C2B3C86E76" pageId="0" pageNumber="89">
<taxonomicName id="BD7F55C33E71FAA5CBE522D5630C1991" authorityName="Araujo &amp; Padua" authorityYear="2020" class="Insecta" family="Ichneumonidae" genus="Dolichomitus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dolichomitus pimmi" order="Hymenoptera" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="pimmi">
