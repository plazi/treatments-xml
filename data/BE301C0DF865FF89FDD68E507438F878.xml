<taxonomicName id="F199D698F865FF8AFD288E507297F8C5" authority="Berg, 1883" authorityName="Berg" authorityYear="1883" box="[674,984,1851,1877]" class="Insecta" family="Miridae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="tribe" tribe="Eccritotarsini">
