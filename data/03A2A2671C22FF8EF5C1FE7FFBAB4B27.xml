<taxonomicName id="4C0B68F21C22FF8EF406FD22FE7E4DE0" box="[336,422,730,755]" class="Arachnida" family="Neobisiidae" genus="Roncus" kingdom="Animalia" order="Pseudoscorpiones" pageId="3" pageNumber="30" phylum="Arthropoda" rank="genus">
