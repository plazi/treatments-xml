<number id="3DC301561C22FF8EF420FCD2FE4C4C57" box="[374,404,810,836]" pageId="3" pageNumber="30" value="11.0">11</number>
: 
<number id="3DC301561C22FF8EF4CEFCD2FE6E4C57" box="[408,438,810,836]" pageId="3" pageNumber="30" value="11.0">11</number>
: 
<number id="3DC301561C22FF8EF4EDFCD2FE054C57" box="[443,477,810,836]" pageId="3" pageNumber="30" value="12.0">12</number>
