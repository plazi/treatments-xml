<taxonomicName id="29DBE4E9FFED4B76FCC6FBA79194FB1D" authorityName="Harold 1869 a:" authorityYear="1232" class="Insecta" family="Rutelidae" genus="Sulcipalpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="399" phylum="Arthropoda" rank="genus">
