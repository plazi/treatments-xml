<taxonomicName id="29DBE4E9FFED4B76FDC3FB5C9263FB1D" authorityName="VIRIDIS GUERIN-MENEVILLE" authorityYear="1838" baseAuthorityName="VIRIDIS GUERIN-MENEVILLE" baseAuthorityYear="1838" box="[628,772,1167,1188]" class="Insecta" family="Rutelidae" genus="Aulacopalpus" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="399" phylum="Arthropoda" rank="genus">
