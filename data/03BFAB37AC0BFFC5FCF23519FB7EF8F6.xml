<taxonomicName id="4C1661A2AC04FFC5FECA3079FCAAFB1C" authority="Cao &amp; Chen" authorityName="CAO &amp; CHEN" authorityYear="2010" box="[342,834,1137,1157]" class="Insecta" family="Drosophilidae" genus="Stegana" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="734" phylum="Arthropoda" rank="species" species="quadrata" status="sp. nov." subGenus="Stegana">
<emphasis id="B962C633AC04FFC5FECA3079FD46FB1D" bold="true" box="[342,686,1137,1156]" pageId="8" pageNumber="734">
<emphasis id="B962C633AC04FFC5FECA3079FE5EFB1D" bold="true" box="[342,438,1137,1156]" italics="true" pageId="8" pageNumber="734">Stegana</emphasis>
(
<emphasis id="B962C633AC04FFC5FE563079FDC2FB1D" bold="true" box="[458,554,1137,1156]" italics="true" pageId="8" pageNumber="734">Stegana</emphasis>
) 
<emphasis id="B962C633AC04FFC5FDA33079FD46FB1D" bold="true" box="[575,686,1137,1156]" italics="true" pageId="8" pageNumber="734">quadrata</emphasis>
