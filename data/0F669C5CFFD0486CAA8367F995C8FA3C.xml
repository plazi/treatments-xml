<taxonomicName id="40CF56C9FFD24861ADA46E15919EF7CC" authorityName="Paperna" authorityYear="1964" box="[1251,1452,2064,2086]" class="Monogenea" family="Tetraonchidae" genus="Ergenstrema" kingdom="Animalia" order="Dactylogyridea" pageId="7" pageNumber="56" phylum="Platyhelminthes" rank="species" species="mugilis">
<emphasis id="B5BBF158FFD24861ADA46E15919EF7CC" box="[1251,1452,2064,2086]" italics="true" pageId="7" pageNumber="56">Ergenstrema mugilis</emphasis>
</taxonomicName>
