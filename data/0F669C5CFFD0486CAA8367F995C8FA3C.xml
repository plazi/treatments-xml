<caption id="D3B07DC2FFD04863A93661F794CFF7AB" ID-DOI="http://doi.org/10.5281/zenodo.5383750" ID-Zenodo-Dep="5383750" httpUri="https://zenodo.org/record/5383750/files/figure.png" pageId="5" pageNumber="54" startId="5.[113,148,2035,2057]" targetBox="[112,1453,720,2024]" targetPageId="5">
<paragraph id="87702D4AFFD04863A93661F794CFF7AB" blockId="5.[113,1452,2035,2115]" pageId="5" pageNumber="54">
