<materialsCitation id="4997575B1A23F47B3C624BFF1E3DFAF5" box="[375,635,1269,1295]" collectorName="In" country="Canada" location="Ohio and Michigan" pageId="51" pageNumber="52" specimenCount="1" stateProvince="Ohio and Michigan">
