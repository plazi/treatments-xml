<taxonomicName id="3EFF26851A23F47B3CF64A4A1E3AFAA0" box="[483,636,1344,1370]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="CB8B81141A23F47B3CF64A4A1E16FAA3" box="[483,592,1344,1369]" italics="true" pageId="51" pageNumber="52">Trifolium</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A23F47B3F9F4A4A1EBAFAA0" box="[650,764,1344,1370]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
), common plantain (
<taxonomicName id="3EFF26851A23F47B3EE54A4A18ECFAA3" box="[1008,1194,1344,1369]" class="Magnoliopsida" family="Plantaginaceae" genus="Plantago" kingdom="Plantae" order="Lamiales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="species" species="major">
<emphasis id="CB8B81141A23F47B3EE54A4A18ECFAA3" box="[1008,1194,1344,1369]" italics="true" pageId="51" pageNumber="52">Plantago major</emphasis>
</taxonomicName>
L., 
<taxonomicName id="3EFF26851A23F47B39CE4A4A19D6FAA0" box="[1243,1424,1344,1370]" class="Magnoliopsida" family="Plantaginaceae" kingdom="Plantae" order="Lamiales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="family">Plantaginaceae</taxonomicName>
), mullein (
<taxonomicName id="3EFF26851A23F47B3C144A6F1DEAFA85" box="[257,428,1381,1407]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="CB8B81141A23F47B3C144A6F1DC4FA84" box="[257,386,1381,1406]" italics="true" pageId="51" pageNumber="52">Verbascum</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="3EFF26851A23F47B3CAC4A6F1EC3FA85" box="[441,645,1381,1407]" class="Magnoliopsida" family="Scrophulariaceae" kingdom="Plantae" order="Lamiales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="family">Scrophulariaceae</taxonomicName>
) (
<bibRefCitation id="9D6E20F71A23F47B3F894A6F1F00FA85" author="Godfrey, G. L." box="[668,838,1381,1407]" pageId="51" pageNumber="52" pagination="1 - 265" refId="ref30677" refString="Godfrey, G. L. (1972) A review and reclassification of larvae of the subfamily Hadeninae (Lepidoptera, Noctuidae) of American north of Mexico. U. S. Department of Agriculture Technical Bulletin, 1450, 1 - 265." type="journal article" year="1972">Godfrey 1972</bibRefCitation>
). Natural native hosts are unknown.
</paragraph>
