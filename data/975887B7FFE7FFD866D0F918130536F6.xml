<paragraph id="1F4E36A1FFE6FFD866D0FBB9130536F6" blockId="51.[192,1440,1123,1243]" pageId="51" pageNumber="48">
<emphasis id="2D85EAB3FFE6FFD866D0FBB9132B3656" bold="true" box="[192,340,1123,1147]" pageId="51" pageNumber="48">Comments</emphasis>
. 
<taxonomicName id="D8F14D22FFE6FFD86774FBB9101B3656" authorityName="Santos-Silva &amp; Heffern &amp; Matsuda" authorityYear="2010" box="[356,612,1123,1147]" class="Insecta" family="Cerambycidae" genus="Melanesiandra" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="48" phylum="Arthropoda" rank="species" species="birai">
