<quantity id="01319B45FFAAFFA4E44D3E19FC45F8C0" box="[836,937,1844,1868]" metricMagnitude="-3" metricUnit="m" metricValue="1.89" pageId="1" pageNumber="429" unit="mm" value="1.89">1.89 mm</quantity>
