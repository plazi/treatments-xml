<quantity id="01319B45FFAAFFA4E5043E1EFD59F8C7" box="[525,693,1843,1868]" metricMagnitude="-4" metricUnit="m" metricValue="4.15" metricValueMax="4.5" metricValueMin="3.8" pageId="1" pageNumber="429" unit="mm" value="0.415" valueMax="0.45" valueMin="0.38">0.38–0.45 mm</quantity>
