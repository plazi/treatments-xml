<subSubSection id="F994E41B270338870EB5D250263998E9" pageId="0" pageNumber="57889" type="nomenclature">
<paragraph id="005749CF88916F93DCB8E7332AFE9DEE" pageId="0" pageNumber="57889">
<taxonomicName id="FD690515129B25C35A1C8B1F7F2B50BC" LSID="3C2150DA-9E43-529A-8312-B78A8D1730FF" authority="De Geer, 1778" authorityName="De Geer" authorityYear="1778" class="Arachnida" family="Lycosidae" genus="Trochosa" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Trochosa ruricola" order="Araneae" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="ruricola">Trochosa ruricola De Geer, 1778</taxonomicName>
