<collectingCounty id="62E74E43FF8CFF80FD9DFCC8B0F9FCA2" box="[623,733,803,827]" pageId="13" pageNumber="4042">Na Haeo</collectingCounty>
, 
<collectingMunicipality id="6BE2ACB5FF8CFF80FD18FCC8B154FCA2" box="[746,880,803,827]" pageId="13" pageNumber="4042">Chang Tok</collectingMunicipality>
waterfall, 
<date id="FF87100FFF8CFF80FC1FFCC8B6B4FCA2" box="[1005,1168,803,827]" pageId="13" pageNumber="4042" value="2003-05-17">
