<collectingCountry id="F32E765FFF8CFF80FEB0FCC8B38BFCA2" box="[322,431,803,827]" name="Thailand" pageId="13" pageNumber="4042">Thailand</collectingCountry>
, 
<collectingRegion id="49FDF82DFF8CFF80FE4FFCC8B045FCA2" box="[445,609,803,827]" country="Thailand" name="Loei" pageId="13" pageNumber="4042">Loei province</collectingRegion>
