<paragraph id="41426C4ACB7D26FFA74B94C3AE036AB3" pageId="0" pageNumber="9042">
<taxonomicName id="14B3D28D190383EC4BF4571D8554A3DB" class="Insecta" family="Ichneumonidae" genus="Pezomachus" kingdom="Animalia" lsidName="Pezomachus annulicornis" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="annulicornis">Pezomachus annulicornis</taxonomicName>
