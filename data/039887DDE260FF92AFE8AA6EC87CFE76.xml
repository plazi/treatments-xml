<number id="3DF924ECE260FF93AB4AA929C81AFA80" box="[1077,1121,1661,1686]" pageId="10" pageNumber="11" value="105112.0">105</number>
</figureCitation>
, 
<figureCitation id="130A2A4EE260FF93AB12A92AC8E1FA80" box="[1133,1178,1662,1686]" captionStart="FIGURES 109 – 112" captionStartId="48.[151,269,1967,1991]" captionTargetBox="[204,1388,223,1950]" captionTargetId="figure@48.[197,1394,194,1954]" captionTargetPageId="48" captionText="FIGURES 109 – 112. Insula curva sp. nov., male holotype. 109, palp, prolateral view; 110, palp, retrolateral view; 111, palp, ventral view; 112, bulbus, prolateral view." httpUri="https://zenodo.org/record/284768/files/figure.png" pageId="10" pageNumber="11">112</figureCitation>
