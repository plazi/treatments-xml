<subSubSection id="C18F654F655BFFB6FF66FAF0C779F9CB" pageId="26" pageNumber="27" type="materials_examined">
<paragraph id="892A36C4655BFFB6FF66FAF0C7A3FACF" blockId="26.[151,1437,1337,1722]" pageId="26" pageNumber="27">
<emphasis id="BBE1EAD6655BFFB6FF66FAF0C379FAEB" bold="true" box="[151,332,1337,1362]" pageId="26" pageNumber="27">
<typeStatus id="562E8866655BFFB6FF66FAF0C2E7FAEB" box="[151,210,1337,1362]" pageId="26" pageNumber="27">Type</typeStatus>
