<taxonomicName id="4C774D03BEA204857BAD3F70FBC48A27" box="[740,1040,1626,1649]" class="Insecta" family="Miridae" genus="Omnivoriphylus" kingdom="Animalia" order="Hemiptera" pageId="155" pageNumber="155" phylum="Arthropoda" rank="species" species="charleville" status="new species">
