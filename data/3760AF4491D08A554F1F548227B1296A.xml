<taxonomicName id="AEEC5487747CC6A09FF1F3F5EE96B9D8" class="Malacostraca" family="Stenothoidae" genus="Metopoides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metopoides" order="Amphipoda" pageId="8" pageNumber="19" phylum="Arthropoda" rank="genus">Metopoides</taxonomicName>
