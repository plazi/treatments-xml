<geoCoordinate id="C1C871AA444AFFD2CBDB57A7FD6EFA0F" box="[668,763,1410,1435]" direction="north" orientation="latitude" pageId="4" pageNumber="100" precision="925" value="45.833332">45°50'N</geoCoordinate>
, 
<geoCoordinate id="C1C871AA444AFFD2CA4057A7FCF6FA0F" box="[775,867,1410,1435]" direction="east" orientation="longitude" pageId="4" pageNumber="100" precision="925" value="23.1">23°06'E</geoCoordinate>
