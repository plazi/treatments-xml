<quantity id="6304BA88444AFFD2CBD35782FC96FA2A" box="[660,771,1447,1471]" metricMagnitude="-1" metricUnit="m" metricValue="1.6" metricValueMax="2.0" metricValueMin="1.2" pageId="4" pageNumber="100" unit="cm" value="16.0" valueMax="20.0" valueMin="12.0">12–20 cm</quantity>
