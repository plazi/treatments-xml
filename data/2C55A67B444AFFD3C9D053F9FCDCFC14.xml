<taxonomicName id="63FC6CEE444BFFD3CBBA5320FC31FE88" baseAuthorityName="Hoser" baseAuthorityYear="2015" box="[765,932,260,284]" class="Clitellata" family="Lumbricidae" genus="Proctodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="5" pageNumber="101" phylum="Annelida" rank="species" species="tuberculatus">
