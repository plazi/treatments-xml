<materialsCitation id="14941D30444AFFD2C9D057A6FEDBFA76" ID-GBIF-Occurrence="1914265954" latitude="45.833332" location="Spini" longLatPrecision="1127" longitude="23.1" pageId="4" pageNumber="100" specimenCount="1" stateProvince="Mures">
