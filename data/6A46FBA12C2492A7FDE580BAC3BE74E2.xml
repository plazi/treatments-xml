<taxonomicName id="C5AA2290D9030382A59750C0A42DAC69" class="Polychaeta" family="Onuphidae" genus="Sarsonuphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sarsonuphis fragosa" order="Eunicida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="fragosa">Sarsonuphis fragosa</taxonomicName>
