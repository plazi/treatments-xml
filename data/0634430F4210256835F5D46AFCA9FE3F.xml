<heading id="D56A45754210256A35F5D46AFD1FFBDF" bold="true" box="[151,696,1085,1111]" fontSize="11" level="1" pageId="9" pageNumber="42" reason="1">
<emphasis id="BCE92E0B4210256A35F5D46AFD1FFBDF" bold="true" box="[151,696,1085,1111]" pageId="9" pageNumber="42">
<taxonomicName id="499D899A4210256A35F5D46AFDC9FBDF" box="[151,622,1085,1111]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="42" phylum="Arthropoda" rank="species" species="rutshuruensis" status="sp. n." subGenus="Chaetalictus">
<emphasis id="BCE92E0B4210256A35F5D46AFEA9FBDF" bold="true" box="[151,270,1085,1111]" italics="true" pageId="9" pageNumber="42">Patellapis</emphasis>
(
<emphasis id="BCE92E0B4210256A347DD46AFE11FBDF" bold="true" box="[287,438,1085,1111]" italics="true" pageId="9" pageNumber="42">Chaetalictus</emphasis>
) 
<emphasis id="BCE92E0B4210256A34A5D46AFDC9FBDF" bold="true" box="[455,622,1085,1111]" italics="true" pageId="9" pageNumber="42">rutshuruensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A7DA93704210256A3717D46AFD1FFBDF" box="[629,696,1085,1111]" pageId="9" pageNumber="42" rank="species">sp. n.</taxonomicNameLabel>
