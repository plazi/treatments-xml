<taxonomicName id="4C204D61FFB8947B4F637C2F27C7FCC6" authorityName="Sars" authorityYear="1895" box="[579,748,824,849]" class="Malacostraca" family="Opisidae" genus="Podoprionella" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis id="B954EAF0FFB8947B4F637C2F27C7FCC6" box="[579,748,824,849]" italics="true" pageId="4" pageNumber="26">Podoprionella</emphasis>
</taxonomicName>
. See also remarks under 
<taxonomicName id="4C204D61FFB8947B49337C2F2145FCC6" authorityName="Stoddart &amp; Lowry" authorityYear="2010" box="[1043,1134,824,849]" class="Malacostraca" family="Opisidae" genus="Normanion" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="26" phylum="Arthropoda" rank="species" species="whoi">
