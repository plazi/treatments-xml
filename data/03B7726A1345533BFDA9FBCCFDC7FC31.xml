<taxonomicName id="4C1EB8FF1346533BFC77FD44FC52FD5B" authorityName="Stal" authorityYear="1866" box="[902,980,702,728]" class="Insecta" family="Cixiidae" genus="Andes" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="54" phylum="Arthropoda" rank="genus">Andes</taxonomicName>
