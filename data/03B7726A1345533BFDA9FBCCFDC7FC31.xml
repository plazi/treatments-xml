a.s.l. The 
<quantity id="4CE66E991346533BFF4CFD18FE9DFD7F" box="[189,283,738,764]" metricMagnitude="3" metricUnit="m" metricValue="2.1" pageId="14" pageNumber="15" unit="m" value="2100.0">2100 m</quantity>
