<caption id="DF765FA56455FFE0FF4FF901FD2751C7" ID-DOI="http://doi.org/10.5281/zenodo.4571868" ID-Zenodo-Dep="4571868" box="[151,742,1687,1709]" httpUri="https://zenodo.org/record/4571868/files/figure.png" pageId="95" pageNumber="96" targetBox="[340,1247,193,1666]" targetPageId="95">
<paragraph id="8BB60F2D6455FFE0FF4FF901FD2751C7" blockId="95.[151,742,1687,1709]" box="[151,742,1687,1709]" pageId="95" pageNumber="96">
