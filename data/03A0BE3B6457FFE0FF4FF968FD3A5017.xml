<taxonomicName id="4C0974AE6455FFE0FE62F901FD4051C7" baseAuthorityName="Peters" baseAuthorityYear="1866" box="[442,641,1687,1709]" class="Mammalia" family="Otariidae" genus="Zalophus" kingdom="Animalia" order="Carnivora" pageId="95" pageNumber="96" phylum="Chordata" rank="species" species="japonicus">
<emphasis id="B97DD33F6455FFE0FE62F901FD4051C7" box="[442,641,1687,1709]" italics="true" pageId="95" pageNumber="96">Zalophus japonicus</emphasis>
