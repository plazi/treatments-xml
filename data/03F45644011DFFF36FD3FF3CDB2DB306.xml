<quantity id="4CA54AB7011DFFF36D79FD4BDAAFB6F3" box="[573,659,736,762]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="38" pageNumber="39" unit="mm" value="1.7">1.7mm</quantity>
