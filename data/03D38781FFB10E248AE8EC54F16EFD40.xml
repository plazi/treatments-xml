<collectingCountry id="F36D7607FFB00E248ACBE966F0F0FDB8" box="[180,227,592,618]" name="Iran" pageId="13" pageNumber="14">Iran</collectingCountry>
and 
<collectingCountry id="F36D7607FFB00E248B63E966F1B8FDB8" box="[284,427,592,618]" name="Afghanistan" pageId="13" pageNumber="14">Afghanistan</collectingCountry>
(
<bibRefCitation id="EFEB4B66FFB00E248BC5E966F276FDB8" author="Smetana" box="[442,613,592,618]" pageId="13" pageNumber="24" refString="Smetana, A. (2004) Staphylinidae, subfamilies Omaliinae - Dasycerinae, Phloeocharinae - Apaticinae, Piestinae - Staphylininae. In: Lobl, I. &amp; Smetana, A. (Eds.): Catalogue of Palaearctic Coleoptera. II. Hydrophiloidea - Histeroidea - Staphylinoidea. Apollo Books, Stenstrup, pp. 237 - 272, 329 - 495, 505 - 698." type="book chapter" year="2004">
Smetana 
<number id="3DB224B0FFB00E248858E966F276FDB8" box="[551,613,592,618]" pageId="13" pageNumber="14" value="2004.0">2004</number>
</bibRefCitation>
, 
<bibRefCitation id="EFEB4B66FFB00E24880FE966F314FDB8" author="Zerche" box="[624,775,592,618]" pageId="13" pageNumber="24" refString="Zerche, L. (1999) Die westpalaarktischen Arten der Oxypoda - Untergattung Mycetodrepa Thomson (Coleoptera: Staphylinidae, Aleocharinae, Oxypodini). Beitrage zur Entomologie, Berlin, 49, 261 - 294." type="journal article" year="1999">
Zerche 
<number id="3DB224B0FFB00E2488B7E966F314FDB8" box="[712,775,592,618]" pageId="13" pageNumber="14" value="1999.0">1999</number>
</bibRefCitation>
