<quantity id="D06C0E86FFE3643CDF01FE5E0CF14FB4" box="[365,409,452,478]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="15" pageNumber="16" unit="in" value="1.0">1 in</quantity>
<bibRefCitation id="7305DE92FFE3643CDFF3FE5E0FAC4FB4" author="Lord A. R. &amp; Lambourne D. C." box="[415,708,452,478]" pageId="15" pageNumber="16" pagination="381 - 387" refId="ref44632" refString="Lord A. R. &amp; Lambourne D. C. 1991. Lower Jurassic ostracods from the Western Pontides, Turkey. Geologica Romana 27: 381 - 387." type="journal article" year="1991">
