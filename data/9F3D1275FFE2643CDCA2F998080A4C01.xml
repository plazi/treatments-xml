<taxonomicName id="D094D8E0FFE3643CDAA3FE3B08394FD0" authorityName="Sars" authorityYear="1866" box="[1231,1361,417,442]" class="Ostracoda" family="Candonidae" genus="Paracypris" kingdom="Animalia" order="Podocopida" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
