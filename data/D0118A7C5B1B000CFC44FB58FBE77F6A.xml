<collectingDate id="3C42E4425B1B0003FB58F8A3FAD97F6D" box="[1175,1282,1845,1869]" pageId="23" pageNumber="259">
<date id="2C061DAA5B1B0003FB58F8A3FB117F6D" box="[1175,1226,1845,1869]" pageId="23" pageNumber="259" value="2006-02">Feb-</date>
2006
</collectingDate>
; 16 sequence 
<accessionNumber id="47EBA6895B1B0003FCF0F8C3FC6E7F4D" box="[831,949,1877,1901]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/FN422379" pageId="23" pageNumber="259">FN422379</accessionNumber>
