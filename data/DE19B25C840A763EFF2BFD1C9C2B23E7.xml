<taxonomicName id="91B078C9840A763EFF2BFD1C9D4826FC" authority="Morrison" authorityName="Morrison" box="[151,567,692,718]" class="Insecta" family="Braconidae" genus="Cremnops" kingdom="Animalia" order="Hymenoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="virginiensis">
