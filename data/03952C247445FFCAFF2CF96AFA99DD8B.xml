(genus 
<taxonomicName id="4C3CE6B17444FFCAFA98FBEBFECDDD57" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Carabidae" genus="Bembidion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="355" phylum="Arthropoda" rank="genus">
