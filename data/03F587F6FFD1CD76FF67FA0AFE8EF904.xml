<taxonomicName id="4C5C4D63FFD1CD76FE4FF9BAFF66F9C9" authority="Caudell, 1909" authorityName="Caudell" authorityYear="1909" class="Insecta" family="Acrididae" genus="Orphulella" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="537" phylum="Arthropoda" rank="species" species="losamattensis">
<emphasis id="B928EAF2FFD1CD76FE4FF9BAFDE1F9F3" box="[405,557,1596,1620]" italics="true" pageId="14" pageNumber="537">O. losamattensis</emphasis>
Caudell, 1909
</taxonomicName>
, 
<taxonomicName id="4C5C4D63FFD1CD76FF6CF9D1FE26F9C9" authority="(De Geer, 1773)" baseAuthorityName="De Geer" baseAuthorityYear="1773" box="[182,490,1622,1646]" class="Insecta" family="Acrididae" genus="Orphulella" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="537" phylum="Arthropoda" rank="species" species="punctata">
<emphasis id="B928EAF2FFD1CD76FF6CF9D1FEFCF9C9" box="[182,304,1623,1646]" italics="true" pageId="14" pageNumber="537">O. punctata</emphasis>
(De Geer, 1773)
</taxonomicName>
, 
<taxonomicName id="4C5C4D63FFD1CD76FE2DF9D1FF31F92E" authority="(Walker, 1870)" baseAuthorityName="Walker" baseAuthorityYear="1870" class="Insecta" family="Acrididae" genus="Orphulella" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="537" phylum="Arthropoda" rank="species" species="concinnula">
<emphasis id="B928EAF2FFD1CD76FE2DF9D1FD4BF9C9" box="[503,647,1622,1646]" italics="true" pageId="14" pageNumber="537">O. concinnula</emphasis>
(Walker, 1870)
</taxonomicName>
and 
<taxonomicName id="4C5C4D63FFD1CD76FEF2F9F7FE3AF92E" authority="Otte, 1979" authorityName="Otte" authorityYear="1979" box="[296,502,1649,1673]" class="Insecta" family="Acrididae" genus="Orphulella" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="537" phylum="Arthropoda" rank="species" species="fluvialis">
<emphasis id="B928EAF2FFD1CD76FEF2F9F7FE47F92E" box="[296,395,1649,1673]" italics="true" pageId="14" pageNumber="537">O. fluvialis</emphasis>
<bibRefCitation id="EFCD4B11FFD1CD76FE55F9F7FE3AF92E" author="OTTE D." box="[399,502,1649,1673]" pageId="14" pageNumber="537" pagination="52 - 88" refId="ref11878" refString="OTTE D. 1979. - Revison of the grasshopper tribe Orphulellini (Gomphocerinae: Acrididae). Proceedings of the Academy of Natural Sciences, Philadelphia 131: 52 - 88." type="journal article" year="1979">Otte, 1979</bibRefCitation>
</taxonomicName>
