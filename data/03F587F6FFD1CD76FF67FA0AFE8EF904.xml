<collectingCountry id="F34B7670FFD1CD76FF72F9A7FF3FF99E" box="[168,243,1569,1593]" name="Canada" pageId="14" pageNumber="537">Canada</collectingCountry>
to 
<collectingCountry id="F34B7670FFD1CD76FEC9F9A7FEBAF99E" box="[275,374,1569,1593]" name="Argentina" pageId="14" pageNumber="537">Argentina</collectingCountry>
