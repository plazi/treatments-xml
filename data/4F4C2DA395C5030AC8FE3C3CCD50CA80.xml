<taxonomicName id="6BBA1F0C30182AE8EC1CDBC677BCEE49" LSID="http://zoobank.org/21D6EB7D-FBBC-405B-9E01-01DAC234809C" authority="Lonsdale, 2021" authorityName="Lonsdale" authorityYear="2021" class="Insecta" family="Phytomyzinae" genus="Phytomyza" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phytomyza avicursa" order="Diptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="avicursa" status="sp. nov.">Phytomyza avicursa</taxonomicName>
