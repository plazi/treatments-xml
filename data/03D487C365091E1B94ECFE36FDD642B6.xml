<taxonomicName id="4C7D4D5665091E1A95CEFD0EFDB642FF" authority="Bonaldo &amp; Lise &amp; Ramírez &amp; Saturnino, 2018" authorityName="Bonaldo &amp; Lise &amp; Ramírez &amp; Saturnino" authorityYear="2018" box="[437,551,654,678]" class="Arachnida" family="Miturgidae" genus="Eutichurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="329" phylum="Arthropoda" rank="species" species="cumbia" status="n. sp.">
<emphasis id="B909EAC765091E1A95CEFD0EFDB642FF" box="[437,551,654,678]" italics="true" pageId="8" pageNumber="329">E. cumbia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23A57BC65091E1A964BFD0FFDE142FE" box="[560,624,654,679]" pageId="8" pageNumber="329" rank="species">
<emphasis id="B909EAC765091E1A964BFD0FFDE142FE" bold="true" box="[560,624,654,679]" pageId="8" pageNumber="329">n. sp.</emphasis>
</taxonomicNameLabel>
