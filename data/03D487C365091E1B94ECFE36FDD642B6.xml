<taxonomicName id="4C7D4D5665091E1A9732FD0EFC2C42FF" authorityName="Simon" authorityYear="1897" box="[841,957,654,678]" class="Arachnida" family="Miturgidae" genus="Eutichurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="329" phylum="Arthropoda" rank="species" species="ravidus">
