<taxonomicName id="2EC553EF197F5850221D3FCBA1D2F7E1" authorityName="Baly" authorityYear="1858" box="[582,692,1454,1477]" class="Insecta" family="Chrysomelidae" genus="Melanispa" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="308" phylum="Arthropoda" rank="genus">
