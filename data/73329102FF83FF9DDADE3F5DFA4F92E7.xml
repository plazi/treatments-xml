<taxonomicName id="3C9B5B97FF83FF9DDBD53F5DFD429544" authority="Moure" authorityName="Moure" box="[412,664,586,612]" class="Insecta" family="Apidae" genus="Leurotrigona" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="genus">
