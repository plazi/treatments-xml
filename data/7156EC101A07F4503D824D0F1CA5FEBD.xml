<emphasis id="CB8B81141A07F45F3CE949691EEAF980" box="[508,684,1635,1658]" italics="true" pageId="23" pageNumber="24">
<taxonomicName id="3EFF26851A07F45F3CE949691EE1F980" authorityName="Wheat" baseAuthorityName="Walker" baseAuthorityYear="1856" box="[508,679,1635,1658]" class="Insecta" family="Noctuidae" genus="Dargida" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="diffusa">Dargida diffusa</taxonomicName>
