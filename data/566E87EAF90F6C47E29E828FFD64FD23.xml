<taxonomicName id="19C74D7FF90F6C47E7028024FEE0FDBE" authority="Schuster, 1949" authorityName="Schuster" authorityYear="1949" class="Insecta" family="Mutillidae" genus="Physetopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="588" phylum="Arthropoda" rank="genus">
<emphasis id="ECB3EAEEF90F6C47E7028024FA41FD92" box="[1291,1436,572,596]" italics="true" pageId="3" pageNumber="588">Physetopoda</emphasis>
Schuster, 
<number id="680F24DBF90F6C47E30D8078FEE0FDBE" box="[260,317,608,632]" pageId="3" pageNumber="588" value="1949.0">1949</number>
