<taxonomicName id="D6DF80869472FFC44C4272C4FD5FFF29" baseAuthorityName="Hartman" baseAuthorityYear="1965" box="[539,665,186,209]" class="Polychaeta" family="Nereididae" genus="Rullierinereis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="2" phylum="Annelida" rank="species" species="profunda">
