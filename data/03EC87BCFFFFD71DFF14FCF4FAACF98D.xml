1. Index card entry: Africa, 
<collectingCountry id="F352763AFFFFD71DFDABFA38FD96FA3B" box="[514,579,1421,1447]" name="Democratic Republic of the Congo" pageId="60" pageNumber="61">Zaire</collectingCountry>
, 
<collectingCountry id="F352763AFFFFD71DFDE6FA38FD79FA3B" box="[591,684,1421,1447]" name="Rwanda" pageId="60" pageNumber="61">Ruanda</collectingCountry>
, 
<collectingCountry id="F352763AFFFFD71DFD1EFA38FCC2FA3B" box="[695,791,1421,1447]" name="Burundi" pageId="60" pageNumber="61">Burundi</collectingCountry>
