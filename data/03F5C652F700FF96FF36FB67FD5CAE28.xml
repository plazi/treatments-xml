<taxonomicName id="4C5C0CC7F703FF91FE78F816FD09ACB4" box="[473,681,1970,1992]" class="Insecta" family="Nymphalidae" genus="Faunula" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="62" phylum="Arthropoda" rank="species" species="patagonica">
<emphasis id="B928AB56F703FF91FE78F816FD09ACB4" box="[473,681,1970,1992]" italics="true" pageId="2" pageNumber="62">Faunula patagonica</emphasis>
</taxonomicName>
male; E. 
<taxonomicName id="4C5C0CC7F703FF91FCAFF816FC12ACB4" box="[782,946,1970,1992]" class="Insecta" family="Nymphalidae" genus="Faunula" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="62" phylum="Arthropoda" rank="species" species="eleates">
<emphasis id="B928AB56F703FF91FCAFF816FC12ACB4" box="[782,946,1970,1992]" italics="true" pageId="2" pageNumber="62">Faunula eleates</emphasis>
</taxonomicName>
male; F. 
<taxonomicName id="4C5C0CC7F703FF91FBB4F816FB40ACB4" box="[1045,1248,1970,1992]" class="Insecta" family="Nymphalidae" genus="Faunula" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="62" phylum="Arthropoda" rank="species" species="leucoglene">
<emphasis id="B928AB56F703FF91FBB4F816FB40ACB4" box="[1045,1248,1970,1992]" italics="true" pageId="2" pageNumber="62">Faunula leucoglene</emphasis>
