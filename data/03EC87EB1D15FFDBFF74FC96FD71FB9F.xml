<collectingCountry id="F352766D1D15FFDBFF74FC21FF51FC5A" box="[178,242,994,1015]" name="Oman" pageId="32" pageNumber="939">Oman</collectingCountry>
, 
<collectingCountry id="F352766D1D15FFDBFEC6FC21FE2FFC55" box="[256,396,994,1016]" name="Saudi Arabia" pageId="32" pageNumber="939">Saudi Arabia</collectingCountry>
