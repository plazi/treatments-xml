<emphasis id="B912EA8DFFB0071AFF22FF2E14F5E7DA" bold="true" box="[151,246,189,215]" pageId="16" pageNumber="17">
<taxonomicNameLabel id="A22157F6FFB0071AFF22FF2E14F5E7DA" box="[151,246,189,215]" pageId="16" pageNumber="17" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C664D1CFFB0071AFEB0FF2E1568E7DB" box="[261,363,189,214]" class="Arachnida" family="Sparassidae" genus="Caayguara" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="ajuba" status="sp. nov.">
