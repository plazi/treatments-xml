<taxonomicName id="920F21E2B87F35B3ED7B9B4BC7001D15" class="Insecta" family="Staphylinidae" genus="Scaphidium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Scaphidium" order="Coleoptera" pageId="17" pageNumber="64" phylum="Ascomycota" rank="genus">Scaphidium</taxonomicName>
. 38, 39 
<taxonomicName id="A68525C8B9FA1F5B11E845815489466A" class="Insecta" family="Staphylinidae" genus="Scaphidium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Scaphidium biwenxuani" order="Coleoptera" pageId="17" pageNumber="64" phylum="Ascomycota" rank="species" species="biwenxuani">Scaphidium biwenxuani</taxonomicName>
(Zhejiang) 40, 41 
<taxonomicName id="8EC0D88B03266001D534033E750BA1E7" class="Insecta" family="Staphylinidae" genus="Scaphidium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Scaphidium robustum" order="Coleoptera" pageId="17" pageNumber="64" phylum="Ascomycota" rank="species" species="robustum">Scaphidium robustum</taxonomicName>
