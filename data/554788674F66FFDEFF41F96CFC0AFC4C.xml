<emphasis id="EF9AE5634F79FFDFFF41F835FF53F85F" bold="true" box="[151,237,1973,1997]" pageId="16" pageNumber="17">
MAP 
<number id="6B262B564F79FFDFFF0CF835FF56F85F" box="[218,232,1973,1997]" pageId="16" pageNumber="17" value="5.0">5</number>
.
</emphasis>
Distribution of 
<taxonomicName id="1AEE42F24F79FFDFFE41F836FCABF85F" authority="Kimsey" authorityName="Kimsey" box="[407,789,1974,1997]" class="Insecta" family="Tiphiidae" genus="Anthobosca" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="namorokaensis" status="new species">
<emphasis id="EF9AE5634F79FFDFFE41F836FD05F85F" box="[407,699,1974,1997]" italics="true" pageId="16" pageNumber="17">Anthobosca namorokaensis</emphasis>
Kimsey
</taxonomicName>
, 
<taxonomicNameLabel id="F4A958184F79FFDFFCC9F836FC1CF85F" box="[799,930,1974,1997]" pageId="16" pageNumber="17" rank="species">new species</taxonomicNameLabel>
, 
<taxonomicName id="1AEE42F24F79FFDFFC7AF836FAB2F85F" authority="Kimsey" authorityName="Kimsey" box="[940,1292,1974,1997]" class="Insecta" family="Tiphiidae" genus="Anthobosca" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="nigrimacula" status="new species">
<emphasis id="EF9AE5634F79FFDFFC7AF836FB0FF85F" box="[940,1201,1974,1997]" italics="true" pageId="16" pageNumber="17">Anthobosca nigrimacula</emphasis>
Kimsey
</taxonomicName>
, 
<taxonomicNameLabel id="F4A958184F79FFDFFAC0F836FA26F85F" box="[1302,1432,1974,1997]" pageId="16" pageNumber="17" rank="species">new species</taxonomicNameLabel>
, and 
<taxonomicName id="1AEE42F24F79FFDFFF12F859FD9FF87D" authority="Kimsey" authorityName="Kimsey" box="[196,545,2008,2032]" class="Insecta" family="Tiphiidae" genus="Anthobosca" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="toliaraensis" status="new species">
<emphasis id="EF9AE5634F79FFDFFF12F859FE78F862" box="[196,454,2009,2032]" italics="true" pageId="16" pageNumber="17">Anthobosca toliaraensis</emphasis>
