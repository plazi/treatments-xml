<taxonomicName id="E02624D4249B2DD1A9D1F997B01047C4" class="Insecta" family="Pyralidae" genus="Teliphasa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Teliphasa amica" order="Lepidoptera" pageId="4" pageNumber="123" phylum="Arthropoda" rank="species" species="amica">Teliphasa amica</taxonomicName>
