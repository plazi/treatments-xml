<taxonomicName id="CDEE4D1F2B39FFBBFECBF99C68E8F15C" authority="Ruebush 1938" authorityName="Ruebush" authorityYear="1938" box="[301,747,1632,1655]" class="Rhabditophora" family="Typhloplanidae" genus="Krumbachia" kingdom="Animalia" order="Rhabdocoela" pageId="8" pageNumber="9" phylum="Platyhelminthes" rank="species" species="opisthoformis">
