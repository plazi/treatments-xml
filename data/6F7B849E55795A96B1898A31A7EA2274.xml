<emphasis id="5D43C3FE9CA54B472881A05BB748C335" italics="true" pageId="0" pageNumber="1">Chamalycaeus</emphasis>
&quot; 
<taxonomicName id="9B4C5940751068125714DEEA528C3966" class="Gastropoda" family="Alycaeidae" genus="Dicharax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="helicodes" order="Architaenioglossa" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="helicodes">
<emphasis id="3294CA85183430AF951F6BA882AEB340" italics="true" pageId="0" pageNumber="1">helicodes</emphasis>
</taxonomicName>
(= synonym of 
<taxonomicName id="B6A190C631DF67E85DBD35B516244B25" baseAuthorityName="Heude" baseAuthorityYear="1885" class="Gastropoda" family="Alycaeidae" genus="Metalycaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metalycaeus muciferus" order="Architaenioglossa" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="muciferus">
