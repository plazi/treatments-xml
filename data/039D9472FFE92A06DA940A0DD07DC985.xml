<taxonomicName id="4C345EE7FFE92A06DBDB0E23D13CC968" box="[472,599,1580,1603]" class="Polychaeta" family="Sabellidae" genus="Pseudobranchiomma" kingdom="Animalia" order="Sabellida" pageId="50" pageNumber="148" phylum="Annelida" rank="species" species="punctata">
<emphasis id="B940F976FFE92A06DBDB0E23D13CC968" box="[472,599,1580,1603]" italics="true" pageId="50" pageNumber="148">P. punctata</emphasis>
