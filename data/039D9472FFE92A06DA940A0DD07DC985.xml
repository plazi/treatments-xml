<taxonomicName id="4C345EE7FFE92A06DEDF0E23D625C968" box="[1244,1358,1580,1603]" class="Polychaeta" family="Sabellidae" genus="Pseudobranchiomma" kingdom="Animalia" order="Sabellida" pageId="50" pageNumber="148" phylum="Annelida" rank="species" species="minima">
<emphasis id="B940F976FFE92A06DEDF0E23D625C968" box="[1244,1358,1580,1603]" italics="true" pageId="50" pageNumber="148">P. minima</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C345EE7FFE92A06DF840E23D1D2C94C" authority="Knight-Jones &amp; Giangrande 2003" authorityName="Knight-Jones &amp; Giangrande" authorityYear="2003" class="Polychaeta" family="Sabellidae" genus="Pseudobranchiomma" kingdom="Animalia" order="Sabellida" pageId="50" pageNumber="148" phylum="Annelida" rank="species" species="tarantoensis">
<emphasis id="B940F976FFE92A06DF840E23D249C94D" italics="true" pageId="50" pageNumber="148">P. tarantoensis</emphasis>
