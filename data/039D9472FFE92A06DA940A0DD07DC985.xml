<taxonomicName id="4C345EE7FFE92A06D9B80E07D75AC90B" box="[955,1073,1543,1568]" class="Polychaeta" family="Sabellidae" genus="Pseudobranchiomma" kingdom="Animalia" order="Sabellida" pageId="50" pageNumber="148" phylum="Annelida" rank="species" species="perkinsi">
