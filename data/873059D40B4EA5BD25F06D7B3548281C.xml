<paragraph id="70A8938C2A8A99160A8171946F1F623F" pageId="116" pageNumber="117">
<taxonomicName id="22D2D08453D750CC33D4A72263B694A0" authority="van Achterberg &amp; Long, 2010" authorityName="van Achterberg &amp; Long" authorityYear="2010" class="Insecta" family="Braconidae" genus="Zelodia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zelodia exornata" order="Hymenoptera" pageId="116" pageNumber="117" phylum="Arthropoda" rank="species" species="exornata">Zelodia exornata</taxonomicName>
