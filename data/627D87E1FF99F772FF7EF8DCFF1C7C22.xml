<taxonomicName id="2DD44D74FF99F77DFF28F8DCFE0E7A69" authorityName="Oláh &amp; Johanson &amp; Barnard" authorityYear="2008" box="[193,478,1863,1889]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="ayopa">
