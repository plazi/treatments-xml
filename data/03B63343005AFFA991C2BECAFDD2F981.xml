<taxonomicName id="4C1FF9D6005AFFA99010B8F3FE47FA3C" authorityName="Kononenko &amp; Ronkay" authorityYear="2000" box="[325,445,1424,1448]" class="Insecta" family="Noctuidae" genus="Stenoloba" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="315" phylum="Arthropoda" rank="species" species="domina">
<emphasis id="B96B5E47005AFFA99010B8F3FEA9FA33" box="[325,339,1424,1447]" italics="true" pageId="14" pageNumber="315">S</emphasis>
