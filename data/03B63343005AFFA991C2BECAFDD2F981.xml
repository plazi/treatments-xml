<taxonomicName id="4C1FF9D6005AFFA994D2B80FFEFBFA3C" authorityName="Kononenko &amp; Ronkay" authorityYear="2001" class="Insecta" family="Noctuidae" genus="Stenoloba" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="315" phylum="Arthropoda" rank="species" species="dominula">
