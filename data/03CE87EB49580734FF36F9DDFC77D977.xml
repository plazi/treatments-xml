<date id="FFD9103D495E0734FE43F886FD84D977" box="[482,588,1806,1832]" pageId="45" pageNumber="46" value="2267-02-02">2267 2/2</date>
, 
<typeStatus id="54DC885F495E0734FDF9F886FD75D977" box="[600,701,1806,1832]" pageId="45" pageNumber="46">paratype</typeStatus>
