area (
<bibRefCitation id="EFB84B53FFFF8920FB97FE4B77B59B74" author="Koehler, R." box="[1026,1185,511,536]" pageId="85" pageNumber="86" refId="ref34913" refString="Koehler, R. (1914) Echinoderma of the Indian Museum, Part 8, An Account of the Echinoidea I. Spatangides. Trustees of the Indian Museum, Calcutta, 258 pp." type="book" year="1914">Koehler 1914</bibRefCitation>
