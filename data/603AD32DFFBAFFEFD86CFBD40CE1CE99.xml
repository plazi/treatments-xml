</emphasis>
<specimenCode id="B835CA40FFBAFFEED88FFB2E0853CF89" box="[925,1096,1244,1266]" collectionCode="FMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="22" pageNumber="97" type="Museum">FMNH 282358</specimenCode>
