<specimenCode id="B835CA40FFBAFFEEDFCDF9E60996CD52" box="[1247,1421,1556,1577]" collectionCode="FMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="22" pageNumber="97" type="Museum">FMNH 282193</specimenCode>
