</emphasis>
<taxonomicName id="0CB5F6F204444820FEF8FA5AFC02BFB7" authority="Waterhouse, 1881: 553" authorityName="Waterhouse" authorityPageNumber="553" authorityYear="1881" box="[383,902,1441,1466]" class="Insecta" family="Rutelidae" genus="Chlorota" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="associata">
