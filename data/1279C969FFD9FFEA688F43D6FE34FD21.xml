<taxonomicName id="5DD003FCFFD8FFEA6934414DFC29FB0F" authority="nigromarginata (Karny, 1928)" authorityName="nigromarginata (Karny" authorityYear="1928" baseAuthorityName="Karny" baseAuthorityYear="1928" box="[300,979,869,894]" class="Insecta" family="Gryllacrididae" genus="Capnogryllacris" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="subSpecies" species="nigromarginata" subSpecies="nigromarginata">
