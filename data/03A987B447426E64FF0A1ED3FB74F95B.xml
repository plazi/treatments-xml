<taxonomicName id="4C004D2147426E64FCA91D1BFBDEF9B6" authority="Meigen" authorityName="Meigen" authorityYear="1804" box="[820,1150,1544,1566]" class="Insecta" family="Ceratopogonidae" genus="Ceratopogon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="560" phylum="Arthropoda" rank="species" species="communis">
