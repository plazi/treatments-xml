<taxonomicName id="8ACCB1C7FF27B3A84784F86ACD370C94" authority="(Benedict, 1892)" baseAuthorityName="Benedict" baseAuthorityYear="1892" box="[962,1329,1954,1976]" class="Malacostraca" family="Paguridae" genus="Pagurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="187" pageNumber="188" phylum="Arthropoda" rank="species" species="hemphilli">
