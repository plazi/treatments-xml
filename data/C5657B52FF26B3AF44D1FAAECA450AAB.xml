<taxonomicName id="8ACCB1C7FF27B3A8462FF8F6CBFA0C78" authority="(Benedict, 1892)" baseAuthorityName="Benedict" baseAuthorityYear="1892" box="[617,1020,1854,1876]" class="Malacostraca" family="Paguridae" genus="Pagurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="187" pageNumber="188" phylum="Arthropoda" rank="species" species="confragosus">
