</emphasis>
<taxonomicName id="AAD74D26040CFFB63A112D5D91BD5032" authority="Fabricius, 1798" authorityName="Fabricius" authorityYear="1798" box="[288,648,541,565]" class="Malacostraca" family="Cancridae" genus="Cancer" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="747" phylum="Arthropoda" rank="species" species="quadratus">
