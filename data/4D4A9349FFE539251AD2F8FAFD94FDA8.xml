<taxonomicName id="02E359DCFFE239251E68FF42FAAEFF24" authority="Grogan &amp; Wirth" authorityName="Grogan &amp; Wirth" box="[1069,1365,151,177]" class="Insecta" family="Ceratopogonidae" genus="Macrurohelea" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="450" phylum="Arthropoda" rank="species" species="irwini">
