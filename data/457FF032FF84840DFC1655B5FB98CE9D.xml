<taxonomicName id="0AD63AA7FF84840DFB11529CFB27CE30" box="[1195,1243,1268,1291]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="12" pageNumber="141" phylum="Mollusca" rank="genus">
