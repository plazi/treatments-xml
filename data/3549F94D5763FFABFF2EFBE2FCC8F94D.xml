<taxonomicName id="7AE033D85765FFABFF50FBA9FEC8F94A" box="[233,347,1106,1131]" class="Diplopoda" family="Julidae" genus="Sibiriulus" kingdom="Animalia" order="Julida" pageId="5" pageNumber="62" phylum="Arthropoda" rank="genus">
