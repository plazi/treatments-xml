<number id="3D8AD33EFFB3FFACA59FFD65FCABF4BF" box="[670,796,655,679]" pageId="2" pageNumber="423" value="2011.0" valueMax="2012.0" valueMin="2010.0">
<date id="FFFCE7D9FFB3FFACA59FFD65FCABF4BF" box="[670,796,655,679]" pageId="2" pageNumber="423" value="2010" valueMax="2012">2010–2012</date>
</number>
