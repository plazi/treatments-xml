<quantity id="4CBA6CFCFFB3FFACA62DFD65FE34F4BF" box="[300,387,655,679]" metricMagnitude="3" metricUnit="m" metricValue="1.9" pageId="2" pageNumber="423" unit="m" value="1900.0">1900 m</quantity>
