(
<bibRefCitation id="EFEF4B1EFF84FC56FEED1421FE6974B8" author="Macfarlane J. M." box="[304,506,637,663]" pageId="15" pageNumber="16" pagination="207 - 210" refId="ref14602" refString="Macfarlane J. M. 1911. New species of Nepenthes. Transactions and Proceedings of the Botanical Society of Pennsylvania 3: 207 - 210." type="journal article" year="1911">Macfarlane 1911</bibRefCitation>
) and 
<taxonomicName id="4C7E4D6CFF84FC56FDE61422FCA074B8" authority="Elmer" authorityName="Elmer" authorityYear="1912" box="[571,819,637,663]" class="Magnoliopsida" family="Nepenthaceae" genus="Nepenthes" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="graciliflora">
<emphasis id="B90AEAFDFF84FC56FDE61422FD7774B8" box="[571,740,637,663]" italics="true" pageId="15" pageNumber="16">N. graciliflora</emphasis>
Elmer
</taxonomicName>
(
<bibRefCitation id="EFEF4B1EFF84FC56FC9C1421FC4274B8" author="Elmer A. D. E." box="[833,977,637,663]" pageId="15" pageNumber="16" pagination="1494 - 1496" refId="ref13693" refString="Elmer A. D. E. 1912. Two Score of New Plants. Nepenthaceae. Leaflets of Philippine Botany 4: 1494 - 1496." type="journal article" year="1912">Elmer 1912</bibRefCitation>
). Elevation: ca. 
<specimenCount id="9D78FD66FF84FC56FB531421FB7374B7" box="[1166,1248,637,664]" pageId="15" pageNumber="16" type="generic">
<quantity id="4C869B0AFF84FC56FB531421FB4F74B7" box="[1166,1244,637,664]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="15" pageNumber="16" unit="m" value="400.0">400 m</quantity>
.
</specimenCount>
</paragraph>
