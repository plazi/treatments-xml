<taxonomicName id="37E0E0A74D44D418F8695343755B8D89" LSID="http://zoobank.org/39028CA9-4ABA-43C6-8845-457E9F88D7B9" authority="Sharkey" authorityName="Sharkey" authorityYear="2018" class="Insecta" family="Braconidae" genus="Zosteragathis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zosteragathis sakaeratensis" order="Hymenoptera" pageId="6" pageNumber="231" phylum="Arthropoda" rank="species" species="sakaeratensis" status="sp. n.">Zosteragathis sakaeratensis Sharkey</taxonomicName>
<taxonomicNameLabel id="7432CD1DAF71B7332DE0F2A3CBC2B199" pageId="6" pageNumber="231">sp. n.</taxonomicNameLabel>
