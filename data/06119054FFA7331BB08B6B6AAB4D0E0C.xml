[1] 
<bibRefCitation id="EA295CB3FFA7331BB1286BB0A8480DFF" author="Touleshkov, K." box="[308,536,1411,1437]" pageId="37" pageNumber="38" pagination="207 - 228" refId="ref27892" refString="Touleshkov, K. (1974) Mallophagen auf den Vogeln des Balkangebirges. Izvestiya na Zoologicheskiya Institut s Muzey, 41, 207 - 228 (In Bulgarian)." type="journal article" year="1974">Touleshkov (1974)</bibRefCitation>
. 
<emphasis id="BCCCFD50FFA7331BB08B6B9EAAB60DA5" bold="true" box="[151,230,1453,1479]" pageId="37" pageNumber="38">Hosts:</emphasis>
<taxonomicName id="49B85AC1FFA7331BB0F26B9EAB3A0DA4" authorityName="Linnaeus" authorityYear="1758" box="[238,362,1453,1478]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="ater">
