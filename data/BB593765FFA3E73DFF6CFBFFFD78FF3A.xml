<taxonomicName id="F4F0FDF0FFA3E73BFF6CFBFFFEF3FBF5" authorityName="Savigny" authorityYear="1816" box="[151,263,1051,1078]" class="Ascidiacea" family="Polyclinidae" genus="Aplidium" kingdom="Animalia" order="Enterogona" pageId="10" pageNumber="11" phylum="Chordata" rank="genus">
