<taxonomicName id="98DF4D12FFAB0D24197BFD4B17BEFD11" authorityName="White" authorityYear="1879" box="[823,929,726,752]" class="Insecta" family="Nepidae" genus="Ranatra" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="473" phylum="Arthropoda" rank="species" species="rabida">
<emphasis id="6DABEA83FFAB0D24197BFD4B17BEFD11" box="[823,929,726,752]" italics="true" pageId="22" pageNumber="473">R. rabida</emphasis>
</taxonomicName>
and 
<emphasis id="6DABEA83FFAB0D24199BFD4B107EFD11" box="[983,1121,727,752]" italics="true" pageId="22" pageNumber="473">
<taxonomicName id="98DF4D12FFAB0D24199BFD4B1043FD11" authorityName="Montandon" authorityYear="1905" box="[983,1116,727,752]" class="Insecta" family="Nepidae" genus="Ranatra" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="473" phylum="Arthropoda" rank="species" species="signoreti">R. signoreti</taxonomicName>
.
