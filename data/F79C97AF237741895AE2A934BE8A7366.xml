<taxonomicName id="036CE95A99BB352E395037CEE730E021" authority="Moore, 1888" authorityName="Moore" authorityYear="1888" class="Insecta" family="Crambidae" genus="Patania" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Patania ruralis" order="Lepidoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="species" species="ruralis">Patania ruralis</taxonomicName>
