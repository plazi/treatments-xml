</emphasis>
<taxonomicName id="4D8AA8DBFFB29233263DFBC25BC5FBE8" authority="Kondorosy, 2017" authorityName="Kondorosy" authorityYear="2017" box="[268,476,1105,1128]" class="Insecta" family="Rhyparochromidae" genus="Latidrymus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="48" phylum="Arthropoda" rank="species" species="puskasi" status="sp. nov.">
