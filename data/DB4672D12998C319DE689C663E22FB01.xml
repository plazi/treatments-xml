<taxonomicName id="ADCBD7911AAF32C39096FA714E439CE1" class="Arachnida" family="Linyphiidae" genus="Glomerosus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glomerosus lateralis" order="Araneae" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="lateralis">Glomerosus lateralis</taxonomicName>
