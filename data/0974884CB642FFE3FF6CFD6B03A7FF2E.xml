(
<taxonomicName id="46DD42D9B642FFE0FD5AFB8A0104FB9E" authorityName="" authorityYear="1789" box="[673,847,1071,1096]" class="Magnoliopsida" family="Euphorbiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="49" pageNumber="50" phylum="Magnoliopsida" rank="family">Euphorbiaceae</taxonomicName>
