<collectionCode id="2EDB51B9FF9EFFCDFCEB13755CE7971E" LSID="urn:lsid:biocol.org:col:34871" box="[881,966,582,606]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="48" pageNumber="49">USNM</collectionCode>
