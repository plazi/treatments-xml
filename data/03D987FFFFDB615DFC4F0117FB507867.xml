<taxonomicName id="4C704D6AFFDB615DFB820117FB667E50" authority="Carriol" authorityName="Carriol &amp; Bonde &amp; Jakobsen &amp; Høeg" authorityYear="2016" box="[1037,1222,853,879]" class="Hexanauplia" family="Neolepadidae" genus="Stipilepas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scalpelliformes" pageId="2" pageNumber="23" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="B904EAFBFFDB615DFB820117FBCF7E50" box="[1037,1135,853,879]" italics="true" pageId="2" pageNumber="23">Stipilepas</emphasis>
Carriol
</taxonomicName>
