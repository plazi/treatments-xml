<subSubSection id="C3196557FF4CFF18FD68FCCAFC034D65" pageId="167" pageNumber="166" type="description">
<paragraph id="8BBC36DCFF4CFF18FD68FCCAFC244B42" blockId="167.[695,1248,276,1401]" box="[695,967,841,862]" pageId="167" pageNumber="166">
<emphasis id="B977EACEFF4CFF18FD68FCCAFC8A4B41" bold="true" box="[695,873,841,861]" pageId="167" pageNumber="166">Species group:</emphasis>
