<geoCoordinate id="17D5502B244CBD15FCADDFF01847A2CD" box="[781,888,434,460]" degrees="75" direction="west" minutes="05" orientation="longitude" pageId="82" pageNumber="83" precision="925" value="-75.083336">75°05’W</geoCoordinate>
