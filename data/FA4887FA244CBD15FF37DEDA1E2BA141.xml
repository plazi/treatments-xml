<geoCoordinate id="17D5502B244CBD15FDBDDFF0194BA2CD" box="[541,628,434,460]" degrees="7" direction="north" minutes="22" orientation="latitude" pageId="82" pageNumber="83" precision="925" value="7.366667">7°22’N</geoCoordinate>
