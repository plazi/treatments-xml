<taxonomicName id="F84A7EE12DEB1BDE4741ABE8FA138CA0" authorityName="LeConte" authorityYear="1884" class="Hexapoda" family="Carabidae" genus="Cychrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="155" pageNumber="156" phylum="Arthropoda" rank="species" species="rickseckeri">Cychrus rickseckeri</taxonomicName>
