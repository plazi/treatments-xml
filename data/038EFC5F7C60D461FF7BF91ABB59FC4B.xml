and 
<taxonomicName id="4C2736CA7C65D461FDB4FD34BB93FD1F" box="[600,739,729,752]" class="Insecta" family="Platygastridae" genus="Trichacis" kingdom="Animalia" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="acarinata">
