<taxonomicName id="B1EAE4994D782CBB4C507EC9DFD34412" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" phylum="Tracheophyta" rank="species" species="sylvatica">Stachys sylvatica</taxonomicName>
