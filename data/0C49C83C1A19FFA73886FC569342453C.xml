<quantity id="4318D4CF1A19FFA73963FBC690FA4550" box="[370,398,1052,1076]" metricMagnitude="-3" metricUnit="kg" metricValue="2.0" pageId="44" pageNumber="45" unit="g" value="2.0">2g</quantity>
