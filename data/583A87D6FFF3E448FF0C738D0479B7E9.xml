<taxonomicName id="17934D43FFF3E448FF75738D0422B2AF" authority="Heyns, 1965" authorityName="Heyns" authorityYear="1965" box="[238,593,221,247]" class="Adenophorea" family="Aporcelaimidae" genus="Aporcelaimellus" kingdom="Animalia" order="Dorylaimida" pageId="1" pageNumber="244" phylum="Nematoda" rank="genus">
