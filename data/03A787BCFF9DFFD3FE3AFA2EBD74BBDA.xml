<paragraph id="8BB136AAFF9DFFD5FFEAFA72BE97B872" blockId="11.[92,718,1519,1541]" box="[92,718,1519,1541]" pageId="11" pageNumber="136">
<treatmentCitationGroup id="AB1E1184FF9DFFD5FFEAFA72BE97B872" box="[92,718,1519,1541]" pageId="11" pageNumber="136">
<treatmentCitation id="0AAF10BBFF9DFFD5FFEAFA72BE5DB872" author="BEZZI, M." box="[92,516,1519,1541]" page="344" pageId="11" pageNumber="136" year="1924">
