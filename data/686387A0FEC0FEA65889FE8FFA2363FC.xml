<taxonomicName id="27CA4D35FEC0FEA65A69FE72FCE06360" authorityName="Rondani" authorityYear="1857" box="[631,851,511,537]" class="Insecta" family="Syrphidae" genus="Syrphus" kingdom="Animalia" order="Diptera" pageId="269" pageNumber="270" phylum="Arthropoda" rank="species" species="bucculatus">
