<taxonomicName id="3BC54D02FFFAD005A9C8FA79FF66901D" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="12" pageNumber="453" phylum="Tracheophyta" rank="species" species="pumilio">
