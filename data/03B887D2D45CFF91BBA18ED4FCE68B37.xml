<taxonomicName id="4C114D47D45CFF91BA28893AFD848968" authority="Matsumura, 1932" authorityName="Matsumura" authorityYear="1932" box="[286,625,1063,1084]" class="Insecta" family="Cicadellidae" genus="Zygina" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="445" phylum="Arthropoda" rank="species" species="nigricans">
