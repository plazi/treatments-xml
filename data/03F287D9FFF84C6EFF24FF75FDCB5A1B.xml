<specimenCount id="9D5DFD46FFF84C6EFD73FEEEFC92593B" box="[704,859,258,283]" pageId="25" pageNumber="226" type="generic">
<number id="3D9324E8FFF84C6EFD73FEEEFD15593B" box="[704,732,259,283]" pageId="25" pageNumber="226" value="19.0">19</number>
