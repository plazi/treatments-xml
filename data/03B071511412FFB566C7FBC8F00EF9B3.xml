<quantity id="4CE16DA21412FFB5640BFA42F0BEFA27" box="[603,699,1413,1437]" metricMagnitude="-1" metricUnit="m" metricValue="3.65" pageId="7" pageNumber="8" unit="mm" value="365.0">365 mm</quantity>
