<collectingCountry id="0BBA76162B233C3639F7E9E9FEBFF87B" box="[151,259,1994,2019]" name="Indonesia" pageId="7" pageNumber="278">Indonesia</collectingCountry>
(
<collectingRegion id="B169F8642B233C363871E9E8FED3F87B" box="[273,367,1995,2019]" country="Indonesia" name="Sumatera Utara" pageId="7" pageNumber="278">Sumatra</collectingRegion>
