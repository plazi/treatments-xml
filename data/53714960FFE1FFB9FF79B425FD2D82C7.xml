<taxonomicName id="1CD883F5FFE1FFB9FCE8B316FC10821F" box="[774,931,1038,1062]" class="Insecta" family="Tortricidae" genus="Olethreutinae" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Lepidoptera" pageId="7" pageNumber="35" phylum="Arthropoda" rank="genus">Olethreutinae</taxonomicName>
