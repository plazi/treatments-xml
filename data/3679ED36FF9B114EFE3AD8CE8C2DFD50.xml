<taxonomicName id="79D027A3FF9B114EFD9DDA058CF2FD8E" authority="Cresson, 1865" authorityName="Cresson" authorityYear="1865" box="[611,950,581,606]" class="Insecta" family="Sphecidae" genus="Podium" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="2939" phylum="Arthropoda" rank="species" species="fulvipes">
