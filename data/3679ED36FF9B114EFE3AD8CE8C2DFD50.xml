<taxonomicName id="79D027A3FF9B114EFD2BDA628CC4FDEB" authorityName="F. Smith" authorityYear="1856" box="[725,896,546,571]" class="Insecta" family="Sphecidae" genus="Podium" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="2939" phylum="Arthropoda" rank="species" species="denticulatum">
