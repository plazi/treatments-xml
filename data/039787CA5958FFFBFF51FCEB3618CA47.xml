, 
<collectingCountry id="F329764C5958FFFBFF51FB6A32D4C883" box="[151,222,1217,1243]" name="Brazil" pageId="52" pageNumber="53">Brazil</collectingCountry>
, 
<collectingCountry id="F329764C5958FFFBFF2AFB6A3341C883" box="[236,331,1217,1243]" name="Ecuador" pageId="52" pageNumber="53">Ecuador</collectingCountry>
, 
<collectingCountry id="F329764C5958FFFBFE9FFB6933BFC884" box="[345,437,1218,1244]" name="Panama" pageId="52" pageNumber="53">Panama</collectingCountry>
