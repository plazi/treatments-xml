<collectionCode id="8AD5AE688E26FE3C28F0F9D1467E9071" box="[1234,1328,1627,1653]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="6" pageNumber="7">MNHN</collectionCode>
