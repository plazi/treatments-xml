<subSubSection id="4DA9652D4C318608FF0AFC1EFBB8FAB3" box="[197,1125,908,933]" pageId="8" pageNumber="65" type="etymology">
<paragraph id="050C36A64C318608FF0AFC1EFBB8FAB3" blockId="8.[151,1436,151,933]" box="[197,1125,908,933]" pageId="8" pageNumber="65">
<emphasis id="37C7EAB44C318608FF0AFC1EFE96FAB3" bold="true" box="[197,331,908,932]" pageId="8" pageNumber="65">Etymology.</emphasis>
