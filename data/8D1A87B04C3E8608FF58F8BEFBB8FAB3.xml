<specimenCount id="13B5FD2F4C318608FE45FCD6FE24FA4B" box="[394,505,836,860]" pageId="8" pageNumber="65" type="female">
<number id="B37B24814C318608FE45FCD6FE45FA4B" box="[394,408,836,860]" pageId="8" pageNumber="65" value="2.0">2</number>
females
</specimenCount>
, Nankunshan, Huizhou, 
<date id="710D10664C318608FCC2FCD6FCA2FA4B" box="[781,895,836,860]" pageId="8" pageNumber="65" value="2004-10-21">
