, 
<quantity id="642F0BC5773DFFFCFDBEFB8D09C04E30" box="[615,753,1076,1103]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" metricValueMax="4.6" metricValueMin="4.2" pageId="6" pageNumber="387" unit="mm" value="4.4" valueMax="4.6" valueMin="4.2">4.2–4.6 mm</quantity>
; 
<specimenCount id="B5D16DA9773DFFFCFD24FB8C08234E30" box="[765,786,1077,1103]" pageId="6" pageNumber="387" type="female">♀</specimenCount>
