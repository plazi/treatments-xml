<taxonomicName id="AD845E95FFBE7E6193126A9AFB9AFC7C" authorityName="Linnaeus" authorityYear="1758" box="[842,1033,983,1007]" class="Actinopterygii" family="Muraenidae" genus="Muraena" kingdom="Animalia" order="Anguilliformes" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="helena">
