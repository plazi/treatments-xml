<taxonomicName id="AD845E95FFBE7E61940F6D67FB32FBAE" authorityName="Linnaeus" authorityYear="1758" box="[1111,1185,1061,1085]" class="Actinopterygii" family="Muraenidae" genus="Muraena" kingdom="Animalia" order="Anguilliformes" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="helena">
<emphasis id="58F0F904FFBE7E61940F6D67FB32FBAE" box="[1111,1185,1061,1085]" italics="true" pageId="29" pageNumber="30">helena</emphasis>
</taxonomicName>
