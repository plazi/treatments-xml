<taxonomicName id="AD845E95FFBE7E61933C6ABCFC63FB85" authorityName="Linnaeus" authorityYear="1758" box="[868,1008,1022,1046]" class="Actinopterygii" family="Muraenidae" genus="Muraena" kingdom="Animalia" order="Anguilliformes" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="anguilla">
<emphasis id="58F0F904FFBE7E61933C6ABCFC63FB85" box="[868,1008,1022,1046]" italics="true" pageId="29" pageNumber="30">M. anguilla</emphasis>
</taxonomicName>
