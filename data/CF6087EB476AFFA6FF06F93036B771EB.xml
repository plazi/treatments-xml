<taxonomicName id="80C94D7E476DFFA6FF67FF7C3129739D" authority="Carpintero &amp; Cherot" authorityName="Carpintero &amp; Cherot" box="[246,818,188,212]" class="Insecta" family="Miridae" genus="Carvalocapsus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="347" phylum="Arthropoda" rank="species" species="scutellosum">
