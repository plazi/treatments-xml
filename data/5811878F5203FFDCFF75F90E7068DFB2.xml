<caption id="84C766115202FFDFFF75F927774CD99E" httpUri="https://zenodo.org/record/275683/files/figure.png" pageId="1" pageNumber="64" targetBox="[568,1011,1065,1648]" targetPageId="1">
<paragraph id="D00736995202FFDFFF75F927774CD99E" blockId="1.[151,1437,1684,1744]" pageId="1" pageNumber="64">
<emphasis id="E2CCEA8B5202FFDFFF75F927726CD9E2" bold="true" box="[151,335,1684,1708]" pageId="1" pageNumber="64">FIGURES 1–3.</emphasis>
<taxonomicName id="17B84D1A5202FFDFFEBAF92671B5D9E2" box="[344,662,1685,1708]" class="Insecta" family="Phoridae" genus="Plethysmochaeta" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="lawrencei">
