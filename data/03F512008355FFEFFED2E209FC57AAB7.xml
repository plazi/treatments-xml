<taxonomicName id="4C5CD8958359FFECFD89E33EFCADA931" box="[595,840,1801,1824]" class="Insecta" family="Sericostomatidae" genus="Cheimacheramus" kingdom="Animalia" order="Trichoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="rossi">
