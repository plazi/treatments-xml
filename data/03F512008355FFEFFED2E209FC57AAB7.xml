<taxonomicName id="4C5CD8958359FFECFB1BE2D1FE7BA931" class="Insecta" family="Sericostomatidae" genus="Rhoizema" kingdom="Animalia" order="Trichoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="mahalevonum">
<emphasis id="B9287F048359FFECFB1BE2D1FE7BA931" italics="true" pageId="25" pageNumber="26">
<taxonomicName id="4C5CD8958359FFECFB1BE2D1FACFA8EC" box="[1217,1322,1766,1789]" class="Insecta" family="Sericostomatidae" genus="Rhoizema" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">Rhoizema</taxonomicName>
