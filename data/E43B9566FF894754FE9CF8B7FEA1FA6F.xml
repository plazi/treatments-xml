<quantity id="AB6A8995FF884754FD45FB22FCC7FB07" box="[721,770,1172,1198]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="23" pageNumber="24" unit="m" value="1.0">1 m</quantity>
.
</specimenCount>
(
<collectionCode id="0A83BCB5FF884754FC81FB22FCB1FB07" box="[789,884,1172,1198]" pageId="23" pageNumber="24">BMNH</collectionCode>
