<taxonomicName id="0BEC86BFFFD1CA622F4C2253216D0C14" authority="Jaschhof &amp; Hippa, 2005" authorityName="Jaschhof &amp; Hippa" authorityYear="2005" box="[299,381,1800,1823]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="rara" status="sp. n.">
