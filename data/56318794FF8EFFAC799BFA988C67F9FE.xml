<taxonomicName id="19984D01FF8FFFAC7815FB598EEFFB23" box="[281,498,1197,1219]" class="Insecta" family="Coccinellidae" genus="Oryssomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" phylum="Arthropoda" rank="species" species="paschoali" status="sp. nov.">
<emphasis id="ECECEA90FF8FFFAC7815FB598EEFFB23" box="[281,498,1197,1219]" italics="true" pageId="7">Oryssomus paschoali</emphasis>
</taxonomicName>
<taxonomicNameLabel id="F7DF57EBFF8FFFAC78F5FB598D54FB23" box="[505,585,1198,1219]" pageId="7" rank="species">
