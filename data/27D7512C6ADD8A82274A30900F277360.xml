<taxonomicName id="65AE22C2EBB8FC163A71ECA88D61B0A8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230854" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Tetramorium (Xiphomyrmex) Forel" order="Hymenoptera" pageNumber="263" phylum="Arthropoda" rank="species" species="xiphomyrmex" subGenus="Xiphomyrmex">TETRAMORIUM (XIPHOMYRMEX) ANDREI</taxonomicName>
