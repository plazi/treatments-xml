; 
<number id="A2730A0CFFD4981BFEC49E48DDE0EA41" box="[383,442,1892,1916]" pageId="10" pageNumber="90" value="1965.0">1965</number>
; 
<number id="A2730A0CFFD4981BFE7D9E48DE58EA41" box="[454,514,1892,1916]" pageId="10" pageNumber="90" value="1969.0">1969</number>
