<taxonomicName id="4D0D87A98F597001FC526A676226487D" LSID="http://zoobank.org/C0597858-AACC-475B-BEA4-DE5CD75E2B35" authority="Liebherr, 2016" authorityName="Liebherr" authorityYear="2016" class="Insecta" family="Carabidae" genus="Cyphocoleus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyphocoleus fasciatus" order="Coleoptera" pageId="29" pageNumber="240" phylum="Arthropoda" rank="species" species="fasciatus" status="sp. n.">Cyphocoleus fasciatus</taxonomicName>
