<paragraph id="EE9D8CDF98EDBB8330704D40CC19FC0A" pageId="1" pageNumber="2">
<taxonomicName id="210DAF2FD990B7E7CB3DC56C1EE82C0F" class="Cestoda" family="Dilepididae" genus="Janinellia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Janinellia peebeehi" order="Cyclophyllidea" pageId="1" pageNumber="2" phylum="Platyhelminthes" rank="species" species="peebeehi">Janinellia peebeehi</taxonomicName>
