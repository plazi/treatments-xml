<taxonomicName id="91EE09C4FFE8C148FE97DF86FE0F7AA0" authorityName="Zahniser" authorityYear="2021" box="[359,505,475,500]" class="Insecta" family="Cicadellidae" genus="Tenucephalus" kingdom="Animalia" order="Hemiptera" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="manuensis">
