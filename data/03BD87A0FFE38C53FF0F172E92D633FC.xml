<taxonomicName id="4C144D35FFE28C53FB6E1354940C33DC" authorityName="Linnaeus" authorityYear="1758" box="[1154,1287,617,641]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
