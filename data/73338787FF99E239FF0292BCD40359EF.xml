</taxonomicName>
. Both 
<taxonomicName id="3C9A4D12FF94E239FB599307D33B5917" authorityName="Ivinskis &amp; Piskunov" authorityYear="1989" box="[1228,1357,1087,1112]" class="Insecta" family="Gelechiidae" genus="Filatima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="405" phylum="Arthropoda" rank="species" species="karsholti">
