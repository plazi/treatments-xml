<taxonomicName id="25F34D3DFFFF5548573CFE1EFC7BFE43" box="[843,952,404,428]" class="Insecta" family="Gasteruptiidae" genus="Gasteruption" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="405" phylum="Arthropoda" rank="species" species="sarramea">Sarramea</taxonomicName>
