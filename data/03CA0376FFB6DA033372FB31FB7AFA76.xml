(
<collectionCode id="ED722AA5FFB6DA023302FA83FD12FA7E" box="[575,662,1406,1428]" country="Hungary" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" lsid="urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="8" pageNumber="221">HNHM</collectionCode>
