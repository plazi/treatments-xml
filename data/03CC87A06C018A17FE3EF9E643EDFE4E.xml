<taxonomicName id="4C654D356C1C8A17FD51FE0D4350FE4E" authorityName="Pall-Gergely" authorityYear="2018" box="[681,1113,461,487]" class="Gastropoda" family="Plectopylidae" genus="Chersaecia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="48" pageNumber="49" phylum="Mollusca" rank="subSpecies" species="shiroiensis" status="subsp. nov." subSpecies="subnagaensis">
