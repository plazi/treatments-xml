, 
<taxonomicName id="4C384D012C70FF86B3613F3DFED9075C" authorityName="Stephensen" authorityYear="1925" class="Malacostraca" family="Ampeliscidae" genus="Haploops" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="168" phylum="Arthropoda" rank="species" species="vallifera">H. vallifera</taxonomicName>
; 
<taxonomicName id="4C384D012C70FF86B73B3F51FEB7075C" authorityName="J. L. Barnard" authorityYear="1961" box="[276,360,331,357]" class="Malacostraca" family="Ampeliscidae" genus="Haploops" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="168" phylum="Arthropoda" rank="species" species="lodo">H. lodo</taxonomicName>
</emphasis>
