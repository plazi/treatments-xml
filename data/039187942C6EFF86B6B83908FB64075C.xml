<taxonomicName id="4C384D012C70FF86B36D3E8DFA480688" authorityName="J. L. Barnard" authorityYear="1961" box="[1346,1431,151,177]" class="Malacostraca" family="Ampeliscidae" genus="Haploops" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="168" phylum="Arthropoda" rank="species" species="lodo">H. lodo</taxonomicName>
