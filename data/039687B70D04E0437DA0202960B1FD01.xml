and 
<taxonomicName id="4C3F4D220D05E0407CC021CF66C4F947" authorityName="Maronna, Miranda, Pena Cantero, Barbeitos &amp; Marques" authorityYear="2016" box="[503,725,1695,1719]" class="Hydrozoa" family="Symplectoscyphidae" kingdom="Animalia" order="Leptothecata" pageId="33" pageNumber="34" phylum="Cnidaria" rank="family">Symplectoscyphidae</taxonomicName>
