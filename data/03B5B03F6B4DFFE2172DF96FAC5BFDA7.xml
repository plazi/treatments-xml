<emphasis id="B968DD3B6B4CFFE11764F987AE9AF9FC" box="[827,1020,1634,1655]" italics="true" pageId="17" pageNumber="340">Associated host:</emphasis>
<taxonomicName id="4C1C7AAA6B4CFFE11056F987A8FAF9F3" authority="Gray, 1858" authorityName="Gray" authorityYear="1858" box="[1033,1436,1634,1656]" class="Hexactinellida" family="Aphrocallistidae" genus="Aphrocallistes" kingdom="Animalia" order="Sceptrulophora" pageId="17" pageNumber="340" phylum="Porifera" rank="species" species="beatrix">
<emphasis id="B968DD3B6B4CFFE11056F987A877F9FC" box="[1033,1297,1634,1655]" italics="true" pageId="17" pageNumber="340">Aphrocallistes beatrix</emphasis>
<bibRefCitation id="EF8D7CD86B4CFFE11144F987A8FAF9F3" author="Gray JE" box="[1307,1436,1634,1656]" pageId="17" pageNumber="340" pagination="114 - 115" refId="ref15157" refString="Gray JE. 1858. On Aphrocallistes, a new genus of Spongiadae from Malacca. Proceedings of the Zoological Society of London 26: 114 - 115." type="journal article" year="1858">Gray, 1858</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C1C7AAA6B4CFFE11764F964A965F91D" box="[827,1027,1665,1686]" class="Hexactinellida" family="Aphrocallistidae" genus="Aphrocallistes" kingdom="Animalia" order="Sceptrulophora" pageId="17" pageNumber="340" phylum="Porifera" rank="species" species="undetermined">
