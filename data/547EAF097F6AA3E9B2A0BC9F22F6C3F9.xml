<taxonomicName id="4EB6E85F06563822BFFB09DEBDE91CCD" class="Insecta" family="Staphylinidae" genus="Stenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stenus xilingmontis" order="Coleoptera" pageId="18" pageNumber="75" phylum="Arthropoda" rank="species" species="xilingmontis">Stenus xilingmontis</taxonomicName>
