<taxonomicName id="4C238C87FF83FFCEBF4CFD56538EFCAF" authority=": Van Name 1945" authorityName=": Van Name" authorityYear="1945" box="[261,496,757,779]" class="Ascidiacea" family="Styelidae" genus="Symplegma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stolidobranchia" pageId="11" pageNumber="302" phylum="Chordata" rank="species" species="viride">
<emphasis id="B9572B16FF83FFCEBF4CFD56533EFCAF" box="[261,320,757,779]" italics="true" pageId="11" pageNumber="302">viride</emphasis>
