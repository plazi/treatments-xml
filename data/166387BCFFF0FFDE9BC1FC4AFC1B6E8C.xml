<taxonomicName id="59CA4D29FFF0FFDE9BC1FC4AFD9669AC" authorityName="Medvedev &amp; Dang" authorityYear="1981" box="[151,537,929,953]" class="Insecta" family="Chrysomelidae" genus="Hesperopenna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="721" phylum="Arthropoda" rank="genus">
