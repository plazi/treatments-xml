<typeStatus id="548D8801FF91477DFF7DFBFCFEEAFB92" box="[242,288,1060,1081]" pageId="85" pageNumber="184">Type</typeStatus>
species: 
<taxonomicName id="4C364D20FF91477DFEFAFBFDFD58FB91" authority="Perroud." authorityName="Perroud." box="[373,658,1060,1082]" class="Insecta" family="Attelabidae" genus="Rhynchites" kingdom="Animalia" order="Coleoptera" pageId="85" pageNumber="184" phylum="Arthropoda" rank="species" species="kanalensis">
<emphasis id="B942EAB1FF91477DFEFAFBFDFDF3FB91" box="[373,569,1061,1082]" italics="true" pageId="85" pageNumber="184">Rhynchites kanalensis</emphasis>
