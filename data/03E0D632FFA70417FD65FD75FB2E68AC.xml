<heading id="D0BED048FFA70416FD65FD75FB246D78" allCaps="true" box="[723,1177,1605,1629]" centered="true" fontSize="8" level="2" pageId="2" pageNumber="165" reason="3">
<emphasis id="B93DBB36FFA70416FD65FD75FC6F6D78" bold="true" box="[723,978,1606,1629]" pageId="2" pageNumber="165">KEY TO THE SPECIES OF</emphasis>
<taxonomicName id="4C491CA7FFA70416FC6CFD76FB246D78" authority="OF" authorityName="OF" box="[986,1177,1605,1629]" class="Insecta" family="Brachyceridae" genus="Lissorhoptrus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="165" phylum="Arthropoda" rank="genus">
