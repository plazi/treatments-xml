<taxonomicName id="59B776BB712C2AF3F4F71996499E10FC" class="Polychaeta" family="Cirratulidae" genus="Monticellina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Monticellina dorsobranchialis" order="Terebellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="dorsobranchialis">Monticellina dorsobranchialis</taxonomicName>
(Kirkegaard, 1959) | 
<taxonomicName id="5F1E6889575A1C88F6B751FD2D1DAF86" class="Polychaeta" family="Cirratulidae" genus="Tharyx" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tharyx dorsobranchialis" order="Terebellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="dorsobranchialis">Tharyx dorsobranchialis</taxonomicName>
