<subSubSection id="34F177A62E52EF4DFD18FA86FBB07BAC" box="[702,1029,1386,1416]" pageId="47" pageNumber="48" type="vernacular_names">
<paragraph id="7C54242D2E52EF4DFD18FA86FBB07BAC" blockId="47.[189,1399,1351,1739]" box="[702,1029,1386,1416]" pageId="47" pageNumber="48">
