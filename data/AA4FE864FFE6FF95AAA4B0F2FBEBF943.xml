<number id="942E4B55FFE6FF96A9C2B654FBC0F9D2" box="[1009,1024,1585,1609]" pageId="17" pageNumber="74" value="1.0">1</number>
: 
<number id="942E4B55FFE6FF96AE35B654FBD5F9D2" box="[1030,1045,1585,1609]" pageId="17" pageNumber="74" value="1.0">1</number>
