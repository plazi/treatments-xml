<number id="942E4B55FFE6FF96AE5FB654FBBBF9D2" box="[1132,1147,1585,1609]" pageId="17" pageNumber="74" value="1.0">1</number>
, P: 
<number id="942E4B55FFE6FF96AE98B654FB01F9D2" box="[1195,1217,1585,1609]" pageId="17" pageNumber="74" value="2.0">2</number>
