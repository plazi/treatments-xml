<number id="942E4B55FFE6FF96AE1DB654FBFDF9D2" box="[1070,1085,1585,1609]" pageId="17" pageNumber="74" value="1.0">1</number>
: 
<number id="942E4B55FFE6FF96AE70B654FB92F9D2" box="[1091,1106,1585,1609]" pageId="17" pageNumber="74" value="1.0">1</number>
