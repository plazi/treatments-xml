<taxonomicName id="80B04D6BE938ED0940B6FA70CA2E71DE" authority="Grimaldi, 2016" authorityName="Grimaldi" authorityYear="2016" box="[209,371,1430,1454]" class="Insecta" genus="Gracilomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="80" pageNumber="80" phylum="Arthropoda" rank="species" species="wit" status="new genus and species">
