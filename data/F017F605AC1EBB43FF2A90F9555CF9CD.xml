″ 
<geoCoordinate id="1D8A21D4AC1EBB43FA3A972056C5FA19" direction="north" orientation="latitude" pageId="1" pageNumber="172" precision="925" value="132.71666">
N 
<number id="CE765534AC1EBB43FF2A97FC56E8FA19" box="[151,193,1409,1434]" pageId="1" pageNumber="172" value="132.0">132</number>
