<taxonomicName id="4C564D11A476FFC37B36FABEFEC545D9" authorityName="E. Geoffroy Saint-Hilaire" authorityYear="1803" box="[258,363,1392,1415]" class="Mammalia" family="Talpidae" genus="Scalopus" kingdom="Animalia" order="Soricomorpha" pageId="23" pageNumber="24" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A476FFC37B36FABEFEC545D9" box="[258,363,1392,1415]" italics="true" pageId="23" pageNumber="24">Scalopus</emphasis>
