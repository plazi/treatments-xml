</taxonomicName>
and 
<taxonomicName id="4C564D11A476FFC37BB6FA65FE46459C" box="[386,488,1451,1474]" class="Mammalia" family="Talpidae" genus="Galemys" kingdom="Animalia" order="Soricomorpha" pageId="23" pageNumber="24" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A476FFC37BB6FA65FE46459C" box="[386,488,1451,1474]" italics="true" pageId="23" pageNumber="24">Galemys</emphasis>
</taxonomicName>
