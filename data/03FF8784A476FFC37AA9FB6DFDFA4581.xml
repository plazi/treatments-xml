<emphasis id="B922EA80A476FFC37826FABEFD2745D9" box="[530,649,1392,1415]" italics="true" pageId="23" pageNumber="24">Scaptonyx</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A476FFC37AB4FA40FF6A45FB" authorityName="Linnaeus" authorityYear="1758" box="[128,196,1422,1445]" class="Mammalia" family="Talpidae" genus="Talpa" kingdom="Animalia" order="Soricomorpha" pageId="23" pageNumber="24" phylum="Chordata" rank="genus">
