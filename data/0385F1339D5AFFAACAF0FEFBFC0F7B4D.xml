<paragraph id="8B9340259D5AFFAACBFBFC31FC0F7B4D" blockId="21.[189,1399,337,1104]" pageId="21" pageNumber="22">
<taxonomicName id="4C2C3BA69D5AFFAACBFBFC31FCB97CC6" authority="B. Boivin, Phytologia" authorityName="B. Boivin, Phytologia" box="[189,777,961,988]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="variety" species="cornuta" variety="glandulosa">
