<taxonomicName id="ADEB26226C271740A78D41A2BD944F26" LSID="urn:lsid:zoobank.org:act:E9A79C2A-7B38-405D-8C45-7B8E67C0C22C" class="Collembola" family="Onychiuridae" genus="Sensillonychiurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sensillonychiurus mirus" order="Poduromorpha" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="mirus">Sensillonychiurus mirus</taxonomicName>
<taxonomicNameLabel id="DD410B2078BD346278E19DA9CE25AEA2" pageId="8" pageNumber="9">sp. n.</taxonomicNameLabel>
