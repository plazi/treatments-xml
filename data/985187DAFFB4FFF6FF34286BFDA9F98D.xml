<collectionCode id="76E9AE09FFB4FFF6FE5129D7FDAAF9D5" box="[498,581,1520,1544]" country="Taiwan" httpUri="http://grbio.org/cool/2nry-s1yg" name="National Museum of Natural Science" pageId="2" pageNumber="303">NMNS</collectionCode>
