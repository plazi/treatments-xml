<taxonomicName id="7888D3C4FFFF753FFABCF8BBFA24E26F" box="[1350,1452,1889,1913]" class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" kingdom="Plantae" order="Caryophyllales" pageId="15" pageNumber="102" phylum="Tracheophyta" rank="genus">
