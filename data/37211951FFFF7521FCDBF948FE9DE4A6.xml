<collectionCode id="D9993082FFFF753FFCDBF858FCBDE28F" box="[801,821,1922,1945]" country="Netherlands" lsid="urn:lsid:biocol.org:col:15678" name="Nationaal Herbarium Nederland, Leiden University branch" pageId="15" pageNumber="102" type="Herbarium">L</collectionCode>
