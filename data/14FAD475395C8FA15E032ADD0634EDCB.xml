<paragraph id="555E48725FD83CF26F4CD607D98F92DE" pageId="6" pageNumber="71">
<taxonomicName id="7ABD99252553503754F09ED07C7EE049" LSID="urn:lsid:zoobank.org:act:5DB16050-0C5D-41AE-A2EA-6D16981D2E2C" authority="Hansson" class="Insecta" family="Eulophidae" genus="Microdonophagus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microdonophagus tertius" order="Hymenoptera" pageId="6" pageNumber="71" phylum="Arthropoda" rank="species" species="tertius">Microdonophagus tertius Hansson</taxonomicName>
<taxonomicNameLabel id="5DFC2F3ABC714A9B6F6236263A562E7B" pageId="6" pageNumber="71">sp. n.</taxonomicNameLabel>
