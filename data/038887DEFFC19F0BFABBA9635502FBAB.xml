<taxonomicName id="4C214D4BFFC19F0BF896AE565438FB82" box="[698,820,1082,1107]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="1" pageNumber="41" phylum="Arthropoda" rank="species" species="fragilis">
