<quantity id="4CD09B77FF926307BABCFC01C023FC23" box="[250,315,917,938]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="5" pageNumber="1458" unit="m" value="3000.0">3000m</quantity>
