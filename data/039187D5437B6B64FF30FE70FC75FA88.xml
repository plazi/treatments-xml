<taxonomicName id="4C384D40437B6B64FDCAFE3EFC69FA88" authority="Roux (1990 a)" authorityName="Roux" authorityYear="1990" box="[621,941,1354,1377]" class="Polypodiopsida" family="Anemiaceae" genus="Mohria" higherTaxonomySource="GBIF" kingdom="Plantae" order="Schizaeales" pageId="12" pageNumber="19" phylum="Tracheophyta" rank="species" species="hirsuta">
