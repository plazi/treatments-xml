34. 
<taxonomicName LSID="urn:lsid:zoobank.org:act:7C5F8EE9-D128-42EB-B329-14077A65EC8E" authority="Chanabun &amp; Panha" authorityName="Chanabun &amp; Panha" authorityYear="2013" class="Clitellata" family="Almidae" genus="Glyphidrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Glyphidrilus trangensis" order="Haplotaxida" pageId="50" pageNumber="51" phylum="Annelida" rank="species" species="trangensis" status="sp. n.">Glyphidrilus trangensis Chanabun &amp; Panha</taxonomicName>
