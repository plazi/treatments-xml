, 
<taxonomicName authority="culminea" class="Gastropoda" family="Bulimulidae" genus="Kuschelenia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="80" pageNumber="81" phylum="Mollusca" rank="subSpecies" species="culminea" subGenus="Kuschelenia" subSpecies="culminea">
<emphasis italics="true" pageId="80" pageNumber="81">Kuschelenia (Kuschelenia) culminea culminea</emphasis>
</taxonomicName>
(
<normalizedToken originalValue="d’Orbigny">d'Orbigny</normalizedToken>
, 1835) (
<emphasis bold="true" pageId="80" pageNumber="81">comb. n.</emphasis>
).
</paragraph>
