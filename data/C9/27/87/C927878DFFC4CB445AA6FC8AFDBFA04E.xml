<subSubSection box="[197,1173,1114,1140]" pageId="25" pageNumber="26" type="etymology">
<paragraph blockId="25.[151,1437,998,1836]" box="[197,1173,1114,1140]" pageId="25" pageNumber="26">
<emphasis bold="true" box="[197,342,1114,1140]" pageId="25" pageNumber="26">Etymology:</emphasis>
