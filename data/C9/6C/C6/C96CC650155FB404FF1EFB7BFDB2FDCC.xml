<quantity box="[760,929,1452,1477]" metricMagnitude="-4" metricUnit="m" metricValue="4.45" metricValueMax="4.5" metricValueMin="4.4" pageId="10" pageNumber="100" unit="mm" value="0.445" valueMax="0.45" valueMin="0.44">0.44–0.45 mm</quantity>
