<taxonomicName class="Arachnida" family="Phthiracaridae" genus="Atropacarus" genus.bestMatchDistance="3426" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="197" phylum="Arthropoda" rank="species" species="striculus">A. striculus</taxonomicName>
