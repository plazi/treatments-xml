<subSubSection pageId="1" pageNumber="44" type="type species">
<paragraph pageId="1" pageNumber="44">Type species.</paragraph>
<paragraph pageId="1" pageNumber="44">
<taxonomicName class="Malacostraca" family="Urothoidae" genus="Urothoe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Urothoe irrostratus" order="Amphipoda" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="irrostratus">Urothoe irrostratus</taxonomicName>
