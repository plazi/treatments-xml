<paragraph blockId="2.[151,1437,476,1040]" box="[199,1351,620,645]" pageId="2" pageNumber="292">
<emphasis bold="true" box="[199,334,620,645]" pageId="2" pageNumber="292">Etymology.</emphasis>
Greek 
<taxonomicName box="[416,522,621,644]" class="Entoprocta" family="Loxosomatidae" genus="Loxosomella" higherTaxonomySource="CoL" kingdom="Animalia" order="Coloniales" pageId="2" pageNumber="292" phylum="Kamptozoa" rank="species" species="unicornis">
