<taxonomicName authority="Volkovitsh, 2012" authorityName="Volkovitsh" authorityYear="2012" box="[382,516,1549,1568]" class="Insecta" family="Buprestidae" genus="Acmaeodera" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="206" phylum="Arthropoda" rank="species" species="kabateki" status="sp. nov." subGenus="Acmaeotethya">
