<taxonomicName class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis sicula" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="sicula">Chrysis sicula</taxonomicName>
