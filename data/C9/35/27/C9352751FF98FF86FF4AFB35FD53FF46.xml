, 
<collectingCountry box="[330,464,188,213]" name="Ivory Coast" pageId="10" pageNumber="461">
Ivory 
<collectingRegion box="[397,464,188,212]" country="Tanzania" name="Pwani" pageId="10" pageNumber="461">Coast</collectingRegion>
</collectingCountry>
, 
<collectingCountry box="[475,535,189,213]" name="Togo" pageId="10" pageNumber="461">Togo</collectingCountry>
, 
<collectingCountry box="[546,668,188,212]" name="Cameroon" pageId="10" pageNumber="461">Cameroon</collectingCountry>
).
</paragraph>
