<subSubSection pageId="164" pageNumber="165" type="nomenclature">
<paragraph pageId="164" pageNumber="165">
<taxonomicName LSID="C9E8C0EB-AD89-5B68-832F-46508F3D9DBE" authority="(L.) Vahl (= S. jamaicensis var. indica (L.) H. J. Lam)" baseAuthorityName="L.) Vahl (= S. jamaicensis var. indica (L.) H. J. Lam" class="Magnoliopsida" family="Verbenaceae" genus="Stachytarpheta" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Stachytarpheta indica" order="Lamiales" pageId="164" pageNumber="165" phylum="Tracheophyta" rank="species" species="indica">Stachytarpheta indica (L.) Vahl (= S. jamaicensis var. indica (L.) H.J. Lam)</taxonomicName>
