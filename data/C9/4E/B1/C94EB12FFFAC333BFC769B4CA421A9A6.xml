<taxonomicName authority="Komai &amp; Rahayu, 2013" authorityName="Komai &amp; Rahayu" authorityYear="2013" box="[202,481,2067,2087]" class="Malacostraca" family="Paguridae" genus="Catapaguroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="155" phylum="Arthropoda" rank="species" species="crassimanus" status="new species">
