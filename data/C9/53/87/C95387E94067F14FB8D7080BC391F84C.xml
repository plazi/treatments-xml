. 
<number box="[277,291,967,991]" pageId="19" pageNumber="407" value="2.0">2</number>
slides each labelled 
<taxonomicName authority="Verh." authorityName="Verh." box="[551,873,966,991]" class="Chilopoda" family="Scolopendridae" genus="Otostigmus" kingdom="Animalia" order="Scolopendromorpha" pageId="19" pageNumber="407" phylum="Arthropoda" rank="species" species="striolatus">
<emphasis box="[551,799,966,990]" italics="true" pageId="19" pageNumber="407">Otostigmus striolatus</emphasis>
Verh.
</taxonomicName>
Pahang 
<quantity box="[983,1107,966,991]" metricMagnitude="2" metricUnit="m" metricValue="8.075" metricValueMax="16.0" metricValueMin="0.15" pageId="19" pageNumber="407" unit="m" value="807.5" valueMax="1600.0" valueMin="15.0">15–1600m</quantity>
. 
<collectingCountry box="[1124,1223,966,991]" name="Malaysia" pageId="19" pageNumber="407">Malacca</collectingCountry>
. A 
<number box="[1260,1372,966,991]" pageId="19" pageNumber="407" value="2.0030598E7">20030598</number>
with forcipules and trunk segments 
<number box="[501,544,1003,1027]" pageId="19" pageNumber="407" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
, and 
