<paragraph blockId="4.[151,1436,1690,1774]" pageId="4" pageNumber="65">
<emphasis bold="true" box="[151,327,1690,1713]" pageId="4" pageNumber="65">FIGURES 9–14.</emphasis>
<taxonomicName box="[334,576,1691,1713]" class="Insecta" family="Elmidae" genus="Elachistelmis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="tetramera" status="n. sp.">
<emphasis box="[334,576,1691,1713]" italics="true" pageId="4" pageNumber="65">Elachistelmis tetramera</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[590,647,1691,1712]" pageId="4" pageNumber="65">
<taxonomicNameLabel box="[590,647,1691,1712]" pageId="4" pageNumber="65" rank="species">n. sp.</taxonomicNameLabel>
