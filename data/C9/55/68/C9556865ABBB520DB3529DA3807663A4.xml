<emphasis italics="true" pageId="0" pageNumber="77">Juglans regia</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Juglandaceae" genus="Carya" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carya cordiformis" order="Fagales" pageId="0" pageNumber="77" phylum="Tracheophyta" rank="species" species="cordiformis">
