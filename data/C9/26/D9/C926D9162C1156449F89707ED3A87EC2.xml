<taxonomicName authorityName="Foerster" authorityYear="1856" class="Insecta" family="Fabaceae" genus="Fabaceae" kingdom="Animalia" lsidName="" order="Hemiptera" pageId="0" pageNumber="77092" phylum="Arthropoda">Fabaceae</taxonomicName>
