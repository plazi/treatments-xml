<taxonomicName authority="Linnaeus, 1758" authorityName="Foerster" authorityYear="1856" class="Insecta" family="Fabaceae" genus="Hemiptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hemiptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="genus">Hemiptera</taxonomicName>
