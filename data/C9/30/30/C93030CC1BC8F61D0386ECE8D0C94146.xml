<geoCoordinate direction="north" orientation="latitude" precision="9" value="14.266167">14°15.97'N</geoCoordinate>
<geoCoordinate direction="east" orientation="longitude" precision="9" value="120.71183">120°42.71'E</geoCoordinate>
