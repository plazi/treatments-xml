<materialsCitation id="3B493C86FFB5FFB6FC130368FBF35793" ID-GBIF-Occurrence="3418229301" collectionCode="NIGP" pageId="1" pageNumber="615" specimenCode="NIGP177330" specimenCount="1" typeStatus="holotype">
<typeStatus id="549A8879FFB5FFB6FC130368FB8E5768" box="[931,1038,597,623]" pageId="1" pageNumber="615">Holotype</typeStatus>
, 
<specimenCode id="DB879EA0FFB5FFB6FBAC036BFB345777" box="[1052,1204,598,624]" collectionCode="NIGP" country="China" httpUri="http://grbio.org/cool/j51y-avsb" name="Naking Institute of Geology and Palaeontology" pageId="1" pageNumber="615">NIGP177330</specimenCode>
