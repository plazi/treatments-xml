, 
<collectionCode id="ED3E96F67F1C1E233C02FF45B1961DE1" LSID="urn:lsid:biocol.org:col:34871" box="[1114,1200,152,176]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="37" pageNumber="388">USNM</collectionCode>
