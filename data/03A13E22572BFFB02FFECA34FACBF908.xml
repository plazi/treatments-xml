<emphasis id="B97C5326572BFFB02FFECAF2FE86FE7B" bold="true" box="[151,309,430,455]" pageId="3" pageNumber="170">Type species.</emphasis>
<taxonomicName id="4C08F4B7572BFFB02E54CAF2FD97FE7B" box="[317,548,430,455]" class="Ostracoda" family="Hemicytheridae" genus="Bulbocythere" kingdom="Animalia" order="Podocopida" pageId="3" pageNumber="170" phylum="Arthropoda" rank="species" species="calida" status="sp. nov.">
<emphasis id="B97C5326572BFFB02E54CAF2FD97FE7B" box="[317,548,430,455]" italics="true" pageId="3" pageNumber="170">Bulbocythere calida</emphasis>
</taxonomicName>
