<typeStatus id="AF393292D17C3A105D91FAD076D905C2" box="[189,250,1393,1419]" pageId="1" pageNumber="2">Type</typeStatus>
species
</emphasis>
</heading>
</paragraph>
<paragraph id="703D8C30D17C3A105D91FA3E77D905F0" blockId="1.[189,506,1439,1465]" box="[189,506,1439,1465]" pageId="1" pageNumber="2">
<taxonomicName id="B782F7B3D17C3A105D91FA3E77BB05F0" authority="Galea, 2015" authorityName="Galea" authorityYear="2015" box="[189,408,1439,1465]" class="Hydrozoa" family="Sertulariidae" genus="Caledoniana" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Leptothecata" pageId="1" pageNumber="2" phylum="Cnidaria" rank="species" species="alata" status="sp. nov.">
<emphasis id="42F65022D17C3A105D91FA3E77BB05F0" box="[189,408,1439,1465]" italics="true" pageId="1" pageNumber="2">
<taxonomicName id="B782F7B3D17C3A105D91FA3E777605F0" box="[189,341,1439,1465]" class="Hydrozoa" family="Sertulariidae" genus="Caledoniana" higherTaxonomySource="CoL" kingdom="Animalia" order="Leptothecata" pageId="1" pageNumber="2" phylum="Cnidaria" rank="genus">Caledoniana</taxonomicName>
