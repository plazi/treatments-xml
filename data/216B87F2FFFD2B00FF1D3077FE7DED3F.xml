<collectionCode id="CFD3AE21FFFD2B07FC8C30FBFCA5EBB2" LSID="urn:lsid:biocol.org:col:34613" box="[774,858,1465,1489]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="11" pageNumber="12">ZFMK</collectionCode>
