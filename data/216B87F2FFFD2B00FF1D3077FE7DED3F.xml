<number id="1F0A24C3FFFA2B00FD19365DFD4DED54" box="[659,690,799,823]" pageId="12" pageNumber="13" value="24.0">24</number>
; body w/o wings: 
<number id="1F0A24C3FFFA2B00FC0E365DFC5DED54" box="[900,930,799,823]" pageId="12" pageNumber="13" value="15.0">15</number>
; pronotum: 
<number id="1F0A24C3FFFA2B00FBA6365DFBADED54" box="[1068,1106,799,823]" pageId="12" pageNumber="13" value="3.5">3.5</number>
; tegmen: 
<number id="1F0A24C3FFFA2B00FB4A365DFB21ED54" box="[1216,1246,799,823]" pageId="12" pageNumber="13" value="17.0">17</number>
; hind femur: 
<number id="1F0A24C3FFFA2B00FAF2365DFA68ED54" box="[1400,1431,799,823]" pageId="12" pageNumber="13" value="10.0">10</number>
; hind wing: 
<quantity id="6E3A9B01FFFA2B00FE9C3606FE82ED3F" box="[278,381,836,860]" metricMagnitude="-2" metricUnit="m" metricValue="2.15" pageId="12" pageNumber="13" unit="mm" value="21.5">21.5 mm</quantity>
