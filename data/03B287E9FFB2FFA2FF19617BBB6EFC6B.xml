<taxonomicName id="4C1B4D7CFFB0FFADFF2D6352B857F9CB" authorityName="Gray" authorityYear="1821" box="[232,319,1625,1642]" class="Mammalia" family="Leporidae" kingdom="Animalia" order="Lagomorpha" pageId="39" pageNumber="442" phylum="Chordata" rank="family">Leporidae</taxonomicName>
