<collectingCountry id="CFDD762FFF21E2858184FBA9D81C61BA" box="[1227,1298,1146,1171]" name="Brazil" pageId="204" pageNumber="205">Brazil</collectingCountry>
, at 
<geoCoordinate id="D2FE5078FF21E285800DFBA9D89261BA" box="[1346,1436,1146,1171]" direction="south" orientation="latitude" pageId="204" pageNumber="205" precision="555" value="-26.75">26.75°S</geoCoordinate>
