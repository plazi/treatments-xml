<geoCoordinate id="D2FE5078FF21E2858402FB4CDC96619E" box="[333,408,1183,1207]" direction="south" orientation="latitude" pageId="204" pageNumber="205" precision="5555" value="-27.3">27.3°S</geoCoordinate>
<geoCoordinate id="D2FE5078FF21E28584EFFB4DDF0B619E" box="[416,517,1182,1207]" direction="west" orientation="longitude" pageId="204" pageNumber="205" precision="555" value="-48.15">48.15°W</geoCoordinate>
