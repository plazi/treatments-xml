<heading id="3F7C3E6DFF90FFECFF22F9DCFDECF916" bold="true" box="[151,629,1653,1679]" fontSize="11" level="1" pageId="30" pageNumber="31" reason="1">
<emphasis id="56FF5513FF90FFECFF22F9DCFDECF916" bold="true" box="[151,629,1653,1679]" pageId="30" pageNumber="31">
<taxonomicName id="A38BF282FF90FFECFF22F9DCFD96F916" authority="Malenovský &amp; Burckhardt, 2009" authorityName="Malenovský &amp; Burckhardt" authorityYear="2009" box="[151,527,1653,1679]" class="Insecta" family="Phacopteronidae" genus="Pseudophacopteron" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="nigritulum" status="sp. nov.">
<emphasis id="56FF5513FF90FFECFF22F9DCFD96F916" bold="true" box="[151,527,1653,1679]" italics="true" pageId="30" pageNumber="31">Pseudophacopteron nigritulum</emphasis>
</taxonomicName>
