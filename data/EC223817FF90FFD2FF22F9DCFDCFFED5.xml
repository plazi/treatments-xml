<taxonomicName id="A38BF282FF91FFEDFE77FB56FD8BFA81" authorityName="Delile" authorityYear="1843" box="[450,530,1279,1304]" class="Magnoliopsida" family="Rutaceae" genus="Teclea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="genus">Teclea</taxonomicName>
