<taxonomicName id="91E94D795922FFE6B074F8B64ECCF8A6" authorityName="Marathe &amp; Sanborn &amp; Kunte" authorityYear="2018" baseAuthorityName="Distant" baseAuthorityYear="1897" box="[1290,1436,1877,1900]" class="Insecta" family="Cicadidae" genus="Rustia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="437" phylum="Arthropoda" rank="species" species="marginata" status="n. comb.">
