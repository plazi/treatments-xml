<subSubSection id="C31067979F5EFFA1FFAEFA6D0FB47CA7" box="[88,507,1460,1481]" pageId="9" pageNumber="10" type="diagnosis">
<paragraph id="8BB5341C9F5EFFA1FFAEFA6D0FB47CA7" blockId="9.[88,757,1237,1565]" box="[88,507,1460,1481]" pageId="9" pageNumber="10">
