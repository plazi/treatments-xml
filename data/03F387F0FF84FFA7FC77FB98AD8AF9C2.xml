elevation 
<quantity id="4CA29B03FF84FFA5FCABFA09AC77FA44" box="[794,852,1459,1478]" metricMagnitude="2" metricUnit="m" metricValue="4.3" pageId="5" pageNumber="366" unit="m" value="430.0">430 m</quantity>
