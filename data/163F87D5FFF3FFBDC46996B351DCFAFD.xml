<quantity id="596E9B26FFF3FFBDC6FF94F85518FD32" box="[984,1029,721,740]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="24" pageNumber="309" unit="km" value="4.0">4 km</quantity>
W. of Gerringong, 
<date id="EA281003FFF3FFBDC19794F8543DFD32" box="[1200,1312,721,740]" pageId="24" pageNumber="309" value="1978-04-05">
<collectingDate id="FA6CE9EBFFF3FFBDC19794F8543DFD32" box="[1200,1312,721,740]" pageId="24" pageNumber="309" value="1978-04-05">5 April 1978</collectingDate>
</date>
, 
<collectorName id="33635315FFF3FFBDC00D94F85497FD32" box="[1322,1418,721,740]" pageId="24" pageNumber="309">G. Wishart</collectorName>
