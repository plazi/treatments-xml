<location id="9B496018FFF3FFBDC6D194C055EAFD2A" LSID="urn:lsid:plazi:treatment:163F87D5FFF3FFBDC46996B351DCFAFD:9B496018FFF3FFBDC6D194C055EAFD2A" box="[1014,1271,745,764]" county="Cheliceral" municipality="Figure" name="Ulladulla Ambulance Station" pageId="24" pageNumber="309" stateProvince="New South Wales">Ulladulla Ambulance Station</location>
; 
<collectionCode id="F887AE06FFF3FFBDC02694C05432FD2A" box="[1281,1327,745,764]" pageId="24" pageNumber="309">AMS</collectionCode>
