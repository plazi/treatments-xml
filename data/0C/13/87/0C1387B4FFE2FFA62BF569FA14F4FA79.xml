<subSubSection pageId="10" pageNumber="105" type="etymology">
<paragraph blockId="10.[151,1437,258,1441]" pageId="10" pageNumber="105">
<emphasis bold="true" box="[199,334,1272,1297]" pageId="10" pageNumber="105">Etymology.</emphasis>
