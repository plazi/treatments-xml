<taxonomicName baseAuthorityName="Cope" baseAuthorityYear="1862" box="[757,870,1815,1840]" class="Amphibia" family="Phyllomedusidae" genus="Pithecopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="azureus">
<emphasis box="[757,870,1815,1840]" italics="true" pageId="14" pageNumber="15">P. azureus</emphasis>
</taxonomicName>
(1/47; 3), 
<taxonomicName box="[987,1123,1815,1841]" class="Chromadorea" family="Kathlaniidae" genus="Pteroxyascaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="14" pageNumber="15" phylum="Nematoda" rank="species" species="similis">
<emphasis box="[987,1008,1815,1841]" italics="true" pageId="14" pageNumber="15">S.</emphasis>
cf. 
<emphasis box="[1050,1123,1815,1841]" italics="true" pageId="14" pageNumber="15">similis</emphasis>
</taxonomicName>
(1/2; 1), 
<taxonomicName box="[1227,1326,1815,1841]" class="Chromadorea" family="Cosmocercidae" genus="Schrankiana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="14" pageNumber="15" phylum="Nematoda" rank="species" species="fuscus">
<emphasis box="[1227,1326,1815,1841]" italics="true" pageId="14" pageNumber="15">L. fuscus</emphasis>
</taxonomicName>
(1/50; 3), 
<taxonomicName baseAuthorityName="Burmeister" baseAuthorityYear="1861" box="[151,305,1851,1876]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="mystacinus">
<emphasis box="[151,305,1851,1876]" italics="true" pageId="14" pageNumber="15">L. mystacinus</emphasis>
</taxonomicName>
(1/8; 4), and 
<taxonomicName baseAuthorityName="Cope" baseAuthorityYear="1862" box="[457,617,1851,1877]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="podicipinus">
<emphasis box="[457,617,1851,1877]" italics="true" pageId="14" pageNumber="15">L. podicipinus</emphasis>
</taxonomicName>
