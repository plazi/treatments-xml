<taxonomicName box="[472,629,987,1013]" class="Amphibia" family="Hylidae" genus="Scinax" kingdom="Animalia" order="Anura" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="fuscovarius">
<emphasis box="[472,629,987,1013]" italics="true" pageId="14" pageNumber="15">S. fuscovarius</emphasis>
</taxonomicName>
(1/51; 1) and 
<taxonomicName box="[788,924,987,1013]" class="Chromadorea" family="Kathlaniidae" genus="Pteroxyascaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="14" pageNumber="15" phylum="Nematoda" rank="species" species="similis">
