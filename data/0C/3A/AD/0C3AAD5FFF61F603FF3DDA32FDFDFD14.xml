<taxonomicName baseAuthorityName="Cope" baseAuthorityYear="1862" box="[1108,1286,627,652]" class="Amphibia" family="Hylidae" genus="Boana" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="raniceps">
