<number box="[1075,1089,924,950]" pageId="7" pageNumber="32" value="4.0">4</number>
with 
<number box="[1154,1211,924,950]" pageId="7" pageNumber="32" value="9.0" valueMax="11.0" valueMin="7.0">7–11</number>
