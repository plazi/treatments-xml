<number box="[1249,1261,964,990]" pageId="7" pageNumber="32" value="5.0">5</number>
with 
<number box="[264,336,1004,1030]" pageId="7" pageNumber="32" value="12.0" valueMax="13.0" valueMin="11.0">11–13</number>
