<taxonomicName authority="Gray, 1832" authorityName="Gray" authorityYear="1832" class="Insecta" family="Archembiidae" genus="Olyntha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Olyntha" order="Embioptera" pageId="0" pageNumber="129" phylum="Arthropoda" rank="genus">Olyntha</taxonomicName>
