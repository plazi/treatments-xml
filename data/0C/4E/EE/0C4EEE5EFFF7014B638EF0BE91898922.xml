<taxonomicName authorityName="Thunberg" authorityYear="1814" box="[639,753,489,512]" class="Insecta" family="Chrysomelidae" genus="Taumacera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="44" phylum="Arthropoda" rank="genus">
