<taxonomicName authorityName="Baly" authorityYear="1879" class="Insecta" family="Chrysomelidae" genus="Acroxena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="44" phylum="Arthropoda" rank="species" species="nasuta">
<emphasis italics="true" pageId="21" pageNumber="44">T. nasuta</emphasis>
</taxonomicName>
