<subSubSection pageId="5" pageNumber="40" type="etymology">
<paragraph blockId="5.[151,1437,151,1223]" box="[198,1396,1010,1036]" pageId="5" pageNumber="40">
<emphasis bold="true" box="[198,339,1010,1036]" pageId="5" pageNumber="40">Etymology.</emphasis>
<taxonomicName box="[346,468,1010,1035]" class="Insecta" family="Scarabaeidae" genus="Deltorhinum" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="47" phylum="Arthropoda" rank="species" species="bilobatum" status="sp. nov.">
<emphasis box="[346,468,1010,1035]" italics="true" pageId="5" pageNumber="40">Bilobatum</emphasis>
</taxonomicName>
