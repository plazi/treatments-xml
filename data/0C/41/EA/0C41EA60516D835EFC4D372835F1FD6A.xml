(
<collectingCountry box="[210,319,1108,1131]" name="Indonesia" pageId="1" pageNumber="54">Indonesia</collectingCountry>
: 
<collectingRegion box="[329,421,1108,1131]" country="Indonesia" name="Sumatera Utara" pageId="1" pageNumber="54">Sumatra</collectingRegion>
