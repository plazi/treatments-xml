), 
<taxonomicName authority="Millidge, 1995" authorityName="Millidge" authorityYear="1995" box="[492,756,1043,1068]" class="Arachnida" family="Linyphiidae" genus="Racata" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="54" phylum="Arthropoda" rank="species" species="grata">
