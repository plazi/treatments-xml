. 
<emphasis box="[525,583,1045,1068]" italics="true" pageId="1" pageNumber="54">grata</emphasis>
<bibRefCitation author="Millidge A. F." box="[593,756,1043,1066]" pageId="1" pageNumber="54" pagination="41 - 56" refId="ref4855" refString="Millidge A. F. 1995. Some linyphiid spiders from south-east Asia. Bulletin of the British Arachnological Society 10: 41 - 56." type="journal article" year="1995">Millidge, 1995</bibRefCitation>
</taxonomicName>
(
<collectingCountry box="[159,268,1076,1099]" name="Indonesia" pageId="1" pageNumber="54">Indonesia</collectingCountry>
: Krakatoa, 
<collectingRegion box="[390,437,1076,1099]" country="Indonesia" name="Jawa Timur" pageId="1" pageNumber="54">Java</collectingRegion>
and Belitung) and 
<taxonomicName authority="Tanasevitch, 2019" authorityName="Tanasevitch" authorityYear="2019" box="[643,719,1076,1100]" class="Arachnida" family="Linyphiidae" genus="Racata" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="54" phylum="Arthropoda" rank="species" species="laxa" status="sp. nov.">
