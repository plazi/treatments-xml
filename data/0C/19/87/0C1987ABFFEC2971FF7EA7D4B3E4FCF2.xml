<quantity box="[682,752,793,819]" metricMagnitude="-1" metricUnit="m" metricValue="9.398" metricValueMax="10.16" metricValueMin="8.636" pageId="10" pageNumber="341" unit="in" value="37.0" valueMax="40.0" valueMin="34.0">34–40</quantity>
</figureCitation>
