<emphasis box="[569,719,1406,1429]" italics="true" pageId="61" pageNumber="62">Gabbia obesa</emphasis>
</taxonomicName>
(o), 
<taxonomicName box="[778,876,1406,1429]" class="Gastropoda" family="Bithyniidae" genus="Gabbia" kingdom="Animalia" order="Littorinimorpha" pageId="61" pageNumber="62" phylum="Mollusca" rank="species" species="affinis">
<emphasis box="[778,876,1406,1429]" italics="true" pageId="61" pageNumber="62">G. affinis</emphasis>
</taxonomicName>
(), 
<taxonomicName authorityName="Ponder" authorityYear="2003" box="[938,1087,1406,1429]" class="Gastropoda" family="Bithyniidae" genus="Gabbia" kingdom="Animalia" order="Littorinimorpha" pageId="61" pageNumber="62" phylum="Mollusca" rank="species" species="microcosta">
<emphasis box="[938,1087,1406,1429]" italics="true" pageId="61" pageNumber="62">G. microcosta</emphasis>
</taxonomicName>
(), 
