<emphasis box="[1147,1275,1406,1429]" italics="true" pageId="61" pageNumber="62">G. clathrata</emphasis>
</taxonomicName>
() and 
<taxonomicName authorityName="Ponder" authorityYear="2003" box="[309,418,1445,1468]" class="Gastropoda" family="Bithyniidae" genus="Gabbia" kingdom="Animalia" order="Littorinimorpha" pageId="61" pageNumber="62" phylum="Mollusca" rank="species" species="spiralis">
