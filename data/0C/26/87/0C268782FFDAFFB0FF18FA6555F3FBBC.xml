<number box="[939,953,764,788]" pageId="3" pageNumber="4" value="3.0">3</number>
<number box="[959,1010,764,789]" pageId="3" pageNumber="4" value="0.85">0.85</number>
–1.00; forewing pad length 
<number box="[1316,1431,764,789]" pageId="3" pageNumber="4" value="2.05" valueMax="2.35" valueMin="1.75">1.75–2.35</number>
; caudal plate length 1.00– 
<number box="[436,485,799,824]" pageId="3" pageNumber="4" value="1.35">1.35</number>
