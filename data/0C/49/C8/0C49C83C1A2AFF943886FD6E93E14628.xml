<taxonomicName authorityName="Williams &amp; Weaver" authorityYear="2018" baseAuthorityName="Williams &amp; Weaver" baseAuthorityYear="1962" box="[1026,1134,1484,1509]" class="Ostracoda" family="Entocytheridae" genus="Ankylocythere" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="hobbsi">
