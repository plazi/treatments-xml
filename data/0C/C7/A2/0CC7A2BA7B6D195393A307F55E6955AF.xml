</taxonomicName>
<taxonomicName authority="L." authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" phylum="Tracheophyta" rank="species" species="tanacetifolia">
