<taxonomicName authorityName="Brunner von Wattenwyl" authorityYear="1893" box="[1245,1350,1475,1496]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="101" phylum="Arthropoda" rank="genus">
<emphasis box="[1245,1350,1475,1496]" italics="true" pageId="4" pageNumber="101">Hemigyrus</emphasis>
</taxonomicName>
(
<emphasis bold="true" pageId="4" pageNumber="101">弧ffȃ 亚Dz</emphasis>
)].............................................................................................. 2
</keyLead>
</paragraph>
<paragraph blockId="4.[151,1437,1475,2025]" pageId="4" pageNumber="101">
<keyLead pageId="4" pageNumber="101">
