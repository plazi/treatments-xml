<taxonomicName authorityName="Walker" authorityYear="1837" class="Insecta" family="Pteromalidae" genus="Cleonymus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cleonymus laticornis" order="Coleoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" species="laticornis">
