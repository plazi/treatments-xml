<taxonomicName authority="Nicolet, 1849" authorityName="Nicolet" authorityYear="1849" box="[298,578,1629,1653]" class="Arachnida" family="Salticidae" genus="Attus" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="130" phylum="Arthropoda" rank="species" species="elegans">
