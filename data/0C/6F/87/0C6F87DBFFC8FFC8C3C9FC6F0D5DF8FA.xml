<taxonomicName authorityName="Krapp-Schickel &amp; Myers" authorityYear="2006" box="[855,999,1711,1736]" class="Malacostraca" family="Aoridae" genus="Bemlos" kingdom="Animalia" order="Amphipoda" pageId="37" pageNumber="257" phylum="Arthropoda" rank="species" species="clypeatus">
