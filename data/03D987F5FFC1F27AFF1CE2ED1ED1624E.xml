<taxonomicName id="4C704D60FFC1F27AFEB3E2ED1F9862D9" authority="LAMARCK, 1816" authorityName="LAMARCK" authorityYear="1816" box="[324,708,427,449]" class="Anthozoa" family="Pocilloporidae" genus="Pocillopora" kingdom="Animalia" order="Scleractinia" pageId="13" pageNumber="14" phylum="Cnidaria" rank="genus">
<emphasis id="B904EAF1FFC1F27AFEB3E2ED1F5862D8" box="[324,516,427,448]" italics="true" pageId="13" pageNumber="14">POCILLOPORA</emphasis>
<bibRefCitation id="EFE14B12FFC1F27AFDF9E2ED1F9862D9" author="Lamarck J" box="[526,708,427,449]" pageId="13" pageNumber="14" refId="ref17292" refString="Lamarck J. 1816. Histoire naturelle des animaux sans vertebres. Vol. 2. Paris." type="book" year="1816">LAMARCK, 1816</bibRefCitation>
