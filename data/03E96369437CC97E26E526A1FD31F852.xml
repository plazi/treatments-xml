<quantity id="4CB87F9A437CC97E228F27ACFA0CF86E" box="[1277,1424,1974,1999]" metricMagnitude="3" metricUnit="m" metricValue="1.225" metricValueMax="2.2" metricValueMin="0.25" pageId="27" pageNumber="28" unit="m" value="1225.0" valueMax="2200.0" valueMin="250.0">250–2200 m</quantity>
