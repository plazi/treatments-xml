<taxonomicName id="1DFB83C3FF8FFFCEAF243E92FEFDFE1C" box="[312,381,444,469]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="12" pageNumber="19" phylum="Chordata" rank="species" species="major">
