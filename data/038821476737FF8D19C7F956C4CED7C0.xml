Holotype ♀, Iran, Khuzestan province, Ahwaz, 31°19'N 48°41'E, 
<date id="FF9FB6916734FF8C1D38F884C289D1E8" box="[1128,1241,1900,1924]" pageId="2" pageNumber="318" value="2011-10-10">10.x.2011</date>
, ex 
<taxonomicName id="4C21EBD26734FF8C1C58F884C7FBD1C4" authority="Tinsley, 1898" authorityName="Tinsley" authorityYear="1898" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="318" phylum="Arthropoda" rank="species" species="solenopsis">
<emphasis id="B9554C436734FF8C1C58F884C75CD1C4" italics="true" pageId="2" pageNumber="318">Phenacoccus solenopsis</emphasis>
Tinsley, 1898
</taxonomicName>
(
<taxonomicName id="4C21EBD26734FF8C18EAF867C467D1C4" box="[442,567,1935,1960]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="318" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
: 
<taxonomicName id="4C21EBD26734FF8C1B12F867C4A7D1C4" box="[578,759,1935,1960]" class="Insecta" family="Pseudococcidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="318" phylum="Arthropoda" rank="family">Pseudococcidae</taxonomicName>
) on 
<emphasis id="B9554C436734FF8C1A79F878C276D1CB" box="[809,1062,1936,1960]" italics="true" pageId="2" pageNumber="318">
<taxonomicName id="4C21EBD26734FF8C1A79F878C5DBD1C4" box="[809,907,1936,1960]" class="Magnoliopsida" family="Malvaceae" genus="Hibiscus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvaceae" pageId="2" pageNumber="318" phylum="Tracheophyta" rank="genus">Hibiscus</taxonomicName>
rosa-sinensis
</emphasis>
(
<taxonomicName id="4C21EBD26734FF8C1D65F867C2E4D1C4" box="[1077,1204,1935,1960]" class="Branchiopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Malvaceae" pageId="2" pageNumber="318" phylum="Arthropoda" rank="order">Malvaceae</taxonomicName>
