<quantity id="1C0888A59769FFBFFF75FEFDFEEDBEA8" box="[151,275,368,392]" metricMagnitude="-2" metricUnit="m" metricValue="2.3" metricValueMax="2.4" metricValueMin="2.2" pageId="62" pageNumber="63" unit="mm" value="23.0" valueMax="24.0" valueMin="22.0">22–24 mm</quantity>
