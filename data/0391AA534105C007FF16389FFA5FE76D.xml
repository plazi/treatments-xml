</taxonomicName>
) .... .................................................................. 
<taxonomicName id="4C3860C64105C007FCFD3BDCFA5FE08D" authority="(Takagi and Kawai)" baseAuthorityName="Takagi and Kawai" box="[811,1440,897,922]" class="Insecta" family="Diaspididae" genus="Pseudaulacaspis" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="latiloba">
<emphasis id="B94CC7574105C007FCFD3BDCFA5FE08D" bold="true" box="[811,1440,897,922]" pageId="7" pageNumber="6">
<emphasis id="B94CC7574105C007FCFD3BDCFB77E08E" bold="true" box="[811,1160,897,922]" italics="true" pageId="7" pageNumber="6">Pseudaulacaspis latiloba</emphasis>
