<taxonomicName id="4C0F4D69FFE698418B7DD158F82DFD9D" authority="Kozłowska &amp; Bates &amp; Zalasiewicz &amp; Radzevičius, 2019" authorityName="Kozłowska &amp; Bates &amp; Zalasiewicz &amp; Radzevičius" authorityYear="2019" box="[1096,1354,548,572]" class="Graptolithina" family="Retiolitidae" genus="Gothograptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Graptoloidea" pageId="12" pageNumber="447" phylum="Hemichordata" rank="species" species="domeyki" status="n. sp.">
<emphasis id="B97BEAF8FFE698418B7DD158F82DFD9D" box="[1096,1354,548,572]" italics="true" pageId="12" pageNumber="447">Gothograptus domeyki</emphasis>
</taxonomicName>
<emphasis id="B97BEAF8FFE698418A66D158F8F3FD9C" bold="true" box="[1363,1428,548,573]" pageId="12" pageNumber="447">
<taxonomicNameLabel id="A2485783FFE698418A66D158F8F3FD9C" box="[1363,1428,548,573]" pageId="12" pageNumber="447" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
; 
<taxonomicName id="4C0F4D69FFE698418FA2D134FC0FFDC1" authority="Kozłowska &amp; Bates &amp; Zalasiewicz &amp; Radzevičius, 2019" authorityName="Kozłowska &amp; Bates &amp; Zalasiewicz &amp; Radzevičius" authorityYear="2019" box="[151,360,584,608]" class="Graptolithina" family="Retiolitidae" genus="Gothograptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Graptoloidea" pageId="12" pageNumber="447" phylum="Hemichordata" rank="species" species="velo" status="n. sp.">
<emphasis id="B97BEAF8FFE698418FA2D134FC0FFDC1" box="[151,360,584,608]" italics="true" pageId="12" pageNumber="447">Gothograptus velo</emphasis>
</taxonomicName>
<emphasis id="B97BEAF8FFE698418E5AD13BFCC9FDC1" bold="true" box="[367,430,583,608]" pageId="12" pageNumber="447">
<taxonomicNameLabel id="A2485783FFE698418E5AD13BFCC9FDC1" box="[367,430,583,608]" pageId="12" pageNumber="447" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C0F4D69FFE698418E89D134FFAAFDC1" authority="Kozłowska &amp; Bates &amp; Zalasiewicz &amp; Radzevičius, 2019" authorityName="Kozłowska &amp; Bates &amp; Zalasiewicz &amp; Radzevičius" authorityYear="2019" box="[444,717,584,608]" class="Graptolithina" family="Retiolitidae" genus="Gothograptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Graptoloidea" pageId="12" pageNumber="447" phylum="Hemichordata" rank="species" species="diminutus" status="n. sp.">
<emphasis id="B97BEAF8FFE698418E89D134FFAAFDC1" box="[444,717,584,608]" italics="true" pageId="12" pageNumber="447">Gothograptus diminutus</emphasis>
