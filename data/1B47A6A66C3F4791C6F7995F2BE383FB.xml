, 
<taxonomicName id="4D2A32784B8ABD35C8EF6A796F4FBB1F" baseAuthorityName="Jonas" baseAuthorityYear="1844" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="82" pageNumber="83" phylum="Mollusca" rank="species" species="trigonostomus" subGenus="Drymaeus">
<emphasis id="9472B111883E856EF614533AEE08E800" italics="true" pageId="82" pageNumber="83">Drymaeus (Drymaeus) trigonostomus</emphasis>
</taxonomicName>
(Jonas, 1844) (
<emphasis id="86F13ABD17126CB7F16A76A3AAAA7C65" bold="true" pageId="82" pageNumber="83">comb.n.</emphasis>
