<taxonomicName id="4C0E589F3273FF8DE3C1FF5EFD82A79C" authorityName="Poche" authorityYear="1926" box="[409,568,231,255]" class="Cestoda" family="Tentaculariidae" kingdom="Animalia" order="Trypanorhyncha" pageId="4" pageNumber="161" phylum="Platyhelminthes" rank="family">Tentaculariidae</taxonomicName>
