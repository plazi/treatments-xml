<taxonomicName id="4C0E589F3273FF8DE2BBFF5EFE2BA79C" box="[227,401,231,255]" class="Cestoda" kingdom="Animalia" order="Trypanorhyncha" pageId="4" pageNumber="161" phylum="Platyhelminthes" rank="order">Trypanorhyncha</taxonomicName>
