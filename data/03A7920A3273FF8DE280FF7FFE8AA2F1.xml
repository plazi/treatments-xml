<heading id="D0F994703273FF8DE280FF7FFD05A79C" centered="true" fontSize="10" level="2" pageId="4" pageNumber="161" reason="2">
<taxonomicName id="4C0E589F3273FF8DE280FF7FFE7BA7BD" authorityName="Okada in Dollfus" authorityYear="1929" box="[216,449,198,222]" class="Cestoda" family="Tentaculariidae" genus="Nybelinia" kingdom="Animalia" order="Trypanorhyncha" pageId="4" pageNumber="161" phylum="Platyhelminthes" rank="species" species="surmenicola">
