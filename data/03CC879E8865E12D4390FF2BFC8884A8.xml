<taxonomicName id="4C654D0B8865E12D4390FED2FE3A8711" authority="Hutton, 1901: 75" authorityName="Hutton" authorityPageNumber="75" authorityYear="1901" box="[151,388,291,314]" class="Insecta" family="Muscidae" genus="Exsul" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="383" phylum="Arthropoda" rank="genus">
<emphasis id="B911EA9A8865E12D4390FED2FF6E8712" box="[151,208,292,314]" italics="true" pageId="1" pageNumber="383">Exsul</emphasis>
Hutton, 
<number id="3DAD24AF8865E12D422FFED2FEE08711" box="[296,350,292,313]" pageId="1" pageNumber="383" value="1901.0">1901</number>
