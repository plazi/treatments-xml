</bibRefCitation>
(as 
<taxonomicName id="24F550E94752FFD4C797E240FDA9FAA7" authority="Pjatnitskii, 1929" authorityName="Pjatnitskii" authorityYear="1929" box="[151,636,1372,1397]" class="Insecta" family="Curculionidae" genus="Hypothenemus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="404" phylum="Arthropoda" rank="species" species="lezhavai">
<emphasis id="D181F7784752FFD4C797E240FE7EFAA6" box="[151,427,1372,1397]" italics="true" pageId="3" pageNumber="404">Hypothenemus lezhavai</emphasis>
Pjatnitskii, 
<number id="553D394D4752FFD4C544E240FDA9FAA7" box="[580,636,1372,1396]" pageId="3" pageNumber="404" value="1929.0">1929</number>
