<taxonomicName id="24F550E94752FFD4C5C4E240FB1BFAA6" authority="Wollaston, 1860" authorityName="Wollaston" authorityYear="1860" box="[708,1230,1372,1397]" class="Insecta" family="Curculionidae" genus="Hypothenemus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="404" phylum="Arthropoda" rank="species" species="aspericollis">
