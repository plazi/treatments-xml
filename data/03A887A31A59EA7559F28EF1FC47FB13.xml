<number id="3DC924921A59EA7559B68D25FEA2FCAF" box="[211,258,835,859]" pageId="3" pageNumber="372" value="2.2">2, 2</number>
, 
<number id="3DC924921A59EA7558748D25FEE0FCAF" box="[273,320,835,859]" pageId="3" pageNumber="372" value="1.2">1, 2</number>
, 
