<taxonomicName id="57C74D62FFB17D3BFE19F9F9FC79F9BB" authority="Berkeley and Berkeley, 1944" authorityName="Berkeley and Berkeley" authorityYear="1944" box="[401,1022,1605,1628]" class="Polychaeta" family="Sphaerodoridae" genus="Sphaerodoropsis" kingdom="Animalia" order="Phyllodocida" pageId="9" pageNumber="10" phylum="Annelida" rank="species" species="biserialis">
