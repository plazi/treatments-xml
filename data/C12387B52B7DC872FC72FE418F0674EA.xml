<collectingCountry id="319D76332B7DC872FBEEFA938FE1778A" box="[1097,1219,1391,1414]" name="Azerbaijan" pageId="5" pageNumber="634">Azerbaijan</collectingCountry>
, 
<collectingCountry id="319D76332B7DC872FB76FA938E08778A" box="[1233,1322,1391,1414]" name="Georgia" pageId="5" pageNumber="634">Georgia</collectingCountry>
