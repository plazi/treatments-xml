<materialsCitation id="3B603CF5FFA7FF86FF01CAC4FEC0C865" ID-GBIF-Occurrence="1306554518" location="Agriculture Farm" municipality="Faculty" pageId="4" pageNumber="483" specimenCount="2" stateProvince="Cairo" typeStatus="holotype">
<emphasis id="B97CEABAFFA7FF86FF01CAC4FEA2C9AD" bold="true" box="[199,315,1198,1223]" pageId="4" pageNumber="483">Remarks.</emphasis>
