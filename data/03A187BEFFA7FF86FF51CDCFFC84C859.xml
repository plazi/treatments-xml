<collectingRegion id="49CCF84AFFA7FF86FF51CA9CFF4CC865" box="[151,213,1270,1295]" country="Egypt" name="Al Qahirah" pageId="4" pageNumber="483">Cairo</collectingRegion>
University
</materialsCitation>
, 
<materialsCitation id="3B603CF5FFA7FF86FEAFCA9CFC84C859" ID-GBIF-Occurrence="1306554519" location="Giza governorate" pageId="4" pageNumber="483" specimenCount="1" stateProvince="Giza governorate" typeStatus="holotype">
