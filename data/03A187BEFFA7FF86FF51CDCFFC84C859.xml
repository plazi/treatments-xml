<location id="8ED76073FFA7FF86FB09CAB8FA01C980" LSID="urn:lsid:plazi:treatment:03A187BEFFA7FF86FF51CDCFFC84C859:8ED76073FFA7FF86FB09CAB8FA01C980" box="[1231,1432,1234,1259]" municipality="Faculty" name="Agriculture Farm" pageId="4" pageNumber="483" stateProvince="Cairo">Agriculture Farm</location>
