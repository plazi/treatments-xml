<quantity id="4CEF9B25FF11FF359C28FA34FEDE35E4" box="[1109,1183,1478,1505]" metricMagnitude="2" metricUnit="m" metricValue="2.35" pageId="128" pageNumber="129" unit="m" value="235.0">235 m</quantity>
(
<quantity id="4CEF9B25FF11FF359CCCFA35FEB835E7" box="[1201,1273,1479,1506]" metricMagnitude="2" metricUnit="m" metricValue="2.350008" pageId="128" pageNumber="129" unit="ft" value="771.0">771 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FF11FF359D25FA35FB403600" author="Peden, A. E." pageId="128" pageNumber="129" pagination="11 - 49" refId="ref310919" refString="Peden, A. E. (1978) A systematic revision of the hemilepidotine fishes (Cottidae). Syesis, 11, 11 - 49." type="journal article" year="1978">Peden 1978</bibRefCitation>
; max.: 
<bibRefCitation id="EF864B31FF11FF35992BFA19F8073600" author="Pucunski" box="[342,582,1515,1541]" etAl="et al." firstAuthor="Pucunski" pageId="128" pageNumber="129" refId="ref314885" refString="Pucunski, R. E., Palsson, W. A. &amp; Greene, H. G. (2013) Estimating fish abundance and community composition on rocky habitats in the San Juan islands using a small remotely operated vehicle. Washington Department of Fisheries and Wildlife, Fish Program, Fisheries Management Division, FPT 13 - 02." type="book" year="2013">
Pucunski 
<emphasis id="B963EAD2FF11FF3599BBFA19F8413600" box="[454,512,1515,1541]" italics="true" pageId="128" pageNumber="129">et al.</emphasis>
2013
</bibRefCitation>
). A Diablo Cove, central 
<collectingRegion id="49D3F822FF11FF359B14FA19F99C3600" box="[873,989,1515,1541]" country="United States of America" name="California" pageId="128" pageNumber="129">California</collectingRegion>
, observation (
<bibRefCitation id="EF864B31FF11FF359C03FA19FFD53600" author="Burge and Schultz" box="[1150,1428,1515,1541]" firstAuthor="Burge" pageId="128" pageNumber="129" refId="ref266400" refString="Burge, R. T. &amp; Schultz, S. A. (1973) The marine environment in the vicinity of Diablo Cove with special reference to abalones and bony fishes. California Department of Fish and Game, Marine Resources Technical Report, 19." type="book" year="1973">Burge and Schultz 1973</bibRefCitation>
