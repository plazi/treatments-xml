<taxonomicName id="0C9316BCFF9ED41C28D87927FDC74E2D" authority="Jacobi, 1928" authorityName="Jacobi" authorityYear="1928" box="[151,580,1350,1376]" class="Insecta" family="Cixiidae" genus="Bajauana" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="praetextata">
