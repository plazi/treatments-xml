Type species 
<taxonomicName id="4C0647F1FFF5FFF5FEC6FB10FD21E8FB" authority="Simon" authorityName="Simon" authorityYear="1891" box="[287,654,1153,1175]" class="Arachnida" family="Oonopidae" genus="Ischnothyreus" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="peltifer">
