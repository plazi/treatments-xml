<collectingCountry id="F31252ADFFB3FF922C1F13C6FC88FB6D" box="[797,894,1200,1221]" name="Argentina" pageId="14" pageNumber="22">Argentina</collectingCountry>
; 
<collectingCountry id="F31252ADFFB3FF922C8B13C6FC22FB6D" box="[905,980,1200,1221]" name="Bolivia" pageId="14" pageNumber="22">Bolivia</collectingCountry>
*; 
<collectingCountry id="F31252ADFFB3FF922CEB13C6FBD3FB6D" box="[1001,1061,1200,1221]" name="Brazil" pageId="14" pageNumber="22">Brazil</collectingCountry>
