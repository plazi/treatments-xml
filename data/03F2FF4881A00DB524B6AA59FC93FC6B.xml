<taxonomicName id="4C5B35DD81A00DB52707AAF0FC1FFE2B" box="[806,913,398,421]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="139" pageNumber="140" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
