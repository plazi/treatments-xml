<number id="3DB1CF0DFFF7FF8DA89AF325FD8F4D31" box="[481,523,1580,1604]" pageId="1" pageNumber="94" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
</figureCitation>
