<typeStatus id="140E83F29723FFF6FE72F898830EF8D4" box="[503,556,1863,1886]" pageId="0" pageNumber="59">Type</typeStatus>
species: 
<taxonomicName id="0CB546D39723FFF6FD0BF897852FF8D4" authority="Matsumura, 1900" authorityName="Matsumura" authorityYear="1900" box="[654,1037,1863,1887]" class="Insecta" family="Tortricidae" genus="Semasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="59" phylum="Arthropoda" rank="species" species="phaseoli">
