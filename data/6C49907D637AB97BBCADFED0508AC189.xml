; 
<location id="E13F77B0637AB97BBCD2FD9F5320C769" LSID="urn:lsid:plazi:treatment:6C49907D637AB97BBCADFED0508AC189:E13F77B0637AB97BBCD2FD9F5320C769" box="[131,264,599,625]" country="India" name="Lushai Hills" pageId="8" pageNumber="21" stateProvince="Assam">Lushai Hills</location>
, about 
<quantity id="23188C8E637AB97BBD07FD905387C76A" box="[342,431,600,626]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="8" pageNumber="21" unit="m" value="1700.0">
