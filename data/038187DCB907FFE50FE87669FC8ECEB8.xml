<location id="8EF76011B907FFEA0F3F73C9FC4ECB9A" LSID="urn:lsid:plazi:treatment:038187DCB907FFE50FE87669FC8ECEB8:8EF76011B907FFEA0F3F73C9FC4ECB9A" box="[857,966,1711,1735]" name="upper Rio" pageId="7" pageNumber="468" stateProvince="Amazonas, Rio Ucayali">upper Río</location>
