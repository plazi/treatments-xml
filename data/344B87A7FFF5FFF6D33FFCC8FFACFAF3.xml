<taxonomicName id="7BE24D32FFF5FFF6D16BFBC7FDC6FBEF" authorityName="Solier" authorityYear="1849" box="[837,960,1078,1097]" class="Insecta" family="Melyridae" genus="Arthrobrachus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="612" phylum="Arthropoda" rank="genus">Arthrobrachus</taxonomicName>
