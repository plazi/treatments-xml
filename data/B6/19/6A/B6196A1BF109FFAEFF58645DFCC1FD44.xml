<subSubSection pageId="3" pageNumber="296" type="etymology">
<paragraph blockId="3.[151,1436,641,702]" box="[199,1436,641,666]" pageId="3" pageNumber="296">
<emphasis bold="true" box="[199,334,641,666]" pageId="3" pageNumber="296">Etymology.</emphasis>
