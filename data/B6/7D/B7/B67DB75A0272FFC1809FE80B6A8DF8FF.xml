<taxonomicName box="[866,956,1173,1196]" class="Insecta" family="Thripidae" genus="Scirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="flavus" status="n. sp.">
<emphasis box="[866,879,1173,1196]" italics="true" pageId="19" pageNumber="20">S</emphasis>
. 
<emphasis box="[892,956,1173,1196]" italics="true" pageId="19" pageNumber="20">flavus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[963,1023,1173,1197]" pageId="19" pageNumber="20">
<taxonomicNameLabel box="[963,1023,1173,1197]" pageId="19" pageNumber="20" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
; (B) 
<taxonomicName box="[1079,1186,1173,1196]" class="Insecta" family="Thripidae" genus="Scirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="katsura" status="n. sp.">
<emphasis box="[1079,1092,1173,1196]" italics="true" pageId="19" pageNumber="20">S</emphasis>
. 
<emphasis box="[1106,1186,1173,1196]" italics="true" pageId="19" pageNumber="20">katsura</emphasis>
