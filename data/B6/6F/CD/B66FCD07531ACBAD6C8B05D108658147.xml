<taxonomicName authority="Pascoe, 1864" authorityName="Pascoe" authorityYear="1864" class="Insecta" family="Cerambycidae" genus="Catypnes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Catypnes" order="Coleoptera" pageId="196" pageNumber="197" phylum="Arthropoda" rank="genus">Catypnes</taxonomicName>
