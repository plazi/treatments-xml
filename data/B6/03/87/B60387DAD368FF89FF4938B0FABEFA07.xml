), Glorioso Islands, 
<collectingCountry box="[151,342,1319,1344]" name="Christmas Island" pageId="2" pageNumber="465">Christmas Island</collectingCountry>
, 
<collectingCountry box="[353,625,1319,1344]" name="Cocos (Keeling) Islands" pageId="2" pageNumber="465">Cocos (Keeling) Islands</collectingCountry>
, 
<collectingCountry box="[636,748,1319,1344]" name="Indonesia" pageId="2" pageNumber="465">Indonesia</collectingCountry>
, 
<collectingCountry box="[759,857,1319,1344]" name="Vietnam" pageId="2" pageNumber="465">Vietnam</collectingCountry>
, 
<collectingCountry box="[867,952,1319,1344]" name="Taiwan" pageId="2" pageNumber="465">Taiwan</collectingCountry>
, 
<collectingCountry box="[963,1030,1320,1343]" name="Japan" pageId="2" pageNumber="465">Japan</collectingCountry>
, eastern 
<collectingCountry box="[1126,1231,1319,1344]" name="Australia" pageId="2" pageNumber="465">Australia</collectingCountry>
, 
<collectingCountry box="[1242,1432,1319,1344]" name="Solomon Islands" pageId="2" pageNumber="465">Solomon Islands</collectingCountry>
, 
<collectingCountry box="[151,225,1356,1381]" name="United Kingdom" pageId="2" pageNumber="465">Wallis</collectingCountry>
, 
<collectingCountry box="[236,317,1357,1380]" name="Wallis and Futuna" pageId="2" pageNumber="465">Futuna</collectingCountry>
, 
<collectingCountry box="[327,406,1356,1380]" name="Samoa" pageId="2" pageNumber="465">Samoa</collectingCountry>
, and 
<collectingCountry box="[464,661,1356,1381]" name="French Polynesia" pageId="2" pageNumber="465">French Polynesia</collectingCountry>
; 
<quantity box="[673,758,1356,1381]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="3.5" metricValueMin="0.0" pageId="2" pageNumber="465" unit="m" value="17.5" valueMax="35.0" valueMin="0.0">0–35 m</quantity>
