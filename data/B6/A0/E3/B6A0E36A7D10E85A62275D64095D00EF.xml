<taxonomicName class="Insecta" family="Curculionidae" genus="Somatodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Somatodes" order="Coleoptera" pageId="261" pageNumber="262" phylum="Arthropoda" rank="genus">Somatodes</taxonomicName>
