; PL 
<quantity box="[821,987,296,320]" metricMagnitude="-4" metricUnit="m" metricValue="3.55" metricValueMax="3.7" metricValueMin="3.4" pageId="6" pageNumber="449" unit="mm" value="0.355" valueMax="0.37" valueMin="0.34">0.34‒0.37 mm</quantity>
; PW 
