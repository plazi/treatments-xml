; EL 
<quantity box="[1266,1432,295,320]" metricMagnitude="-4" metricUnit="m" metricValue="5.6499999999999995" metricValueMax="5.8999999999999995" metricValueMin="5.4" pageId="6" pageNumber="449" unit="mm" value="0.565" valueMax="0.59" valueMin="0.54">0.54‒0.59 mm</quantity>
; EW 
<quantity box="[203,370,332,356]" metricMagnitude="-4" metricUnit="m" metricValue="8.049999999999999" metricValueMax="8.4" metricValueMin="7.7" pageId="6" pageNumber="449" unit="mm" value="0.8049999999999999" valueMax="0.84" valueMin="0.77">0.77‒0.84 mm</quantity>
; AL 
