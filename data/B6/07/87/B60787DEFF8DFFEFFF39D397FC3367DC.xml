; HW 
<quantity box="[823,989,1770,1795]" metricMagnitude="-4" metricUnit="m" metricValue="4.4" metricValueMax="4.5" metricValueMin="4.3" pageId="6" pageNumber="449" unit="mm" value="0.44" valueMax="0.45" valueMin="0.43">0.43‒0.45 mm</quantity>
; PL 
