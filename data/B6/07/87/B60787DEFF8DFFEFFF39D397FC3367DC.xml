; PW 
<quantity box="[1266,1432,1770,1795]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="6" pageNumber="449" unit="mm" value="0.45" valueMax="0.5" valueMin="0.4">0.40‒0.50 mm</quantity>
; EL 
<quantity box="[194,362,1806,1831]" metricMagnitude="-4" metricUnit="m" metricValue="5.85" metricValueMax="6.5" metricValueMin="5.2" pageId="6" pageNumber="449" unit="mm" value="0.585" valueMax="0.65" valueMin="0.52">0.52‒0.65 mm</quantity>
; EW 
<quantity box="[428,597,1806,1831]" metricMagnitude="-4" metricUnit="m" metricValue="7.949999999999999" metricValueMax="8.5" metricValueMin="7.4" pageId="6" pageNumber="449" unit="mm" value="0.7949999999999999" valueMax="0.85" valueMin="0.74">0.74‒0.85 mm</quantity>
; AL 
<quantity box="[656,824,1806,1831]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="8.1" metricValueMin="6.8999999999999995" pageId="6" pageNumber="449" unit="mm" value="0.75" valueMax="0.81" valueMin="0.69">0.69‒0.81 mm</quantity>
; AW 
<quantity box="[891,1060,1806,1831]" metricMagnitude="-4" metricUnit="m" metricValue="7.25" metricValueMax="7.9" metricValueMin="6.6" pageId="6" pageNumber="449" unit="mm" value="0.725" valueMax="0.79" valueMin="0.66">0.66‒0.79 mm</quantity>
; width between discal carinae of tergite IV 
<quantity box="[265,428,1842,1867]" metricMagnitude="-4" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="6" pageNumber="449" unit="mm" value="0.275" valueMax="0.3" valueMin="0.25">0.25‒0.30 mm</quantity>
