<paragraph blockId="7.[151,1436,566,662]" pageId="7" pageNumber="66">
<taxonomicName box="[151,243,566,590]" class="Insecta" family="Noctuidae" genus="Galgula" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="66" phylum="Arthropoda" rank="genus">
