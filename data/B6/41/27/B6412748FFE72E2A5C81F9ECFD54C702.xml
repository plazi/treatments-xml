<collectingRegion box="[1038,1114,153,179]" country="China" name="Fujian" pageId="6" pageNumber="201">Fujian</collectingRegion>
, 
<collectingRegion box="[1127,1261,153,179]" country="China" name="Guangdong" pageId="6" pageNumber="201">Guangdong</collectingRegion>
, 
<collectingRegion box="[1273,1372,153,179]" country="China" name="Guangxi" pageId="6" pageNumber="201">Guangxi</collectingRegion>
, 
<collectingRegion country="China" name="Guizhou" pageId="6" pageNumber="201">Guizhou</collectingRegion>
, 
<collectingRegion box="[220,296,189,215]" country="China" name="Henan" pageId="6" pageNumber="201">Henan</collectingRegion>
, 
<collectingRegion box="[307,378,189,215]" country="China" name="Hubei" pageId="6" pageNumber="201">Hubei</collectingRegion>
, 
<collectingRegion box="[390,483,189,215]" country="China" name="Ningxia" pageId="6" pageNumber="201">Ningxia</collectingRegion>
, 
<collectingRegion box="[494,587,189,215]" country="China" name="Shaanxi" pageId="6" pageNumber="201">Shaanxi</collectingRegion>
, 
<collectingRegion box="[598,691,189,215]" country="China" name="Sichuan" pageId="6" pageNumber="201">Sichuan</collectingRegion>
