<quantity box="[229,393,583,607]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" metricValueMax="4.3" metricValueMin="3.7" pageId="15" pageNumber="516" unit="mm" value="0.4" valueMax="0.43" valueMin="0.37">0.37–0.43 mm</quantity>
