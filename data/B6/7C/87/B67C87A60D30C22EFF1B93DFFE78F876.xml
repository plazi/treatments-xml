ɞ labeled [
<typeStatus box="[827,891,1699,1725]" pageId="20" pageNumber="21">Type</typeStatus>
] [
<taxonomicName box="[915,1018,1700,1725]" class="Insecta" family="Fulgoridae" genus="Scamandra" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="basigera">
<emphasis box="[915,1018,1700,1725]" italics="true" pageId="20" pageNumber="21">basigera</emphasis>
</taxonomicName>
