<geoCoordinate direction="north" orientation="latitude" precision="1" value="39.163002">39°09'46.8&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="19.439333">19°26'21.6&quot;E</geoCoordinate>
