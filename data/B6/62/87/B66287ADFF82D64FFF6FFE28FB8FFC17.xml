<paragraph blockId="18.[151,520,402,428]" box="[151,520,402,428]" pageId="18" pageNumber="535">
<heading bold="true" box="[151,520,402,428]" fontSize="11" level="1" pageId="18" pageNumber="535" reason="1">
<emphasis bold="true" box="[151,520,402,428]" pageId="18" pageNumber="535">
Genus 
<taxonomicName authority="Bamber, 2012" authorityName="Bamber" authorityYear="2012" box="[238,520,402,428]" class="Malacostraca" family="Nototanaidae" genus="Gamboa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tanaidacea" pageId="18" pageNumber="535" phylum="Arthropoda" rank="genus">
