<taxonomicName authority="W.A.J.Lichtenstein, 1876" authorityName="W.A.J.Lichtenstein" authorityYear="1876" class="Insecta" family="Chrysididae" genus="Trichrysis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trichrysis cyanea" order="Hymenoptera" pageId="1" pageNumber="110" phylum="Arthropoda" rank="species" species="cyanea">Trichrysis cyanea</taxonomicName>
