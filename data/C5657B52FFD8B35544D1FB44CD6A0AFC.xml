, 
<collectingRegion id="8F0804A6FFDAB3554595FF16CA420BDB" box="[467,580,223,247]" country="United States of America" name="California" pageId="70" pageNumber="71">California</collectingRegion>
</materialsCitation>
<materialsCitation id="FDA4C019FFDAB355460DFF29CBEC0BDB" box="[587,1002,223,248]" location="Santa Inez Bay" pageId="70" pageNumber="71" specimenCount="1" stateProvince="California">
to 
<location id="48139C9FFFDAB3554621FF16CB170BD4" LSID="urn:lsid:plazi:treatment:C5657B52FFD8B35544D1FB44CD6A0AFC:48139C9FFFDAB3554621FF16CB170BD4" box="[615,785,223,248]" name="Santa Inez Bay" pageId="70" pageNumber="71" stateProvince="California">Santa Inez Bay</location>
, Gulf of 
<collectingRegion id="8F0804A6FFDAB3554733FF16CBEC0BDB" box="[885,1002,223,247]" country="United States of America" name="California" pageId="70" pageNumber="71">California</collectingRegion>
</materialsCitation>
. 
<materialsCitation id="FDA4C019FFDAB35547B2FF29CD9E0BDB" box="[1012,1432,223,248]" location="Bodega Bay" pageId="70" pageNumber="71" specimenCount="1" stateProvince="California">
