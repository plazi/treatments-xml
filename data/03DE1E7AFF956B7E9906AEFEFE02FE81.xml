<number id="3DBFBD4BFF956B7C9CE3AD15FAF7FDAE" box="[1261,1316,599,625]" pageId="1" pageNumber="28" value="0.18">0.18</number>
; posterior space 
<number id="3DBFBD4BFF956B7C99CFAD3DFE2BFD46" box="[449,504,639,665]" pageId="1" pageNumber="28" value="0.4">0.40</number>
