<paragraph id="8BD836D3FFA1E5300B16FD265A0F9AB4" blockId="39.[264,1324,284,1871]" pageId="39" pageNumber="40">
<collectionCode id="ED76AE16FFA1E5300B16FD265AE39A44" box="[312,410,1605,1631]" pageId="39" pageNumber="40">OTHER</collectionCode>
MATERIAL EXAMINED. 
<collectingCountry id="F3707643FFA1E5300929FD2658EB9A44" box="[775,914,1605,1631]" name="Ethiopia" pageId="39" pageNumber="40">ETHIOPIA</collectingCountry>
