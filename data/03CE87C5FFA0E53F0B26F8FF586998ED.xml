<collectionCode id="ED76AE16FFA1E5300BA7FEAE5AA099FC" box="[393,473,1485,1511]" pageId="39" pageNumber="40">DATA</collectionCode>
. 
<typeStatus id="54DC8871FFA1E5300BC2FEAE592099FC" box="[492,601,1485,1511]" pageId="39" pageNumber="40">Holotype</typeStatus>
male (MEP Acc. 
<number id="3DAF24F4FFA1E5300900FEAE582499FC" box="[814,861,1485,1511]" pageId="39" pageNumber="40" value="723.0">723</number>
