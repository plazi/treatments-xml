<taxonomicName id="7E5AFD7D7FB9F4FDC934950D79EE25D4" class="Actinopterygii" family="Cyprinidae" genus="Pseudobarbus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudobarbus senticeps" order="Cypriniformes" pageId="8" pageNumber="117" phylum="Chordata" rank="species" species="senticeps">Pseudobarbus senticeps</taxonomicName>
