<taxonomicName id="4C264D12FFABFFBAFEC75816FC84FCA0" authority="Mulsant 1850" authorityName="Mulsant" authorityYear="1850" box="[379,777,835,859]" class="Insecta" family="Coccinellidae" genus="Cleothera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="3" phylum="Arthropoda" rank="species" species="jucunda">
