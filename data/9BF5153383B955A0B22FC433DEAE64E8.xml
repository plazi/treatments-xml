<taxonomicName id="BC671EA1B07B0EBDF61580D3C8048060" class="Magnoliopsida" family="Adoxaceae" genus="Sambucus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Sambucus" order="Dipsacales" pageId="0" pageNumber="109" phylum="Angiospermae" rank="genus">
