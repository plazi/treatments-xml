<taxonomicName id="CFC4F979D4843CE9735139DDCA1E6D60" authority="Fleutiaux, 1928" authorityName="Fleutiaux" authorityYear="1928" class="Insecta" family="Elateridae" genus="Gamepenthes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gamepenthes" order="Coleoptera" pageId="0" pageNumber="109" phylum="Arthropoda" rank="genus">
<emphasis id="AA5360168F266C20260DB78EBEC344FF" italics="true" pageId="0" pageNumber="109">Gamepenthes</emphasis>
