<taxonomicName id="61C96E7D18CC00D18CFA4B3EE677DE39" genus="G." lsidName="G. pictipennis" pageId="0" pageNumber="109" rank="species" species="pictipennis">
<emphasis id="5C6ADC61EC648B5CB40A85FD0A0FA0F3" italics="true" pageId="0" pageNumber="109">G. pictipennis</emphasis>
</taxonomicName>
(Lewis, 1894); 
<taxonomicName id="32F9339D78B82269F453E5088B8CC86A" genus="G." lsidName="G. similis" pageId="0" pageNumber="109" rank="species" species="similis">
<emphasis id="6382C7D488F1F129FEB852259C33720F" italics="true" pageId="0" pageNumber="109">G. similis</emphasis>
</taxonomicName>
(Lewis, 1894); 
<taxonomicName id="63DC9D8BEF6DAE6625E3BF37630059E4" genus="G." lsidName="G. versipellis" pageId="0" pageNumber="109" rank="species" species="versipellis">
<emphasis id="81DC641014A1BBA677E2E42DA8D02432" italics="true" pageId="0" pageNumber="109">G. versipellis</emphasis>
</taxonomicName>
(Lewis, 1894); 
<taxonomicName id="96817856C1577338EED3432082561E58" genus="G." lsidName="G. yoshidai" pageId="0" pageNumber="109" rank="species" species="yoshidai">
<emphasis id="0D03B81E0BDBDA9A477BEEFE228857E7" italics="true" pageId="0" pageNumber="109">G. yoshidai</emphasis>
</taxonomicName>
<normalizedToken id="834D7D868C96317C5FFA1545A2C9725B" originalValue="Ôhira">Ohira</normalizedToken>
, 1995.
</paragraph>
