<taxonomicName id="87A6797A86595FA90E7419038839400A" class="Magnoliopsida" family="Araliaceae" genus="Aralia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Aralia cordata" order="Apiales" pageId="0" pageNumber="109" phylum="Tracheophyta" rank="species" species="cordata">
