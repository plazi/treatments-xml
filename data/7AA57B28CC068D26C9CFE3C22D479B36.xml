<taxonomicName id="03C826FC1F87FA2BA8D05B89319577C0" class="Insecta" family="Sphingidae" genus="Xylophanes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xylophanes tersa" order="Lepidoptera" pageId="10" pageNumber="147" phylum="Arthropoda" rank="species" species="tersa">Xylophanes tersa</taxonomicName>
