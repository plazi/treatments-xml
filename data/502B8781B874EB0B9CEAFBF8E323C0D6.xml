<taxonomicName id="1F824D14B874EB0B9D89FB8FE34EC2E5" authorityName="Vincent &amp; Laguerre" authorityYear="2013" baseAuthorityName="Dyar" baseAuthorityYear="1912" box="[936,1109,1144,1168]" class="Insecta" family="Arctiidae" genus="Elysius" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="440" phylum="Arthropoda" rank="species" species="paranomon" status="n. comb.">
