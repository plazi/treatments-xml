<taxonomicName id="4EF444E5FFB5FFBAFB7D01C5FAA2B8C9" authorityName="Distant" authorityYear="1903" box="[1178,1294,730,756]" class="Insecta" family="Reduviidae" genus="Gerbelius" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="genus">
