<taxonomicName id="2BF04D73AF69FF925CD6F974DEFF38F8" authority="Hoberlandt (1956)" authorityName="Hoberlandt" authorityYear="1956" box="[651,1005,1732,1757]" class="Insecta" family="Pentatomidae" genus="Tarisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="osmanica">
