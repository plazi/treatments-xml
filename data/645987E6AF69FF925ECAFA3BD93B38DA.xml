<taxonomicName id="2BF04D73AF69FF925FBAF958DF413925" box="[487,595,1769,1792]" class="Insecta" family="Pentatomidae" genus="Tarisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="osmanica">
