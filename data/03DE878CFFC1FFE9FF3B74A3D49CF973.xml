<collectingCountry id="F360760AFFC1FFE9FC357643D3FEF9E3" box="[921,1006,1555,1581]" name="Bolivia" pageId="57" pageNumber="58">Bolivia</collectingCountry>
, 
<collectingCountry id="F360760AFFC1FFE9FC577643D452F9E3" box="[1019,1090,1555,1581]" name="Brazil" pageId="57" pageNumber="58">Brazil</collectingCountry>
