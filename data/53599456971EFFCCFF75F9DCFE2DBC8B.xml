</taxonomicName>
; 
<emphasis id="E984F952971CFFCAFB76FA5CFB40BAC8" bold="true" box="[1172,1214,1489,1511]" pageId="75" pageNumber="76">G–I</emphasis>
, 
<taxonomicName id="1CF05EC3971CFFCAFB2EFA5FFF31B929" authority="(Griffini, 1911)" authorityName="Griffini" authorityYear="1911" baseAuthorityName="Griffini" baseAuthorityYear="1911" class="Insecta" family="Gryllacrididae" genus="Ocellarnaca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="braueri">
<emphasis id="E984F952971CFFCAFB2EFA5FFAC6BAC8" box="[1228,1336,1489,1511]" italics="true" pageId="75" pageNumber="76">O. braueri</emphasis>
