<taxonomicName id="4C764D6FFFF14C67FF5AECDE6E4EF900" authority="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord, 2022" authorityName="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord" authorityYear="2022" box="[208,382,1684,1708]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="armata" status="n. sp.">
