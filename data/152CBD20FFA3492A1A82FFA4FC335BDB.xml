<taxonomicName id="5A8577B5FFA3492A1B3DFE02FD865D46" authority="Bates, 1864" authorityName="Bates" authorityYear="1864" box="[296,598,790,813]" class="Insecta" family="Cerambycidae" genus="Stenolis" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="undulata">
<emphasis id="AFF1D024FFA3492A1B3DFE02FE075D47" box="[296,471,791,813]" italics="true" pageId="1" pageNumber="40">Stenolis undulata</emphasis>
<bibRefCitation id="F91471C7FFA3492A1BCBFE02FD865D46" author="Bates" box="[478,598,790,812]" pageId="1" pageNumber="40" refString="Bates, H. W. (1864) Contributions to an insect fauna of the Amazon Valley. Coleoptera: Longicornes. The Annals and Magazine of Natural History, London, (3) 13, 144 - 164." type="journal article" year="1864">
Bates, 
<number id="2B4D1E11FFA3492A1836FE03FD865D46" box="[547,598,790,812]" pageId="1" pageNumber="40" value="1864.0">1864</number>
</bibRefCitation>
</taxonomicName>
by monotypy [= 
<taxonomicName id="5A8577B5FFA3492A1910FE02FB585D46" authority="Fabricius, 1801" authorityName="Fabricius" authorityYear="1801" box="[773,1160,790,813]" class="Insecta" family="Cerambycidae" genus="Stenocorus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="angulatus">
