(
<bibRefCitation id="052B4B43FFD2FFCBD393A83CFA9FD712" author="Desvignes T." box="[1191,1390,842,869]" pageId="3" pageNumber="4" refId="ref11137" refString="Desvignes T. 1856. Catalogue of British Ichneumonidae in the collection of the British Museum. London." type="book" year="1856">Desvignes, 1856</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
