<taxonomicName authority="Buquet." authorityName="Buquet." box="[1227,1437,896,922]" class="Insecta" family="Cerambycidae" genus="Hemicladus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="44" phylum="Arthropoda" rank="species" species="dejeanii">
