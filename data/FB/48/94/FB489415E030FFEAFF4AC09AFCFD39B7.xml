<taxonomicName baseAuthorityName="Bolivar" baseAuthorityYear="1912" box="[1293,1388,908,933]" class="Insecta" family="Pamphagidae" genus="Pseudoglauia" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="414" phylum="Arthropoda" rank="species" species="terrea">
