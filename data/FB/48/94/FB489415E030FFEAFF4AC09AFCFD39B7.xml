<taxonomicName baseAuthorityName="Bolivar" baseAuthorityYear="1914" box="[151,310,943,969]" class="Insecta" family="Pamphagidae" genus="Pseudoglauia" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="414" phylum="Arthropoda" rank="species" species="tarudantica">
<emphasis box="[151,310,943,969]" italics="true" pageId="5" pageNumber="414">P. tarudantica</emphasis>
