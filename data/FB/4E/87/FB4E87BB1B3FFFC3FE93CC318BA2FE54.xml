<subSubSection box="[312,1094,1167,1194]" pageId="6" pageNumber="63" type="etymology">
<paragraph blockId="6.[264,1323,697,1899]" box="[312,1094,1167,1194]" pageId="6" pageNumber="63">
<emphasis bold="true" box="[312,456,1167,1193]" pageId="6" pageNumber="63">Etymology:</emphasis>
