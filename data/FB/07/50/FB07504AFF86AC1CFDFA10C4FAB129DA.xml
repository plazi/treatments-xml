<taxonomicName authority="Dalman" authorityName="Dalman" authorityYear="1823" box="[685,985,1518,1545]" class="Insecta" family="Figitidae" genus="Anacharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
