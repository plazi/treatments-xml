<taxonomicName class="Insecta" family="Carabidae" genus="Omaseus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="509" pageNumber="510" phylum="Arthropoda" rank="genus">
