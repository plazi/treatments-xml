<taxonomicName class="Hexapoda" family="Carabidae" genus="Carabus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="509" pageNumber="510" phylum="Arthropoda" rank="species" species="nigrita">
