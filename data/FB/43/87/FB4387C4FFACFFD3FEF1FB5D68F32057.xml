<collectionCode box="[606,676,1626,1650]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" lsid="urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="12" pageNumber="258">ANIC</collectionCode>
