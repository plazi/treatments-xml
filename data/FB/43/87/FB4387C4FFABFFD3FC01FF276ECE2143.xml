<subSubSection pageId="12" pageNumber="258" type="distribution">
<paragraph blockId="12.[121,749,607,690]" pageId="12" pageNumber="258">
<emphasis bold="true" box="[121,257,607,631]" pageId="12" pageNumber="258">Distribution</emphasis>
