<paragraph blockId="6.[227,1436,623,755]" pageId="6" pageNumber="7">
Body 
<quantity box="[302,448,623,649]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" metricValueMax="2.1" metricValueMin="1.9" pageId="6" pageNumber="7" unit="mm" value="2.0" valueMax="2.1" valueMin="1.9">1.9–2.1 mm</quantity>
