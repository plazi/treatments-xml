- 
<accessionNumber box="[974,1105,979,1005]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY570827" pageId="8" pageNumber="33">
AY 
<number box="[1017,1105,979,1005]" pageId="8" pageNumber="33" value="570827.0">570827</number>
</accessionNumber>
, respectively) or mitochondrial gene 
<collectionCode LSID="urn:lsid:biocol.org:col:14498" box="[274,324,1016,1042]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14498" name="University of Coimbra Botany Department" pageId="8" pageNumber="33">COI</collectionCode>
(sequences 
<accessionNumber box="[468,598,1016,1042]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY570844" pageId="8" pageNumber="33">
AY 
<number box="[510,598,1016,1042]" pageId="8" pageNumber="33" value="570844.0">570844</number>
</accessionNumber>
- 
<accessionNumber box="[605,735,1016,1042]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY570892" pageId="8" pageNumber="33">
AY 
