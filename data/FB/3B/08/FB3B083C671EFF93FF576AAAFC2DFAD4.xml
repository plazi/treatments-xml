and 
<number box="[359,390,979,1005]" pageId="8" pageNumber="33" value="28.0">28</number>
S (sequences 
<accessionNumber box="[554,685,979,1005]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY570812" pageId="8" pageNumber="33">
AY 
<number box="[596,685,979,1005]" pageId="8" pageNumber="33" value="570812.0">570812</number>
</accessionNumber>
- 
<accessionNumber box="[692,822,979,1005]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY570813" pageId="8" pageNumber="33">
AY 
<number box="[734,822,979,1005]" pageId="8" pageNumber="33" value="570813.0">570813</number>
