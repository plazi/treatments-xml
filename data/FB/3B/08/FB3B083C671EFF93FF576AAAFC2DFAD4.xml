, 
<accessionNumber box="[836,967,979,1005]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AY570826" pageId="8" pageNumber="33">
AY 
<number box="[879,967,979,1005]" pageId="8" pageNumber="33" value="570826.0">570826</number>
