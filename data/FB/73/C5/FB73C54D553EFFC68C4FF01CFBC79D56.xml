<quantity box="[548,670,1618,1640]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="22" pageNumber="244" unit="mm" value="16.0" valueMax="17.0" valueMin="15.0">15–17 mm</quantity>
