<quantity box="[1191,1313,1407,1429]" metricMagnitude="-2" metricUnit="m" metricValue="1.75" metricValueMax="1.8" metricValueMin="1.7" pageId="25" pageNumber="247" unit="mm" value="17.5" valueMax="18.0" valueMin="17.0">17–18 mm</quantity>
