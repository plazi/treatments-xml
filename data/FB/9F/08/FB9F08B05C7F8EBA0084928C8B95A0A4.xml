<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:F8DE3983-ADF9-4923-9717-01450368E584" class="Actinopterygii" family="Congridae" genus="Kenyaconger" kingdom="Animalia" lsidName-ZBK="Kenyaconger Smith &amp; Karmovskaya 2003:14" order="Anguilliformes" pageNumber="14" phylum="Chordata" rank="genus">Kenyaconger</taxonomicName>
