des 
<taxonomicName authority="L." class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="496" phylum="Tracheophyta" rank="species" species="nemorensis">
