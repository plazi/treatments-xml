<taxonomicName id="0442F42E85A3144FD852AD0AFB28633C" authorityName="Kimsey" authorityYear="2014" class="Hexapoda" family="Chrysididae" genus="Noumeasega" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="3" pageNumber="22" phylum="Arthropoda" rank="species" species="bicolorata">
