<taxonomicName id="25C587D9FFE4E6140A216882515B6FC7" authority="Wilson" authorityName="Wilson" box="[305,579,429,452]" class="Aves" family="Fringillidae" genus="Fringilla" kingdom="Animalia" order="Passeriformes" pageId="1" pageNumber="399" phylum="Chordata" rank="species" species="arborea">
