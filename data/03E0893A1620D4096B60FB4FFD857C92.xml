<quantity id="4CB195C9163FD4096B97FE55FD3D7CB7" box="[691,735,441,467]" metricMagnitude="-1" metricUnit="m" metricValue="1.524" pageId="17" pageNumber="68" unit="in" value="6.0">6 in</quantity>
