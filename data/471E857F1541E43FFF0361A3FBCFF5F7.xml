<taxonomicName id="08B74FEA1546E43FFD8B60A0FD5FF5F6" authorityName="Pocock" authorityYear="1897" box="[543,679,404,429]" class="Arachnida" family="Rhagodidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="52" pageNumber="53" phylum="Arthropoda" rank="family">Rhagodidae</taxonomicName>
, 
