<collectionCode id="ED5BAE05FFB0FFCCDCBFF231E4960E5F" box="[425,486,1054,1077]" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="28" pageNumber="85" type="Museum">ZMB</collectionCode>
