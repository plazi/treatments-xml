</emphasis>
<taxonomicName id="59E304AABE03FFB7FEB7FD6EFDCEB787" authority="Leach, 1814" authorityName="Leach" authorityYear="1814" box="[299,521,690,715]" class="Malacostraca" family="Melitidae" genus="Melita" kingdom="Animalia" order="Amphipoda" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">
