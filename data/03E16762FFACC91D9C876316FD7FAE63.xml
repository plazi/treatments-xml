<date id="FFF6F0B4FFACC91C985663D9FAD6A8E6" box="[1094,1292,1933,1959]" pageId="48" pageNumber="49" value="1998-05-30" valueMax="1998-06-02" valueMin="1998-05-30">30.V–2.VI.1998</date>
; 
<date id="FFF6F0B4FFACC91C990A63D9FA4DA8E6" box="[1306,1431,1933,1959]" pageId="48" pageNumber="49" value="1999-05-13">13.V.1999</date>
