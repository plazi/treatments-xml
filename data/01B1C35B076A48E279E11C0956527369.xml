<taxonomicName id="AE43BB6E1A22966EE47265731CB7E869" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia monteleagreana" order="Myrtales" pageId="132" pageNumber="133" phylum="Tracheophyta" rank="species" species="monteleagreana">
