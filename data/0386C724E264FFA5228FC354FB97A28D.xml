<taxonomicName id="4C2F0DB1E264FFBA228FC3DEFDB6A4C3" authority="Johnston and Tiegs, 1921" authorityName="Johnston and Tiegs" authorityYear="1921" box="[151,594,1967,1989]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="163" phylum="Arthropoda" rank="species" species="delta">
