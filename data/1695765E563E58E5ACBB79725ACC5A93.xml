<subSubSection id="7DAB18076EB8D0941F63405F760CA0ED" pageId="0" pageNumber="5" type="nomenclature">
<paragraph id="FE0964C2E8E68CFAB04D7971A3D35D9B" pageId="0" pageNumber="5">
<taxonomicName id="C7A9EF624AADA2A6996CDFAAE25BF0DC" LSID="http://zoobank.org/69DA47BC-585A-43AD-A1DE-10BEAD7139C6" authority="Awad" authorityName="Awad" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Synopeas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synopeas toto" order="Hymenoptera" pageId="0" pageNumber="5" phylum="Arthropoda" rank="species" species="toto" status="sp. nov.">Synopeas toto Awad</taxonomicName>
