</taxonomicName>
, 
<emphasis id="B942EA8CFFFFFFD7A8D9FA86137745D7" box="[461,578,1397,1423]" italics="true" pageId="31" pageNumber="32">
<taxonomicName id="4C364D1DFFFFFFD7A8D9FA86130B45D7" box="[461,574,1397,1423]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="species" species="sylvestris">sylvestris</taxonomicName>
