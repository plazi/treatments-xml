<paragraph id="8B89369EFFFFFFD7A9A9FA86137745D7" blockId="31.[189,578,1397,1423]" box="[189,578,1397,1423]" pageId="31" pageNumber="32">
<taxonomicName id="4C364D1DFFFFFFD7A9A9FA86101F45D7" box="[189,298,1397,1423]" class="Pinopsida" family="Pinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
: 
<taxonomicName id="4C364D1DFFFFFFD7A821FA85108A45D7" box="[309,447,1398,1423]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="species" species="nigra">
