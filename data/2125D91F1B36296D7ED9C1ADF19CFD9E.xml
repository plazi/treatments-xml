<taxonomicName id="6E8C138A1B36296D7F5AC1E3F5B3FE7A" box="[276,424,474,499]" class="Demospongiae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hadromerida" pageId="22" pageNumber="23" phylum="Porifera" rank="order">Hadromerida</taxonomicName>
