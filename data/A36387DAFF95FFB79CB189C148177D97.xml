<quantity id="EC329B29FF94FFB798EF8BFD4C367D1F" box="[1225,1345,1278,1304]" metricMagnitude="2" metricUnit="m" metricValue="3.25" metricValueMax="6.0" metricValueMin="0.5" pageId="19" pageNumber="20" unit="m" value="325.0" valueMax="600.0" valueMin="50.0">50â€“600 m</quantity>
