<number id="9D0224EBFF94FFB7993F8C694C227B83" box="[1305,1365,874,900]" pageId="19" pageNumber="20" value="7.5" valueMax="10.0" valueMin="5.0">5â€“10</number>
), IIL- 
<number id="9D0224EBFF94FFB79CB18C9649DF7BA8" box="[151,168,917,943]" pageId="19" pageNumber="20" value="4.0">4</number>
, IIL- 
<number id="9D0224EBFF94FFB79CC18C9649827BA8" box="[231,245,917,943]" pageId="19" pageNumber="20" value="6.0">6</number>
(Fig. 
<number id="9D0224EBFF94FFB79D678C9648267BA8" box="[321,337,917,943]" pageId="19" pageNumber="20" value="5.0">5</number>
), and IIIL- 
<number id="9D0224EBFF94FFB79DFC8C9648907BA8" box="[474,487,917,943]" pageId="19" pageNumber="20" value="5.0">5</number>
(Fig. 
<number id="9D0224EBFF94FFB79E158C964B357BA8" box="[563,578,917,943]" pageId="19" pageNumber="20" value="6.0">6</number>
