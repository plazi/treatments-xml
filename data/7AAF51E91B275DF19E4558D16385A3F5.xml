<taxonomicName id="76DF20BB95370152817424936C004D37" authorityName="Latrielle" authorityYear="1804" class="Insecta" family="Braconidae" genus="Vipio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Vipio" order="Hymenoptera" pageId="0" pageNumber="89" phylum="Arthropoda" rank="genus">
