<emphasis id="B91A023EFFA0FFFFFF3FCDFB1C0218AB" bold="true" box="[151,276,1704,1728]" pageId="2" pageNumber="595">FIGURE 2.</emphasis>
<taxonomicName id="4C6EA5AFFFA0FFFFFEB3CDFB1CF918AB" authorityName="Lin &amp; Peng" authorityYear="2022" box="[283,495,1704,1728]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="595" phylum="Arthropoda" rank="species" species="caiyujiei">
<emphasis id="B91A023EFFA0FFFFFEB3CDFB1CF918AB" box="[283,495,1704,1728]" italics="true" pageId="2" pageNumber="595">Lathrobium caiyujiei</emphasis>
</taxonomicName>
. 
<emphasis id="B91A023EFFA0FFFFFE55CDFB1F1918AB" bold="true" box="[509,527,1704,1728]" pageId="2" pageNumber="595">A</emphasis>
female tergite VIII 
<emphasis id="B91A023EFFA0FFFFFD73CDFB1FFA18AB" bold="true" box="[731,748,1704,1728]" pageId="2" pageNumber="595">B</emphasis>
female sternite VIII 
<emphasis id="B91A023EFFA0FFFFFC6ACDFB1EC218AB" bold="true" box="[962,980,1704,1728]" pageId="2" pageNumber="595">C</emphasis>
female tergites IX–X. 
<emphasis id="B91A023EFFA0FFFFFB17CDFB19C718AB" bold="true" box="[1215,1233,1704,1728]" pageId="2" pageNumber="595">D</emphasis>
male sternite VII 
<emphasis id="B91A023EFFA0FFFFFA23CDFB188A18AB" bold="true" box="[1419,1436,1704,1728]" pageId="2" pageNumber="595">E</emphasis>
male sternite VIII 
<emphasis id="B91A023EFFA0FFFFFEF9CD9F1C76188F" bold="true" box="[337,352,1740,1764]" pageId="2" pageNumber="595">F</emphasis>
aedeagus in ventral view 
<emphasis id="B91A023EFFA0FFFFFDC0CD9F1F6A188F" bold="true" box="[616,636,1740,1764]" pageId="2" pageNumber="595">G</emphasis>
