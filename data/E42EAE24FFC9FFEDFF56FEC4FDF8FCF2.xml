<taxonomicName id="AB8764B1FFC9FFEDFA2CFCDEFECFFCF2" authorityName="Robillard &amp; Desutter-Grandcolas" authorityYear="2008" class="Insecta" family="Gryllidae" genus="Matuanus" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="281" phylum="Arthropoda" rank="species" species="bruneonervus">
<emphasis id="5EF3C320FFC9FFEDFA2CFCDEFECFFCF2" italics="true" pageId="8" pageNumber="281">M. bruneonervus</emphasis>
</taxonomicName>
; bifurcations of 
<collectionCode id="0A9687F7FFC9FFEDFDADFCF8FD8AFCF3" box="[521,537,821,844]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="8" pageNumber="281">R</collectionCode>
