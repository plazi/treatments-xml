<taxonomicName id="16AECEBCC35FBA1FFF53F9848343F99D" authority="Goeze" authorityName="Goeze" box="[220,607,1593,1619]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="subSpecies" species="persicae" subSpecies="rotundus">
