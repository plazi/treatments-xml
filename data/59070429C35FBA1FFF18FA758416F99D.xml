</taxonomicName>
with 
<taxonomicName id="16AECEBCC35FBA1FFD2EF98485D0F99D" authority="(Boyer de Fonscolombe)" baseAuthorityName="Boyer de Fonscolombe" box="[673,1228,1593,1619]" class="Insecta" family="Coccidae" genus="Eulecanium" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="prunastri">
