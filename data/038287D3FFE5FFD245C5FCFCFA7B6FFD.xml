<number id="3DE324E2FFE5FFD146BDFBDBFBF46C5C" box="[1007,1050,1075,1099]" pageId="1" pageNumber="321" value="109.0">109</number>
° 
<number id="3DE324E2FFE5FFD14176FBDBFBAE6C5C" box="[1060,1088,1075,1099]" pageId="1" pageNumber="321" value="12.0">12</number>
’ 
<number id="3DE324E2FFE5FFD14116FBDBFB646C5C" box="[1092,1162,1075,1099]" pageId="1" pageNumber="321" value="29.44">29.44</number>
