<subSubSection id="9D50941BFC0BC5224DD6D83D887110F9" lastPageId="6" lastPageNumber="105" pageId="5" pageNumber="104" type="habitat">
<paragraph id="233A8D84340603EF4FD4226DFB0C6081" pageId="5" pageNumber="104">Habitat.</paragraph>
<paragraph id="66112D9DCEC993A49D80031FDE8BD06E" pageId="6" pageNumber="105">
<taxonomicName id="317B3E657DD1F0C07B4680D65BFA3E60" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium squaminode" order="Hymenoptera" pageId="6" pageNumber="105" phylum="Arthropoda" rank="species" species="squaminode">
<pageBreakToken id="9BD59F558A00FD9BE604A303A014DBFD" pageId="6" pageNumber="105" start="start">Tetramorium</pageBreakToken>
squaminode
</taxonomicName>
