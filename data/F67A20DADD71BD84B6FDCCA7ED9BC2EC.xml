<taxonomicName id="3433A556F0DD51410C73B6193239E53E" class="Magnoliopsida" family="Capparaceae" genus="Maerua" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Maerua crassifolia" order="Brassicales" pageId="6" pageNumber="105" phylum="Tracheophyta" rank="species" species="crassifolia">Maerua crassifolia</taxonomicName>
