<subSubSection id="C30E657B6A49FEEDAFD9FC09FAB63931" box="[199,1384,1123,1149]" pageId="296" pageNumber="297" type="etymology">
<paragraph id="8BAB36F06A49FEEDAFD9FC09FAB63931" blockId="296.[151,1437,151,1186]" box="[199,1384,1123,1149]" pageId="296" pageNumber="297">
<emphasis id="B960EAE26A49FEEDAFD9FC09FE903931" bold="true" box="[199,334,1123,1149]" pageId="296" pageNumber="297">Etymology.</emphasis>
