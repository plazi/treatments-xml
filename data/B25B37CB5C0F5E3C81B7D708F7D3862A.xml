, 
<taxonomicName id="82282A0C1C6F64F2EE8526693233237D" baseAuthorityName="Broderip &amp; Sowerby" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Scutalus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="37" pageNumber="38" phylum="Mollusca" rank="species" species="versicolor">
