<taxonomicName id="4C434D051133FFBFFCA666D0D72DF900" box="[891,1342,1670,1694]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="18" pageNumber="419" phylum="Echinodermata" rank="variety" species="abyssicola" variety="otagoensis">
