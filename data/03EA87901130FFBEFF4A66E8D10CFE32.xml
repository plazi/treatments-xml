<taxonomicName id="4C434D051133FFBFFCE866FDD1C0F95C" box="[821,979,1707,1730]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="18" pageNumber="419" phylum="Echinodermata" rank="species" species="otagoensis">
<emphasis id="B937EA941133FFBFFCE866FDD1C0F95C" box="[821,979,1707,1730]" italics="true" pageId="18" pageNumber="419">
