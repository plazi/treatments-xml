<taxonomicName id="4C434D051133FFBFFA5C66D1D320F95C" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="18" pageNumber="419" phylum="Echinodermata" rank="species" species="brachygnatha">
