<taxonomicName id="9FC501AEFFE24F33D777FD27E86D4DBB" authority="Zhang, 2017" authorityName="Zhang" authorityYear="2017" box="[294,570,680,702]" class="Insecta" family="Issidae" genus="Kodaianellissus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="356" phylum="Arthropoda" rank="species" species="intorqueus" status="sp. nov.">
<emphasis id="6AB1A63FFFE24F33D777FD27E86D4DBB" box="[294,570,680,702]" italics="true" pageId="1" pageNumber="356">Kodaianellissus intorqueus</emphasis>
</taxonomicName>
