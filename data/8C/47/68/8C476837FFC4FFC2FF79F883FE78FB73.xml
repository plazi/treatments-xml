, 
<date box="[486,619,1424,1448]" pageId="34" pageNumber="35" value="1928-06-09">
<collectingDate box="[486,619,1424,1448]" pageId="34" pageNumber="35" value="1928-06-09">9 Jun. 1928</collectingDate>
</date>
