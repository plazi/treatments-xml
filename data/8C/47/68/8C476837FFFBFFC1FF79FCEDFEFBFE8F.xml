<quantity box="[179,295,295,319]" metricMagnitude="2" metricUnit="m" metricValue="2.1" metricValueMax="3.8" metricValueMin="0.4" pageId="32" pageNumber="33" unit="m" value="210.0" valueMax="380.0" valueMin="40.0">
40– 
<specimenCount box="[222,295,295,319]" pageId="32" pageNumber="33" type="generic">380 m</specimenCount>
