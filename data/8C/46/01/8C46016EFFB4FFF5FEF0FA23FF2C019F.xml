(
<figureCitation box="[159,237,1451,1476]" captionStart="FIGURE 2" captionStartId="8.[151,250,1530,1552]" captionTargetBox="[151,1436,193,1508]" captionTargetId="figure@8.[151,1436,193,1508]" captionTargetPageId="8" captionText="FIGURE 2. Wing of male (except for Leucophenga subpollinosa): A. L. argentina; B. L. flavicosta; C. L. formosa; D. L. nigroscutellata; E. L. subpollinosa; F. L. umbratula; G. L. aculeata sp. nov.; H. L. acuticauda sp. nov.; I. L. cultella sp. nov.; J. L. cyclophylla sp. nov.; K. L. flaviclypeata sp. nov.; L. L. fuscipedes sp. nov.; M. L. gracilenta sp. nov.; N. L. latipenis sp. nov.; O. L. magnicauda sp. nov.; P. L. rhombura sp. nov.; Q. L. rugatifolia sp. nov. Scale bars = 1 mm." httpUri="https://zenodo.org/record/438518/files/figure.png" pageId="26" pageNumber="227">
Figs. 
<date box="[222,237,1451,1475]" pageId="26" pageNumber="227">2</date>
</figureCitation>
I, 4G, 4H, 8J, 8K, 17)
