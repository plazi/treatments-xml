<caption httpUri="https://zenodo.org/record/225771/files/figure.png" pageId="3" pageNumber="449" targetBox="[415,1171,196,1280]" targetPageId="3">
<paragraph blockId="3.[151,1436,1305,1468]" pageId="3" pageNumber="449">
