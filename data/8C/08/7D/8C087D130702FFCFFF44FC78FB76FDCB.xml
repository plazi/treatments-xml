<taxonomicName box="[1128,1275,1834,1859]" class="Insecta" family="Cicadidae" genus="Fidicinoides" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="52" phylum="Arthropoda" rank="genus">
<emphasis box="[1128,1275,1834,1859]" italics="true" pageId="4" pageNumber="52">Fidicinoides</emphasis>
</taxonomicName>
. 
<taxonomicName class="Insecta" family="Cicadidae" genus="Fidicinoides" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="52" phylum="Arthropoda" rank="species" species="sarutaiensis" status="sp. n.">
<emphasis italics="true" pageId="4" pageNumber="52">Fidicinoides sarutaiensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[308,378,1870,1896]" pageId="4" pageNumber="52">
