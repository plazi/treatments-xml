<subSubSection pageId="9" pageNumber="38" type="nomenclature">
<paragraph pageId="9" pageNumber="38">
<taxonomicName authority="Hoser, 2000" authorityName="Hoser" authorityYear="2000" class="Reptilia" family="Pythonidae" genus="Aspidites" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aspidites ramsayi" order="Squamata" pageId="9" pageNumber="38" phylum="Chordata" rank="subSpecies" species="ramsayi" subSpecies="richardjonesii">Aspidites ramsayi richardjonesii Hoser, 2000</taxonomicName>
<taxonomicNameLabel pageId="9" pageNumber="38">[junior synonym of Aspidites ramsayi panoptes]</taxonomicNameLabel>
</paragraph>
</subSubSection>
