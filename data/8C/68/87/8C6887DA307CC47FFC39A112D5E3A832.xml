<collectingRegion box="[342,462,303,325]" country="Brazil" name="Sao Paulo" pageId="7" pageNumber="44">São Paulo</collectingRegion>
and 
<collectingRegion box="[532,690,303,325]" country="Brazil" name="Parana" pageId="7" pageNumber="44">Paraná State</collectingRegion>
