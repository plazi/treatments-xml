<taxonomicName class="Secernentea" family="Camallanidae" genus="Procamallanus" kingdom="Animalia" order="Camallanida" pageId="18" pageNumber="19" phylum="Nematoda" rank="species" species="rebecae" subGenus="Spirocamallanus">
<emphasis box="[1144,1322,888,913]" italics="true" pageId="18" pageNumber="19">Procamallanus</emphasis>
(
<emphasis box="[274,471,931,956]" italics="true" pageId="18" pageNumber="19">Spirocamallanus</emphasis>
) 
<emphasis box="[493,585,931,956]" italics="true" pageId="18" pageNumber="19">rebecae</emphasis>
</taxonomicName>
