<taxonomicName box="[212,278,1083,1101]" class="Insecta" family="Mycetophilidae" genus="Manota" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="252" phylum="Arthropoda" rank="genus">Manota</taxonomicName>
<taxonomicName box="[284,349,1082,1101]" pageId="3" pageNumber="252" rank="species" species="axillata" status="sp. nov.">axillata</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[355,422,1082,1101]" pageId="3" pageNumber="252" rank="species">sp. nov.</taxonomicNameLabel>
