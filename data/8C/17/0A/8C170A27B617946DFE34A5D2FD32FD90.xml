<paragraph blockId="9.[344,894,891,939]" pageId="9" pageNumber="476">
<taxonomicName authority="Pelsue and Zhang" authorityName="Pelsue and Zhang" box="[344,741,891,913]" class="Insecta" family="Curculionidae" genus="Curculio" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="476" phylum="Arthropoda" rank="species" species="jilinensis" status="New Species">
