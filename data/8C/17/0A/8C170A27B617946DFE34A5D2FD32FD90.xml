, 
<emphasis bold="true" box="[755,894,891,912]" pageId="9" pageNumber="476">
<taxonomicNameLabel box="[755,894,891,912]" pageId="9" pageNumber="476" rank="species">New Species</taxonomicNameLabel>
</emphasis>
