<taxonomicName class="Insecta" family="Staphylinidae" genus="Aleochara" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleochara" order="Coleoptera" pageId="2" pageNumber="83" phylum="Arthropoda" rank="genus">Aleochara</taxonomicName>
