<paragraph blockId="13.[151,1436,753,1030]" pageId="13">
<emphasis bold="true" box="[199,449,969,994]" pageId="13">Taxonomic remarks.</emphasis>
<taxonomicName authorityName="Quiroga" authorityYear="2008" baseAuthorityName="Marcus" baseAuthorityYear="1952" box="[460,691,969,994]" class="Rhabditophora" family="Pseudocerotidae" genus="Phrikoceros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="13" phylum="Platyhelminthes" rank="species" species="mopsus">
<emphasis box="[460,691,969,994]" italics="true" pageId="13">Phrikoceros mopsus</emphasis>
</taxonomicName>
