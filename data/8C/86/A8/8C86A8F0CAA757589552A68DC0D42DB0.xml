<taxonomicName authority="Gale, 2016" authorityName="Gale" authorityYear="2016" baseAuthorityName="Hoek" baseAuthorityYear="1883" class="Hexanauplia" family="Scalpellidae" genus="Regioscalpellum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Regioscalpellum moluccanum" order="Scalpelliformes" pageId="0" pageNumber="649" phylum="Arthropoda" rank="species" species="moluccanum">
