<heading bold="true" box="[151,580,1802,1829]" fontSize="11" level="1" pageId="20" pageNumber="21" reason="1">
<taxonomicName authority="Savigny in Lamarck, 1818" authorityName="Savigny in Lamarck" authorityYear="1818" box="[151,580,1802,1829]" class="Polychaeta" family="Eunicidae" genus="Leodice" kingdom="Animalia" order="Eunicida" pageId="20" pageNumber="21" phylum="Annelida" rank="genus">
