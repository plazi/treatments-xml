<taxonomicName authorityName="Kanda" authorityYear="1934" class="Insecta" family="Coccidae" genus="Eulecanium" kingdom="Animalia" lsidName="Eulecanium kuwanai" order="Hemiptera" pageId="0" pageNumber="129" phylum="Arthropoda" rank="species" species="kuwanai">
<emphasis italics="true" pageId="0" pageNumber="129">Eulecanium kuwanai</emphasis>
</taxonomicName>
on
</paragraph>
<paragraph pageId="0" pageNumber="129">
<taxonomicName class="Magnoliopsida" family="Ulmaceae" genus="Ulmus" kingdom="Plantae" lsidName="Ulmus pumila" order="Rosales" pageId="0" pageNumber="129" phylum="Tracheophyta" rank="species" species="pumila">
