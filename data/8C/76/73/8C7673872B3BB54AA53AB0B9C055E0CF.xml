<paragraph pageId="null" pageNumber="741">
<pageBreakToken pageId="null" pageNumber="741" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName authority="L." class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="741" phylum="Tracheophyta" rank="species" species="hydropiper">
Polygonum 
<normalizedToken originalValue="Hydrópiper" pageId="null" pageNumber="741">Hydropiper</normalizedToken>
<authorityName pageId="null" pageNumber="741">L.</authorityName>
</taxonomicName>
</paragraph>
