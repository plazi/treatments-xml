<subSubSection box="[151,872,793,818]" pageId="63" pageNumber="64" type="materials_examined">
<paragraph blockId="63.[151,1436,793,2013]" box="[151,872,793,818]" pageId="63" pageNumber="64">
<emphasis bold="true" box="[151,325,793,818]" pageId="63" pageNumber="64">
<typeStatus box="[151,209,793,817]" pageId="63" pageNumber="64">Type</typeStatus>
material.
</emphasis>
See account of 
<taxonomicName authorityName="Gimmel" authorityYear="2013" box="[503,790,793,817]" class="Insecta" family="Phalacridae" genus="Platyphalacrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="lawrencei">
