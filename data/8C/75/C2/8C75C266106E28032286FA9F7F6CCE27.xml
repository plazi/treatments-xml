species: 
<taxonomicName authorityName="Gimmel" authorityYear="2013" box="[294,448,1511,1533]" class="Insecta" family="Phalacridae" genus="Sveculus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="lewisi">
