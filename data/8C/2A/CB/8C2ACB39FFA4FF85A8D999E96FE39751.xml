<paragraph blockId="9.[160,407,142,202]" box="[160,407,178,202]" pageId="9" pageNumber="1721">
<collectingCountry box="[160,220,178,202]" name="Brazil" pageId="9" pageNumber="1721">Brazil</collectingCountry>
(
<collectingRegion box="[235,397,178,202]" country="Brazil" name="Espirito Santo" pageId="9" pageNumber="1721">Espírito Santo</collectingRegion>
