, and 
<taxonomicName authority="Jin, 2020" authorityName="Jin" authorityYear="2020" box="[839,1388,1699,1725]" class="Insecta" family="Tettigoniidae" genus="Xiphidiopsis" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="129" phylum="Arthropoda" rank="species" species="compressa" subGenus="Xiphidiopsis">
