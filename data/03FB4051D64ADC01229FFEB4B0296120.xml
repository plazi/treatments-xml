<taxonomicName id="4C528AC4D64ADC01265DFF6BB6FF6633" authorityName="Pardo Alcaide" authorityYear="1950" box="[1109,1275,832,857]" class="Insecta" family="Meloidae" genus="Croscherichia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
