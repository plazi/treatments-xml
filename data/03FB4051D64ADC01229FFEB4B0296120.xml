, 
<collectingCountry id="F345B1D7D64ADC01275CFEDBB2C26658" name="Palestine" pageId="13" pageNumber="14">Palestine</collectingCountry>
, 
<collectingCountry id="F345B1D7D64ADC0122DAFF33B3006658" box="[210,260,792,818]" name="Iraq" pageId="13" pageNumber="14">Iraq</collectingCountry>
