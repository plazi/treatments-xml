<taxonomicName id="876E56EC0A73023EA7977B12EC49203B" class="Diplopoda" family="Haplodesmidae" genus="Agathodesmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agathodesmus quintanus" order="Polydesmida" pageId="17" pageNumber="50" phylum="Arthropoda" rank="species" species="quintanus">Agathodesmus quintanus</taxonomicName>
