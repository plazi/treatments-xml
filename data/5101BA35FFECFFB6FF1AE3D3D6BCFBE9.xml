<taxonomicName id="1EA870A0FFECFFB7FEF0E69DD549F827" box="[381,581,1945,1967]" class="Insecta" family="Ulidiidae" genus="Physiphora" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="tarsata">
