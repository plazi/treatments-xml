<number id="947724B2FFA7E91CFB46B8933EB93496" box="[1187,1245,1445,1471]" pageId="13" pageNumber="14" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
setae. Tentorium 
<quantity id="E5479B70FFA7E91CFE6EB8FB384734CE" box="[395,547,1485,1511]" metricMagnitude="2" metricUnit="m" metricValue="1.185" metricValueMax="1.24" metricValueMin="1.13" pageId="13" pageNumber="14" unit="m" value="118.5" valueMax="124.0" valueMin="113.0">113–124 m</quantity>
long, 
<quantity id="E5479B70FFA7E91CFD94B8FB388F34CE" box="[625,747,1485,1511]" metricMagnitude="1" metricUnit="m" metricValue="1.3" metricValueMax="1.5" metricValueMin="1.1" pageId="13" pageNumber="14" unit="m" value="13.0" valueMax="15.0" valueMin="11.0">11–15 m</quantity>
