<quantity id="E5479B70FFA7E91CFC6AB8FB3E6E34CE" box="[911,1034,1485,1511]" metricMagnitude="1" metricUnit="m" metricValue="8.85" metricValueMax="9.4" metricValueMin="8.3" pageId="13" pageNumber="14" unit="m" value="88.5" valueMax="94.0" valueMin="83.0">83–94 m</quantity>
