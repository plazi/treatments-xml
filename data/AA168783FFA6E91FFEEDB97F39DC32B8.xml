<quantity id="E5479B70FFA7E91CFBBCB8FB3ECC34CE" box="[1113,1192,1485,1511]" metricMagnitude="1" metricUnit="m" metricValue="1.9" pageId="13" pageNumber="14" unit="m" value="19.0">19 m</quantity>
