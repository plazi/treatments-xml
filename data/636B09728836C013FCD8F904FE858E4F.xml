<emphasis id="D9B664768837C013FC99FD11FCE388F4" bold="true" box="[780,868,701,720]" pageId="5" pageNumber="287">Abb. 2:</emphasis>
<taxonomicName id="2CC2C3E78837C013FCE1FD10FB6B88F4" authority="(Clemens, 1861)" baseAuthorityName="Clemens" baseAuthorityYear="1861" box="[884,1260,700,720]" class="Insecta" family="Tortricidae" genus="Clepsis" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="287" phylum="Arthropoda" rank="species" species="peritana">
