<location id="6034604B59449865D2A9883F76FF9D7B" LSID="urn:lsid:plazi:treatment:ED42878659459865D18D8C5574009A3F:6034604B59449865D2A9883F76FF9D7B" box="[947,989,799,825]" country="Chile" county="Obviously Attems" municipality="Coique" name="Mx" pageId="17" pageNumber="18">Mx</location>
, 
<location id="6034604B59449865D2F0883F71339D7B" LSID="urn:lsid:plazi:treatment:ED42878659459865D18D8C5574009A3F:6034604B59449865D2F0883F71339D7B" box="[1002,1041,799,825]" country="Chile" county="Obviously Attems" municipality="Coique" name="Md" pageId="17" pageNumber="18">Md</location>
on slide mounted by 
<location id="6034604B59449865D413883F70789D7B" LSID="urn:lsid:plazi:treatment:ED42878659459865D18D8C5574009A3F:6034604B59449865D413883F70789D7B" box="[1289,1370,799,825]" country="Chile" county="Obviously Attems" municipality="Coique" name="Attems" pageId="17" pageNumber="18">Attems</location>
(
<typeStatus id="BA50883259449865D471883F75E99D1F" pageId="17" pageNumber="18">syntype</typeStatus>
), from 
<collectingCountry id="1DFC760059449865D006886374799D1F" box="[284,347,835,861]" name="Chile" pageId="17" pageNumber="18">Chile</collectingCountry>
, labelled 
<location id="6034604B59449865D0DC886377029D1F" LSID="urn:lsid:plazi:treatment:ED42878659459865D18D8C5574009A3F:6034604B59449865D0DC886377029D1F" box="[454,544,835,861]" country="Chile" county="Obviously Attems" municipality="Coique" name="Coronel" pageId="17" pageNumber="18">Coronel</location>
