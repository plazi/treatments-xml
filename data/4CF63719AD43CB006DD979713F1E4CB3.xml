<taxonomicName id="7213B900F4E53555F0D4F5C86814A11F" class="Insecta" family="Eucinetidae" genus="Eucinetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucinetus morio" order="Coleoptera" pageId="5" pageNumber="46" phylum="Arthropoda" rank="species" species="morio">Eucinetus morio</taxonomicName>
