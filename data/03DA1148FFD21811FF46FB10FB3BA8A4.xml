) 
<taxonomicName id="4C73DBDDFFD21811FCEDF911FBD5A8A4" authority="Yersin, 1854" authorityName="Yersin" authorityYear="1854" box="[828,1074,1714,1738]" class="Insecta" family="Tettigoniidae" genus="Platycleis" kingdom="Animalia" order="Orthoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="sepium">
