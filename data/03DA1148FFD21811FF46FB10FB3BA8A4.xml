(Yersin, 
<number id="3DBBB279FFD21811FC20F910FBCDA8A4" box="[1009,1066,1714,1738]" pageId="16" pageNumber="17" value="1854.0">1854</number>
)
</taxonomicName>
(
<typeStatus id="54C81EFCFFD21811FB90F910FB94A8A4" box="[1089,1139,1714,1738]" pageId="16" pageNumber="17">type</typeStatus>
