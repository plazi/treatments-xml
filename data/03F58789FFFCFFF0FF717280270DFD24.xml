<number id="3D9424B8FFFCFFEDFE5976F325B1F850" box="[447,505,1952,1976]" pageId="13" pageNumber="41" value="5575.0">5575</number>
, HW: 
<number id="3D9424B8FFFCFFEDFDA076F326C9F850" box="[582,641,1952,1976]" pageId="13" pageNumber="41" value="4138.0">4138</number>
, F: 
<number id="3D9424B8FFFCFFEDFD4976F326A2F850" box="[687,746,1952,1976]" pageId="13" pageNumber="41" value="1225.0">1225</number>
, T: 
<number id="3D9424B8FFFCFFEDFCFE76F3271BF850" box="[792,851,1952,1976]" pageId="13" pageNumber="41" value="2325.0">2325</number>
, t 
<number id="3D9424B8FFFCFFEDFC8B76F32732F850" box="[877,890,1952,1976]" pageId="13" pageNumber="41" value="1.0">1</number>
: 
<number id="3D9424B8FFFCFFEDFC6E76F327FDF850" box="[904,949,1952,1976]" pageId="13" pageNumber="41" value="800.0">800</number>
, t 
<number id="3D9424B8FFFCFFEDFC2A76F32794F850" box="[972,988,1952,1976]" pageId="13" pageNumber="41" value="2.0">2</number>
: 
<number id="3D9424B8FFFCFFEDFC0F76F3205DF850" box="[1001,1045,1952,1976]" pageId="13" pageNumber="41" value="163.0">163</number>
, ctt 
<number id="3D9424B8FFFCFFEDFBA176F32019F850" box="[1095,1105,1952,1976]" pageId="13" pageNumber="41" value="1.0">1</number>
: 
<number id="3D9424B8FFFCFFEDFBB976F32036F850" box="[1119,1150,1952,1976]" pageId="13" pageNumber="41" value="35.0">35</number>
, Mx 
