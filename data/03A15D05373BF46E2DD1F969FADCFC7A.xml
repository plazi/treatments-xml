<number id="3DC0FE343738F46E2804FD8DFAADFD7A" box="[1346,1381,626,652]" pageId="18" pageNumber="19" value="1.8">1.8</number>
/1.0/ 
<number id="3DC0FE343738F46E2DD1FD68FF77FD47" box="[151,191,663,689]" pageId="18" pageNumber="19" value="1.4">1.4</number>
; PM/ST = 
<number id="3DC0FE343738F46E2C79FD68FEAFFD47" box="[319,359,663,689]" pageId="18" pageNumber="19" value="0.8">0.8</number>
