<paragraph id="8BD28D2EFFC03D4081A014B5FE83FA40" blockId="5.[151,327,1504,1530]" box="[151,327,1504,1530]" pageId="5" pageNumber="8">
<heading id="D09A3A42FFC03D4081A014B5FE83FA40" bold="true" box="[151,327,1504,1530]" fontSize="11" level="1" pageId="5" pageNumber="8" reason="1">
<taxonomicName id="4C6DF6ADFFC03D4081A014B5FE83FA40" box="[151,327,1504,1530]" class="Actinopterygii" family="Chaenopsidae" kingdom="Animalia" order="Perciformes" pageId="5" pageNumber="8" phylum="Chordata" rank="family">
