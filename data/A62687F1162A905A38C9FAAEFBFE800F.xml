<subSubSection id="6695656C162A905B38C9FAE6FD108491" box="[151,687,1389,1411]" pageId="8" pageNumber="391" type="materials_examined">
<paragraph id="2E3036E7162A905B38C9FAE6FD108491" blockId="8.[151,687,1389,1411]" box="[151,687,1389,1411]" pageId="8" pageNumber="391">
<typeStatus id="F1348845162A905B38C9FAE6FF768491" box="[151,201,1390,1411]" pageId="8" pageNumber="391">Type</typeStatus>
species: 
<taxonomicName id="E98F4D64162A905B3978FAE5FD148491" authority="Kerzhner, 1979" authorityName="Kerzhner" authorityYear="1979" box="[294,683,1389,1411]" class="Insecta" family="Miridae" genus="Europiella" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="391" phylum="Arthropoda" rank="species" species="lividellus">
<emphasis id="1CFBEAF5162A905B3978FAE5FE4B8491" box="[294,500,1389,1411]" italics="true" pageId="8" pageNumber="391">Europiella lividellus</emphasis>
(
<bibRefCitation id="4A1E4B16162A905B3A5FFAE5FD1B8491" author="Kerzhner" box="[513,676,1389,1411]" pageId="8" pageNumber="391" refString="Kerzhner, I. M. (1979) New Heteroptera from the Far East of the USSR. Trudy Zoologicheskogo Instituta Akademii Nauk SSSR, 81, 14 - 65 [in Russian]." type="journal article" year="1979">
Kerzhner, 
<number id="984724C0162A905B3A33FAE5FD1B8491" box="[621,676,1389,1411]" pageId="8" pageNumber="391" value="1979.0">1979</number>
</bibRefCitation>
