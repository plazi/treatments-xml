<taxonomicName id="4C7E4D35FFB5FFBAFDC2F8A6FDB332A9" box="[547,629,1902,1924]" class="Insecta" family="Thripidae" genus="Caliothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="tongi">
<emphasis id="B90AEAA4FFB5FFBAFDC2F8A6FDB332A9" box="[547,629,1902,1924]" italics="true" pageId="3" pageNumber="60">C. tongi</emphasis>
