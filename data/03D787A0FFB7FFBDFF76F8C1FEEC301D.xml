<emphasis id="B90AEAA4FFB7FFB8FF76F8C1FE6B320F" bold="true" box="[151,429,1800,1827]" pageId="1" pageNumber="58">
<taxonomicName id="4C7E4D35FFB7FFB8FF76F8C1FEA5320E" box="[151,355,1801,1827]" class="Insecta" family="Thripidae" genus="Caliothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="species" species="tongi" status="sp. n.">
<emphasis id="B90AEAA4FFB7FFB8FF76F8C1FEA5320E" bold="true" box="[151,355,1801,1827]" italics="true" pageId="1" pageNumber="58">Caliothrips tongi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23957DFFFB7FFB8FE8AF8C0FE6B320F" box="[363,429,1800,1826]" pageId="1" pageNumber="58" rank="species">sp. n.</taxonomicNameLabel>
