species. 
<emphasis id="B90AEAA4FFB5FFBAFDFEF886FDFB3248" bold="true" box="[543,573,1870,1893]" pageId="3" pageNumber="60">
(
<number id="3DB62491FFB5FFBAFDC6F886FDF33248" box="[551,565,1870,1893]" pageId="3" pageNumber="60" value="6.0">6</number>
)
</emphasis>
<taxonomicName id="4C7E4D35FFB5FFBAFDA2F898FD503248" box="[579,662,1872,1893]" class="Insecta" family="Thripidae" genus="Caliothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="tongi">
