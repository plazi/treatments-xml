</taxonomicName>
, tergites IV–V. 
<emphasis id="B90AEAA4FFB5FFBAFCD6F886FC933248" bold="true" box="[823,853,1870,1893]" pageId="3" pageNumber="60">
(
<number id="3DB62491FFB5FFBAFCDEF886FC8B3248" box="[831,845,1870,1893]" pageId="3" pageNumber="60" value="7.0">7</number>
)
</emphasis>
<taxonomicName id="4C7E4D35FFB5FFBAFCBDF898FC123248" box="[860,980,1871,1893]" class="Insecta" family="Thripidae" genus="Caliothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="fasciatus">
<emphasis id="B90AEAA4FFB5FFBAFCBDF898FC123248" box="[860,980,1871,1893]" italics="true" pageId="3" pageNumber="60">C. fasciatus</emphasis>
</taxonomicName>
, tergites IV–V. 
<emphasis id="B90AEAA4FFB5FFBAFB97F886FB523248" bold="true" box="[1142,1172,1870,1893]" pageId="3" pageNumber="60">
(
<number id="3DB62491FFB5FFBAFB9FF886FB4A3248" box="[1150,1164,1870,1893]" pageId="3" pageNumber="60" value="8.0">8</number>
)
</emphasis>
<taxonomicName id="4C7E4D35FFB5FFBAFB7AF898FB2B3248" box="[1179,1261,1872,1893]" class="Insecta" family="Thripidae" genus="Caliothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="tongi">
<emphasis id="B90AEAA4FFB5FFBAFB7AF898FB2B3248" box="[1179,1261,1872,1893]" italics="true" pageId="3" pageNumber="60">C. tongi</emphasis>
</taxonomicName>
, tergites VIII–X. 
<emphasis id="B90AEAA4FFB5FFBAFF76F8A5FF7332A9" bold="true" box="[151,181,1901,1924]" pageId="3" pageNumber="60">
(
<number id="3DB62491FFB5FFBAFF7EF8A5FF6B32A9" box="[159,173,1901,1924]" pageId="3" pageNumber="60" value="9.0">9</number>
)
</emphasis>
<taxonomicName id="4C7E4D35FFB5FFBAFF5AF8A6FECA32A9" box="[187,268,1902,1924]" class="Insecta" family="Thripidae" genus="Caliothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="tongi">
<emphasis id="B90AEAA4FFB5FFBAFF5AF8A6FECA32A9" box="[187,268,1902,1924]" italics="true" pageId="3" pageNumber="60">C. tongi</emphasis>
</taxonomicName>
, male ventral surface. 
<emphasis id="B90AEAA4FFB5FFBAFE12F8A5FDDB32A9" bold="true" box="[499,541,1901,1924]" pageId="3" pageNumber="60">
(
<number id="3DB62491FFB5FFBAFE1BF8A5FDD332A9" box="[506,533,1901,1924]" pageId="3" pageNumber="60" value="10.0">10</number>
