</taxonomicName>
, 
<taxonomicNameLabel id="A23657DAFF8B8933FDDDF9A5CCE1F95E" box="[513,648,1660,1683]" pageId="100" pageNumber="101" rank="species">new comb.</taxonomicNameLabel>
; 
<taxonomicName id="4C714D30FF8B8933FFA0F940CC2CF97D" authority="(Snyder, 1972)" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Snyder" baseAuthorityYear="1972" box="[124,581,1689,1712]" class="Amphibia" family="Hylidae" genus="Exerodonta" kingdom="Animalia" order="Anura" pageId="100" pageNumber="101" phylum="Chordata" rank="species" species="juanitae" status="new comb.">
<emphasis id="B905EAA1FF8B8933FFA0F940CF1FF97D" box="[124,374,1689,1712]" italics="true" pageId="100" pageNumber="101">Exerodonta juanitae</emphasis>
