<taxonomicName id="E8C84D57BE63E67DFDFEF8F8EC60F8AF" authority="C.L.Koch, 1836" authorityName="C.L.Koch" authorityYear="1836" box="[687,832,1871,1896]" class="Arachnida" family="Laelapidae" genus="Laelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="3" pageNumber="226" phylum="Arthropoda" rank="species" species="alaskensis">
