<quantity id="4CB29B7D2C4EFFF27971B89B0FCE5CDA" box="[1146,1241,1431,1457]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="6" pageNumber="57" unit="mm" value="2.8">2.8 mm</quantity>
