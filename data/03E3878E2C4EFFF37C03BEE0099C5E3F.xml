<quantity id="4CB29B7D2C4EFFF27C7FB89B0AC45CDA" box="[372,467,1431,1457]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" pageId="6" pageNumber="57" unit="mm" value="4.9">4.9 mm</quantity>
