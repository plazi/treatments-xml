<paragraph id="A775CD26DC5CC202FAFE0D2F5FC2FD48" blockId="1.[151,1437,1447,2036]" box="[198,765,1559,1586]" pageId="1" pageNumber="30">
<emphasis id="95BE1134DC5CC202FAFE0D2F5C61FD4B" bold="true" box="[198,350,1559,1585]" pageId="1" pageNumber="30">Description.</emphasis>
BL: 
<quantity id="603260C3DC5CC202FBA30D205F14FD48" box="[411,555,1560,1586]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" metricValueMax="3.2" metricValueMin="2.8" pageId="1" pageNumber="30" unit="mm" value="3.0" valueMax="3.2" valueMin="2.8">2.8–3.2 mm</quantity>
