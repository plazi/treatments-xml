<subSubSection id="C29E04CDD931FFA9073BFD70FBF4FD61" box="[113,1025,732,755]" pageId="11" pageNumber="116" type="etymology">
<paragraph id="8A3B5746D931FFA9073BFD70FBF4FD61" blockId="11.[113,1089,220,787]" box="[113,1025,732,755]" pageId="11" pageNumber="116">
<emphasis id="B8F08B54D931FFA9073BFD70FF07FD61" bold="true" box="[113,242,732,755]" pageId="11" pageNumber="116">Etymology.</emphasis>
