<taxonomicName id="65B69C8B62751903FFA3FE5F4AE7FDB4" authority="Lansberge, 1875" authorityName="Lansberge" authorityYear="1875" box="[113,419,504,529]" class="Insecta" family="Scarabaeidae" genus="Cheironitis" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="390" phylum="Arthropoda" rank="genus">
