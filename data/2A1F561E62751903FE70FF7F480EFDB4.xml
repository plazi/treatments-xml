<emphasis id="90C23B1A62751903FFA3FE7C4B92FE55" bold="true" box="[113,214,473,497]" pageId="3" pageNumber="390">Remark.</emphasis>
<taxonomicName id="65B69C8B62751903FF0CFE7F4AB5FE55" authority="Janssens, 1937" authorityName="Janssens" authorityYear="1937" box="[222,497,473,497]" class="Insecta" family="Scarabaeidae" genus="Chironitis" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="390" phylum="Arthropoda" rank="genus">
