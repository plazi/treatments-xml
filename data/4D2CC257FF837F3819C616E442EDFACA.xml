<taxonomicNameLabel id="ECC21228FF807F271B9810704361FB8C" box="[713,793,1029,1053]" pageId="15" pageNumber="84" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="028508C2FF807F271A1A10734427FB8C" authority="Starkevich &amp; Men &amp; Saldaitis, 2019" authorityName="Starkevich &amp; Men &amp; Saldaitis" authorityYear="2019" box="[843,1119,1029,1053]" class="Insecta" family="Bibionidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="84" phylum="Arthropoda" rank="species" species="maoxianensis" status="sp. nov." subGenus="Vestiplex">
