v
</date>
= 
<number id="AD6524A2FF967B5CFC56FF1222D99FF7" box="[929,978,188,213]" pageId="11" pageNumber="360" value="2.45">2.45</number>
(
<number id="AD6524A2FF967B5CFC17FF12255C9FF7" box="[992,1111,188,213]" pageId="11" pageNumber="360" value="2.4450000000000003" valueMax="2.66" valueMin="2.23">2.23–2.66</number>
); 
<date id="6F131045FF967B5CFB9DFF12258D9FF7" box="[1130,1158,188,213]" pageId="11" pageNumber="360">
