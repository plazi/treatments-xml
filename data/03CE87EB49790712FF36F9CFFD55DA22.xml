<taxonomicName id="4C674D7E49780712FC4DFC14FB66DDEA" authorityName="Fittkau" authorityYear="1962" box="[1004,1198,924,949]" class="Insecta" family="Chironomidae" genus="Apsectrotanypus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
