<taxonomicName id="B29147DEFFFCFFFAFD11FD94FC86FD3F" box="[677,783,621,648]" class="Arachnida" family="Sparassidae" genus="Sinopoda" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="undata">
<emphasis id="47E5E04FFFFCFFFAFD11FD94FC86FD3F" box="[677,783,621,648]" italics="true" pageId="9" pageNumber="10">S. undata</emphasis>
