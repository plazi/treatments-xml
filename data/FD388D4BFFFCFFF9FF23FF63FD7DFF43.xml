<taxonomicName id="B29147DEFFFCFFFAFF07FD68FE95FD13" box="[179,284,657,684]" class="Arachnida" family="Sparassidae" genus="Sinopoda" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="undata">
