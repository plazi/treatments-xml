<taxonomicName id="4C6A4D67FF9BFFAEFF5B4B465451FBD1" authority="Haas, 1957" authorityName="Haas" authorityYear="1957" box="[151,553,1124,1149]" class="Reptilia" family="Gekkonidae" genus="Pseudoceramodactylus" kingdom="Animalia" order="Squamata" pageId="23" pageNumber="24" phylum="Chordata" rank="genus">
<emphasis id="B91EEAF6FF9BFFAEFF5B4B465451FBD1" bold="true" box="[151,553,1124,1149]" pageId="23" pageNumber="24">
<emphasis id="B91EEAF6FF9BFFAEFF5B4B4657E7FBD1" bold="true" box="[151,415,1124,1149]" italics="true" pageId="23" pageNumber="24">Pseudoceramodactylus</emphasis>
<bibRefCitation id="EFFB4B15FF9BFFAEFE6A4B465451FBD1" author="Haas, G." box="[422,553,1124,1149]" pageId="23" pageNumber="24" pagination="47 - 86" refId="ref52729" refString="Haas, G. (1957) Some amphibians and reptiles from Arabia. Proceedings of the California Academy of Sciences, 29, 47 - 86." type="journal article" year="1957">Haas, 1957</bibRefCitation>
