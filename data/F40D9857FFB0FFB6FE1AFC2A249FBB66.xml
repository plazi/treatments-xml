<taxonomicName id="BBA452C2FFB2FFB6FDA5FF68254CBEA1" authorityName="Wollaston" authorityYear="1867" box="[585,636,220,239]" class="Insecta" family="Curculionidae" genus="Dinas" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="321" phylum="Arthropoda" rank="genus">
