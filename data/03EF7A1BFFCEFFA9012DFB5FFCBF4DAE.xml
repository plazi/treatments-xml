<emphasis id="B932171FFFCEFFAA027AFAB2FD5F4B03" box="[338,643,1308,1334]" italics="true" pageId="45" pageNumber="46">
<taxonomicName id="4C46B08EFFCEFFAA027AFAB2FDA34B03" authority="Vea &amp; Grimaldi, 2015" authorityName="Vea &amp; Grimaldi" authorityYear="2015" box="[338,639,1308,1334]" class="Insecta" family="Hemiptera incertae sedis" genus="Pedicellicoccus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="marginatus" status="n. sp.">Pedicellicoccus marginatus</taxonomicName>
