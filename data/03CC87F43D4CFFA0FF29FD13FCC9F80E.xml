<taxonomicName id="4C654D613D4CFFA0FD94F974FD33F91F" box="[554,671,1725,1750]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="curticalyx">
