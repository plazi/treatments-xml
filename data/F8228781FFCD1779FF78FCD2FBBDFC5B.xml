<taxonomicName id="B78B4D14FFCD1779FF78FCD2FDF4FCA0" authority="Liskiewicz, 1925" authorityName="Liskiewicz" authorityYear="1925" box="[151,629,797,823]" class="Clitellata" family="Erpobdellidae" genus="Erpobdella" kingdom="Animalia" order="Arhynchobdellida" pageId="14" pageNumber="46" phylum="Annelida" rank="species" species="vilnensis">
