<geoCoordinate id="9AE850200339FF88DCFCF87E5A4855C0" box="[151,296,1929,1954]" direction="east" orientation="longitude" pageId="6" pageNumber="120" precision="9" value="129.22667">129°13.60’E</geoCoordinate>
, 
<quantity id="38249B020339FF88DD5FF87E5AD455C3" box="[308,436,1929,1954]" metricMagnitude="2" metricUnit="m" metricValue="2.8649999999999998" metricValueMax="2.88" metricValueMin="2.85" pageId="6" pageNumber="120" unit="m" value="286.5" valueMax="288.0" valueMin="285.0">285–288 m</quantity>
