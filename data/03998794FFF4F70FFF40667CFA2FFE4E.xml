<quantity id="4CC89B67FFF4F70CFF366485FE99F84B" box="[225,298,1980,2002]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="1" pageNumber="527" unit="mm" value="2.5">2.5 mm</quantity>
