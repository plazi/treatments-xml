<geoCoordinate id="EE6911C2480F9E7C6C32F9E6FE21F96C" box="[338,396,1646,1664]" degrees="6" direction="north" minutes="35" orientation="latitude" pageId="5" pageNumber="216" precision="925" value="6.5833335">6°35′N</geoCoordinate>
