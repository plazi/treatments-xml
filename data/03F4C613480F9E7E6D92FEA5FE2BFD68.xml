<taxonomicName id="4C5D0C86480F9E7C6CE5FBDFFD2EFB81" authorityName="Cumberlidge &amp; Sachs" authorityYear="1989" box="[389,643,1110,1133]" class="Malacostraca" family="Potamonautidae" genus="Liberonautes" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="216" phylum="Arthropoda" rank="species" species="rubigimanus">
