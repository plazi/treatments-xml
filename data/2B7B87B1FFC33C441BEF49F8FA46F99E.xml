<quantity id="642A9B42FFC33C4419E44C6DFD30FB9E" box="[668,746,1121,1144]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" metricValueMax="7.0" metricValueMin="3.0" pageId="2" pageNumber="67" unit="cm" value="5.0" valueMax="7.0" valueMin="3.0">3–7 cm</quantity>
.................................................................................... 
