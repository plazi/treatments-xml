<taxonomicName id="64D24D24FFC33C441F124ACDFAC7FD3E" authorityName="Ormerod" authorityYear="2002" baseAuthorityName="K.Y.Lang" box="[1130,1309,705,728]" class="Liliopsida" family="Orchidaceae" genus="Zeuxine" kingdom="Plantae" order="Asparagales" pageId="2" pageNumber="67" phylum="Tracheophyta" rank="species" species="gengmanensis">
