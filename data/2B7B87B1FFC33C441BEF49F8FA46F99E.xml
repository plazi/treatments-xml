<quantity id="642A9B42FFC33C4419894C8DFCE5FB7E" box="[753,831,1153,1176]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="2" pageNumber="67" unit="cm" value="2.0" valueMax="3.0" valueMin="1.0">1–3 cm</quantity>
