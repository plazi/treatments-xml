<taxonomicName id="4C2901BE04051556FCB64A4AFB58FB89" authority="Beu, 2004" authorityName="Beu" authorityYear="2004" box="[917,1203,1123,1149]" class="Gastropoda" family="Triphoridae" genus="Brucetriphora" higherTaxonomySource="GBIF" kingdom="Animalia" order="[unassigned] Caenogastropoda" pageId="12" pageNumber="13" phylum="Mollusca" rank="genus">
