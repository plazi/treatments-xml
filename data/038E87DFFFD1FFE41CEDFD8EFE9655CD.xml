<emphasis id="B953EADBFFD1FFEB1CEDFD8EFE065627" bold="true" box="[151,441,583,610]" pageId="7" pageNumber="8">
<taxonomicName id="4C274D4AFFD1FFEB1CEDFD8EFECF5624" box="[151,368,583,609]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="aristoseta" status="sp. n.">
<emphasis id="B953EADBFFD1FFEB1CEDFD8EFECF5624" bold="true" box="[151,368,583,609]" italics="true" pageId="7" pageNumber="8">Manota aristoseta</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26057A0FFD1FFEB1D0DFD81FE065627" box="[375,441,584,610]" pageId="7" pageNumber="8" rank="species">sp. n.</taxonomicNameLabel>
