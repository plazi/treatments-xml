<location id="8EA160343575FFFB1E73EDDF118AE1F7" LSID="urn:lsid:plazi:treatment:03D787F93575FFFB1CF1EC39102BE602:8EA160343575FFFB1E73EDDF118AE1F7" box="[533,691,942,967]" county="Situated" municipality="EaStern Siberia" name="Selenga RiVer" pageId="22" pageNumber="67" stateProvince="Chukotka">Selenga RiVer</location>
