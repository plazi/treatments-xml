</treatmentCitation>
[
<taxonomicName id="4C794D340716FFFA6CC0FF33FD9E51F2" box="[504,574,227,251]" class="Insecta" family="Embiidae" genus="Embia" kingdom="Animalia" order="Embioptera" pageId="3" pageNumber="224" phylum="Arthropoda" rank="genus">
