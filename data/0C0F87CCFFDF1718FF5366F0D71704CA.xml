<taxonomicName id="43A64D59FFDF1718FF5366AFD62605C2" authority="Priesner, 1954: 54" authorityName="Priesner" authorityPageNumber="54" authorityYear="1954" box="[151,554,1747,1769]" class="Insecta" family="Phlaeothripidae" genus="Neoheegeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="568" phylum="Arthropoda" rank="species" species="persica">
