<specimenCount id="19BEB6D5FF9CFFE6FC90FEF74F18FE82" box="[774,837,264,284]" pageId="6" pageNumber="1577" type="generic">10 exs.</specimenCount>
, East Khasi Hills district, Mawphlang, Lawkyntang, 
<geoCoordinate id="6A8C1B9BFF9CFFE6FE58FEDF4E45FEAA" box="[462,536,288,308]" degrees="25" direction="north" minutes="27" orientation="latitude" pageId="6" pageNumber="1577" precision="925" value="25.45">25째27'N</geoCoordinate>
, 
<geoCoordinate id="6A8C1B9BFF9CFFE6FDB4FEDF4E34FEAA" box="[546,617,288,308]" degrees="91" direction="east" minutes="45" orientation="longitude" pageId="6" pageNumber="1577" precision="925" value="91.75">91째45'E</geoCoordinate>
