<quantity id="C840D0B9FF9CFFE6FDE2FEDF4EE7FEAA" box="[628,698,288,308]" metricMagnitude="3" metricUnit="m" metricValue="1.75" pageId="6" pageNumber="1577" unit="m" value="1750.0">1750 m</quantity>
