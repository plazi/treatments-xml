<taxonomicName id="ED4D45160DF1BCFF445286DF5FD5023F" authority="Gorham, 1884" authorityName="Gorham" authorityYear="1884" class="Insecta" family="Lycidae" genus="Lycinella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lycinella humeralis" order="Coleoptera" pageId="0" pageNumber="69" phylum="Arthropoda" rank="species" species="humeralis">Lycinella humeralis</taxonomicName>
