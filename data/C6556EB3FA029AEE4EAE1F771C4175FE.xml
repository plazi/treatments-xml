<taxonomicName id="A23A26C078CAB25D8AC5F29FAC4650A2" authority="Gorham, 1884" authorityName="Gorham" authorityYear="1884" class="Insecta" family="Lycidae" genus="Lycinella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lycinella" order="Coleoptera" pageId="0" pageNumber="69" phylum="Arthropoda" rank="genus">Lycinella</taxonomicName>
; sp. det. K.G. Blair/ 
<taxonomicName id="E7EC0B60034408F4CBD87CC38075EB61" authority="Gorham, 1884" authorityName="Gorham" authorityYear="1884" class="Insecta" family="Lycidae" genus="Lycinella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lycinella" order="Coleoptera" pageId="0" pageNumber="69" phylum="Arthropoda" rank="genus">Lycinella</taxonomicName>
; 
<taxonomicName id="8AB4CB4DBCD48B2940D4B0AD931B47BC" lsidName="humeralis" pageId="0" pageNumber="69" rank="species" species="humeralis">humeralis</taxonomicName>
