<subSubSection id="1AB2A73BA888179EE15D16CE32D7329B" pageId="0" pageNumber="69" type="type material">
<paragraph id="7E0F698F431CCA94085150AD5D322B16" pageId="0" pageNumber="69">Type material examined (1).</paragraph>
<paragraph id="A57F339BDA05C126430B1C262DA142CE" pageId="0" pageNumber="69">
