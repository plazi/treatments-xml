<taxonomicName id="BDBDB155BE916F7C7A72F5749757F82B" authorityName="Ferussac" authorityYear="1822" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis dufourii" order="Pteropoda" pageId="60" pageNumber="61" phylum="Mollusca" rank="species" species="dufourii">
<emphasis id="E1B701F78AC66871D2CDFF80DE12A216" italics="true" pageId="60" pageNumber="61">Melanopsis dufourii</emphasis>
