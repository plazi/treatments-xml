<emphasis id="B94BB114BF45C249CD84B025E68A1A1B" bold="true" pageId="74" pageNumber="227">
<taxonomicName id="4C3F1685BF45C249CD84B025E58B1A76" authority="E.M.FRIIS, P.R.CRANE et K.R.PEDERSEN" authorityName="E. M. FRIIS, P. R. CRANE et K. R. PEDERSEN" authorityYear="2019" box="[220,755,1552,1577]" class="Magnoliopsida" family="Chloranthaceae" genus="Vedresia" kingdom="Plantae" order="Chloranthales" pageId="74" pageNumber="227" phylum="Tracheophyta" rank="genus" status="gen. nov.">
<emphasis id="B94BB114BF45C249CD84B025E6411A79" bold="true" box="[220,313,1552,1576]" italics="true" pageId="74" pageNumber="227">Vedresia</emphasis>
E.M.FRIIS, P.R.CRANE et K.R.PEDERSEN
</taxonomicName>
<taxonomicNameLabel id="A2780C6FBF45C249CCD6B007E68A1A1B" box="[398,498,1586,1610]" pageId="74" pageNumber="227" rank="genus">gen. nov.</taxonomicNameLabel>
