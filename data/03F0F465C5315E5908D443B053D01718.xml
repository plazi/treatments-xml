<taxonomicName id="4C593EF0C5315E590A96447153AF1718" box="[725,956,1341,1367]" class="Liliopsida" family="Juncaceae" genus="Deschapsia" kingdom="Plantae" order="Poales" pageId="97" pageNumber="98" phylum="Tracheophyta" rank="species" species="flexuosa">
<emphasis id="B92D9961C5315E590A96447153AF1718" box="[725,956,1341,1367]" italics="true" pageId="97" pageNumber="98">Deschapsia flexuosa</emphasis>
