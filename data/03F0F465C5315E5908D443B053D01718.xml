<taxonomicName id="4C593EF0C5315E590D784455556D177C" box="[1339,1406,1305,1331]" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="97" pageNumber="98" phylum="Tracheophyta" rank="genus">
<emphasis id="B92D9961C5315E590D784455556D177C" box="[1339,1406,1305,1331]" italics="true" pageId="97" pageNumber="98">Carex</emphasis>
</taxonomicName>
or 
<taxonomicName id="4C593EF0C5315E5908844471514D1718" box="[199,350,1341,1367]" class="Liliopsida" family="Juncaceae" genus="Luzula" kingdom="Plantae" order="Poales" pageId="97" pageNumber="98" phylum="Tracheophyta" rank="species" species="pilosa">
<emphasis id="B92D9961C5315E5908844471514D1718" box="[199,350,1341,1367]" italics="true" pageId="97" pageNumber="98">Luzula pilosa</emphasis>
</taxonomicName>
