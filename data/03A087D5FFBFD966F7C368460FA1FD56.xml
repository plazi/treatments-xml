<heading id="D0FE81AFFFBFD967F7C368460DD3F977" bold="true" box="[151,463,1644,1671]" fontSize="11" level="1" pageId="4" pageNumber="139" reason="1">
<taxonomicName id="4C094D40FFBFD967F7C368460DD3F977" authority="Whitley, 1945" authorityName="Whitley" authorityYear="1945" box="[151,463,1644,1671]" class="Actinopterygii" family="Eleotridae" genus="Milyeringa" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="139" phylum="Chordata" rank="genus">
<emphasis id="B97DEAD1FFBFD967F7C368460DD3F977" bold="true" box="[151,463,1644,1671]" pageId="4" pageNumber="139">
<emphasis id="B97DEAD1FFBFD967F7C368460D01F976" bold="true" box="[151,285,1645,1671]" italics="true" pageId="4" pageNumber="139">Milyeringa</emphasis>
Whitley, 
<number id="3DC124E4FFBFD967F6C068470DD3F977" box="[404,463,1644,1670]" pageId="4" pageNumber="139" value="1945.0">1945</number>
</emphasis>
</taxonomicName>
