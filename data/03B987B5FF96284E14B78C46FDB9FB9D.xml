<emphasis id="B964EAB1FF96284B14B78C46FDF5FBA2" bold="true" box="[151,565,1043,1069]" pageId="1" pageNumber="22">
Family 
<taxonomicName id="4C104D20FF96284B14D68C46FDF5FBA2" authority="Koch, 1842" authorityName="Koch" authorityYear="1842" box="[246,565,1043,1069]" class="Arachnida" family="Hygrobatidae" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="22" phylum="Arthropoda" rank="family">
Hygrobatidae Koch, 
<number id="3DD82484FF96284B15DB8C46FDF5FBA2" box="[507,565,1043,1069]" pageId="1" pageNumber="22" value="1842.0">1842</number>
</taxonomicName>
