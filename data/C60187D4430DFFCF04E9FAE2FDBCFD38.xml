<subSubSection id="06B26549430AFFCF04B9FDF8FDBCFD38" pageId="5" pageNumber="538" type="etymology">
<paragraph id="4E1736C2430AFFCF04B9FDF8FDBCFD38" blockId="5.[151,1436,151,680]" pageId="5" pageNumber="538">
<emphasis id="7CDCEAD0430AFFCF04B9FDF8FED1FDF0" bold="true" box="[199,334,583,608]" pageId="5" pageNumber="538">Etymology.</emphasis>
