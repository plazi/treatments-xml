<taxonomicName id="B89F01CBFFB3FFF9CD07F77AE69419FF" authority="CASE (1920)" authorityName="CASE" authorityYear="1920" box="[318,670,1649,1673]" class="Reptilia" family="Desmatosuchidae" genus="Desmatosuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thecodontia" pageId="2" pageNumber="99" phylum="Chordata" rank="genus">
