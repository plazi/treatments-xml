<taxonomicName id="C7F14D37FFFA8A1CFBC9FCADFA44DA50" authorityName="Yoshiyuki &amp; Lim" authorityYear="2005" box="[1029,1473,780,819]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="53" pageNumber="332" phylum="Chordata" rank="species" species="chiewkweeae">
