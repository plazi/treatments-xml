<taxonomicName id="95BA4D5BC9695846FCFDF19094D1F195" authorityName="Voss &amp; Jansa" authorityYear="2003" box="[865,985,906,929]" class="Mammalia" family="Didelphidae" genus="Tlacuatzin" kingdom="Animalia" order="Didelphimorphia" pageId="99" pageNumber="100" phylum="Chordata" rank="genus">
<emphasis id="60CEEACAC9695846FCFDF19094D1F195" box="[865,985,906,929]" italics="true" pageId="99" pageNumber="100">Tlacuatzin</emphasis>
</taxonomicName>
