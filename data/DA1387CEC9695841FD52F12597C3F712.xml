<taxonomicName id="95BA4D5BC9695846FCDEF17494B8F1B7" authorityName="Rafinesque" authorityYear="1815" box="[834,944,878,899]" class="Mammalia" family="Didelphidae" genus="Marmosa" kingdom="Animalia" order="Didelphimorphia" pageId="99" pageNumber="100" phylum="Chordata" rank="genus">
<emphasis id="60CEEACAC9695846FCDEF17494B8F1B7" box="[834,944,878,899]" italics="true" pageId="99" pageNumber="100">Marmosa</emphasis>
</taxonomicName>
, 
<taxonomicName id="95BA4D5BC9695846FC5DF177935BF1B0" authorityName="Burnett" authorityYear="1830" box="[961,1107,877,900]" class="Mammalia" family="Didelphidae" genus="Monodelphis" kingdom="Animalia" order="Didelphimorphia" pageId="99" pageNumber="100" phylum="Chordata" rank="genus">
<emphasis id="60CEEACAC9695846FC5DF177935BF1B0" box="[961,1107,877,900]" italics="true" pageId="99" pageNumber="100">Monodelphis</emphasis>
</taxonomicName>
, 
<emphasis id="60CEEACAC9695846FBF9F177937BF1B7" bold="true" box="[1125,1139,877,899]" pageId="99" pageNumber="100">â€ </emphasis>
<taxonomicName id="95BA4D5BC9695846FBEEF1779414F195" authorityName="Reig" authorityYear="1952" class="Mammalia" family="Didelphidae" genus="Thylatheridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Didelphimorphia" pageId="99" pageNumber="100" phylum="Chordata" rank="genus">
<emphasis id="60CEEACAC9695846FBEEF1779414F195" italics="true" pageId="99" pageNumber="100">Thylatheridium</emphasis>
</taxonomicName>
