<taxonomicName id="BDBC83A2EB2F0DD8C645DCC5E6AEAAAC" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon chlorophthalmus" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="species" species="chlorophthalmus">Bracon chlorophthalmus</taxonomicName>
