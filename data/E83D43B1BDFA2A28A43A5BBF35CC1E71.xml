<paragraph id="CE89E505C368299E934347E3471E2578" pageId="0" pageNumber="8151">
<normalizedToken id="A8094944C765B16F14DB23382A654F06" originalValue="Nixon’s">Nixon's</normalizedToken>
description of 
<taxonomicName id="B182640AF4DEC1D46D07439566EB4193" class="Insecta" family="Braconidae" genus="Zele" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zele chlorophthalmus" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="species" species="chlorophthalmus">Zele chlorophthalmus</taxonomicName>
