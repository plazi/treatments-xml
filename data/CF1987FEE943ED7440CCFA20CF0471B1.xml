<taxonomicName id="80B04D6BE945ED74401FFA32CA4E727E" authority="Martins-Neto and Kucera-Santos, 1994: 291" authorityName="Martins-Neto and Kucera-Santos" authorityPageNumber="291" authorityYear="1994" class="Insecta" family="Tabanidae" genus="Cratotabanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="45" phylum="Arthropoda" rank="genus">
