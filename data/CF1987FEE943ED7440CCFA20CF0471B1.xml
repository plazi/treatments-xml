: 
<taxonomicName id="80B04D6BE945ED744311FD69CF7076B9" authority="Martins-Neto and Kucera-Santos" authorityName="Martins-Neto and Kucera-Santos" class="Insecta" family="Tabanidae" genus="Cratotabanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="45" phylum="Arthropoda" rank="species" species="stenomyomorphus">
