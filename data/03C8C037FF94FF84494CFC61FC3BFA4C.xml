<quantity id="4C99DCC4FF94FF84498FFAF5FCDFFACC" box="[706,788,1350,1374]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="35" pageNumber="1497" unit="m" value="1600.0">
<elevation id="004C9612FF94FF84498FFAF5FCDFFACC" box="[706,788,1350,1374]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="35" pageNumber="1497" unit="m" value="1600.0">1600 m</elevation>
