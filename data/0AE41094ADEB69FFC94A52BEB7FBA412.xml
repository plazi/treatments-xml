<taxonomicName id="B29596F21598396BA7849A18693F2D06" class="Insecta" family="Staphylinidae" genus="Anotylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anotylus tetracarinatus" order="Coleoptera" pageId="10" pageNumber="249" phylum="Arthropoda" rank="species" species="tetracarinatus">Anotylus tetracarinatus</taxonomicName>
