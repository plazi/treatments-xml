<emphasis id="B6B2EADFFFC8FFCEC13FFC820A1CFCDB" box="[609,846,809,834]" italics="true" pageId="35" pageNumber="255">
<collectingCountry id="FCD1765DFFC8FFCEC13FFC820A1CFCDB" box="[609,846,809,834]" name="Papua New Guinea" pageId="35" pageNumber="255">Papua New Guinea</collectingCountry>
