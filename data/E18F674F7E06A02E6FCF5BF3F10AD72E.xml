<paragraph id="6528C6F993F6F1CA9E7C66DE23AE508D" pageId="35" pageNumber="80">
<taxonomicName id="650469C35DD1BFAACDCF156061BBF249" authority="Mannerheim" class="Insecta" family="Staphylinidae" genus="Oligota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oligota inflata" order="Coleoptera" pageId="35" pageNumber="80" phylum="Arthropoda" rank="species" species="inflata">Oligota inflata (Mannerheim)</taxonomicName>
