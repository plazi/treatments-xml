<taxonomicName id="8DA74C85BD6230149E7DD52160804261" authorityName="G. Horn" authorityYear="1892" class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="577" pageNumber="578" phylum="Arthropoda" rank="species" species="nupera">
