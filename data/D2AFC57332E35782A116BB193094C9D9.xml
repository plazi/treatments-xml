<taxonomicName id="38D3DC0AB281AAA65A405FE46555AB9C" authorityName="G. Horn" authorityYear="1892" class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="577" pageNumber="578" phylum="Arthropoda" rank="species" species="nupera">
