<taxonomicName id="F1E24B8AC7A9EE3FC7B576765B7F6CBD" authorityName="G. Horn" authorityYear="1892" class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="577" pageNumber="578" phylum="Arthropoda" rank="species" species="nupera">
