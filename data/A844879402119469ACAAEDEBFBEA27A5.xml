<quantity id="E7159B6702109469AF46E9DDFBA626E9" box="[868,1047,194,221]" metricMagnitude="0" metricUnit="m" metricValue="1.6" metricValueMax="1.8" metricValueMin="1.4" pageId="5" pageNumber="180" unit="mm" value="1600.0" valueMax="1800.0" valueMin="1400.0">1400–1800 mm</quantity>
