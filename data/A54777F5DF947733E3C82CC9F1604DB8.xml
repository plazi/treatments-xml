<taxonomicName id="6C9493FAF3D79970B28B412881E7F773" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:183802" authority="Forel 1916" authorityName="Forel" authorityYear="1916" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus (Myrmomalis) Forel" order="Hymenoptera" pageNumber="251" phylum="Arthropoda" rank="subGenus" subGenus="Myrmomalis">Myrmomalis Forel 1916</taxonomicName>
