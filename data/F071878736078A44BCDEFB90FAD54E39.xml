<taxonomicName id="BFD84D1236018A44BD6CF92AFD384F81" authority="Stal, 1787" authorityName="Stal" authorityYear="1787" box="[293,676,1667,1689]" class="Insecta" family="Phaneropteridae" genus="Trigonocorypha" kingdom="Animalia" order="Orthoptera" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="unicolor">
