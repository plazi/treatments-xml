<taxonomicName id="CD83405403BD43B84DBF4047AB4B7241" class="Hexapoda" family="Dermestidae" genus="Dermestes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="subSpecies" species="intermedius" subGenus="Dermestinus" subSpecies="iranicus">
