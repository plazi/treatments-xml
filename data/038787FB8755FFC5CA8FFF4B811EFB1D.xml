<emphasis id="B95AEAFF8755FFC5CB82FEF484AEFEEC" box="[151,393,324,347]" italics="true" pageId="17" pageNumber="206">Material examined</emphasis>
Finike, 
<quantity id="4CD69B088755FFC5CA27FED98441FE37" box="[306,358,361,384]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="17" pageNumber="206" unit="m" value="2.0">2 m</quantity>
, 
<taxonomicName id="4C2E4D6E8755FFC5CA66FED98773FE37" box="[371,596,361,384]" class="Phaeophyceae" family="Laminariaceae" genus="Laurencia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Laminariales" pageId="17" pageNumber="206" phylum="Heterokontophyta" rank="species" species="obtusa">
