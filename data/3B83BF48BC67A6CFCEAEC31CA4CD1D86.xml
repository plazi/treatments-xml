<taxonomicName id="0A4C6014EC9D6C95D9A0E354E8C95AE5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:36108" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Simopone" kingdom="Animalia" lsidName-HNS="Simopone grandidieri Forel" order="Hymenoptera" pageNumber="141" phylum="Arthropoda" rank="species" species="grandidieri" status="n. sp.">SIMOPONE GRANDIDIERI</taxonomicName>
