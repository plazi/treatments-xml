<taxonomicName id="4C3E56D8FF87236AFD24D5CCED77FF34" authority="Alder et Hancock, 1848" authorityName="Alder et Hancock" authorityYear="1848" box="[670,1089,188,212]" class="Ascidiacea" family="Styelidae" genus="Styela" kingdom="Animalia" order="Pleurogona" pageId="5" pageNumber="68" phylum="Chordata" rank="species" species="coriacea">
