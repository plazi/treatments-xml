<taxonomicName id="4C784D37FFE1FFCBA3CC1CFA2415FA45" box="[471,606,1424,1447]" class="Insecta" family="Reduviidae" genus="Aulacogenia" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="31" phylum="Arthropoda" rank="genus">Aulacogenia</taxonomicName>
