<taxonomicName id="4C784D37FFE1FFCBA3CC1CFA2577FA45" authority="Zhao &amp; Cai" authorityName="Zhao &amp; Cai" box="[471,828,1424,1447]" class="Insecta" family="Reduviidae" genus="Aulacogenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="31" phylum="Arthropoda" rank="species" species="dentata" status="sp. nov.">
