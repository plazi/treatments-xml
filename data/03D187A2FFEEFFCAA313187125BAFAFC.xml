<subSubSection id="C362653FFFE0FFCAA3231D7E2501FBB4" pageId="9" pageNumber="32" type="etymology">
<paragraph id="8BC736B4FFE0FFCAA3231D7E2501FBB4" blockId="9.[264,1323,284,1310]" pageId="9" pageNumber="32">
<emphasis id="B90CEAA6FFE0FFCAA3231D7E278FFBCC" bold="true" box="[312,452,1044,1070]" pageId="9" pageNumber="32">Etymology.</emphasis>
