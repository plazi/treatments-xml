<taxonomicName id="3EAE5DF41070FFD7AA937A6598671D94" authorityName="Ricarte &amp; Marcos-García &amp; Hancock &amp; Rotheray" authorityYear="2012" box="[468,525,881,902]" class="Insecta" family="Syrphidae" genus="Quichuana" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="96" phylum="Arthropoda" rank="species" species="inbio">
