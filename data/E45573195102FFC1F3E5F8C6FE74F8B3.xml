<quantity id="AB046FEA5102FFC3F10AF856FD16F83F" box="[632,722,1974,1998]" metricMagnitude="3" metricUnit="m" metricValue="4.007" pageId="4" pageNumber="525" unit="m" value="4007.0">4007 m</quantity>
