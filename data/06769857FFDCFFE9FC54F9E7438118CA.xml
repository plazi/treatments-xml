<taxonomicName id="49DF52C2FFDCFFE8FC45F8BB45951E40" baseAuthorityName="Walch" baseAuthorityYear="1775" box="[1010,1220,1831,1854]" class="Insecta" family="Hesperiidae" genus="Astraptes" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="487" phylum="Arthropoda" rank="species" species="fulgerator">
