<emphasis id="B92EEAF8FF29FF2C2EE1FA82FD10FA69" box="[913,1090,1404,1427]" italics="true" pageId="253" pageNumber="254">
<taxonomicName id="4C5A4D69FF29FF2C2EE1FA82FD6CFA69" authorityName="FIEBER, WITH" authorityYear="1858" box="[913,1086,1404,1427]" class="Insecta" family="Miridae" genus="Plagiognathus" kingdom="Animalia" order="Hemiptera" pageId="253" pageNumber="254" phylum="Arthropoda" rank="genus">Plagiognathus</taxonomicName>
