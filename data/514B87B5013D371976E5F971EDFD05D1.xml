<taxonomicName id="1EE24D20013D371A76E5F918ED9D037A" authority="Haliday, 1833 a: 268" authorityName="Haliday" authorityPageNumber="268" authorityYear="1833" box="[151,459,1775,1799]" class="Insecta" family="Mymaridae" genus="Anagrus" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis id="EB96EAB1013D371A76E5F918ECBB037B" box="[151,237,1775,1798]" italics="true" pageId="13" pageNumber="14">Anagrus</emphasis>
<bibRefCitation id="BD734B52013D371A7684F918ED9D037A" author="Haliday, A. H." box="[246,459,1775,1799]" pageId="13" pageNumber="14" pagination="259 - 276" refId="ref50013" refString="Haliday, A. H. (1833 a) Essay on the classification of the parasitic Hymenoptera of Britain, which correspond with the Ichneumones minuti of Linnaeus. Entomological Magazine, 1 (3), 259 - 276. https: // www. biodiversitylibrary. org / page / 25334028" type="journal article" year="1833">Haliday, 1833a: 268</bibRefCitation>
</taxonomicName>
</treatmentCitation>
; 
<treatmentCitation id="584310B2013D371A77AAF918EE06037A" author="Haliday, A. H." box="[472,592,1775,1799]" page="346" pageId="13" pageNumber="14" year="1833">
<bibRefCitation id="BD734B52013D371A77AAF918EE06037A" author="Haliday, A. H." box="[472,592,1775,1799]" pageId="13" pageNumber="14" pagination="333 - 350" refId="ref50067" refString="Haliday, A. H. (1833 b) An essay on the classification of the parasitic Hymenoptera, etc. (continued). Entomological Magazine, 1 (4), 333 - 350. https: // www. biodiversitylibrary. org / page / 25334028" type="journal article" year="1833">1833b: 346</bibRefCitation>
</treatmentCitation>
. 
<typeStatus id="06598801013D371A742EF918EED8037A" box="[604,654,1775,1799]" pageId="13" pageNumber="14">Type</typeStatus>
species: 
<taxonomicName id="1EE24D20013D371A7482F918E80B037A" authority="Linnaeus, 1767" authorityName="Linnaeus" authorityYear="1767" box="[752,1117,1775,1799]" class="Insecta" family="Ichneumonidae" genus="Ichneumon" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="atomus">
