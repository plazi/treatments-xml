<taxonomicName id="D4AABAAF633864D42219482A2A9A8032" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:35166" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName-HNS="Polyrhachis wolfi Forel" order="Hymenoptera" pageNumber="79" phylum="Arthropoda" rank="species" species="wolfi">Polyrhachis Wolfi</taxonomicName>
