<quantity id="4CC858F04F176D52388AFDB9FF015657" box="[151,219,554,578]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" pageId="8" pageNumber="61" unit="mm" value="3.0">3 mm</quantity>
