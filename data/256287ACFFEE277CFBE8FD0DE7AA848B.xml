<emphasis id="9FBFEAA8FFEE277CFFE6F941E6D885F7" box="[1177,1257,1747,1771]" italics="true" pageId="43" pageNumber="44">L. leysi</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFEE277CFF88F941E7A485F6" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" box="[1271,1429,1747,1770]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="masonensis">
