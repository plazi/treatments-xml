<quantity id="E43E9B0910258166FE57A28A816A428F" box="[453,523,2009,2034]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="3" pageNumber="258" unit="km" value="15.0">15 km</quantity>
