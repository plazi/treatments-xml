</emphasis>
<taxonomicName id="4C484D660031FFB917557B5586AFFA8A" authority="Solem, 1984" authorityName="Solem" authorityYear="1984" box="[319,734,1335,1357]" class="Gastropoda" family="Camaenidae" genus="Ordtrachia" kingdom="Animalia" order="Stylommatophora" pageId="15" pageNumber="118" phylum="Mollusca" rank="species" species="intermedia">
