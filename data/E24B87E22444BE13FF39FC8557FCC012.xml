L. (
<taxonomicName id="ADE24D772444BE13FD2AFC435517C6A1" box="[644,790,903,927]" class="Magnoliopsida" family="Sapindaceae" kingdom="Plantae" order="Sapindales" pageId="10" pageNumber="53" phylum="Tracheophyta" rank="family">Sapindaceae</taxonomicName>
