<taxonomicName id="4C3125E97028FFCFFEF1FBE4FE96FB93" box="[264,373,1044,1069]" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="crassa">
<emphasis id="B94582787028FFCFFEF1FBE4FE96FB93" box="[264,373,1044,1069]" italics="true" pageId="3" pageNumber="4">A. crassa</emphasis>
</taxonomicName>
