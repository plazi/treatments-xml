<quantity id="13609B79FFD4FFE46B74FD7E5680A6A3" box="[334,425,706,732]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" metricValueMax="9.0" metricValueMin="4.0" pageId="46" pageNumber="47" unit="mm" value="6.5" valueMax="9.0" valueMin="4.0">4–9 µm</quantity>
