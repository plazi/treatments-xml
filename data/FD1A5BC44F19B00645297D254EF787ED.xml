<subSubSection id="D7FCF169166D17F6609426D20FD94C21" pageId="8" pageNumber="9" type="color pattern ontogeny">
<paragraph id="4AD0AC9BF093939E7AF647407C41AFD2" pageId="8" pageNumber="9">Color pattern ontogeny.</paragraph>
<paragraph id="4D9B21E93FB5E8E972E80B4525FB1A53" pageId="8" pageNumber="9">
