<taxonomicName id="F242CCAF1DDCDFEFD666BE68BE144E8A" class="Arachnida" family="Theraphosidae" genus="Typhochlaena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Typhochlaena amma" order="Araneae" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="amma">Typhochlaena amma</taxonomicName>
sp. n., female, Santa Teresa, state of Espirito Santo 25 
<taxonomicName id="603EC05055B11EEA14CE72D0816CEE6D" class="Arachnida" family="Theraphosidae" genus="Typhochlaena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Typhochlaena costae" order="Araneae" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="costae">Typhochlaena costae</taxonomicName>
sp. n., female, Palmas, state of Tocantins 26 
<taxonomicName id="712809AA09846DC4A8F4C02CF26AEA4E" genus="Typhochlena" lsidName="Typhochlena curumim" pageId="8" pageNumber="9" rank="species" species="curumim">Typhochlena curumim</taxonomicName>
sp. n., female, Areia, state of Paraiba 27 
<taxonomicName id="46D571D62115EB0A420BF1F9B3036928" class="Arachnida" family="Theraphosidae" genus="Typhochlaena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Typhochlaena paschoali" order="Araneae" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="paschoali">Typhochlaena paschoali</taxonomicName>
