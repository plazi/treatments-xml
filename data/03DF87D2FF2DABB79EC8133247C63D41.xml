<taxonomicName id="4C764D47FF2EABB79D1C150545E73D1A" authorityName="Lane" authorityYear="1942" box="[835,954,1312,1337]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="194" pageNumber="195" phylum="Arthropoda" rank="species" species="travassosi">
