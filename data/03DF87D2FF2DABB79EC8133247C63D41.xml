<collectingMunicipality id="6BADACBEFF2EABB79C9E142D44B33C01" box="[705,750,1032,1058]" pageId="194" pageNumber="195">The</collectingMunicipality>
<typeStatus id="54CD8866FF2EABB79CA9142D45013C01" box="[758,860,1032,1058]" pageId="194" pageNumber="195">holotype</typeStatus>
