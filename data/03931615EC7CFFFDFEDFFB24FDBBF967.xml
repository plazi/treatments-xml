<paragraph id="8B85A703EC7CFFFDFEEFFA6FFEE1FA77" blockId="12.[264,1322,1391,1497]" pageId="12" pageNumber="13">
<typeStatus id="548119A1EC7CFFFDFEEFFA6FFEE0FA1F" box="[312,370,1431,1457]" pageId="12" pageNumber="13">Type</typeStatus>
material of 
<taxonomicName id="4C3ADC80EC7CFFFDFDD5FA6FFDECFA1E" box="[514,638,1431,1456]" class="Malacostraca" family="Phronimidae" genus="Phronima" kingdom="Animalia" order="Amphipoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="pacifica">
