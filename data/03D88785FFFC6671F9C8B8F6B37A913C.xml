, 
<collectingRegion id="49B5F871FFFC6677F9C8B9C1B36E959D" box="[151,352,412,436]" country="Vietnam" name="Lao Cai" pageId="2" pageNumber="29">Province de Lào Cai</collectingRegion>
, 
<date id="FFCF1053FFFC6677F835B9C1B3E3959D" box="[362,493,411,436]" pageId="2" pageNumber="29" value="1929-11-17">
