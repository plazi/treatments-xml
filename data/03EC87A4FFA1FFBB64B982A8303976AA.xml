<taxonomicName id="4C454D31FFA0FFB8653287F4367076E7" box="[284,555,980,1002]" class="Actinopterygii" family="Serranidae" genus="Pseudogramma" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="255" phylum="Chordata" rank="species" species="paucilepis" status="new species">
<emphasis id="B931EAA0FFA0FFB8653287F4367076E7" box="[284,555,980,1002]" italics="true" pageId="9" pageNumber="255">Pseudogramma paucilepis</emphasis>
</taxonomicName>
