<taxonomicName id="4C454D31FFA0FFB865CB855036CE7485" box="[485,661,368,392]" class="Actinopterygii" family="Serranidae" genus="Pseudogramma" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="255" phylum="Chordata" rank="genus">
