<taxonomicName id="6EF34D0EFFF2A2517683C817FDF9F8C1" authority="Van der Hammen 1979" authorityName="Van der Hammen" authorityYear="1979" box="[151,627,1860,1883]" class="Arachnida" family="Opilioacaridae" genus="Salfacarus" kingdom="Animalia" order="Opilioacarida" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="legendrei">
