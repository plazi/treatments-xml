<taxonomicName id="8AA732F09060FF9EC703FBAEFD3D9DEA" baseAuthorityName="Chromaphyosemion" baseAuthorityYear="1924" box="[518,646,1134,1160]" class="Actinopterygii" family="Nothobranchiidae" genus="Aphyosemion" kingdom="Animalia" order="Cyprinodontiformes" pageId="10" pageNumber="859" phylum="Chordata" rank="genus">
