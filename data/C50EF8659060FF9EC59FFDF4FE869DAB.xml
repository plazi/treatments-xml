<taxonomicName id="8AA732F09060FF9EC5F0FBAEFEC09DEA" authorityName="Paperna" authorityYear="1960" box="[245,379,1134,1160]" class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="10" pageNumber="859" phylum="Platyhelminthes" rank="genus">
