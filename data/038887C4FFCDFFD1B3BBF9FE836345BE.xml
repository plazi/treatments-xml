sp. 
<number id="3DE924F5FFCEFFD1B269F902806245DF" box="[325,338,1664,1686]" pageId="10" pageNumber="167" value="1.0">1</number>
</emphasis>
<taxonomicName id="4C214D51FFCEFFD1B2A9F90380CE45DF" box="[389,510,1664,1686]" class="Actinopterygii" family="Sparidae" genus="Sarpa" kingdom="Animalia" order="Perciformes" pageId="10" pageNumber="167" phylum="Chordata" rank="species" species="salpa">
<emphasis id="B955EAC0FFCEFFD1B2A9F90380CE45DF" box="[389,510,1664,1686]" italics="true" pageId="10" pageNumber="167">Sarpa salpa</emphasis>
</taxonomicName>
<collectingCountry id="F3367642FFCEFFD1B123F902836A45DF" box="[527,602,1664,1686]" name="Tunisia" pageId="10" pageNumber="167">Tunisia</collectingCountry>
(Gulf of 
