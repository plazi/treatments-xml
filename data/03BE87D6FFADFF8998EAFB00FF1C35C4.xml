<quantity id="4CEF9B25FFADFF899AECFAE5F8883534" box="[657,713,1303,1329]" metricMagnitude="3" metricUnit="m" metricValue="6.0" pageId="60" pageNumber="61" unit="km" value="6.0">6 km</quantity>
