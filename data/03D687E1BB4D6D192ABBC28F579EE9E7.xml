</taxonomicName>
<emphasis id="B90BEAE5BB4D6D192892C15350FCEF81" bold="true" box="[702,766,619,645]" pageId="1" pageNumber="274">
<taxonomicNameLabel id="A238579EBB4D6D192892C15350FCEF81" box="[702,766,619,645]" pageId="1" pageNumber="274" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C7F4D74BB4D6D192927C15351D4EF81" authority="Shavrin, 2022" authorityName="Shavrin" authorityYear="2022" box="[779,982,619,645]" class="Insecta" family="Staphylinidae" genus="Anthobium" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="274" phylum="Arthropoda" rank="species" species="fortepunctatum" status="sp. n.">
<emphasis id="B90BEAE5BB4D6D192927C15351D4EF81" box="[779,982,619,645]" italics="true" pageId="1" pageNumber="274">A. fortepunctatum</emphasis>
