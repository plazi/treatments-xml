<taxonomicName id="4C7F19A10146A576FEDF0B6BFE33FEAF" box="[356,472,359,383]" class="Insecta" family="Staphylinidae" genus="Pseudolathra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="600" phylum="Arthropoda" rank="species" species="unicolor">
