<typeStatus id="54A788046D0EFFF3FF54FD2363CAFD52" box="[151,207,717,743]" pageId="2" pageNumber="3">Type</typeStatus>
Species: 
<taxonomicName id="4C1C4D256D0EFFF3FE82FD2361EBFD52" authority="Attems, 1928" authorityName="Attems" authorityYear="1928" box="[321,750,717,743]" class="Diplopoda" family="Spirostreptidae" genus="Bicoxidens" kingdom="Animalia" order="Spirostreptida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="nigerrimus">
<emphasis id="B968EAB46D0EFFF3FE82FD23614FFD53" box="[321,586,717,742]" italics="true" pageId="2" pageNumber="3">Bicoxidens nigerrimus</emphasis>
<bibRefCitation id="EF8D4B576D0EFFF3FD92FD2361EBFD52" author="Attems" box="[593,750,717,743]" pageId="2" pageNumber="23" refString="Attems, C. (1928) The Myriapoda of South Africa. Annals of the South African Museum, 26, 1 - 431" type="journal article" year="1928">
Attems, 
<number id="3DD424816D0EFFF3FD77FD2361EBFD52" box="[692,750,717,743]" pageId="2" pageNumber="3" value="1928.0">1928</number>
</bibRefCitation>
</taxonomicName>
