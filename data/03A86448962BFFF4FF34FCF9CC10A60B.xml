<taxonomicName id="4C01AEDD9628FFCAFE90F8FDCA07A02D" authority="Lowry &amp; Stoddart, 2010" authorityName="Lowry &amp; Stoddart" authorityYear="2010" box="[307,534,1843,1866]" class="Malacostraca" family="Sophrosynidae" genus="Sophrosyne" kingdom="Animalia" order="Amphipoda" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="rodondo" status="sp. nov.">
<emphasis id="B975094C9628FFCAFE90F8FDCA07A02D" box="[307,534,1843,1866]" italics="true" pageId="30" pageNumber="31">Sophrosyne rodondo</emphasis>
</taxonomicName>
<emphasis id="B975094C9628FFCAFDBDF8FDCA66A02C" bold="true" box="[542,631,1843,1867]" pageId="30" pageNumber="31">
