<taxonomicName id="4C6903CFFFF2CE1BFF33315BFE4FF8AE" authorityName="Krantz" authorityYear="1963" box="[234,495,1885,1911]" class="Arachnida" family="Macrochelidae" genus="Calholaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="413" phylum="Arthropoda" rank="species" species="trianothrix">
