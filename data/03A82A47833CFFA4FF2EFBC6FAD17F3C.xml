<emphasis id="B9754743833CFFA4FCE2FDA1FCC57959" box="[813,828,532,555]" italics="true" pageId="33" pageNumber="486">c.</emphasis>
<quantity id="4CF936B4833CFFA4FC8DFDA1FC767959" box="[834,911,532,556]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="33" pageNumber="486" unit="m" value="1000.0">1000 m</quantity>
