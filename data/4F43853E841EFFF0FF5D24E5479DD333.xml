<taxonomicName id="00EA4FAB841EFFFBFEF1254F4735D522" authority="Germain, 1855" authorityName="Germain" authorityYear="1855" box="[315,833,1883,1908]" class="Insecta" family="Tenebrionidae" genus="Myrmecodema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="296" phylum="Arthropoda" rank="species" species="nycterinoides">
