<taxonomicName id="4C35DCDBFFE1FFEDDDC4FC8BFDD17BAF" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[502,587,412,435]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="isolatus">
