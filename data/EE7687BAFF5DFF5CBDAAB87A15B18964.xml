<quantity id="A1279B49FF5DFF5CB854BB7B11EC890D" box="[1385,1428,1561,1585]" metricMagnitude="-2" metricUnit="kg" metricValue="3.0" pageId="170" pageNumber="171" unit="g" value="30.0">30g</quantity>
