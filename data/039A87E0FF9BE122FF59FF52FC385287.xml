<collectingCountry id="F3247666FF9BE122FD00FECBFCA15411" box="[718,855,326,352]" name="Madagascar" pageId="1" pageNumber="119">Madagascar</collectingCountry>
, 
<location id="8EEC602DFF9BE122FCADFECBFB745411" LSID="urn:lsid:plazi:treatment:039A87E0FF9BE122FF59FF52FC385287:8EEC602DFF9BE122FCADFECBFB745411" box="[867,1154,326,352]" country="Madagascar" name="Amoron'i Mania Region" pageId="1" pageNumber="119">Amoron’i Mania Region</location>
