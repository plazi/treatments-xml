<taxonomicName id="30AB2FA5FFB3FFED6A1941737FD16B42" authorityName="Linnaeus" authorityYear="1758" box="[870,1012,1806,1832]" class="Aves" family="Picidae" genus="Picus" kingdom="Animalia" order="Piciformes" pageId="65" pageNumber="66" phylum="Chordata" rank="species" species="viridis">
