<number box="[990,1053,312,338]" pageId="14" pageNumber="45" value="1989.0">1989</number>
). 
<emphasis bold="true" box="[151,870,351,377]" pageId="14" pageNumber="45">
<taxonomicName authority="Sabrosky, Bennett" authorityName="Sabrosky, Bennett" box="[151,687,351,377]" class="Insecta" family="Calliphoridae" genus="Protocalliphora" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="45" phylum="Arthropoda" rank="species" species="deceptor">
