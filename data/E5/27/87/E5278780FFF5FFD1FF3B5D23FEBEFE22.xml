<subSubSection pageId="24" pageNumber="55" type="discussion">
<paragraph blockId="24.[151,1438,231,858]" pageId="24" pageNumber="55">
Whitaker &amp; Wilson (
<number box="[444,505,392,418]" pageId="24" pageNumber="55" value="1974.0">1974</number>
) reported 
<taxonomicName box="[625,790,392,417]" class="Arachnida" family="Myocoptidae" genus="Myocoptes" kingdom="Animalia" order="Astigmata" pageId="24" pageNumber="55" phylum="Arthropoda" rank="species" species="japonensis">
