<paragraph blockId="22.[151,1437,152,658]" pageId="22" pageNumber="53">
<taxonomicName box="[198,441,592,617]" class="Arachnida" family="Laelapidae" genus="Myonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="22" pageNumber="53" phylum="Arthropoda" rank="species" species="jamesoni">
