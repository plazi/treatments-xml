<paragraph blockId="21.[151,1438,151,2018]" pageId="21" pageNumber="52">
<taxonomicName box="[198,315,1432,1457]" class="Arachnida" family="Laelapidae" genus="Eulaelaps" kingdom="Animalia" order="Mesostigmata" pageId="21" pageNumber="52" phylum="Arthropoda" rank="genus">
