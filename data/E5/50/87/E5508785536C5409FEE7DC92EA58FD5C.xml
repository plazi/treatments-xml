<paragraph blockId="7.[264,1323,284,710]" pageId="7" pageNumber="8">
Female. Body length 
<quantity box="[564,656,404,430]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="7" pageNumber="8" unit="mm" value="2.6">2.6 mm</quantity>
, wing length 
<quantity box="[814,907,404,430]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="7" pageNumber="8" unit="mm" value="2.8">2.8 mm</quantity>
