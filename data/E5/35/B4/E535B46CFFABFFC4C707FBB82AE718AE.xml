<emphasis bold="true" box="[264,622,1057,1083]" pageId="6" pageNumber="25">
<taxonomicName box="[264,453,1057,1083]" class="Adenophorea" family="Monhysteridae" kingdom="Animalia" order="Monhysterida" pageId="6" pageNumber="25" phylum="Nematoda" rank="family">Monhysteridae</taxonomicName>
de Man 
<number box="[563,622,1057,1083]" pageId="6" pageNumber="25" value="1876.0">1876</number>
</emphasis>
