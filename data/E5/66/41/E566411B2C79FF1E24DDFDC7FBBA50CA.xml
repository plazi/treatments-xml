<paragraph blockId="5.[151,1436,543,712]" pageId="5" pageNumber="366">
<taxonomicName authorityName="Senevet &amp; Abonnenc" authorityYear="1939" box="[199,384,614,640]" class="Insecta" family="Culicidae" genus="Culex" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="366" phylum="Arthropoda" rank="species" species="productus">
