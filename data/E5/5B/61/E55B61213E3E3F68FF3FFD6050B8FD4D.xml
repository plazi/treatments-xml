/ 
<number box="[647,673,1834,1858]" pageId="1" pageNumber="398" value="11.0">11</number>
, 
<number box="[685,801,1834,1858]" pageId="1" pageNumber="398" value="0.895" valueMax="0.92" valueMin="0.87">0.87–0.92</number>
. Labial setae 
<number box="[954,968,1833,1858]" pageId="1" pageNumber="398" value="5.0">5</number>
(
<emphasis box="[984,1017,1833,1857]" italics="true" pageId="1" pageNumber="398">
ls 
<number box="[1002,1017,1833,1857]" pageId="1" pageNumber="398" value="5.0">5</number>
</emphasis>
) 
<number box="[1034,1083,1834,1858]" pageId="1" pageNumber="398" value="0.08">0.08</number>
long, latero-ventral fringe with 
<number box="[151,221,1869,1894]" pageId="1" pageNumber="398" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
