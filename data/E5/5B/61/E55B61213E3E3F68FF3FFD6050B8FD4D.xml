<number box="[914,931,1978,2002]" pageId="1" pageNumber="398" value="4.0">4</number>
; II, 
<number box="[975,991,1978,2002]" pageId="1" pageNumber="398" value="9.0">9</number>
; III–IV, 
<number box="[1085,1114,1978,2002]" pageId="1" pageNumber="398" value="11.0">11</number>
; V, 
<number box="[1157,1188,1978,2002]" pageId="1" pageNumber="398" value="12.0">12</number>
; VI, 
<number box="[1243,1259,1978,2002]" pageId="1" pageNumber="398" value="8.0">8</number>
; VII, 
<number box="[1324,1341,1978,2002]" pageId="1" pageNumber="398" value="4.0">4</number>
; VIII, 
<number box="[1415,1432,1978,2002]" pageId="1" pageNumber="398" value="4.0">4</number>
