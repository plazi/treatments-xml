<number box="[981,991,224,248]" pageId="5" pageNumber="566" value="1.0">1</number>
<number box="[999,1035,223,248]" pageId="5" pageNumber="566" value="1.5">1.5</number>
×, F 
<number box="[1080,1093,224,248]" pageId="5" pageNumber="566" value="2.0">2</number>
<number box="[1101,1137,224,248]" pageId="5" pageNumber="566" value="1.3">1.3</number>
