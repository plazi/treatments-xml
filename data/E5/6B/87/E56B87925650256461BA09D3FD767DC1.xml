<subSubSection lastPageId="3" lastPageNumber="284" pageId="2" pageNumber="283" type="description">
<paragraph blockId="2.[151,1437,1577,2034]" lastBlockId="3.[151,1436,151,429]" lastPageId="3" lastPageNumber="284" pageId="2" pageNumber="283">
<emphasis bold="true" box="[199,443,1577,1602]" pageId="2" pageNumber="283">Description. Female</emphasis>
(
<typeStatus box="[464,575,1577,1602]" pageId="2" pageNumber="283">Holotype</typeStatus>
): Total length 
<number box="[750,803,1577,1602]" pageId="2" pageNumber="283" value="6.66">6.66</number>
. Prosoma 
<number box="[929,978,1577,1602]" pageId="2" pageNumber="283" value="3.15">3.15</number>
long, 
<number box="[1059,1108,1577,1602]" pageId="2" pageNumber="283" value="3.06">3.06</number>
wide. Clypeus height 
<number box="[1380,1433,1577,1601]" pageId="2" pageNumber="283" value="0.34">0.34</number>
. Chelicera: 
<number box="[274,323,1614,1638]" pageId="2" pageNumber="283" value="1.32">1.32</number>
long, 
<number box="[396,445,1614,1638]" pageId="2" pageNumber="283" value="0.99">0.99</number>
wide. Endites: 
<number box="[622,671,1613,1638]" pageId="2" pageNumber="283" value="1.06">1.06</number>
long, 
<number box="[743,792,1614,1638]" pageId="2" pageNumber="283" value="0.49">0.49</number>
wide. Labium 
<number box="[964,1013,1613,1638]" pageId="2" pageNumber="283" value="0.61">0.61</number>
long, 
<number box="[1086,1135,1613,1638]" pageId="2" pageNumber="283" value="0.65">0.65</number>
wide. Sternum: 
<number box="[1323,1372,1613,1638]" pageId="2" pageNumber="283" value="1.35">1.35</number>
long, 
<number box="[151,200,1649,1673]" pageId="2" pageNumber="283" value="1.1">1.10</number>
