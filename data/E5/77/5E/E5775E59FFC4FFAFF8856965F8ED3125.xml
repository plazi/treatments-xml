</taxonomicNameLabel>
and 
<taxonomicName box="[1218,1324,1834,1858]" class="Amphibia" family="Hyperoliidae" genus="Hyperolius" kingdom="Animalia" order="Anura" pageId="9" pageNumber="310" phylum="Chordata" rank="species" species="viridis">
