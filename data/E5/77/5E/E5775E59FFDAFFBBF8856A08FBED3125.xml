); ZMB 
<number box="[1366,1436,1953,1977]" pageId="23" pageNumber="324" value="77415.0">77415</number>
(Kérouane, 
<collectingCountry box="[283,369,1989,2013]" name="Guinea-Bissau" pageId="23" pageNumber="324">Guinea</collectingCountry>
); ZMB 
<number box="[456,526,1989,2013]" pageId="23" pageNumber="324" value="77416.0">77416</number>
(Konsankoro, 
<collectingCountry box="[693,779,1989,2013]" name="Guinea-Bissau" pageId="23" pageNumber="324">Guinea</collectingCountry>
); ZMB 
<number box="[865,935,1989,2013]" pageId="23" pageNumber="324" value="77410.0">77410</number>
(Dantilla, 
<collectingCountry box="[1056,1139,1989,2013]" name="Guinea-Bissau" pageId="23" pageNumber="324">Guinea</collectingCountry>
