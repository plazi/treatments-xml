<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:27BFA05B-0A54-4C56-AE35-8F7CCEEC4577" class="Actinopterygii" family="Sisoridae" genus="Euchiloglanis" kingdom="Animalia" lsidName-ZBK="Euchiloglanis Regan 1907:158" order="Siluriformes" pageNumber="82" phylum="Chordata" rank="genus">Euchiloglanis</taxonomicName>
