<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:03336E7C-2732-46B2-8FB4-12EAF8E099DD" class="Actinopterygii" family="Sisoridae" genus="Myersglanis" kingdom="Animalia" lsidName-ZBK="Myersglanis Hora &amp; Silas 1952:19" order="Siluriformes" pageNumber="82" phylum="Chordata" rank="genus">Myersglanis</taxonomicName>
