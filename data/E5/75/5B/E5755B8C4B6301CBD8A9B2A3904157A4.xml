<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:987310A1-9E85-4272-AD9D-9DB5D24D6779" class="Actinopterygii" family="Sisoridae" genus="Exostoma" kingdom="Animalia" lsidName-ZBK="Exostoma Blyth 1860:155" order="Siluriformes" pageNumber="82" phylum="Chordata" rank="genus">Exostoma</taxonomicName>
