<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:E68EC2A2-F2FB-48AE-BBCE-ABCFD9555304" class="Actinopterygii" family="Sisoridae" genus="Oreoglanis" kingdom="Animalia" lsidName-ZBK="Oreoglanis Smith 1933:70" order="Siluriformes" pageNumber="82" phylum="Chordata" rank="genus">Oreoglanis</taxonomicName>
