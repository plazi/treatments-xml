<collectingCountry box="[360,466,1015,1040]" name="Australia" pageId="27" pageNumber="28">Australia</collectingCountry>
. Western 
<collectingCountry box="[578,685,1015,1040]" name="Australia" pageId="27" pageNumber="28">Australia</collectingCountry>
