<emphasis bold="true" box="[658,803,1210,1236]" pageId="19" pageNumber="472">
In 
<collectingCountry box="[696,798,1210,1236]" name="Ukraine" pageId="19" pageNumber="472">Ukraine</collectingCountry>
.
</emphasis>
<collectingRegion box="[813,911,1210,1236]" country="Ukraine" name="Kharkiv" pageId="19" pageNumber="472">Kharkiv</collectingRegion>
