<emphasis bold="true" box="[831,980,435,460]" pageId="5" pageNumber="144">
<typeStatus box="[831,887,436,460]" pageId="5" pageNumber="144">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Saussure, 1895" authorityName="Saussure" authorityYear="1895" box="[986,1400,435,460]" class="Insecta" family="Blaberidae" genus="Calolampra" kingdom="Animalia" order="Blattodea" pageId="5" pageNumber="144" phylum="Arthropoda" rank="species" species="carinulata">
