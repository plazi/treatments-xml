<collectingCountry box="[454,550,2054,2077]" name="Tanzania" pageId="5" pageNumber="144">Zanzibar</collectingCountry>
(Saussure, 
<number box="[686,744,2054,2077]" pageId="5" pageNumber="144" value="1895.0">1895</number>
).
</paragraph>
<paragraph blockId="5.[831,1436,258,313]" pageId="5" pageNumber="144">
Later 
<taxonomicName box="[901,1075,259,282]" class="Insecta" family="Blaberidae" genus="Epilampra" kingdom="Animalia" order="Blattodea" pageId="5" pageNumber="144" phylum="Arthropoda" rank="species" species="punctipennis">
