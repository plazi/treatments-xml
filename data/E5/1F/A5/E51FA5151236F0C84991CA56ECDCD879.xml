<taxonomicName family="Euphorbiaceae" genus="A." kingdom="Plantae" lsidName="A. emirnensis" order="Malpighiales" pageId="0" pageNumber="85" rank="species" species="emirnensis">
<emphasis italics="true" pageId="0" pageNumber="85">A. emirnensis</emphasis>
</taxonomicName>
; 
<bibRefCitation author="Leandri, J" journalOrPublisher="Notulae Systematicae" pageId="0" pageNumber="85" pagination="182 - 187" refId="B32" refString="Leandri, J, 1948. Euphorbiacees recoltees a Madagascar et aux Mascareignes par MM. Lam et Meeuse en 1938. Notulae Systematicae 13: 182 - 187" title="Euphorbiacees recoltees a Madagascar et aux Mascareignes par MM. Lam et Meeuse en 1938." volume="13" year="1948">Leandri (1948</bibRefCitation>
: 186) as 
<taxonomicName family="Euphorbiaceae" genus="A." infraspecific-rank="var." kingdom="Plantae" lsidName="A. reticulata var. goudotiana" order="Malpighiales" pageId="0" pageNumber="85" rank="variety" species="reticulata" variety="goudotiana">A. reticulata var. goudotiana</taxonomicName>
; 
<bibRefCitation author="Govaerts, R" journalOrPublisher="Kew Bulletin" pageId="0" pageNumber="85" refId="B22" refString="Govaerts, R, Frodin, DG, Radcliffe-Smith, A, 2000. World Checklist and Bibliography of Euphorbiaceae (and Pandaceae). Royal Botanic Gardens, Kew, 1-1621." title="World Checklist and Bibliography of Euphorbiaceae (and Pandaceae). Royal Botanic Gardens, Kew, 1 - 1621." year="2000">Govaerts et al. (2000</bibRefCitation>
