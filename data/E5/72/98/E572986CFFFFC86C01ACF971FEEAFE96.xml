). 
<collectingCountry box="[472,643,296,320]" name="Costa Rica" pageId="11" pageNumber="216">COSTA RICA</collectingCountry>
: 
<collectingRegion box="[656,783,296,319]" country="Costa Rica" name="Puntarenas" pageId="11" pageNumber="216">Puntarenas</collectingRegion>
. 
<collectingRegion box="[796,919,296,320]" country="Panama" name="Panama" pageId="11" pageNumber="216">
<collectingCountry box="[796,919,296,320]" name="Panama" pageId="11" pageNumber="216">PANAMA</collectingCountry>
</collectingRegion>
: 
<collectingRegion box="[932,1003,295,320]" country="Panama" name="Colon" pageId="11" pageNumber="216">Colón</collectingRegion>
, 
<collectingRegion box="[1017,1109,296,319]" country="Panama" name="Panama" pageId="11" pageNumber="216">
<collectingCountry box="[1017,1109,296,319]" name="Panama" pageId="11" pageNumber="216">Panama</collectingCountry>
</collectingRegion>
