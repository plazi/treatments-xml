<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus aspersus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="aspersus">Heterosmylus aspersus</taxonomicName>
<normalizedToken originalValue="Krüger">Krueger</normalizedToken>
, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus curvagradatus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="curvagradatus">Heterosmylus curvagradatus</taxonomicName>
Yang, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus flavidus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="flavidus">Heterosmylus flavidus</taxonomicName>
Yang, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus limulus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="limulus">Heterosmylus limulus</taxonomicName>
Yang, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus primus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="primus">Heterosmylus primus</taxonomicName>
Nakahara, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus processus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="processus">Heterosmylus processus</taxonomicName>
sp. n., 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus shennonganus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="shennonganus">Heterosmylus shennonganus</taxonomicName>
Yang, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus wolonganus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="wolonganus">Heterosmylus wolonganus</taxonomicName>
Yang, 
<taxonomicName class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus yunnanus" order="Neuroptera" pageId="2" pageNumber="109" phylum="Arthropoda" rank="species" species="yunnanus">Heterosmylus yunnanus</taxonomicName>
Yang.
</paragraph>
