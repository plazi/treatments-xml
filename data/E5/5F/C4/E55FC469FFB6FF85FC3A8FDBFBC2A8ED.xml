<taxonomicName authorityName="Azpelicueta &amp; Almiron" authorityYear="2001" box="[1302,1383,1539,1562]" class="Actinopterygii" family="Characidae" genus="Bryconamericus" kingdom="Animalia" order="Characiformes" pageId="3" pageNumber="58" phylum="Chordata" rank="species" species="agna">
