<emphasis box="[1355,1439,1379,1402]" italics="true" pageId="3" pageNumber="58">lambari</emphasis>
</taxonomicName>
; 
<taxonomicName authorityName="Silva" authorityYear="2004" class="Actinopterygii" family="Characidae" genus="Bryconamericus" kingdom="Animalia" order="Characiformes" pageId="3" pageNumber="58" phylum="Chordata" rank="species" species="patriciae">
<emphasis box="[1451,1467,1379,1402]" italics="true" pageId="3" pageNumber="58">B</emphasis>
. 
<emphasis box="[832,928,1411,1434]" italics="true" pageId="3" pageNumber="58">patriciae</emphasis>
</taxonomicName>
; 
<taxonomicName authorityName="Eigenmann" authorityYear="1908" box="[942,1088,1411,1434]" class="Actinopterygii" family="Characidae" genus="Bryconamericus" kingdom="Animalia" order="Characiformes" pageId="3" pageNumber="58" phylum="Chordata" rank="species" species="stramineus">
