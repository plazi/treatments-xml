<quantity id="4CC09B2BFFC5BB12FE5484DDFDA2FF4E" box="[475,575,151,177]" metricMagnitude="-5" metricUnit="m" metricValue="7" pageId="3" pageNumber="154" unit="mm" value="0.07">0.07 mm</quantity>
