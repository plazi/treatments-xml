<quantity id="4CE6E0BDC03B6E77FE5BFDA56D7AE273" box="[438,528,548,572]" metricMagnitude="-1" metricUnit="m" metricValue="5.9" pageId="6" pageNumber="331" unit="mm" value="590.0">590 mm</quantity>
