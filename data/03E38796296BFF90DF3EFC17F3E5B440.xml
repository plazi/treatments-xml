<emphasis id="B93EEA92296BFF90DF3EFC17F1BAB550" bold="true" box="[302,900,1003,1028]" pageId="1" pageNumber="174">
<taxonomicName id="4C4A4D03296BFF90DF3EFC17F0DDB557" authorityName="Cho &amp; Borowiec" authorityYear="2016" box="[302,739,1003,1028]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="174" phylum="Arthropoda" rank="species" species="mauroi" subGenus="Brachyphytodecta">
<emphasis id="B93EEA92296BFF90DF3EFC17F393B557" bold="true" box="[302,429,1004,1028]" italics="true" pageId="1" pageNumber="174">Gonioctena</emphasis>
