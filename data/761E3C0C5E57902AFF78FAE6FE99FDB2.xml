<typeStatus id="210C33B85E57902BFF78FAAEFF6BFA57" box="[151,204,1402,1425]" pageId="4" pageNumber="24">Type</typeStatus>
species: 
<taxonomicName id="39B7F6995E57902BFEC2FAAEFD41FA57" authority="Bagnall, 1937" authorityName="Bagnall" authorityYear="1937" box="[301,742,1402,1425]" class="Entognatha" family="Onychiuridae" genus="Onychiurus" kingdom="Animalia" order="Collembola" pageId="4" pageNumber="24" phylum="Arthropoda" rank="species" species="thalassophilus">
