<quantity id="4C945E81FFABA017FE39719C435E5F0F" box="[444,531,1515,1541]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" pageId="3" pageNumber="4" unit="mm" value="16.0">16 mm</quantity>
