<taxonomicName id="43BBCF882350FFC6FEF1FA5BCD90B597" authorityName="Ponder" authorityYear="2003" box="[309,418,1445,1468]" class="Gastropoda" family="Bithyniidae" genus="Gabbia" kingdom="Animalia" order="Littorinimorpha" pageId="61" pageNumber="62" phylum="Mollusca" rank="species" species="spiralis">
