<typeStatus id="5B000AA92351FFC7FCB8FD52CFD1B2ED" box="[892,995,684,710]" pageId="60" pageNumber="61">holotype</typeStatus>
and figured 
<typeStatus id="5B000AA92351FFC7FB4AFD52C8C1B2ED" box="[1166,1267,684,710]" pageId="60" pageNumber="61">paratype</typeStatus>
