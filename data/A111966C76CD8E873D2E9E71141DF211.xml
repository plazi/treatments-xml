<subSubSection id="51E3331EAB46084352DCF894697BE072" pageId="null" pageNumber="574" type="nomenclature">
<paragraph id="6FA11354640B3D0C0BD30E5E04AA58A6" pageId="null" pageNumber="574">
<pageBreakToken id="199CDDEB461F628FD94DF89F6548E6A9" pageId="null" pageNumber="574">Artengruppe</pageBreakToken>
der 
<taxonomicName id="9A8082F98998A70D9BECB2CD74556837" authority="Scop." class="Magnoliopsida" family="Fabaceae" genus="Onobrychis" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="574" phylum="Tracheophyta" rank="species" species="viciifolia">Onobrychis viciifolia Scop.</taxonomicName>
