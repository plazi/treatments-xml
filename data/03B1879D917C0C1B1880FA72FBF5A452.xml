<heading id="D0EF81E7917C0C1C1880FA72FDD1A3F2" bold="true" box="[151,620,1413,1439]" fontSize="11" level="1" pageId="27" pageNumber="28" reason="1">
<emphasis id="B96CEA99917C0C1C1880FA72FDD1A3F2" bold="true" box="[151,620,1413,1439]" pageId="27" pageNumber="28">
Family 
<taxonomicName id="4C184D08917C0C1C18E1FA72FDD1A3F2" authority="Chitwood, 1937" authorityName="Chitwood" authorityYear="1937" box="[246,620,1413,1439]" class="Adenophorea" family="Mononchidae" kingdom="Animalia" order="Dorylaimida" pageId="27" pageNumber="28" phylum="Nematoda" rank="family">Mononchidae Chitwood, 1937</taxonomicName>
