<collectingRegion box="[577,748,1212,1235]" country="China" name="Nei Mongol" pageId="12" pageNumber="581">Inner Mongolia</collectingRegion>
, 
<collectingRegion box="[761,810,1212,1235]" country="China" name="Jilin" pageId="12" pageNumber="581">Jilin</collectingRegion>
, 
<collectingRegion box="[822,911,1212,1235]" country="China" name="Shaanxi" pageId="12" pageNumber="581">Shaanxi</collectingRegion>
, 
<collectingRegion box="[923,1012,1212,1235]" country="China" name="Sichuan" pageId="12" pageNumber="581">Sichuan</collectingRegion>
