<collectingCountry box="[923,1007,1084,1107]" name="Austria" pageId="12" pageNumber="581">Austria</collectingCountry>
, 
<collectingCountry box="[1021,1122,1084,1107]" name="Germany" pageId="12" pageNumber="581">Germany</collectingCountry>
, 
<collectingCountry box="[151,248,1116,1139]" name="Slovakia" pageId="12" pageNumber="581">Slovakia</collectingCountry>
, 
<collectingCountry box="[263,395,1116,1139]" name="Switzerland" pageId="12" pageNumber="581">Switzerland</collectingCountry>
, 
<collectingCountry box="[409,500,1116,1139]" name="Ukraine" pageId="12" pageNumber="581">Ukraine</collectingCountry>
: 
<collectingRegion box="[513,677,1116,1139]" country="Ukraine" name="Zakarpattya" pageId="12" pageNumber="581">Transcarpathia</collectingRegion>
), in 
<collectingCountry box="[730,802,1116,1139]" name="Russia" pageId="12" pageNumber="581">Russia</collectingCountry>
(Central European Territory: Ekaterinburg Province, West Siberia: 
<collectingRegion box="[548,739,1148,1171]" country="Russia" name="Tyumen'" pageId="12" pageNumber="581">Tyumen Province</collectingRegion>
, East Siberia, Far East), 
<collectingCountry box="[995,1122,1148,1171]" name="Kazakhstan" pageId="12" pageNumber="581">Kazakhstan</collectingCountry>
, 
<collectingCountry box="[151,257,1180,1203]" name="Mongolia" pageId="12" pageNumber="581">Mongolia</collectingCountry>
, 
<collectingCountry box="[266,332,1180,1203]" name="China" pageId="12" pageNumber="581">China</collectingCountry>
