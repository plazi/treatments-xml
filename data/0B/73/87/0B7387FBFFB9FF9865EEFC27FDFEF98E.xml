</taxonomicName>
<taxonomicName authority="(Kaszab, 1981)" authorityName="Fouquè" authorityYear="2015" baseAuthorityName="Kaszab" baseAuthorityYear="1981" box="[415,910,1063,1086]" class="Insecta" family="Tenebrionidae" genus="Indochillus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="226" phylum="Arthropoda" rank="species" species="bangaloreanus" subGenus="Pseudochillus">
(
<emphasis box="[424,572,1063,1086]" italics="true" pageId="9" pageNumber="226">Pseudochillus</emphasis>
) 
<emphasis box="[586,744,1063,1086]" italics="true" pageId="9" pageNumber="226">bangaloreanus</emphasis>
(
<bibRefCitation author="KASZAB Z." box="[755,902,1063,1086]" pageId="9" pageNumber="226" pagination="273 - 313" refId="ref12775" refString="KASZAB Z. 1981: Neue orientalische Stenosini (Coleoptera: Tenebrionidae). Acta Zoologica Academiae Scientiarum Hungaricae 27: 273 - 313." type="journal article" year="1981">Kaszab, 1981</bibRefCitation>
)
</taxonomicName>
