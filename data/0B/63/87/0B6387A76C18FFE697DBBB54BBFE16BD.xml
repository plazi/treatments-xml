<subSubSection pageId="76" pageNumber="77" type="discussion">
<paragraph blockId="76.[189,303,1800,1826]" box="[189,303,1800,1826]" pageId="76" pageNumber="77">
<heading bold="true" box="[189,303,1800,1826]" fontSize="11" level="3" pageId="76" pageNumber="77" reason="3">
