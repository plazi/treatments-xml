<geoCoordinate degrees="23.5663" direction="north" orientation="latitude" precision="5" value="23.5663">23.5663N</geoCoordinate>
, 
<geoCoordinate degrees="107.2622" direction="east" orientation="longitude" precision="5" value="107.2622">107.2622E</geoCoordinate>
