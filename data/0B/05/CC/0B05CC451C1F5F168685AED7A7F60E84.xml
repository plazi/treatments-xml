<subSubSection pageId="0" pageNumber="15" type="diagnosis">
<paragraph pageId="0" pageNumber="15">Diagnosis.</paragraph>
<paragraph pageId="0" pageNumber="15">
<taxonomicName authorityName="Zhao &amp; Li &amp; Zhang" authorityYear="2020" baseAuthorityName="Lin &amp; Li" baseAuthorityYear="2010" class="Arachnida" family="Telemidae" genus="Pinelema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pinelema tortutheca" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="tortutheca">
<emphasis italics="true" pageId="0" pageNumber="15">Pinelema tortutheca</emphasis>
</taxonomicName>
