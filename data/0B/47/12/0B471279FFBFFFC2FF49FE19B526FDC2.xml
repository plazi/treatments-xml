<emphasis box="[197,313,565,590]" italics="true" pageId="6" pageNumber="211">
<taxonomicName authorityName="White" authorityYear="1841" box="[197,309,565,590]" class="Arachnida" family="Tetragnathidae" genus="Leucauge" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="211" phylum="Arthropoda" rank="genus">Leucauge</taxonomicName>
