<emphasis box="[831,863,1166,1187]" italics="true" pageId="5" pageNumber="210">Em</emphasis>
— embolus; 
<emphasis box="[989,1008,1166,1187]" italics="true" pageId="5" pageNumber="210">Ll</emphasis>
— lamellate process of 
<emphasis box="[170,195,1192,1213]" italics="true" pageId="5" pageNumber="210">Eb</emphasis>
; 
<emphasis box="[206,266,1193,1214]" italics="true" pageId="5" pageNumber="210">Mp —</emphasis>
