<taxonomicName authorityName="Wang &amp; Yao" authorityYear="2020" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pholcus tongyaoi" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="tongyaoi">
