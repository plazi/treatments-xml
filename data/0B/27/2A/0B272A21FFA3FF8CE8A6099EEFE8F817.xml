<caption httpUri="https://zenodo.org/record/2682384/files/figure.png" pageId="11" pageNumber="276" startId="11.[151,250,1946,1971]" targetBox="[203,1398,211,1885]" targetPageId="11">
<paragraph blockId="11.[151,1437,1946,2043]" pageId="11" pageNumber="276">
