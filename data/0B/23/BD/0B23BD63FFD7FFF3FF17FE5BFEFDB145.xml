<collectingCountry box="[151,246,654,679]" name="Portugal" pageId="4" pageNumber="155">Portugal</collectingCountry>
and 
<collectingCountry box="[300,364,654,679]" name="Spain" pageId="4" pageNumber="155">Spain</collectingCountry>
