<collectingCountry box="[1030,1106,1157,1180]" name="France" pageId="16" pageNumber="167">France</collectingCountry>
and 
<collectingCountry box="[1160,1211,1156,1181]" name="Italy" pageId="16" pageNumber="167">Italy</collectingCountry>
to 
<collectingCountry box="[1245,1327,1156,1180]" name="Greece" pageId="16" pageNumber="167">Greece</collectingCountry>
, 
<collectingCountry box="[1338,1436,1156,1181]" name="Slovakia" pageId="16" pageNumber="167">Slovakia</collectingCountry>
and Crimea.
</paragraph>
