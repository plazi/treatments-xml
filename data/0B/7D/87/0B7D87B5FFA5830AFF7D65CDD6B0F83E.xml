<heading bold="true" box="[151,495,1414,1440]" fontSize="11" level="1" pageId="4" pageNumber="304" reason="1">
<emphasis bold="true" box="[151,495,1414,1440]" pageId="4" pageNumber="304">
<taxonomicName box="[151,394,1414,1440]" class="Insecta" family="Leiodidae" genus="Camisolus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="304" phylum="Arthropoda" rank="species" species="ptinoides" status="sp. nov.">
<emphasis bold="true" box="[151,394,1414,1440]" italics="true" pageId="4" pageNumber="304">Camisolus ptinoides</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[402,495,1414,1440]" pageId="4" pageNumber="304" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
