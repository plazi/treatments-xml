<paragraph pageId="null" pageNumber="660">
<taxonomicName authority="Ry" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="660" phylum="Tracheophyta" rank="species" species="loiseleurii">
