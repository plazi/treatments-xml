<paragraph blockId="7.[151,1435,152,252]" pageId="7" pageNumber="8">
<taxonomicName baseAuthorityName="Bai, Heiss &amp; Cai" baseAuthorityYear="2006" box="[198,499,152,177]" class="Insecta" family="Aradidae" genus="Heterobarcinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="kormilevi">
