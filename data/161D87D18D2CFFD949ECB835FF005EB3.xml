. 
<taxonomicName id="59B44D448D2DFFDE4862B824FE185B8E" box="[281,489,1919,1941]" class="Insecta" family="Coccinellidae" genus="Chnoodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="280" phylum="Arthropoda" rank="species" species="machadoi" status="sp. nov.">
<emphasis id="ACC0EAD58D2DFFDE4862B824FE185B8E" box="[281,489,1919,1941]" italics="true" pageId="11" pageNumber="280">
<taxonomicName id="59B44D448D2DFFDE4862B824FE8F5B8E" authority="Chevrolat, 1837" authorityName="Chevrolat" authorityYear="1837" box="[281,382,1919,1941]" class="Insecta" family="Coccinellidae" genus="Chnoodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="280" phylum="Arthropoda" rank="genus">Chnoodes</taxonomicName>
machadoi
</emphasis>
</taxonomicName>
<emphasis id="ACC0EAD58D2DFFDE4894B824FDB15B8F" bold="true" box="[495,576,1919,1940]" pageId="11" pageNumber="280">
