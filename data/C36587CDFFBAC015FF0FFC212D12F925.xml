<number id="FD0424FCFFBAC015FD00FAD12BC9FA8D" box="[664,713,1375,1399]" pageId="12" pageNumber="209" value="3.02">3.02</number>
(n= 
<number id="FD0424FCFFBAC015FD65FAD12A07FA8D" box="[765,775,1375,1399]" pageId="12" pageNumber="209" value="1.0">1</number>
). 
<typeStatus id="94778879FFBAC015FC83FAD02A88FA8D" box="[795,904,1374,1399]" pageId="12" pageNumber="209">Holotype</typeStatus>
: body 
<number id="FD0424FCFFBAC015FC48FAD12D01FA8D" box="[976,1025,1375,1399]" pageId="12" pageNumber="209" value="3.02">3.02</number>
long; carapace 
<number id="FD0424FCFFBAC015FB28FAD12DE1FA8D" box="[1200,1249,1375,1399]" pageId="12" pageNumber="209" value="1.44">1.44</number>
long, 
