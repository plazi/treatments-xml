[[ Genus 
<taxonomicName id="DF74D33DA1B5AB79CD818B33D30FCADE" LSID-ZBK="urn:lsid:zoobank.org:act:A3AFED16-DCBE-40C1-B346-02CFDA1DF5AC" class="Actinopterygii" family="Pseudochromidae" genus="Lubbockichthys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Lubbockichthys Gill &amp; Edwards 1999:144" order="Perciformes" pageNumber="43" phylum="Chordata" rank="genus">Lubbockichthys</taxonomicName>
