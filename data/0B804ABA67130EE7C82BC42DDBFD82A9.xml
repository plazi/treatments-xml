, b = 200 
<normalizedToken id="E155A1FB1B9A98331F39604FAD4415D2" originalValue="μm">μm</normalizedToken>
, c, n = 30 
<normalizedToken id="22F066376C3362DE4B6B3A11D36635FC" originalValue="μm">μm</normalizedToken>
, d, e = 10 
