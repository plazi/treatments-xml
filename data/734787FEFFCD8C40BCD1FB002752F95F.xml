<taxonomicName id="3CEE4D6BFFCD8C40BD2AFAFF24D9FAE7" authority="Khan &amp; Baig" authorityName="Khan &amp; Baig" box="[364,712,1297,1321]" class="Reptilia" family="Gekkonidae" genus="Tenuidactylus" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="317" phylum="Chordata" rank="species" species="baturensis">
