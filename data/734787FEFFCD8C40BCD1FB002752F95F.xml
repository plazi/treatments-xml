</taxonomicName>
, 
<taxonomicName id="3CEE4D6BFFCD8C40B8CEFAFF2707FA83" authority="Golubev &amp; Szczerbak" authorityName="Golubev &amp; Szczerbak" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="317" phylum="Chordata" rank="species" species="mintoni">
<emphasis id="C99AEAFAFFCD8C40B8CEFAFF2313FAE7" box="[1160,1282,1297,1321]" italics="true" pageId="16" pageNumber="317">A. mintoni</emphasis>
(Golubev &amp; Szczerbak)
</taxonomicName>
, 
<taxonomicName id="3CEE4D6BFFCD8C40BD6AFADB2468FA83" authority="Steindachner" authorityName="Steindachner" box="[300,633,1333,1357]" genus="Altigekko" kingdom="Animalia" pageId="16" pageNumber="317" rank="species" species="stoliczkai">
<emphasis id="C99AEAFAFFCD8C40BD6AFADB27D1FA83" box="[300,448,1333,1357]" italics="true" pageId="16" pageNumber="317">A. stoliczkai</emphasis>
