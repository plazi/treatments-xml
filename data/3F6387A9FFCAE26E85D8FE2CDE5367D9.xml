<taxonomicName id="70CA4D3CFFCAE26E818BFDBED86667AD" box="[1220,1384,621,644]" class="Hydrozoa" family="Corynidae" genus="Coryne" kingdom="Animalia" order="Anthoathecata" pageId="39" pageNumber="40" phylum="Cnidaria" rank="species" species="eximia">
