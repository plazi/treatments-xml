<taxonomicName id="4C5738DEA97F6A47FDBBFAFF1FEBC4E5" authorityName="Pocock" authorityYear="1894" class="Diplopoda" kingdom="Animalia" order="Chordeumatida" pageId="10" pageNumber="85" phylum="Arthropoda" rank="order">Chordeumatida</taxonomicName>
