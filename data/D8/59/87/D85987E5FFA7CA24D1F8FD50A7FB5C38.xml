<paragraph blockId="2.[646,941,736,763]" box="[646,941,736,763]" pageId="2" pageNumber="3">
<heading box="[646,941,736,763]" centered="true" fontSize="11" level="3" pageId="2" pageNumber="3" reason="4">
<taxonomicName authority="Simon, 1914" authorityName="Simon" authorityYear="1914" box="[646,941,736,763]" class="Arachnida" family="Oonopidae" genus="Tapinesthis" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[646,783,736,763]" italics="true" pageId="2" pageNumber="3">Tapinesthis</emphasis>
<bibRefCitation author="Simon E." box="[791,941,736,763]" journalOrPublisher="Synopsis Generale et Catalogue des Especes Francaises de l'Ordre des Araneae, Paris" pageId="2" pageNumber="3" pagination="1 - 308" part="6" refString="Simon E. 1914. Les arachnides de France. Synopsis Generale et Catalogue des Especes Francaises de l'Ordre des Araneae, Paris, 6 (1): 1 - 308." title="Les arachnides de France" type="journal article" year="1914">Simon, 1914</bibRefCitation>
</taxonomicName>
</heading>
</paragraph>
