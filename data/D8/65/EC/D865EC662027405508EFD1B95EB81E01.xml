<emphasis box="[422,596,825,850]" italics="true" pageId="27" pageNumber="328">
<taxonomicName box="[422,497,825,850]" class="Insecta" family="Cleridae" genus="Clerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="328" phylum="Arthropoda" rank="genus">Clerus</taxonomicName>
