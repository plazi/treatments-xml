<subSubSection pageId="6" pageNumber="307" type="materials_examined">
<paragraph blockId="6.[151,1436,404,1077]" box="[199,560,764,789]" pageId="6" pageNumber="307">
<emphasis bold="true" box="[199,357,764,789]" pageId="6" pageNumber="307">
<typeStatus box="[199,258,764,789]" pageId="6" pageNumber="307">Type</typeStatus>
locality.
