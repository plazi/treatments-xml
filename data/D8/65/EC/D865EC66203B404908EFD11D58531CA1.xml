<paragraph blockId="7.[151,1436,725,1254]" box="[151,671,725,750]" pageId="7" pageNumber="308">
<emphasis bold="true" box="[151,416,725,750]" pageId="7" pageNumber="308">Original combination.</emphasis>
<taxonomicName box="[422,671,725,750]" class="Insecta" family="Cleridae" genus="Hydnocera" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="308" phylum="Arthropoda" rank="species" species="haematica">
