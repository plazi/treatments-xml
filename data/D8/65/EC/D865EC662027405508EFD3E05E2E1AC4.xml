<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[331,420,331,354]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="27" pageNumber="328">MNHN</collectionCode>
