<paragraph blockId="13.[151,1436,258,679]" box="[151,632,258,283]" pageId="13" pageNumber="314">
<emphasis bold="true" box="[151,416,258,283]" pageId="13" pageNumber="314">Original combination.</emphasis>
<taxonomicName box="[422,632,258,283]" class="Insecta" family="Cleridae" genus="Hydnocera" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="314" phylum="Arthropoda" rank="species" species="rufipes">
