<number box="[1252,1337,1729,1753]" pageId="7" pageNumber="73" value="8.3" valueMax="9.1" valueMin="7.5">7.5–9.1</number>
/ 
<number box="[1346,1432,1729,1753]" pageId="7" pageNumber="73" value="8.850000000000001" valueMax="9.3" valueMin="8.4">8.4–9.3</number>
; prothorax: length, 
<number box="[371,456,1765,1789]" pageId="7" pageNumber="73" value="1.65" valueMax="1.8" valueMin="1.5">1.5–1.8</number>
/ 
<number box="[465,551,1765,1789]" pageId="7" pageNumber="73" value="1.75" valueMax="1.8" valueMin="1.7">1.7–1.8</number>
; anterior width, 
<number box="[753,839,1765,1789]" pageId="7" pageNumber="73" value="1.0" valueMax="1.1" valueMin="0.9">0.9–1.1</number>
/ 
<number box="[848,932,1765,1789]" pageId="7" pageNumber="73" value="1.15" valueMax="1.2" valueMin="1.1">1.1–1.2</number>
; posterior width, 
<number box="[1147,1230,1765,1789]" pageId="7" pageNumber="73" value="1.2000000000000002" valueMax="1.3" valueMin="1.1">1.1–1.3</number>
/ 
<number box="[1240,1326,1765,1789]" pageId="7" pageNumber="73" value="1.35" valueMax="1.4" valueMin="1.3">1.3–1.4</number>
