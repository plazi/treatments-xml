<number box="[323,408,1801,1825]" pageId="7" pageNumber="73" value="1.7000000000000002" valueMax="1.8" valueMin="1.6">1.6–1.8</number>
; elytral length, 
<number box="[582,616,1801,1825]" pageId="7" pageNumber="73" value="5.1">5.1</number>
