<subSubSection pageId="24" pageNumber="425" type="etymology">
<paragraph blockId="24.[151,1437,151,536]" pageId="24" pageNumber="425">
<emphasis bold="true" box="[199,326,476,501]" pageId="24" pageNumber="425">Etymology</emphasis>
