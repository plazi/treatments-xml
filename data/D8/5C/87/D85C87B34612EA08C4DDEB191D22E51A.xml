<collectingCountry box="[647,740,689,713]" name="Sweden" pageId="15" pageNumber="366">Sweden</collectingCountry>
, 
<collectingCountry box="[756,854,689,713]" name="Lithuania" pageId="15" pageNumber="366">Lituania</collectingCountry>
