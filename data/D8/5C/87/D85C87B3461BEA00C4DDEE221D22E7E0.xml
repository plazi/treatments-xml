<collectingCountry box="[512,580,188,212]" name="Spain" pageId="7" pageNumber="358">Spain</collectingCountry>
, 
<collectingCountry box="[602,681,189,212]" name="France" pageId="7" pageNumber="358">France</collectingCountry>
