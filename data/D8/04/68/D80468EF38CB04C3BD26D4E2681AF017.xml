<subSubSection pageId="2" pageNumber="81" type="type species">
<paragraph pageId="2" pageNumber="81">Type species.</paragraph>
<paragraph pageId="2" pageNumber="81">
<taxonomicName class="Insecta" family="Erotylidae" genus="Megalodacne" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megalodacne ulkei" order="Coleoptera" pageId="2" pageNumber="81" phylum="Arthropoda" rank="species" species="ulkei">Megalodacne ulkei</taxonomicName>
