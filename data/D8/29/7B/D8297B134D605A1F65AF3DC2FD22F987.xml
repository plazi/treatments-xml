<subSubSection box="[151,434,978,1004]" pageId="71" pageNumber="72" type="nomenclature">
<paragraph blockId="71.[151,434,978,1004]" box="[151,434,978,1004]" pageId="71" pageNumber="72">
<heading bold="true" box="[151,434,978,1004]" fontSize="11" level="1" pageId="71" pageNumber="72" reason="1">
<emphasis bold="true" box="[151,434,978,1004]" pageId="71" pageNumber="72">
<taxonomicName box="[151,317,978,1004]" class="Malacostraca" family="Maeridae" genus="Maeraceterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,317,978,1004]" italics="true" pageId="71" pageNumber="72">Maeraceterus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[324,434,978,1004]" pageId="71" pageNumber="72" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
