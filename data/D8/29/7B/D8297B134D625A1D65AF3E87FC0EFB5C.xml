<subSubSection pageId="69" pageNumber="70" type="discussion">
<paragraph blockId="69.[151,1437,319,1173]" pageId="69" pageNumber="70">
<emphasis bold="true" box="[199,315,1004,1029]" pageId="69" pageNumber="70">Remarks.</emphasis>
