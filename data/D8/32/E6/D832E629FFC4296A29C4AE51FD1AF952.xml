<taxonomicName box="[281,428,1060,1082]" class="Malacostraca" family="Lysianassidae" genus="Pseudambasia" higherTaxonomySource="CoL" kingdom="Animalia" order="Amphipoda" pageId="12" pageNumber="313" phylum="Arthropoda" rank="genus">Pseudambasia</taxonomicName>
<taxonomicName box="[435,500,1061,1082]" pageId="12" pageNumber="313" rank="species" species="poorei" status="sp. nov.">poorei</taxonomicName>
