<taxonomicName authorityName="Casey" authorityYear="1914" class="Insecta" family="Carabidae" genus="Anisotarsus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="637" pageNumber="638" phylum="Arthropoda" rank="species" species="extraneus">Anisotarsus extraneus</taxonomicName>
