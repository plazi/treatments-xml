<taxonomicName id="4C424D1CFFCC4453A0F77566FDD6FE87" authority="von Hagenow, 1851" authorityName="von Hagenow" authorityYear="1851" box="[251,580,289,313]" class="Gymnolaemata" family="Onychocellidae" genus="Onychocellaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="36" pageNumber="1692" phylum="Bryozoa" rank="species" species="rhombea">
