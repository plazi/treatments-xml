<paragraph id="9A4DB95AFFD9FFE2926124CFDF6FF4C3" blockId="5.[264,892,1473,1497]" box="[264,892,1473,1497]" pageId="5" pageNumber="6">
<treatmentCitationGroup id="BAE29E74FFD9FFE2926124CFDF6FF4C3" box="[264,892,1473,1497]" pageId="5" pageNumber="6">
<taxonomicName id="5DF2C2D9FFD9FFE2926124CFDD98F4C2" box="[264,395,1474,1497]" class="Insecta" family="Chironomidae" genus="Chironomus" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
<emphasis id="A8866548FFD9FFE2926124CFDD98F4C2" box="[264,395,1474,1497]" italics="true" pageId="5" pageNumber="6">Chironomus</emphasis>
</taxonomicName>
&quot; 
<taxonomicName id="5DF2C2D9FFD9FFE292F324CCDE54F4C3" authority="Holmgr." authorityName="Holmgr." box="[410,583,1473,1496]" class="Insecta" family="Chironomidae" genus="Chironomus" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="polaris">polaris Holmgr.</taxonomicName>
&quot; sensu 
<treatmentCitation id="1B539F4BFFD9FFE291F124CCDF6BF4C3" author="Lundstrom" box="[664,888,1473,1496]" page="6" pageId="5" pageNumber="6" year="1915">
<bibRefCitation id="FE63C4ABFFD9FFE291F124CCDF6BF4C3" author="Lundstrom" box="[664,888,1473,1496]" pageId="5" pageNumber="34" refString="Lundstrom, C. (1915) Resultats scientifiques de lExpedition Polaire Russe en 1900 - 1903, sous la direction du Baron E. Toll. Section E: Zoologie. Volume II, livr. 8. Diptera Nematocera aus den arctischen Gegenden Sibiriens. Memoires de lAcademie Imperiale des Sciences, Classe Physico-Mathematique, VIII Serie, 29 (8), 1 - 33." type="journal article" year="1915">
Lundström (
<number id="2C3AAB7DFFD9FFE2907224CCDF46F4C3" box="[795,853,1473,1496]" pageId="5" pageNumber="6" value="1915.0">1915</number>
: 
<number id="2C3AAB7DFFD9FFE2903624CCDF7CF4C3" box="[863,879,1473,1496]" pageId="5" pageNumber="6" value="6.0">6</number>
)
</bibRefCitation>
