<collectingCountry box="[772,871,1806,1832]" name="Vietnam" pageId="1" pageNumber="26">Vietnam</collectingCountry>
in 
<collectingRegion box="[910,1052,1806,1832]" country="Vietnam" name="Quang Nam" pageId="1" pageNumber="26">Quang Nam</collectingRegion>
and 
<collectingRegion box="[1110,1305,1806,1832]" country="Vietnam" name="Thua Thien-Hue" pageId="1" pageNumber="26">Thua Thien-Hue</collectingRegion>
