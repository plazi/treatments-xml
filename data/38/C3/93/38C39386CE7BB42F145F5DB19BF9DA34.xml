<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142846" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" lsidName="Ponera parallela Smith" lsidName-HNS="Ponera parallela Smith" order="Hymenoptera" pageNumber="143" phylum="Arthropoda" rank="species" species="parallela">Ponera parallela</taxonomicName>
