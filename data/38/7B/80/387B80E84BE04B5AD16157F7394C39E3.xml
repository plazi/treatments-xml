<paragraph pageId="32" pageNumber="33">
<taxonomicName class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristiphora sootryeni" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="sootryeni">Pristiphora sootryeni</taxonomicName>
