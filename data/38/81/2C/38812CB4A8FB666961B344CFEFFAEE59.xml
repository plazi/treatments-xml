<subSubSection pageId="35" pageNumber="174" type="type species">
<paragraph pageId="35" pageNumber="174">Type species.</paragraph>
<paragraph pageId="35" pageNumber="174">
<taxonomicName authorityName="Heraty" authorityYear="1994" class="Insecta" family="Eucharitidae" genus="Orasema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Orasema koghisiana" order="Hymenoptera" pageId="35" pageNumber="174" phylum="Arthropoda" rank="species" species="koghisiana">
<emphasis italics="true" pageId="35" pageNumber="174">Orasema koghisiana</emphasis>
