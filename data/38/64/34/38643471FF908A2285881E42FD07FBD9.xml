<taxonomicName authority="Simon, 1897" authorityName="Simon" authorityYear="1897" box="[936,1222,879,903]" class="Arachnida" family="Ctenidae" genus="Enoploctenus" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="71" phylum="Arthropoda" rank="genus">
