<taxonomicName baseAuthorityName="Cameron" baseAuthorityYear="1905" class="Insecta" family="Ichneumonidae" genus="Cymodusa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cymodusa taprobanica" order="Hymenoptera" pageId="0" pageNumber="103" phylum="Arthropoda" rank="species" species="taprobanica">
