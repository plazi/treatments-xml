<subSubSection pageId="6" pageNumber="2317" type="nomenclature">
<paragraph blockId="6.[160,1155,141,236]" box="[160,362,141,167]" pageId="6" pageNumber="2317">
<heading box="[160,362,141,167]" fontSize="11" level="3" pageId="6" pageNumber="2317" reason="5">
<emphasis bold="true" box="[160,362,141,167]" inLineHeading="true" italics="true" pageId="6" pageNumber="2317" reason="3">
<taxonomicName authorityName="Diakonoff" authorityYear="1982" box="[160,205,141,167]" class="Insecta" family="Tortricidae" genus="Age" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="2317" phylum="Arthropoda" rank="genus">Age</taxonomicName>
