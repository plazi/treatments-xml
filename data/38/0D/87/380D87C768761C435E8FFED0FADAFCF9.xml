<number box="[498,529,1790,1814]" pageId="2" pageNumber="41" value="56.0">56</number>
; third valvula length, 
<number box="[772,837,1790,1814]" pageId="2" pageNumber="41" value="11.25">11.25</number>
; mid tibia length, 
<number box="[1037,1068,1790,1814]" pageId="2" pageNumber="41" value="44.0">44</number>
; mid basitarsus length, 
<number box="[1329,1381,1790,1814]" pageId="2" pageNumber="41" value="22.5">22.5</number>
; mid tibial spur length, 
<number box="[355,407,1825,1849]" pageId="2" pageNumber="41" value="20.5">20.5</number>
.
</paragraph>
