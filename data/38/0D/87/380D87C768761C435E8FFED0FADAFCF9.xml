<collectionCode box="[685,741,1754,1777]" pageId="2" pageNumber="41">TVII</collectionCode>
without setae; and 
<collectionCode box="[959,1024,1754,1777]" pageId="2" pageNumber="41">TVIII</collectionCode>
with 
<number box="[1087,1101,1754,1777]" pageId="2" pageNumber="41" value="7.0">7</number>
setae. 
