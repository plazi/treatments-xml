<subSubSection pageId="5" pageNumber="44" type="etymology">
<paragraph blockId="5.[151,1436,1444,2009]" pageId="5" pageNumber="44">
<emphasis bold="true" box="[199,334,1877,1901]" pageId="5" pageNumber="44">Etymology.</emphasis>
