<taxonomicName box="[215,419,758,783]" class="Insecta" family="Hexagenitidae" genus="Epicharmeropsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="4" pageNumber="43" phylum="Arthropoda" rank="species" species="hexavenulosus">
