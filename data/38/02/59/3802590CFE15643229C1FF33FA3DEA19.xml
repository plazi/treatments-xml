<emphasis box="[1172,1330,306,331]" italics="true" pageId="1" pageNumber="52">
.... 
<taxonomicName authority="Okada" authorityName="Okada" box="[1205,1330,306,331]" class="Insecta" family="Keroplatidae" genus="Isoneuromyia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="52" phylum="Arthropoda" rank="species" species="formosana">formosana</taxonomicName>
