<collectionCode LSID="urn:lsid:biocol.org:col:14498" box="[1029,1077,1954,1978]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14498" name="University of Coimbra Botany Department" pageId="29" pageNumber="30">COI</collectionCode>
sequence differs by &lt;
<number box="[1335,1349,1954,1978]" pageId="29" pageNumber="30" value="2.0">2</number>
% from GenBank accession 
<accessionNumber box="[378,489,1990,2014]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/JF949750" pageId="29" pageNumber="30">
JF 
<number box="[405,489,1990,2014]" pageId="29" pageNumber="30" value="949750.0">949750</number>
</accessionNumber>
and by&gt; 
<number box="[594,608,1990,2014]" pageId="29" pageNumber="30" value="4.0">4</number>
% from other 
<taxonomicName box="[764,854,1991,2014]" class="Arachnida" family="Salticidae" genus="Servaea" kingdom="Animalia" order="Araneae" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis box="[764,854,1991,2014]" italics="true" pageId="29" pageNumber="30">Servaea</emphasis>
</taxonomicName>
species.
</paragraph>
