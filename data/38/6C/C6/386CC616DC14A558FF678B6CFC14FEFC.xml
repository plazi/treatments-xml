<taxonomicName authority="(Row, 1909)" authorityName="Row" authorityYear="1909" baseAuthorityName="Row" baseAuthorityYear="1909" box="[295,720,1821,1843]" class="Calcarea" family="Grantiidae" genus="Leucandrilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosolenida" pageId="104" pageNumber="105" phylum="Porifera" rank="species" species="intermedia">
<taxonomicName box="[295,584,1821,1843]" class="Calcarea" family="Grantiidae" genus="Leucandrilla" higherTaxonomySource="CoL" kingdom="Animalia" order="Leucosolenida" pageId="104" pageNumber="105" phylum="Porifera" rank="species" species="intermedia">
