<collectionCode box="[822,909,620,644]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="53" pageNumber="54">USNM</collectionCode>
