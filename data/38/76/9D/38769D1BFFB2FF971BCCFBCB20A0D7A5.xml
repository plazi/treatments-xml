<collectingCountry box="[653,810,1688,1714]" name="Taiwan Island" pageId="4" pageNumber="393">Taiwan Island</collectingCountry>
(
<bibRefCitation author="Fang, Ch." box="[826,956,1688,1714]" pageId="4" pageNumber="393" pagination="63 - 72" refId="ref3547" refString="Fang, Ch. (1982) Arctiidae. In: Wang, P. (Ed.), Iconographia heterocerorum sinicorum. Vol. 2. Science Press, Beijing, pp. 190 - 277, pls. 63 - 72. [in Chinese]" type="book chapter" year="1982">Fang, 1982</bibRefCitation>
; 
<bibRefCitation author="Fang, C." box="[968,1024,1688,1714]" pageId="4" pageNumber="393" refId="ref3510" refString="Fang, C. (2000) Lepidoptera. Arctiidae. Fauna Sinica (Insecta). Vol. 19. Science Press, Beijing, 590 pp. [in Chinese, with English summary]" type="book" year="2000">2000</bibRefCitation>
partim; Chang 1989; 
<bibRefCitation author="Heppner, J. B." box="[1270,1432,1688,1715]" pageId="4" pageNumber="393" pagination="1 - 84" refId="ref3644" refString="Heppner, J. B. (2012) Taiwan Lepidoptera Catalog. Supplement 1. Corrections and additions. Lepidoptera Novae, 5 (1), 1 - 84." type="journal article" year="2012">Heppner 2012</bibRefCitation>
, as 
<taxonomicName box="[182,472,1724,1750]" class="Magnoliopsida" family="Scrophulariaceae" genus="Manulea" kingdom="Plantae" order="Lamiales" pageId="4" pageNumber="393" phylum="Tracheophyta" rank="variety" species="costipuncta" variety="postmaculosa">
<emphasis box="[182,472,1724,1750]" italics="true" pageId="4" pageNumber="393">costipuncta postmaculosa</emphasis>
