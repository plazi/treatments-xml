<typeStatus box="[198,257,1901,1927]" pageId="1" pageNumber="45">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[391,505,1901,1927]" pageId="1" pageNumber="45">Holotype</typeStatus>
: ɗ, 
<collectingCountry box="[553,650,1901,1927]" name="China" pageId="1" pageNumber="45">CHINA</collectingCountry>
. Guangxi Province: 
<taxonomicName authority="Defu" authorityName="Defu" box="[894,1036,1901,1927]" class="Insecta" family="Cicadellidae" genus="Napo" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="45" phylum="Arthropoda" rank="genus">Napo, Defu</taxonomicName>
, 
<date box="[1049,1177,1901,1927]" pageId="1" pageNumber="45" value="2000-06-19">
<number box="[1049,1078,1901,1927]" pageId="1" pageNumber="45" value="19.0">19</number>
.vi. 
<number box="[1110,1177,1901,1927]" pageId="1" pageNumber="45" value="2000.0">2000</number>
