, coll. Chaodong Zhu; 
<typeStatus box="[151,255,1941,1967]" pageId="1" pageNumber="45">paratype</typeStatus>
, 
<number box="[266,281,1941,1967]" pageId="1" pageNumber="45" value="1.0">1</number>
ɗ, 
