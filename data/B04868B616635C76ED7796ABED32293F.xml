<taxonomicName id="D5B2FC7452E5EDEE37EC6CEF5764F02E" class="Anthozoa" family="Neozoanthidae" genus="Neozoanthus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neozoanthus" order="Zoantharia" pageId="2" pageNumber="71" phylum="Cnidaria" rank="genus">Neozoanthus</taxonomicName>
