<taxonomicName id="C6E5D2C5FFD8FF96FD88B6F0A71CF956" authority="Koehler and Vaney, 1908" authorityName="Koehler and Vaney" authorityYear="1908" box="[563,1056,1706,1731]" class="Holothuroidea" family="Holothuriidae" genus="Holothuria" kingdom="Animalia" order="Holothuriida" pageId="10" pageNumber="2496" phylum="Echinodermata" rank="species" species="exilis">
