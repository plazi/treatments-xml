<taxonomicName id="C9A14D0DFFE4FF93FEADE1EBAD96FE12" authority="Gory and Percheron, 1833: 31" authorityName="Gory and Percheron" authorityPageNumber="31" authorityYear="1833" box="[372,765,417,437]" class="Insecta" family="Cetoniidae" genus="Diplognatha" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="183" phylum="Arthropoda" rank="genus">
