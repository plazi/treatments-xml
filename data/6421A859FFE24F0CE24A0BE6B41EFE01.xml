<taxonomicName id="2B8862CCFFE04F0CE1980F4CB774FEA2" authority="d'Orbigny" authorityName="d'Orbigny" authorityYear="1850" box="[190,377,321,345]" class="Gastropoda" family="Hydrobiidae" genus="Paludina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="15" pageNumber="32" phylum="Mollusca" rank="species" species="sublenta">
