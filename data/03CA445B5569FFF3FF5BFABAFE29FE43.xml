<emphasis id="B917295F5568FFF3FF09FEEAFEB5FEB0" bold="true" box="[197,357,311,337]" pageId="3" pageNumber="63">Distribution.</emphasis>
<collectingCountry id="F374B5DD5568FFF3FEA0FEE5FE67FEB3" box="[364,439,312,338]" name="South Korea" pageId="3" pageNumber="63">Korea</collectingCountry>
, 
<collectingCountry id="F374B5DD5568FFF3FE0FFEE5FDD9FEB3" box="[451,521,312,338]" name="Japan" pageId="3" pageNumber="63">Japan</collectingCountry>
, 
<collectingCountry id="F374B5DD5568FFF3FDD8FEE5FDBDFEB3" box="[532,621,312,338]" name="Taiwan" pageId="3" pageNumber="63">Taiwan</collectingCountry>
, South 
<collectingCountry id="F374B5DD5568FFF3FD08FEE5FCDDFEB3" box="[708,781,312,338]" name="China" pageId="3" pageNumber="63">China</collectingCountry>
, 
<collectingCountry id="F374B5DD5568FFF3FCD5FEE5FCAFFEB3" box="[793,895,312,338]" name="Vietnam" pageId="3" pageNumber="63">Vietnam</collectingCountry>
, 
<collectingCountry id="F374B5DD5568FFF3FC46FEE5FC17FEB3" box="[906,967,312,338]" name="Laos" pageId="3" pageNumber="63">Laos</collectingCountry>
