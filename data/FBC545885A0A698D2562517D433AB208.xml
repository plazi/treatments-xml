<taxonomicName id="29B27C5B1F5F1AD9BD5084E12C79DB1D" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="2" pageNumber="275" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
