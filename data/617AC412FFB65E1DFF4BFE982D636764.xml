<quantity id="2E2BD8E1FFB65E1DFF4BFB852CF8664C" box="[151,253,1104,1130]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="9" pageNumber="572" unit="mm" value="0.8">0.80 mm</quantity>
, 
<geoCoordinate id="8CE713C3FFB65E1DFED4FB852D5F664C" box="[264,346,1104,1130]" degrees="0.92" direction="west" orientation="longitude" pageId="9" pageNumber="572" precision="555" value="-0.92">
W 
<quantity id="2E2BD8E1FFB65E1DFEF5FB852D5F664C" box="[297,346,1104,1130]" metricMagnitude="-4" metricUnit="m" metricValue="9.2" pageId="9" pageNumber="572" unit="mm" value="0.92">0.92</quantity>
