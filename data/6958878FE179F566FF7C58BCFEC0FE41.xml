<collectingCountry id="99E67609E178F563FE33593BFDB2F8A3" box="[472,545,1817,1843]" name="Brazil" pageId="3" pageNumber="556">Brazil</collectingCountry>
: 
<collectingRegion id="2335F87BE178F563FDC2593BFD0CF8A3" box="[553,671,1817,1843]" country="Brazil" name="Amazonas" pageId="3" pageNumber="556">Amazonas</collectingRegion>
(
<figureCitation id="79CA2A1CE178F563FD44593BFC91F8A3" box="[687,770,1817,1843]" captionStart="FIGURE 10" captionStartId="10.[151,250,1119,1143]" captionTargetBox="[151,1436,181,1095]" captionTargetId="figure-26@10.[151,1436,181,1095]" captionTargetPageId="10" captionText="FIGURE 10. Map of Capanema gen. nov. species geographical records." figureDoi="http://doi.org/10.5281/zenodo.5119812" httpUri="https://zenodo.org/record/5119812/files/figure.png" pageId="3" pageNumber="556">Fig. 10</figureCitation>
).
</paragraph>
</subSubSection>
