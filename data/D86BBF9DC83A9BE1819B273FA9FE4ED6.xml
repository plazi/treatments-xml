<taxonomicName id="64418F344D883E8057A23B421056E61F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:155824" class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Strumigenys gorgon Fisher" lsidName-HNS="Strumigenys gorgon Fisher" order="Hymenoptera" pageNumber="642" phylum="Arthropoda" rank="species" species="gorgon">Strumigenys gorgon Fisher</taxonomicName>
