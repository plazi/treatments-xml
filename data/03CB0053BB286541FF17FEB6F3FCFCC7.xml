<taxonomicName id="4C62CAC6BB286540FE35F90BF591F8B7" box="[437,644,1784,1807]" class="Insecta" family="Tingidae" genus="Phatnoma" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="mattijoae">
