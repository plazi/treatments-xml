<emphasis id="B959CA50CB5BFFC80E4513ACA75E9AE0" box="[311,326,1157,1182]" italics="true" pageId="30" pageNumber="31">d</emphasis>
) 
<number id="3DE50465CB5BFFC80E2413ACA76B9AE1" box="[342,371,1157,1183]" pageId="30" pageNumber="31" value="10.0">10</number>
(
<number id="3DE50465CB5BFFC80EF113ACA7DA9AE1" box="[387,450,1157,1183]" pageId="30" pageNumber="31" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
