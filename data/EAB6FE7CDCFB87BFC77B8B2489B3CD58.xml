<geoCoordinate id="5E6305F3677254E90F0B8AD1A8390D6B" direction="north" orientation="latitude" precision="5" value="45.8967">45.8967°N</geoCoordinate>
, 
<geoCoordinate id="BC318A838A4A1A7702CBD533B605BD27" direction="west" orientation="longitude" precision="5" value="-67.6344">67.6344°W</geoCoordinate>
