, 
<collectingCountry id="E49F7673FFCAFF83F5C2FE47FE7CFDD4" box="[422,477,507,531]" name="India" pageId="12" pageNumber="173">India</collectingCountry>
(this paper); 
<collectingCountry id="E49F7673FFCAFF83F60EFE47FD0DFDD4" box="[618,684,507,531]" name="Nepal" pageId="12" pageNumber="173">Nepal</collectingCountry>
, 
<collectingCountry id="E49F7673FFCAFF83F6D3FE47FCABFDD4" box="[695,778,507,531]" name="Nigeria" pageId="12" pageNumber="173">Nigeria</collectingCountry>
, 
<collectingCountry id="E49F7673FFCAFF83F770FE47FCCAFDD4" box="[788,875,507,531]" name="Senegal" pageId="12" pageNumber="173">Senegal</collectingCountry>
