<taxonomicName id="BF37B10565CC20CD3A1BF21D9618E349" authority="Champion, 1888" authorityName="Champion" authorityYear="1888" class="Insecta" family="Tenebrionidae" genus="Charisius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Charisius subalatus" order="Coleoptera" pageId="1" pageNumber="270" phylum="Arthropoda" rank="species" species="subalatus">Charisius subalatus</taxonomicName>
