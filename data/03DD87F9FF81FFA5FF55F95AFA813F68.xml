</taxonomicName>
<taxonomicNameLabel id="A2335786FF81FFA4FC30F803FBB73BC9" box="[1010,1102,1948,1973]" pageId="16" pageNumber="317" rank="species">
<emphasis id="B900EAFDFF81FFA4FC30F803FBB73BC9" bold="true" box="[1010,1102,1948,1973]" pageId="16" pageNumber="317">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="4C744D6CFF81FFA4FB9FF802FB1D3BC8" authority="Rheims &amp; Alayón, 2016" authorityName="Rheims &amp; Alayón" authorityYear="2016" box="[1117,1252,1949,1972]" class="Arachnida" family="Sparassidae" genus="Neostasina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="317" phylum="Arthropoda" rank="species" species="mammee" status="sp. nov.">
