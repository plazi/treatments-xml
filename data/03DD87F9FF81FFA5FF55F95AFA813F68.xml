<taxonomicName id="4C744D6CFF81FFA4FC90F802FC103BC9" authority="Rheims &amp; Alayón, 2016" authorityName="Rheims &amp; Alayón" authorityYear="2016" box="[850,1001,1948,1973]" class="Arachnida" family="Sparassidae" genus="Neostasina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="317" phylum="Arthropoda" rank="species" species="bermudezi" status="sp. nov.">
