<subSubSection id="FA9957826BEB7F5FF76E11DD7635479E" pageId="4" pageNumber="79" type="material examined">
<paragraph id="5CC284ADC67E26C0295BCADB00EFC1B0" pageId="4" pageNumber="79">Material examined.</paragraph>
<paragraph id="52DDC60DC3D6C81E039DAD30CB217A9B" pageId="4" pageNumber="79">
