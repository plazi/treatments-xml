<emphasis id="E5DB3C408166FFCAFF4CFBEFFECDFBB5" bold="true" box="[199,367,1113,1138]" pageId="40" pageNumber="41">Other species.</emphasis>
<taxonomicName id="10AF9BD18166FFCAFEFDFBEFFDC9FBB6" box="[374,619,1113,1138]" class="Maxillopoda" family="Cyclopidae" genus="Brevicyclops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="asetosus">
