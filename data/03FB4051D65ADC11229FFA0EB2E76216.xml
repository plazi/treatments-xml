<collectingCountry id="F345B1D7D65ADC1120CCFA59B12063E6" box="[708,804,1650,1676]" name="Gambia" pageId="29" pageNumber="30">Gambia</collectingCountry>
, 
<collectingCountry id="F345B1D7D65ADC112139FA59B17F63E6" box="[817,891,1650,1676]" name="Chad" pageId="29" pageNumber="30">Tchad</collectingCountry>
