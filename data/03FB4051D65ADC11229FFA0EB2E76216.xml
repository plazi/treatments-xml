<collectingCountry id="F345B1D7D65ADC112180FA59B1D063E6" box="[904,980,1650,1676]" name="Sudan" pageId="29" pageNumber="30">Sudan</collectingCountry>
, 
<collectingCountry id="F345B1D7D65ADC1121EAFA59B62A63E6" box="[994,1070,1650,1676]" name="Kenya" pageId="29" pageNumber="30">Kenya</collectingCountry>
