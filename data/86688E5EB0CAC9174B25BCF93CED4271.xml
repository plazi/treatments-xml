<taxonomicName id="919178D228C5C5D7D5406910C35163A0" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis nobilis" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="nobilis">Chrysis nobilis</taxonomicName>
, Kl.; 
<taxonomicName id="7026EADA9163E540AA8EBD3A6255E1F9" authority="Lepeletier &amp; Serville, 1828" authorityName="Lepeletier &amp; Serville" authorityYear="1828" class="Insecta" family="Chrysididae" genus="Pyria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pyria producta" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="producta">Pyria producta</taxonomicName>
