<taxonomicName id="413B91CCCF4711AA8897F3359C459051" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis nobilis" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="nobilis">Chrysis nobilis</taxonomicName>
