<collectionCode id="A8975993367E1045FCB50BE2D109DF77" box="[800,872,760,784]" pageId="15" pageNumber="1505">MNP</collectionCode>
); 
<specimenCount id="D8800ADF367E1045FCE80BE2D1FBDF76" box="[893,922,760,785]" pageId="15" pageNumber="1505" type="female">1♀</specimenCount>
, 
<specimenCount id="D8800ADF367E1045FC3D0BE2D679DF77" box="[936,1048,760,784]" pageId="15" pageNumber="1505" type="juvenile">1 juvenile</specimenCount>
<emphasis id="FCF21D44367E1045FB8A0BE3D650DF77" box="[1055,1073,761,784]" italics="true" pageId="15" pageNumber="1505">„</emphasis>
, 
<location id="CB59978D367E1045FBAA0BE2D334DF57" LSID="urn:lsid:plazi:treatment:462F7040367E1058FECA0826D1DADD75:CB59978D367E1045FBAA0BE2D334DF57" country="Algeria" county="Foret de Chenes" municipality="Tala Kitane" name="Foret de Pins de Bainem" pageId="15" pageNumber="1505" stateProvince="Tizi Ouzou">Forêt de Pins de Baïnem</location>
