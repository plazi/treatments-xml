<geoCoordinate id="EE0F5032B003985EF40D51E5FBA7FF24" box="[1051,1144,152,179]" degrees="66" direction="east" minutes="27" orientation="longitude" pageId="10" pageNumber="483" precision="925" value="66.45">66°27ʹE</geoCoordinate>
