77. 
<taxonomicName id="821EF90094CDEF0F4AC45DCB80CF3894" LSID="http://zoobank.org/3E578B9D-3D2B-4E3B-89A4-14A27A3B86AB" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus rhombiformis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="rhombiformis">Trigonopterus rhombiformis Riedel</taxonomicName>
