</paragraph>
<paragraph id="18093682FF8C7479FFC4F9C7FB13FDD8" blockId="24.[113,649,1578,1700]" lastBlockId="24.[676,1212,216,560]" pageId="24" pageNumber="43">
<location id="1D696059FF8C7479FFC4F9C7FF5EF9AD" LSID="urn:lsid:plazi:treatment:901F8794FF8C7479FF01FA26FB13FDD8:1D696059FF8C7479FFC4F9C7FF5EF9AD" box="[113,202,1578,1605]" county="Epines" municipality="Pronotum" name="Couleur" pageId="24" pageNumber="43">Couleur</location>
