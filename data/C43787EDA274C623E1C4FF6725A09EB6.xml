<taxonomicName id="8B9E4D78A275C625E075FCF823519F2D" authorityName="Endruweit" authorityYear="2018" box="[294,567,776,798]" class="Actinopterygii" family="Gobiidae" genus="Rhinogobius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="11" pageNumber="295" phylum="Chordata" rank="species" species="nanophyllum">
