<specimenCount id="9D29FD3DFF80FFFCA1E4B1C1D027F82A" box="[464,576,1990,2016]" pageId="11" pageNumber="3201" type="female">1 female</specimenCount>
, 
<date id="FF911074FF80FFFCA26CB1C1D0B5F82A" box="[600,722,1990,2016]" pageId="11" pageNumber="3201" value="2009-06-21">21.vi.2009</date>
