<taxonomicName id="4C794D27FFF6BE3CFE0A736FF586FAF2" authority="Taki, 1956" authorityName="Taki" authorityYear="1956" box="[505,687,1280,1297]" class="Gastropoda" family="Acteonidae" genus="Japonacteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="18" pageNumber="605" phylum="Mollusca" rank="genus">
<emphasis id="B90DEAB6FFF6BE3CFE0A736FF572FAF2" box="[505,603,1280,1297]" italics="true" pageId="18" pageNumber="605">Japonacteon</emphasis>
Taki, 
<quantity id="4C819B41FFF6BE3CFD74736FF586FAF2" box="[647,687,1280,1297]" metricMagnitude="1" metricUnit="m" metricValue="4.96824" pageId="18" pageNumber="605" unit="in" value="1956.0">1956</quantity>
</taxonomicName>
in the southwest Pacific: °, 
<taxonomicName id="4C794D27FFF6BE3CFC7C736FF38FFAF2" authority="(Reeve, 1842)" authorityName="Taki" authorityYear="1956" baseAuthorityName="Reeve" baseAuthorityYear="1842" box="[911,1190,1280,1297]" class="Gastropoda" family="Acteonidae" genus="Japonacteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="18" pageNumber="605" phylum="Mollusca" rank="species" species="sieboldii">
<emphasis id="B90DEAB6FFF6BE3CFC7C736FF31AFAF2" box="[911,1075,1280,1297]" italics="true" pageId="18" pageNumber="605">Japonacteon sieboldii</emphasis>
(Reeve, 1842)
</taxonomicName>
. Z, 
<taxonomicName id="4C794D27FFF6BE3CFB35736FF2AFFAF2" authority="Valdés, 2008" authorityName="Valdés" authorityYear="2008" box="[1222,1414,1280,1297]" class="Gastropoda" family="Acteonidae" genus="Japonacteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cephalaspidea" pageId="18" pageNumber="605" phylum="Mollusca" rank="species" species="longissimus" status="n. sp.">
<emphasis id="B90DEAB6FFF6BE3CFB35736FF2AFFAF2" box="[1222,1414,1280,1297]" italics="true" pageId="18" pageNumber="605">Japonacteon longissimus</emphasis>
