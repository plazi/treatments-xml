<number id="2E4224ACFFD5CB60A78C47A6FAD865BE" box="[1248,1300,1731,1756]" pageId="10" pageNumber="411" value="0.06">0.06</number>
, 
<number id="2E4224ACFFD5CB60A64E47A1FA9A65BE" box="[1314,1366,1732,1756]" pageId="10" pageNumber="411" value="0.34">0.34</number>
