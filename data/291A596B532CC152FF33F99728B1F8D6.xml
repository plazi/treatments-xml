and the 
<collectingCountry id="D9A4A8ED532BC152FDA2F8E829E7F8D6" box="[518,725,1844,1869]" name="South Korea" pageId="12" pageNumber="206">Republic of Korea</collectingCountry>
(
<collectingCountry id="D9A4A8ED532BC152FD40F8E8284AF8D6" box="[740,888,1844,1869]" name="South Korea" pageId="12" pageNumber="206">South Korea</collectingCountry>
