<quantity id="5A261FAD80F7FF54E964CBF51D390B88" box="[1039,1124,1500,1524]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="2.0" metricValueMin="0.1" pageId="243" pageNumber="244" unit="m" value="10.5" valueMax="20.0" valueMin="1.0">1–20 m</quantity>
