<taxonomicName id="4C181FE1FFA2F119CD4428A4FD33FAD4" box="[341,662,1926,1949]" class="Arachnida" family="Arrenuridae" genus="Arrenurus" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="pseudomargatellus" status="n. sp.">
<emphasis id="B96CB870FFA2F119CD4428A4FD33FAD4" box="[341,662,1926,1949]" italics="true" pageId="12" pageNumber="13">
