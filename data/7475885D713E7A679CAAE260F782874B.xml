<taxonomicNameLabel id="D59B5822713D7A6799F1EB80FE8C8E89" box="[233,304,434,475]" pageId="6" pageNumber="13" rank="species">n.sp.</taxonomicNameLabel>
, G. 
<taxonomicName id="3BDC42C8713D7A679882EB80FC908E89" authority="(Dahl, 1959)" baseAuthorityName="Dahl" baseAuthorityYear="1959" box="[410,812,434,475]" class="Malacostraca" family="Uristidae" genus="Schisturella" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="13" phylum="Arthropoda" rank="species" species="galatheae">
<emphasis id="CEA8E559713D7A679882EB80FD838E89" box="[410,575,434,475]" italics="true" pageId="6" pageNumber="13">galatheae</emphasis>
