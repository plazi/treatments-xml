<taxonomicName id="3BDC42C8713D7A679D34EBB7FB008EFE" authority="Lowry &amp; Stoddart, 1995" authorityName="Lowry &amp; Stoddart" authorityYear="1995" box="[1068,1212,389,428]" class="Malacostraca" family="Uristidae" genus="Galathella" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="13" phylum="Arthropoda" rank="species" species="bassiana" status="n.sp.">
