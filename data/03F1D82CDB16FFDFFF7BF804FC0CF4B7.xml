<emphasis id="B92CB528DB17FFDCFF2BFB97FE7AF190" bold="true" box="[199,403,1927,1950]" pageId="1" pageNumber="599">Male macroptera.</emphasis>
Body length 
<quantity id="4CA0C4DFDB17FFDCFDCFFB97FD4FF190" box="[547,678,1927,1950]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" metricValueMax="1.2" metricValueMin="1.1" pageId="1" pageNumber="599" unit="mm" value="1.15" valueMax="1.2" valueMin="1.1">1.1–1.2 mm</quantity>
