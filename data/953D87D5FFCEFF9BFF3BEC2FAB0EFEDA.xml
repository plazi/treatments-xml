<taxonomicName id="DA944D40FFCFFF99FD9EE870ADFCFEE2" box="[562,665,306,331]" class="Eurotatoria" family="Testudinellidae" genus="Testudinella" kingdom="Animalia" order="Flosculariaceae" pageId="5" pageNumber="6" phylum="Rotifera" rank="species" species="elongata">
