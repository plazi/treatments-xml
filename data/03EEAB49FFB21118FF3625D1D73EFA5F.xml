and 
<taxonomicName id="4C4761DCFFBC1118FF362360D4F5FAE6" box="[151,247,1381,1404]" class="Insecta" family="Sciaridae" genus="Dolichosciara" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="352" phylum="Arthropoda" rank="species" species="ninae">
