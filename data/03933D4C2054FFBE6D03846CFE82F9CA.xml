<quantity id="4CC221BF2054FFBE6D22824DFD10FBDA" box="[607,714,1086,1112]" metricMagnitude="-4" metricUnit="m" metricValue="2.9" pageId="12" pageNumber="13" unit="mm" value="0.29">0.29 mm</quantity>
