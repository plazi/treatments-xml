<quantity id="4CC221BF2054FFBE6DEC8585FD27FB93" box="[657,765,1014,1041]" metricMagnitude="-3" metricUnit="m" metricValue="1.32" pageId="12" pageNumber="13" unit="mm" value="1.32">1.32 mm</quantity>
