<normalizedToken id="F8A0B5C3E903691EAAC9EE9CC3B75596" originalValue="Blütenstand" pageId="null" pageNumber="807">Bluetenstand</normalizedToken>
<normalizedToken id="C6C4920511765FE3A727C3F3C2D51B6C" originalValue="ähnlich" pageId="null" pageNumber="807">aehnlich</normalizedToken>
wie die obersten 
<normalizedToken id="7DA75FD74FE9934DE4D74B9309AE88A4" originalValue="Stengelblätter" pageId="null" pageNumber="807">Stengelblaetter</normalizedToken>
