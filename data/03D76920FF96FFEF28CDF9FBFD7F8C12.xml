<bibRefCitation id="EFEFA5C7FF96FFEE2C2CF89DFAA38A69" author="Swanton" box="[1142,1336,1826,1852]" pageId="8" pageNumber="23" refString="Swanton, E. W. (1912) British Plant Galls. Methuen &amp; Co. Ltd., London. 287 pp." type="book" year="1912">
Swanton (
<number id="3DB6CA11FF96FFEE2CAAF89DFAB48A69" box="[1264,1327,1826,1852]" pageId="8" pageNumber="23" value="1912.0">1912</number>
)
</bibRefCitation>
from 
<taxonomicName id="4C7EA3B5FF96FFEE2DD8F89DFE5B8A34" authority="Wheine &amp; Nees" authorityName="Wheine &amp; Nees" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="8" pageNumber="23" phylum="Tracheophyta" rank="species" species="plicatus">
