<subSubSection id="2DFA3298DB1AFF9D9AD7FBDFFDEFFBFE" box="[151,520,1111,1137]" pageId="5" pageNumber="22" type="nomenclature">
<paragraph id="655F6113DB1AFF9D9AD7FBDFFDEFFBFE" blockId="5.[151,520,1111,1137]" box="[151,520,1111,1137]" pageId="5" pageNumber="22">
<heading id="3E17D67FDB1AFF9D9AD7FBDFFDEFFBFE" bold="true" box="[151,520,1111,1137]" fontSize="11" level="1" pageId="5" pageNumber="22" reason="1">
<emphasis id="5794BD01DB1AFF9D9AD7FBDFFDEFFBFE" bold="true" box="[151,520,1111,1137]" pageId="5" pageNumber="22">
Genus 
<taxonomicName id="A2E01A90DB1AFF9D9AAEFBDFFE74FBFE" box="[238,403,1111,1137]" class="Malacostraca" family="Paratanaidae" genus="Triparatanais" kingdom="Animalia" order="Tanaidacea" pageId="5" pageNumber="22" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="5794BD01DB1AFF9D9AAEFBDFFE74FBFE" bold="true" box="[238,403,1111,1137]" italics="true" pageId="5" pageNumber="22">Triparatanais</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4CA7007ADB1AFF9D9BDAFBDFFDEFFBFE" box="[410,520,1111,1137]" pageId="5" pageNumber="22" rank="genus">gen. nov.</taxonomicNameLabel>
