<subSubSection id="48EB653FFFCC8A2AF578F94EF400D130" pageId="3" pageNumber="282" type="discussion">
<paragraph id="004E36B4FFCC8A2AF578F94EF400D130" blockId="3.[2737,3539,1616,2184]" pageId="3" pageNumber="282">
<taxonomicName id="C7F14D37FFCC8A2AF578F94EF3C8DE75" authorityName="Eisentraut" authorityYear="1960" box="[2740,3149,1775,1814]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="3" pageNumber="282" phylum="Chordata" rank="species" species="guineensis">
