<taxonomicNameLabel id="A27957B55923FF80FB34F92A3753CAC3" box="[1266,1369,1665,1691]" pageId="79" pageNumber="80" rank="species">n. comb.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C3E4D5F5923FF80FAAEF92933A2CAE7" authority="Sanborn, 2019" authorityName="Sanborn" authorityYear="2019" class="Insecta" family="Cicadidae" genus="Herrera" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="melanomesocranon" status="n. sp.">
