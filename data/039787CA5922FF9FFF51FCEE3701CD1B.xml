<emphasis id="B94AEACE5923FF80FBDEF8D536B0CBCF" box="[1048,1210,1918,1943]" italics="true" pageId="79" pageNumber="80">H. coyamensis</emphasis>
<bibRefCitation id="EFAF4B2D5923FF80FB79F8D63763CBCF" author="Sanborn, A. F." box="[1215,1385,1917,1943]" pageId="79" pageNumber="80" pagination="1 - 42" refId="ref90120" refString="Sanborn, A. F. (2007 a) New species, new records and checklist of cicadas from Mexico (Hemiptera: Cicadoidea: Cicadidae). Zootaxa, 1651, 1 - 42. https: // doi. org / 10.11646 / zootaxa. 1651.1. 1" type="journal article" year="2007">Sanborn 2007a</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="4C3E4D5F5923FF80FF51F809305BCBE3" authority="Sanborn &amp; Heath, 2014" authorityName="Sanborn &amp; Heath" authorityYear="2014" box="[151,593,1953,1979]" class="Insecta" family="Cicadidae" genus="Herrera" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="umbraphila">
