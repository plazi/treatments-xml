</emphasis>
and 
<taxonomicName id="4C3E4D5F5923FF80FDE5F90D30BACAE7" authority="Sanborn, 2019" authorityName="Sanborn" authorityYear="2019" box="[547,688,1701,1727]" class="Insecta" family="Cicadidae" genus="Herrera" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="phyllodes" status="n. sp.">
