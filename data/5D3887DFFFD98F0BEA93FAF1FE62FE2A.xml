<taxonomicName id="12914D4AFFDB8F0BEBF4FEB6FEE5FEED" authorityName="Schmidt &amp; Polaszek" authorityYear="2007" box="[138,315,347,371]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="82" pageNumber="2179" phylum="Arthropoda" rank="species" species="leptosa">
