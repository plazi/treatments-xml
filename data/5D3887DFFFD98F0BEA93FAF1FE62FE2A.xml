<taxonomicName id="12914D4AFFDB8F0BEAA2FEB1FDE3FEED" box="[476,573,347,371]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="82" pageNumber="2179" phylum="Arthropoda" rank="species" species="pilosa">
