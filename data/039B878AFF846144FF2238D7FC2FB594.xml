<emphasis id="B946EA8EFF846145FF2238D7FE87B2AB" bold="true" box="[151,379,902,928]" pageId="4" pageNumber="458">
<taxonomicName id="4C324D1FFF846145FF2238D7FF17B2AB" box="[151,235,902,928]" class="Malacostraca" family="Leptocheliidae" genus="Ogleus" kingdom="Animalia" order="Tanaidacea" pageId="4" pageNumber="458" phylum="Arthropoda" rank="genus" status="new genus">
