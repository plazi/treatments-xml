<emphasis id="0FF5396BFF96FFA5FF471CDEFEC1FBE3" bold="true" box="[203,279,1021,1048]" pageId="24" pageNumber="225">
<collectingRegion id="FF452B9BFF96FFA5FF471CDEFEC4FBE3" box="[203,274,1021,1048]" country="Panama" name="Cocle" pageId="24" pageNumber="225">Coclé</collectingRegion>
