<paragraph id="595D36CB0007FF80FCF8F914FAC7F880" blockId="6.[829,1513,1732,1815]" pageId="6" pageNumber="6">
<taxonomicName id="9EE24D480007FF80FCF8F914FC7DF96E" authorityName="Mather, Lee, Camens and Worthy" box="[829,964,1762,1785]" class="Aves" family="Falconidae" genus="Archaehierax" kingdom="Animalia" order="Falconiformes" pageId="6" pageNumber="6" phylum="Chordata" rank="genus">
<emphasis id="6B96EAD90007FF80FCF8F914FC7DF96E" box="[829,964,1762,1785]" italics="true" pageId="6" pageNumber="6">Archaehierax</emphasis>
</taxonomicName>
