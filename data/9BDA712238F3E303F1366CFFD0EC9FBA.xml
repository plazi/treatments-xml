<taxonomicName id="B7C4CD5B146368FC560C31FD530A6F8D" LSID-ZBK="urn:lsid:zoobank.org:act:3F1F6AF2-6F38-42C0-9C82-C1468F19FAF0" class="Actinopterygii" family="Ariidae" genus="Hemiarius" kingdom="Animalia" lsidName-ZBK="Hemiarius Bleeker 1862:7, 29" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Hemiarius</taxonomicName>
, 
<taxonomicName id="6C1AECA4B6181DF86039B50383FF30FE" LSID-ZBK="urn:lsid:zoobank.org:act:B824B602-4F87-4771-B334-61385645D6C4" class="Actinopterygii" family="Ariidae" genus="Ketengus" kingdom="Animalia" lsidName-ZBK="Ketengus Bleeker 1847:167" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Ketengus</taxonomicName>
, 
<taxonomicName id="7E705EF79D98CB2F4F2747CAA666F223" LSID-ZBK="urn:lsid:zoobank.org:act:086E7B0F-D030-44BD-9DCC-8CAEB178CBAA" class="Actinopterygii" family="Ariidae" genus="Notarius" kingdom="Animalia" lsidName-ZBK="Notarius Gill 1863:171" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Notarius</taxonomicName>
