<taxonomicName id="7C0728CF14046928658886F345DF9DB9" LSID-ZBK="urn:lsid:zoobank.org:act:FB0B3553-B6DA-48F8-BA0A-EE430A96E0DB" class="Actinopterygii" family="Ariidae" genus="Arius" kingdom="Animalia" lsidName-ZBK="Arius madagascariensis Vaillant 1894:77 [10]" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="species" species="madagascariensis">A. madagascariensis</taxonomicName>
