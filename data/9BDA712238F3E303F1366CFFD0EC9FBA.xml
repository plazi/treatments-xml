<taxonomicName id="40EFCF6A87BEF87677B3437FADA84C24" class="Actinopterygii" family="Ariidae" genus="Netuma" kingdom="Animalia" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Netuma</taxonomicName>
, 
<taxonomicName id="9CD8C612D30A2821114C8D01AA55D21B" LSID-ZBK="urn:lsid:zoobank.org:act:1AE6B6D0-01A8-43E7-9505-E6855D1FC99A" class="Actinopterygii" family="Ariidae" genus="Plicofollis" kingdom="Animalia" lsidName-ZBK="Plicofollis Kailola 2004:141" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Plicofollis</taxonomicName>
and 
<taxonomicName id="CAC212B078A668B804A00E0CB7B7F21B" class="Actinopterygii" family="Ariidae" genus="Potamosilurus" kingdom="Animalia" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Potamosilurus</taxonomicName>
