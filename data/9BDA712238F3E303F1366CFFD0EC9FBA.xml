<taxonomicName id="9DA14655AF61C5FA03864E36DAA8567D" LSID-ZBK="urn:lsid:zoobank.org:act:5AD62E91-395D-4C95-8E2D-515D1D752271" authority="Bleeker, 1857" authorityName="Bleeker" authorityYear="1857" class="Teleostei" family="Ariidae" genus="Hemipimelodus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-ZBK="Hemipimelodus dayi Ramsay &amp; Ogilby 1886:16" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="species" species="dayi">Hemipimelodus dayi</taxonomicName>
