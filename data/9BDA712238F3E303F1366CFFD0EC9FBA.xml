<taxonomicName id="E915A4FF4AC4B49AE7DEE3F3BBB6EEDF" LSID-ZBK="urn:lsid:zoobank.org:act:A8968D59-B441-4D26-BC5F-B90032C1F1D1" class="Actinopterygii" family="Ariidae" genus="Batrachocephalus" kingdom="Animalia" lsidName-ZBK="Batrachocephalus Bleeker 1846:176" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Batrachocephalus</taxonomicName>
, 
<taxonomicName id="6A05E8BA63F5AA8BC9E47F32BCA3EBBF" class="Actinopterygii" family="Ariidae" genus="Cathorops" kingdom="Animalia" order="Siluriformes" pageNumber="73" phylum="Chordata" rank="genus">Cathorops</taxonomicName>
