<taxonomicName id="4C1B4D374206FFCFFEA8FAA4FCADFAE6" authority="Malloch, 1912: 514" authorityName="Malloch" authorityPageNumber="514" authorityYear="1912" box="[313,779,1365,1388]" class="Insecta" family="Phoridae" genus="Aphiochaeta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="82" pageNumber="586" phylum="Arthropoda" rank="species" species="tasmaniensis">
<emphasis id="B96FEAA64206FFCFFEA8FAA4FD9CFAE1" box="[313,570,1366,1387]" italics="true" pageId="82" pageNumber="586">Aphiochaeta tasmaniensis</emphasis>
