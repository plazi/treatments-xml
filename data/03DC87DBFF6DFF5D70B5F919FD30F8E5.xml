<taxonomicName id="4C754D4EFF6EFF5D750DFE0DFA39FBCA" authorityName="Leach" authorityYear="1820" box="[1327,1432,404,428]" class="Aves" family="Corvidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Corvidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D70B5FE2EFE86FBA9" box="[151,295,439,463]" class="Aves" family="Emberizidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Emberizidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D7115FE2EFE1CFBA9" authorityName="Leach" authorityYear="1820" box="[311,445,439,463]" class="Aves" family="Fringillidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Fringillidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D71EFFE2EFD94FBA9" authorityName="Bonaparte" authorityYear="1853" box="[461,565,439,463]" class="Aves" family="Mimidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Mimidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D7264FE2EFD44FBA9" authorityName="Fleming" authorityYear="1822" box="[582,741,439,463]" class="Aves" family="Muscicapidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Muscicapidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D72D7FE2EFC38FBA9" authorityName="Baird" authorityYear="1858" box="[757,921,439,463]" class="Aves" family="Ptilogonatidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Ptilogonatidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D7388FE2EFB8DFBA9" authorityName="Richmond" authorityYear="1908" box="[938,1068,439,463]" class="Aves" family="Prunellidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Prunellidae</taxonomicName>
