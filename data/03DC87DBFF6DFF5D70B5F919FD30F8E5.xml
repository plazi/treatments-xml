, 
<taxonomicName id="4C754D4EFF6EFF5D74B0FE0DFA85FBCA" authorityName="Ridgway" authorityYear="1901" box="[1170,1316,404,428]" class="Aves" family="Cardinalidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Cardinalidae</taxonomicName>
