<taxonomicName id="4C754D4EFF6EFF5D741EFE2EFB76FBA9" authorityName="G. R. Gray" authorityYear="1840" box="[1084,1239,439,463]" class="Aves" family="Pycnonotidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Pycnonotidae</taxonomicName>
, 
<taxonomicName id="4C754D4EFF6EFF5D74CAFE2EFAC6FBA9" authorityName="Cabanis" authorityYear="1847" box="[1256,1383,439,463]" class="Aves" family="Thraupidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Thraupidae</taxonomicName>
and 
<taxonomicName id="4C754D4EFF6EFF5D70B5FE45FF5AFB92" authorityName="Rafinesque" authorityYear="1815" box="[151,251,476,500]" class="Aves" family="Turdidae" kingdom="Animalia" order="Passeriformes" pageId="226" pageNumber="227" phylum="Chordata" rank="family">Turdidae</taxonomicName>
(
<tableCitation id="C6F70376FF6EFF5D712BFE45FECEFB92" box="[265,367,476,500]" captionStart="TABLE 14" captionStartId="226.[151,231,720,741]" captionText="TABLE 14. Distribution of Torotrogla spp. on hosts" pageId="226" pageNumber="227">Table 14</tableCitation>
