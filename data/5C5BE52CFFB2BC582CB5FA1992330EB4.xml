<taxonomicName id="13F22FB9FFB2BC582CCCFA1992BB0E05" box="[238,399,1520,1546]" class="Insecta" family="Scirtidae" genus="Contacyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="486" phylum="Arthropoda" rank="genus">
