<taxonomicName id="4C364D1DFFFEFFD6A9A9FB31101F4484" box="[189,298,1218,1244]" class="Pinopsida" family="Pinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
