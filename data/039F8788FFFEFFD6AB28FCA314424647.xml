<taxonomicName id="4C364D1DFFFEFFD6A8C6FB3113194484" box="[466,556,1218,1244]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="sibirica">
