<taxonomicName id="4C364D1DFFFEFFD6AB2EFB3113BA4484" box="[570,655,1218,1244]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="strobus">
