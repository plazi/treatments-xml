<taxonomicName id="A5E04D707A6422217EA0F9946056F9AD" authority=", Lavauden (1929)" authorityName=", Lavauden" authorityYear="1929" class="Aves" family="Psittacidae" genus="Coracopsis" kingdom="Animalia" order="Psittaciformes" pageId="16" pageNumber="479" phylum="Chordata" rank="subSpecies" species="vasa" subSpecies="drouhardi">
