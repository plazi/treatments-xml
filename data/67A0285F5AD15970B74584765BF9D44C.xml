<normalizedToken id="AA7CABF7128F3D43CFABBF5D4B7CCF46" originalValue="“var.”">&quot;var.&quot;</normalizedToken>
of 
<taxonomicName id="030C4D6A734D2C88F8BB69A496D37171" class="Magnoliopsida" family="Rubiaceae" genus="Danais" kingdom="Plantae" lsidName="Danais gilippus" order="Gentianales" pageId="0" pageNumber="193" phylum="Tracheophyta" rank="species" species="gilippus">
