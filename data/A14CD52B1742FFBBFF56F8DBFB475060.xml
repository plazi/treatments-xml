<taxonomicName id="EEE51FBE174CFFBBFE16FE5EFDDF53CB" box="[471,587,476,500]" class="Insecta" family="Aclopidae" genus="Gracilaclopus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="bidentulus">
