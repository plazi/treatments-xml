<taxonomicName id="5EAA4295FFF9FF88FCD335E269E70DFD" box="[773,938,225,248]" class="Magnoliopsida" family="Moraceae" genus="Parartocarpus" kingdom="Plantae" order="Rosales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
<emphasis id="ABDEE504FFF9FF88FCD335E269E70DFD" box="[773,938,225,248]" italics="true" pageId="22" pageNumber="23">Parartocarpus</emphasis>
