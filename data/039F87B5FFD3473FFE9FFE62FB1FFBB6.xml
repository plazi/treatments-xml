<taxonomicName id="4C364D20FFD3473FFEEFFE6FFDA6FE64" authority="Crotch" authorityName="Crotch" box="[352,620,439,463]" class="Insecta" family="Curculionidae" genus="Asynonychus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="122" phylum="Arthropoda" rank="genus">
<emphasis id="B942EAB1FFD3473FFEEFFE6FFDD7FE65" bold="true" box="[352,541,439,462]" italics="true" pageId="23" pageNumber="122">ASYNONYCHUS</emphasis>
