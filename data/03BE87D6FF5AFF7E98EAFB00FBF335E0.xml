<quantity id="4CEF9B25FF5AFF7E9D77FA70FF043598" box="[1290,1349,1410,1437]" metricMagnitude="1" metricUnit="m" metricValue="4.5" pageId="203" pageNumber="204" unit="m" value="45.0">45 m</quantity>
