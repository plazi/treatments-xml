<quantity id="4CEF9B25FF5AFF7E9D2AFA71FA9935C7" metricMagnitude="1" metricUnit="m" metricValue="2.27076" metricValueMax="4.5110399999999995" metricValueMin="0.03048" pageId="203" pageNumber="204" unit="ft" value="74.5" valueMax="148.0" valueMin="1.0">1–148 ft</quantity>
