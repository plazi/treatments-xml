<quantity id="4C8873DAFFBEFF9BFEBAFF5EFE080EEA" box="[336,475,226,248]" metricMagnitude="3" metricUnit="m" metricValue="1.2" metricValueMax="1.8" metricValueMin="0.6" pageId="27" pageNumber="472" unit="m" value="1200.0" valueMax="1800.0" valueMin="600.0">600–1800 m</quantity>
(
<bibRefCitation id="EFE1A3CEFFBEFF9BFE07FF5EFDAF0EEA" author="Chace FA Jr." box="[493,636,226,248]" pageId="27" pageNumber="472" pagination="117 - 209" refId="ref62609" refString="Chace FA Jr. 1940. Plankton of the Bermuda Oceanographic Expeditions, IX. The bathypelagic caridean Crustacea. Zoologica 25: 117 - 209." type="journal article" year="1940">Chace, 1940</bibRefCitation>
