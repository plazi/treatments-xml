<paragraph id="43F26A3483C71DC654D856F79AE41CBF" pageId="null" pageNumber="250">
<taxonomicName id="B5DB5B98301E40882491AD4D80E2A6D8" authority="Squamarla L." class="Magnoliopsida" family="Orobanchaceae" genus="Lathraea" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="250" phylum="Tracheophyta" rank="genus">
