<taxonomicName id="8FEE4D41FF63FF48FBBDFF306058CBF7" authorityName="Warburton" authorityYear="1933" box="[1069,1307,151,176]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="138" pageNumber="139" phylum="Arthropoda" rank="species" species="imitans">
