<taxonomicName id="4C2F0DB1E266FFB8228FC159FDBFA639" authority="Johnston and Tiegs, 1921" authorityName="Johnston and Tiegs" authorityYear="1921" box="[151,603,1320,1342]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="161" phylum="Arthropoda" rank="species" species="sigma">
