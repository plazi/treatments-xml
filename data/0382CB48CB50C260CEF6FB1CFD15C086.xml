<collectingRegion id="49EFB4BCCB50C261CD5EF849FC13C698" box="[831,923,1954,1980]" country="Mexico" name="Sinaloa" pageId="76" pageNumber="77">Sinaloa</collectingRegion>
, 
<collectingRegion id="49EFB4BCCB50C261CDC6F849FC77C698" box="[935,1023,1954,1980]" country="Mexico" name="Sonora" pageId="76" pageNumber="77">Sonora</collectingRegion>
