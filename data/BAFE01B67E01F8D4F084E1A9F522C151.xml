<normalizedToken id="ADC245897B746F21F7AB5DC25254EC33" originalValue="darüberliegenden" pageId="null" pageNumber="299">darueberliegenden</normalizedToken>
Internodien; 
<normalizedToken id="51C7EF56CE4266D08BD3CE95AC531A74" originalValue="Blätter" pageId="null" pageNumber="299">Blaetter</normalizedToken>
bis 2 mm breit, 
<normalizedToken id="A1D46E62939D5DB31301F8D567B841BA" originalValue="±" pageId="null" pageNumber="299">+/-</normalizedToken>
