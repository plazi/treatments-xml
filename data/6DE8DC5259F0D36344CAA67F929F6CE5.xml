<taxonomicName id="0010BDABB4C72F8E2BBAA9911BAC0356" authority="Neumayr, 1880" authorityName="Neumayr" authorityYear="1880" class="Gastropoda" family="Melanopsidae" genus="Melanoptychia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Melanoptychia" order="Sorbeoconcha" pageId="45" pageNumber="46" phylum="Mollusca" rank="genus">
