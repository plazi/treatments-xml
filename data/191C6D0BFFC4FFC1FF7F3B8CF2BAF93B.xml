<taxonomicName id="56B5A79EFFC0FFCFFE8D3AB3F479F84F" box="[357,585,1976,1999]" class="Insecta" family="Staphylinidae" genus="Osakatheta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="45" phylum="Arthropoda" rank="species" species="yasukoae">
<emphasis id="A3C1000FFFC0FFCFFE8D3AB3F479F84F" box="[357,585,1976,1999]" italics="true" pageId="6" pageNumber="45">Osakatheta yasukoae</emphasis>
