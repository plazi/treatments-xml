<caption id="C5CA8C95FFC0FFCFFF7F3969F2C9FB1C" httpUri="https://zenodo.org/record/180439/files/figure.png" pageId="6" pageNumber="45" targetBox="[226,1360,182,1098]" targetPageId="6">
