<caption id="C5CA8C95FFC1FFCEFF7F3A58F2CAF830" httpUri="https://zenodo.org/record/180441/files/figure.png" pageId="7" pageNumber="46" targetBox="[155,1424,194,1846]" targetPageId="7">
