<taxonomicName id="56B5A79EFFC0FFCFFE8C3968F477FBFA" box="[356,583,1123,1146]" class="Insecta" family="Staphylinidae" genus="Osakatheta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="45" phylum="Arthropoda" rank="species" species="yasukoae">
<emphasis id="A3C1000FFFC0FFCFFE8C3968F477FBFA" box="[356,583,1123,1146]" italics="true" pageId="6" pageNumber="45">Osakatheta yasukoae</emphasis>
</taxonomicName>
