<paragraph id="28481D3C957E169AADE9DE3B710EE9A9" pageId="0" pageNumber="535">
<taxonomicName id="CDC93570BB933DEC3A25FD1498B1ED71" class="Insecta" family="Dytiscidae" genus="Hydroporus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydroporus nodieri" order="Coleoptera" pageId="0" pageNumber="535" phylum="Arthropoda" rank="species" species="nodieri">
<emphasis id="E31C5DD7FB0DEA829CC5CC87BE9E632B" italics="true" pageId="0" pageNumber="535">Hydroporus nodieri</emphasis>
</taxonomicName>
<normalizedToken id="5E38A62DEF909AFDFD28688F956F038F" originalValue="Régimbart">Regimbart</normalizedToken>
