<taxonomicName id="4C3D07E8FFC5BA0583024B52FDE1B2E6" authority="Degner, 1934" authorityName="Degner" authorityYear="1934" box="[287,529,625,642]" class="Gastropoda" family="Streptaxidae" genus="Gulella" kingdom="Animalia" order="Stylommatophora" pageId="21" pageNumber="204" phylum="Mollusca" rank="species" species="lambda">
