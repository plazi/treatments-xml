<collectingCountry id="3D8776182B5A4936FDEA62A55116FD5B" box="[550,598,650,676]" name="Iran" pageId="49" pageNumber="50">Iran</collectingCountry>
, 
<collectingCountry id="3D8776182B5A4936FDAD62A551F6FD5B" box="[609,694,650,676]" name="Kuwait" pageId="49" pageNumber="50">Kuwait</collectingCountry>
, 
<collectingCountry id="3D8776182B5A4936FD0E62A55067FD5B" box="[706,807,650,676]" name="Lebanon" pageId="49" pageNumber="50">Lebanon</collectingCountry>
