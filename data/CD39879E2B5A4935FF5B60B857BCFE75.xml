<collectingCountry id="3D8776182B5A4936FCDD626D5023FDA3" box="[785,867,578,604]" name="Yemen" pageId="49" pageNumber="50">Yemen</collectingCountry>
; Palaearctic Region: 
<collectingCountry id="3D8776182B5A4936FB9D626D579DFDA3" box="[1105,1245,578,604]" name="Afghanistan" pageId="49" pageNumber="50">Afghanistan</collectingCountry>
, 
<collectingCountry id="3D8776182B5A4936FB2B626D567FFDA3" box="[1255,1343,578,604]" name="Algeria" pageId="49" pageNumber="50">Algeria</collectingCountry>
