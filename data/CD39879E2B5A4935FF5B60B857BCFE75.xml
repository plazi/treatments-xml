<collectingCountry id="3D8776182B5A4936FA9E624956D8FD7F" box="[1362,1432,614,640]" name="China" pageId="49" pageNumber="50">China</collectingCountry>
, 
<collectingCountry id="3D8776182B5A4936FF5B62A5539DFD5B" box="[151,221,650,676]" name="Egypt" pageId="49" pageNumber="50">Egypt</collectingCountry>
