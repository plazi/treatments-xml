<taxonomicName id="4C06A6F6FFD7FFCA6FF78B42A1DCF8A9" authority="Hsueh, 2020" authorityName="Hsueh" authorityYear="2020" box="[234,390,1839,1862]" class="Polychaeta" family="Nereididae" genus="Nereis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="12" pageNumber="13" phylum="Annelida" rank="species" species="paporae" status="n. sp.">
<emphasis id="B9720167FFD7FFCA6FF78B42A1DCF8A9" box="[234,390,1839,1862]" italics="true" pageId="12" pageNumber="13">Nereis paporae</emphasis>
</taxonomicName>
