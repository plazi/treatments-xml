<paragraph id="B6AF5EB1FAFA7B2268686743235AA1B8" pageId="212" pageNumber="213">
<taxonomicName id="313C71C33F81A6A56CDBDA708F3B399B" class="Hexapoda" family="Carabidae" genus="Cicindela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="species" species="gratiosa">Cicindela gratiosa</taxonomicName>
<normalizedToken id="4A1BD9450E1B825923671F2492A622B3" originalValue="Guérin-Méneville">Guerin-Meneville</normalizedToken>
