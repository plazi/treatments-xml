<elevation id="0057D18BC251FFF3F21DFD5DFC2A56A4" box="[874,949,689,713]" metricMagnitude="2" metricUnit="m" metricValue="6.4" pageId="3" pageNumber="255" unit="m" value="640.0">640 m</elevation>
