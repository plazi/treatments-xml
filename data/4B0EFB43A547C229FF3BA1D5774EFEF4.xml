<taxonomicName id="04A731D6A544C22EFEB4A0237789FA4D" authority="Hiwatari, 2003" authorityName="Hiwatari" authorityYear="2003" box="[280,710,1392,1414]" class="Malacostraca" family="Hyalidae" genus="Protohyale" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="242" phylum="Arthropoda" rank="species" species="kajiharai" subGenus="Boreohyale">
<emphasis id="F1D39647A544C22EFEB4A02374C7FA4D" box="[280,392,1393,1414]" italics="true" pageId="11" pageNumber="242">Protohyale</emphasis>
