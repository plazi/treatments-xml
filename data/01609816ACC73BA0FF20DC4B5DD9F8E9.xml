<emphasis id="BBBDF512ACC73BA0FC96DC8F5FC1FB7F" box="[839,898,1226,1246]" italics="true" pageId="139" pageNumber="532">thoides</emphasis>
<taxonomicName id="4EC95283ACC73BA0FC5BDC8F5857FB7F" authority="Girard" authorityName="Girard" box="[906,1044,1226,1246]" class="Insecta" family="Elateridae" genus="Melanthoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="139" pageNumber="532" phylum="Arthropoda" rank="species" species="lamottei">
