<td id="5699275460A305FBB5F836671F7AF770" align="left" pageId="null" pageNumber="213" valign="top">
2. Innere 
<normalizedToken id="6A0F1643E1A6DB5631253BB6A0BC165B" originalValue="Perigonblätter" pageId="null" pageNumber="213">Perigonblaetter</normalizedToken>
am Grunde rot; 
