<geoCoordinate id="EE60506BFF8B2E4C6777FC74D115C9D1" box="[799,858,973,998]" degrees="69" direction="east" orientation="longitude" pageId="103" pageNumber="104" precision="55555" value="69.0">69°E</geoCoordinate>
