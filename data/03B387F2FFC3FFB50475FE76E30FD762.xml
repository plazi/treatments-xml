</taxonomicName>
on dead 
<taxonomicName id="4C1A4D67FFC3FFB504E4F8F0E340D941" box="[1742,1900,1869,1907]" class="Anthozoa" family="Acroporidae" genus="Acropora" kingdom="Animalia" order="Scleractinia" pageId="21" pageNumber="208" phylum="Cnidaria" rank="genus">
