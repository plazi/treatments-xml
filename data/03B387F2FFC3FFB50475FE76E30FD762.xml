<taxonomicName id="4C1A4D67FFC3FFB507B5F8F0E201D941" box="[1439,1581,1869,1907]" class="Phaeophyceae" family="Dictyotaceae" genus="Dictyota" higherTaxonomySource="CoL" kingdom="Animalia" order="Dictyotales" pageId="21" pageNumber="208" phylum="Ochrophyta" rank="genus">
