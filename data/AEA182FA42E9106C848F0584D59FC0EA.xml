<taxonomicName id="36A46F57536D46268D9DA9A2BEE5080B" class="Arachnida" family="Nesticidae" genus="Nesticus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nesticus murgis" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="murgis">Nesticus murgis</taxonomicName>
