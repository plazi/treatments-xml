<number id="ED7E2417FFCBEF7BC0DDFB20FB7DFB3C" box="[1215,1246,1158,1182]" pageId="8" pageNumber="9" value="12.0">12</number>
), 
<number id="ED7E2417FFCBEF7BC091FB20FAACFB3C" box="[1267,1295,1158,1182]" pageId="8" pageNumber="9" value="17.0">17</number>
