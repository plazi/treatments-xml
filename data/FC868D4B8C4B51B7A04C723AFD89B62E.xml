, 
<geoCoordinate id="A84E0D41C459BCE313EC8EE35E37B434" degrees="29" direction="south" minutes="40" orientation="latitude" precision="1" seconds="10.5" value="-29.669582">29°40'10.5&quot;S</geoCoordinate>
