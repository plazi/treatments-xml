, hard bottom, 
<specimenCount id="9D3CFD45FF8F7877FB02FB17D746FAC3" box="[1259,1334,1261,1287]" pageId="14" pageNumber="15" type="generic">
<quantity id="4CC29B29FF8F7877FB02FB17D742FAC3" box="[1259,1330,1261,1287]" metricMagnitude="2" metricUnit="m" metricValue="7.76" pageId="14" pageNumber="15" unit="m" value="776.0">776m</quantity>
