<treatmentCitation id="BF7A10B57C18FF8CFF24AF439706FDA6" author="Simon E." box="[189,611,542,569]" page="308" pageId="45" pageNumber="46" year="1906">
<taxonomicName id="F9DB4D277C18FF8CFF24AF439706FDA6" authority="Simon, 1906: 308" authorityName="Simon" authorityPageNumber="308" authorityYear="1906" box="[189,611,542,569]" class="Arachnida" family="Filistatidae" genus="Filistata" kingdom="Animalia" order="Araneae" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="hasselti">
<emphasis id="0CAFEAB67C18FF8CFF24AF4394E6FDA7" box="[189,387,542,568]" italics="true" pageId="45" pageNumber="46">Filistata hasselti</emphasis>
<bibRefCitation id="5A4A4B557C18FF8CFE12AF439706FDA6" author="Simon E." box="[395,611,542,569]" pageId="45" pageNumber="46" pagination="284 - 308" refId="ref19795" refString="Simon E. 1906. Etude sur les araignees de la section des cribellates. Annales de la Societe entomologique de Belgique 50: 284 - 308. https: // doi. org / 10.5962 / bhl. part. 19947" type="journal article" year="1906">Simon, 1906: 308</bibRefCitation>
