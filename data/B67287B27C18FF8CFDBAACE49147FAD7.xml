. Female 
<typeStatus id="E16088067C18FF8CFD57AF439650FDA7" box="[718,821,542,568]" pageId="45" pageNumber="46">holotype</typeStatus>
from 
<collectingCountry id="46CC76347C18FF8CFCE6AF439691FDA7" box="[895,1012,542,568]" name="Indonesia" pageId="45" pageNumber="46">Indonesia</collectingCountry>
