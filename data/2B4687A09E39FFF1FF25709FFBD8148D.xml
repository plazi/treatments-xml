<specimenCode id="F3499ECD9E39FFF1FD0E74E8FC80174A" box="[701,886,242,263]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="61" pageNumber="61">AMNH 713382</specimenCode>
