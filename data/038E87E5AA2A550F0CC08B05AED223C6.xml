<taxonomicName id="4C274D70AA2C550F09428CBBA98E23E3" authorityName="" authorityYear="1758" box="[1301,1436,439,464]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="170" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
