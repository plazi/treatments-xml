<subSubSection id="509E651F036BFFC9FF19E3C3D78005C6" box="[151,985,1442,1466]" pageId="8" pageNumber="9" type="materials_examined">
<paragraph id="183B3694036BFFC9FF19E3C3D78005C6" blockId="8.[151,985,1442,1466]" box="[151,985,1442,1466]" pageId="8" pageNumber="9">
<typeStatus id="C73F8836036BFFC9FF19E3C3D49505C6" box="[151,204,1442,1465]" pageId="8" pageNumber="9">Type</typeStatus>
species: 
<taxonomicName id="DF844D17036BFFC9FEA3E3C3D62C05C6" authority="Mathis" authorityName="Mathis" box="[301,629,1442,1465]" class="Insecta" family="Ephydridae" genus="Neoephydra" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="araucaria" status="sp. nov.">
