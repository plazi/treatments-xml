<quantity id="4C819B41FFBBBE71FB78714DF3FEF8DA" box="[1163,1239,1826,1850]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="95" pageNumber="682" unit="mm" value="10.0">10 mm</quantity>
