<collectionCode id="DDEDAE61C40F1C56FE04F933FDE8FC84" box="[443,525,835,861]" country="Australia" httpUri="http://grbio.org/cool/0k2v-y6wz" name="Australian National Fish Collection" pageId="88" pageNumber="89" type="Museum">CSIRO</collectionCode>
<specimenCode id="EB5A9EDFC40F1C56FDA8F933FD9DFC84" box="[535,632,835,861]" collectionCode="CSIRO" country="Australia" httpUri="http://grbio.org/cool/0k2v-y6wz" name="Australian National Fish Collection" pageId="88" pageNumber="89" type="Museum">H 643-4</specimenCode>
, 
<quantity id="7C049B41C40F1C56FD3AF932FD0EFC84" box="[645,747,834,861]" metricMagnitude="-2" metricUnit="m" metricValue="5.32" pageId="88" pageNumber="89" unit="mm" value="53.2">53.2 mm</quantity>
SL, 
<collectingCountry id="C3EB7634C40F1C56FC9CF933FC68FC84" box="[803,909,835,861]" name="Australia" pageId="88" pageNumber="89">Australia</collectingCountry>
, 
