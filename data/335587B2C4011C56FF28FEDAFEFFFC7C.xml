<taxonomicName id="7CFC4D27C40F1C56FBDDFAAFFB48FF20" box="[1122,1197,223,249]" class="Actinopterygii" family="Serranidae" genus="Plectranthias" kingdom="Animalia" order="Perciformes" pageId="88" pageNumber="89" phylum="Chordata" rank="species" species="lasti">
