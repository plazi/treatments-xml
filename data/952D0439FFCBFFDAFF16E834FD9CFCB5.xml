<quantity id="DA7C18CAFFCBFFDAFCA0E8DDFC7BFE64" box="[836,897,453,473]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="65" pageNumber="296" unit="m" value="750.0">750 m</quantity>
