<quantity id="4CAA9B67FF8FB32DE9F20950FDA6FB81" box="[477,571,1124,1150]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="51" pageNumber="52" unit="mm" value="1.5">1.5 mm</quantity>
