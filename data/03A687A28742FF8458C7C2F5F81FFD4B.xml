<subSubSection id="C315653F8743FF845897C39CF81FFD4B" pageId="23" pageNumber="24" type="discussion">
<paragraph id="8BB036B48743FF845897C39CF81FFD4B" blockId="23.[151,1436,151,645]" pageId="23" pageNumber="24">
<emphasis id="B97BEAA68743FF845897C39CFD50FDD6" bold="true" box="[199,335,510,536]" pageId="23" pageNumber="24">Comments.</emphasis>
<taxonomicName id="4C0F4D378743FF845905C39DFE21FDD6" authority="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz, 2020" authorityName="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz" authorityYear="2020" box="[341,574,511,537]" class="Insecta" family="Psyllidae" genus="Mitrapsylla" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="aurantia" status="sp. nov.">
<emphasis id="B97BEAA68743FF845905C39DFE21FDD6" box="[341,574,511,537]" italics="true" pageId="23" pageNumber="24">Mitrapsylla aurantia</emphasis>
</taxonomicName>
<emphasis id="B97BEAA68743FF845A15C39DFE81FDD7" bold="true" box="[581,670,511,537]" pageId="23" pageNumber="24">
<taxonomicNameLabel id="A24857DD8743FF845A15C39DFE81FDD7" box="[581,670,511,537]" pageId="23" pageNumber="24" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
resembles light specimens of 
<taxonomicName id="4C0F4D378743FF845BBBC39DF8A5FDD7" authority="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz, 2020" authorityName="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz" authorityYear="2020" box="[1003,1210,511,537]" class="Insecta" family="Psyllidae" genus="Mitrapsylla" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="aeschynomenis" status="sp. nov.">
<emphasis id="B97BEAA68743FF845BBBC39DF8A5FDD7" box="[1003,1210,511,537]" italics="true" pageId="23" pageNumber="24">M. aeschynomenis</emphasis>
</taxonomicName>
<emphasis id="B97BEAA68743FF845C90C39DF907FDD7" bold="true" box="[1216,1304,511,537]" pageId="23" pageNumber="24">
<taxonomicNameLabel id="A24857DD8743FF845C90C39DF907FDD7" box="[1216,1304,511,537]" pageId="23" pageNumber="24" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C0F4D378743FF845D75C39DFD1DFDF3" authority="Crawford" authorityName="Crawford. Scale" authorityYear="1914" class="Insecta" family="Psyllidae" genus="Mitrapsylla" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="cubana">
