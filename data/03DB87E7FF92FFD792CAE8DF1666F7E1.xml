<taxonomicName id="4C724D72FF92FFD792F0E8DF1560F901" authority="Grether (2000: 34)" authorityName="R. Grether" authorityPageNumber="34" authorityYear="2000" box="[178,605,1706,1733]" class="Magnoliopsida" family="Fabaceae" genus="Mimosa" kingdom="Plantae" order="Fabales" pageId="20" pageNumber="265" phylum="Tracheophyta" rank="species" species="candollei">
