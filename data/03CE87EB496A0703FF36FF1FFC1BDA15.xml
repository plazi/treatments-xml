<emphasis id="B913EAEF496A0700FF36FB63FEE0DB5C" bold="true" box="[151,296,1259,1283]" pageId="25" pageNumber="26">FIGURE 22.</emphasis>
<taxonomicName id="4C674D7E496A0700FE90FB64FDB9DB5C" authority="Published, 2007" authorityName="Published" authorityYear="2007" box="[305,625,1260,1283]" class="Insecta" family="Chironomidae" genus="Spinorthocladius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="enigmaticus" status="n. gen., n. sp.">
