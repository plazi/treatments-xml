<taxonomicName id="4C6C4D007505FFA1D682FDD9FAD5A636" box="[1175,1298,591,615]" class="Magnoliopsida" family="Lythraceae" kingdom="Plantae" order="Myrtales" pageId="36" pageNumber="518" phylum="Tracheophyta" rank="family">Lythraceae</taxonomicName>
