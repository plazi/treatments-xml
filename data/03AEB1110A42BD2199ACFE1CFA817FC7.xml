<taxonomicName id="4C077B840A43BD219BB3F8DEFCAC7FF8" box="[648,866,1909,1933]" class="Hydrozoa" family="Aglaopheniidae" genus="Cladocarpus" kingdom="Animalia" order="Leptothecata" pageId="9" pageNumber="510" phylum="Cnidaria" rank="species" species="diana">
