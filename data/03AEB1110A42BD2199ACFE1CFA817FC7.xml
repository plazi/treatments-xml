, ca. 
<quantity id="4CFFADE20A43BD219DCEF8FAFA597F1C" box="[1269,1431,1873,1898]" metricMagnitude="3" metricUnit="m" metricValue="1.4294999999999998" metricValueMax="1.504" metricValueMin="1.355" pageId="9" pageNumber="510" unit="m" value="1429.5" valueMax="1504.0" valueMin="1355.0">1355–1504 m</quantity>
