and 
<taxonomicName id="4C3ADC80EC7EFFFFFC96FAE1FC4AFA9C" box="[833,984,1305,1330]" class="Malacostraca" family="Phronimidae" genus="Phronima" kingdom="Animalia" order="Amphipoda" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="bucephala">
