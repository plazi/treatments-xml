<number id="DC2A24D32444BE13FB42F9CA5326C318" box="[1260,1319,1550,1574]" pageId="10" pageNumber="53" value="1991.0">1991</number>
</bibRefCitation>
), 
<collectingCountry id="12F576642444BE13FA90F9CA539AC318" box="[1342,1435,1550,1574]" name="United Kingdom" pageId="10" pageNumber="53">England</collectingCountry>
(recorded here), 
<collectingCountry id="12F576642444BE13FEFFF9F657B2C374" box="[337,435,1586,1610]" name="Lebanon" pageId="10" pageNumber="53">Lebanon</collectingCountry>
(
<bibRefCitation id="0E734B052444BE13FE6FF9F6544BC374" author="Bayan" box="[449,586,1586,1610]" pageId="10" pageNumber="53" refString="Bayan, A. (1988) Mites on plum trees in Lebanon. I. A general survey and a diagnostic key. Arab Journal of Plant Protection, 6 (1), 1 - 6." type="journal article" year="1988">
Bayan 
<number id="DC2A24D32444BE13FDBFF9F6544BC374" box="[529,586,1586,1610]" pageId="10" pageNumber="53" value="1988.0">1988</number>
</bibRefCitation>
