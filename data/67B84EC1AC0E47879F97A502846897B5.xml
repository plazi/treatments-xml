<taxonomicName id="9086A4594ED261E7AB22A5BFDB7C79B6" class="Insecta" family="Formicidae" genus="Dolichoderus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dolichoderus laotius" order="Hymenoptera" pageId="10" pageNumber="27" phylum="Arthropoda" rank="species" species="laotius">Dolichoderus laotius</taxonomicName>
