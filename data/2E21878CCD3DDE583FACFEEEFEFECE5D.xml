<taxonomicName id="61884D19CD3EDE583D65FF72FE60CE60" authority="Fassio &amp; Russini &amp; Buge &amp; Schiaparelli &amp; Modica &amp; Bouchet &amp; Oliverio, 2020" authorityName="Fassio &amp; Russini &amp; Buge &amp; Schiaparelli &amp; Modica &amp; Bouchet &amp; Oliverio" authorityYear="2020" box="[269,386,254,275]" class="Gastropoda" family="Capulidae" genus="Hyalorisia" kingdom="Animalia" order="Littorinimorpha" pageId="14" pageNumber="415" phylum="Mollusca" rank="species" species="melanesica" status="n. sp.">
<emphasis id="94FCEA88CD3EDE583D65FF72FE60CE60" box="[269,386,254,275]" italics="true" pageId="14" pageNumber="415">H. melanesica</emphasis>
</taxonomicName>
<taxonomicNameLabel id="8FCF57F3CD3EDE583DE3FF72FE22CE60" box="[395,448,254,275]" pageId="14" pageNumber="415" rank="species">n. sp.</taxonomicNameLabel>
(MOTU-8) in 
<collectingRegion id="644CF878CD3EDE583E3DFF72FD30CE67" box="[597,722,254,276]" country="Papua New Guinea" name="New Ireland" pageId="14" pageNumber="415">New Ireland</collectingRegion>
, 
<collectingCountry id="DE9F760ACD3EDE583EB5FF72FEFACE5D" name="Papua New Guinea" pageId="14" pageNumber="415">Papua New Guinea</collectingCountry>
