, including species 
<taxonomicName id="41ED4D0B1F3CFFB48A197DD942D857E7" authority="Hering 1933" authorityName="Hering" authorityYear="1933" box="[833,1229,934,960]" class="Insecta" family="Agromyzidae" genus="Agromyza" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="592" phylum="Arthropoda" rank="species" species="megalopsis">
<emphasis id="B499EA9A1F3CFFB48A197DD9422257E7" box="[833,1079,934,960]" italics="true" pageId="4" pageNumber="592">Agromyza megalopsis</emphasis>
Hering 1933
</taxonomicName>
and 
<taxonomicName id="41ED4D0B1F3CFFB48C5D7DD9479657C3" authority="Meigen, 1830" authorityName="Meigen" authorityYear="1830" class="Insecta" family="Agromyzidae" genus="Agromyza" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="592" phylum="Arthropoda" rank="species" species="nigripes">
<emphasis id="B499EA9A1F3CFFB48C5D7DD946C857C3" italics="true" pageId="4" pageNumber="592">Agromyza nigripes</emphasis>
