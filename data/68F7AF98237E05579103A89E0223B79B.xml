<paragraph id="D84A269B1956B5A9F948CDE351AFD7E0" pageId="18" pageNumber="67">
<taxonomicName id="6FBAC40663730B74002FAEB40A57179E" class="Insecta" family="Bombyliidae" genus="Neodiplocampta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neodiplocampta" order="Diptera" pageId="18" pageNumber="67" phylum="Arthropoda" rank="genus">Neodiplocampta</taxonomicName>
