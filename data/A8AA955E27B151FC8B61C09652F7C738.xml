<taxonomicName id="DBC809D5F250BC458FD1F9FC392AF579" class="Dicotyledoneae" family="Salicaceae" genus="Populus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Salicales" pageId="9" pageNumber="92" phylum="Angiospermae" rank="genus">
