<taxonomicName id="EC844D411C57536D5646FD61209FE36E" baseAuthorityName="Cockerell" baseAuthorityYear="1905" box="[783,908,763,789]" class="Insecta" family="Pseudococcidae" genus="Planococcus" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="lilacinus">
<emphasis id="19F0EAD01C57536D5646FD61209FE36E" box="[783,908,763,789]" italics="true" pageId="28" pageNumber="29">P. lilacinus</emphasis>
</taxonomicName>
in 
<collectingRegion id="E940F8201C57536D56FDFD6120F5E36E" box="[948,998,763,789]" country="Indonesia" name="Jawa Timur" pageId="28" pageNumber="29">Java</collectingRegion>
