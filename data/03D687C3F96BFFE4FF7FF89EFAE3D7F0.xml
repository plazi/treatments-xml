<taxonomicName id="4C7F4D56F96CFFE6FEF1F8FCFE6CD710" authority="Zhong &amp; Xie &amp; Hu &amp; Chen &amp; Feng &amp; Zhang &amp; Wang, 2019" authorityName="Zhong &amp; Xie &amp; Hu &amp; Chen &amp; Feng &amp; Zhang &amp; Wang" authorityYear="2019" box="[281,453,1804,1827]" class="Rhabditophora" family="Provorticidae" genus="Pogaina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabdocoela" pageId="4" pageNumber="357" phylum="Platyhelminthes" rank="species" species="sinensis" status="n. sp.">
