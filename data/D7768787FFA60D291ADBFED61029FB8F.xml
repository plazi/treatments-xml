<taxonomicName id="98DF4D12FFA60D291B1CFC3C17D5FC5A" authority="(Armua de Reyes &amp; Estevez 2016)" baseAuthorityName="Armua de Reyes &amp; Estevez" baseAuthorityYear="2016" box="[336,970,928,955]" class="Liliopsida" family="Pontederiaceae" genus="Pontederia" kingdom="Plantae" order="Commelinales" pageId="27" pageNumber="478" phylum="Tracheophyta" rank="species" species="crassipes">
