<subSubSection id="D24217CC84515FE541088FC27D4397C9" type="reference_group">
<paragraph id="42DD53EBC9E341593221A24C474C7308" pageNumber="143">
<taxonomicName id="D28E1D75AFC241C3713B6CF0D24B8828" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:144883" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium (Xiphomyrmex) andrei Forel" lsidName-HNS="Tetramorium (Xiphomyrmex) andrei Forel" order="Hymenoptera" pageNumber="143" phylum="Arthropoda" rank="species" species="andrei" subGenus="Xiphomyrmex">Tetramorium {Xiphomyrmex) andrei Forel</taxonomicName>
