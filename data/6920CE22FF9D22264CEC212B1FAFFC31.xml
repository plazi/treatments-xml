<taxonomicName id="268904B7FF9D22254F55235C1802F99E" authority="Durante &amp; Apinda-Legnouo, 2020" authorityName="Durante &amp; Apinda-Legnouo" authorityYear="2020" box="[814,893,1556,1582]" class="Insecta" family="Erebidae" genus="Tumicla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="99" phylum="Arthropoda" rank="species" species="pallida" status="n. sp.">
<emphasis id="D3FDA326FF9D22254F55235C1802F99E" box="[814,893,1556,1582]" italics="true" pageId="9" pageNumber="99">pallida</emphasis>
</taxonomicName>
