<taxonomicName id="4C08C0E5FFC8FF96FDAAFD43EE93FD14" box="[616,680,688,713]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="19" pageNumber="261" phylum="Tracheophyta" rank="genus">
