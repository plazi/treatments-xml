<taxonomicName id="4C08C0E5FFC8FF96FD2DFD5CEF62FD17" box="[751,857,687,714]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="19" pageNumber="261" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
