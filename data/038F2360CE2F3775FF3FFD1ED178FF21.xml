<specimenCount id="9D2059FFCE293775FD90FF7CD058FF79" box="[568,589,152,175]" pageId="36" pageNumber="137" type="female">♀</specimenCount>
, 
<quantity id="4CDE3F93CE293775FDF3FF7CD0A1FF79" box="[603,692,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="36" pageNumber="137" unit="mm" value="5.9">5.9 mm</quantity>
