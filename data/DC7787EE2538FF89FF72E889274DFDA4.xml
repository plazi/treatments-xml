<taxonomicName id="93DE4D7B2534FF89FD5EEF7A27AEFDD1" box="[699,901,534,559]" class="Arachnida" family="Eriophyidae" genus="Jiangsuacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="huaguoshanensis">
