<taxonomicName id="64AF4BBBCD07A8891484537C88BBF400" class="Arachnida" family="Suctobelbidae" genus="Suctobelba" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="2" pageNumber="136" phylum="Arthropoda" rank="genus">
<pageBreakToken id="BFF0F6BF55578FD398935787FF8F552A" pageId="2" pageNumber="136" start="start">Suctobelba</pageBreakToken>
