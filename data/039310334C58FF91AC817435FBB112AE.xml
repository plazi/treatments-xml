<quantity id="4CC20CC04C58FF96AE85746BFD6D137B" box="[660,728,1377,1403]" metricMagnitude="2" metricUnit="m" metricValue="8.76" pageId="3" pageNumber="12" unit="m" value="876.0">876 m</quantity>
