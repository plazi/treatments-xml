<elevation id="B383A30DFFD3FFDE1278FD82FB7D5B46" box="[1080,1199,546,573]" metricMagnitude="1" metricUnit="m" metricValue="3.1" pageId="11" pageNumber="12" unit="m" value="31.0">
<quantity id="FF56E9DBFFD3FFDE1278FD82FBA05B46" box="[1080,1138,547,573]" metricMagnitude="1" metricUnit="m" metricValue="3.1" pageId="11" pageNumber="12" unit="m" value="31.0">31 m</quantity>
a.s.l.
</elevation>
], 
<collectingRegion id="FA6A8ADCFFD3FFDE1284FD82FAA95B46" box="[1220,1403,547,573]" country="Angola" name="Bengo" pageId="11" pageNumber="12">Bengo Province</collectingRegion>
