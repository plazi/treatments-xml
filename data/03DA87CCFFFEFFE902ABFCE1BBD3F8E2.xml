<collectingCountry id="F364764AFFFEFFE90025F97BBB5BF8CA" box="[537,581,1789,1815]" name="Fiji" pageId="26" pageNumber="27">Viti</collectingCountry>
Levu, Karst de Qalimare, 
<collectingCountry id="F364764AFFFEFFE90140F97BBAA7F8CA" box="[892,953,1789,1815]" name="Tonga" pageId="26" pageNumber="27">Toga</collectingCountry>
, 
<quantity id="4C8B9B3FFFFEFFE901F8F97BBD1DF8CA" box="[964,1027,1789,1815]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="26" pageNumber="27" unit="m" value="50.0">50 m</quantity>
, PM, 
<date id="FFCD101AFFFEFFE9067BF97BBDF7F8CA" box="[1095,1257,1789,1815]" pageId="26" pageNumber="27" value="1998-08-21">
<number id="3DBB24FDFFFEFFE9067BF97BBD7BF8CA" box="[1095,1125,1789,1815]" pageId="26" pageNumber="27" value="21.0">21</number>
