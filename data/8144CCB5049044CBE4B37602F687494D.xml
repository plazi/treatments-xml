<taxonomicName id="3442F73944EBCCE2F257F6ED623CC89D" LSID="http://zoobank.org/28E2B152-07C6-4E03-8A92-5F91E7353881" authority="Oka, 1895" authorityName="Oka" authorityYear="1895" class="Clitellata" family="Orobdellidae" genus="Orobdella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Orobdella okanoi" order="Arhynchobdellida" pageId="6" pageNumber="85" phylum="Annelida" rank="species" species="okanoi">Orobdella okanoi</taxonomicName>
