Lap. Soula det.// 
<taxonomicName id="7F3F9A521F6F07D3CF7A60F283DE71A9" baseAuthorityName="Laporte" baseAuthorityYear="1840" class="Insecta" family="Scarabaeidae" genus="Hoplopelidnota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hoplopelidnota metallica" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="metallica">
