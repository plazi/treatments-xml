<quantity id="4CE99B44A472FFEEEBBBF877FCEE9CD1" box="[687,853,1943,1968]" metricMagnitude="2" metricUnit="m" metricValue="4.35" metricValueMax="7.58" metricValueMin="1.1199999999999999" pageId="28" pageNumber="29" unit="m" value="435.0" valueMax="758.0" valueMin="112.0">
112 to 
<specimenCount id="9D17FD28A472FFEEEA12F878FCEE9CD1" box="[774,853,1943,1968]" pageId="28" pageNumber="29" type="generic">758 m</specimenCount>
</quantity>
