</materialsCitation>
</paragraph>
<paragraph id="8B8F366ED052FFE6FEA1F994348F05EB" blockId="7.[313,1269,1454,1733]" box="[353,1265,1644,1669]" pageId="7" pageNumber="343">
<materialsCitation id="3B583C33D052FFE6FEA1F994349305EB" box="[353,1261,1644,1669]" collectionCode="R, V" pageId="7" pageNumber="343" specimenCount="1" typeStatus="holotype">
<emphasis id="B944EA7CD052FFE6FEA1F994318A05EB" box="[353,500,1645,1669]" italics="true" pageId="7" pageNumber="343">
<typeStatus id="548B88CCD052FFE6FEA1F99431E705EB" box="[353,409,1646,1669]" pageId="7" pageNumber="343">Type</typeStatus>
