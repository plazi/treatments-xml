<paragraph id="D5C749E2D3336E3897FEB2595911C8CA" pageId="0" pageNumber="15">
<taxonomicName id="6986BA821AC8831DD3DE4A8C1889B7CE" authority="Zhao &amp; Li, 2020" authorityName="Zhao &amp; Li" authorityYear="2020" class="Arachnida" family="Telemidae" genus="Zhuanlema" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zhuanlema peteri" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="peteri">
