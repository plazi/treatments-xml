<taxonomicName id="E3A3A08EFF945678FAE0BDE99AC32BDE" authority="Hayata" authorityName="Hayata" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="3" pageNumber="50" phylum="Tracheophyta" rank="species" species="albopurpurascens">
