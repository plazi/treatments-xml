<taxonomicName id="E3A3A08EFF945678FDFBBD8C99902BDE" box="[636,740,477,501]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="50" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
