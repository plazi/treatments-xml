</taxonomicName>
(
<taxonomicName id="E3A3A08EFF945678FE41BD8D994A2BDF" box="[454,574,476,500]" class="Magnoliopsida" family="Aceraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="3" pageNumber="50" phylum="Tracheophyta" rank="family">Aceraceae</taxonomicName>
