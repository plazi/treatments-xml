<subSubSection id="C30B65355630D01FFF64EB8DFDA9EE03" pageId="1" pageNumber="58" type="description">
<paragraph id="8BAE36BE5630D01FFF64EB8DFD5DE9BC" blockId="1.[151,1437,605,1560]" box="[198,756,725,751]" pageId="1" pageNumber="58">
<emphasis id="B965EAAC5630D01FFF64EB8DFEF7E9BC" bold="true" box="[198,350,725,751]" pageId="1" pageNumber="58">Description.</emphasis>
<typeStatus id="54AA881C5630D01FFEC4EB8DFE7EE9BC" box="[358,471,725,751]" pageId="1" pageNumber="58">Holotype</typeStatus>
