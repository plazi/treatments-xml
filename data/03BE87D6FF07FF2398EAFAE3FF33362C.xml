<quantity id="4CEF9B25FF07FF239CCDFAE0FF413528" box="[1200,1280,1298,1325]" metricMagnitude="-1" metricUnit="m" metricValue="5.9944" pageId="150" pageNumber="151" unit="in" value="23.6">23.6 in</quantity>
