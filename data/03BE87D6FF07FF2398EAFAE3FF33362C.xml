<quantity id="4CEF9B25FF07FF239C21FAE0FEE03529" box="[1116,1185,1298,1324]" metricMagnitude="-1" metricUnit="m" metricValue="6.0" pageId="150" pageNumber="151" unit="cm" value="60.0">60 cm</quantity>
