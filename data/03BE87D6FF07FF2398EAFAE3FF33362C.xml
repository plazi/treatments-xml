<geoCoordinate id="EE235007FF07FF239AACFAC4F90B3555" box="[721,842,1334,1360]" degrees="34" direction="north" minutes="20.7" orientation="latitude" pageId="150" pageNumber="151" precision="92" value="34.345">34°20.7’N</geoCoordinate>
