<geoCoordinate id="EE235007FF07FF239B25FAC4F98C3555" box="[856,973,1334,1360]" degrees="119" direction="west" minutes="30" orientation="longitude" pageId="150" pageNumber="151" precision="925" value="-119.5">119°30’W</geoCoordinate>
