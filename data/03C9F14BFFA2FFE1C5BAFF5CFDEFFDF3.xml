µm; kw 
<number id="3DA8527AFFA2FFE7C607FDAFFCE5FD83" box="[810,847,612,638]" pageId="2" pageNumber="51" value="8.1">8.1</number>
µm; hw 
<number id="3DA8527AFFA2FFE7C694FDAFFBAFFD83" box="[953,1029,612,638]" pageId="2" pageNumber="51" value="31.0" valueMax="32.0" valueMin="30.0">30–32</number>
µm; nw 
<number id="3DA8527AFFA2FFE7C148FDAFFB04FD83" box="[1125,1198,612,638]" pageId="2" pageNumber="51" value="24.5" valueMax="26.0" valueMin="23.0">23–26</number>
µm; trw 
<number id="3DA8527AFFA2FFE7C036FDAFFACFFD83" box="[1307,1381,612,638]" pageId="2" pageNumber="51" value="43.5" valueMax="46.0" valueMin="41.0">41–46</number>
µm; hscL 
<number id="3DA8527AFFA2FFE7C5FAFD41FE85FD59" box="[215,303,650,676]" pageId="2" pageNumber="51" value="2.55" valueMax="2.8" valueMin="2.3">2.3–2.8</number>
µm; trscL 
<number id="3DA8527AFFA2FFE7C49CFD41FDA3FD59" box="[433,521,650,676]" pageId="2" pageNumber="51" value="6.25" valueMax="6.4" valueMin="6.1">6.1–6.4</number>
µm; hhyL 
<number id="3DA8527AFFA2FFE7C7A1FD41FD4EFD59" box="[652,740,650,676]" pageId="2" pageNumber="51" value="3.75" valueMax="3.9" valueMin="3.6">3.6–3.9</number>
µm; trhyL 
<number id="3DA8527AFFA2FFE7C646FD41FC69FD59" box="[875,963,650,676]" pageId="2" pageNumber="51" value="7.699999999999999" valueMax="7.8" valueMin="7.6">7.6–7.8</number>
µm.
</paragraph>
</subSubSection>
