<keyStep id="03547DA7FFEEFFA539B6FE49FC08A390" box="[151,921,439,466]" pageId="23" pageNumber="124">
<emphasis id="8AD4B910FFEEFFA539B6FE49FC08A390" bold="true" box="[151,921,439,466]" pageId="23" pageNumber="124">
<number id="0E687725FFEEFFA539B6FE49FF27A390" box="[151,182,440,466]" pageId="23" pageNumber="124" value="16.0">16</number>
. 
<taxonomicName id="7FA01E81FFEEFFA539E2FE46FC08A390" authority="Blanchard, 1851" authorityName="Blanchard" authorityYear="1851" box="[195,921,439,466]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="124" phylum="Arthropoda" rank="species" species="fragilipennis" subGenus="Phyllophaga">
<emphasis id="8AD4B910FFEEFFA539E2FE46FECDA393" bold="true" box="[195,348,439,465]" italics="true" pageId="23" pageNumber="124">Phyllophaga</emphasis>
(
<emphasis id="8AD4B910FFEEFFA5384CFE46FD97A393" bold="true" box="[365,518,439,465]" italics="true" pageId="23" pageNumber="124">Phyllophaga</emphasis>
) 
<emphasis id="8AD4B910FFEEFFA53B36FE46FD20A393" bold="true" box="[535,689,439,465]" italics="true" pageId="23" pageNumber="124">fragilipennis</emphasis>
(Blanchard, 
<number id="0E687725FFEEFFA53A74FE49FC00A390" box="[853,913,440,466]" pageId="23" pageNumber="124" value="1851.0">1851</number>
)
</taxonomicName>
