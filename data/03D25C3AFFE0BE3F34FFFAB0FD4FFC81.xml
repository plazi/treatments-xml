<collectingRegion id="49BF23CEFFE2BE3E353AFE9DFC5AFE96" box="[818,902,275,296]" country="Mexico" name="Durango" pageId="6" pageNumber="148">Durango</collectingRegion>
, 
<collectingCountry id="F36CADBCFFE2BE3E3599FE9DFC04FE96" box="[913,984,275,296]" name="Mexico" pageId="6" pageNumber="148">Mexico</collectingCountry>
(
<figureCitation id="1340F1A9FFE2BE3E35EEFE9DFBF4FE96" box="[998,1064,275,296]" captionStart="Fig" captionStartId="6.[175,206,1683,1702]" captionTargetBox="[152,1171,865,1659]" captionTargetId="figure-280@6.[151,1171,864,1660]" captionTargetPageId="6" captionText="Fig. 16. Distribution map for Hologymnetis argenteola, Hologymnetis reyesi, Hologymnetis cinerea, and Hologymnetis kinichahau." figureDoi="http://doi.org/10.5281/zenodo.5361970" httpUri="https://zenodo.org/record/5361970/files/figure.png" pageId="6" pageNumber="148">Fig. 16</figureCitation>
)................ ...................................... 
