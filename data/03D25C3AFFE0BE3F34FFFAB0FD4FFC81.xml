<collectingRegion id="49BF23CEFFE3BE3F3783FD0DFE0FFD26" box="[395,467,643,664]" country="Mexico" name="Mexico" pageId="7" pageNumber="149">Mexico</collectingRegion>
(
<collectingRegion id="49BF23CEFFE3BE3F37ECFD0DFD53FD26" box="[484,655,643,664]" isEnumeration="true" pageId="7" pageNumber="149">Colima, Guerrero</collectingRegion>
