. 
<collectingRegion id="49BF23CEFFE2BE3E324FFF76FB4CFEB3" box="[1095,1168,248,269]" country="United States of America" name="Arizona" pageId="6" pageNumber="148">Arizona</collectingRegion>
, 
<collectingCountry id="F36CADBCFFE2BE3E34EBFE9DFCCEFE96" box="[739,786,275,296]" name="United States of America" pageId="6" pageNumber="148">USA</collectingCountry>
