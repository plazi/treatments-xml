<taxonomicName id="4C7B96AFFFE2BE3E3553FE7AFB48FDB7" authority="Ratcliffe and Deloya" authorityName="Ratcliffe and Deloya" box="[859,1172,500,521]" class="Insecta" family="Cetoniidae" genus="Hologymnetis" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="148" phylum="Arthropoda" rank="species" species="moroni">
