<taxonomicName id="C79F4D6A0648FF9C8E1B3709E291FBC7" box="[1294,1436,1089,1112]" class="Insecta" family="Chironomidae" genus="Corynoneura" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="42" phylum="Arthropoda" rank="genus">
