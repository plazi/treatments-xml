<collectingRegion id="49C9B0D4FFD6DF7FFD15A6D5FD28FCC7" box="[660,766,746,770]" country="United States of America" name="California" pageId="11" pageNumber="595">California</collectingRegion>
and 
<collectingRegion id="49C9B0D4FFD6DF7FFCB9A6D5FC58FCC7" box="[824,910,746,770]" country="United States of America" name="Oregon" pageId="11" pageNumber="595">Oregon</collectingRegion>
) (
