<location id="8EDA602FFF3EFF48FF22FECD38CFFEFB" LSID="urn:lsid:plazi:treatment:03AC87E2FF3FFF48FD4CFC3F387FFE12:8EDA602FFF3EFF48FF22FECD38CFFEFB" box="[221,424,329,351]" name="Mount Karimui" pageId="133" pageNumber="133">Mount Karimui</location>
, 
<emphasis id="B971EAE6FF3EFF48FE3FFECE39BFFED8" bold="true" pageId="133" pageNumber="133">
<specimenCode id="DBA39E8FFF3EFF48FE3FFECE39B3FED8" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="133" pageNumber="133">AMNH 809398– 809400</specimenCode>
