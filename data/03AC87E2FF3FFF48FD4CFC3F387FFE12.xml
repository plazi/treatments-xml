<specimenCode id="DBA39E8FFF3EFF48FE94FEAB3BEEFEE5" box="[363,649,300,321]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="133" pageNumber="133">AMNH 809394–809397</specimenCode>
