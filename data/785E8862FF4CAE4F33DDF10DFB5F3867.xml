<paragraph id="F0483974FF4EAE4F3291F7FCFE803FD8" blockId="23.[211,344,652,676]" box="[211,344,652,676]" pageId="23" pageNumber="1101">
<emphasis id="C283E566FF4EAE4F3291F7FCFE803FD8" box="[211,344,652,676]" italics="true" pageId="23" pageNumber="1101">Distribution</emphasis>
</paragraph>
<paragraph id="F0483974FF4EAE4F3291F7C4FED33F90" blockId="23.[211,1171,692,748]" pageId="23" pageNumber="1101">
<collectingCountry id="88E079E4FF4EAE4F3291F7C4FEE53FB0" box="[211,317,692,716]" name="Thailand" pageId="23" pageNumber="1101">Thailand</collectingCountry>
