<quantity id="B5199B092425BD7CFBA9D9211F7DA47B" box="[1033,1090,1891,1914]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="59" pageNumber="60" unit="m" value="50.0">50 m</quantity>
