<taxonomicName id="49B85AC1FF8A3336B2C36FDEA9D20A64" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[735,898,493,518]" class="Aves" family="Ciconiidae" genus="Ciconia" kingdom="Animalia" order="Ciconiiformes" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="nigra">
