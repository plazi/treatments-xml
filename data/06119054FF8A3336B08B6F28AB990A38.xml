<taxonomicName id="49B85AC1FF8A3336B53B6C25AFC10A4D" authorityName="Bedford" authorityYear="1930" box="[1319,1425,534,559]" class="Insecta" family="Menoponidae" genus="Colpocephalum" kingdom="Animalia" order="Psocodea" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="ferrisi">
