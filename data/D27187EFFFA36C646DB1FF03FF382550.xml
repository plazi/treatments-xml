<taxonomicName id="9DD84D7AFFA36C64683AFED2FEEE26C0" authority="Wicht, 1970" authorityName="Wicht" authorityYear="1970" class="Arachnida" family="Siteroptidae" genus="Pediculaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="53" phylum="Arthropoda" rank="species" species="kneeboni">
