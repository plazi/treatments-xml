<taxonomicName id="D7F64D08DF177256C079FBECFD8E736F" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[563,622,1125,1148]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="mora">
