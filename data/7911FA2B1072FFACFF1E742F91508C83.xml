<quantity id="3640E6D81072FFACFF39753992228E8B" box="[176,272,1394,1418]" metricMagnitude="3" metricUnit="m" metricValue="1.755" pageId="16" pageNumber="117" unit="m" value="1755.0">1, 755 m</quantity>
