: Stn. S 
<date id="85066DFD1072FFACFD69750590338E67" box="[736,769,1358,1382]" pageId="16" pageNumber="117">
<number id="4770591A1072FFACFD69750591D88E67" box="[736,746,1358,1382]" pageId="16" pageNumber="117" value="1.0">1</number>
V
</date>
, 
<quantity id="3640E6D81072FFACFC867505906B8E67" box="[783,857,1358,1382]" metricMagnitude="2" metricUnit="m" metricValue="7.459999999999999" pageId="16" pageNumber="117" unit="m" value="746.0">746 m</quantity>
