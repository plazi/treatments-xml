<date id="85066DFD1072FFACFDAD7539917A8E8B" box="[548,584,1394,1418]" pageId="16" pageNumber="117">
V 
<quantity id="3640E6D81072FFACFDB47539917A8E8B" box="[573,584,1394,1418]" metricMagnitude="3" metricUnit="m" metricValue="1.761" pageId="16" pageNumber="117" unit="m" value="1761.0">1</quantity>
