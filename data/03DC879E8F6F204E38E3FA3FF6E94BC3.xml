<taxonomicName id="4C754D0B8F6C204F396EF974F3914DCE" authorityName="Morton" authorityYear="1905" box="[282,390,1759,1783]" class="Insecta" family="Hydroptilidae" genus="Neotrichia" kingdom="Animalia" order="Trichoptera" pageId="10" pageNumber="526" phylum="Arthropoda" rank="genus">
