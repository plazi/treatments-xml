Male (n = 1) TL 
<quantity id="3E45129FF2761429719CE84557062A1D" box="[702,803,513,539]" metricMagnitude="-3" metricUnit="m" metricValue="1.73" pageId="31" pageNumber="32" unit="mm" value="1.73">1.73 mm</quantity>
, PL 
<quantity id="3E45129FF27614297077E845579E2A1D" box="[853,955,513,539]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="31" pageNumber="32" unit="mm" value="0.4">0.40 mm</quantity>
, PW 
<quantity id="3E45129FF276142970D4E84550792A1D" box="[1014,1116,513,539]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="31" pageNumber="32" unit="mm" value="0.9">0.90 mm</quantity>
, EL 
<quantity id="3E45129FF276142977ADE84450D02A1D" box="[1167,1269,512,539]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="31" pageNumber="32" unit="mm" value="1.5">1.50 mm</quantity>
