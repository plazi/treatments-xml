<subSubSection id="C31A6563256BD435FF2DFD62FD35FD20" pageId="4" pageNumber="231" type="etymology">
<paragraph id="8BBF36E8256BD435FF2DFD62FD35FD20" blockId="4.[151,1437,289,2006]" pageId="4" pageNumber="231">
<emphasis id="B974EAFA256BD435FF2DFD62FEE8FD0F" bold="true" box="[199,334,685,710]" pageId="4" pageNumber="231">Etymology.</emphasis>
<taxonomicName id="4C004D6B256BD435FEB3FD61FE53FD0F" authority="Greek" authorityName="Greek" box="[345,501,685,710]" class="Insecta" family="Xeninae" genus="Xenos" kingdom="Animalia" order="Strepsiptera" pageId="4" pageNumber="231" phylum="Arthropoda" rank="genus">
<emphasis id="B974EAFA256BD435FEB3FD61FE3BFD0C" box="[345,413,686,709]" italics="true" pageId="4" pageNumber="231">Xenos</emphasis>
, Greek
</taxonomicName>
meaning stranger and 
<taxonomicName id="4C004D6B256BD435FCECFD61FC3FFD0F" authority="Greek" authorityName="Greek" box="[774,921,685,710]" class="Actinopterygii" family="Amiidae" genus="Amia" kingdom="Animalia" order="Amiiformes" pageId="4" pageNumber="231" phylum="Chordata" rank="genus">
<emphasis id="B974EAFA256BD435FCECFD61FCE7FD0C" box="[774,833,686,709]" italics="true" pageId="4" pageNumber="231">Amia</emphasis>
, Greek
</taxonomicName>
