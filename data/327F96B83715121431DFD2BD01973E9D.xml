<taxonomicName id="36C23D98AA037752595677E52FCE66F0" authority="Pollich" class="Liliopsida" family="Poaceae" genus="Festuca" kingdom="Plantae" order="Poales" pageId="null" pageNumber="356" phylum="Tracheophyta" rank="species" species="duriuscula">
