<taxonomicName id="46DCD9FFFFE3CE33FCD5F91BD8FF3ACD" baseAuthorityName="Doleschall" baseAuthorityYear="1871" box="[810,940,1738,1764]" class="Arachnida" family="Actinopodidae" genus="Actinopus" kingdom="Animalia" order="Araneae" pageId="110" pageNumber="111" phylum="Arthropoda" rank="species" species="nattereri">
