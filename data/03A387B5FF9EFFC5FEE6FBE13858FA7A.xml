. 
<taxonomicName id="4C0A4D20FF99FFC4FE1BF9613973F979" baseAuthorityName="Bagnall" baseAuthorityYear="1918" box="[501,770,1712,1735]" class="Insecta" family="Thripidae" genus="Mycterothrips" kingdom="Animalia" order="Thysanoptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="setiventris">
