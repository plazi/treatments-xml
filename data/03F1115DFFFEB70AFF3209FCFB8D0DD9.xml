<paragraph id="8BE7A04BFFFEB70AFF320B89FD6E09C9" blockId="2.[151,763,952,975]" box="[151,763,952,975]" pageId="2" pageNumber="18">
<treatmentCitationGroup id="AB488765FFFEB70AFF320B89FD6E09C9" box="[151,763,952,975]" pageId="2" pageNumber="18">
