<taxonomicName id="4C462DB1FFA4333321CD95471F748C19" authorityName="Spinola" authorityYear="1839" box="[1265,1347,1048,1073]" class="Insecta" family="Fulgoridae" genus="Pyrops" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
