<quantity id="4CA3FA836759472DDB9FF945FCECCACD" box="[718,856,1738,1765]" metricMagnitude="-3" metricUnit="m" metricValue="7.9" metricValueMax="8.5" metricValueMin="7.3" pageId="22" pageNumber="263" unit="mm" value="7.9" valueMax="8.5" valueMin="7.3">7.3–8.5 mm</quantity>
, BL 
<quantity id="4CA3FA836759472DDADFF945FB87CACD" box="[910,1075,1738,1765]" metricMagnitude="-2" metricUnit="m" metricValue="1.13" metricValueMax="1.18" metricValueMin="1.08" pageId="22" pageNumber="263" unit="mm" value="11.3" valueMax="11.8" valueMin="10.8">10.8–11.8 mm</quantity>
