<quantity id="4CA3FA836759472DDC19F945FF71CB21" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="2.8" metricValueMin="2.7" pageId="22" pageNumber="263" unit="mm" value="2.75" valueMax="2.8" valueMin="2.7">2.7–2.8 mm</quantity>
