<specimenCode id="DBE89EBF8949FFEDFDC88D23C08BF8C2" box="[601,723,1880,1903]" collectionCode="ZMB" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="79" pageNumber="80" type="Museum">ZMB 2475</specimenCode>
</materialsCitation>
