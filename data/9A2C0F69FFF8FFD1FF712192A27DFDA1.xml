.
</emphasis>
Male, 
<collectingCountry id="6A92FEEFFFFAFFD1FE0326DEA288FE48" box="[485,648,461,487]" name="Indonesia" pageId="18" pageNumber="19">INDONESIA</collectingCountry>
, 
<collectingRegion id="D041709DFFFAFFD1FD7326DEA2FAFE48" box="[661,762,461,487]" country="Indonesia" name="Sumatera Utara" pageId="18" pageNumber="19">Sumatra</collectingRegion>
, 
<collectingRegion id="D041709DFFFAFFD1FCE026DEA3C1FE48" box="[774,961,461,487]" country="Indonesia" name="Sumatera Utara" pageId="18" pageNumber="19">Sumatera Utara</collectingRegion>
, Semangat Gunung, 
<quantity id="D57D139AFFFAFFD1FB5126DEA513FE48" box="[1207,1299,461,487]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="18" pageNumber="19" unit="m" value="1300.0">1300 m</quantity>
