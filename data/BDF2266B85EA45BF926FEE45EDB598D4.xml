<taxonomicName id="B7E170FA0B6567F68DA32EFB49934BBF" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus robustipennis" order="Lecanorales" pageId="2" pageNumber="107" phylum="Ascomycota" rank="species" species="robustipennis">Xyleborus robustipennis</taxonomicName>
