. 
<taxonomicName id="3CC9BEFDFFB8FF98FED1AC91FDB6FB0C" authorityName="Weld" authorityYear="1922" baseAuthorityName="Ashmead" baseAuthorityYear="1885" box="[322,569,1156,1182]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="succinipedis">
