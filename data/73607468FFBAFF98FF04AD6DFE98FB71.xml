</taxonomicName>
(= 
<taxonomicName id="3CC9BEFDFFB8FF98FBAAAC09FED3FBCB" authority="(Ashmead, 1881)" baseAuthorityName="Ashmead" baseAuthorityYear="1881" class="Insecta" family="Cynipidae" genus="Disholcaspis" kingdom="Animalia" order="Hymenoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="quercussuccinipes">
