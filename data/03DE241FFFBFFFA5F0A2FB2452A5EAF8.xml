<taxonomicName id="4C77EE8AFFBFFFA5F170F9F3525DEA68" authorityName="K. H. Barnard" authorityYear="1925" box="[325,600,1588,1613]" class="Malacostraca" family="Uristidae" genus="Schisturella" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="518" phylum="Arthropoda" rank="species" species="adversicola">
