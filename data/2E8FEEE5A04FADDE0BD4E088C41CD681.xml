<taxonomicName id="267EB39DD0DE95A9ACF3C46E5CC722DB" class="Hexapoda" family="Carabidae" genus="Pterostichus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="508" pageNumber="509" phylum="Arthropoda" rank="species" species="purpuratus">
