<subSubSection id="13AC65BBFFCBEF7BC4F5FF31FEC8FE25" pageId="8" pageNumber="9" type="nomenclature">
<paragraph id="5B093630FFCBEF7BC4F5FF31FDE5FF12" blockId="8.[151,582,151,176]" box="[151,582,151,176]" pageId="8" pageNumber="9">
<heading id="0041815CFFCBEF7BC4F5FF31FDE5FF12" bold="true" box="[151,582,151,176]" fontSize="10" level="2" pageId="8" pageNumber="9" reason="2">
<taxonomicName id="9CB64DB3FFCBEF7BC4F5FF31FDE5FF12" authority="Hood, 1954" authorityName="Hood" authorityYear="1954" box="[151,582,151,176]" class="Insecta" family="Heterothripidae" genus="Heterothrips" kingdom="Animalia" order="Thysanoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="angusticeps">
<emphasis id="69C2EA22FFCBEF7BC4F5FF31FDE5FF12" bold="true" box="[151,582,151,176]" pageId="8" pageNumber="9">
<emphasis id="69C2EA22FFCBEF7BC4F5FF31FE14FF12" bold="true" box="[151,439,151,176]" italics="true" pageId="8" pageNumber="9">Heterothrips angusticeps</emphasis>
<bibRefCitation id="3F274BC1FFCBEF7BC5DCFF3EFDE5FF12" author="Hood" box="[446,582,151,176]" pageId="8" pageNumber="9" refString="Hood, J. D. (1954) Brasilian Thysanoptera V. Proceedings of the Biological Society of Washington, 67, 195 - 214." type="journal article" year="1954">
Hood, 
<number id="ED7E2417FFCBEF7BC66CFF31FDE5FF12" box="[526,582,151,176]" pageId="8" pageNumber="9" value="1954.0">1954</number>
</bibRefCitation>
