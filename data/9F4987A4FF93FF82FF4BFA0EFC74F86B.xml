<taxonomicName id="D0E04D31FF92FF82FD64F84FFB2FF813" authority="Zheng, Wei &amp; Li, 2009" authorityName="Zheng, Wei &amp; Li" authorityYear="2009" box="[696,1161,1926,1950]" class="Insecta" family="Tetrigidae" genus="Criotettix" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="394" phylum="Arthropoda" rank="species" species="strivertex">
