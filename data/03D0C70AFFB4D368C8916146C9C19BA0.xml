<taxonomicName id="4C790D9FFFB6D368CD826554C8DB9BCC" class="Arachnida" family="Ascidae" genus="Antennoseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="perseus">
