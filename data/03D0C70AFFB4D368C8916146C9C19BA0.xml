<taxonomicName id="4C790D9FFFB6D368CBF6651CCC539B50" box="[1008,1124,800,824]" class="Arachnida" family="Ascidae" genus="Antennoseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="perseus">
