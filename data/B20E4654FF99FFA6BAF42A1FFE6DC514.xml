<paragraph id="3A18F742FF99FFA7BAF4293AFD14C486" blockId="50.[151,1436,473,682]" pageId="50" pageNumber="51">
<taxonomicName id="FDA78CC1FF99FFA7BAF4293AFEEDC443" baseAuthorityName="Portevin" baseAuthorityYear="1903" box="[151,354,596,620]" class="Insecta" family="Leiodidae" genus="Paulipalpina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="dispar">
