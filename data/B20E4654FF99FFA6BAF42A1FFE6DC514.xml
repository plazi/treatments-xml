<taxonomicName id="FDA78CC1FF99FFA7BBA3291DFD00C4A5" baseAuthorityName="Kirsch" baseAuthorityYear="1871" box="[448,655,627,651]" class="Insecta" family="Leiodidae" genus="Paulipalpina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="exigua">
