<materialsCitation id="8ACFFD1FFF99FFA7BAF429B6FE4CC5AD" ID-GBIF-Occurrence="2451575219" collectionCode="MNHN" collectorName="G. Portevin" country="Colombia" location="Colombie" pageId="50" pageNumber="51" specimenCount="3" typeStatus="holotype">
