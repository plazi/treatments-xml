<paragraph id="07B79D2FAB1DD1E3CE2476D51266E9FA" pageId="null" pageNumber="76">
<pageBreakToken id="FB7D80C18421B4C770B8A32058548C6C" pageId="null" pageNumber="76">Artengruppe</pageBreakToken>
der 
<taxonomicName id="5457A4278C1EB51CD465BB9DB80A008C" authority="Ehrh." class="Magnoliopsida" family="Boraginaceae" genus="Myosotis" kingdom="Plantae" order="Boraginales" pageId="null" pageNumber="76" phylum="Tracheophyta" rank="species" species="silvatica">
Myosotis 
<normalizedToken id="6C63C404F0AD60F3EF9A64DF1D6ED5B6" originalValue="silvática" pageId="null" pageNumber="76">silvatica</normalizedToken>
Ehrh.
</taxonomicName>
</paragraph>
