<taxonomicName family="Lacistemataceae" genus="Ichneumonoidea" lsidName="Ichneumonoidea" pageId="0" pageNumber="119" rank="family">Lacistemataceae</taxonomicName>
), and 
<taxonomicName class="Magnoliopsida" family="Lacistemataceae" genus="Lozania" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lozania pittieri" order="Malpighiales" pageId="0" pageNumber="119" phylum="Tracheophyta" rank="species" species="pittieri">
<emphasis italics="true" pageId="0" pageNumber="119">Lozania pittieri</emphasis>
</taxonomicName>
