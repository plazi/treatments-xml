<taxonomicName class="Magnoliopsida" family="Lacistemataceae" genus="Lacistema" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lacistema aggregatum" order="Malpighiales" pageId="0" pageNumber="119" phylum="Tracheophyta" rank="species" species="aggregatum">
<emphasis italics="true" pageId="0" pageNumber="119">Lacistema aggregatum</emphasis>
</taxonomicName>
