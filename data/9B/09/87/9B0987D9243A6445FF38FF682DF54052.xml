, 
<taxonomicName box="[1018,1184,1739,1763]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="verruculata">
<emphasis box="[1018,1184,1739,1763]" italics="true" pageId="2" pageNumber="91">H. verruculata</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[1249,1352,1740,1763]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="ramia">
<emphasis box="[1249,1352,1740,1763]" italics="true" pageId="2" pageNumber="91">H. ramia</emphasis>
</taxonomicName>
), 
<number box="[1377,1391,1740,1764]" pageId="2" pageNumber="91" value="4.0">4</number>
(
<taxonomicName class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="mongolica">
<emphasis italics="true" pageId="2" pageNumber="91">H. mongolica</emphasis>
</taxonomicName>
) or 
<number box="[316,361,1775,1799]" pageId="2" pageNumber="91" value="3.0" valueMax="4.0" valueMin="2.0">2–4</number>
, usually 
<number box="[462,476,1775,1799]" pageId="2" pageNumber="91" value="3.0">3</number>
(
<taxonomicName box="[493,636,1775,1799]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="oreophila">
<emphasis box="[493,636,1775,1799]" italics="true" pageId="2" pageNumber="91">H. oreophila</emphasis>
</taxonomicName>
) setae on the dens can be also easily distinguished from 
<taxonomicName box="[1277,1395,1776,1799]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="persica" status="sp. nov.">
<emphasis box="[1277,1395,1776,1799]" italics="true" pageId="2" pageNumber="91">H. persica</emphasis>
</taxonomicName>
<taxonomicNameLabel pageId="2" pageNumber="91" rank="species">
<emphasis bold="true" pageId="2" pageNumber="91">sp. nov.</emphasis>
</taxonomicNameLabel>
</paragraph>
