</taxonomicName>
, 
<taxonomicName box="[874,1002,1740,1763]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="capitata">
