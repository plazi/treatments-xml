<taxonomicName box="[380,497,1524,1547]" class="Entognatha" family="Hypogastruridae" genus="Hypogastrura" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="91" phylum="Arthropoda" rank="species" species="persica" status="sp. nov.">
<emphasis box="[380,497,1524,1547]" italics="true" pageId="2" pageNumber="91">H. persica</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[503,592,1523,1548]" pageId="2" pageNumber="91" rank="species">
<emphasis bold="true" box="[503,592,1523,1548]" pageId="2" pageNumber="91">sp. nov.</emphasis>
</taxonomicNameLabel>
having 
<number box="[682,696,1523,1548]" pageId="2" pageNumber="91" value="5.0">5</number>
