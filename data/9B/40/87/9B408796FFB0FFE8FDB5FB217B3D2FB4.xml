<emphasis italics="true" pageId="1" pageNumber="304">
ag 
<subScript attach="left" box="[1410,1421,1880,1899]" fontSize="8" pageId="1" pageNumber="304">1</subScript>
– ag 
<subScript attach="left" box="[183,194,1917,1936]" fontSize="8" pageId="1" pageNumber="304">3</subScript>
</emphasis>
) present; anal valves with two pairs of smooth anal setae (
<emphasis box="[977,1016,1909,1937]" italics="true" pageId="1" pageNumber="304">
ps 
<subScript attach="left" box="[1005,1016,1917,1936]" fontSize="8" pageId="1" pageNumber="304">1</subScript>
</emphasis>
and 
<emphasis box="[1080,1120,1909,1937]" italics="true" pageId="1" pageNumber="304">
ps 
<subScript attach="right" box="[1109,1120,1917,1936]" fontSize="8" pageId="1" pageNumber="304">2</subScript>
</emphasis>
), 
<emphasis box="[1149,1189,1909,1937]" italics="true" pageId="1" pageNumber="304">
ps 
<subScript attach="left" box="[1178,1189,1917,1936]" fontSize="8" pageId="1" pageNumber="304">1</subScript>
</emphasis>
