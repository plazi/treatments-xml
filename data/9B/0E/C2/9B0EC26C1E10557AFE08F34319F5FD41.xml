<quantity box="[876,1044,1523,1542]" metricMagnitude="-3" metricUnit="m" metricValue="6.88" metricValueMax="7.32" metricValueMin="6.44" pageId="1" pageNumber="280" unit="mm" value="6.88" valueMax="7.32" valueMin="6.44">6.44 to 7.32 mm</quantity>
