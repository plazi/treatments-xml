<emphasis box="[705,823,1428,1449]" italics="true" pageId="1" pageNumber="280">
<taxonomicName authorityName="Erwin" authorityYear="2000" box="[705,819,1428,1449]" class="Insecta" family="Carabidae" genus="Quammenis" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="280" phylum="Arthropoda" rank="species" species="spectabilis">spectabilis</taxonomicName>
