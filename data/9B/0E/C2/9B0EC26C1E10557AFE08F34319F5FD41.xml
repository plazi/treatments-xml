<quantity box="[313,454,1547,1566]" metricMagnitude="-2" metricUnit="m" metricValue="6.7564" metricValueMax="7.0104" metricValueMin="6.5024" pageId="1" pageNumber="280" unit="in" value="2.66" valueMax="2.76" valueMin="2.56">2.56 to 2.76 in</quantity>
