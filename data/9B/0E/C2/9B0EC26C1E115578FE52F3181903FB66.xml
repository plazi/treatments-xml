<taxonomicName authority="Laporte" authorityName="Laporte" authorityYear="1832" box="[594,804,1199,1220]" class="Insecta" family="Carabidae" genus="Stenocheila" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="280" phylum="Arthropoda" rank="genus">
