<taxonomicName box="[567,733,1555,1581]" class="Collembola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poduromorpha" pageId="8" pageNumber="215" phylum="Arthropoda" rank="order">Poduromorpha</taxonomicName>
