<subSubSection pageId="708" pageNumber="709" type="diversity">
<paragraph pageId="708" pageNumber="709">Diversity.</paragraph>
<paragraph pageId="708" pageNumber="709">Two allopatric species in temperate areas of North America.</paragraph>
</subSubSection>
<subSubSection pageId="708" pageNumber="709" type="identification">
