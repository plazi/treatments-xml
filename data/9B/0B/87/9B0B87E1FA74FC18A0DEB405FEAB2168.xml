<collectingCountry box="[359,426,1671,1697]" name="China" pageId="10" pageNumber="355">China</collectingCountry>
(
<collectingRegion box="[441,537,1671,1698]" country="China" name="Sichuan" pageId="10" pageNumber="355">Sichuan</collectingRegion>
), 
