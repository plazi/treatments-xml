<taxonomicName class="Dicotyledoneae" family="Ranunculaceae" genus="Cimicifuga" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Cimicifuga" order="Ranunculales" pageId="0" pageNumber="109" phylum="Angiospermae" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="109">Cimicifuga</emphasis>
