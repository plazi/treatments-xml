<taxonomicName authority="Hansf." authorityName="Hansf." box="[898,1196,1826,1852]" class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="aglaiicola">
<emphasis box="[898,1109,1826,1852]" italics="true" pageId="3" pageNumber="227">Meliola aglaiicola</emphasis>
Hansf.
</taxonomicName>
, 
<taxonomicName authority="Syd." authorityName="Syd." box="[1210,1452,1826,1852]" class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="aglaiae">
<emphasis box="[1210,1391,1826,1852]" italics="true" pageId="3" pageNumber="227">Meliola aglaiae</emphasis>
Syd.
</taxonomicName>
(synonymised to 
<taxonomicName authority="Syd." authorityName="Syd." box="[329,570,1862,1889]" class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="parvula">
<emphasis box="[329,510,1862,1888]" italics="true" pageId="3" pageNumber="227">Meliola parvula</emphasis>
Syd.
</taxonomicName>
), 
<taxonomicName authority="Hansf." authorityName="Hansf." box="[590,854,1862,1888]" class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="aglaina">
<emphasis box="[590,770,1862,1888]" italics="true" pageId="3" pageNumber="227">Meliola aglaina</emphasis>
Hansf.
</taxonomicName>
, 
<taxonomicName authority="Jana et al." authorityName="Jana" box="[864,1237,1862,1888]" class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="aglaiae-edulis">
<emphasis box="[864,1118,1862,1888]" italics="true" pageId="3" pageNumber="227">Meliola aglaiae-edulis</emphasis>
Jana 
<emphasis box="[1182,1231,1862,1888]" italics="true" pageId="3" pageNumber="227">et al</emphasis>
.
</taxonomicName>
, 
<taxonomicName authority="Syd." authorityName="Syd." class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="obvallata">
<emphasis box="[1252,1452,1862,1888]" italics="true" pageId="3" pageNumber="227">Meliola obvallata</emphasis>
Syd.
</taxonomicName>
, 
<taxonomicName authority="Hosag. &amp; Archana" authorityName="Hosag. &amp; Archana" box="[201,673,1898,1924]" class="Sordariomycetes" family="Meliolaceae" genus="Meliola" kingdom="Fungi" order="Meliolales" pageId="3" pageNumber="227" phylum="Ascomycota" rank="species" species="sairandhriana">
