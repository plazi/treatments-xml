<taxonomicName authorityName="Fall" authorityYear="1917" class="Hexapoda" family="Carabidae" genus="Cicindela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="244" pageNumber="245" phylum="Arthropoda" rank="subSpecies" species="tranquebarica" subSpecies="inyo">
