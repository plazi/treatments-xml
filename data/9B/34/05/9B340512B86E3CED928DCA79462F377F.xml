<paragraph pageId="38" pageNumber="39">
<pageBreakToken pageId="38" pageNumber="39" start="start">Tribe</pageBreakToken>
<taxonomicName family="Mymaridae" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="38" pageNumber="39" rank="tribe" tribe="Triadomerini">Triadomerini</taxonomicName>
<taxonomicNameLabel pageId="38" pageNumber="39">stat. n.</taxonomicNameLabel>
