<taxonomicName authority="Pall-Gergely" authorityName="Pall-Gergely" authorityYear="2019" box="[945,1310,189,213]" class="Gastropoda" family="Camaenidae" genus="Entadella" kingdom="Animalia" order="Stylommatophora" pageId="6" pageNumber="700" phylum="Mollusca" rank="species" species="kilchomani" status="new species">
<emphasis box="[945,1170,189,213]" italics="true" pageId="6" pageNumber="700">Entadella kilchomani</emphasis>
