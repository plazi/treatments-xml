<paragraph pageId="443" pageNumber="444">
<taxonomicName authorityName="Dejean" authorityYear="1831" class="Insecta" family="Carabidae" genus="Bembidium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="443" pageNumber="444" phylum="Arthropoda" rank="species" species="pulicarium">Bembidium pulicarium</taxonomicName>
