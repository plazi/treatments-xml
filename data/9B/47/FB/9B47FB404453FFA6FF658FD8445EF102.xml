<geoCoordinate degrees="77" direction="west" minutes="53.4" orientation="longitude" pageId="7" pageNumber="360" precision="92" value="-77.89">W 77°53.4’</geoCoordinate>
, 
<quantity box="[266,357,1511,1536]" metricMagnitude="3" metricUnit="m" metricValue="2.163" pageId="7" pageNumber="360" unit="m" value="2163.0">2163 m</quantity>
, host collected 
<date box="[541,709,1511,1536]" pageId="7" pageNumber="360" value="2010-03-04">4 March 2010</date>
, as parasitoid of 
<taxonomicName box="[913,1017,1511,1536]" class="Insecta" family="Erebidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="353" phylum="Arthropoda" rank="subFamily" subFamily="Arctiinae">Arctiinae</taxonomicName>
larva 3 
<superScript attach="left" box="[1108,1120,1508,1522]" fontSize="6" pageId="7" pageNumber="360">th</superScript>
instar on 
<taxonomicName authority="(Hieron.) C. Jeffrey" authorityName="C. Jeffrey" baseAuthorityName="Hieron." class="Magnoliopsida" family="Asteraceae" genus="Dendrophorbium" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="360" phylum="Tracheophyta" rank="species" species="iloense">
