. 
<taxonomicName authority="Steel, Moens, Scholaert, Boshoff, Houthoofd &amp; Bert, 2011" authorityName="Steel, Moens, Scholaert, Boshoff, Houthoofd &amp; Bert" authorityYear="2011" box="[285,1228,1925,1947]" class="Secernentea" family="Diplogasteridae" genus="Mononchoides" kingdom="Animalia" order="Diplogasterida" pageId="3" pageNumber="144" phylum="Nematoda" rank="species" species="composticola">
