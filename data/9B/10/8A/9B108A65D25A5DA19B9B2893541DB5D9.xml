<geoCoordinate degrees="55" direction="east" minutes="22" orientation="longitude" precision="1" seconds="38.1" value="55.377247">55°22'38.1&quot;E</geoCoordinate>
) and Saint-Joseph, Langevin (
<geoCoordinate degrees="21" direction="south" minutes="22" orientation="latitude" precision="1" seconds="53.4" value="-21.3815">21°22'53.4&quot;S</geoCoordinate>
, 
<geoCoordinate degrees="55" direction="east" minutes="38" orientation="longitude" precision="1" seconds="48.4" value="55.64678">55°38'48.4&quot;E</geoCoordinate>
