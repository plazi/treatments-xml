<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia melanoxylon" order="Fabales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="melanoxylon">
