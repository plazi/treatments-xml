<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:58DF1FD7-7471-4E34-9657-7C6DC45AD79A" class="Actinopterygii" family="Gobiidae" genus="Elacatinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Gobiosoma Elacatinus atronasum Böhlke &amp; Robins 1968:88" order="Perciformes" pageNumber="4" phylum="Chordata" rank="species" species="atronasum">
