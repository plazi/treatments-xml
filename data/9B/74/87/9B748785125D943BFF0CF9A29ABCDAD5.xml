on left (
<number box="[833,845,872,896]" pageId="33" pageNumber="34" value="1.0">1</number>
), or 
<number box="[901,915,872,896]" pageId="33" pageNumber="34" value="3.0">3</number>
on left and 
<number box="[1051,1065,872,896]" pageId="33" pageNumber="34" value="4.0">4</number>
on right (
<number box="[1178,1194,872,896]" pageId="33" pageNumber="34" value="2.0">2</number>
