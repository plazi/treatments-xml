<taxonomicName id="65B94D44FFBAFFE3E82FFCCFF139140E" authorityName="Bale" authorityYear="1914" box="[537,834,821,844]" class="Hydrozoa" family="Syntheciidae" genus="Synthecium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="15" pageNumber="16" phylum="Cnidaria" rank="species" species="subventricosum">
Synthecium 
