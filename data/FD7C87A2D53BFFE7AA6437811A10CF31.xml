<taxonomicName id="B2D54D37D53BFFE7A9BB308A1D31CEA9" authority="Evans, 1961" authorityName="Evans" authorityYear="1961" box="[727,1063,1719,1745]" class="Insecta" family="Bethylidae" genus="Pseudisobrachium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="rettenmeyeri">
