<taxonomicName id="9DA54D3EAF584847D981A24FFD14FDD8" authority="Brolemann, 1926" authorityName="Brolemann" authorityYear="1926" box="[189,692,520,546]" class="Not" family="Scolopendrellidae" genus="Ribautiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Symphyla" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="zagnanadina">
