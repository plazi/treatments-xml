, 
<collectingCountry id="F32276096B7BFFD4419FE3B7FDE04BCA" box="[463,533,426,452]" name="China" pageId="5" pageNumber="44">China</collectingCountry>
(
<geoCoordinate id="EE01505E6B7BFFD44275E3B7FD754BCA" box="[549,640,426,452]" degrees="29" direction="north" minutes="5" orientation="latitude" pageId="5" pageNumber="44" precision="925" value="29.083334">29º5' N</geoCoordinate>
, 
<geoCoordinate id="EE01505E6B7BFFD442DCE3B7FD044BCA" box="[652,753,426,452]" degrees="106" direction="east" minutes="5" orientation="longitude" pageId="5" pageNumber="44" precision="925" value="106.083336">106º5' E</geoCoordinate>
)
