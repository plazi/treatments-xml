<heading id="200781CBFF8DFFD8FF3C540417DEDD5B" bold="true" box="[151,653,1606,1632]" fontSize="11" level="1" pageId="14" pageNumber="97" reason="1">
<taxonomicName id="BCF04D24FF8DFFD8FF3C540417DEDD5B" authority="Fittkau 1962" authorityName="Fittkau" authorityYear="1962" box="[151,653,1606,1632]" class="Insecta" family="Chironomidae" genus="Macropelopia" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="97" phylum="Arthropoda" rank="species" species="paranebulosa">
<emphasis id="4984EAB5FF8DFFD8FF3C540417DEDD5B" bold="true" box="[151,653,1606,1632]" pageId="14" pageNumber="97">
<emphasis id="4984EAB5FF8DFFD8FF3C540414B4DD5B" bold="true" box="[151,487,1606,1632]" italics="true" pageId="14" pageNumber="97">Macropelopia paranebulosa</emphasis>
<bibRefCitation id="1F614B56FF8DFFD8FE45540417DEDD5B" author="Fittkau" box="[494,653,1606,1632]" pageId="14" pageNumber="97" refString="Fittkau, E. J. (1962) Die Tanypodinae (Diptera, Chironomidae) (die Tribus Anatopyniini, Macropelopiini und Pentaneurini). Abhandlungen zur Larvalsystematik der Insecten, 6, 1 - 453." type="journal article" year="1962">
Fittkau 
<number id="CD382480FF8DFFD8FDF9540417DEDD5B" box="[594,653,1606,1632]" pageId="14" pageNumber="97" value="1962.0">1962</number>
</bibRefCitation>
