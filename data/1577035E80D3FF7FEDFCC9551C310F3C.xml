<quantity id="5A261FAD80DCFF7FEC65CEB1183E0ED3" box="[270,355,152,176]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.8" metricValueMin="0.3" pageId="216" pageNumber="217" unit="m" value="10.5" valueMax="18.0" valueMin="3.0">3–18 m</quantity>
