<paragraph id="B2B632AEBE410ED98AB06DCAE654907F" pageId="84" pageNumber="85">
<taxonomicName id="FE078E46F82C3C2AA246D6039583FC11" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus anae" order="Hymenoptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="anae">Euplectrus anae</taxonomicName>
