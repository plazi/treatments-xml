<taxonomicName id="04B211ABC943FF885E67FCF0FDEF1914" baseAuthorityName="Dybowski" baseAuthorityYear="1872" box="[367,518,783,806]" class="Actinopterygii" family="Bagridae" genus="Pseudobagrus" kingdom="Animalia" order="Siluriformes" pageId="33" pageNumber="34" phylum="Chordata" rank="species" species="ussuriensis">
