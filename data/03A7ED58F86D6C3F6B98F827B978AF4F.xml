<taxonomicName id="4C0E27CDF86A6C3F6A96FF38B919AEE2" box="[409,625,151,176]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="danieli">
