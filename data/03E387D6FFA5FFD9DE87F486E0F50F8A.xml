, 
<taxonomicName id="4C4A4D43FFA5FFD9DE29F3E6E0F10F8A" authority="(S. Moricand, 1841)" baseAuthorityName="S. Moricand" baseAuthorityYear="1841" box="[831,1409,1481,1504]" class="Gastropoda" family="Simpulopsidae" genus="Leiostracus" kingdom="Animalia" order="Stylommatophora" pageId="9" pageNumber="66" phylum="Mollusca" rank="species" species="cinnamomeolineatus">
