<taxonomicName id="4C6B6DB932175C52FDF6D417AEFBFAA1" baseAuthorityName="Hendel" baseAuthorityYear="1915" box="[553,691,1314,1338]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="14" phylum="Arthropoda" rank="species" species="latifrons">
