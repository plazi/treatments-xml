<emphasis id="B957EADB2B623221FF05FB78FD1AF0EE" bold="true" box="[151,661,1240,1267]" pageId="5" pageNumber="362">
Genus 
<taxonomicName id="4C234D4A2B623221FF7CFB79FD1AF0EE" authority="A. Milne-Edwards, 1875" authorityName="A. Milne-Edwards" authorityYear="1875" box="[238,661,1240,1267]" class="Malacostraca" family="Epialtidae" genus="Rochinia" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="362" phylum="Arthropoda" rank="genus">
<emphasis id="B957EADB2B623221FF7CFB79FED2F0EF" bold="true" box="[238,349,1240,1266]" italics="true" pageId="5" pageNumber="362">Rochinia</emphasis>
A. Milne-Edwards, 
<number id="3DEB24EE2B623221FDC8FB78FD1AF0EE" box="[602,661,1241,1267]" pageId="5" pageNumber="362" value="1875.0">1875</number>
</taxonomicName>
