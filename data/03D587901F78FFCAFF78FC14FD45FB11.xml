<collectingCountry id="F36B76161F78FFCAFC9FFBD1FC92FB3C" box="[836,895,1140,1162]" name="Israel" pageId="19" pageNumber="24">Israel</collectingCountry>
, 
<collectingCountry id="F36B76161F78FFCAFC57FBD1FC1CFB3C" box="[908,1009,1140,1162]" name="Jordan" pageId="19" pageNumber="24">Jordanien</collectingCountry>
