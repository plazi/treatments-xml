<quantity id="89699B68FF9BFFAE012DEC3EFD74CA2C" box="[524,648,582,606]" metricMagnitude="-2" metricUnit="m" metricValue="5.15" pageId="22" pageNumber="3971" unit="mm" value="51.5">51.50 mm</quantity>
