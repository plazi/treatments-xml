<quantity id="89699B68FF9BFFAE00A0EC5EFC01CA4C" box="[897,1021,550,574]" metricMagnitude="-2" metricUnit="m" metricValue="5.175" pageId="22" pageNumber="3971" unit="mm" value="51.75">51.75 mm</quantity>
