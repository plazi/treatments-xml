<emphasis id="A1E8EAB4FFE7FFDDFF33C5D3FE0FE1B2" bold="true" box="[151,483,1750,1777]" pageId="3" pageNumber="182">
<taxonomicName id="549C4D25FFE7FFDDFF33C5D3FE92E1B2" authority="Neto &amp; Passos, 2020" authorityName="Neto &amp; Passos" authorityYear="2020" box="[151,382,1750,1777]" class="Insecta" family="Hydroptilidae" genus="Neotrichia" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="species" species="cauame" status="sp. nov.">
