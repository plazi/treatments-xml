, 
<taxonomicName id="B236F12203A15BEA409FF48CB933CEAC" authority="rhodolarynx" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="40" pageNumber="41" phylum="Mollusca" rank="subSpecies" species="rhodolarynx" subSpecies="rhodolarynx">
