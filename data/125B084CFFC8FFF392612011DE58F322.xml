<paragraph id="9A4DB95AFFC8FFF3926120C2DE58F322" blockId="20.[264,1323,463,569]" pageId="20" pageNumber="21">
A 
<typeStatus id="454907F8FFC8FFF3924E20C2DDD6F0F2" box="[295,453,463,489]" pageId="20" pageNumber="21">paralectotype</typeStatus>
of 
<taxonomicName id="5DF2C2D9FFC8FFF3929B20C2DE67F0F3" box="[498,628,463,488]" class="Insecta" family="Chironomidae" genus="Camptocladius" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="globifer">
<emphasis id="A8866548FFC8FFF3929B20C2DE67F0F3" box="[498,628,463,488]" italics="true" pageId="20" pageNumber="21">C. globifer</emphasis>
</taxonomicName>
