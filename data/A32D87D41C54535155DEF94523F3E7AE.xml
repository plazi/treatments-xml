<quantity id="EC7C9B271C54536E505DF8282678E7B6" box="[1300,1387,1970,1997]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="31" pageNumber="32" unit="m" value="1500.0">1500 m</quantity>
