<taxonomicName id="EC844D411C6B5351541FFB3E2127E4C7" authorityName="Felt" authorityYear="1927" box="[342,564,1188,1212]" class="Insecta" family="Cecidomyiidae" genus="Gynodiplosis" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="humata">
