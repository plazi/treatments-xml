<taxonomicName id="52A84FC27EACBCF7529C52A9695C98E8" LSID="http://zoobank.org/8E3CFC0E-4741-40B7-8736-04AD48B28E51" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus levis" order="Hymenoptera" pageId="105" pageNumber="106" phylum="Arthropoda" rank="species" species="levis">Heterospilus levis Marsh</taxonomicName>
