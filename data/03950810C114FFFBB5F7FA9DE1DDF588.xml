<taxonomicName id="4C3CC285C115FFFBB702FC3FE00CF3BE" authorityName="Melsheimer" authorityYear="1844" box="[633,854,914,935]" class="Insecta" family="Cryptophagidae" genus="Antherophagus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="20" phylum="Arthropoda" rank="species" species="ochraceus">Antherophagus ochraceus</taxonomicName>
