<taxonomicName id="66AB4D7AFFD2FFC28CC9FC90FEFAA06A" box="[226,265,831,852]" class="Anthozoa" family="Isididae" genus="Isis" kingdom="Animalia" order="Alcyonacea" pageId="54" pageNumber="55" phylum="Cnidaria" rank="genus">
