<subSubSection id="C31D4381FFF9FFB370B69D37FC16F8AA" box="[199,959,1836,1862]" pageId="2" pageNumber="65" type="etymology">
<paragraph id="8BB8100AFFF9FFB370B69D37FC16F8AA" blockId="2.[151,1436,1636,2028]" box="[199,959,1836,1862]" pageId="2" pageNumber="65">
<emphasis id="B973CC18FFF9FFB370B69D37FEFAF8AA" bold="true" box="[199,339,1836,1862]" pageId="2" pageNumber="65">Etymology.</emphasis>
