<quantity id="F7289B51FFB1672CA20A93BBFD76F833" box="[578,663,1931,1955]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="558" unit="mm" value="20.0">20 mm</quantity>
