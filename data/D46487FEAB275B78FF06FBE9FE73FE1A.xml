<quantity id="9B359B0DAB275B7BFE6EFA71FDDEFA59" box="[511,619,1443,1467]" metricMagnitude="-3" metricUnit="m" metricValue="1.39" pageId="25" pageNumber="176" unit="mm" value="1.39">1.39 mm</quantity>
