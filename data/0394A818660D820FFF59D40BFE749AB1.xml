<taxonomicName id="4C3D628D660D8210FF59D40BFD869C6C" authority="Adams, 1860" authorityName="Adams" authorityYear="1860" box="[151,556,1837,1863]" class="Rhynchonellata" family="Dallinidae" genus="Campages" kingdom="Animalia" order="Terebratulida" pageId="15" pageNumber="16" phylum="Brachiopoda" rank="species" species="mariae">
<emphasis id="B949C51C660D8210FF59D40BFD869C6C" bold="true" box="[151,556,1837,1863]" pageId="15" pageNumber="16">
<emphasis id="B949C51C660D8210FF59D40BFEDB9C6C" bold="true" box="[151,369,1837,1863]" italics="true" pageId="15" pageNumber="16">Campages mariae</emphasis>
(Adams, 
<number id="3DF50B29660D8210FE26D40BFD8E9C6C" box="[488,548,1837,1863]" pageId="15" pageNumber="16" value="1860.0">1860</number>
)
