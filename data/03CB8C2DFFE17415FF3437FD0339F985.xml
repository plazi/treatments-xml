<paragraph id="8BDD3D3BFFE17415FF6632DD06D3FB1B" blockId="1.[151,1437,694,1648]" pageId="1" pageNumber="31">
<emphasis id="B916E129FFE17415FF6632DD0327FB55" bold="true" box="[197,364,1158,1184]" pageId="1" pageNumber="31">Host plants.</emphasis>
<taxonomicName id="4C6246B8FFE17415FED632DD03B4FB55" box="[373,511,1158,1184]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="1" pageNumber="31" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
