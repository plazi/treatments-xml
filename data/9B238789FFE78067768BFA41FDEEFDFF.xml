<taxonomicName id="D48A4D1CFFE78078768BFA41FDB5FA02" authority="Kroyer, 1863" authorityName="Kroyer" authorityYear="1863" box="[151,532,1447,1473]" class="Maxillopoda" family="Caligidae" genus="Caligus" kingdom="Animalia" order="Siphonostomatoida" pageId="15" pageNumber="36" phylum="Arthropoda" rank="species" species="stromatei">
<emphasis id="21FEEA8DFFE78078768BFA41FDB5FA02" bold="true" box="[151,532,1447,1473]" pageId="15" pageNumber="36">
<emphasis id="21FEEA8DFFE78078768BFA41FECDFA02" bold="true" box="[151,364,1447,1473]" italics="true" pageId="15" pageNumber="36">Caligus stromatei</emphasis>
<bibRefCitation id="771B4B6EFFE78078776FFA41FDB5FA02" author="Kroyer" box="[371,532,1447,1473]" pageId="15" pageNumber="36" refString="Kroyer, H. (1863) Bidrag til Kundskab om Snyltekrebsene. Naturhistorisk Tidsskrift, series 3, 2, 75 - 320, pls 1 - 9." type="journal article" year="1863">
Krøyer, 
<number id="A54224B8FFE7807877C6FA41FDB5FA02" box="[474,532,1447,1473]" pageId="15" pageNumber="36" value="1863.0">1863</number>
</bibRefCitation>
