</emphasis>
<taxonomicName id="4C584D42FFC9843FFEADFA51FD33BF1F" authority="Butler, 1868" authorityName="Butler" authorityYear="1868" box="[299,746,1521,1543]" class="Insecta" family="Nymphalidae" genus="Stuardosatyrus" kingdom="Animalia" order="Lepidoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="williamsianus">
