<paragraph id="8BC43691FFB8B348FF3B4D6BFD20FBA0" blockId="90.[151,1437,1107,1347]" box="[151,753,1107,1132]" pageId="90" pageNumber="91">
<emphasis id="B90FEA83FFB8B348FF3B4D6BFF31FBA7" bold="true" box="[151,224,1107,1132]" inLineHeading="true" pageId="90" pageNumber="91" reason="1">Color.</emphasis>
