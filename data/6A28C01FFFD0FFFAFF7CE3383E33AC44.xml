<taxonomicName id="25810A8AFFD1FFF8FEA2E2F53D0EAAAD" baseAuthorityName="Valenciennes" baseAuthorityYear="1840" box="[281,559,1983,2007]" class="Actinopterygii" family="Loricariidae" genus="Pseudorinelepis" kingdom="Animalia" order="Siluriformes" pageId="1" pageNumber="194" phylum="Chordata" rank="species" species="genibarbis">
