<paragraph id="8BCF36A83F60FFA49AE8F9F974399B63" blockId="6.[144,382,1559,1584]" box="[144,382,1559,1584]" pageId="6" pageNumber="2672">
<emphasis id="B904EABA3F60FFA49AE8F9F9745B9B7C" box="[144,284,1560,1584]" italics="true" pageId="6" pageNumber="2672">Body length.</emphasis>
<quantity id="4C889B4D3F60FFA49B5AF9F6743D9B63" box="[290,378,1559,1583]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="6" pageNumber="2672" unit="mm" value="4.5">4.5 mm</quantity>
