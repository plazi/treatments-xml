<taxonomicName id="6DFD27A9BF75FFC95A4BFEFB36664A59" authority="(Harris) Reaper Dart" authorityName="Reaper Dart" baseAuthorityName="Harris" box="[308,776,283,310]" class="Insecta" family="Noctuidae" genus="Euxoa" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="messoria">
