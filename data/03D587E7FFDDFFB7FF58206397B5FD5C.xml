<taxonomicNameLabel id="A23B5798FFC2FFB6FF1224A49733FA0F" box="[198,261,1470,1496]" pageId="16" pageNumber="17" rank="species">sp. n.</taxonomicNameLabel>
, 
<taxonomicName id="4C7C4D72FFC2FFB6FEC624A7942AFA00" authority="Curletti &amp; Sakalian, 2009" authorityName="Curletti &amp; Sakalian" authorityYear="2009" box="[274,540,1469,1496]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="penevi" status="sp. n." subGenus="Paralophotus">
