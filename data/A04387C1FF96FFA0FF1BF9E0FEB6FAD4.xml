<heading id="731D81BBFF96FFA6FF1BF9E0FD90F9AC" bold="true" box="[151,547,1554,1581]" fontSize="11" level="1" pageId="18" pageNumber="469" reason="1">
<taxonomicName id="EFEA4D54FF96FFA6FF1BF9E0FD90F9AC" authority="Neboiss 1984" authorityName="Neboiss" authorityYear="1984" box="[151,547,1554,1581]" class="Insecta" family="Calocidae" genus="Caenota" kingdom="Animalia" order="Trichoptera" pageId="18" pageNumber="469" phylum="Arthropoda" rank="species" species="nemorosa">
<emphasis id="1A9EEAC5FF96FFA6FF1BF9E0FD90F9AC" bold="true" box="[151,547,1554,1581]" pageId="18" pageNumber="469">
<emphasis id="1A9EEAC5FF96FFA6FF1BF9E0FEC9F9AD" bold="true" box="[151,378,1554,1580]" italics="true" pageId="18" pageNumber="469">Caenota nemorosa</emphasis>
<bibRefCitation id="4C7B4B26FF96FFA6FE0CF9E1FD90F9AC" author="Neboiss" box="[384,547,1555,1581]" pageId="18" pageNumber="469" refString="Neboiss, A. (1984) Calocidae of northern Queensland (Calocidae: Trichoptera). In: Morse, J. C. (Ed.), Proceedings of the 4 th International Symposium on Trichoptera, Clemson, South Carolina. Dr W. Junk Publishers, The Hague, pp. 267 - 276." type="book chapter" year="1984">
Neboiss 
<number id="9E2224F0FF96FFA6FE64F9E1FD90F9AC" box="[488,547,1555,1581]" pageId="18" pageNumber="469" value="1984.0">1984</number>
</bibRefCitation>
