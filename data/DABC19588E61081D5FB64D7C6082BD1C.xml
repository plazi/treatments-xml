; 
<taxonomicName id="6FFE78194AC5B1A9B056FFF31AF94FD2" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina lillae" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="lillae">Montenegrina lillae</taxonomicName>
