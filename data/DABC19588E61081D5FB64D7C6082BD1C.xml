<taxonomicName id="BF6C660C5B803E2986B3CFEF4AE52EF8" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina minuscula" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="minuscula">Montenegrina minuscula</taxonomicName>
