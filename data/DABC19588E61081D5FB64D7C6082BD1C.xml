<taxonomicName id="562BC01283F8ACFA2C21DF9160C0FCED" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina minuscula" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="minuscula">Montenegrina minuscula</taxonomicName>
. 
<taxonomicName id="7D4A60752B22C2344045B1A12ABEF671" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina janinensis" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="janinensis">Montenegrina janinensis</taxonomicName>
(circle); 
<taxonomicName id="F509B6F94603936CD2FE4F22CD899CB2" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina lillae" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="lillae">Montenegrina lillae</taxonomicName>
