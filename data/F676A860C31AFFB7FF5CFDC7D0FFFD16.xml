<taxonomicName id="B9DF62F5C31AFFB7FF5CFDB9D2E4FDD3" baseAuthorityName="BOHEMAN" baseAuthorityYear="1847" box="[178,411,616,637]" class="Insecta" family="Cicadellidae" genus="Eupteryx" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="848" phylum="Arthropoda" rank="species" species="signatipennis">
