<paragraph id="8BC436F9FF87E46DA8B5575542F3FA96" blockId="39.[151,1437,1230,1507]" pageId="39" pageNumber="40">
<emphasis id="B90FEAEBFF87E46DA8B557554049FAB2" bold="true" box="[199,330,1338,1363]" pageId="39" pageNumber="40">Leaf mine.</emphasis>
<collectingRegion id="49BFF81BFF87E46DA92157544064FAB2" box="[339,359,1339,1363]" country="Nigeria" name="Rivers" pageId="39" pageNumber="40">A</collectingRegion>
