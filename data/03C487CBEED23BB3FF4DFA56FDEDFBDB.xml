<taxonomicName id="4C6D4D5EEED43BB0FEF3F8BAFDD3F84C" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[310,534,1907,1933]" class="Maxillopoda" family="Notodelphyidae" genus="Doropygus" kingdom="Animalia" order="Cyclopoida" pageId="389" pageNumber="390" phylum="Arthropoda" rank="species" species="callosus" status="sp. nov.">
