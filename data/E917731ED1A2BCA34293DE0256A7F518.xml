<taxonomicName id="CB9BA020FEB8EFE816A4087E0A00ED2C" LSID="http://zoobank.org/48016D68-2999-44B6-868C-A62DCFF3F851" authority="Peng &amp; Li" class="Insecta" family="Staphylinidae" genus="Lathrobium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lathrobium kuan" order="Coleoptera" pageId="4" pageNumber="117" phylum="Arthropoda" rank="species" species="kuan">Lathrobium kuan Peng &amp; Li</taxonomicName>
