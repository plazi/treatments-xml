<taxonomicName id="9979B1A3AB7AD862AA3701493956C1CA" class="Insecta" family="Staphylinidae" genus="Scaphisoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scaphisoma repandum" order="Coleoptera" pageId="4" pageNumber="243" phylum="Arthropoda" rank="species" species="repandum">Scaphisoma repandum</taxonomicName>
