<paragraph id="8BE336C8FFD5FFC2FF5E5BE0FE486390" blockId="1.[151,440,1697,1758]" box="[151,440,1697,1722]" pageId="1" pageNumber="400">
<heading id="D0AB81A4FFD5FFC2FF5E5BE0FE486390" bold="true" box="[151,440,1697,1722]" fontSize="10" level="2" pageId="1" pageNumber="400" reason="2">
<taxonomicName id="4C5C4D4BFFD5FFC2FF5E5BE0FE486390" authority="Forster" authorityName="Forster" box="[151,440,1697,1722]" class="Insecta" family="Geometridae" genus="Cidaria" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="400" phylum="Arthropoda" rank="species" species="fulvata">
