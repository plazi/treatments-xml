<number id="3DFB774E5E3B895AFD4C1C4EFD13F9C8" box="[700,752,1580,1604]" pageId="7" pageNumber="38" value="1.85">1.85</number>
. Carapace 
<number id="3DFB774E5E3B895AFC9F1C4EFC43F9C8" box="[879,928,1580,1604]" pageId="7" pageNumber="38" value="0.93">0.93</number>
long, 
<number id="3DFB774E5E3B895AFC1B1C4EFBFFF9C8" box="[1003,1052,1580,1604]" pageId="7" pageNumber="38" value="0.75">0.75</number>
