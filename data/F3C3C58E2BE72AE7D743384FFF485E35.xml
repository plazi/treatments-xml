<taxonomicName id="319647E0C6BA2186A51A607FB1B67502" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230401" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus vividus (Smith)" lsidName-HNS="Camponotus vividus (Smith)" order="Hymenoptera" pageNumber="264" phylum="Arthropoda" rank="species" species="vividus" subGenus="Orthonotomyrmex">Camponotus (Orthonotomyrmex) vividus Sm.</taxonomicName>
(syn. 
<taxonomicName id="33793C9B130B1E921B4540E755ED7FA5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136290" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus meinerti Forel" lsidName-HNS="Camponotus meinerti Forel" order="Hymenoptera" pageNumber="264" phylum="Arthropoda" rank="species" species="meinerti">Meinerti</taxonomicName>
