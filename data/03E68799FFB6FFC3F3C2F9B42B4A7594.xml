/ 
<taxonomicName id="4C4F4D0CFFB5FFC3F034FF412A3874EB" authority="Schmidt" authorityName="Schmidt" authorityYear="1926" box="[865,1072,188,212]" class="Insecta" family="Derbidae" genus="Decora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="86" pageNumber="86" phylum="Arthropoda" rank="species" species="biermani">Biermani Schmidt</taxonomicName>
