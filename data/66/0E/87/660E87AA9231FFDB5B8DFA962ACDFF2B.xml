<taxonomicName authority="Thomson, 1880" authorityName="Thomson" authorityYear="1880" box="[1088,1436,1410,1435]" class="Insecta" family="Cetoniidae" genus="Tapinoschema" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="18" phylum="Arthropoda" rank="genus">
