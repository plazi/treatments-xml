, 
<collectionCode box="[151,238,1378,1402]" pageId="8" pageNumber="501">QMBA</collectionCode>
, 
<collectionCode LSID="urn:lsid:biocol.org:col:34982" box="[249,333,1378,1402]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34982" name="South Australia Museum" pageId="8" pageNumber="501">SAMA</collectionCode>
