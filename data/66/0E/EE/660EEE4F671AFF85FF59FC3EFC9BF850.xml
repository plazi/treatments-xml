<collectionCode LSID="urn:lsid:biocol.org:col:32981" box="[704,775,1342,1366]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="8" pageNumber="501">ANIC</collectionCode>
