<geoCoordinate box="[1187,1287,1852,1879]" direction="north" orientation="latitude" pageId="17" pageNumber="88" precision="925" value="57.35">57°21’N</geoCoordinate>
, 
<geoCoordinate box="[1299,1396,1853,1879]" direction="east" orientation="longitude" pageId="17" pageNumber="88" precision="925" value="66.05">66°03’E</geoCoordinate>
, 
<date pageId="17" pageNumber="88" value="2018-04-27" valueMax="2018-09-06" valueMin="2018-04-27">27 April and 06 September 2018</date>
O. Joharchi coll., on 
<taxonomicName box="[724,814,1889,1915]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="88" phylum="Arthropoda" rank="genus">
<emphasis box="[724,814,1889,1915]" italics="true" pageId="17" pageNumber="88">Bombus</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[867,1026,1889,1915]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="88" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
