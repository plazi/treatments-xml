<specimenCount box="[424,551,1944,1971]" pageId="2" pageNumber="73" type="female">14 females</specimenCount>
and one male, 
<collectingCountry box="[742,844,1945,1971]" name="Russia" pageId="2" pageNumber="73">RUSSIA</collectingCountry>
, 
<collectingRegion box="[860,1065,1944,1971]" country="Russia" name="Tyumen'" pageId="2" pageNumber="73">Tyumen Province</collectingRegion>
, vicinity of Uspenka, 
<geoCoordinate box="[1332,1432,1944,1971]" direction="north" orientation="latitude" pageId="2" pageNumber="73" precision="925" value="57.066666">57°04’N</geoCoordinate>
, 
<geoCoordinate box="[151,248,1980,2007]" direction="east" orientation="longitude" pageId="2" pageNumber="73" precision="925" value="65.066666">65°04’E</geoCoordinate>
, 
<date box="[259,411,1981,2007]" pageId="2" pageNumber="73" value="2018-05-21">21 May 2018</date>
, O. Joharchi coll., in the nest of 
<taxonomicName box="[772,934,1981,2007]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="73" phylum="Arthropoda" rank="species" species="fusca">
<emphasis box="[772,934,1981,2007]" italics="true" pageId="2" pageNumber="73">Formica fusca</emphasis>
</taxonomicName>
L. (
<taxonomicName box="[979,1138,1981,2007]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="73" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
