<taxonomicName authority="Bucquoy, Dautzenberg &amp; Dollfus, 1883" authorityName="Bucquoy, Dautzenberg &amp; Dollfus" authorityYear="1883" box="[238,868,1266,1292]" class="Gastropoda" family="Pyramidellidae" genus="Parthenina" kingdom="Animalia" pageId="13" pageNumber="52" phylum="Mollusca" rank="genus">
<emphasis bold="true" box="[238,375,1266,1292]" italics="true" pageId="13" pageNumber="52">Parthenina</emphasis>
Bucquoy, Dautzenberg &amp; Dollfus, 
<number box="[810,868,1266,1292]" pageId="13" pageNumber="52" value="1883.0">1883</number>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="13.[151,1326,1340,1363]" box="[151,1326,1340,1363]" pageId="13" pageNumber="52">
<taxonomicName authority="Bucquoy, Dautzenberg &amp; Dollfus, 1883" authorityName="Bucquoy, Dautzenberg &amp; Dollfus" authorityYear="1883" box="[151,704,1340,1363]" class="Gastropoda" family="Pyramidellidae" genus="Parthenina" kingdom="Animalia" pageId="13" pageNumber="52" phylum="Mollusca" rank="genus">
<emphasis box="[151,271,1340,1363]" italics="true" pageId="13" pageNumber="52">Parthenina</emphasis>
Bucquoy, Dautzenberg &amp; Dollfus, 
<number box="[648,704,1340,1363]" pageId="13" pageNumber="52" value="1883.0">1883</number>
</taxonomicName>
. 
<typeStatus box="[715,768,1340,1363]" pageId="13" pageNumber="52">Type</typeStatus>
species by monotypy: 
<taxonomicName authority="Montagu." authorityName="Montagu." box="[1013,1326,1340,1363]" class="Gastropoda" family="Turbinidae" genus="Turbo" higherTaxonomySource="CoL" kingdom="Animalia" order="Trochida" pageId="13" pageNumber="52" phylum="Mollusca" rank="species" species="interstinctus">
