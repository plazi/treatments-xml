<quantity box="[919,974,544,570]" metricMagnitude="1" metricUnit="m" metricValue="7.1" pageId="15" pageNumber="54" unit="m" value="71.0">71m</quantity>
), 
<date box="[995,1117,544,570]" pageId="15" pageNumber="54" value="1996-02-22">
<number box="[995,1027,544,570]" pageId="15" pageNumber="54" value="22.0">22</number>
.ii. 
<number box="[1053,1117,544,570]" pageId="15" pageNumber="54" value="1996.0">1996</number>
</date>
, 
<collectionCode box="[1129,1214,544,570]" pageId="15" pageNumber="54">NOAN</collectionCode>
coll [
<number box="[1282,1296,544,570]" pageId="15" pageNumber="54" value="1.0">1</number>
].
</paragraph>
