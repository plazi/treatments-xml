<paragraph pageId="15" pageNumber="66">
<pageBreakToken pageId="15" pageNumber="66" start="start">Type</pageBreakToken>
species.
</paragraph>
<paragraph pageId="15" pageNumber="66">
<taxonomicName class="Insecta" family="Curculionidae" genus="Poecilma" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Poecilma stipitosum" order="Coleoptera" pageId="15" pageNumber="66" phylum="Arthropoda" rank="species" species="stipitosum">
<emphasis italics="true" pageId="15" pageNumber="66">Poecilma stipitosum</emphasis>
