. 
<typeStatus box="[309,364,1968,1992]" pageId="13" pageNumber="34">Type</typeStatus>
locality, depth and collection date, 
<collectionCode LSID="urn:lsid:biocol.org:col:34978" box="[762,827,1968,1991]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34978" name="Museum Victoria" pageId="13" pageNumber="34">NMV</collectionCode>
F 
<number box="[850,934,1967,1991]" pageId="13" pageNumber="34" value="157399.0">157399</number>
(
<number box="[951,963,1967,1991]" pageId="13" pageNumber="34" value="1.0">1</number>
) (damaged).
