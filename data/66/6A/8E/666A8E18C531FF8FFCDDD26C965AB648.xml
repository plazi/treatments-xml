<emphasis box="[931,1069,2061,2084]" italics="true" pageId="2" pageNumber="255">M. laosensis</emphasis>
</taxonomicName>
; 7, 8: 
<taxonomicName box="[1148,1277,2061,2084]" class="Insecta" family="Lycidae" genus="Metriorrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="255" phylum="Arthropoda" rank="species" species="sericans">
