<taxonomicName authorityName="Ing" authorityYear="1982" baseAuthorityName="Meylan" baseAuthorityYear="1933" box="[271,418,1088,1109]" class="Myxomycetes" family="Liceaceae" genus="Licea" kingdom="Protozoa" order="Liceales" pageId="19" pageNumber="40" phylum="Mycetozoa" rank="species" species="pygmaea">
