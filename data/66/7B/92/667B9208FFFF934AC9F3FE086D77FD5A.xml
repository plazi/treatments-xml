<subSubSection pageId="6" pageNumber="27" type="discussion">
<paragraph blockId="6.[136,1452,518,689]" pageId="6" pageNumber="27">
<emphasis box="[189,253,627,652]" italics="true" pageId="6" pageNumber="27">Notes</emphasis>
