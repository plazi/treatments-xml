<taxonomicName authorityName="H. Milne Edwards &amp; Lucas" authorityYear="1842" class="Malacostraca" family="Inachoididae" genus="Inachoides" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="115" phylum="Arthropoda" rank="species" species="microrhynchus">
<emphasis italics="true" pageId="14" pageNumber="115">Inachoides microrhynchus</emphasis>
</taxonomicName>
; 
<bibRefCitation author="Santana, W. R. A." box="[334,492,831,857]" pageId="14" pageNumber="115" refId="ref24839" refString="Santana, W. R. A. (2008) Revisao taxonomica e relacoes filogeneticas em Inachoididae Dana, 1851 (Crustacea, Brachyura, Majoidea). Unpublished doctoral thesis, Universidade de Sao Paulo, Sao Paulo, 244 pp." type="book" year="2008">Santana 2008</bibRefCitation>
: figs. 44A, 45A, B, 46A; 
<bibRefCitation author="Santana &amp; Tavares" box="[789,1072,831,858]" firstAuthor="Santana" pageId="14" pageNumber="115" pagination="62 - 68" refId="ref24988" refString="Santana, W. R. &amp; Tavares, M. (2009) Podochela meloi Sankarankutty, Ferreira &amp; Cunha, 2001, a junior synonym of the spider crab Inachoides forceps A. Milne-Edwards, 1879 (Crustacea: Brachyura: Inachoididae). Zootaxa, 2294 (1), 62 - 68. https: // doi. org / 10.11646 / zootaxa. 2294.1.5" type="journal article" year="2009">Santana &amp; Tavares 2009</bibRefCitation>
: fig. 2: 
<taxonomicName authorityName="A. Milne-Edwards" authorityYear="1879" box="[1163,1269,831,857]" class="Malacostraca" family="Inachoididae" genus="Inachoides" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="115" phylum="Arthropoda" rank="species" species="forceps">
<emphasis box="[1163,1269,831,857]" italics="true" pageId="14" pageNumber="115">I. forceps</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="De Haan" authorityYear="1839" class="Malacostraca" family="Inachoididae" genus="Inachoides" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="115" phylum="Arthropoda" rank="species" species="lambriformis">
<emphasis italics="true" pageId="14" pageNumber="115">I. lambriformis</emphasis>
</taxonomicName>
