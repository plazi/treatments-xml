<typeStatus box="[151,210,965,991]" pageId="17" pageNumber="118">Type</typeStatus>
genus
</emphasis>
. 
<taxonomicName authority="Lamarck, 1815" authorityName="Lamarck" authorityYear="1815" box="[306,653,964,991]" class="Insecta" family="Brentidae" genus="Stenorhynchus" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="118" phylum="Arthropoda" rank="genus">
<emphasis box="[306,470,964,990]" italics="true" pageId="17" pageNumber="118">Stenorhynchus</emphasis>
Lamarck, 1815
</taxonomicName>
[
<typeStatus box="[669,720,965,991]" pageId="17" pageNumber="118">type</typeStatus>
species by designation by 
<bibRefCitation author="Rathbun" box="[1037,1221,964,991]" firstAuthor="Rathbun" pageId="17" pageNumber="118" pagination="153 - 167" refId="ref24724" refString="Rathbun, M. J. (1897) A revision of the nomenclature of the Brachyura. Proceedings of the Biological Society of Washington, 11, 153 - 167." type="journal article" year="1897">Rathbun (1897)</bibRefCitation>
: 
<taxonomicName authority="Herbst, 1788" authorityName="Herbst" authorityYear="1788" class="Malacostraca" family="Cancridae" genus="Cancer" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="118" phylum="Arthropoda" rank="species" species="seticornis">
