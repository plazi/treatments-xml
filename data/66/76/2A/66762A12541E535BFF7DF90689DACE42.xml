<collectingCountry box="[596,679,295,320]" name="Angola" pageId="9" pageNumber="556">Angola</collectingCountry>
and 
<collectingCountry box="[733,847,296,319]" name="Botswana" pageId="9" pageNumber="556">Botswana</collectingCountry>
