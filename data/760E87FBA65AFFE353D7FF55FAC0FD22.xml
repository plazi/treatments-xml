<taxonomicName id="39A74D6EA65AFFE3526DFEC0FD4BFEF7" authority="Reitter, 1885" authorityName="Reitter" authorityYear="1885" box="[301,658,258,281]" class="Insecta" family="Leiodidae" genus="Pseudcolenis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="hilleri">
