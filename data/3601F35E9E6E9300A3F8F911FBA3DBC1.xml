locality in 
<collectingCountry id="C6BF02D89E609300A014FE70FC21DBC1" box="[891,995,404,428]" name="Paraguay" pageId="57" pageNumber="58">Paraguay</collectingCountry>
(
<figureCitation id="26935ECD9E609300A09DFE70FB97DBC1" box="[1010,1109,404,428]" captionStart="FIGURE 128" captionStartId="88.[151,250,2001,2024]" captionTargetBox="[283,1304,469,1981]" captionTargetId="figure-67@88.[283,1304,469,1981]" captionTargetPageId="88" captionText="FIGURE 128. Geographical distributions of Epimetopus species." pageId="57" pageNumber="58">Fig. 128</figureCitation>
).
</paragraph>
