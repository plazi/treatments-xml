<paragraph id="8BB73696C66EFFA50DD3DF80FE22F905" blockId="38.[151,418,1693,1752]" box="[151,418,1693,1719]" pageId="38" pageNumber="39">
<heading id="D0FF81FAC66EFFA50DD3DF80FE22F905" bold="true" box="[151,418,1693,1719]" fontSize="11" level="2" pageId="38" pageNumber="39" reason="4">
