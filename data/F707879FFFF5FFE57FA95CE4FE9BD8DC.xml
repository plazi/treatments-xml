<taxonomicName id="B8AE4D0AFFF5FFE67FA95D70FCBFD863" authority=": Medvedev &amp; Zaitsev 1978: 118" authorityName=": Medvedev &amp; Zaitsev" authorityPageNumber="118" authorityYear="1978" box="[151,835,866,888]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="558" phylum="Arthropoda" rank="species" species="coreana" subGenus="Gonioctena">
<emphasis id="4DDAEA9BFFF5FFE67FA95D70FEF1D863" box="[151,269,867,888]" italics="true" pageId="1" pageNumber="558">Gonioctena</emphasis>
(
<emphasis id="4DDAEA9BFFF5FFE67E225D70FE6ED863" box="[284,402,867,888]" italics="true" pageId="1" pageNumber="558">Gonioctena</emphasis>
) 
<emphasis id="4DDAEA9BFFF5FFE67E9F5D70FE0FD863" box="[417,499,867,888]" italics="true" pageId="1" pageNumber="558">coreana</emphasis>
