<taxonomicName id="4C489C8D98270B22FEA6FA1A4FC71FB9" box="[383,644,1506,1530]" class="Insecta" family="Heteroceridae" genus="Heterocerus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="schwarzi">
