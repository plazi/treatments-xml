<subSubSection id="C694A087E7084946FF00276B6311FB8B" box="[199,818,1050,1077]" pageId="15" pageNumber="238" type="distribution">
<paragraph id="8E31F30CE7084946FF00276B6311FB8B" blockId="15.[151,1437,151,1293]" box="[199,818,1050,1077]" pageId="15" pageNumber="238">
<emphasis id="BCFA2F1EE7084946FF00276B6142FB8A" bold="true" box="[199,353,1050,1076]" pageId="15" pageNumber="238">Distribution.</emphasis>
Known only from 
<typeStatus id="51354DAEE7084946FDF0276A6244FB8B" box="[567,615,1051,1077]" pageId="15" pageNumber="238">type</typeStatus>
