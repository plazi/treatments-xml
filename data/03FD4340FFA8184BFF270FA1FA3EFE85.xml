<quantity id="4CAC5FB3FFA8184AFBCF0D74FAB7FD88" box="[1151,1287,578,605]" metricMagnitude="2" metricUnit="m" metricValue="5.26" metricValueMax="5.67" metricValueMin="4.85" pageId="18" pageNumber="19" unit="m" value="526.0" valueMax="567.0" valueMin="485.0">
