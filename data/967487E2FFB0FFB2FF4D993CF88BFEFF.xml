<taxonomicName id="D9DD4D77FFB1FFB2FB239F05F849FEFF" box="[1273,1369,269,290]" class="Insecta" family="Ephydridae" genus="Neoephydra" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="araucaria">
<emphasis id="2CA9EAE6FFB1FFB2FB239F05F849FEFF" box="[1273,1369,269,290]" italics="true" pageId="33" pageNumber="34">araucaria</emphasis>
