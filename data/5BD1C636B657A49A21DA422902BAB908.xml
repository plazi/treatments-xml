<taxonomicName id="C2FBE73940A7BAA63AFC7730110A5DD8" class="Insecta" family="Cerambycidae" genus="Hypocephalus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypocephalus" order="Coleoptera" pageId="195" pageNumber="196" phylum="Arthropoda" rank="genus">Hypocephalus</taxonomicName>
