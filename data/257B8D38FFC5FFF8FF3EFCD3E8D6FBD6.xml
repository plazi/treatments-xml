<taxonomicName id="6AD247ADFFC5FFF8FDB0FC8FEB7BFCBE" box="[537,608,835,861]" class="Insecta" family="Pentatomidae" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="73" phylum="Arthropoda" rank="subFamily" subFamily="Brazil">Brazil</taxonomicName>
