<taxonomicName id="311DAA38DDC1BE2F077CFC2A57ECF345" authority="Miller, 1971" authorityName="Miller" authorityYear="1971" class="Gastropoda" family="Paracoryphellidae" genus="Paracoryphella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracoryphella" order="Nudibranchia" pageId="11" pageNumber="12" phylum="Mollusca" rank="genus">
