<quantity id="06519B40FFB86D25FAA4FAC85AC0FA47" metricMagnitude="-4" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="5" pageNumber="106" unit="mm" value="0.16" valueMax="0.17" valueMin="0.15">0.15–0.17 mm</quantity>
