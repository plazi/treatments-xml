<taxonomicName id="6F6B71D71C9D997BB8D5F815B9306CDF" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus zamorai" order="Hymenoptera" pageId="116" pageNumber="117" phylum="Arthropoda" rank="species" species="zamorai">Euplectrus zamorai</taxonomicName>
