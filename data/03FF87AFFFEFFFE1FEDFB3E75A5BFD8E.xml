<number id="3D9E249EFFE8FFE1FC0CB7255DFCFC4E" box="[987,1001,604,630]" pageId="21" pageNumber="22" value="7.0">7</number>
brown. Sternite 
<number id="3D9E249EFFE8FFE1FB7BB7255AAEFC4E" box="[1196,1211,604,630]" pageId="21" pageNumber="22" value="7.0">7</number>
