<collectingMunicipality box="[1196,1338,449,475]" pageId="28" pageNumber="29">Parry Island</collectingMunicipality>
(lagoon side), Enewetak atoll: 
<geoCoordinate box="[434,540,486,512]" degrees="11" direction="north" minutes="24" orientation="latitude" pageId="28" pageNumber="29" precision="925" value="11.4">11º24’N</geoCoordinate>
, 
<geoCoordinate box="[555,674,486,512]" degrees="162" direction="east" minutes="23" orientation="longitude" pageId="28" pageNumber="29" precision="925" value="162.38333">162º23’E</geoCoordinate>
, 
<quantity box="[689,750,486,512]" metricMagnitude="1" metricUnit="m" metricValue="2.7432" pageId="28" pageNumber="29" unit="ft" value="90.0">
<elevation box="[689,750,486,512]" metricMagnitude="1" metricUnit="m" metricValue="2.7432" pageId="28" pageNumber="29" unit="ft" value="90.0">90 ft</elevation>
</quantity>
, summer 1957, 1 complete spm (
<specimenCode box="[1168,1358,486,512]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="28" pageNumber="29" type="Museum">USNM 118681</specimenCode>
as 
<taxonomicName authorityName="? Imajima" authorityYear="1970" class="Polychaeta" family="Nephtyidae" genus="Micronephthys" kingdom="Animalia" order="Phyllodocida" pageId="28" pageNumber="29" phylum="Annelida" rank="species" species="sphaerocirrata">
<emphasis italics="true" pageId="28" pageNumber="29">M. sphaerocirrata</emphasis>
</taxonomicName>
)
