= 1.042); female 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="2.4" metricValueMax="2.7" metricValueMin="2.1" pageId="3" pageNumber="1545" unit="mm" value="24.0" valueMax="27.0" valueMin="21.0">21– 27 mm</quantity>
(mean (
<emphasis box="[387,400,854,878]" italics="true" pageId="3" pageNumber="1545">n</emphasis>
= 11) 
<quantity box="[479,596,854,878]" metricMagnitude="-2" metricUnit="m" metricValue="2.532" pageId="3" pageNumber="1545" unit="mm" value="25.32">25.32 mm</quantity>
, 
