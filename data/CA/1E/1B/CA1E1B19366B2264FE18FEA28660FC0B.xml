<quantity box="[475,594,503,527]" metricMagnitude="-2" metricUnit="m" metricValue="1.85" metricValueMax="2.2" metricValueMin="1.5" pageId="21" pageNumber="1563" unit="mm" value="18.5" valueMax="22.0" valueMin="15.0">15–22 mm</quantity>
(mean (
<emphasis box="[689,702,503,527]" italics="true" pageId="21" pageNumber="1563">n</emphasis>
= 5) 
<quantity box="[766,882,503,527]" metricMagnitude="-2" metricUnit="m" metricValue="1.978" pageId="21" pageNumber="1563" unit="mm" value="19.78">19.78 mm</quantity>
, 
<collectingRegion box="[894,930,503,527]" country="Serbia" name="Sid" pageId="21" pageNumber="1563">SD</collectingRegion>
= 2.575); female 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="2.05" metricValueMax="2.3" metricValueMin="1.8" pageId="21" pageNumber="1563" unit="mm" value="20.5" valueMax="23.0" valueMin="18.0">18– 23 mm</quantity>
(mean (
<emphasis box="[387,400,535,559]" italics="true" pageId="21" pageNumber="1563">n</emphasis>
= 5) 
<quantity box="[466,582,535,559]" metricMagnitude="-2" metricUnit="m" metricValue="1.984" pageId="21" pageNumber="1563" unit="mm" value="19.84">19.84 mm</quantity>
, 
<collectingRegion box="[595,631,535,559]" country="Serbia" name="Sid" pageId="21" pageNumber="1563">SD</collectingRegion>
