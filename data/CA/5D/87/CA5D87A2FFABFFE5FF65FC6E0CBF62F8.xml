<collectingCountry box="[226,310,1981,2003]" name="Vietnam" pageId="1" pageNumber="339">Vietnam</collectingCountry>
, 
<collectingCountry box="[320,408,1981,2003]" name="Thailand" pageId="1" pageNumber="339">Thailand</collectingCountry>
