<taxonomicName genus="M." lsidName="M. ferruginea" pageId="71" pageNumber="172" rank="species" species="ferruginea">
<emphasis italics="true" pageId="71" pageNumber="172">M. ferruginea</emphasis>
</taxonomicName>
