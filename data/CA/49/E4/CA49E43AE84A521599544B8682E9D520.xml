<taxonomicName genus="Z." lsidName="Z. c. subsp. pinskica" pageId="0" pageNumber="39" rank="subSpecies" species="c." subSpecies="pinskica">
<emphasis italics="true" pageId="0" pageNumber="39">Z. c. pinskica</emphasis>
</taxonomicName>
