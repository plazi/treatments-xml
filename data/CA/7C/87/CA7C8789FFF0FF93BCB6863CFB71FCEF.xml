<subSubSection lastPageId="2" lastPageNumber="44" pageId="1" pageNumber="43" type="distribution">
<paragraph blockId="1.[151,1436,1881,2008]" box="[199,1422,1881,1905]" pageId="1" pageNumber="43">
<emphasis bold="true" box="[199,517,1881,1905]" pageId="1" pageNumber="43">Distribution and bionomics.</emphasis>
<taxonomicName box="[523,723,1882,1905]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="43" phylum="Arthropoda" rank="species" species="parauapebensis">
<emphasis box="[523,723,1882,1905]" italics="true" pageId="1" pageNumber="43">C. parauapebensis</emphasis>
</taxonomicName>
is known only from farm area in northern Brazilian state of Pará.
</paragraph>
<paragraph blockId="1.[151,1436,1881,2008]" lastBlockId="2.[151,1436,152,775]" lastPageId="2" lastPageNumber="44" pageId="1" pageNumber="43">
<emphasis bold="true" box="[199,270,1915,1939]" pageId="1" pageNumber="43">
<typeStatus box="[199,266,1915,1939]" pageId="1" pageNumber="43">Types</typeStatus>
.
</emphasis>
<typeStatus box="[280,379,1916,1939]" pageId="1" pageNumber="43">Holotype</typeStatus>
female, Fazenda 
<taxonomicName authority="Parauapebas" authorityName="Parauapebas" box="[574,813,1916,1939]" class="Arachnida" family="Gonyleptidae" genus="Bocaina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="1" pageNumber="43" phylum="Arthropoda" rank="genus">Bocaina, Parauapebas</taxonomicName>
, Pará, 
<collectingCountry box="[888,985,1916,1939]" name="Brazil" pageId="1" pageNumber="43">BRASIL</collectingCountry>
, 
<date box="[998,1149,1916,1939]" pageId="1" pageNumber="43" value="2007-05-27" valueMax="2007-05-28" valueMin="2007-05-27">
<number box="[998,1067,1916,1939]" pageId="1" pageNumber="43" value="27.5" valueMax="28.0" valueMin="27.0">27–28</number>
.V. 
<number box="[1093,1149,1916,1939]" pageId="1" pageNumber="43" value="2007.0">2007</number>
</date>
, 
