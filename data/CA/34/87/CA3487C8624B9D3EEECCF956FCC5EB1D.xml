<subSubSection pageId="43" pageNumber="44" type="etymology">
<paragraph blockId="43.[151,1437,151,2020]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[199,379,1960,1985]" pageId="43" pageNumber="44">ETYMOLOGY</emphasis>
