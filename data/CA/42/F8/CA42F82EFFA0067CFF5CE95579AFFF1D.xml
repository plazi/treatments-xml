<collectingCountry box="[1134,1225,1017,1044]" name="Mexico" pageId="6" pageNumber="57">Mexico</collectingCountry>
), 
<collectingCountry box="[1244,1360,1017,1043]" name="Bermuda" pageId="6" pageNumber="57">Bermudas</collectingCountry>
, Antilles, Central America, 
<collectingCountry box="[393,506,1053,1079]" name="Colombia" pageId="6" pageNumber="57">Colombia</collectingCountry>
