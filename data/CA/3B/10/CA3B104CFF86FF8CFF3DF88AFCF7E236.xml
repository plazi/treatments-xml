<taxonomicName authorityName="O. Pickard-Cambridge" authorityYear="1869" box="[1104,1304,1879,1901]" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="ceylonicus">
