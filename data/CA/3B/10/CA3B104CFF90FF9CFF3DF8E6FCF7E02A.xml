<taxonomicName authority="Huber, 2019" authorityName="Huber" authorityYear="2019" box="[1190,1365,1905,1927]" class="Arachnida" family="Pholcidae" genus="Belisana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="badulla" status="sp. n.">
