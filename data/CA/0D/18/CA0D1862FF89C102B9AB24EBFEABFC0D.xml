<taxonomicNameLabel box="[898,981,1776,1800]" pageId="8" pageNumber="9" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, paratype, female, slide No. YS20263; 2. 
<taxonomicName authority="Yu &amp; Zhu &amp; Wang, 2022" authorityName="Yu &amp; Zhu &amp; Wang" authorityYear="2022" class="Insecta" family="Lecithoceridae" genus="Torodora" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="atomata" status="sp. nov.">
<emphasis italics="true" pageId="8" pageNumber="9">T. atomata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[242,323,1812,1836]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[242,323,1812,1836]" pageId="8" pageNumber="9" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype, male, slide No. ZYM06217; 3. 
<taxonomicName authority="Yu &amp; Zhu &amp; Wang, 2022" authorityName="Yu &amp; Zhu &amp; Wang" authorityYear="2022" box="[762,915,1812,1836]" class="Insecta" family="Lecithoceridae" genus="Torodora" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="canaliculata" status="sp. nov.">
<emphasis box="[762,915,1812,1836]" italics="true" pageId="8" pageNumber="9">T. canaliculata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[922,1003,1812,1836]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[922,1003,1812,1836]" pageId="8" pageNumber="9" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype, male, slide No. YS20133; 4. 
<taxonomicName authority="Yu &amp; Zhu &amp; Wang, 2022" authorityName="Yu &amp; Zhu &amp; Wang" authorityYear="2022" class="Insecta" family="Lecithoceridae" genus="Torodora" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="costatiprolata" status="sp. nov.">
<emphasis italics="true" pageId="8" pageNumber="9">T. costatiprolata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[299,380,1848,1872]" pageId="8" pageNumber="9">
<taxonomicNameLabel box="[299,380,1848,1872]" pageId="8" pageNumber="9" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype, male, slide No. YS20140; 5. 
<taxonomicName authority="Yu &amp; Zhu &amp; Wang, 2022" authorityName="Yu &amp; Zhu &amp; Wang" authorityYear="2022" box="[791,944,1848,1872]" class="Insecta" family="Lecithoceridae" genus="Torodora" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="dentisaccula" status="sp. nov.">
