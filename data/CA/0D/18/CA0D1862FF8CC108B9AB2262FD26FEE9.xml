<subSubSection pageId="3" pageNumber="4" type="etymology">
<paragraph blockId="3.[151,1436,151,321]" pageId="3" pageNumber="4">
<emphasis bold="true" box="[199,334,259,285]" pageId="3" pageNumber="4">Etymology.</emphasis>
