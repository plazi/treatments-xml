<taxonomicName authorityName="Zerche" authorityYear="1990" box="[565,781,1507,1531]" class="Insecta" family="Staphylinidae" genus="Archaeoboreaphilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="2513" phylum="Arthropoda" rank="genus">
