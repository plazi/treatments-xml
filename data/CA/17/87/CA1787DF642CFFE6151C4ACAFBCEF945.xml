<taxonomicName authorityName="Kim &amp; Ahn" authorityYear="2012" box="[424,595,1698,1722]" class="Insecta" family="Staphylinidae" genus="Archaeoboreaphilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="2513" phylum="Arthropoda" rank="species" species="rubromaculatus">
