<taxonomicName family="Coelopidae" lsidName="" pageId="20" pageNumber="57" rank="family">Coelopidae</taxonomicName>
, 
<taxonomicName family="Fannidae" lsidName="" pageId="20" pageNumber="57" rank="family">Fannidae</taxonomicName>
, 
<taxonomicName family="Muscidae" lsidName="" pageId="20" pageNumber="57" rank="family">Muscidae</taxonomicName>
, 
<taxonomicName family="Ottidae" lsidName="" pageId="20" pageNumber="57" rank="family">Ottidae</taxonomicName>
