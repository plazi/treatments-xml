<taxonomicName authority="L." class="Magnoliopsida" family="Lamiaceae" genus="Chamaedrys" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="95" phylum="Tracheophyta" rank="genus">
