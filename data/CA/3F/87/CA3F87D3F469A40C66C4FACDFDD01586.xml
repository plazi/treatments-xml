<typeStatus box="[151,210,1449,1474]" pageId="6" pageNumber="258">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName authority="Marusik, Azarkina &amp; Koponen, 2004" authorityName="Marusik, Azarkina &amp; Koponen" authorityYear="2004" box="[314,940,1449,1474]" class="Arachnida" family="Lycosidae" genus="Sibirocosa" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="258" phylum="Arthropoda" rank="species" species="alpina">
