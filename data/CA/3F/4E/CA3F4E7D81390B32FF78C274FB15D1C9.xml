<subSubSection box="[151,582,1122,1144]" pageId="33" pageNumber="184" type="reference_group">
<paragraph blockId="33.[151,582,1122,1144]" box="[151,582,1122,1144]" pageId="33" pageNumber="184">
<typeStatus box="[151,202,1123,1144]" pageId="33" pageNumber="184">Type</typeStatus>
genus 
<taxonomicName authority="Klunzinger 1870" authorityName="Klunzinger" authorityYear="1870" box="[273,582,1122,1144]" class="Actinopterygii" family="Apogonidae" genus="Pristiapogon" kingdom="Animalia" order="Perciformes" pageId="33" pageNumber="184" phylum="Chordata" rank="genus">
<emphasis box="[273,406,1123,1144]" italics="true" pageId="33" pageNumber="184">Pristiapogon</emphasis>
<bibRefCitation author="Klunzinger, C. B." box="[411,582,1122,1144]" pageId="33" pageNumber="184" pagination="669 - 834" refId="ref34194" refString="Klunzinger, C. B. (1870) Synopsis der Fische des Rothen Meeres. I. Theil. Percoiden-Mugiloiden. Verhandlungen der K. - K. zoologisch-botanischen Gesellschaft in Wien, 20, 669 - 834." type="journal article" year="1870">Klunzinger 1870</bibRefCitation>
