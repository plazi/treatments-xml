, 28 
<emphasis bold="true" box="[173,183,1336,1360]" italics="true" pageId="13" pageNumber="366">°</emphasis>
069N, 85 
<emphasis bold="true" box="[283,293,1336,1360]" italics="true" pageId="13" pageNumber="366">°</emphasis>
199E, 
<date box="[363,513,1336,1360]" pageId="13" pageNumber="366" value="1968-05-30">
