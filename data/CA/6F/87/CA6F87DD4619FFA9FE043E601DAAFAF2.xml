; forewing length 
<quantity box="[640,718,322,346]" metricMagnitude="-2" metricUnit="m" metricValue="5.3" pageId="12" pageNumber="365" unit="mm" value="53.0">53 mm</quantity>
, hindwing length 
<quantity box="[925,1003,322,346]" metricMagnitude="-2" metricUnit="m" metricValue="4.4" pageId="12" pageNumber="365" unit="mm" value="44.0">44 mm</quantity>
