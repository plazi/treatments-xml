<paragraph id="8BD9368A8B8FFF3CFE9FFA12FC44FA78" blockId="193.[264,925,1409,1475]" box="[312,925,1449,1475]" pageId="193" pageNumber="194">
<taxonomicName id="4C664D098B8FFF3CFE9FFA12FDD1FA79" authorityName="Sun" authorityYear="2003" box="[312,520,1449,1474]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="193" pageNumber="194" phylum="Arthropoda" rank="species" species="cylindrata">
