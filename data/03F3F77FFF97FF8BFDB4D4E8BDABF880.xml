<taxonomicName id="4C5A3DEAFF97FF8BFB40D5A1BA0BFA48" baseAuthorityName="E. Geoffroy" baseAuthorityYear="1810" box="[1217,1344,1490,1514]" class="Mammalia" family="Pteropodidae" genus="Macroglossus" kingdom="Animalia" order="Chiroptera" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="minimus">
