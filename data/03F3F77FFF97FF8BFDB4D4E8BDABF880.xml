<bibRefCitation id="EFCB3B98FF97FF8BFC21D5A0BCF2FA48" author="Kingston T &amp; Lim BL &amp; Akbar Z." box="[928,953,1490,1514]" journalOrPublisher="Penerbit Universiti Kebangsaan Malaysia" pageId="9" pageNumber="10" refId="ref38130" refString="11. Kingston T, Lim BL, Akbar Z. Bats of Krau wildlife reserve. Penerbit Universiti Kebangsaan Malaysia; 2006." title="Bats of Krau wildlife reserve" type="book" year="2006">11</bibRefCitation>
, 
<bibRefCitation id="EFCB3B98FF97FF8BFC47D5A0BC94FA48" author="Francis CM" box="[966,991,1490,1514]" journalOrPublisher="New Holland Publishers" pageId="9" pageNumber="10" refId="ref38235" refString="14. Francis CM. A field guide to the mammals of South-East Asia. New Holland Publishers; 2008." title="A field guide to the mammals of South-East Asia" type="book" year="2008">14</bibRefCitation>
, 
<bibRefCitation id="EFCB3B98FF97FF8BFC6AD5A0BB4FFA48" author="Boonsong L &amp; McNeely JA" bookContentInfo="pp. 758" box="[1003,1028,1490,1514]" journalOrPublisher="Association for the Conservation of Wildlife" pageId="9" pageNumber="10" refId="ref41156" refString="74. Boonsong L, McNeely JA. Mammals of Thailand. Association for the Conservation of Wildlife; 1977. pp. 758." title="Mammals of Thailand" type="book" year="1977">74</bibRefCitation>
