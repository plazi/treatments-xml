<taxonomicName id="4C5A3DEAFF97FF8BFD25D5DDBCC0FA65" authorityName="Andersen" authorityYear="1911" box="[676,907,1455,1479]" class="Mammalia" family="Pteropodidae" genus="Macroglossus" kingdom="Animalia" order="Chiroptera" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="sobrinus">
