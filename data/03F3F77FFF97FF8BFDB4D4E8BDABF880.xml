<taxonomicName id="4C5A3DEAFF97FF8BFA99D5C2BADDFA65" baseAuthorityName="E. Geoffroy" baseAuthorityYear="1810" box="[1304,1430,1456,1480]" class="Mammalia" family="Pteropodidae" genus="Macroglossus" kingdom="Animalia" order="Chiroptera" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="minimus">
