; length of scutellum 
<number id="7500BC6DE160FFA8B27544F038A7E99C" box="[1379,1431,915,939]" pageId="14" pageNumber="415" value="0.23">0.23</number>
; claval commissure 052; length of hind femur 
<number id="7500BC6DE160FFA8B5B244D43FE7E9F8" box="[676,727,951,975]" pageId="14" pageNumber="415" value="0.91">0.91</number>
