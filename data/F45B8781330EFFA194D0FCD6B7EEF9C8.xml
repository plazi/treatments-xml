<number id="CA3A24B0330EFFA190C3FA66B2C7FA0E" box="[1156,1208,1503,1528]" pageId="12" pageNumber="413" value="2.68">2.68</number>
, AW 
<number id="CA3A24B0330EFFA190B9FA59B34EFA0E" box="[1278,1329,1504,1528]" pageId="12" pageNumber="413" value="1.48">1.48</number>
, L 
<number id="CA3A24B0330EFFA19111FA59B31FFA0E" box="[1366,1376,1504,1528]" pageId="12" pageNumber="413" value="1.0">1</number>
