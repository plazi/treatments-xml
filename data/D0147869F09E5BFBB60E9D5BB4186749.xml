<taxonomicName id="6C67C035C46BF198706C23290F8277EB" authorityName="Clos" authorityYear="1994" class="Magnoliopsida" family="Leguminosae" genus="Balsamocarpon" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Balsamocarpon brevifolium" order="Fabales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="brevifolium">
<emphasis id="6293D3E83133A40F8FE4DE40DFB6FBBD" italics="true" pageId="65" pageNumber="66">Balsamocarpon brevifolium</emphasis>
