<taxonomicName id="FE797C5C7FFD46077447F341F422BB22" class="Insecta" family="Formicidae" genus="Messor" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Messor" order="Hymenoptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">Messor</taxonomicName>
