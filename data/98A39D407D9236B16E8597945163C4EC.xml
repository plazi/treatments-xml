<taxonomicName id="7EA36535F843EA6408492135D33771E0" class="Insecta" family="Formicidae" genus="Tapinoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tapinoma simrothi" order="Hymenoptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="species" species="simrothi">Tapinoma simrothi</taxonomicName>
