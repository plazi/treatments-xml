<subSubSection id="C35E65736450FFEAE0E0FBC1FCE8FBB6" pageId="9" pageNumber="454" type="diagnosis">
<paragraph id="8BFB36F86450FFEAE0E0FBC1FCE8FBB6" blockId="9.[151,1437,1014,1759]" pageId="9" pageNumber="454">
<emphasis id="B930EAEA6450FFEAE0E0FBC1FE95FB9A" bold="true" box="[199,321,1086,1111]" pageId="9" pageNumber="454">Diagnosis.</emphasis>
