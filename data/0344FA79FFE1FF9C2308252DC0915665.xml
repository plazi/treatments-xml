(
<emphasis id="B999977DFFE1FF9C234125E6C0B251C4" box="[321,487,943,968]" italics="true" pageId="8" pageNumber="37">
&quot; 
<taxonomicName id="4CED30ECFFE1FF9C234A25E6C09551C4" box="[330,448,943,968]" class="Ostracoda" family="Paradoxostomatidae" genus="Cytherois" kingdom="Animalia" order="Podocopida" pageId="8" pageNumber="37" phylum="Arthropoda" rank="genus">Cytherois</taxonomicName>
