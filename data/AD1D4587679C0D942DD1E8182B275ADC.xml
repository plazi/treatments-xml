<geoCoordinate id="26B3E8E166CF2205E51B1CD5A9116EDF" direction="north" orientation="latitude" precision="5" value="46.1927">46.1927°N</geoCoordinate>
, 
<geoCoordinate id="0C1665EFDB6D13DC024BE26705EBFB74" direction="west" orientation="longitude" precision="5" value="-67.6803">67.6803°W</geoCoordinate>
, 16.IX.2006, R.P. Webster, coll. // Queens Co., Cranberry Lake P.N.A., 
<geoCoordinate id="5FE215A43F119F77A7CC3821EE964D2F" direction="north" orientation="latitude" precision="5" value="46.1125">46.1125°N</geoCoordinate>
, 
<geoCoordinate id="60153B48BB10A73438C3DEBCEA63E780" direction="west" orientation="longitude" precision="5" value="-65.6075">65.6075°W</geoCoordinate>
, 22.IX.2009, R.P. Webster, coll. // Red oak forest, in decaying gilled mushrooms (6 ♂, RWC). Saint John Co., Dipper Harbour, 
<geoCoordinate id="833CB0CA72FF648AD64697E1FA23A496" direction="north" orientation="latitude" precision="5" value="45.1176">45.1176°N</geoCoordinate>
