<emphasis id="B952CD2AFFD47A744B06BD2956D7F619" box="[276,679,1342,1367]" italics="true" pageId="72" pageNumber="73">
<taxonomicName id="4C266ABBFFD47A744B06BD295604F618" box="[276,628,1342,1367]" class="Heterotardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="72" pageNumber="73" phylum="Tardigrada" rank="subSpecies" species="occidentalis" subSpecies="sensu">
