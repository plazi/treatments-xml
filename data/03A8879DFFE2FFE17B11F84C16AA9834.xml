<taxonomicName id="4C014D08FFE2FFE17FBEF93310AC99D4" baseAuthorityName="Lesson" baseAuthorityYear="1830" class="Reptilia" family="Scincidae" genus="Emoia" kingdom="Animalia" order="Squamata" pageId="19" pageNumber="723" phylum="Chordata" rank="species" species="cyanogaster">
