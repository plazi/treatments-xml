(
<tableCitation id="C6FB13F2FFF8584188D2D38CB82DFCAF" box="[436,521,1732,1758]" captionStart="TABLE 1" captionStartId="3.[151,244,152,179]" captionTargetBox="[163,1405,230,1341]" captionText="TABLE 1. Tanaidacean species from the sublittoral zone of Yucatan and their distribution within the Gulf of Mexico (GoM). Geographical boundaries in fauna distribution taken from Felder et al. (2009)" httpUri="http://table.plazi.org/id/DF0676C1FFF1584889F1D5D0BE7DFAA6" pageId="10" pageNumber="61" tableUuid="DF0676C1FFF1584889F1D5D0BE7DFAA6">Table 1</tableCitation>
, 
<figureCitation id="13423ACCFFF858418B71D38CB87FFCAF" box="[535,603,1732,1758]" captionStart="FIGURE 2" captionStartId="2.[151,250,1726,1751]" captionTargetBox="[151,1435,957,1701]" captionTargetId="figure@2.[151,1435,957,1701]" captionTargetPageId="2" captionText="FIGURE 2. Diversity, abundance, and distribution of tanaidaceans of the sublittoral zone of the northern Yucatan Peninsula." figureDoi="http://doi.org/10.5281/zenodo.3573460" httpUri="https://zenodo.org/record/3573460/files/figure.png" pageId="10" pageNumber="61">Fig. 2</figureCitation>
