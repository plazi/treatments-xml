<date id="06714E87FFB2FF9FFE85814D6BD9B83B" box="[292,461,295,320]" pageId="19" pageNumber="586" value="2011-02">February 2011</date>
</collectingDate>
, associated with 
<taxonomicName id="B5CF13C4FFB2FF9FFD2D814D6978B83B" baseAuthorityName="Verrill" baseAuthorityYear="1902" box="[652,876,295,320]" class="Anthozoa" family="Mussidae" genus="Mussismilia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="19" pageNumber="586" phylum="Cnidaria" rank="species" species="hispida">
