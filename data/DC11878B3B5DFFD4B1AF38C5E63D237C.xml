(
<number id="E27024BA3B5CFFD4B1A7383BE4D82359" box="[159,219,296,320]" pageId="17" pageNumber="496" value="2011.0">2011</number>
), early Cambrian, Sekwi Formation, Mackenzie Mountains, Northwest Territories, 
<collectingCountry id="2CAF760D3B5CFFD4B5843834E1102359" box="[1212,1299,295,320]" name="Canada" pageId="17" pageNumber="496">Canada</collectingCountry>
