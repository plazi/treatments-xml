<quantity id="4C8D4B80264D535532D4F80EFCFBF841" box="[740,881,1940,1965]" metricMagnitude="-3" metricUnit="m" metricValue="7.8" metricValueMax="8.1" metricValueMin="7.5" pageId="15" pageNumber="242" unit="mm" value="7.8" valueMax="8.1" valueMin="7.5">7.5–8.1 mm</quantity>
