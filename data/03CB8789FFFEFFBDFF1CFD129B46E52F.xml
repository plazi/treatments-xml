<taxonomicName id="4C624D1CFFFEFFBDFB4AFBFD987BE511" authority="Meigen, 1824" authorityName="Meigen" authorityYear="1824" class="Insecta" family="Tachinidae" genus="Tachina" kingdom="Animalia" order="Diptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="nemorum">
