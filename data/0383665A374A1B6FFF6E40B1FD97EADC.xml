<emphasis id="B95E0B5E374F1B6FFF6E45B2FE05ECFF" bold="true" box="[192,396,993,1018]" pageId="12" pageNumber="11">Type Material.</emphasis>
<materialsCitation id="3B42DD11374F1B6FFE3B45B2FD7EEB7C" collectingDate="1988-01-08" collectingDateMax="2009-02-08" collectingDateMin="1988-01-08" collectionCode="MNHN, JTPC, ENPC, PHDC" collectorName="Piegeage &amp; Odette Baloup &amp; F. Route de Kaw &amp; Touroult &amp; de Kaw &amp; E. Giesbert &amp; F. Hovore &amp; Chevaux GF &amp; P. - H. Dalens" country="French Guiana" county="Montagne des Chevaux" location="Piste Coralie" municipality="Rte de Kaw" pageId="12" pageNumber="11" specimenCount="2" specimenCount-male="2" typeStatus="holotype">
