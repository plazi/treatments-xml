<taxonomicName id="6183B088FF8FE010FD8F67C7FCF1F939" box="[594,824,1765,1790]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="pilae">
