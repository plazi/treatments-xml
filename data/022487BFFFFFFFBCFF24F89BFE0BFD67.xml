PN. 
<taxonomicName id="4D8D4D2AFFFEFFBCFDEAFEC6FD51FEA3" authorityName="Krefft" authorityYear="1871" box="[601,743,295,321]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="35" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
width 
<quantity id="4D759B4CFFFEFFBCFC81FEC6FC25FEA3" box="[818,915,295,321]" metricMagnitude="0" metricUnit="m" metricValue="2.0827999999999998" metricValueMax="2.2352" metricValueMin="1.9304" pageId="35" pageNumber="236" unit="in" value="82.0" valueMax="88.0" valueMin="76.0">76–88 in</quantity>
