<quantity id="4CAF9B03FFBEFFC5FD2BF82AFCF4F826" box="[747,829,1981,2006]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" pageId="25" pageNumber="26" unit="mm" value="16.0">16 mm</quantity>
