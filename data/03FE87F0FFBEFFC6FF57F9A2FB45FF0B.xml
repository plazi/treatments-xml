<quantity id="4CAF9B03FFBDFFC6FC61FF0FFBE9FF5C" box="[929,1056,152,176]" metricMagnitude="2" metricUnit="m" metricValue="7.7" metricValueMax="8.2" metricValueMin="7.2" pageId="26" pageNumber="27" unit="m" value="770.0" valueMax="820.0" valueMin="720.0">720–820 m</quantity>
