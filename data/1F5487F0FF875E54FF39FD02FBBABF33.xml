<quantity id="50059B03FF845E54FEFFFC1EFE72BFF5" box="[369,464,993,1016]" metricMagnitude="-3" metricUnit="m" metricValue="3.35" pageId="2" pageNumber="45" unit="mm" value="3.35">3.35 mm</quantity>
in 
<typeStatus id="48468844FF845E54FE7AFC1EFDF6BFF5" box="[500,596,993,1016]" pageId="2" pageNumber="45">holotype</typeStatus>
, range in 
<specimenCount id="81FBFD6FFF845E54FD30FC1EFCA5BFF5" box="[702,775,993,1016]" pageId="2" pageNumber="45" type="male">2 male</specimenCount>
<typeStatus id="48468844FF845E54FF06FBFEFF50B815" box="[136,242,1025,1048]" pageId="2" pageNumber="45">paratypes</typeStatus>
: (2.55-2.75); ThL = 
<quantity id="50059B03FF845E54FE5AFBFEFD97B815" box="[468,565,1025,1048]" metricMagnitude="-3" metricUnit="m" metricValue="1.58" pageId="2" pageNumber="45" unit="mm" value="1.58">1.58 mm</quantity>
