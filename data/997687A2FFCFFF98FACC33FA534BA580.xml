<taxonomicName id="D6DF4D37FFC8FF99F8E3339C552BA3F9" authority="maxillosus" authorityName="maxillosus (Linnaeus" authorityYear="1758" box="[696,1036,2008,2032]" class="Insecta" family="Staphylinidae" genus="Creophilus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="338" phylum="Arthropoda" rank="subSpecies" species="maxillosus" subSpecies="maxillosus">
