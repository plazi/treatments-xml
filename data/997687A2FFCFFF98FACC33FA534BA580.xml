<taxonomicName id="D6DF4D37FFC8FF99FE2A339C5419A3F9" authorityName="Smetana &amp; Hu" authorityYear="2019" box="[1137,1342,2008,2032]" class="Insecta" family="Staphylinidae" genus="Agelosus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="338" phylum="Arthropoda" rank="species" species="auricomus">
