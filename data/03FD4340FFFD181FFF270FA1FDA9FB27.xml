<quantity id="4CAC5FB3FFFD181FFB040D95FAB2FD68" box="[1204,1282,675,701]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="71" pageNumber="72" unit="m" value="100.0">100 m</quantity>
