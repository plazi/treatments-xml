<taxonomicName id="0A904D29430FBD45FEA3FDB0FDA5FD21" box="[328,586,620,644]" class="Malacostraca" family="Paguridae" genus="Anisopagurus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="31" phylum="Arthropoda" rank="species" species="bartletti">
