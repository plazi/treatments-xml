<taxonomicName id="295D1017EE7F618346CC6CCDAB39AB29" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Insecta" family="Tenthredinidae" genus="Tenthredo" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tenthredo liturata" order="Hymenoptera" pageId="10" pageNumber="357" phylum="Arthropoda" rank="species" species="liturata">Tenthredo liturata</taxonomicName>
