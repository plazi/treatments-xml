<collectingCountry id="AA86762EFF800E650D8CFD1619980AB3" box="[669,710,696,720]" name="Italy" pageId="17" pageNumber="392">Italy</collectingCountry>
(Keim 
<emphasis id="E0E5EAACFF800E650F95FD7D1BEF0A89" box="[132,177,722,746]" italics="true" pageId="17" pageNumber="392">et al.</emphasis>
2001); 
<taxonomicName id="15914D3DFF800E650FE8FD7C1A080A89" authority="Torres" authorityName="Torres" box="[249,342,722,746]" class="Liliopsida" family="Poaceae" genus="Nicoraella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="17" pageNumber="392" phylum="Tracheophyta" rank="genus">
