<quantity id="4CB19B2AFFE0FFA4FD36FEFEFCA4C542" box="[752,825,330,357]" metricMagnitude="2" metricUnit="m" metricValue="7.85" pageId="21" pageNumber="222" unit="m" value="785.0">785 m</quantity>
