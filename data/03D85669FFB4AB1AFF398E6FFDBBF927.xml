<taxonomicName id="4C719CFCFFBAAB1AFE728EB2FE3BF940" authorityName="Opitz" authorityYear="2014" box="[410,458,1681,1702]" class="Insecta" family="Cleridae" genus="Pujoliclerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="24" phylum="Arthropoda" rank="species" species="flora">flora</taxonomicName>
