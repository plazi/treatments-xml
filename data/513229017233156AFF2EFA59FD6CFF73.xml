and 
<taxonomicName id="1E9BE39472321565FE99F856FE3BF821" box="[288,420,1940,1962]" class="Insecta" family="Ciidae" genus="Cis" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="499" phylum="Arthropoda" rank="species" species="lugowoji" status="sp. nov.">
