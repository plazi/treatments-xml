<taxonomicName id="57BE87D4FF38FF12FF7B9A3D4CE00A14" authorityName="- Nolf &amp; Stringer" authorityYear="2003" box="[189,427,1289,1315]" class="Actinopterygii" family="Congridae" genus="Ariosoma" kingdom="Animalia" order="Anguilliformes" pageId="189" pageNumber="190" phylum="Chordata" rank="species" species="nonsector">
