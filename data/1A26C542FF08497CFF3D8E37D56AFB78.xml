<taxonomicName id="558F0FD7FF08497CFDAC8903D68CFBAE" box="[572,659,1092,1113]" class="Magnoliopsida" family="Fabaceae" genus="Ebenopsis" kingdom="Plantae" order="Fabales" pageId="7" pageNumber="745" phylum="Tracheophyta" rank="species" species="ebano">
