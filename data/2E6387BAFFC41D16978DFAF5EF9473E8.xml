<taxonomicName id="61CA4D2FFFC41D16938FF9DCE917735C" authorityName="Schiodte" authorityYear="1849" box="[1173,1290,1557,1583]" class="Malacostraca" family="Niphargidae" genus="Niphargus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="533" phylum="Arthropoda" rank="genus">
