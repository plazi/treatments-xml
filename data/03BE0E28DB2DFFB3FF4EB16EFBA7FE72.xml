<quantity id="4CEF12DBDB2EFFB3FD83B5A9FD00FF0A" box="[602,709,192,218]" metricMagnitude="-3" metricUnit="m" metricValue="4.85" pageId="34" pageNumber="35" unit="mm" value="4.85">4.85 mm</quantity>
; PW 
<quantity id="4CEF12DBDB2EFFB3FCDCB5A9FCB4FF0A" box="[773,881,192,218]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" pageId="34" pageNumber="35" unit="mm" value="2.05">2.05 mm</quantity>
; PL 
<quantity id="4CEF12DBDB2EFFB3FC7FB5A9FBD7FF0A" box="[934,1042,192,218]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="34" pageNumber="35" unit="mm" value="0.9">0.90 mm</quantity>
; EL 
