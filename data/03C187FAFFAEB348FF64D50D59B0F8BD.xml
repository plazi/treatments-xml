<collectingCountry id="F37F767CFFAEB348FAB1D5A758BBF969" box="[1346,1432,1690,1715]" name="Tunisia" pageId="12" pageNumber="447">Tunisia</collectingCountry>
, Djebel Bireno, 
<specimenCount id="9D6EFD65FFAEB348FEB9D5825C4CF90D" box="[330,367,1727,1751]" pageId="12" pageNumber="447" type="generic">
