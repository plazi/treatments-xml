<paragraph id="D91367F2DF4941DE4A8A199E7B3D5B4F" pageNumber="140">
<taxonomicName id="6BBF452160098EA4BA15E3EA3E342DE5" class="Insecta" family="Formicidae" genus="Aulacopone" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageNumber="140" phylum="Arthropoda" rank="species" species="relicta">
<pageStartToken id="5F7EA27E74E0D8DB7E56EF852BEA3B6B" pageNumber="140">
<pageStartToken id="BFD78CD7605D880018BD50BA4A0A0D5B" pageNumber="140">Aulacopone</pageStartToken>
</pageStartToken>
relicta
</taxonomicName>
<taxonomicNameLabel id="8793CEE06A753BE55BC974E7C18500B3" rank="species">sp. n.</taxonomicNameLabel>
</paragraph>
