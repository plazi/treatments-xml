<specimenCount id="9D3E208A3A0AFF93FC1BEAA2FB8BFA21" box="[931,1085,1507,1529]" pageId="10" pageNumber="1143" type="generic" typeStatus="paratypes">three paratypes</specimenCount>
(
<specimenCount id="9D3E208A3A0AFF93FF01E940FEA0F9CE" box="[185,278,1536,1558]" pageId="10" pageNumber="1143" type="male">one male</specimenCount>
