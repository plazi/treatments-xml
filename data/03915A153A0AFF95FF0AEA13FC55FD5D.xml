<taxonomicName id="4C3890803A0CFF95FE23ED2EFDE4FD5C" authorityName="O. Scheerpeltz" authorityYear="1957" box="[411,594,623,644]" class="Insecta" family="Staphylinidae" genus="Lithocharis" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="1145" phylum="Arthropoda" rank="species" species="boops">
