<subSubSection id="E6BA653F04084B44FF26F808C820F829" box="[199,1296,1950,1975]" pageId="7" pageNumber="227" type="etymology">
<paragraph id="AE1F36B404084B44FF26F808C820F829" blockId="7.[151,1437,402,2011]" box="[199,1296,1950,1975]" pageId="7" pageNumber="227">
<emphasis id="9CD4EAA604084B44FF26F808CC76F829" bold="true" box="[199,326,1950,1975]" pageId="7" pageNumber="227">Etymology</emphasis>
. 
<taxonomicName id="69A04D3704084B44FEB5F809CC84F828" box="[340,436,1951,1974]" class="Insecta" family="Cerambycidae" genus="Blabicentrus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="229" phylum="Arthropoda" rank="species" species="lineatus" status="sp. nov.">
<emphasis id="9CD4EAA604084B44FEB5F809CC84F828" box="[340,436,1951,1974]" italics="true" pageId="7" pageNumber="227">Lineatus</emphasis>
</taxonomicName>
