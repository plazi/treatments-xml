<collectingCountry id="0AF6767C2469BD30FEF2DCB71A93A00E" box="[338,428,757,783]" name="Mexico" pageId="119" pageNumber="120">Mexico</collectingCountry>
and 
<collectingCountry id="0AF6767C2469BD30FE44DCB7195CA00E" box="[484,611,757,783]" name="Guatemala" pageId="119" pageNumber="120">Guatemala</collectingCountry>
