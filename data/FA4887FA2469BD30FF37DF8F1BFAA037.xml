<geoCoordinate id="17D5502B2469BD30FC66DC8C1F0DA1E9" box="[966,1074,718,744]" degrees="92" direction="west" minutes="37" orientation="longitude" pageId="119" pageNumber="120" precision="925" value="-92.61667">92°37’W</geoCoordinate>
