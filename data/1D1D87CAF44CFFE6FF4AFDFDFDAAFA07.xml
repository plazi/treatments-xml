<number id="237C24FBF44CFFE4FC4EFC0EFC09FBAA" box="[915,959,1001,1027]" pageId="5" pageNumber="62" value="436.0">436</number>
(
<number id="237C24FBF44CFFE4FC12FC0EFB8DFBAA" box="[975,1083,1001,1027]" pageId="5" pageNumber="62" value="450.0" valueMax="516.0" valueMin="384.0">384–516</number>
