<taxonomicName id="4C0518E38F2DFF90FEFEFA5F423D3A92" authority="HUBER, 2000" authorityName="HUBER" authorityYear="2000" box="[336,555,1511,1532]" class="Arachnida" family="Pholcidae" genus="Papiamenta" kingdom="Animalia" order="Araneae" pageId="121" pageNumber="122" phylum="Arthropoda" rank="species" species="savonet" status="n. gen., n. sp.">
