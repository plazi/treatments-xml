<taxonomicName id="5CA6958AFFDF07340999FEC59911AB70" authority="Cabanis, 1870c: 457" authorityName="Cabanis" authorityPageNumber="457" authorityYear="1870" box="[151,622,260,283]" class="Aves" family="Prunellidae" genus="Accentor" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="2" pageNumber="66" phylum="Chordata" rank="species" species="erythropygus">
