[
<bibRefCitation id="DD494B2AFFCA7734BD83A7199D208F7B" author="Stossich" box="[932,1102,1337,1362]" pageId="38" pageNumber="39" refString="Stossich, M. (1902) Monostomum mutabile Zeder e le sue forme affini. Bollettino della Societ'a Adriatica di Scienz Naturali in Trieste, 21, 1 - 40." type="journal article" year="1902">
