<taxonomicName id="B5F693C2F6BDBFB0CF4990AB46EC3658" class="Magnoliopsida" family="Asteraceae" genus="Achillea" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="562" phylum="Tracheophyta" rank="species" species="nana">
