<paragraph id="D0026A223958FFB3FF19FEC5FD87FDE7" blockId="16.[189,1399,313,516]" pageId="16" pageNumber="17">
<taxonomicName id="17BD11A13958FFB3FF19FEC5FE43FEB0" authority="Grootaert, 2019" authorityName="Grootaert" authorityYear="2019" box="[189,477,313,339]" class="Insecta" family="Hybotidae" genus="Elaphropeza" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="hongshulin" status="sp. nov.">
