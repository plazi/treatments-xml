<taxonomicName id="4C364D1DFFFCFFD4AACCF97D140046F1" authority="(Cinara) Curtis, 1835" authorityName="Curtis" authorityYear="1835" baseAuthorityName="Cinara" box="[984,1333,1678,1705]" class="Insecta" family="Aphididae" genus="Cinara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="subGenus" subGenus="Cinara">
