<emphasis id="CF8C52457B64FFC3FF37EB4F901EFC78" box="[185,307,870,896]" inLineHeading="true" pageId="21" pageNumber="22" reason="1">Diagnosis</emphasis>
.
</emphasis>
<taxonomicName id="3AF8F5D47B64FFC3FEB1EB4E90DBFC79" authorityName="Crabo &amp; Lafontaine" authorityYear="2009" box="[319,502,871,897]" class="Insecta" family="Noctuidae" genus="Lasionycta" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="haida">
