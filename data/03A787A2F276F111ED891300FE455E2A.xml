<taxonomicName id="4C0E4D37F277F111ED6116B8FEE75C7A" class="Aves" family="Paridae" genus="Poecile" kingdom="Animalia" order="Passeriformes" pageId="67" pageNumber="67" phylum="Chordata" rank="subSpecies" species="communis" subSpecies="hellmayri">
