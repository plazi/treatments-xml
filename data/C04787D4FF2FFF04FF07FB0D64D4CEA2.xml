<taxonomicName id="8FEE4D41FF2FFF04FF67FB0D66F6CF82" authority="Apanaskevich and Tomlinson, 2019" authorityName="Apanaskevich and Tomlinson" authorityYear="2019" box="[247,949,1194,1221]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="198" pageNumber="199" phylum="Arthropoda" rank="species" species="walkerae">
