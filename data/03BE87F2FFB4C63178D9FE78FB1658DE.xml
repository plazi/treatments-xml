<quantity id="4CEF9B01FFB4C6317A31FC16FD4B5C9B" box="[639,750,923,949]" metricMagnitude="-4" metricUnit="m" metricValue="8.4" pageId="7" pageNumber="8" unit="mm" value="0.84">0.84 mm</quantity>
