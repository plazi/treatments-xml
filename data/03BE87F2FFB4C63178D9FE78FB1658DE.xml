(n = 
<number id="3DDF24C3FFB4C631790AFCF9FEF75CA0" box="[324,338,884,910]" pageId="7" pageNumber="8" value="1.0">1</number>
).
</paragraph>
<paragraph id="8BA836E4FFB4C6317888FC16FE925CF2" blockId="7.[151,1437,613,1491]" pageId="7" pageNumber="8">
Total length 
<quantity id="4CEF9B01FFB4C631792FFC16FE745C9B" box="[353,465,923,949]" metricMagnitude="-3" metricUnit="m" metricValue="1.32" pageId="7" pageNumber="8" unit="mm" value="1.32">1.32 mm</quantity>
