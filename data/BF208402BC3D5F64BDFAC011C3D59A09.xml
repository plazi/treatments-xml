<paragraph id="6C73D953E205F990FD4FD5F7312451CE" pageId="64" pageNumber="89">Diagnostic characters.</paragraph>
<paragraph id="CFAF70E02AE94F8DD2D8F3D6E058343E" pageId="64" pageNumber="89">
<taxonomicName id="E0B2639D5D6BA4D37A3BCA00F83F1DD8" class="Magnoliopsida" family="Asteraceae" genus="Strobocalyx" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="64" pageNumber="89" phylum="Tracheophyta" rank="species" species="arborea">
