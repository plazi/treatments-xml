<treatmentCitation id="8413D275FFE3FFE1D73A327895ACCF5E" author="Rambur, J. P." box="[94,442,1338,1363]" page="359" pageId="13" pageNumber="14" year="1842">
<taxonomicName id="C2B28FE7FFE3FFE1D73A327895ACCF5E" authority="Rambur, 1842: 359" authorityName="Rambur" authorityPageNumber="359" authorityYear="1842" box="[94,442,1338,1363]" class="Insecta" family="Ascalaphidae" genus="Cordulecerus" kingdom="Animalia" order="Neuroptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis id="37C62876FFE3FFE1D73A327894FFCF5E" box="[94,233,1338,1363]" italics="true" pageId="13" pageNumber="14">Cordulecerus</emphasis>
<bibRefCitation id="61238995FFE3FFE1D789327895ACCF5E" author="Rambur, J. P." box="[237,442,1338,1363]" pageId="13" pageNumber="14" refId="ref12445" refString="Rambur, J. P. 1842. Histoire naturelle des insectes, nevropteres. Paris, Librairie encyclopedique de Roret. [xviii] + 534 p." type="book" year="1842">Rambur,1842:359</bibRefCitation>
</taxonomicName>
</treatmentCitation>
