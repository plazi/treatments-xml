<paragraph id="F8578FAB2CC6276DC3BFE98AA596107D" pageId="22" pageNumber="23">
<taxonomicName id="5356DD6367805DC5C853CF0240D2F118" LSID="35E7A3CA-9104-5BE1-9AAF-7406B626F979" authority="(L.) L." authorityName="(L.) L." class="Magnoliopsida" family="Asteraceae" genus="Ageratum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ageratum conyzoides" order="Asterales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="conyzoides">Ageratum conyzoides (L.) L.</taxonomicName>
