<taxonomicName class="Gastropoda" family="Discodorididae" genus="Rostanga" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rostanga orientalis" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="species" species="orientalis">
<emphasis italics="true" pageId="0" pageNumber="73">Rostanga orientalis</emphasis>
</taxonomicName>
