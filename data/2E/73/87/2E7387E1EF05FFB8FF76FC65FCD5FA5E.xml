<collectionCode LSID="urn:lsid:biocol.org:col:34700" box="[844,898,1427,1451]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="15">CAS</collectionCode>
