<taxonomicName authority="Becker" authorityName="Becker" authorityYear="1903" box="[1225,1435,1332,1353]" class="Insecta" family="Ephydridae" genus="Clanoneurum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="85" phylum="Arthropoda" rank="genus">
