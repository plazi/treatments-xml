<taxonomicName authority="Arnaud" authorityName="Arnaud, Mathis &amp; Zatwarnicki" authorityYear="2004" box="[1221,1436,1156,1177]" class="Insecta" family="Ephydridae" genus="Cressonomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="85" phylum="Arthropoda" rank="genus">
