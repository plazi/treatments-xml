<collectingRegion box="[702,929,837,864]" country="Brazil" name="Mato Grosso do Sul" pageId="13" pageNumber="474">Mato Grosso do Sul</collectingRegion>
, 
<collectingCountry box="[940,1008,837,863]" name="Brazil" pageId="13" pageNumber="474">Brazil</collectingCountry>
(
