<quantity box="[395,441,964,990]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="145" pageNumber="146" unit="in" value="5.0">5 in</quantity>
<collectionCode box="[448,524,964,990]" country="Costa Rica" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="145" pageNumber="146">INBio</collectionCode>
, 
<quantity box="[536,581,964,990]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" pageId="145" pageNumber="146" unit="in" value="3.0">3 in</quantity>
