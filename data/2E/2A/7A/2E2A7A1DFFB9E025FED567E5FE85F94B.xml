, P[arque] N[acional] Corcovado, Prov[incia] Punt[arenas], 
<collectingCountry box="[574,752,1410,1436]" name="Costa Rica" pageId="106" pageNumber="107">
<collectionCode box="[574,668,1410,1436]" pageId="106" pageNumber="107">COSTA</collectionCode>
