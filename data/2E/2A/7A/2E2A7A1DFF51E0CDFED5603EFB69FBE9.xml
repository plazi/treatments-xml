<collectingCountry box="[839,932,1044,1070]" name="Panama" pageId="130" pageNumber="131">Panamá</collectingCountry>
(Walsingham, 
<number box="[1109,1171,1044,1070]" pageId="130" pageNumber="131" value="1911.0">1911</number>
