of p 
<number box="[836,844,1178,1192]" pageId="7" pageNumber="46" value="3.0">3</number>
<number box="[852,896,1162,1186]" pageId="7" pageNumber="46" value="1.5" valueMax="3.0" valueMin="0.0">0–3</number>
, 
<number box="[910,962,1162,1186]" pageId="7" pageNumber="46" value="0.42">0.42</number>
