<taxonomicName authority="DE GEER" authorityName="DE GEER" box="[342,571,1556,1578]" class="Insecta" family="Aphididae" genus="Aphis" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="506" phylum="Arthropoda" rank="species" species="pomi">
<emphasis box="[342,463,1556,1578]" italics="true" pageId="1" pageNumber="506">Aphis pomi</emphasis>
DE GEER
</taxonomicName>
(
<taxonomicName box="[589,698,1556,1578]" class="Insecta" family="Aphididae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="506" phylum="Arthropoda" rank="family">Aphididae</taxonomicName>
