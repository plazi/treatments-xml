<taxonomicName baseAuthorityName="REUTER" baseAuthorityYear="1884" class="Insecta" family="Anthocoridae" genus="Orius" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="506" phylum="Arthropoda" rank="species" species="horvathi">
