<emphasis italics="true" pageId="0" pageNumber="83">C. siamensis</emphasis>
</taxonomicName>
, and 
<taxonomicName genus="C." lsidName="C. flavicans" pageId="0" pageNumber="83" rank="species" species="flavicans">
<emphasis italics="true" pageId="0" pageNumber="83">C. flavicans</emphasis>
