<taxonomicName authority="Tilbrook, Hayward &amp; Gordon, 2001" authorityName="Tilbrook, Hayward &amp; Gordon" authorityYear="2001" class="Gymnolaemata" family="Adeonidae" genus="Reptadeonella" kingdom="Animalia" order="Cheilostomatida" pageId="8" pageNumber="356" phylum="Bryozoa" rank="species" species="cellulanus">
<emphasis box="[1150,1436,1919,1943]" italics="true" pageId="8" pageNumber="356">Reptadeonella cellulanus</emphasis>
<bibRefCitation author="Tilbrook" box="[151,583,1955,1980]" pageId="8" pageNumber="356" refString="Tilbrook, K. J., Hayward, P. J. &amp; Gordon, D. P. (2001) Cheilostomatous Bryozoa from Vanuatu. Zoological Journal of the Linnean Society, 131, 35 - 109. http: // dx. doi. org / 10.1006 / zjls. 2000.0249" type="journal article" year="2001">
Tilbrook, Hayward &amp; Gordon, 
<number box="[525,583,1955,1979]" pageId="8" pageNumber="356" value="2001.0">2001</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Tilbrook, 2006" authorityName="Tilbrook" authorityYear="2006" box="[600,1086,1955,1980]" class="Gymnolaemata" family="Adeonidae" genus="Reptadeonella" kingdom="Animalia" order="Cheilostomatida" pageId="8" pageNumber="356" phylum="Bryozoa" rank="species" species="falciformis">
<emphasis box="[600,896,1955,1979]" italics="true" pageId="8" pageNumber="356">Reptadeonella falciformis</emphasis>
<bibRefCitation author="Tilbrook" box="[910,1086,1955,1980]" pageId="8" pageNumber="356" refString="Tilbrook, K. J. (2006) Cheilostomatous Bryozoa from the Solomon Islands. Santa Barbara Museum of Natural History, Monographs 4, Studies in Biodiversity Number 3, 1 - 386." type="journal article" year="2006">
Tilbrook, 
<number box="[1027,1086,1955,1980]" pageId="8" pageNumber="356" value="2006.0">2006</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName box="[1103,1254,1955,1979]" class="Gymnolaemata" family="Adeonidae" genus="Reptadeonella" kingdom="Animalia" order="Cheilostomatida" pageId="8" pageNumber="356" phylum="Bryozoa" rank="species" species="granulosa">
<emphasis box="[1103,1254,1955,1979]" italics="true" pageId="8" pageNumber="356">R. granulosa</emphasis>
</taxonomicName>
, 
<taxonomicName authority="Tilbrook, 2006" authorityName="Tilbrook" authorityYear="2006" class="Gymnolaemata" family="Adeonidae" genus="Reptadeonella" kingdom="Animalia" order="Cheilostomatida" pageId="8" pageNumber="356" phylum="Bryozoa" rank="species" species="hystricosus">
<emphasis italics="true" pageId="8" pageNumber="356">Reptadeonella hystricosus</emphasis>
<bibRefCitation author="Tilbrook" box="[287,460,1991,2016]" pageId="8" pageNumber="356" refString="Tilbrook, K. J. (2006) Cheilostomatous Bryozoa from the Solomon Islands. Santa Barbara Museum of Natural History, Monographs 4, Studies in Biodiversity Number 3, 1 - 386." type="journal article" year="2006">
Tilbrook, 
<number box="[401,460,1991,2016]" pageId="8" pageNumber="356" value="2006.0">2006</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Borg, 1940" authorityName="Borg" authorityYear="1940" box="[474,905,1991,2015]" class="Gymnolaemata" family="Adeonidae" genus="Reptadeonella" kingdom="Animalia" order="Cheilostomatida" pageId="8" pageNumber="356" phylum="Bryozoa" rank="species" species="levinseni">
<emphasis box="[474,746,1991,2015]" italics="true" pageId="8" pageNumber="356">Reptadeonella levinseni</emphasis>
(
<bibRefCitation author="Borg" box="[763,897,1991,2015]" pageId="8" pageNumber="356" refString="Borg, F. (1940) On the genus Tubiporella and on a new boring bryozoan. Zoologiska Bidrag fran Uppsala, 18, 415 - 437." type="journal article" year="1940">
Borg, 
<number box="[837,897,1991,2015]" pageId="8" pageNumber="356" value="1940.0">1940</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName box="[920,1064,1991,2015]" class="Gymnolaemata" family="Adeonidae" genus="Adeona" kingdom="Animalia" order="Cheilostomatida" pageId="8" pageNumber="356" phylum="Bryozoa" rank="species" species="tubulifera">
<emphasis box="[920,1064,1991,2015]" italics="true" pageId="8" pageNumber="356">R. tubulifera</emphasis>
