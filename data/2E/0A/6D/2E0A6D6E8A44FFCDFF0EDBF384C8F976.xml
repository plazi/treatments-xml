<paragraph blockId="4.[151,934,1521,1548]" box="[151,934,1521,1548]" pageId="4" pageNumber="29">
<heading bold="true" box="[151,934,1521,1548]" fontSize="11" level="1" pageId="4" pageNumber="29" reason="1">
<emphasis bold="true" box="[151,934,1521,1548]" pageId="4" pageNumber="29">
b. 
<taxonomicName authority="David &amp; Ranganath" authorityName="David &amp; Ranganath" box="[182,835,1521,1548]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="29" phylum="Arthropoda" rank="species" species="chettalli" status="sp.nov." subGenus="Calodacus">
<emphasis bold="true" box="[182,315,1521,1547]" italics="true" pageId="4" pageNumber="29">Bactrocera</emphasis>
(
<emphasis bold="true" box="[332,459,1521,1547]" italics="true" pageId="4" pageNumber="29">Calodacus</emphasis>
) 
<emphasis bold="true" box="[476,574,1521,1547]" italics="true" pageId="4" pageNumber="29">chettalli</emphasis>
David &amp; Ranganath
</taxonomicName>
, 
<taxonomicNameLabel box="[848,934,1522,1548]" pageId="4" pageNumber="29" rank="species">sp.nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
