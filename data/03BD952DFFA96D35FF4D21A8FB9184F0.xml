<taxonomicName id="4C145FB8FFAA6D35FDBF2471FB918647" authority="Michener, Engel &amp; Ayala" authorityName="Michener, Engel &amp; Ayala" box="[638,1107,342,369]" class="Insecta" family="Colletidae" genus="Caupolicana" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="species" species="rozenorum">
