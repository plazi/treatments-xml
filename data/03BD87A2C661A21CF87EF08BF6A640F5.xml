<paragraph id="8BAB36B4C661A213F542EF08F2235211" blockId="23.[2744,3541,4084,4650]" pageId="23" pageNumber="250">
<taxonomicName id="4C144D37C661A213F542EF08F3A853FF" baseAuthorityName="Peters" baseAuthorityYear="1871" box="[2746,3143,4242,4281]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="23" pageNumber="250" phylum="Chordata" rank="species" species="coronatus">
