<number id="1F59248BFFD6FF8E5091F9A29AFD3523" box="[966,1021,1537,1563]" pageId="2" pageNumber="3" value="0.34">0.34</number>
; PSL &amp; MSL, obscured; MW, 
<number id="1F59248BFFD6FF8E521DF98A9881357B" box="[330,385,1577,1603]" pageId="2" pageNumber="3" value="0.53">0.53</number>
; 
<collectionCode id="CF80AE69FFD6FF8E52DBF98A98D9357B" box="[396,473,1577,1603]" pageId="2" pageNumber="3">AWIV</collectionCode>
