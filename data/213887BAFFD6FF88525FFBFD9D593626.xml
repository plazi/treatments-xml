<number id="1F59248BFFD6FF8E5191F9A29BFD3523" box="[710,765,1537,1563]" pageId="2" pageNumber="3" value="0.52">0.52</number>
; HL, 
<number id="1F59248BFFD6FF8E5012F9A29A7C3523" box="[837,892,1537,1563]" pageId="2" pageNumber="3" value="0.33">0.33</number>
