<taxonomicName id="4C564D11A46FFFDA7B07FB70FE1D448B" authorityName="Temminck" authorityYear="1841" box="[307,435,1214,1237]" class="Mammalia" family="Talpidae" genus="Urotrichus" kingdom="Animalia" order="Soricomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
