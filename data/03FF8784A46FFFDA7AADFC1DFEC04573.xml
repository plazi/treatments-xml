<taxonomicName id="4C564D11A46FFFDA7A48FB70FF41448B" authorityName="Milne-Edwards" authorityYear="1871" box="[124,239,1214,1237]" class="Mammalia" family="Talpidae" genus="Uropsilus" kingdom="Animalia" order="Soricomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
