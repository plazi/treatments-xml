<taxonomicName id="4C564D11A46FFFDA7BABFB6FFDB144E6" authorityName="Milne-Edwards" authorityYear="1872" box="[415,543,1185,1208]" class="Mammalia" family="Talpidae" genus="Scaptonyx" kingdom="Animalia" order="Soricomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46FFFDA7BABFB6FFDB144E6" box="[415,543,1185,1208]" italics="true" pageId="14" pageNumber="15">Scaptonyx</emphasis>
