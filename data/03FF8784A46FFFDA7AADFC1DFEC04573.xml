<taxonomicName id="4C564D11A46FFFDA7B72FB15FE6C44AC" authorityName="Illiger" authorityYear="1811" box="[326,450,1243,1266]" class="Mammalia" family="Talpidae" genus="Condylura" kingdom="Animalia" order="Soricomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
