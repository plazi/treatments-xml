<taxonomicName id="4C564D11A46FFFDA7B3EFB6FFE2F44E6" box="[266,385,1185,1208]" class="Mammalia" family="Talpidae" genus="Scapanus" kingdom="Animalia" order="Soricomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46FFFDA7B3EFB6FFE2F44E6" box="[266,385,1185,1208]" italics="true" pageId="14" pageNumber="15">Scapanus</emphasis>
