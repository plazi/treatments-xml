<taxonomicName id="4C564D11A46FFFDA7808FB6FFD2A44E6" authorityName="Linnaeus" authorityYear="1758" box="[572,644,1185,1208]" class="Mammalia" family="Talpidae" genus="Talpa" kingdom="Animalia" order="Soricomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
