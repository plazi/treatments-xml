<taxonomicName id="CFCDAF2C3137A9536A0A5967CAAE646C" authority="Berlese, 1916" authorityName="Berlese" authorityYear="1916" class="Arachnida" family="Ameroseiidae" genus="Epicriopsis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epicriopsis" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
