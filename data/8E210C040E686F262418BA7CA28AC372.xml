(as 
<taxonomicName id="8BD5FA596D12A928221CE3FAB70DE78F" authority="Westwood, 1838" authorityName="Westwood" authorityYear="1838" class="Insecta" family="Apidae" genus="Mesotrichia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Mesotrichia lepeletieri" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="lepeletieri">Mesotrichia lepeletieri</taxonomicName>
