<taxonomicName id="4C434D4A7551FFD27A106103FE21BB35" authorityName="FAUVEL" authorityYear="1878" box="[274,393,1346,1367]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="1010" phylum="Arthropoda" rank="species" species="prismalis">
