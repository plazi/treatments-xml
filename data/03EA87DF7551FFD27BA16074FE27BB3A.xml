<taxonomicName id="4C434D4A7551FFD2797160ABFC8CBA9D" authority="Mainda &amp; M &amp; M, 2021" authorityName="Mainda &amp; M &amp; M" authorityYear="2021" box="[627,804,1258,1279]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="1010" phylum="Arthropoda" rank="species" species="attenboroughi" status="nov.sp.">
