<taxonomicName id="4C434D4A7551FFD27F186164FF49BB35" authorityName="PUTHZ" authorityYear="2016" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="1010" phylum="Arthropoda" rank="species" species="balkei">
