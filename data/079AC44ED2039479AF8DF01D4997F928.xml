<taxonomicName id="25168406840AA37564F5DA48740879CA" class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus balsamifera" order="Malpighiales" pageId="11" pageNumber="360" phylum="Tracheophyta" rank="species" species="balsamifera">Populus balsamifera</taxonomicName>
