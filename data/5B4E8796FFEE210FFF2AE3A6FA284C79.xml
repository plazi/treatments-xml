<taxonomicName id="14E74D03FFE9210FFC9FE2C5FB0C48A9" authority="(Link)" baseAuthorityName="Link" baseAuthorityYear="1795" box="[885,1156,441,465]" class="Mammalia" family="Tayassuidae" genus="Tayassu" kingdom="Animalia" order="Artiodactyla" pageId="4" pageNumber="3" phylum="Chordata" rank="species" species="pecari">
