<taxonomicName id="D591488006E8812A86027718C77F6DD1" authorityName="Yoo &amp; Thi &amp; Karanovic" authorityYear="2020" class="Ostracoda" family="Paradoxostomatidae" genus="Cytherois" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cytherois gajinensis" order="Podocopida" pageId="0" pageNumber="21" phylum="Arthropoda" rank="species" species="gajinensis">
<emphasis id="662C2D9D3048671950545ED1B61C229D" italics="true" pageId="0" pageNumber="21">Cytherois gajinensis</emphasis>
