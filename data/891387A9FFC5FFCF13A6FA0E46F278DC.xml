<taxonomicName id="C6BA4D3CFFC4FFCF1293FEC9438F782F" box="[418,502,312,338]" class="Insecta" family="Pieridae" genus="Catasticta" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="flisa">
<emphasis id="33CEEAADFFC4FFCF1293FEC943CC782F" box="[418,437,312,337]" italics="true" pageId="3" pageNumber="4">C</emphasis>
