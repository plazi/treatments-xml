<taxonomicName id="4C7D4D667901FFA0433C9F8DD4B3F851" authority="(Boie)" baseAuthorityName="Boie" box="[621,913,2002,2026]" class="Reptilia" family="Lamprophiidae" genus="Boaedon" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="273" phylum="Chordata" rank="species" species="fuliginosus">
<emphasis id="B909EAF77901FFA0433C9F8DD465F851" box="[621,839,2002,2026]" italics="true" pageId="9" pageNumber="273">Boaedon fuliginosus</emphasis>
(Boie)
</taxonomicName>
(
<taxonomicName id="4C7D4D667901FFA042CE9F8DD32EF851" box="[927,1036,2002,2026]" class="Reptilia" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="273" phylum="Chordata" rank="order">Squamata</taxonomicName>
: 
<taxonomicName id="4C7D4D667901FFA045489F8DD386F851" authorityName="Oppell" authorityYear="1811" box="[1049,1188,2002,2026]" class="Reptilia" family="Colubridae" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="273" phylum="Chordata" rank="family">Colubridae</taxonomicName>
