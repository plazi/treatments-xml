<subSubSection id="C37D65360C2EF8036DA09C34FAA7702E" box="[192,1356,1571,1595]" pageId="62" pageNumber="61" type="etymology">
<paragraph id="8BD836BD0C2EF8036DA09C34FAA7702E" blockId="62.[192,1356,1571,1595]" box="[192,1356,1571,1595]" pageId="62" pageNumber="61">
<emphasis id="B913EAAF0C2EF8036DA09C34FEB6702E" bold="true" box="[192,349,1571,1595]" pageId="62" pageNumber="61">Etymology.</emphasis>
