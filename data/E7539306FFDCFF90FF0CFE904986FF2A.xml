<quantity id="A8028FF5FFDCFF96FEC6FD1C49E1FD3A" box="[349,428,686,710]" metricMagnitude="-2" metricUnit="m" metricValue="3.1" pageId="3" pageNumber="242" unit="mm" value="31.0">31 mm</quantity>
