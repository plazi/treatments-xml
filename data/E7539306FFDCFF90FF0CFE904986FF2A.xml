<taxonomicName id="A8FA5993FFDCFF96FF05FD474952FCF1" box="[158,287,757,782]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="242" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
