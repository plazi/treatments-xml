<taxonomicName id="4C334D73FFF2FF9AFE708FCEFDE6FC31" authorityName="Alonso-Zarazaga" authorityYear="1990" box="[485,622,980,1006]" class="Insecta" family="Brentidae" genus="Neotropion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="560" phylum="Arthropoda" rank="genus">
