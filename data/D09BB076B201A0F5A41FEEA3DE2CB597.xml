<taxonomicName id="7E34F10091810DD9C5479E7256D235B4" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis fasciata" order="Hymenoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="fasciata">Chrysis fasciata</taxonomicName>
