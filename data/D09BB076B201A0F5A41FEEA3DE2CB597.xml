<taxonomicName id="BD7A2204AE5CD89364A4192FE6D2F1CB" authority="Linsenmaier.Later, 1951" authorityName="Linsenmaier.Later" authorityYear="1951" class="Insecta" family="Chrysididae" genus="Pseudospinolia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pseudospinolia uniformis" order="Hymenoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="uniformis">Pseudospinolia uniformis</taxonomicName>
