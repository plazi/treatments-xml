<paragraph id="8BCA322F9076FFF3FF6CF9C5FC09793E" blockId="17.[151,1436,1590,2011]" box="[199,965,1626,1651]" pageId="17" pageNumber="88">
<materialsCitation id="3B1D38729076FFF3FF6CF9C5FC09793E" ID-GBIF-Occurrence="1314597123" box="[199,965,1626,1651]" collectingDate="2014-07" latitude="20.775" location="Hai Phong" longLatPrecision="21" longitude="107.077225" pageId="17" pageNumber="88" specimenCount="1" stateProvince="Hai Phong">
