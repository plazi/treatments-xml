<taxonomicName id="4C27E3B77E66FFEAFC4E769ADBC3FA33" authority="Evans, 1955" authorityName="Evans" authorityYear="1955" box="[945,1245,1456,1480]" class="Insecta" family="Hesperiidae" genus="Dalla" kingdom="Animalia" order="Lepidoptera" pageId="20" pageNumber="19" phylum="Arthropoda" rank="species" species="decca">
