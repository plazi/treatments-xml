<taxonomicName id="4C27E3B77E66FFEAFD77769ADCB9FA33" authority="Evans, 1955" authorityName="Evans" authorityYear="1955" box="[648,935,1456,1480]" class="Insecta" family="Hesperiidae" genus="Dalla" kingdom="Animalia" order="Lepidoptera" pageId="20" pageNumber="19" phylum="Arthropoda" rank="species" species="tona">
