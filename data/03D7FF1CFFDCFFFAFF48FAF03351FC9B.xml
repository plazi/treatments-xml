<taxonomicName id="4C7E3589FFDCFFFAFBC2F85D359EFCBB" authorityName="Westwood" authorityYear="1846" box="[1098,1250,819,843]" class="Insecta" family="Hybosoridae" genus="Coilodes" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="castaneus">
<emphasis id="B90A9218FFDCFFFAFBC2F85D359EFCBB" box="[1098,1250,819,843]" italics="true" pageId="23" pageNumber="22">C. castaneus</emphasis>
