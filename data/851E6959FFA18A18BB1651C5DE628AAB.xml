<bibRefCitation id="6926A5BEFFA18A19BB345690D9188C98" author="Brown" box="[769,891,1708,1729]" pageId="4" pageNumber="683" pagination="65 - 78" refId="ref4737" refString="Brown, W. J. 1940. Notes on the American distribution of some species of Coleoptera common to the European and North American continents. The Canadian Entomologist 72: 65 - 78." type="journal article" year="1940">Brown 1940</bibRefCitation>
) and is associated with species of 
<taxonomicName id="CAB7A3CCFFA08A18B88150FEDB6D8A8E" box="[180,270,194,215]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="684" phylum="Tracheophyta" rank="genus">
<emphasis id="3FC3045DFFA08A18B88150FEDB6D8A8E" box="[180,270,194,215]" italics="true" pageId="5" pageNumber="684">Trifolium</emphasis>
</taxonomicName>
, 
<taxonomicName id="CAB7A3CCFFA08A18B92B50FEDB1D8A8E" box="[286,382,194,215]" class="Magnoliopsida" family="Fabaceae" genus="Medicago" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="684" phylum="Tracheophyta" rank="genus">
<emphasis id="3FC3045DFFA08A18B92B50FEDB1D8A8E" box="[286,382,194,215]" italics="true" pageId="5" pageNumber="684">Medicago</emphasis>
</taxonomicName>
, and 
<taxonomicName id="CAB7A3CCFFA08A18B98C50FEDB8D8A8E" box="[441,494,194,215]" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="684" phylum="Tracheophyta" rank="genus">
<emphasis id="3FC3045DFFA08A18B98C50FEDB8D8A8E" box="[441,494,194,215]" italics="true" pageId="5" pageNumber="684">Lotus</emphasis>
</taxonomicName>
L. (
<taxonomicName id="CAB7A3CCFFA08A18BA2E50FED8148A8E" baseAuthorityName="Hoffmann" baseAuthorityYear="1954" box="[539,631,194,215]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="684" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
) (
