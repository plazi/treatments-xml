<taxonomicName id="F5AD4D6D7E024147FEC1FC21FF6E7DA6" authority="(Ridley &amp; Dendy, 1887)" authorityName="Ridley &amp; Dendy" authorityYear="1887" baseAuthorityName="Ridley &amp; Dendy" baseAuthorityYear="1887" box="[297,807,896,918]" class="Demospongiae" family="Suberitidae" genus="Pseudosuberites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Suberitida" pageId="23" pageNumber="424" phylum="Porifera" rank="species" species="hyalinus">
