<geoCoordinate id="E78335817CE27A3339E504942FF3BA1F" direction="north" orientation="latitude" precision="15" value="29.951668">N29°57'06&quot;</geoCoordinate>
, 
<geoCoordinate id="79E0F6C43E072ACC1839EF233351A66B" direction="east" orientation="longitude" precision="15" value="109.49694">E109°29'49&quot;</geoCoordinate>
