<taxonomicName id="55D84D5AFFC51772F99DFAD0E5CF5FF1" authorityName=", Matthew" authorityYear="1916" box="[1575,1975,1390,1419]" class="Mammalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Multituberculata" pageId="47" pageNumber="364" phylum="Chordata" rank="order">MULTITUBERCULATA</taxonomicName>
</heading>
</paragraph>
