<heading id="D0F5BC7CFFBEFFDEA181F9A6FCBBF9CA" bold="true" box="[151,880,1553,1579]" fontSize="11" level="1" pageId="21" pageNumber="516" reason="1">
<taxonomicName id="4C027093FFBEFFDEA181F9A6FCBBF9CA" authority="Lansberge, 1886" authorityName="Lansberge" authorityYear="1886" box="[151,880,1553,1579]" class="Insecta" family="Bolboceratidae" genus="Bolbochromus" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="516" phylum="Arthropoda" rank="species" species="ludekingi" subGenus="Bolbochromops">
<emphasis id="B976D702FFBEFFDEA181F9A6FCBBF9CA" bold="true" box="[151,880,1553,1579]" pageId="21" pageNumber="516">
<emphasis id="B976D702FFBEFFDEA181F9A6FE82F9CA" bold="true" box="[151,329,1553,1579]" italics="true" pageId="21" pageNumber="516">Bolbochromus</emphasis>
(
<emphasis id="B976D702FFBEFFDEA04FF9A6FDDCF9CA" bold="true" box="[345,535,1553,1579]" italics="true" pageId="21" pageNumber="516">Bolbochromops</emphasis>
) 
<emphasis id="B976D702FFBEFFDEA33FF9A6FD50F9CA" bold="true" box="[553,667,1553,1579]" italics="true" pageId="21" pageNumber="516">ludekingi</emphasis>
Lansberge, 
<number id="3DCA1937FFBEFFDEA223F9A6FCBBF9CA" box="[821,880,1553,1579]" pageId="21" pageNumber="516" value="1886.0">1886</number>
</emphasis>
