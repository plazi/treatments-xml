<taxonomicName id="4C167FE4FFF2FFD0FEEFFAE5A1AAFA99" authority="Hernández-García &amp; Burgos-Guerrero &amp; Rousseau &amp; James, 2018" authorityName="Hernández-García &amp; Burgos-Guerrero &amp; Rousseau &amp; James" authorityYear="2018" box="[297,458,1384,1406]" class="Clitellata" family="Ocnerodrilidae" genus="Brasilisia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="1" pageNumber="473" phylum="Annelida" rank="species" species="punki" status="n. sp.">
<emphasis id="B962D875FFF2FFD0FEEFFAE5A1AAFA99" box="[297,458,1384,1406]" italics="true" pageId="1" pageNumber="473">Brasilisia punki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A251650EFFF2FFD0FE17FAE5A26AFA99" box="[465,522,1384,1406]" pageId="1" pageNumber="473" rank="species">
<emphasis id="B962D875FFF2FFD0FE17FAE5A26AFA99" bold="true" box="[465,522,1384,1406]" pageId="1" pageNumber="473">n. sp.</emphasis>
