<taxonomicName id="A2EB4D13591C983DD24D8C50768999C8" baseAuthorityName="Haase" baseAuthorityYear="1887" box="[855,939,1904,1930]" class="Chilopoda" family="Scolopendridae" genus="Scolopendra" kingdom="Animalia" order="Scolopendromorpha" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="laeta">
<emphasis id="579FEA82591C983DD24D8C50768999C8" box="[855,939,1904,1930]" italics="true" pageId="73" pageNumber="74">S. laeta</emphasis>
</taxonomicName>
.
</paragraph>
