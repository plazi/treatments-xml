</taxonomicName>
, 
<taxonomicName id="03FD4D09B618CD705AD2FACBFB49A105" authority="Sanborn, 2020" authorityName="Sanborn" authorityYear="2020" box="[1019,1212,1323,1349]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="rumipataensis" status="n. sp.">
<emphasis id="F689EA98B618CD705AD2FACBFB49A105" box="[1019,1212,1323,1349]" italics="true" pageId="69" pageNumber="70">C. rumipataensis</emphasis>
</taxonomicName>
<emphasis id="F689EA98B618CD705DECFACAFAF0A105" bold="true" box="[1221,1285,1322,1348]" pageId="69" pageNumber="70">
<taxonomicNameLabel id="EDBA57E3B618CD705DECFACAFAF0A105" box="[1221,1285,1322,1348]" pageId="69" pageNumber="70" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="03FD4D09B618CD705C3CFACBFA86A105" authorityName="Uhler" authorityYear="1875" box="[1301,1395,1323,1349]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="socia">
<emphasis id="F689EA98B618CD705C3CFACBFA86A105" box="[1301,1395,1323,1349]" italics="true" pageId="69" pageNumber="70">C. socia</emphasis>
