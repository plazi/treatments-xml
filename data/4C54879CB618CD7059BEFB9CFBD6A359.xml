<taxonomicName id="03FD4D09B618CD705BC3FACBFC90A105" authorityName="Boulard" authorityYear="1986" box="[746,869,1322,1349]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="gemella">
