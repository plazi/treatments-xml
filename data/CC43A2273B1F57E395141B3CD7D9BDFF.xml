<taxonomicName id="FBFE9F9DD55C4AD4D8C992C167F9218C" authorityName="Nonfried" authorityYear="1894" class="Insecta" family="Scarabaeidae" genus="Pelidnota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pelidnota tricolor" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="tricolor">
