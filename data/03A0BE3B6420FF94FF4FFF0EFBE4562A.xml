<taxonomicName id="4C0974AE6420FF95FE08FE2AFDA556B9" authorityName=": Han" authorityYear="1994" box="[464,612,442,467]" class="Mammalia" family="Vespertilionidae" genus="Myotis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Chiroptera" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="bombinus">
