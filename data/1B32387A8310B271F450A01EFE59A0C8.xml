, 
<collectingMunicipality id="73401316831EB271F441A439FDAFA708" box="[372,563,829,853]" pageId="9" pageNumber="43">San Juan Creek</collectingMunicipality>
, 
<quantity id="54632489831EB271F774A439FD11A708" box="[577,653,829,853]" metricMagnitude="2" metricUnit="m" metricValue="6.437376" pageId="9" pageNumber="43" unit="mi" value="0.4">0.4 mi</quantity>
(
<quantity id="54632489831EB271F7ABA439FD68A708" box="[670,756,829,853]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="9" pageNumber="43" unit="km" value="0.6">0.6 km</quantity>
) N 
<location id="9644DFB7831EB271F588A464FE26A725" LSID="urn:lsid:plazi:treatment:1B32387A8310B271F450A01EFE59A0C8:9644DFB7831EB271F588A464FE26A725" box="[189,442,864,889]" county="Riverside Co." municipality="San Juan Creek" name="Lion Canyon Creek" pageId="9" pageNumber="43" stateProvince="California">Lion Canyon Creek</location>
, 
