<taxonomicName id="F18D43B14047D25D718CADDCC772DE93" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24775" authorityName="Mayr" authorityYear="1861" class="Insecta" family="Formicidae" genus="Prenolepis" kingdom="Animalia" lsidName-HNS="Prenolepis Mayr" order="Hymenoptera" pageNumber="80" phylum="Arthropoda" rank="genus">PRENOLEPIS</taxonomicName>
