“Neukaledοnien (
<location id="8EF383AF9547FFE8FE70FC5FFDAC793F" LSID="urn:lsid:plazi:treatment:038564629547FFE8FF77FC07FE4E7857:8EF383AF9547FFE8FE70FC5FFDAC793F" box="[400,630,1015,1040]" municipality="Pandanusblattachseln'" name="Gipfel des Ignambi" pageId="35" pageNumber="236">Gipfel des Ignambi</location>
, ‘ca. 
<quantity id="4CD478919547FFE8FD5BFC50FCCD7920" box="[699,791,1016,1040]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="35" pageNumber="236" unit="m" value="1300.0">
<elevation id="000132479547FFE8FD5BFC50FCCD7920" box="[699,791,1016,1040]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="35" pageNumber="236" unit="m" value="1300.0">1300 m</elevation>
</quantity>
, zwischen 
<collectingMunicipality id="6BF74F0E9547FFE8FC7BFC5FFB47793F" box="[923,1181,1015,1040]" pageId="35" pageNumber="236">Pandanusblattachseln’</collectingMunicipality>
)”. 
<typeStatus id="54976BD69547FFE8FB5EFC50FAFF793F" box="[1214,1317,1016,1040]" pageId="35" pageNumber="236">Syntypes</typeStatus>
