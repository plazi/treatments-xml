<quantity id="4CEF9B25FFC6FFE29B7BFBCDF9DA345C" box="[774,923,1087,1113]" metricMagnitude="2" metricUnit="m" metricValue="8.0" metricValueMax="13.0" metricValueMin="3.0" pageId="87" pageNumber="88" unit="m" value="800.0" valueMax="1300.0" valueMin="300.0">300–1,300 m</quantity>
