<quantity id="4CEF9B25FFC6FFE29BD6FBCDFE7E345F" box="[939,1087,1087,1114]" metricMagnitude="2" metricUnit="m" metricValue="7.997952" metricValueMax="12.996672" metricValueMin="2.999232" pageId="87" pageNumber="88" unit="ft" value="2624.0" valueMax="4264.0" valueMin="984.0">984–4,264 ft</quantity>
