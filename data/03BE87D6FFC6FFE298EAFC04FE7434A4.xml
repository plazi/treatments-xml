<taxonomicName id="4C174D43FFC6FFE29BF4FB91FE1D3478" box="[905,1116,1123,1149]" class="Actinopterygii" family="Himantolophidae" genus="Himantolophus" kingdom="Animalia" order="Lophiiformes" pageId="87" pageNumber="88" phylum="Chordata" rank="species" species="undetermined">
