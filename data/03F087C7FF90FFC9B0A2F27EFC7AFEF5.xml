<taxonomicName id="4C594D52FF92FFC9B190F03FFE4AFE8D" authorityName="Yang &amp; Morse" authorityYear="2020" box="[421,499,260,286]" class="Insecta" family="Leptoceridae" genus="Setodes" kingdom="Animalia" order="Trichoptera" pageId="15" pageNumber="153" phylum="Arthropoda" rank="species" species="charax">
