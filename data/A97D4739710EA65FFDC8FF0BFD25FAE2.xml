, 
<collectingRegion id="E31038CD710EA65FFD55FAE5FCACFAC2" box="[700,829,1332,1356]" country="Indonesia" name="Kalimantan Utara" pageId="2" pageNumber="549">Kalimantan</collectingRegion>
: 
<collectingRegion id="E31038CD710EA65FFCACFAE5FC1BFAC2" box="[837,906,1332,1356]" country="Malaysia" name="Sabah" pageId="2" pageNumber="549">Sabah</collectingRegion>
; 
<collectingCountry id="59C3B6BF710EA65FFC7AFAE5FC6EFAC2" box="[915,1023,1332,1356]" name="Indonesia" pageId="2" pageNumber="549">Indonesia</collectingCountry>
