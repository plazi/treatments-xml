<quantity id="4C98F7F491397336FDB2F4F5FDB06DD9" box="[548,599,699,720]" metricMagnitude="3" metricUnit="m" metricValue="4.828032" pageId="8" pageNumber="674" unit="mi" value="3.0">3 mi.</quantity>
