<collectingRegion id="49A494F391397336FDBFF422FD9A6D88" box="[553,637,620,641]" country="United States of America" name="Montana" pageId="8" pageNumber="674">Montana</collectingRegion>
and 
<collectingRegion id="49A494F391397336FF5BF4C8FEF36D92" box="[205,276,646,667]" country="United States of America" name="Oregon" pageId="8" pageNumber="674">Oregon</collectingRegion>
and represent a 
<emphasis id="B914860391397336FE70F4C8FF196DBF" bold="true" pageId="8" pageNumber="674">NEW STATE RECORD</emphasis>
: Butte Co., 
<quantity id="4C98F7F491397336FEF9F4EFFE476DBF" box="[367,416,673,694]" metricMagnitude="3" metricUnit="m" metricValue="9.656064" pageId="8" pageNumber="674" unit="mi" value="6.0">6 mi.</quantity>
