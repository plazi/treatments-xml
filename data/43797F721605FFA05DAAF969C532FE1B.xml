<number id="7D18DC431605FFA35DE2F94CC11AF8E1" box="[223,295,1782,1806]" pageId="4" pageNumber="24" value="11.605" valueMax="13.21" valueMin="10.0">10–13</number>
</figureCitation>
, 
<figureCitation id="53EBD2E11605FFA35C0FF94CC16DF8E1" box="[306,336,1782,1806]" captionStart="FIGURE 21" captionStartId="7.[151,250,1236,1259]" captionText="FIGURE 21. Dolichopeza (Sinoropeza) cuspidigera sp. nov. Wing." pageId="4" pageNumber="24">21</figureCitation>
, 
<figureCitation id="53EBD2E11605FFA35C60F94CC199F8E1" box="[349,420,1782,1806]" captionStart="FIGURES 31 – 34" captionStartId="10.[151,264,1841,1864]" captionTargetBox="[171,1421,204,1807]" captionTargetId="figure@10.[151,1436,193,1821]" captionTargetPageId="10" captionText="FIGURES 31 – 34. Dolichopeza (Sinoropeza) cuspidigera sp. nov. Male. 31. Hypopygium, lateral view; 32. ninth tergite, dorsal view; 33. lobe of gonostylus, lateral view; 34. clasper of gonostylus, lateral view. Scale bar of 31 – 32 = 0.33 mm, 33 – 34 = 0.2 mm." httpUri="https://zenodo.org/record/203482/files/figure.png" pageId="4" pageNumber="24">
<number id="7D18DC431605FFA35C60F94CC199F8E1" box="[349,420,1782,1806]" pageId="4" pageNumber="24" value="32.5" valueMax="34.0" valueMin="31.0">31–34</number>
</figureCitation>
)
</paragraph>
