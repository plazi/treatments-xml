<keyLead id="30A6C969FF9987680886FD03F8A878F3" pageId="14" pageNumber="367">
<number id="3D9F787BFF9987680886FD03FD9678D1" box="[151,165,685,707]" pageId="14" pageNumber="367" value="3.0">3</number>
