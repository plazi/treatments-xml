<taxonomicName id="AFD9A5961238FFB5FF54FB08FDF8FAD7" authority="(Clessin, 1900)" authorityName="Clessin" authorityYear="1900" baseAuthorityName="Clessin" baseAuthorityYear="1900" box="[151,605,1273,1298]" class="Gastropoda" family="Pyramidellidae" genus="Parthenina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="66" pageNumber="67" phylum="Mollusca" rank="species" species="monterosatii">
