<taxonomicName id="AFD9A5961238FFB5FC5DFB40FB0CFB0C" authorityName="Clessin" authorityYear="1900" box="[926,1193,1201,1226]" class="Gastropoda" family="Pyramidellidae" genus="Parthenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="66" pageNumber="67" phylum="Mollusca" rank="species" species="monterosatii">
