<taxonomicName id="AFD9A5961238FFB5FEA6FB24FCAAFB2B" authority="Jeffreys, 1883" authorityName="Jeffreys" authorityYear="1883" box="[357,783,1237,1262]" class="Gastropoda" family="Pyramidellidae" genus="Odostomia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="66" pageNumber="67" phylum="Mollusca" rank="species" species="brevicula">
