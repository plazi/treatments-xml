<number id="3D82E35BFFED4B29FCD9F978FCE5F88B" box="[801,873,1771,1794]" pageId="35" pageNumber="36" value="56308.0">56308</number>
). 
<number id="3D82E35BFFED4B29FC83F978FC29F88B" box="[891,933,1771,1794]" pageId="35" pageNumber="36" value="101.0">101</number>
, dorsal habitus; 
