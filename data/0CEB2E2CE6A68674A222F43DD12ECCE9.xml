<taxonomicName id="AE46FB9D2F17A604781BC3207C2C8043" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Chrysididae" genus="Hedychrum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hedychrum" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">Hedychrum</taxonomicName>
