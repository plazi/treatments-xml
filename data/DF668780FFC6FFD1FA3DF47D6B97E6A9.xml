<paragraph id="57703696FFC6FFD6FA3CF43D671BEC8C" blockId="19.[1447,2649,2950,3038]" lastBlockId="19.[1446,2648,3080,3468]" pageId="19" pageNumber="324">
<subSubSection id="1FD5651DFFC6FFD6FA3CF43D66CAEBB9" box="[1448,2434,3014,3035]" pageId="19" pageNumber="324" type="vernacular_name">
<heading id="0C3881FAFFC6FFD6FA3CF43D66CAEBB9" box="[1448,2434,3014,3035]" pageId="19" pageNumber="324">
