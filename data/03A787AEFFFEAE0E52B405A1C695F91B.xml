<taxonomicName id="4C0E4D3BFFFCAE0E536D017FC5A3FDAB" authorityName="Schellenberg" authorityYear="1926" box="[284,505,549,571]" class="Malacostraca" family="Dexaminidae" genus="Lepechinella" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="1106" phylum="Arthropoda" rank="species" species="arctica">
