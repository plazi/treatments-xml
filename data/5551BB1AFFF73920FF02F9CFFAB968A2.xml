<taxonomicName id="1AF8718FFFF73920FB00FB38FB206849" box="[1173,1266,608,631]" class="Insecta" family="Geometridae" genus="Eupithecia" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="innotata">
