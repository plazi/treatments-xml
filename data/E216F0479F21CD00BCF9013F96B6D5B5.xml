<taxonomicName id="A4479745AA7A8D77EE009283560F19B4" LSID-ZBK="urn:lsid:zoobank.org:act:5D5C78EA-1E5B-4C98-8CB2-677B409FBBD2" class="Actinopterygii" family="Cyprinidae" genus="Epalzeorhynchos" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Epalzeorhynchos Bleeker 1855:258, 270" order="Cypriniformes" pageNumber="21" phylum="Chordata" rank="genus">Epalzeorhynchos Bleeker</taxonomicName>
