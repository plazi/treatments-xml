<collectingCountry id="F3A15F858B7AFFFDFCBDF826FCD16D89" box="[803,863,1959,1985]" name="India" pageId="14" pageNumber="15">India</collectingCountry>
, 
<collectingCountry id="F3A15F858B7AFFFDFCF3F826FC3A6D89" box="[877,948,1959,1985]" name="Oman" pageId="14" pageNumber="15">Oman</collectingCountry>
, 
<collectingCountry id="F3A15F858B7AFFFDFC5CF826FBD76D89" box="[962,1113,1959,1985]" name="Saudi Arabia" pageId="14" pageNumber="15">Saudi Arabia</collectingCountry>
