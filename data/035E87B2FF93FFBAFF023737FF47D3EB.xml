<quantity id="4C0F9B41FF93FFBDFBC3318DFB47D455" box="[1110,1276,1276,1303]" metricMagnitude="-4" metricUnit="m" metricValue="5.45" metricValueMax="6.4" metricValueMin="4.5" pageId="35" pageNumber="36" unit="mm" value="0.545" valueMax="0.64" valueMin="0.45">0.45–0.64 mm</quantity>
