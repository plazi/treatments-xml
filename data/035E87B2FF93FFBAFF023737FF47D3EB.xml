<quantity id="4C0F9B41FF93FFBDFF023051FE81D479" box="[151,314,1312,1339]" metricMagnitude="-4" metricUnit="m" metricValue="1.1" metricValueMax="1.3" metricValueMin="0.8999999999999999" pageId="35" pageNumber="36" unit="mm" value="0.11" valueMax="0.13" valueMin="0.09">0.09–0.13 mm</quantity>
