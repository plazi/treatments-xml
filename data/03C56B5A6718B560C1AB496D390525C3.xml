</taxonomicName>
and 
<emphasis id="B918065E6718B560C1FA4F0B390525C3" box="[198,287,1147,1170]" italics="true" pageId="333" pageNumber="334">
<taxonomicName id="4C6CA1CF6718B560C1FA4F0B390125C3" box="[198,283,1147,1170]" class="Magnoliopsida" family="Fabaceae" genus="Ononis" kingdom="Plantae" order="Fabales" pageId="333" pageNumber="334" phylum="Tracheophyta" rank="genus">Ononis</taxonomicName>
