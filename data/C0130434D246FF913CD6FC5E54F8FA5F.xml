<collectionCode id="2EAB2DE7D246FF943D04FB6B56A9FB1E" LSID="urn:lsid:biocol.org:col:34777" box="[325,390,1184,1210]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34777" name="Museum of Vertebrate Zoology, University of California Berkeley" pageId="1" pageNumber="58">MVZ</collectionCode>
<number id="FE72A705D246FF943DCFFB6B557FFB1E" box="[398,592,1184,1210]" pageId="1" pageNumber="58" value="222496.5" valueMax="222503.0" valueMin="222490.0">222490–222503</number>
