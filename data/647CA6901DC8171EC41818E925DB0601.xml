<taxonomicName id="7DDEB2A2A53E8F76CACA36CA080F95A0" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Castanopsis armata" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="armata">
