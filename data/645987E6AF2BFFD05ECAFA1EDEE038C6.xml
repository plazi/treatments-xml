<collectingCountry id="94E77660AF2BFFD05A1BF932D96538BF" box="[1094,1143,1667,1690]" name="Iran" pageId="10" pageNumber="11">Iran</collectingCountry>
. 
<emphasis id="DE84EAE2AF2BFFD05E9AF917DC5D389A" bold="true" box="[199,335,1702,1727]" pageId="10" pageNumber="11">Comments.</emphasis>
Like 
<taxonomicName id="2BF04D73AF2BFFD05FC8F916DCEE389B" box="[405,508,1703,1726]" class="Insecta" family="Pentatomidae" genus="Arma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="custos">
<emphasis id="DE84EAE2AF2BFFD05FC8F916DCEE389B" box="[405,508,1703,1726]" italics="true" pageId="10" pageNumber="11">A. custos</emphasis>
</taxonomicName>
, 
<taxonomicName id="2BF04D73AF2BFFD05C50F916DF8A389B" box="[525,664,1703,1726]" class="Insecta" family="Pentatomidae" genus="Arma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="insperata">
<emphasis id="DE84EAE2AF2BFFD05C50F916DF8A389B" box="[525,664,1703,1726]" italics="true" pageId="10" pageNumber="11">A. insperata</emphasis>
</taxonomicName>
