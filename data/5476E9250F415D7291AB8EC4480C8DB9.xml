<taxonomicName id="60A3D6B9CBC4E89D80C111C583C89D3A" authorityName="Wang &amp; Lyu &amp; Zeng &amp; Lin &amp; Yang &amp; Nguyen &amp; Le &amp; Ziegler &amp; Wang" authorityYear="2020" class="Reptilia" family="Natricidae" genus="Opisthotropis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opisthotropis hungtai" order="Squamata" pageId="0" pageNumber="141" phylum="Chordata" rank="species" species="hungtai">
