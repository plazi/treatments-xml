<paragraph id="1F5191FF4C482AC98690A4297046C842" pageId="0" pageNumber="141">
<taxonomicName id="67C1B94FD5956125E18F9CBCDA437070" authorityName="Wang &amp; Lyu &amp; Zeng &amp; Lin &amp; Yang &amp; Nguyen &amp; Le &amp; Ziegler &amp; Wang" authorityYear="2020" class="Reptilia" family="Natricidae" genus="Opisthotropis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opisthotropis hungtai" order="Squamata" pageId="0" pageNumber="141" phylum="Chordata" rank="species" species="hungtai">
