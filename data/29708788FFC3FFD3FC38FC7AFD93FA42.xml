<taxonomicName id="66D94D1DFFC3FFD0FCE5F9EDFA95F993" authority="Gittenberger &amp; Menkhorst, 1991" authorityName="Gittenberger &amp; Menkhorst" authorityYear="1991" box="[824,1339,1611,1636]" class="Gastropoda" family="Enidae" genus="Buliminus" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="5" pageNumber="8" phylum="Mollusca" rank="species" species="akkumensis">
<emphasis id="93ADEA8CFFC3FFD0FCE5F9EDFCE9F994" box="[824,839,1612,1635]" italics="true" pageId="5" pageNumber="8">B</emphasis>
. 
<emphasis id="93ADEA8CFFC3FFD0FC89F9EAFC64F994" box="[852,970,1611,1635]" italics="true" pageId="5" pageNumber="8">akkumensis</emphasis>
<bibRefCitation id="C5484B6FFFC3FFD0FC0DF9EAFA95F993" author="GITTENBERGER, E. &amp; MENKHORST, H. P. M. G." box="[976,1339,1611,1636]" journalOrPublisher="Basteria" pageId="5" pageNumber="8" pagination="73 - 88" part="55" refString="GITTENBERGER, E. &amp; MENKHORST, H. P. M. G., 1991. The Turkish Enidae: the genus Buliminus Beck (Gastropoda Pulmonata: Pupillacea). - Basteria 55 (1 / 3): 73 - 88." title="The Turkish Enidae: the genus Buliminus Beck (Gastropoda Pulmonata: Pupillacea)" type="journal article" year="1991">Gittenberger &amp; Menkhorst, 1991</bibRefCitation>
</taxonomicName>
; 
<emphasis id="93ADEA8CFFC3FFD0FA9BF9EDFAFBF994" box="[1350,1365,1612,1635]" italics="true" pageId="5" pageNumber="8">B</emphasis>
. 
<taxonomicName id="66D94D1DFFC3FFD0FCE5F9CFFBF5F971" authority="L. Pfeiffer, 1841" authorityName="L. Pfeiffer" authorityYear="1841" box="[824,1115,1646,1670]" class="Gastropoda" family="Enidae" genus="Buliminus" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="5" pageNumber="8" phylum="Mollusca" rank="species" species="alepensis">
<emphasis id="93ADEA8CFFC3FFD0FCE5F9CFFC3AF971" box="[824,916,1646,1670]" italics="true" pageId="5" pageNumber="8">alepensis</emphasis>
