<location id="8EF8A0FD6E2BC15CE68CA238FF6C3D2B" LSID="urn:lsid:plazi:treatment:038E47306E2AC15CE561A668FF6D3ECB:8EF8A0FD6E2BC15CE68CA238FF6C3D2B" county="This cave" latitude="-5.016667" longLatPrecision="1305" longitude="141.13333" municipality="Krom River" name="Dokfuma Base Camp" pageId="31" pageNumber="44">Dokfuma Base Camp</location>
