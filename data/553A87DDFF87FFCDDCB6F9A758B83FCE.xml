<taxonomicName id="1A934D48FF84FFCDDD68FBAA5CFA3ECE" authorityName="sensu Traser" authorityYear="2000" box="[329,479,1148,1170]" class="Collembola" family="Entomobryidae" genus="Lepidocyrtus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="2" pageNumber="550" phylum="Arthropoda" rank="species" species="arrabonicus">
