SEM micrograph of 
<taxonomicName id="4C7ED3EA197AFFD0508DF8ACFD0CF8F2" authorityName="Illies" authorityYear="1963" box="[492,652,1889,1913]" class="Insecta" family="Gripopterygidae" genus="Araucanioperla" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="32" phylum="Arthropoda" rank="genus">
<emphasis id="B90A747B197AFFD0508DF8ACFD0CF8F2" box="[492,652,1889,1913]" italics="true" pageId="6" pageNumber="32">Araucanioperla</emphasis>
</taxonomicName>
spp. adults. 
<emphasis id="B90A747B197AFFD0526CF8ACFCC5F8F1" bold="true" box="[781,837,1889,1914]" pageId="6" pageNumber="32">A–D,</emphasis>
<taxonomicName id="4C7ED3EA197AFFD0522DF8AFFC3AF8F2" baseAuthorityName="Navas" baseAuthorityYear="1933" box="[844,954,1889,1913]" class="Insecta" family="Gripopterygidae" genus="Araucanioperla" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="32" phylum="Arthropoda" rank="species" species="bullocki">
<emphasis id="B90A747B197AFFD0522DF8AFFC3AF8F2" box="[844,954,1889,1913]" italics="true" pageId="6" pageNumber="32">A. bullocki</emphasis>
</taxonomicName>
; 
<emphasis id="B90A747B197AFFD052A9F8ACFC79F8F1" bold="true" box="[968,1017,1889,1914]" pageId="6" pageNumber="32">E–G</emphasis>
, 
<taxonomicName id="4C7ED3EA197AFFD05567F8AFFBEBF8F2" baseAuthorityName="Froehlich" baseAuthorityYear="1960" box="[1030,1131,1889,1913]" class="Insecta" family="Gripopterygidae" genus="Araucanioperla" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="32" phylum="Arthropoda" rank="species" species="brincki">
