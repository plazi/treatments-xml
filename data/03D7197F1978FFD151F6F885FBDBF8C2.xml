<taxonomicName id="4C7ED3EA197BFFD15088F96DFD09F933" authorityName="Illies" authorityYear="1963" box="[489,649,1696,1720]" class="Insecta" family="Gripopterygidae" genus="Araucanioperla" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="33" phylum="Arthropoda" rank="genus">
