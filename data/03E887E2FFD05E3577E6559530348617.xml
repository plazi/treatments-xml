<taxonomicName id="4C414D77FFD05E3577E65447361C8260" authority="Heyer 1994: 82" authorityName="* (Heyer" authorityPageNumber="82" authorityYear="1994" box="[168,558,1280,1300]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="11" pageNumber="398" phylum="Chordata" rank="species" species="colombiensis">
<emphasis id="B935EAE6FFD05E3577E6544735AE8260" box="[168,412,1280,1299]" italics="true" pageId="11" pageNumber="398">Leptodactylus colombiensis</emphasis>
