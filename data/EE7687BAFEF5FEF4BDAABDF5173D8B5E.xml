<typeStatus id="B964880EFEF5FEF4BF1CB92816288B5E" box="[545,592,1098,1122]" pageId="258" pageNumber="259">type</typeStatus>
locality in 
<collectingRegion id="A41BF84EFEF5FEF4BFF6B92817398B5E" box="[715,833,1098,1122]" country="United States of America" name="New York" pageId="258" pageNumber="259">New York</collectingRegion>
