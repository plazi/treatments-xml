<keyLead id="AA1B9609FF90FFFB59875841FA339F0C" box="[151,1435,1825,1847]" pageId="1" pageNumber="552">
2 Female........................................................... 
<taxonomicName id="D6EA4EBFFF90FFFB5AFF5841FB789F0C" authority="Girault" authorityName="Girault" box="[1007,1232,1825,1847]" class="Insecta" family="Eulophidae" genus="Goetheana" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="552" phylum="Arthropoda" rank="species" species="shakespearei">
<emphasis id="239EE92EFF90FFFB5AFF5841FB2E9F0C" box="[1007,1158,1825,1847]" italics="true" pageId="1" pageNumber="552">G. shakespearei</emphasis>
Girault
</taxonomicName>
/ 
<taxonomicName id="D6EA4EBFFF90FFFB5DF25841FA339F0C" authority="Annecke" authorityName="Annecke" box="[1250,1435,1825,1847]" class="Insecta" family="Eulophidae" genus="Goetheana" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="552" phylum="Arthropoda" rank="species" species="incerta">
<emphasis id="239EE92EFF90FFFB5DF25841FAE89F0C" box="[1250,1344,1825,1847]" italics="true" pageId="1" pageNumber="552">G. incerta</emphasis>
