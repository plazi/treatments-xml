<taxonomicName id="80AC4D7D0D787A09FEB4F9ADD57DF9DE" authority="Dall &amp; Bartsch, 1904" authorityName="Dall &amp; Bartsch" authorityYear="1904" box="[308,604,1579,1605]" class="Gastropoda" family="Pyramidellidae" genus="Egila" kingdom="Animalia" pageId="12" pageNumber="169" phylum="Mollusca" rank="genus">
