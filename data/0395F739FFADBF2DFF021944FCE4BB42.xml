</taxonomicName>
, and 
<taxonomicName id="4C3C3DACFFADBF2DFB711A9BFA54BABB" box="[1252,1429,1734,1758]" class="Insecta" family="Staphylinidae" genus="Discoxenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="malaysiensis">
<emphasis id="B9489A3DFFADBF2DFB711A9BFA54BABB" box="[1252,1429,1734,1758]" italics="true" pageId="8" pageNumber="209">D. malaysiensis</emphasis>
</taxonomicName>
