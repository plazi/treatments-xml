<taxonomicName id="4C3C3DACFFADBF2DFC8C1A9BFC5EBABB" box="[793,927,1734,1758]" class="Insecta" family="Staphylinidae" genus="Discoxenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="assmuthi">
<emphasis id="B9489A3DFFADBF2DFC8C1A9BFC5EBABB" box="[793,927,1734,1758]" italics="true" pageId="8" pageNumber="209">D. assmuthi</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3C3DACFFADBF2DFC381A9BFBE7BABB" box="[941,1062,1734,1758]" class="Insecta" family="Staphylinidae" genus="Discoxenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="lepisma">
<emphasis id="B9489A3DFFADBF2DFC381A9BFBE7BABB" box="[941,1062,1734,1758]" italics="true" pageId="8" pageNumber="209">D. lepisma</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3C3DACFFADBF2DFBA11A9BFB67BABB" box="[1076,1190,1734,1758]" class="Insecta" family="Staphylinidae" genus="Discoxenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="indicus">
