<taxonomicName id="EE09AC2DED8D35A7D95B9E9D2BAB9557" class="Polypodiopsida" family="Schizaeaceae" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Schizaeales" phylum="Tracheophyta" rank="family">Schizaeaceae</taxonomicName>
