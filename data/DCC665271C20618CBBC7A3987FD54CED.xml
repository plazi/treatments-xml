<taxonomicName id="CF2E28131D55F4FE3643D3A99CB3C138" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2407" class="Insecta" family="Formicidae" genus="Brachymyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachymyrmex Mayr" lsidName-HNS="Brachymyrmex Mayr" order="Hymenoptera" phylum="Arthropoda" rank="genus">Brachymyrmex</taxonomicName>
