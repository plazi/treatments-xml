<taxonomicName id="4C0B4D17FFE86E665CF5F688FD5CFA09" authority="(GRAVENHORST)" baseAuthorityName="GRAVENHORST" box="[408,729,1415,1437]" class="Insecta" family="Ichneumonidae" genus="Phobetes" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="315" phylum="Arthropoda" rank="species" species="nigriceps">
<emphasis id="B97FEA86FFE86E665CF5F688FE22FA08" box="[408,423,1416,1437]" italics="true" pageId="2" pageNumber="315">P</emphasis>
. 
<emphasis id="B97FEA86FFE86E665CADF687FD9BFA09" box="[448,542,1415,1436]" italics="true" pageId="2" pageNumber="315">nigriceps</emphasis>
(GRAVENHORST)
</taxonomicName>
