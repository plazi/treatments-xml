<taxonomicName id="4C0B4D17FFE86E665961F688FEB1FA2F" class="Diplopoda" family="Cambalidae" genus="Eclytus" kingdom="Animalia" order="Spirostreptida" pageId="2" pageNumber="315" phylum="Arthropoda" rank="species" species="transsylvanicus">
<emphasis id="B97FEA86FFE86E665961F688FB9EFA08" box="[1036,1051,1416,1437]" italics="true" pageId="2" pageNumber="315">E</emphasis>
