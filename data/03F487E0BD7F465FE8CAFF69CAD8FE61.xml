<taxonomicName id="4C5D4D75BD7F465FE811FE10CB99FE21" box="[352,444,419,442]" class="Magnoliopsida" family="Malvaceae" genus="Abutilon" kingdom="Plantae" order="Malvales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="genus">
<emphasis id="B929EAE4BD7F465FE811FE10CB99FE21" box="[352,444,419,442]" italics="true" pageId="26" pageNumber="27">Abutilon</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C5D4D75BD7F465FE89DFE10C847FE21" box="[492,610,419,442]" class="Magnoliopsida" family="Malvaceae" genus="Gossypium" kingdom="Plantae" order="Malvales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="genus">
<emphasis id="B929EAE4BD7F465FE89DFE10C847FE21" box="[492,610,419,442]" italics="true" pageId="26" pageNumber="27">Gossypium</emphasis>
</taxonomicName>
. Eremian, extending from 
<collectingCountry id="F34A7666BD7F465FEA08FE11C99CFE21" box="[889,953,418,442]" name="Egypt" pageId="26" pageNumber="27">Egypt</collectingCountry>
