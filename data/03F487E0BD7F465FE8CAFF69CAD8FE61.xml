<collectingCountry id="F34A7666BD7F465FEAA9FE11CE46FE21" box="[984,1123,418,442]" name="Saudi Arabia" pageId="26" pageNumber="27">Saudi Arabia</collectingCountry>
, 
<collectingCountry id="F34A7666BD7F465FE9E6FE71CAC1FE42" box="[151,228,450,473]" name="Yemen" pageId="26" pageNumber="27">Yemen</collectingCountry>
, 
<collectingCountry id="F34A7666BD7F465FE981FE71CB3EFE42" box="[240,283,450,473]" name="Iran" pageId="26" pageNumber="27">Iran</collectingCountry>
(
<collectingRegion id="4999F814BD7F465FE85BFE71C834FE41" box="[298,529,450,474]" country="Iran" name="Hormozgan" pageId="26" pageNumber="27">Hormozgan province</collectingRegion>
) and the adjacent Sudanese subregion (from 
<collectingCountry id="F34A7666BD7F465FEA8BFE71CE6FFE41" box="[1018,1098,450,474]" name="Nigeria" pageId="26" pageNumber="27">Nigeria</collectingCountry>
