, 
<collectingCountry id="F329764C5958FFFBFC3AF81B3649CB92" box="[1020,1091,1968,1994]" name="Brazil" pageId="52" pageNumber="53">Brazil</collectingCountry>
, and 
<collectingCountry id="F329764C5958FFFBFB40F81B373BCB92" box="[1158,1329,1968,1994]" name="French Guiana" pageId="52" pageNumber="53">French Guiana</collectingCountry>
(
