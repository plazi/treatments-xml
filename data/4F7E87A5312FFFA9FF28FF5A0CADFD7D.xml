<taxonomicName id="00D74D30312FFFA9FB02FD9A0CADFDF2" authority="Enderlein" authorityName="Enderlein" box="[1213,1436,600,621]" class="Insecta" family="Evaniidae" genus="Zeuxevania" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="130" phylum="Arthropoda" rank="species" species="tenuistilus">
