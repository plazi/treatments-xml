<taxonomicName id="A2EB4D13597A985BD05A8ADF749B9C5A" authority="Kohlrausch, 1878" authorityName="Kohlrausch" authorityYear="1879" box="[320,441,511,537]" class="Chilopoda" family="Scolopendridae" genus="Branchiotrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scolopendromorpha" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="astenon">
<emphasis id="579FEA82597A985BD05A8ADF749B9C5A" box="[320,441,511,537]" italics="true" pageId="47" pageNumber="48">O. astenon</emphasis>
