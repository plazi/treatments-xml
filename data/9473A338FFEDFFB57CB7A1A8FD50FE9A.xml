<paragraph id="1C65122EFFEDFFB67CB7A1A8FE10F959" blockId="1.[151,430,1672,1730]" box="[151,430,1672,1696]" pageId="1" pageNumber="437">
<heading id="472DA542FFEDFFB67CB7A1A8FE10F959" bold="true" box="[151,430,1672,1696]" fontSize="10" level="2" pageId="1" pageNumber="437" reason="2">
<taxonomicName id="DBDA69ADFFEDFFB67CB7A1A8FE10F959" authority="Erichson 1845" authorityName="Erichson" authorityYear="1845" box="[151,430,1672,1696]" class="Insecta" family="Ptiliidae" genus="Nossidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="437" phylum="Arthropoda" rank="genus">
