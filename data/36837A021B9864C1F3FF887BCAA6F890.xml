<taxonomicName id="840F4836E14F92ECF1EB5F156E4BA266" authority="Hoser, 2004" authorityName="Hoser" authorityYear="2004" class="Reptilia" family="Pythonidae" genus="Morelia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Morelia macburniei" order="Squamata" pageId="16" pageNumber="45" phylum="Chordata" rank="species" species="macburniei">Morelia macburniei Hoser, 2004</taxonomicName>
