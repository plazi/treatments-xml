<paragraph id="8BB3BD39FFDAFFB2FF64DD1563FE7BEB" blockId="25.[151,1437,834,930]" pageId="25" pageNumber="377">
(
<number id="3DC4AF1EFFDAFFB2FF6CDD1561C77B37" box="[159,174,870,894]" pageId="25" pageNumber="377" value="7.0">7</number>
genera, 
<number id="3DC4AF1EFFDAFFB2FEE2DD1560447B37" box="[273,301,870,894]" pageId="25" pageNumber="377" value="13.0">13</number>
