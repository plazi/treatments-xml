<taxonomicName id="4C0CC6BAFFDAFFB2FF64DA8260D17D43" authority="Werman 1992" authorityName="Werman" authorityYear="1992" box="[151,440,1265,1290]" class="Reptilia" family="Viperidae" genus="Atropoides" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="377" phylum="Chordata" rank="genus">
