<taxonomicName id="4C52CFAEF74AFFA5D7E1EDABEAF7FF0E" authority="Desutter-Grandcolas, 2003" authorityName="Desutter-Grandcolas" authorityYear="2003" box="[151,722,184,207]" class="Insecta" family="Prophalangopsidae" genus="Prophalangopsis" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="56" phylum="Arthropoda" rank="species" species="obscura">
