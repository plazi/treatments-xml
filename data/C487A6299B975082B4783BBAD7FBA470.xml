<taxonomicName id="898BE83B8C4CCBA1FE16DB08716ABECF" family="Orchidaceae" genus="H." kingdom="Plantae" lsidName="H. monophyllum" order="Asparagales" pageId="5" pageNumber="6" rank="species" species="monophyllum">
