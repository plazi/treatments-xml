</taxonomicName>
<emphasis id="B945EAD9E26EFF9DADB6AADECF5FF9B5" bold="true" box="[713,804,1418,1443]" pageId="4" pageNumber="5">
<taxonomicNameLabel id="A27657A2E26EFF9DADB6AADECF5FF9B5" box="[713,804,1418,1443]" pageId="4" pageNumber="5" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C314D48E26EFF9DAC24AADFCF8DF9B4" box="[859,1014,1419,1442]" class="Arachnida" family="Salticidae" genus="Corusca" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="sanyaensis" status="sp. nov.">
<emphasis id="B945EAD9E26EFF9DAC24AADFCF8DF9B4" box="[859,1014,1419,1442]" italics="true" pageId="4" pageNumber="5">C. sanyaensis</emphasis>
</taxonomicName>
<emphasis id="B945EAD9E26EFF9DAC81AADEC823F9B5" bold="true" box="[1022,1112,1418,1443]" pageId="4" pageNumber="5">
<taxonomicNameLabel id="A27657A2E26EFF9DAC81AADEC823F9B5" box="[1022,1112,1418,1443]" pageId="4" pageNumber="5" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
