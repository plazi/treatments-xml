<taxonomicName id="4C364D3A2472FF9FFF0AFAC4FDD1FA6D" authority="Hutchison, 1940" authorityName="Hutchison" authorityYear="1940" box="[189,561,1405,1431]" class="Insecta" family="Corixidae" genus="Micronecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="subGenus" subGenus="Dichaetonecta">
