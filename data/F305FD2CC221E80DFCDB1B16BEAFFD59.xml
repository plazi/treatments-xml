<taxonomicName id="BCAC37B9C23FE80DFFB818B0BFA4FDE7" box="[111,233,501,528]" class="Insecta" kingdom="Animalia" order="Orthoptera" pageId="49" pageNumber="574" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
