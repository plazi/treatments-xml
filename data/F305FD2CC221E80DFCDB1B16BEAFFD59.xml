<taxonomicName id="BCAC37B9C221E813FD021C92BC0BFA18" authorityName="Karny" authorityYear="1926" box="[725,838,1495,1519]" class="Insecta" family="Tettigoniidae" genus="Phisis" kingdom="Animalia" order="Orthoptera" pageId="47" pageNumber="572" phylum="Arthropoda" rank="species" species="holdhausi">
