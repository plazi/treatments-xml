<taxonomicName id="BCAC37B9C23FE80DFEFF1B70BEEAFDB9" baseAuthorityName="Willemse" baseAuthorityYear="1940" box="[296,423,564,591]" class="Insecta" family="Tettigoniidae" genus="Segestidea" kingdom="Animalia" order="Orthoptera" pageId="49" pageNumber="574" phylum="Arthropoda" rank="species" species="leefmansi">
