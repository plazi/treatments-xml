<taxonomicName id="BCAC37B9C23FE80DFE211B11BEFFFD78" authority="(Gerstaecker, 1860)" baseAuthorityName="Gerstaecker" baseAuthorityYear="1860" class="Insecta" family="Gryllacrididae" genus="Paragryllacris" kingdom="Animalia" order="Orthoptera" pageId="49" pageNumber="574" phylum="Arthropoda" rank="species" species="combusta">
