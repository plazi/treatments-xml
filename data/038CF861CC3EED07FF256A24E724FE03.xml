<taxonomicName id="4C2532F4CC3CED06FEAC6A8AE4F1F831" authorityName="Keifer" authorityYear="1959" box="[286,484,1968,1991]" class="Arachnida" family="Eriophyidae" genus="Dichopelmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="6" pageNumber="369" phylum="Arthropoda" rank="species" species="notus">
