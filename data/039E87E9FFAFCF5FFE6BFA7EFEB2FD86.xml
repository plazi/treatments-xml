<caption id="DF486677FFA0CF5DFF36F991FE5FF934" ID-DOI="http://doi.org/10.5281/zenodo.5215521" ID-Zenodo-Dep="5215521" httpUri="https://zenodo.org/record/5215521/files/figure.png" pageId="8" pageNumber="2263" startId="8.[138,198,1637,1656]" targetBox="[184,1138,184,1598]" targetPageId="8">
<paragraph id="8B8836FFFFA0CF5DFF36F991FE5FF934" blockId="8.[138,1186,1637,1682]" pageId="8" pageNumber="2263">
