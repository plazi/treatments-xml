(
<number id="3DECAB78FFCD2E5FFF56FE8DFF1FFE96" box="[159,205,260,285]" pageId="7" pageNumber="374" value="453.0">453</number>
), W 
<number id="3DECAB78FFCD2E5FFECDFE8DFEFCFE97" box="[260,302,260,284]" pageId="7" pageNumber="374" value="429.0">429</number>
(
