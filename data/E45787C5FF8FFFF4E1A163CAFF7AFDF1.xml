spp.: a–c, 
<taxonomicName id="ABFE4D50FF8FFFF5E0C267B1FB96FA0B" authority="F.O. Pickard-Cambridge 1902" authorityName="F.O. Pickard-Cambridge" authorityYear="1902" box="[500,1040,1484,1506]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="attenuata">
<emphasis id="5E8AEAC1FF8FFFF5E0C267B1FD39FA0B" box="[500,703,1484,1506]" italics="true" pageId="14" pageNumber="15">Novalena attenuata</emphasis>
(F.O. Pickard-Cambridge 1902)
</taxonomicName>
; d–f, 
<taxonomicName id="ABFE4D50FF8FFFF5E57967B1FA1AFA0B" authority="Roth 1967" authorityName="Roth" authorityYear="1967" box="[1103,1436,1484,1506]" class="Arachnida" family="Agelenidae" genus="Novalena" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="bipunctata" status="stat. rev.">
<emphasis id="5E8AEAC1FF8FFFF5E57967B1FAA1FA0B" box="[1103,1319,1484,1506]" italics="true" pageId="14" pageNumber="15">Novalena bipunctata</emphasis>
