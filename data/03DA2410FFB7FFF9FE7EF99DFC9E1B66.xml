<taxonomicName id="4C73EE85FFB3FFF8FDB1FA69FD4A1F81" authorityName="Costa-Silva &amp; Strümpher &amp; Vaz-de-Mello" authorityYear="2021" box="[600,716,1504,1527]" class="Insecta" family="Trogidae" genus="Omorgus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="2020" phylum="Arthropoda" rank="species" species="triestinae">
