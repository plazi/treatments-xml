<paragraph id="E080E91C3A22654426F1996AB88C2B57" pageId="1" pageNumber="106">
<taxonomicName id="7466DAC1278C8EF9D4F974D27E458735" class="Gastropoda" family="Viviparidae" genus="Sinotaia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinotaia guangdungensis" order="Architaenioglossa" pageId="1" pageNumber="106" phylum="Mollusca" rank="species" species="guangdungensis">Sinotaia guangdungensis</taxonomicName>
