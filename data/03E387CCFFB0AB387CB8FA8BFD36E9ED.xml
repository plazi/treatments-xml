<taxonomicName id="4C4A4D59FFB5AB387B2EFD96FD3AE9ED" authority="(Szepligeti, 1905) in Morley (1926)" authorityName="in Morley" authorityYear="1926" baseAuthorityName="Szepligeti" baseAuthorityYear="1905" class="Insecta" family="Ichneumonidae" genus="Trathala" kingdom="Animalia" order="Hymenoptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="concolor">
