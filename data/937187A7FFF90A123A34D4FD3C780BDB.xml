<taxonomicName id="DCD84D32FFF90A1239F7D72F3D270D2B" authorityName="Looss" authorityYear="1899" box="[756,875,827,851]" class="Trematoda" family="Heterophyidae" genus="Ascocotyle" kingdom="Animalia" order="Plagiorchiida" pageId="9" pageNumber="1740" phylum="Platyhelminthes" rank="genus">
