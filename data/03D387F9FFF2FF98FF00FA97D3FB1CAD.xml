<collectionCode id="ED6BAE2AFFF2FF99FDAAF8E5D3B11C28" box="[573,649,1824,1848]" country="Brazil" httpUri="http://grbio.org/cool/4wb1-ykjs" name="Crustacean Collection of the Department of Biology" pageId="10" pageNumber="341">CCDB</collectionCode>
