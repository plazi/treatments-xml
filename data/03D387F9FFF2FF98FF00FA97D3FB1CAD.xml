<quantity id="4C829B0AFFF2FF99FBB4F912D5AF1DFF" box="[1059,1175,1751,1776]" metricMagnitude="2" metricUnit="m" metricValue="1.1" metricValueMax="1.75" metricValueMin="0.45" pageId="10" pageNumber="341" unit="m" value="110.0" valueMax="175.0" valueMin="45.0">45–175 m</quantity>
