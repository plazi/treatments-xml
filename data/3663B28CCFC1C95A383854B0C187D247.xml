<taxonomicName id="AC6B34FCCA78465F2767C8F082C37306" class="Insecta" family="Braconidae" genus="Gastralysia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gastralysia garambana" order="Hymenoptera" pageId="0" pageNumber="149" phylum="Arthropoda" rank="species" species="garambana">Gastralysia garambana</taxonomicName>
