<quantity id="4CEF9B25FF4CFF689C4CF9FEFE303622" box="[1073,1137,1548,1575]" metricMagnitude="-1" metricUnit="m" metricValue="2.1082" pageId="221" pageNumber="222" unit="in" value="8.3">8.3 in</quantity>
