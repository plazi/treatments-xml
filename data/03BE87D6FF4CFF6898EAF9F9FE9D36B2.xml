<quantity id="4CEF9B25FF4CFF689B9DF9FEFE623622" box="[992,1059,1548,1575]" metricMagnitude="-1" metricUnit="m" metricValue="2.1" pageId="221" pageNumber="222" unit="cm" value="21.0">21 cm</quantity>
