</taxonomicName>
<taxonomicName id="04B94D4B9B65E83D006FBB77FAE38E17" authority="Staniszewski" authorityName="Staniszewski" box="[605,993,816,842]" class="Amphibia" family="Mantellidae" genus="Mantella" kingdom="Animalia" order="Anura" pageId="9" pageNumber="30" phylum="Chordata" rank="species" species="milotympanum">
