<taxonomicName id="AC954D28C61CFFC1F16565C4746A77B2" baseAuthorityName="Dall" baseAuthorityYear="1927" box="[1288,1392,225,251]" class="Gastropoda" family="Triphoridae" genus="Strobiligera" kingdom="Animalia" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="enopla">
