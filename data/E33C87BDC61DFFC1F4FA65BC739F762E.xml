<taxonomicName id="AC954D28C61CFFC1F5E7642073187656" baseAuthorityName="Dall" baseAuthorityYear="1927" box="[394,514,261,287]" class="Gastropoda" family="Triphoridae" genus="Strobiligera" kingdom="Animalia" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="meteora">
<emphasis id="59E1EAB9C61CFFC1F5E7642070827656" box="[394,408,261,287]" italics="true" pageId="39" pageNumber="40">S</emphasis>
. 
<emphasis id="59E1EAB9C61CFFC1F5CB642373187656" box="[422,514,262,287]" italics="true" pageId="39" pageNumber="40">meteora</emphasis>
