) and 
<taxonomicName id="AC954D28C61CFFC1F4A56598735C779E" authority="(Dall, 1927)" authorityName="Dall" authorityYear="1927" baseAuthorityName="Dall" baseAuthorityYear="1927" box="[200,582,189,215]" class="Gastropoda" family="Triphoridae" genus="Strobiligera" kingdom="Animalia" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="meteora">
