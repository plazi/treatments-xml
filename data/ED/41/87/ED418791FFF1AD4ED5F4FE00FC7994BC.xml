<taxonomicName authority="(Kutzing 1834: 580) D. M. Williams (2012: 260" authorityName="D. M. Williams" authorityYear="1834" baseAuthorityName="Kutzing" baseAuthorityPageNumber="580" baseAuthorityYear="1834" box="[136,948,1569,1596]" class="Magnoliopsida" family="Rhizophoraceae" genus="Diatoma" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="223" phylum="Tracheophyta" rank="species" species="moniliformis">
