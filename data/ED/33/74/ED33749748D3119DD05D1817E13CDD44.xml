<subSubSection pageId="28" pageNumber="29" type="type species">
<paragraph pageId="28" pageNumber="29">Type species.</paragraph>
<paragraph pageId="28" pageNumber="29">
<taxonomicName class="Insecta" family="Curculionidae" genus="Laparocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Laparocerus maxorata" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="maxorata">Laparocerus maxorata</taxonomicName>
