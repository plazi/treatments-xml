<subSubSection pageId="2" pageNumber="57" type="etymology">
<paragraph blockId="2.[151,1436,152,818]" pageId="2" pageNumber="57">
<emphasis bold="true" box="[198,339,751,777]" pageId="2" pageNumber="57">Etymology.</emphasis>
