<taxonomicName authority="(Pena Cantero 2009)" baseAuthorityName="Pena Cantero" baseAuthorityYear="2009" class="Hydrozoa" family="Campanulariidae" genus="Obelia" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="329" phylum="Cnidaria" rank="species" species="bidentata">
