<taxonomicName baseAuthorityName="Hartlaub" baseAuthorityYear="1904" class="Hydrozoa" family="Lafoeidae" genus="Filellum" kingdom="Animalia" order="Leptothecata" pageId="4" pageNumber="325" phylum="Cnidaria" rank="species" species="antarcticum">
