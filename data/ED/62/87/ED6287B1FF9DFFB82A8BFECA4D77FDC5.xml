, 
<geoCoordinate box="[578,739,1730,1754]" degrees="121" direction="west" minutes="40.081" orientation="longitude" pageId="9" pageNumber="8" precision="1" value="-121.668015">W121°40.081</geoCoordinate>
<date box="[745,962,1730,1754]" pageId="9" pageNumber="8" value="2004-02-25">25 February 2004</date>
