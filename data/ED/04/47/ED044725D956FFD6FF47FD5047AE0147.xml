<subSubSection box="[199,1419,942,967]" pageId="59" pageNumber="60" type="etymology">
<paragraph blockId="59.[151,1436,834,2010]" box="[199,1419,942,967]" pageId="59" pageNumber="60">
<emphasis box="[199,333,943,967]" inLineHeading="true" pageId="59" pageNumber="60" reason="1">
