<taxonomicName authority="(Fabricius)" baseAuthorityName="Fabricius" baseAuthorityYear="1787" box="[539,878,295,320]" class="Insecta" family="Cerambycidae" genus="Ceresium" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="unicolor">
