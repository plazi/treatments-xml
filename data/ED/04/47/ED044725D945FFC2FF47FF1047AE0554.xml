<subSubSection pageId="41" pageNumber="42" type="etymology">
<paragraph blockId="41.[151,1436,151,391]" pageId="41" pageNumber="42">
<emphasis box="[199,333,152,176]" inLineHeading="true" pageId="41" pageNumber="42" reason="1">
<emphasis bold="true" box="[199,327,152,176]" pageId="41" pageNumber="42">Etymology</emphasis>
.
</emphasis>
