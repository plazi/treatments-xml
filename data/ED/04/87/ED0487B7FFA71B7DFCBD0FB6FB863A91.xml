<subSubSection pageId="3" pageNumber="82" type="distribution">
<paragraph blockId="3.[816,1428,1407,1496]" pageId="3" pageNumber="82">
<emphasis bold="true" box="[816,956,1407,1431]" pageId="3" pageNumber="82">Distribution</emphasis>
