<collectingCountry box="[475,581,1304,1330]" name="Australia" pageId="11" pageNumber="182">Australia</collectingCountry>
, 
<collectingCountry box="[591,630,1304,1330]" name="Fiji" pageId="11" pageNumber="182">Fiji</collectingCountry>
and 
<collectingCountry box="[685,765,1305,1331]" name="Samoa" pageId="11" pageNumber="182">Samoa</collectingCountry>
.
