<paragraph pageId="24" pageNumber="125">Remarks.</paragraph>
<paragraph pageId="24" pageNumber="125">
<taxonomicName authorityName="Bates" authorityYear="1888" class="Insecta" family="Scarabaeidae" genus="Aspidolea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aspidolea singularis" order="Coleoptera" pageId="24" pageNumber="125" phylum="Arthropoda" rank="species" species="singularis">
