<emphasis box="[491,661,1651,1676]" italics="true" pageId="9" pageNumber="40">Measurements</emphasis>
: BL 
<quantity box="[724,824,1650,1676]" metricMagnitude="-3" metricUnit="m" metricValue="5.67" pageId="9" pageNumber="40" unit="mm" value="5.67">5.67mm</quantity>
; WL 
<quantity box="[892,992,1650,1676]" metricMagnitude="-3" metricUnit="m" metricValue="3.47" pageId="9" pageNumber="40" unit="mm" value="3.47">3.47mm</quantity>
; HL 
<quantity box="[1054,1155,1650,1676]" metricMagnitude="-3" metricUnit="m" metricValue="1.58" pageId="9" pageNumber="40" unit="mm" value="1.58">1.58mm</quantity>
