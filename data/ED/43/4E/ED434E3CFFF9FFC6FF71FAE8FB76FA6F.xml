<taxonomicName baseAuthorityName="Walker" baseAuthorityYear="1858" box="[688,839,1411,1437]" class="Insecta" family="Noctuidae" genus="Eudocima" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="discrepans">
