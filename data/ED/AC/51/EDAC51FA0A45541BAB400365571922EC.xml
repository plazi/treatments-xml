<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:B8A94766-C505-4E59-98D6-19AEF7E7B80A" class="Actinopterygii" family="Cichlidae" genus="Cryptoheros" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Cryptoheros altoflavus Allgayer 2001:16" order="Perciformes" pageNumber="33" phylum="Chordata" rank="species" species="altoflavus">Cr. altoflavus</taxonomicName>
