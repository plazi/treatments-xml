<location LSID="urn:lsid:plazi:treatment:ED42878659459865D18D8C5574009A3F:6034604B59449865D0DC886377029D1F" box="[454,544,835,861]" country="Chile" county="Obviously Attems" municipality="Coique" name="Coronel" pageId="17" pageNumber="18">Coronel</location>
