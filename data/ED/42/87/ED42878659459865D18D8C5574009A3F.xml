<location LSID="urn:lsid:plazi:treatment:ED42878659459865D18D8C5574009A3F:6034604B59449865D413883F70789D7B" box="[1289,1370,799,825]" country="Chile" county="Obviously Attems" municipality="Coique" name="Attems" pageId="17" pageNumber="18">Attems</location>
