<location LSID="urn:lsid:plazi:treatment:ED428786594A9854D18D8C7176D39CB3:6034604B59759854D20189AF76BC9CEB" box="[795,926,655,681]" municipality="West Indies" name="West Indies" pageId="32" pageNumber="33" stateProvince="Hamburg">West Indies</location>
