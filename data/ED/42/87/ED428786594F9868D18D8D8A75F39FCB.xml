<location LSID="urn:lsid:plazi:treatment:ED428786594F9868D18D8D8A75F39FCB:6034604B59499868D5F38BFF70BE9EBB" box="[1257,1436,223,249]" county="Gharyan Distirct" municipality="The" name="Bruno Klaptocz" pageId="28" pageNumber="29" stateProvince="Tarabulus">Bruno Klaptocz</location>
