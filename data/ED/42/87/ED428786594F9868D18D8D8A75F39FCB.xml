<location LSID="urn:lsid:plazi:treatment:ED428786594F9868D18D8D8A75F39FCB:6034604B59499868D3D98A2376249F5F" box="[707,774,259,285]" county="Gharyan Distirct" municipality="The" name="Barka" pageId="28" pageNumber="29" stateProvince="Tarabulus">Barka</location>
