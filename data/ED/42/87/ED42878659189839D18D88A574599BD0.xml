<location LSID="urn:lsid:plazi:treatment:ED42878659189839D18D88A574599BD0:6034604B59189839D3758FC876819B43" box="[623,931,1255,1282]" country="Paraguay" county="Silvestri" municipality="Rio Apa" name="Museum of Natural History" pageId="77" pageNumber="78">
