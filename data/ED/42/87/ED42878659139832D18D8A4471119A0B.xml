<taxonomicName authorityName="Newport" authorityYear="1844" box="[261,389,927,953]" class="Chilopoda" family="Scolopendridae" genus="Scolopendra" kingdom="Animalia" order="Scolopendromorpha" pageId="70" pageNumber="71" phylum="Arthropoda" rank="species" species="canidens">
