var. 
<emphasis box="[258,353,891,917]" italics="true" pageId="70" pageNumber="71">africana</emphasis>
Verhoeff
</taxonomicName>
with 
<taxonomicName authorityName="Newport" authorityYear="1844" box="[520,647,891,917]" class="Chilopoda" family="Scolopendridae" genus="Scolopendra" kingdom="Animalia" order="Scolopendromorpha" pageId="70" pageNumber="71" phylum="Arthropoda" rank="species" species="canidens">
