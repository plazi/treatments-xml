<emphasis box="[1061,1156,855,881]" italics="true" pageId="70" pageNumber="71">africana</emphasis>
Verhoeff
</taxonomicName>
and 
<taxonomicName authority="Verhoeff" authorityName="Verhoeff" class="Chilopoda" family="Scolopendridae" genus="Scolopendra" kingdom="Animalia" order="Scolopendromorpha" pageId="70" pageNumber="71" phylum="Arthropoda" rank="variety" species="mediterrana" variety="africana">
