<materialsCitation ID-GBIF-Occurrence="2293862269" collectingDate="2017-03-26" collectorName="de Gibara" country="Cuba" location="Bahia de Gibara" pageId="2" pageNumber="3" specimenCount="1">
<emphasis bold="true" box="[543,615,1304,1331]" pageId="2" pageNumber="3">
