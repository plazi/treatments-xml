<taxonomicName authority="Diez &amp; Reygel &amp; Artois, 2019" authorityName="Diez &amp; Reygel &amp; Artois" authorityYear="2019" box="[249,541,1961,1985]" class="Rhabditophora" family="Schizorhynchidae" genus="Schizochilus" kingdom="Animalia" order="Rhabdocoela" pageId="24" pageNumber="25" phylum="Platyhelminthes" rank="species" species="bueycabonensis" status="sp. n.">
<emphasis box="[249,541,1961,1985]" italics="true" pageId="24" pageNumber="25">
<taxonomicName box="[249,376,1961,1985]" class="Liliopsida" family="Schizorhynchidae" genus="Schizochilus" higherTaxonomySource="CoL" kingdom="Animalia" order="Rhabdocoela" pageId="24" pageNumber="25" phylum="Platyhelminthes" rank="genus">Schizochilus</taxonomicName>
<taxonomicName box="[381,541,1961,1985]" pageId="24" pageNumber="25" rank="species" species="bueycabonensis" status="sp. n.">bueycabonensis</taxonomicName>
