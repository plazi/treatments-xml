<subSubSection pageId="0" pageNumber="129" type="reference_group">
<paragraph pageId="0" pageNumber="129">
<taxonomicName authorityName="Davis" authorityYear="1940" class="Insecta" family="Archembiidae" genus="Pararhagadochir" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pararhagadochir" order="Embioptera" pageId="0" pageNumber="129" phylum="Arthropoda" rank="genus">Pararhagadochir</taxonomicName>
