<normalizedToken originalValue="für" pageId="1" pageNumber="168">fuer</normalizedToken>
<taxonomicName class="Chilopoda" family="Lithobiidae" genus="Lithobius" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="1" pageNumber="168" phylum="Arthropoda" rank="species" species="aeruginosus">aeruginosus</taxonomicName>
