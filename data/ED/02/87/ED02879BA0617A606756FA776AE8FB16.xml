<taxonomicName authority="Chen &amp; Watabe &amp; Gao &amp; Takamori &amp; Zhang &amp; Aotsuka, 2005" authorityName="Chen &amp; Watabe &amp; Gao &amp; Takamori &amp; Zhang &amp; Aotsuka" authorityYear="2005" box="[564,739,1013,1037]" class="Insecta" family="Drosophilidae" genus="Amiota" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="295" phylum="Arthropoda" rank="species" species="arcuata" status="sp. nov." subGenus="Amiota">
