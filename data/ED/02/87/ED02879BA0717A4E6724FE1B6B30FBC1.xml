<quantity box="[1043,1172,1182,1207]" metricMagnitude="-2" metricUnit="m" metricValue="5.13" pageId="13" pageNumber="276" unit="mm" value="51.3">51.30 mm</quantity>
(1.27–1.34); WL 
<quantity box="[324,453,1214,1239]" metricMagnitude="-2" metricUnit="m" metricValue="5.25" pageId="13" pageNumber="276" unit="mm" value="52.5">52.50 mm</quantity>
