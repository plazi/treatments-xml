<collectingRegion id="EF6FF807FFF0FFFEFDA2FD7CFD7DFCB5" box="[571,664,762,787]" country="Guinea-Bissau" name="Biombo" pageId="29" pageNumber="30">Biombo</collectingRegion>
, 
<collectingRegion id="EF6FF807FFF0FFFEFD31FD7CFCE5FCB5" box="[680,768,762,787]" country="Guinea-Bissau" name="Cacheu" pageId="29" pageNumber="30">Cacheu</collectingRegion>
</collectingRegion>
