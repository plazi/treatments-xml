locality
</emphasis>
. 
<collectingCountry id="9C93D5929066A02DFE82FA24B1A7FA0E" box="[364,501,1503,1528]" name="Madagascar" pageId="9" pageNumber="460">Madagascar</collectingCountry>
, 
<location id="E15BC3D99066A02DFDEEFA24B2EBFA0E" LSID="urn:lsid:plazi:treatment:6C2D24149066A02FFF79FBAEB3EBF9DA:E15BC3D99066A02DFDEEFA24B2EBFA0E" box="[512,697,1503,1528]" country="Madagascar" name="Mahajamba Bay" pageId="9" pageNumber="460">Mahajamba Bay</location>
, 
