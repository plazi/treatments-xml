<paragraph id="3C3621725362FFEDFF5FF9D2FCD96B0E" blockId="31.[151,1436,1589,1902]" box="[151,795,1589,1614]" pageId="31" pageNumber="182">
<emphasis id="0EFDFD605362FFEDFF5FF9D2FEDC6B0E" bold="true" box="[151,286,1589,1614]" pageId="31" pageNumber="182">Etymology.</emphasis>
<emphasis id="0EFDFD605362FFEDFEECF9D2FEAF6B0E" box="[292,365,1589,1614]" italics="true" pageId="31" pageNumber="182">longi-,</emphasis>
elongate; 
<emphasis id="0EFDFD605362FFEDFE2BF9D2FD8A6B0E" box="[483,584,1589,1614]" italics="true" pageId="31" pageNumber="182">caudatus</emphasis>
