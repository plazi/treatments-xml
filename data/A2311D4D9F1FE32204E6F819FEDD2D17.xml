<taxonomicName id="ED98D7D89F1EE3220740FF7BFBFA29A2" authority="Poli, 1795" authorityName="Poli" authorityYear="1795" box="[817,1106,222,247]" class="Bivalvia" family="Tellinidae" genus="Tellina" kingdom="Animalia" order="Veneroida" pageId="7" pageNumber="8" phylum="Mollusca" rank="species" species="togata">
