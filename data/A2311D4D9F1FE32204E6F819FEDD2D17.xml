species.
</emphasis>
<taxonomicName id="ED98D7D89F1EE3220548FCACFD252A77" authority="Totten, 1834" authorityName="Totten" authorityYear="1834" box="[313,653,777,802]" class="Bivalvia" family="Solemyidae" genus="Solemya" kingdom="Animalia" order="Solemyoida" pageId="7" pageNumber="8" phylum="Mollusca" rank="species" species="borealis">
