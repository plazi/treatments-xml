<quantity id="EE1DC9D81751FFA6FCA4FABEFB98576B" box="[869,1036,1340,1364]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" metricValueMax="5.3" metricValueMin="3.9" pageId="21" pageNumber="22" unit="mm" value="4.6" valueMax="5.3" valueMin="3.9">3.90–5.30 mm</quantity>
