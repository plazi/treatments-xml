<taxonomicName id="A1BC4D0F1843FFB5FECFE389FDCCF853" authority="Li &amp; Achterberg &amp; Chen, 2020" authorityName="Li &amp; Achterberg &amp; Chen" authorityYear="2020" box="[294,593,2004,2028]" class="Insecta" family="Braconidae" genus="Campyloneurus" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="quadraticeps" status="sp. nov.">
<emphasis id="54C8EA9E1843FFB5FECFE389FDCCF853" box="[294,593,2004,2028]" italics="true" pageId="29" pageNumber="30">Campyloneurus quadraticeps</emphasis>
</taxonomicName>
<emphasis id="54C8EA9E1843FFB5FDBEE389FD35F853" bold="true" box="[599,680,2004,2028]" pageId="29" pageNumber="30">
