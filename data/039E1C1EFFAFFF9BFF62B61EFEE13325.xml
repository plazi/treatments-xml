<taxonomicName id="4C37D68BFFAFFF9AFF62B61EFDD9355C" authority="(Soar, 1900)" baseAuthorityName="Soar" baseAuthorityYear="1900" box="[151,630,1866,1893]" class="Arachnida" family="Unionicolidae" genus="Unionicola" kingdom="Animalia" order="Trombidiformes" pageId="14" pageNumber="115" phylum="Arthropoda" rank="species" species="minor" subGenus="Hexatax">
