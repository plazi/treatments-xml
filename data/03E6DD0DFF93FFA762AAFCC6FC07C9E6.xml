<taxonomicName id="4C4F1798FF93FFA762AAFCC6FEC4C928" box="[151,317,804,830]" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="31" pageNumber="132" phylum="Chordata" rank="genus">
<emphasis id="B93BB009FF93FFA762AAFCC6FEC4C928" bold="true" box="[151,317,804,830]" italics="true" pageId="31" pageNumber="132">Cyrtodactylus</emphasis>
