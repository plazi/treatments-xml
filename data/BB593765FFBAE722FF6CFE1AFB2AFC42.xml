<collectingCountry id="4BE7C6E3FFBAE722FE9BFD1FFE1CFCD6" box="[352,488,763,789]" name="Hong Kong" pageId="19" pageNumber="20">Hong Kong</collectingCountry>
, 
<collectingCountry id="4BE7C6E3FFBAE722FE0DFD1FFDA6FCD6" box="[502,594,763,789]" name="Bahrain" pageId="19" pageNumber="20">Bahrain</collectingCountry>
, Gulf of Suez, Red Sea, 
<collectingCountry id="4BE7C6E3FFBAE722FC89FD1FFBFDFCD6" box="[882,1033,763,789]" name="Mozambique" pageId="19" pageNumber="20">Mozambique</collectingCountry>
, 
<collectingCountry id="4BE7C6E3FFBAE722FBEDFD1FFB3CFCD6" box="[1046,1224,763,789]" name="New Caledonia" pageId="19" pageNumber="20">New Caledonia</collectingCountry>
, 
<collectingCountry id="4BE7C6E3FFBAE722FB2DFD1FFAC2FCD6" box="[1238,1334,763,789]" name="Djibouti" pageId="19" pageNumber="20">Djibouti</collectingCountry>
, Arabian Gulf, Hawaii, 
<collectingCountry id="4BE7C6E3FFBAE722FECCFCFBFE8DFCFA" box="[311,377,799,825]" name="Palau" pageId="19" pageNumber="20">Palau</collectingCountry>
