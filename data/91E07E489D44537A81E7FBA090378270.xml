<taxonomicName id="28015F34904C8A22A5A08DB6D8A9D04C" authorityName="van Achterberg" authorityYear="1998" class="Insecta" family="Braconidae" genus="Bobekoides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bobekoides fulvus" order="Hymenoptera" pageId="0" pageNumber="25" phylum="Arthropoda" rank="species" species="fulvus">Bobekoides fulvus</taxonomicName>
