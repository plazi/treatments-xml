<collectingCountry id="F3767641FFE3FF8A2336ECAB18D7F93E" box="[879,952,1730,1756]" name="China" pageId="1" pageNumber="63">China</collectingCountry>
, on 
<taxonomicName id="4C614D52FFE3FF8A23B0ECAA1F81F93E" box="[1001,1262,1731,1756]" class="Malacostraca" family="Portunidae" genus="Charybdis" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="63" phylum="Arthropoda" rank="species" species="bimaculata">
<emphasis id="B915EAC3FFE3FF8A23B0ECAA1F81F93E" box="[1001,1262,1731,1756]" italics="true" pageId="1" pageNumber="63">Charybdis bimaculata</emphasis>
</taxonomicName>
