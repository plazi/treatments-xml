5. 
<taxonomicName id="E9EF4D0AFF92FFE76B9861DCFE142C90" box="[177,415,209,231]" class="Gymnolaemata" family="Cribrilinidae" genus="Cribrilina" kingdom="Animalia" order="Cheilostomatida" pageId="10" pageNumber="35" phylum="Bryozoa" rank="species" species="cryptoecium">
<emphasis id="1C9BEA9BFF92FFE76B9861DCFE142C90" bold="true" box="[177,415,209,231]" italics="true" pageId="10" pageNumber="35">Cribrilina cryptoecium</emphasis>
</taxonomicName>
and 
<emphasis id="1C9BEA9BFF92FFE76AFD61DCFD072C90" bold="true" box="[468,652,209,231]" italics="true" pageId="10" pageNumber="35">Arctonula arctica</emphasis>
