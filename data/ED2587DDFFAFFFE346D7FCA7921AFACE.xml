<taxonomicName id="A28C4D48FFAFFFE34424FB689700FB26" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[612,1067,1158,1183]" class="Insecta" family="Melandryidae" genus="Melandrya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="431" phylum="Arthropoda" rank="species" species="caraboides">
