<taxonomicName id="61E389CFFCBD1B71FE55D1DB7CEDA327" authority="Curoe, 2011" authorityName="Curoe" authorityYear="2011" class="Insecta" family="Scarabaeidae" genus="Trigonopeltastes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopeltastes aurovelutinus" order="Coleoptera" pageId="15" pageNumber="106" phylum="Arthropoda" rank="species" species="aurovelutinus">Trigonopeltastes aurovelutinus Curoe, 2011</taxonomicName>
