<quantity id="4C92A6B57A2664772535F899FD5F39BF" box="[542,680,1905,1933]" metricMagnitude="-3" metricUnit="m" metricValue="5.55" metricValueMax="5.7" metricValueMin="5.4" pageId="23" pageNumber="424" unit="mm" value="5.55" valueMax="5.7" valueMin="5.4">5.4−5.7 mm</quantity>
