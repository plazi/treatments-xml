<taxonomicName id="F7954D73FFB3FFE73EE7FBAAFDC4FB65" box="[441,595,1145,1170]" class="Insecta" family="Scarabaeidae" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="290" phylum="Arthropoda" rank="family">Scarabaeidae</taxonomicName>
