<geoCoordinate id="EE7BB7A4FFF0FFCEA3C6320EFD4A6412" box="[671,736,1280,1304]" degrees="5" direction="north" minutes="24" orientation="latitude" pageId="6" pageNumber="317" precision="925" value="5.4">
<emphasis id="B93B0D71FFF0FFCEA3C6320EFD066412" box="[671,684,1281,1304]" italics="true" pageId="6" pageNumber="317">n</emphasis>
