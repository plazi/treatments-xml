<geoCoordinate id="EE7BB7A4FFF0FFCEA105322FFF37643D" box="[92,157,1311,1336]" degrees="5" direction="north" minutes="22" orientation="latitude" pageId="6" pageNumber="317" precision="925" value="5.366667">
<emphasis id="B93B0D71FFF0FFCEA105322FFFC3643D" box="[92,105,1312,1335]" italics="true" pageId="6" pageNumber="317">n</emphasis>
