<collectingCountry id="F36676318121FF8DFEF9F8FAFE55F8B6" box="[373,486,1866,1892]" name="Sri Lanka" pageId="14" pageNumber="15">Sri Lanka</collectingCountry>
, 
<collectingCountry id="F36676318121FF8DFE7DF8FAFD98F8B6" box="[497,555,1866,1892]" name="India" pageId="14" pageNumber="15">India</collectingCountry>
and 
<collectingCountry id="F36676318121FF8DFDEDF8FBFD63F8B7" box="[609,720,1867,1893]" name="Myanmar" pageId="14" pageNumber="15">Myanmar</collectingCountry>
