<taxonomicName id="79AD049FFFA12155FDAD7E67D0D0BE23" authorityName="Wiltshire" authorityYear="1980" box="[528,654,805,830]" class="Insecta" family="Noctuidae" genus="Craniophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="melanisans">
