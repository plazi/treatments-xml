<taxonomicName id="79AD049FFFA12155FC997B04D1C5BB43" authorityName="Berio" authorityYear="1943" box="[804,923,1605,1630]" class="Insecta" family="Noctuidae" genus="Berionycta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="limbata">
