<taxonomicName id="4C244D13FFB7FF9AFACBE96FFEDA883A" authorityName="Decaisne" authorityYear="1842" class="Ulvophyceae" family="Halimedaceae" genus="Halimeda" kingdom="Plantae" order="Bryopsidales" pageId="72" pageNumber="73" phylum="Chlorophyta" rank="species" species="cylindracea">
<emphasis id="B950EA82FFB7FF9AFACBE96FFEDA883A" italics="true" pageId="72" pageNumber="73">Halimeda cylindracea</emphasis>
</taxonomicName>
