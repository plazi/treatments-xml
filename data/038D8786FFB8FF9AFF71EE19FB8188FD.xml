<taxonomicName id="4C244D13FFB7FF9AFECFE943FE36883A" authorityName="C. Agardh" authorityYear="1817" baseAuthorityName="M. Vahl" box="[297,503,296,320]" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" kingdom="Plantae" order="Bryopsidales" pageId="72" pageNumber="73" phylum="Chlorophyta" rank="species" species="taxifolia">
