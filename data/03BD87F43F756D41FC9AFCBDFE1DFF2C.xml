<collectingCounty id="62CA4E6E3F756D40FCA1FCF0FC45FC43" box="[847,901,872,898]" pageId="4" pageNumber="363">This</collectingCounty>
species was previously recorded from 
<collectingMunicipality id="6BCFAC983F756D40FCC7FC12FC4DFC65" box="[809,909,906,932]" pageId="4" pageNumber="363">Liberian</collectingMunicipality>
<collectingRegion id="49D0F8003F756D40FC71FC12FC2FFC65" box="[927,1007,906,932]" country="Liberia" name="Nimba" pageId="4" pageNumber="363">Nimba</collectingRegion>
(
<bibRefCitation id="EF854B133F756D40FBE5FC12FB55FC65" author="COE, M." box="[1035,1173,906,932]" pageId="4" pageNumber="363" pagination="523 - 588" refId="ref12096" refString="COE, M. 1975. Mammalian ecological studies at Mount Nimba, Liberia. Mammalia, 39: 523 - 588." type="journal article" year="1975">Coe, 1975</bibRefCitation>
