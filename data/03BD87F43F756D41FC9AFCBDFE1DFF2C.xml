<taxonomicName id="4C144D613F756D40FCC7FC77FB84FBC8" baseAuthorityName="Peters" baseAuthorityYear="1868" box="[809,1092,1007,1033]" class="Mammalia" family="Pteropodidae" genus="Micropteropus" kingdom="Animalia" order="Chiroptera" pageId="4" pageNumber="363" phylum="Chordata" rank="species" species="pusillus">
