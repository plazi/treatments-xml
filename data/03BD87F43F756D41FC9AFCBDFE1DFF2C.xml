<location id="8ECB60393F756D40FBAFFC33FBB5FC04" LSID="urn:lsid:plazi:treatment:03BD87F43F756D41FC9AFCBDFE1DFF2C:8ECB60393F756D40FBAFFC33FBB5FC04" box="[1089,1141,939,965]" county="This" municipality="Liberian" name="This" pageId="4" pageNumber="363" stateProvince="Nimba">This</location>
