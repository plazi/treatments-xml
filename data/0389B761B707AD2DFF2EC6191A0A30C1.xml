<taxonomicName id="4C207DF4B707AD2DFC3FC6F11FF436F9" authority="Jullien, 1880" authorityName="Jullien" authorityYear="1880" box="[902,1259,716,740]" class="Hydrozoa" family="Lafoeidae" genus="Filellum" kingdom="Animalia" order="Leptothecata" pageId="21" pageNumber="22" phylum="Cnidaria" rank="species" species="bouvieri">
