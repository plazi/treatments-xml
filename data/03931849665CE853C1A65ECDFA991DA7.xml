<taxonomicName id="4C3AD2DC665CE852C1C65FF6FE071AA4" box="[247,430,1954,1979]" class="Magnoliopsida" family="Asteraceae" genus="Tagetes" kingdom="Plantae" order="Asterales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="minuta">
