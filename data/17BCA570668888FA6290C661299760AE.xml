<taxonomicName id="FF008104075AF8BB45CC0F9F9C9210CA" authority="Kirby, 1882" authorityName="Kirby" authorityYear="1882" class="Insecta" family="Tenthredinidae" genus="Xenapates" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Xenapates" order="Hymenoptera" pageId="1" pageNumber="10" phylum="Arthropoda" rank="genus">Xenapates</taxonomicName>
