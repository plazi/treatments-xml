97. 
<taxonomicName id="4C4A9AB9992B8559E8D1F9C7D766F63D" authority="Mik, 1883" authorityName="Mik" authorityYear="1883" box="[193,567,1592,1619]" class="Insecta" family="Dolichopodidae" genus="Poecilobothrus" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="bigoti">
