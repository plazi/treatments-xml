<taxonomicNameLabel id="A275E3096F72C9544D2DB79665F892AC" box="[1115,1178,647,673]" pageId="27" pageNumber="28" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C32F9E36F72C9544DA7B799643892AC" authorityName="♀ Swinhoe" authorityYear="1906" box="[1233,1370,648,673]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="syntypica">
