<taxonomicName id="4C32F9E36F72C9544A39B70D629E9238" authority="Volynkin &amp; Černý &amp; Huang, 2019" authorityName="Volynkin &amp; Černý &amp; Huang" authorityYear="2019" box="[847,1020,539,565]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="syntypicoida" status="sp. n.">
