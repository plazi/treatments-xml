<heading id="D0CE81FCFFE5482488FAFBDD0656FBAE" bold="true" box="[151,541,1052,1078]" fontSize="11" level="1" pageId="20" pageNumber="21" reason="1">
<emphasis id="B94DEA82FFE5482488FAFBDD0656FBAE" bold="true" box="[151,541,1052,1078]" pageId="20" pageNumber="21">
<taxonomicName id="4C394D13FFE5482488FAFBDD05F3FBAE" authority="Kejval, 2017" authorityName="Kejval" authorityYear="2017" box="[151,440,1052,1078]" class="Insecta" family="Anthicidae" genus="Anthelephila" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="bolavensis" status="sp. nov.">
<emphasis id="B94DEA82FFE5482488FAFBDD05F3FBAE" bold="true" box="[151,440,1052,1078]" italics="true" pageId="20" pageNumber="21">Anthelephila bolavensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27E57F9FFE5482489D2FBDD0656FBAE" box="[447,541,1052,1078]" pageId="20" pageNumber="21" rank="species">sp. nov.</taxonomicNameLabel>
