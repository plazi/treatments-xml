<quantity id="4CA430FDFF80AB7FFE9DFD1FFEF2FD09" box="[280,383,738,760]" metricMagnitude="-2" metricUnit="m" metricValue="1.85" metricValueMax="1.9" metricValueMin="1.8" pageId="10" pageNumber="459" unit="mm" value="18.5" valueMax="19.0" valueMin="18.0">18–19 mm</quantity>
