<collectingRegion id="499853FAFF80AB7FFAB5FDE8FE91FDB9" country="Australia" name="New South Wales" pageId="10" pageNumber="459">New South Wales</collectingRegion>
................................................................................ 
<taxonomicName id="4C5CE69BFF80AB7FFB64FDCFFA11FDB9" authority="Carne, 1957" authorityName="Carne" authorityYear="1957" box="[1249,1436,562,584]" class="Insecta" family="Dynastidae" genus="Cavonus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="459" phylum="Arthropoda" rank="species" species="leai">
<emphasis id="B928410AFF80AB7FFB64FDCFFB7CFDB6" box="[1249,1265,562,583]" italics="true" pageId="10" pageNumber="459">C</emphasis>
