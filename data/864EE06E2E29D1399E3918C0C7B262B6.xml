<geoCoordinate id="3B575969E106F3538055D4596A276F97" direction="north" orientation="latitude" precision="5" value="45.6848">45.6848°N</geoCoordinate>
, 
<geoCoordinate id="4261B7B848BDBAB000BDBE28A49C65BF" direction="west" orientation="longitude" precision="5" value="-66.8821">66.8821°W</geoCoordinate>
, 1-8.VI.2009, 28.VI-7.VII.2009, R. Webster &amp; M.-A. 
<normalizedToken id="94B3514A7BCDC9ED41F960F97DA6E054" originalValue="Giguère">Giguere</normalizedToken>
