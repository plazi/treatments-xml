<taxonomicName id="A0D82C5880957E39978743889751F825" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="56" pageNumber="141" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
