<subSubSection id="C31EEDA1D80FFF8AFF67658CEC4EE1BC" box="[199,1123,402,427]" pageId="7" pageNumber="181" type="etymology">
<paragraph id="8BBBBE2AD80FFF8AFF67658CEC4EE1BC" blockId="7.[151,1436,258,931]" box="[199,1123,402,427]" pageId="7" pageNumber="181">
<emphasis id="B9706238D80FFF8AFF67658CE963E1BC" bold="true" box="[199,334,402,427]" pageId="7" pageNumber="181">Etymology.</emphasis>
