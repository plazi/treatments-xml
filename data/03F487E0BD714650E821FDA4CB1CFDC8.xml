<collectingRegion id="4999F814BD704650EB69FF4FC883FE88" box="[536,678,251,275]" country="Iran" name="Fars" pageId="21" pageNumber="22">Fars province</collectingRegion>
(
<date id="FFE31036BD704650EBC2FF48C946FE88" box="[691,867,251,275]" pageId="21" pageNumber="22" value="1976-08-18" valueMax="1976-08-19" valueMin="1976-08-18">
