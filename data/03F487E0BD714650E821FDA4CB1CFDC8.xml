<location id="8E82602DBD704650E816FF4FCBF3FE88" LSID="urn:lsid:plazi:treatment:03F487E0BD714650E821FDA4CB1CFDC8:8E82602DBD704650E816FF4FCBF3FE88" box="[359,470,252,275]" municipality="This" name="Deno Mts." pageId="21" pageNumber="22" stateProvince="Fars">Deno Mts.</location>
