, 
<taxonomicName id="139E6DA0FFF5BA6EFD3AB5FFF5DDD388" box="[645,715,643,667]" class="Liliopsida" family="Asparagaceae" genus="Agave" kingdom="Plantae" order="Asparagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="genus">
<emphasis id="E6EACA31FFF5BA6EFD3AB5FFF5DDD388" box="[645,715,643,667]" italics="true" pageId="15" pageNumber="16">Agave</emphasis>
</taxonomicName>
sp., and 
<taxonomicName id="139E6DA0FFF5BA6EFC8FB5F8F47FD388" box="[816,873,644,667]" class="Liliopsida" family="Bromeliaceae" genus="Puya" kingdom="Plantae" order="Poales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="genus">
<emphasis id="E6EACA31FFF5BA6EFC8FB5F8F47FD388" box="[816,873,644,667]" italics="true" pageId="15" pageNumber="16">Puya</emphasis>
