<caption id="73CBAFB9AD0D5F67FF0BE4AC395F3BB1" ID-DOI="http://doi.org/10.5281/zenodo.4649785" ID-Zenodo-Dep="4649785" httpUri="https://zenodo.org/record/4649785/files/figure.png" pageId="116" pageNumber="117" startId="116.[189,232,1685,1711]" targetBox="[190,1396,267,1643]" targetPageId="116">
<paragraph id="270BFF31AD0D5F67FF0BE4AC395F3BB1" blockId="116.[189,1398,1685,1782]" pageId="116" pageNumber="117">
