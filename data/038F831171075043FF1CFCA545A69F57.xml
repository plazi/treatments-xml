<quantity id="4CDE9FE271045043FB69FF1742579DA7" box="[1250,1343,152,178]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="6" pageNumber="7" unit="mm" value="2.1">2,1 mm</quantity>
