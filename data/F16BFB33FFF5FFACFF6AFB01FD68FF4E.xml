, and 
<taxonomicName id="BEC231A6FFF4FFACFC02FF6DFB44FF6C" box="[1023,1161,188,213]" class="Malacostraca" family="Chasmocarcinidae" genus="Alainthesius" kingdom="Animalia" order="Decapoda" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="bertrandi" status="n. sp.">
