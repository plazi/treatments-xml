<taxonomicName id="4C25F795C6651571EE9D3BDD78E8D7C6" baseAuthorityName="Schmidt" baseAuthorityYear="1921" box="[179,505,1926,1950]" class="Insecta" family="Nymphalidae" genus="Junonia" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="588" phylum="Arthropoda" rank="subSpecies" species="westermanni" subSpecies="splendens">Junonia westermanni splendens</taxonomicName>
