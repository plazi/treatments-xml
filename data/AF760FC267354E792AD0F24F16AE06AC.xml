<normalizedToken id="1619F16CF98291091C70D8F7445722F9" originalValue="“Sìchuān">&quot;Sichuan</normalizedToken>
, 
<normalizedToken id="A47725195691653410D8F2CE904F8E6C" originalValue="Qīng-chéng-shān">Qīng-cheng-shan</normalizedToken>
, 32 KM S.W. of 
<normalizedToken id="88D577A5316C47B5775D1AC13CDF7F6E" originalValue="Guàn">Guan</normalizedToken>
<normalizedToken id="B334303A3BC50D7C5B3F8EFE33F86A9C" originalValue="xiàn">xian</normalizedToken>
, 
<normalizedToken id="ED7D46C315265DFFC23530BB76BD0D71" originalValue="Wèi-jiāng-hé">Wei-jiang-he</normalizedToken>
, 900 M elev., 27 June, 1990, Coll. Morse, Yang, Li, Chen&quot; &quot; 
<taxonomicName id="770CFE88C5B40A1ED10360482D83F321" class="Insecta" family="Psychomyiidae" genus="Metalype" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metalype truncata" order="Trichoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="truncata">Metalype truncata</taxonomicName>
