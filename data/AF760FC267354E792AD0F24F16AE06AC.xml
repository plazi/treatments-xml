<subSubSection id="D96EB9755E33DEDB269CFA62DFFD86CF" pageId="7" pageNumber="8" type="type specimen">
<paragraph id="2B3C3F6B4B72F01E61E5E9591CD26FF3" pageId="7" pageNumber="8">Type specimen.</paragraph>
<paragraph id="E8A3D6DB9BA2EB9D2EA744ED484F2C87" pageId="7" pageNumber="8">
