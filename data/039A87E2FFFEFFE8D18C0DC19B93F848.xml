<taxonomicName id="4C334D77FFFEFFE8D18C0DC19E9DF8B7" authority="Amyot &amp; Serville, 1843" authorityName="Amyot &amp; Serville" authorityYear="1843" box="[151,511,1785,1812]" class="Insecta" family="Reduviidae" genus="Heza" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="390" phylum="Arthropoda" rank="genus">
