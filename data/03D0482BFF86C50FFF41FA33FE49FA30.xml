<emphasis id="B90D252FFF86C50FFC4DFB7EFB8CFAFD" box="[923,1060,1312,1337]" italics="true" pageId="10" pageNumber="11">
<taxonomicName id="4C7982BEFF86C50FFC4DFB7EFB88FAFD" box="[923,1056,1312,1337]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="13" phylum="Arthropoda" rank="species" species="flavescens">Flavescens</taxonomicName>
,
</emphasis>
Schh., 
<number id="3DB1EB1AFF86C50FFBA8FB7EFB71FAFE" box="[1150,1241,1312,1338]" pageId="10" pageNumber="11" value="119.0" valueMax="141.0" valueMin="97.0">97–141</number>
