<typeStatus id="54C2479FFF86C50FFC1BFB34FBE8FA40" box="[973,1088,1386,1412]" pageId="10" pageNumber="11">lectotype</typeStatus>
with “ 
<typeStatus id="54C2479FFF86C50FFB45FB34FA8CFA40" box="[1171,1316,1386,1412]" pageId="10" pageNumber="11">Lectotypus</typeStatus>
, 
<number id="3DB1EB1AFF86C50FFAE7FB34FAEFFA40" box="[1329,1351,1386,1412]" pageId="10" pageNumber="11" value="3.0">3</number>
, 
<taxonomicName id="4C7982BEFF86C50FFA8FFB35FE6BFA6E" authority="Boh., Gultekin" authorityName="Boh., Gultekin" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="flavescens">
