<taxonomicName id="FAF04D54FFD5FFB8FBEE9CC2FA5FDAF4" authority="(Borradaile, 1916)" baseAuthorityName="Borradaile" baseAuthorityYear="1916" box="[1097,1431,665,689]" class="Malacostraca" family="Dynomenidae" genus="Dynomene" kingdom="Animalia" order="Decapoda" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="hispida">
