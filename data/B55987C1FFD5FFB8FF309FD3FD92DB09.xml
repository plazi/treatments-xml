</taxonomicName>
, from 
<collectingCountry id="45E77647FFD5FFB8FF739C9AFEA2DA9D" box="[212,362,704,728]" name="New Zealand" pageId="51" pageNumber="52">New Zealand</collectingCountry>
and 
<taxonomicName id="FAF04D54FFD5FFB8FE069C9BFD0EDA9D" authority="Yokoya, 1933" authorityName="Yokoya" authorityYear="1933" box="[417,710,704,728]" class="Malacostraca" family="Inachidae" genus="Cyrtomaia" kingdom="Animalia" order="Decapoda" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="platypes">
