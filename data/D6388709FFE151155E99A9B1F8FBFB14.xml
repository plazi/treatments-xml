8. 
<taxonomicName id="99914D9CFFE151175D5BA9B1FC9FF883" authority="Zimmerman, 1942" authorityName="Zimmerman." authorityYear="1942" box="[512,869,1771,1813]" class="Insecta" family="Curculionidae" genus="Trigonops" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="90" phylum="Arthropoda" rank="species" species="convexa" status="new species">Trigonops convexa</taxonomicName>
