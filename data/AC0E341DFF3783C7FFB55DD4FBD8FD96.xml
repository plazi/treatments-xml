var. 
<emphasis id="16D35919FF3783C7FAA45A55FA3DFDB6" box="[1376,1453,565,589]" italics="true" pageId="138" pageNumber="138">kotschyi</emphasis>
</taxonomicName>
and 
<taxonomicName id="E3A7FE88FF3783C7FC945A35FBD1FD96" box="[848,1089,597,621]" class="Insecta" family="Noctuidae" genus="Crenularia" kingdom="Animalia" order="Lepidoptera" pageId="138" pageNumber="138" phylum="Arthropoda" rank="species" species="eunomioides">
