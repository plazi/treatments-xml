<quantity id="82729B5CFFC1FFEBFD22C312FD27FE38" box="[664,735,366,390]" metricMagnitude="1" metricUnit="m" metricValue="1.6509999999999998" pageId="19" pageNumber="20" unit="in" value="650.0">650 in</quantity>
