<paragraph id="8B89A4219158FFC728BAC489FAEDF731" blockId="2.[151,881,1939,2026]" lastBlockId="3.[151,1437,151,448]" lastPageId="3" lastPageNumber="62" pageId="2" pageNumber="61">
<taxonomicName id="4C36DFA29158FFC628BAC489FDC4F1FD" authority="Wu, 1962" authorityName="Wu" authorityYear="1962" box="[151,575,2003,2026]" class="Insecta" family="Nemouridae" genus="Sphaeronemoura" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="hamistyla">
<emphasis id="B94278339158FFC628BAC489FE47F1FD" box="[151,444,2003,2026]" italics="true" pageId="2" pageNumber="61">Sphaeronemoura hamistyla</emphasis>
(
<bibRefCitation id="EFA7D9D09158FFC629E4C489FDCCF1FD" author="Wu" box="[457,567,2003,2026]" pageId="2" pageNumber="64" refString="Wu, C. F. (1962) Results of the Zoologico - Botanical expedition to Southwest China, 1955 - 1957 (Plecoptera). Acta Entomologica Sinica, 11 (Supplement), 139 - 153." type="journal article" year="1962">
