<taxonomicName id="4C0E4D37F21FF179EB6314C2FB315FB0" box="[1081,1200,686,707]" class="Aves" family="Pachycephalidae" genus="Pachycephala" kingdom="Animalia" order="Passeriformes" pageId="43" pageNumber="43" phylum="Chordata" rank="species" species="examinata">
