<taxonomicName id="248EE37FDFFDA5236C57AD2141E172C0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29759" class="Insecta" family="Formicidae" genus="Lasius" kingdom="Animalia" lsidName-HNS="Lasius umbratus (Nylander)" order="Hymenoptera" pageNumber="103" phylum="Arthropoda" rank="species" species="umbratus">Lasius umbratus (Nyl.)</taxonomicName>
