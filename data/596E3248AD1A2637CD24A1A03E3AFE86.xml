<taxonomicName id="16C7F8DDAD1A2634CDF3A24F39FCF9B9" authority="Griseb" authorityName="Griseb" box="[906,1221,1543,1567]" class="Magnoliopsida" family="Amaranthaceae" genus="Woehleria" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="21" pageNumber="965" phylum="Tracheophyta" rank="species" species="serpyllifolia">
