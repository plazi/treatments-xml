. 
<materialsCitation id="9BEC3C9F1C6D535754CCFAAB2710E5E9" ID-GBIF-Occurrence="3026660442" collectingDate="1919-07" collectionCode="NYSM" location="Cibodas" pageId="38" pageNumber="39" specimenCount="1" stateProvince="Jawa Timur" typeStatus="syntype">
