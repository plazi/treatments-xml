<collectedFrom id="9898753F1C6D535756E2FAAB2609E530" box="[939,1306,1328,1355]" pageId="38" pageNumber="39">
on 
<taxonomicName id="EC844D411C6D53575684FAAA2609E530" authority="(Miq.) Kurz" authorityName="Kurz" authorityYear="1867" baseAuthorityName="Miq." box="[973,1306,1328,1355]" class="Liliopsida" family="Pandanaceae" genus="Pandanus" kingdom="Plantae" order="Pandanales" pageId="38" pageNumber="39" phylum="Tracheophyta" rank="species" species="nitidus">
