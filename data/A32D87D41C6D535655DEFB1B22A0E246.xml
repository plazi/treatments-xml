</taxonomicName>
</collectedFrom>
at 
<location id="2E5B60191C6D53575070FAAA268BE531" LSID="urn:lsid:plazi:treatment:A32D87D41C6D535655DEFB1B22A0E246:2E5B60191C6D53575070FAAA268BE531" box="[1337,1432,1328,1354]" name="Cibodas" pageId="38" pageNumber="39" stateProvince="Jawa Timur">Cibodas</location>
, 
