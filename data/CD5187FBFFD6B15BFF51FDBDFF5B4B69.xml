<taxonomicName id="82F84D6EFFD5B15BFEDAF9FBFDB94A18" box="[284,539,1580,1602]" class="Insecta" family="Chrysomelidae" genus="Clytra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="truncatula" status="sp. nov." subGenus="Clytraria">
<emphasis id="778CEAFFFFD5B15BFEDAF9FBFE914A18" box="[284,307,1581,1602]" italics="true" pageId="6" pageNumber="7">C.</emphasis>
