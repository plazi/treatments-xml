<subSubSection id="C37C65018BD6FF65FEAFFB5AFEF7FAF0" pageId="152" pageNumber="153" type="etymology">
<paragraph id="8BD9368A8BD6FF65FEAFFB5AFE14FB40" blockId="152.[264,1323,1249,1355]" box="[264,461,1249,1275]" pageId="152" pageNumber="153">
<heading id="D09181E68BD6FF65FEAFFB5AFE14FB40" bold="true" box="[264,461,1249,1275]" fontSize="11" level="1" pageId="152" pageNumber="153" reason="1">
