<collectingCountry id="36A08397FFCD4C4AFEB037A63391B7AD" box="[359,441,1737,1763]" name="Greece" pageId="77" pageNumber="78">Greece</collectingCountry>
, 
<collectingCountry id="36A08397FFCD4C4AFE1237A6306CB7AD" box="[453,580,1737,1763]" name="Macedonia" pageId="77" pageNumber="78">Macedonia</collectingCountry>
