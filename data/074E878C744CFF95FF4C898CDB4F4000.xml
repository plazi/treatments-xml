<quantity id="481F9B7F744BFF95FED68E0ADE014000" box="[296,375,525,547]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="4" pageNumber="901" unit="m" value="3000.0">3000 m</quantity>
