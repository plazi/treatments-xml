<taxonomicName id="D96C1A5DF019162B8C56CA9F00C3851F" class="Arachnida" family="Theraphosidae" genus="Theraphosa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Theraphosa panamana" order="Araneae" pageId="8" pageNumber="83" phylum="Arthropoda" rank="species" species="panamana">Theraphosa panamana</taxonomicName>
