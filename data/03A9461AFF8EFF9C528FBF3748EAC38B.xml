</keyLead>
</paragraph>
<paragraph id="8BBFF70CFF8EFF9C528FBD0448EAC22B" blockId="23.[151,1437,1366,2030]" pageId="23" pageNumber="24">
<keyLead id="30F15439FF8EFF9C528FBD0448EAC22B" pageId="23" pageNumber="24">
<number id="3DC8E52BFF8EFF9C528FBD044DD3C24B" box="[151,165,1558,1581]" pageId="23" pageNumber="24" value="2.0">2</number>
