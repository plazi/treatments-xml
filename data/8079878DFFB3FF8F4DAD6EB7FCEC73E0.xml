<number id="BE1824BCFFB2FF8F4CFC6AD4FE537798" box="[454,468,655,679]" pageId="27" pageNumber="78" value="6.0">6</number>
J, 
<number id="BE1824BCFFB2FF8F4CC26ACBFD817798" box="[504,518,656,679]" pageId="27" pageNumber="78" value="7.0">7</number>
J, 
