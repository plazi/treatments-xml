<taxonomicName id="4432A9502740D7E8E07F9EEE73E806D7" class="Insecta" family="Acrididae" genus="Rhabdoplea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhabdoplea angusticornis" order="Orthoptera" pageId="15" pageNumber="52" phylum="Arthropoda" rank="species" species="angusticornis">Rhabdoplea angusticornis</taxonomicName>
