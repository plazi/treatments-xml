<taxonomicNameLabel id="789D57D79D3DFF9AFE0088D7FDE18A2E" box="[467,524,1265,1286]" pageId="13" pageNumber="268" rank="species">
<emphasis id="63AEEAAC9D3DFF9AFE0088D7FDE18A2E" bold="true" box="[467,524,1265,1286]" pageId="13" pageNumber="268">n. sp.</emphasis>
</taxonomicNameLabel>
, yellow triangles. 
<taxonomicName id="96DA4D3D9D3DFF9AFD1488D7FC798A2F" authority="Ronderos &amp; Spinelli &amp; Grogan, 2017" authorityName="Ronderos &amp; Spinelli &amp; Grogan" authorityYear="2017" box="[711,916,1265,1287]" class="Insecta" family="Ceratopogonidae" genus="Austrohelea" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="268" phylum="Arthropoda" rank="species" species="spinosa" status="n. sp.">
<emphasis id="63AEEAAC9D3DFF9AFD1488D7FC798A2F" box="[711,916,1265,1287]" italics="true" pageId="13" pageNumber="268">Austrohelea spinosa</emphasis>
</taxonomicName>
<taxonomicNameLabel id="789D57D79D3DFF9AFC4888D7FC398A2E" box="[923,980,1265,1286]" pageId="13" pageNumber="268" rank="species">
<emphasis id="63AEEAAC9D3DFF9AFC4888D7FC398A2E" bold="true" box="[923,980,1265,1286]" pageId="13" pageNumber="268">n. sp.</emphasis>
