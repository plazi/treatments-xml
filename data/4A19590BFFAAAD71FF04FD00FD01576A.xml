<quantity id="054845F8FFAAAD72FE2BF84DFDB0529B" box="[440,542,1946,1970]" metricMagnitude="-2" metricUnit="m" metricValue="1.1199999999999999" pageId="12" pageNumber="332" unit="mm" value="11.2">11.2 mm</quantity>
; forewing length 
