<taxonomicName id="1CCACE42AB1C63597DEAD18A0A5196AA" baseAuthorityName="Agnew" baseAuthorityYear="1961" class="Insecta" family="BaetidaeEphemeroptera " genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis glaucus" order="Ephemeroptera" pageId="0" pageNumber="427" phylum="Arthropoda" rank="species" species="glaucus">
<emphasis id="99E31EE707278EC35150DB5FF2DE8152" italics="true" pageId="0" pageNumber="427">Labiobaetis glaucus</emphasis>
</taxonomicName>
and 
<taxonomicName id="59A3A26CC747AC14A114C037F61A8F71" baseAuthorityName="Barnard" baseAuthorityYear="1932" class="Insecta" family="BaetidaeEphemeroptera " genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis vinosus" order="Ephemeroptera" pageId="0" pageNumber="427" phylum="Arthropoda" rank="species" species="vinosus">
