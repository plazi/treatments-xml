<taxonomicName id="4C224D288977783AF922301244C2D617" authority="Hentz, 1841" authorityName="Hentz" authorityYear="1841" box="[636,952,528,555]" class="Arachnida" family="Pholcidae" genus="Spermophora" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="19" phylum="Arthropoda" rank="genus">
