<taxonomicName id="4C5D01B5FFE060186861BD4EECBCFEE7" authority="Jason G. H. Londt, 2012" authorityName="Jason G. H. Londt" authorityYear="2012" box="[592,727,345,361]" class="Insecta" family="Asilidae" genus="Fishermyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="222" phylum="Arthropoda" rank="genus" status="gen. n.">
