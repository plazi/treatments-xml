<taxonomicName id="4C344D29FFBB4A20FE1EC689FD0DECDC" authorityName="Galil" authorityYear="2009" baseAuthorityName="De Haan" baseAuthorityYear="1841" box="[510,647,944,970]" class="Malacostraca" family="Leucosiidae" genus="Hiplyra" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="296" phylum="Arthropoda" rank="species" species="platycheir" status="n. comb.">
<emphasis id="B940EAB8FFBB4A20FE1EC689FD0DECDC" box="[510,647,944,970]" italics="true" pageId="17" pageNumber="296">H. platycheir</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27357C3FFBB4A20FF91C6E8FF5EECFD" box="[113,212,976,1003]" pageId="17" pageNumber="296" rank="species">n. comb.</taxonomicNameLabel>
