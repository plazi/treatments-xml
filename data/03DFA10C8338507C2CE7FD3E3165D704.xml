<location id="8EA946C18338507C2939FC6B31C2D19C" LSID="urn:lsid:plazi:treatment:03DFA10C8338507C2CE7FD3E3165D704:8EA946C18338507C2939FC6B31C2D19C" box="[1353,1432,993,1018]" country="Italy" municipality="Vicenza" name="Alonte" pageId="6" pageNumber="49">Alonte</location>
, 
<collectingMunicipality id="6BAD8A608338507C2CE7FB8F34AED678" box="[151,244,1029,1054]" pageId="6" pageNumber="49">Vicenza</collectingMunicipality>
, 
<collectingCountry id="F361508A8338507C2C8FFB8F3569D678" box="[255,307,1029,1054]" name="Italy" pageId="6" pageNumber="49">Italy</collectingCountry>
, 
<collectingDate id="EF8CCF328338507C2D4EFB8C35E4D678" box="[318,446,1030,1054]" pageId="6" pageNumber="49" value="2014-02-01">
<date id="FFC836DA8338507C2D4EFB8C35E4D678" box="[318,446,1030,1054]" pageId="6" pageNumber="49" value="2014-02-01">01/02/2014</date>
</collectingDate>
leg. 
<collectorName id="268375CC8338507C2D85FB8F3694D678" box="[501,718,1029,1054]" pageId="6" pageNumber="49">Federico Gavinelli.</collectorName>
(
