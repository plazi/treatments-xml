<taxonomicName id="C8E02C864A533A22FBEEFAEEFB4CFA66" authorityName="VOSS &amp; LUNDE &amp; JANSA" authorityYear="2005" box="[1079,1229,1392,1415]" class="Mammalia" family="Didelphidae" genus="Cryptonanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Didelphimorphia" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
