<keyLead id="30F79205095A032BFF26534133B5F9D2" box="[151,1429,1570,1593]" pageId="37" pageNumber="38">
1. Length&gt; 
<quantity id="4CFE9CD5095A032BFE81534137A5F9D2" box="[304,389,1570,1593]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="37" pageNumber="38" unit="mm" value="3.7">3.7 mm</quantity>
