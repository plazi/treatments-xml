<collectingRegion id="49A733AFFFE6FFFBFE3C11AFFDD4F3FE" box="[450,549,226,252]" country="China" name="Ningxia" pageId="7" pageNumber="66">Ningxia</collectingRegion>
, 
<collectingRegion id="49A733AFFFE6FFFBFDCF11AFFD74F3FE" box="[561,645,226,252]" country="China" name="Shanxi" pageId="7" pageNumber="66">Shanxi</collectingRegion>
, 
