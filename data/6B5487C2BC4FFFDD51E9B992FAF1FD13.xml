<collectingCounty id="0A234E58BC4FFFDD51A2B8B1FEC6FE47" box="[220,379,441,468]" pageId="9" pageNumber="32">Museum Paris</collectingCounty>
[printed] 
<collectingCountry id="9BEA7644BC4FFFDD5099B8B2FD97FE47" box="[487,554,441,467]" name="Brazil" pageId="9" pageNumber="32">Brésil</collectingCountry>
[handwritten]\ [red label] 
<typeStatus id="3C468876BC4FFFDD5232B8B2FC13FE47" box="[844,942,441,467]" pageId="9" pageNumber="32">holotype</typeStatus>
