<collectionCode id="85ECAE11BC4FFFDD5447B8B2FA2DFE47" box="[1337,1424,441,467]" country="USA" httpUri="http://grbio.org/cool/3kvn-7rck" name="Princeton University" pageId="9" pageNumber="32">MNHP</collectionCode>
