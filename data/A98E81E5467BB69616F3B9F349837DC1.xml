1. 
<taxonomicName id="B145723978778482EFF633DBA41C2134" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142737" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole militaris Smith" order="Hymenoptera" pageId="7" pageNumber="74" phylum="Arthropoda" rank="species" species="militaris">Pheidole militaris</taxonomicName>
