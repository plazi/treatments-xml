<taxonomicName id="4C76E7A6AB4AFFB1FE05265EFCBAE20F" authority="Marino &amp; Spinelli, 2004" authorityName="Marino &amp; Spinelli" authorityYear="2004" box="[405,835,576,600]" class="Insecta" family="Ceratopogonidae" genus="Forcipomyia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="2257" phylum="Arthropoda" rank="species" species="sayhuequei" status="sp. n." subGenus="Trichohelea">
