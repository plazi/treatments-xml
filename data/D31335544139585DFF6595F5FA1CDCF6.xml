<quantity id="9C4229A74139585DFADC94C9FAD4DCD1" box="[1326,1364,1967,1989]" metricMagnitude="-2" metricUnit="kg" metricValue="1.5" pageId="26" pageNumber="327" unit="g" value="15.0">15g</quantity>
, h
