<quantity id="9C4229A74139585DFB1494D6FA8CDCD1" box="[1254,1292,1968,1989]" metricMagnitude="-2" metricUnit="kg" metricValue="1.4" pageId="26" pageNumber="327" unit="g" value="14.0">14g</quantity>
, h
