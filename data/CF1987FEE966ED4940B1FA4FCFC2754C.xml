(
<taxonomicName id="80B04D6BE967ED564213FA44C9AF71CA" box="[628,754,1442,1466]" class="Insecta" family="Xylomyidae" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="15" phylum="Arthropoda" rank="family">Xylomyidae</taxonomicName>
