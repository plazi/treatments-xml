<bibRefCitation id="EFA098CD1965FF89692290DB1B80FF6D" author="Hart and Pilling" box="[862,1185,213,235]" firstAuthor="Hart" page="101" pageId="90" pageNumber="90" refId="ref124601" refString="Hart, C. W. M., and A. R. Pilling. 1964. The Tiwi of north Australia. New York: Holt, Rinehart and Winston, 118 pp." type="book" year="1964">Hart and Pilling, 1964: 101</bibRefCitation>
) at 
<geoCoordinate id="EE0583FB1965FF8968C790FC1C2BFE8E" box="[699,778,242,264]" degrees="11.45" direction="south" orientation="latitude" pageId="90" pageNumber="90" precision="555" value="-11.45">11.45S</geoCoordinate>
, 
<geoCoordinate id="EE0583FB1965FF89696490FC1C57FE81" box="[792,886,242,263]" degrees="130.41" direction="east" orientation="longitude" pageId="90" pageNumber="90" precision="555" value="130.41">130.41E</geoCoordinate>
(Times 
<taxonomicName id="4C319EBF1965FF8969AB90FC1B38FE8E" box="[983,1049,242,264]" class="Mollusca (awaiting allocation)" family="Mollusca (awaiting allocation)" genus="Atlas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca (awaiting allocation)" pageId="90" pageNumber="3" phylum="Mollusca" rank="genus">Atlas</taxonomicName>
).
</paragraph>
