<taxonomicName id="4C58BFAAFFC8FFBAFAE053B6CA8DFD49" authorityName="Wilkinson" authorityYear="1931" box="[1327,1427,729,755]" class="Insecta" family="Braconidae" genus="Spathius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cursor">
<emphasis id="B92C183BFFC8FFBAFAE053B6CA8DFD49" box="[1327,1427,729,755]" italics="true" pageId="14" pageNumber="15">S. cursor</emphasis>
</taxonomicName>
) and eye as high as broad (1.3 × higher than broad in 
<taxonomicName id="4C58BFAAFFC8FFBAFD2C5392CC56FCAD" authorityName="Wilkinson" authorityYear="1931" box="[739,840,765,791]" class="Insecta" family="Braconidae" genus="Spathius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cursor">
<emphasis id="B92C183BFFC8FFBAFD2C5392CC56FCAD" box="[739,840,765,791]" italics="true" pageId="14" pageNumber="15">S. cursor</emphasis>
</taxonomicName>
). Differences between 
<taxonomicName id="4C58BFAAFFC8FFBAFB825392CA22FCAD" authority="Ranjith &amp; Belokobylskij &amp; Priyadarsanan &amp; Aswaj &amp; Nasser, 2020" authorityName="Ranjith &amp; Belokobylskij &amp; Priyadarsanan &amp; Aswaj &amp; Nasser" authorityYear="2020" box="[1101,1340,765,791]" class="Insecta" family="Braconidae" genus="Spathius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="himalayicus" status="sp. nov.">
<emphasis id="B92C183BFFC8FFBAFB825392CA22FCAD" box="[1101,1340,765,791]" italics="true" pageId="14" pageNumber="15">Spathius himalayicus</emphasis>
</taxonomicName>
<emphasis id="B92C183BFFC8FFBAFA8C5392CA82FCAD" bold="true" box="[1347,1436,765,791]" pageId="14" pageNumber="15">
<taxonomicNameLabel id="A21FA540FFC8FFBAFA8C5392CA82FCAD" box="[1347,1436,765,791]" pageId="14" pageNumber="15" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C58BFAAFFC8FFBAFF08524ECD5DFC81" authority="Belokobylskij et Austin" authorityName="Belokobylskij et Austin" authorityYear="2013" box="[199,579,801,827]" class="Insecta" family="Braconidae" genus="Spathius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="thorpei">
<emphasis id="B92C183BFFC8FFBAFF08524ECE2AFC81" box="[199,308,801,827]" italics="true" pageId="14" pageNumber="15">S. thorpei</emphasis>
