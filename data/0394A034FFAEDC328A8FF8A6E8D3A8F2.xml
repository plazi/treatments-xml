<taxonomicName id="4C3D6AA1FFAFDC328F4BFEE8EAD7A81F" class="Insecta" family="Cicadellidae" genus="Eupteryx" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="596" phylum="Arthropoda" rank="species" species="hela" subGenus="Stacla">
