<quantity id="4CB512DCFFD0D44DFF5F6946FE93FEC9" box="[228,290,332,353]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="4" pageNumber="687" unit="mm" value="7.0">7 mm</quantity>
