<typeStatus id="D433885DFEC8FE81FD8E63F8FD54FDB3" box="[621,669,504,528]" pageId="322" pageNumber="323">type</typeStatus>
material of 
<taxonomicName id="CC884D7CFEC8FE81FCC663F9FC37FDB3" authorityName="Carriker" authorityYear="1903" box="[805,1022,503,528]" class="Insecta" family="Philopteridae" genus="Nirmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="322" pageNumber="323" phylum="Arthropoda" rank="species" species="rhamphasti">
