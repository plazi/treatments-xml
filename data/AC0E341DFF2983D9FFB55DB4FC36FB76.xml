<collectionCode id="42B61DCEFF2983D9FABF5C75FA01FBD6" box="[1403,1425,1045,1069]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="148" pageNumber="148" type="Herbarium">G</collectionCode>
