<emphasis id="B96C6272FF9FDB41FF7BBEBC7A2827A6" bold="true" box="[151,291,1202,1224]" pageId="19" pageNumber="120">FIGURE 13.</emphasis>
<taxonomicName id="4C18C5E3FF9FDB41FEC0BEBC7AEB27A9" box="[300,480,1202,1224]" class="Gymnolaemata" family="Cribrilinidae" genus="Collarina" kingdom="Animalia" order="Cheilostomatida" pageId="19" pageNumber="120" phylum="Bryozoa" rank="species" species="spicata" status="sp. nov.">
<emphasis id="B96C6272FF9FDB41FEC0BEBC7AEB27A9" box="[300,480,1202,1224]" italics="true" pageId="19" pageNumber="120">Collarina spicata</emphasis>
</taxonomicName>
<emphasis id="B96C6272FF9FDB41FE06BEBC793527A9" bold="true" box="[490,574,1202,1224]" pageId="19" pageNumber="120">
<taxonomicNameLabel id="A25FDF09FF9FDB41FE06BEBC793527A9" box="[490,574,1202,1224]" pageId="19" pageNumber="120" rank="species">sp. nov.</taxonomicNameLabel>
