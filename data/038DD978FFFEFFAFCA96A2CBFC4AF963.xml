<taxonomicName id="4C2413EDFFFEFFAFCB9BA11BFD44F9BE" authority="Walker, 1865" authorityName="Walker" authorityYear="1865" box="[410,655,1621,1644]" class="Insecta" family="Bombycidae" genus="Andraca" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="65" phylum="Arthropoda" rank="genus">
<emphasis id="B950B47CFFFEFFAFCB9BA11BFE3DF9BE" box="[410,502,1621,1644]" italics="true" pageId="0" pageNumber="65">Andraca</emphasis>
