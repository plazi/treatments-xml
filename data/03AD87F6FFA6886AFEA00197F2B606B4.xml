<taxonomicName id="4C044D63FFA7886AFBC9063CF7920607" box="[1121,1253,284,309]" class="Lycopodiopsida" family="Selaginellaceae" genus="Selaginella" kingdom="Plantae" order="Selaginellales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="genus">
