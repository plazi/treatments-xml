<quantity id="EA539B00FFD4FFDAFE9CFC99FEB4FC91" box="[261,337,799,823]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="57" pageNumber="58" unit="mm" value="19.0">19mm</quantity>
