<taxonomicName id="4C744D5DFF83730F18C4FB28FD1F93E1" authority="Ramsay, 1876 a" authorityName="Ramsay" authorityYear="1876" box="[246,714,1219,1246]" class="Mammalia" family="Macropodidae" genus="Halmaturus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diprotodontia" pageId="58" pageNumber="335" phylum="Chordata" rank="species" species="crassipes">
