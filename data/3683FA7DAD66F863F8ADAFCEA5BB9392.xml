<taxonomicName id="4805F0642821963D2C30A32CC31221A5" LSID="http://zoobank.org/94DDB038-2111-44D1-A940-766BF8F15E51" class="Mammalia" family="Procyonidae" genus="Bassaricyon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bassaricyon neblina" order="Carnivora" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="neblina">Bassaricyon neblina</taxonomicName>
