<taxonomicName id="28B1B9A0FFE7FFA3FB72FC93FA8D275B" authority="Jordal, 2021" authorityName="Jordal" authorityYear="2021" box="[1240,1348,797,818]" class="Insecta" family="Curculionidae" genus="Lanurgus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="91" phylum="Arthropoda" rank="species" species="mattheei" status="sp. nov.">
