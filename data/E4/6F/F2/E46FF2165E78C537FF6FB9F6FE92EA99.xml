<specimenCount box="[752,889,1938,1962]" pageId="122" pageNumber="123" type="generic">9 specimens</specimenCount>
(
<specimenCount box="[905,994,1938,1962]" pageId="122" pageNumber="123" type="male">6 males</specimenCount>
