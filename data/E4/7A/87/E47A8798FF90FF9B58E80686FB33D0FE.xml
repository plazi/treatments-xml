<emphasis bold="true" box="[281,420,1461,1485]" italics="true" pageId="3" pageNumber="518">Anurogryllus</emphasis>
(
<emphasis bold="true" box="[436,549,1461,1485]" italics="true" pageId="3" pageNumber="518">Urogryllus</emphasis>
) 
<emphasis bold="true" box="[563,715,1461,1485]" italics="true" pageId="3" pageNumber="518">edithsantosum</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[721,779,1462,1486]" pageId="3" pageNumber="518" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
male habitus 
<emphasis bold="true" box="[920,944,1462,1486]" pageId="3" pageNumber="518">A.</emphasis>
Lateral and 
<emphasis bold="true" box="[1072,1095,1462,1486]" pageId="3" pageNumber="518">B.</emphasis>
