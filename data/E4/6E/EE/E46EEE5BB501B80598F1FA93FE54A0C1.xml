); length leg I 
<number box="[292,336,1805,1831]" pageId="2" pageNumber="53" value="148.0">148</number>
(
<number box="[352,460,1805,1831]" pageId="2" pageNumber="53" value="147.5" valueMax="158.0" valueMin="137.0">137–158</number>
); leg II 
<number box="[551,595,1805,1831]" pageId="2" pageNumber="53" value="142.0">142</number>
