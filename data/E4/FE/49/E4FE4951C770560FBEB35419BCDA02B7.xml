<paragraph pageId="3" pageNumber="22">
<emphasis bold="true" pageId="3" pageNumber="22">Figures 10-16.</emphasis>
<taxonomicName authority="Nikitsky, 1998" authorityName="Nikitsky" authorityYear="1998" class="Insecta" family="Tetratomidae" genus="Cyanopenthe" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cyanopenthe granulata" order="Coleoptera" pageId="3" pageNumber="22" phylum="Arthropoda" rank="species" species="granulata">
