<taxonomicName class="Magnoliopsida" family="Malvaceae" genus="Embioptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Malvales" pageId="737" pageNumber="738" phylum="Tracheophyta" rank="genus">Embioptera</taxonomicName>
