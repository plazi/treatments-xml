<taxonomicName authority="Spiridonov &amp; Apel, 2007" authorityName="Spiridonov &amp; Apel" authorityYear="2007" box="[264,705,665,688]" class="Malacostraca" family="Calappidae" genus="Mursia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="587" phylum="Arthropoda" rank="species" species="minuta">
