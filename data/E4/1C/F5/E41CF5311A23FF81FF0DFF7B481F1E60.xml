<collectingCountry box="[359,430,367,393]" name="China" pageId="3" pageNumber="487">China</collectingCountry>
: 
<collectingRegion box="[441,620,367,393]" country="China" name="Hubei" pageId="3" pageNumber="487">Hubei Province</collectingRegion>
