: 
<taxonomicName authority="Pascoe, 1888" authorityName="Pascoe" authorityYear="1888" box="[298,620,222,244]" class="Insecta" family="Cerambycidae" genus="Sparna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="macilenta">
