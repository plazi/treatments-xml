<taxonomicName authority="Oketch &amp; Li, 2020" authorityName="Oketch &amp; Li" authorityYear="2020" class="Arachnida" family="Palpimanidae" genus="Sceliscelis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sceliscelis marshi" order="Araneae" pageId="0" pageNumber="93" phylum="Arthropoda" rank="species" species="marshi">
