<collectingCountry box="[409,485,1763,1787]" name="Kenya" pageId="34" pageNumber="335">Kenya</collectingCountry>
, 
<collectingCountry box="[500,675,1762,1786]" name="Democratic Republic of the Congo" pageId="34" pageNumber="335">Belgian Congo</collectingCountry>
, 
<collectingCountry box="[691,767,1762,1786]" name="Ghana" pageId="34" pageNumber="335">Ghana</collectingCountry>
, 
<collectingCountry box="[782,842,1763,1787]" name="Togo" pageId="34" pageNumber="335">Togo</collectingCountry>
