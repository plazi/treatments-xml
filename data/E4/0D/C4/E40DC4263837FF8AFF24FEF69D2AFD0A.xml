<quantity box="[1101,1172,510,534]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="19" pageNumber="320" unit="m" value="1.5" valueMax="2.0" valueMin="1.0">1–2 m</quantity>
