<paragraph blockId="23.[151,1436,1005,1750]" pageId="23" pageNumber="324">
<emphasis bold="true" box="[201,267,1257,1282]" pageId="23" pageNumber="324">Song.</emphasis>
<taxonomicName box="[275,456,1257,1281]" class="Insecta" family="Phaneropteridae" genus="Arantia" kingdom="Animalia" order="Orthoptera" pageId="23" pageNumber="324" phylum="Arthropoda" rank="species" species="fasciata">
<emphasis box="[275,456,1257,1281]" italics="true" pageId="23" pageNumber="324">Arantia fasciata</emphasis>
</taxonomicName>
