<paragraph blockId="22.[151,1436,1587,1828]" pageId="22" pageNumber="323">
<emphasis bold="true" box="[201,267,1695,1720]" pageId="22" pageNumber="323">Song.</emphasis>
<taxonomicName box="[274,387,1697,1720]" class="Insecta" family="Phaneropteridae" genus="Lamecosoma" kingdom="Animalia" order="Orthoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="inermis">
<emphasis box="[274,387,1697,1720]" italics="true" pageId="22" pageNumber="323">L. inermis</emphasis>
</taxonomicName>
