<quantity box="[1028,1102,1732,1756]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="22" pageNumber="323" unit="m" value="1.5" valueMax="2.0" valueMin="1.0">
