<paragraph blockId="25.[151,1436,1938,2034]" pageId="25" pageNumber="326">
<emphasis bold="true" box="[201,300,1974,1999]" pageId="25" pageNumber="326">Habitat.</emphasis>
Hemp 
<emphasis box="[382,431,1974,1999]" italics="true" pageId="25" pageNumber="326">et al</emphasis>
., 
<number box="[452,511,1974,1998]" pageId="25" pageNumber="326" value="2013.0">2013</number>
