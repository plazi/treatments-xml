<subSubSection lastPageId="36" lastPageNumber="55" pageId="35" pageNumber="54" type="description">
<paragraph blockId="35.[189,464,1253,1281]" box="[189,464,1253,1281]" pageId="35" pageNumber="54">
<emphasis bold="true" box="[189,334,1253,1279]" pageId="35" pageNumber="54">Description</emphasis>
(
<typeStatus box="[349,455,1254,1281]" pageId="35" pageNumber="54">holotype</typeStatus>
