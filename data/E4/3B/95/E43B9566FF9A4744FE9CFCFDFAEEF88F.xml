<taxonomicName authority="Walker" authorityName="Walker" class="Insecta" family="Sciomyzidae" genus="Thecomyia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="lateralis">
