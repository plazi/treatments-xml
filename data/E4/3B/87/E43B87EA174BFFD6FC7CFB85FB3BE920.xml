of 
<taxonomicName authority="V.T. Tran, N.V. Duy &amp; N.H. Xia" authorityName="V. T. Tran, N. V. Duy &amp; N. H. Xia" authorityYear="2015" class="Magnoliopsida" family="Magnoliaceae" genus="Magnolia" kingdom="Plantae" order="Magnoliales" pageId="1" pageNumber="14" phylum="Tracheophyta" rank="species" species="lamdongensis" status="sp. nov.">
