, 
<taxonomicName authority="Dandy" authorityName="Dandy" authorityYear="1930" class="Magnoliopsida" family="Magnoliaceae" genus="Manglietia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Magnoliales" pageId="1" pageNumber="14" phylum="Tracheophyta" rank="species" species="chevalieri">
