<subSubSection pageId="25" pageNumber="26" type="distribution">
<paragraph blockId="25.[151,1437,397,675]" pageId="25" pageNumber="26">
<emphasis bold="true" box="[199,353,433,459]" pageId="25" pageNumber="26">Distribution.</emphasis>
Western Atlantic— 
<collectingCountry box="[582,686,433,459]" name="Bermuda" pageId="25" pageNumber="26">Bermuda</collectingCountry>
, 
<collectingCountry box="[700,754,433,459]" name="United States of America" pageId="25" pageNumber="26">USA</collectingCountry>
(
<collectingRegion box="[772,945,433,459]" country="United States of America" name="North Carolina" pageId="25" pageNumber="26">North Carolina</collectingRegion>
to Florida), Gulf of Mexico, 
<collectingCountry box="[1293,1432,433,460]" name="Puerto Rico" pageId="25" pageNumber="26">Puerto Rico</collectingCountry>
, Antilles, 
<collectingCountry box="[252,370,469,495]" name="Venezuela" pageId="25" pageNumber="26">Venezuela</collectingCountry>
, and 
<collectingCountry box="[427,495,469,495]" name="Brazil" pageId="25" pageNumber="26">Brazil</collectingCountry>
(
<collectingRegion box="[509,594,469,495]" country="Brazil" name="Amapa" pageId="25" pageNumber="26">Amapá</collectingRegion>
, Pará, 
<collectingRegion box="[667,754,469,495]" country="Brazil" name="Paraiba" pageId="25" pageNumber="26">Paraíba</collectingRegion>
, 
<collectingRegion box="[764,907,469,495]" country="Brazil" name="Pernambuco" pageId="25" pageNumber="26">Pernambuco</collectingRegion>
, 
<collectingRegion box="[915,1010,469,495]" country="Brazil" name="Alagoas" pageId="25" pageNumber="26">Alagoas</collectingRegion>
, 
<collectingRegion box="[1020,1088,469,495]" country="Brazil" name="Bahia" pageId="25" pageNumber="26">Bahia</collectingRegion>
, 
<collectingRegion box="[1098,1260,469,495]" country="Brazil" name="Espirito Santo" pageId="25" pageNumber="26">Espírito Santo</collectingRegion>
, 
<collectingRegion box="[1270,1432,469,495]" country="Brazil" name="Rio de Janeiro" pageId="25" pageNumber="26">Rio de Janeiro</collectingRegion>
, 
<collectingRegion box="[151,266,505,532]" country="Brazil" name="Sao Paulo" pageId="25" pageNumber="26">São Paulo</collectingRegion>
) (Verril 1922; 
<bibRefCitation author="Schmitt, W. L." box="[429,582,505,532]" pageId="25" pageNumber="26" pagination="125 - 227" refId="ref77660" refString="Schmitt, W. L. (1935) Crustacea Macrura and Anomura of Porto Rico and the Virgin Islands. New York Academy of Sciences, 15 (2), 125 - 227." type="journal article" year="1935">Schmitt 1935</bibRefCitation>
; 
<bibRefCitation author="Rouse, W." box="[593,729,505,532]" pageId="25" pageNumber="26" pagination="127 - 152" refId="ref76647" refString="Rouse, W. (1970) Littoral Crustacea from Southwest Florida. Quarterly Journal of the Florida Academy of Sciences, 32 (2), 127 - 152." type="journal article" year="1970">Rouse 1970</bibRefCitation>
; 
<bibRefCitation author="Abele, L. G. &amp; Kim, W." box="[739,956,505,531]" pageId="25" pageNumber="26" pagination="1 - 760" refId="ref51541" refString="Abele, L. G. &amp; Kim, W. (1986) An illustrated guide to the marine decapod crustaceans of Florida. Florida Department of Environmental Regulation, Technical Series, 8 (1 - 2), 1 - 760." type="journal article" year="1986">Abele &amp; Kim 1986</bibRefCitation>
; 
<bibRefCitation author="Coelho, P. A. &amp; Ramos-Porto, M." box="[967,1297,505,532]" pageId="25" pageNumber="26" pagination="113 - 127" refId="ref58590" refString="Coelho, P. A. &amp; Ramos-Porto, M. (1994 / 1995) Distribuicao ecologica dos crustaceos decapodos marinhos do nordeste do Brasil. Trabalhos do Instituto Oceanografico da Universidade Federal de Pernambuco, 23, 113 - 127. https: // doi. org / 10.5914 / tropocean. v 23 i 1.2682" type="journal article" year="1994">Coelho &amp; Ramos-Porto 1994</bibRefCitation>
