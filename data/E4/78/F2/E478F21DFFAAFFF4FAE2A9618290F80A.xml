, 
<collectingCountry box="[796,906,1751,1777]" name="Suriname" pageId="30" pageNumber="31">Suriname</collectingCountry>
, and 
<collectingCountry box="[962,1030,1751,1777]" name="Brazil" pageId="30" pageNumber="31">Brazil</collectingCountry>
