, Azores, Canary Islands, 
<collectingCountry box="[151,278,1858,1884]" name="Cape Verde" pageId="21" pageNumber="22">Cape Verde</collectingCountry>
Islands, Madeira Islands, Mediterranean Sea, 
<collectingCountry box="[786,868,1858,1884]" name="Greece" pageId="21" pageNumber="22">Greece</collectingCountry>
, 
<collectingCountry box="[877,954,1858,1884]" name="Democratic Republic of the Congo" pageId="21" pageNumber="22">Congo</collectingCountry>
, 
<collectingCountry box="[962,1047,1858,1884]" name="Angola" pageId="21" pageNumber="22">Angola</collectingCountry>
, and 
<collectingCountry box="[1102,1243,1858,1884]" name="South Africa" pageId="21" pageNumber="22">South Africa</collectingCountry>
