<collectingRegion box="[867,989,542,568]" country="Brazil" name="Sao Paulo" pageId="33" pageNumber="34">São Paulo</collectingRegion>
, Santa Catarina, 
<collectingRegion box="[1194,1424,542,569]" country="Brazil" name="Rio Grande do Sul" pageId="33" pageNumber="34">Rio Grande do Sul</collectingRegion>
), 
<collectingCountry box="[151,250,578,604]" name="Uruguay" pageId="33" pageNumber="34">Uruguay</collectingCountry>
