<geoCoordinate box="[670,760,1834,1861]" degrees="24" direction="south" minutes="19" orientation="latitude" pageId="24" pageNumber="25" precision="925" value="-24.316668">24°19′S</geoCoordinate>
<geoCoordinate box="[771,870,1834,1861]" degrees="45" direction="west" minutes="09" orientation="longitude" pageId="24" pageNumber="25" precision="925" value="-45.15">45°09′W</geoCoordinate>
