<quantity box="[1199,1270,1820,1841]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="43" pageNumber="480" unit="m" value="200.0">
<elevation box="[1199,1270,1820,1841]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="43" pageNumber="480" unit="m" value="200.0">200 m</elevation>
