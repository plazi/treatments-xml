<location LSID="urn:lsid:plazi:treatment:E45287EFFFDC4333FC10F92287B45268:69246022FFDE4333FCF9F8CE861F5256" box="[875,1034,1850,1872]" country="Taiwan" county="Gekou area" municipality="The" name="Taroko valley" pageId="43" pageNumber="480" stateProvince="Hualien">Taroko valley</location>
, 
<collectingRegion box="[1050,1237,1850,1872]" country="Taiwan" name="Hualien" pageId="43" pageNumber="480">Hualien County</collectingRegion>
