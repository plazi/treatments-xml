<paragraph blockId="46.[151,1436,151,824]" pageId="46" pageNumber="47">
<materialsCitation ID-GBIF-Occurrence="1500194705" box="[199,833,583,608]" country="Mexico" location="MEXICO" pageId="46" pageNumber="47" specimenCount="1" typeStatus="paratype">
