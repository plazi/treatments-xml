<subSubSection box="[199,1128,1422,1447]" pageId="54" pageNumber="55" type="etymology">
<paragraph blockId="54.[151,1437,1350,2023]" box="[199,1128,1422,1447]" pageId="54" pageNumber="55">
<emphasis bold="true" box="[199,326,1422,1447]" pageId="54" pageNumber="55">Etymology</emphasis>
