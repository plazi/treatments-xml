<subSubSection box="[199,1258,1266,1291]" pageId="67" pageNumber="68" type="etymology">
<paragraph blockId="67.[151,1436,1194,1399]" box="[199,1258,1266,1291]" pageId="67" pageNumber="68">
<emphasis bold="true" box="[199,326,1266,1291]" pageId="67" pageNumber="68">Etymology</emphasis>
