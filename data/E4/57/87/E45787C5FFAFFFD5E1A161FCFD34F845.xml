<subSubSection box="[199,1258,1076,1101]" pageId="46" pageNumber="47" type="etymology">
<paragraph blockId="46.[151,1437,1004,1965]" box="[199,1258,1076,1101]" pageId="46" pageNumber="47">
<emphasis bold="true" box="[199,326,1076,1101]" pageId="46" pageNumber="47">Etymology</emphasis>
