<heading id="D0F781C2FFFFEF2A6245FC77FCC7FC2D" box="[367,813,946,968]" centered="true" fontSize="9" level="2" pageId="16" pageNumber="399" reason="2">
<taxonomicName id="4C004D2DFFFFEF2A6245FC77FCC7FC2D" authority="(Sharp)" baseAuthorityName="Sharp" baseAuthorityYear="1903" box="[367,813,946,968]" class="Insecta" family="Carabidae" genus="Blackburnia" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="399" phylum="Arthropoda" rank="species" species="depressa" subGenus="Metromenus">
<emphasis id="B974EABCFFFFEF2A6245FC77FCC7FC2D" bold="true" box="[367,813,946,968]" pageId="16" pageNumber="399">
<emphasis id="B974EABCFFFFEF2A6245FC77FE02FC2D" bold="true" box="[367,488,946,968]" italics="true" pageId="16" pageNumber="399">Blackburnia</emphasis>
(
<emphasis id="B974EABCFFFFEF2A62D2FC77FD9FFC2D" bold="true" box="[504,629,946,968]" italics="true" pageId="16" pageNumber="399">Metromenus</emphasis>
) 
<emphasis id="B974EABCFFFFEF2A61AFFC77FD33FC2D" bold="true" box="[645,729,946,968]" italics="true" pageId="16" pageNumber="399">depressa</emphasis>
(Sharp)
</emphasis>
</taxonomicName>
