<location id="8EF744910067FFD664BCFC44FA977B14" LSID="urn:lsid:plazi:treatment:0381A35C0065FFD66099FAC3FB857962:8EF744910067FFD664BCFC44FA977B14" box="[1202,1286,1002,1028]" country="Madagascar" latitude="-25.051855" longLatPrecision="1" longitude="46.8959" name="Petriky" pageId="14" pageNumber="53" stateProvince="Toliara">Petriky</location>
