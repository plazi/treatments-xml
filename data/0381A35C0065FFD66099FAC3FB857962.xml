, 
<geoCoordinate id="EE1C748D0067FFD66139FBBFFE7B7B3B" box="[311,490,1041,1067]" degrees="46" direction="east" minutes="53" orientation="longitude" pageId="14" pageNumber="53" precision="1" seconds="45.24" value="46.8959">46°53’45.24”E</geoCoordinate>
, 
<date id="FF96348A0067FFD661F9FBBFFD587B3B" box="[503,713,1041,1067]" pageId="14" pageNumber="53" value="2007-02-21">
