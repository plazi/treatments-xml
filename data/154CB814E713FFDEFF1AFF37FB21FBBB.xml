<subSubSection id="D5FF5A89E713FFDEFF1AFF37FEDFFE8B" pageId="4" pageNumber="229" type="nomenclature">
<paragraph id="9D5A0902E713FFDEFF1AFF37FC98FEB7" blockId="4.[136,836,244,271]" box="[136,836,244,271]" pageId="4" pageNumber="229">
<heading id="C612BE6EE713FFDEFF1AFF37FC98FEB7" box="[136,836,244,271]" fontSize="11" level="2" pageId="4" pageNumber="229" reason="3">
<taxonomicName id="5AE57281E713FFDEFF1AFF37FD3AFEB7" authority="Gusev, Shkurina &amp; Kulikovskiy" authorityName="Gusev, Shkurina &amp; Kulikovskiy" authorityYear="2021" box="[136,742,244,271]" class="Chrysophyceae" family="Mallomonadaceae" genus="Mallomonas" kingdom="Chromista" order="Synurales" pageId="4" pageNumber="229" phylum="Ochrophyta" rank="species" species="loricata" status="sp. nov.">
