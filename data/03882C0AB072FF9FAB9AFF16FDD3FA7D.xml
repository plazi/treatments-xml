<taxonomicName id="4C21E69FB072FF9FAEC7FBEDFABCFBDE" authorityName="Wishart" authorityYear="2006" box="[1228,1310,1122,1145]" class="Arachnida" family="Idiopidae" genus="Misgolas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="beni" status="n.sp.">
