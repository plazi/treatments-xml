<emphasis id="7ABE156EFFBCFFEFFF0D13745E749721" bold="true" box="[151,341,583,609]" pageId="18" pageNumber="19">
<taxonomicName id="8FCAB2FFFFBCFFEFFF0D13745FDD9721" box="[151,252,583,609]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="82" phylum="Arthropoda" rank="species" species="curvosa">
