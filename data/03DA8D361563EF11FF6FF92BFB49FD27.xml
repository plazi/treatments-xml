<collectingCountry id="F3647CB01563EF12FEE7F82FFED0F81F" box="[287,363,1937,1961]" name="Kenya" pageId="5" pageNumber="27">Kenya</collectingCountry>
, 
<collectingCountry id="F3647CB01563EF12FE8EF82EFE66F81E" box="[374,477,1936,1960]" name="Tanzania" pageId="5" pageNumber="27">Tanzania</collectingCountry>
