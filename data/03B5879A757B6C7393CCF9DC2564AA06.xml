<taxonomicName id="4C1C4D0F75796C6F914DF89F262DAF12" authority="(SMNHI 1540)" baseAuthorityName="SMNHI" baseAuthorityYear="1540" box="[271,621,1816,1835]" class="Gastropoda" family="Cadlinidae" genus="Aldisa" kingdom="Animalia" order="Nudibranchia" pageId="46" pageNumber="581" phylum="Mollusca" rank="species" species="zetlandica">
