<taxonomicName id="4C5176BBFF8AFFC2CFEAF8B0B3549B18" box="[1242,1328,1878,1904]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="genus">
<emphasis id="B925D12AFF8AFFC2CFEAF8B0B3549B18" box="[1242,1328,1878,1904]" italics="true" pageId="19" pageNumber="20">Feihyla</emphasis>
</taxonomicName>
