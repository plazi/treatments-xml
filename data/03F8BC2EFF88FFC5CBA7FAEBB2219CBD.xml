<taxonomicName id="4C5176BBFF8AFFC2C84BF8D4B5959B24" baseAuthorityName="Frost" baseAuthorityYear="2020" box="[891,1009,1842,1868]" genus="Chirixalus" kingdom="Animalia" pageId="19" pageNumber="20" rank="genus">
