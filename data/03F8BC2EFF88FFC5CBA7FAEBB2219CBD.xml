, and 
<taxonomicName id="4C5176BBFF8AFFC2CF67F89DB2A09BFC" authorityName="Biju &amp; Garg &amp; Gokulakrishnan &amp; Chandrakasan &amp; Thammachoti &amp; Ren &amp; Gopika &amp; Bisht &amp; Hamidy &amp; Shouche" authorityYear="2020" baseAuthorityName="Boulenger" baseAuthorityYear="1897" box="[1111,1220,1915,1940]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="vittiger" status="comb. nov.">
