, 
<taxonomicName id="4C5176BBFF8AFFC2CAECF89DB4519BFC" authorityName="Li et al." authorityYear="2013" baseAuthorityName="Dring" baseAuthorityYear="1983" box="[476,565,1914,1940]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="kajau">
<emphasis id="B925D12AFF8AFFC2CAECF89DB4519BFC" box="[476,565,1914,1940]" italics="true" pageId="19" pageNumber="20">F. kajau</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C5176BBFF8AFFC2C974F89DB4BB9BFC" authorityName="Frost et al." authorityYear="2006" baseAuthorityName="Smith" baseAuthorityYear="1924" box="[580,735,1914,1940]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="palpebralis">
<emphasis id="B925D12AFF8AFFC2C974F89DB4BB9BFC" box="[580,735,1914,1940]" italics="true" pageId="19" pageNumber="20">F. palpebralis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C5176BBFF8AFFC2C9DDF89DB5F69BFC" authorityName="Biju &amp; Garg &amp; Gokulakrishnan &amp; Chandrakasan &amp; Thammachoti &amp; Ren &amp; Gopika &amp; Bisht &amp; Hamidy &amp; Shouche" authorityYear="2020" baseAuthorityName="Grismer, Thy, Chav, and Holden" baseAuthorityYear="2007" box="[749,914,1914,1940]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="samkosensis" status="comb. nov.">
