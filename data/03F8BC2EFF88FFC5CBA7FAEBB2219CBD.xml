<taxonomicName id="4C5176BBFF8AFFC2CBF8F89DB7409BFC" authorityName="Fei, Ye, and Jiang" authorityYear="2010" box="[200,292,1914,1940]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="fuhua">
<emphasis id="B925D12AFF8AFFC2CBF8F89DB7409BFC" box="[200,292,1914,1940]" italics="true" pageId="19" pageNumber="20">F. fuhua</emphasis>
