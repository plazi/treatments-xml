<taxonomicName id="4C5176BBFF8AFFC2CE20F8D4B3029B24" box="[1296,1382,1842,1868]" class="Amphibia" family="Rhacophoridae" genus="Feihyla" kingdom="Animalia" order="Anura" pageId="19" pageNumber="20" phylum="Chordata" rank="genus">
<emphasis id="B925D12AFF8AFFC2CE20F8D4B3029B24" box="[1296,1382,1842,1868]" italics="true" pageId="19" pageNumber="20">Feihyla</emphasis>
</taxonomicName>
