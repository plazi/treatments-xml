<quantity id="4CF79B33FFA2FF8BC1A7FB73FB89FB52" box="[990,1055,1194,1214]" metricMagnitude="3" metricUnit="m" metricValue="1.025" pageId="77" pageNumber="232" unit="m" value="1025.0">1025m</quantity>
