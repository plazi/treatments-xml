<emphasis id="8EAAFD3DDA23FFDEBEE4FA8BFF53FACD" box="[201,240,1304,1328]" italics="true" pageId="7" pageNumber="8">
ps 
<number id="0A163308DA23FFDEBECEFA8BFF53FACD" box="[227,240,1304,1328]" pageId="7" pageNumber="8" value="3.0">3</number>
