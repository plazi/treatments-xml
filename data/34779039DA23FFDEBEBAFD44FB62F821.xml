<emphasis id="8EAAFD3DDA23FFDEBB0BFB43FAF2FB15" box="[1318,1361,1232,1256]" italics="true" pageId="7" pageNumber="8">
ad 
<number id="0A163308DA23FFDEBB6BFB43FAF2FB15" box="[1350,1361,1232,1256]" pageId="7" pageNumber="8" value="1.0">1</number>
</emphasis>
