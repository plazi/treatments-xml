<quantity id="7B268CCADA23FFDEBDFDFA84FBABFAD2" box="[976,1032,1303,1327]" metricMagnitude="-1" metricUnit="m" metricValue="3.048" pageId="7" pageNumber="8" unit="in" value="12.0">12 in</quantity>
