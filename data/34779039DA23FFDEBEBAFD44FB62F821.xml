<emphasis id="8EAAFD3DDA23FFDEBB76FC6BFA3FFBED" box="[1371,1436,1016,1040]" italics="true" pageId="7" pageNumber="8">
h 
<number id="0A163308DA23FFDEBB44FC6BFAD4FBED" box="[1385,1399,1016,1040]" pageId="7" pageNumber="8" value="3.0">3</number>
-h 
<number id="0A163308DA23FFDEBBA3FC6BFA3FFBED" box="[1422,1436,1016,1040]" pageId="7" pageNumber="8" value="3.0">3</number>
</emphasis>
<number id="0A163308DA23FFDEBEBAFB8FFF67FBC9" box="[151,196,1052,1076]" pageId="7" pageNumber="8" value="100.0">100</number>
