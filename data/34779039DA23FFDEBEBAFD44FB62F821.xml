<typeStatus id="63659F8DDA23FFDEBB47FA84FA3FFAD2" box="[1386,1436,1303,1327]" pageId="7" pageNumber="8">type</typeStatus>
<collectionCode id="DACFB9EADA23FFDEBEBAFAAFFF0DFAA9" box="[151,174,1340,1364]" pageId="7" pageNumber="8">A</collectionCode>
