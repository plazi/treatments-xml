<quantity id="4CB878D2FFE55F4DBDF5FA19FD5CCF8F" box="[671,745,1486,1507]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="3" pageNumber="32" unit="m" value="2300.0">2300 m</quantity>
