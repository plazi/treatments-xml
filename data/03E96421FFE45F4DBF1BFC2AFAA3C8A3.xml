<quantity id="4CB878D2FFE55F4DBE2DF9B5FE24CC1B" box="[327,401,1634,1655]" metricMagnitude="3" metricUnit="m" metricValue="2.02" pageId="3" pageNumber="32" unit="m" value="2020.0">2020 m</quantity>
, 
