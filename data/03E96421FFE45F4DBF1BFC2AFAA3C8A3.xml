<date id="FFFEF3F7FFE55F4DBA24FEBAFA0BCBEE" box="[1358,1470,365,386]" pageId="3" pageNumber="32" value="1941-07-05">5.VII.1941</date>
, 
<emphasis id="B9340925FFE55F4DBC52FE5CFC17CBCC" box="[824,930,395,416]" italics="true" pageId="3" pageNumber="32">G. Malcuit</emphasis>
(G [G00000880], original material of 
<taxonomicName id="4C40AEB4FFE55F4DBA71FE5CFA74CBCC" authorityName="van Soest" authorityYear="2013" box="[1307,1473,395,416]" class="Magnoliopsida" family="Asteraceae" genus="Taraxacum" kingdom="Plantae" order="Asterales" pageId="3" pageNumber="32" phylum="Tracheophyta" rank="species" species="cucullatiforme">
<emphasis id="B9340925FFE55F4DBA71FE5CFA74CBCC" box="[1307,1473,395,416]" italics="true" pageId="3" pageNumber="32">T. cucullatiforme</emphasis>
</taxonomicName>
determined by van 
<taxonomicName id="4C40AEB4FFE55F4DBC99FE7FFB9DCBD1" authority="Štepánek &amp; Kirschner, 2013" authorityName="Štepánek &amp; Kirschner" authorityYear="2013" box="[1011,1064,424,445]" pageId="3" pageNumber="32" rank="species" species="Soest" status="nov. spec.">Soest</taxonomicName>
as 
<taxonomicName id="4C40AEB4FFE55F4DBB23FE7FFAE1CBD1" authority="van Soest" authorityName="van Soest" authorityYear="2013" box="[1097,1364,424,445]" class="Magnoliopsida" family="Asteraceae" genus="Taraxacum" kingdom="Plantae" order="Asterales" pageId="3" pageNumber="32" phylum="Tracheophyta" rank="species" species="cucullatiforme" status="nov. spec.">
<emphasis id="B9340925FFE55F4DBB23FE7FFB5ACBD1" box="[1097,1263,424,445]" italics="true" pageId="3" pageNumber="32">T. cucullatiforme</emphasis>
van Soest
</taxonomicName>
<taxonomicNameLabel id="A207B45EFFE55F4DBA30FE7FFA0BCBD1" box="[1370,1470,424,445]" pageId="3" pageNumber="32" rank="species">nov. spec.</taxonomicNameLabel>
, n° det. 23270); env. de Bocognano: Monte Niello, couloirs herbeux à l’ubac, silice, 
<quantity id="4CB878D2FFE55F4DBCB8FE34FBABCB94" box="[978,1054,483,504]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="32" unit="m" value="2000.0">2000 m</quantity>
, 
