<taxonomicName id="4C74CCA0D623FFE6FA9EFDA8FA3CFDD4" authorityName="Delle Chiaje" authorityYear="1841" box="[1286,1436,573,595]" class="Polychaeta" family="Onuphidae" genus="Diopatra" kingdom="Animalia" order="Eunicida" pageId="28" pageNumber="533" phylum="Annelida" rank="species" species="neapolitana">
