<emphasis id="B902612ECC2DFFC1FB32002AFAC8C123" box="[1212,1364,1343,1365]" italics="true" pageId="6" pageNumber="297">A. longispeculi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A231DC55CC2DFFC1FAD50055FA09C122" box="[1371,1429,1343,1364]" pageId="6" pageNumber="297" rank="species">
<emphasis id="B902612ECC2DFFC1FAD50055FA09C122" bold="true" box="[1371,1429,1343,1364]" pageId="6" pageNumber="297">sp. n.</emphasis>
</taxonomicNameLabel>
; (D, H, N) 
<taxonomicName id="4C76C6BFCC2DFFC1FE8E0035FE3FC102" box="[256,419,1374,1396]" class="Insecta" family="Tettigoniidae" genus="Anatlanticus" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="297" phylum="Arthropoda" rank="species" species="liaoningensis" status="sp. n.">
<emphasis id="B902612ECC2DFFC1FE8E0035FE3FC102" box="[256,419,1374,1396]" italics="true" pageId="6" pageNumber="297">A. liaoningensis</emphasis>
</taxonomicName>
<emphasis id="B902612ECC2DFFC1FE250034FE78C105" bold="true" box="[427,484,1374,1395]" pageId="6" pageNumber="297">
