<emphasis id="B902612ECC2DFFC1FD650055FCB9C122" bold="true" box="[747,805,1343,1364]" pageId="6" pageNumber="297">sp. n.</emphasis>
</taxonomicNameLabel>
; (B, F, J, L) 
<taxonomicName id="4C76C6BFCC2DFFC1FC27002AFBD8C123" box="[937,1092,1343,1365]" class="Insecta" family="Tettigoniidae" genus="Mongolodectes" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="297" phylum="Arthropoda" rank="species" species="alashanicus">
<emphasis id="B902612ECC2DFFC1FC27002AFBD8C123" box="[937,1092,1343,1365]" italics="true" pageId="6" pageNumber="297">M. alashanicus</emphasis>
</taxonomicName>
; (C, G, M) 
