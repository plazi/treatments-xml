lebt auf 
<collectingRegion id="49F1F8500A1DE70EFC24FDC7FB80B557" box="[994,1081,611,633]" country="Indonesia" name="Sumatera Utara" pageId="8" pageNumber="1499">Sumatra</collectingRegion>
, 
<taxonomicName id="4C354D310A1DE70EFF74FD25FE92B5BB" authorityName="ULMER" authorityYear="1905" box="[178,299,640,662]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="8" pageNumber="1499" phylum="Arthropoda" rank="species" species="annulata">
<emphasis id="B941EAA00A1DE70EFF74FD25FE92B5BB" box="[178,299,640,662]" italics="true" pageId="8" pageNumber="1499">H. annulata</emphasis>
</taxonomicName>
auf 
<collectingRegion id="49F1F8500A1DE70EFE9EFD24FE32B5BB" box="[344,395,640,661]" country="Indonesia" name="Jawa Timur" pageId="8" pageNumber="1499">Jawa</collectingRegion>
und 
<collectingRegion id="49F1F8500A1DE70EFE7BFD24FE53B5B8" box="[445,490,640,662]" country="Indonesia" name="Bali" pageId="8" pageNumber="1499">Bali</collectingRegion>
, 
<taxonomicName id="4C354D310A1DE70EFE32FD25FD39B5BB" authorityName="Malicky &amp; Ivanov &amp; Melnitsky" authorityYear="2011" box="[500,640,640,662]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="8" pageNumber="1499" phylum="Arthropoda" rank="species" species="sasakorum">
<emphasis id="B941EAA00A1DE70EFE32FD25FD39B5BB" box="[500,640,640,662]" italics="true" pageId="8" pageNumber="1499">H. sasakorum</emphasis>
</taxonomicName>
