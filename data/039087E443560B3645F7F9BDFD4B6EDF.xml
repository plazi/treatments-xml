: Sikkim, Lachen, 
<quantity id="4CC19B1743540B314613F877FC2869FE" box="[883,971,1982,2006]" metricMagnitude="3" metricUnit="m" metricValue="3.2" pageId="3" pageNumber="395" unit="m" value="3200.0">3200 m</quantity>
, 
<date id="FF87103243540B3146B8F877FBAD69FE" box="[984,1102,1982,2006]" pageId="3" pageNumber="395" value="2011-05-24">
