<specimenCount id="169BFD4F3246FFC0DA91FCDBA92ECD68" box="[443,525,813,840]" pageId="33" pageNumber="66" type="generic">
<quantity id="C7659B233246FFC0DA91FCDBA92BCD68" box="[443,520,813,840]" metricMagnitude="2" metricUnit="m" metricValue="5.5" pageId="33" pageNumber="66" unit="m" value="550.0">550 m</quantity>
.
