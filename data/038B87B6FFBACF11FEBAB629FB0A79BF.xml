<taxonomicName id="4C224D23FFBACF11FDDFB425FCA27879" authority=", Hypnea" authorityName="Hypnea" box="[593,794,1536,1555]" class="Ulvophyceae" family="Ulvaceae" genus="Ulva" kingdom="Plantae" order="Ulvales" pageId="6" pageNumber="67" phylum="Chlorophyta" rank="species" species="lactuca">Ulva lactuca, Hypnea</taxonomicName>
cornuta
</emphasis>
and 
<taxonomicName id="4C224D23FFBACF11FEACB43DFD947841" authority=", Choisy" authorityName="Choisy" box="[290,556,1560,1579]" class="Phaeophyceae" family="Dictyotaceae" genus="Dictyota" kingdom="Chromista" order="Dictyotales" pageId="6" pageNumber="67" phylum="Ochrophyta" rank="species" species="divaricata">
<emphasis id="B956EAB2FFBACF11FEACB43DFE647841" box="[290,476,1560,1579]" italics="true" pageId="6" pageNumber="67">Dictyota divaricata</emphasis>
, Choisy
</taxonomicName>
(
<geoCoordinate id="EE165067FFBACF11FDB3B43DFD3E7841" box="[573,646,1560,1579]" degrees="20" direction="south" minutes="01" orientation="latitude" pageId="6" pageNumber="67" precision="925" value="-20.016666">20°01'S</geoCoordinate>
<geoCoordinate id="EE165067FFBACF11FD1EB43DFD577841" box="[656,751,1560,1579]" degrees="57" direction="east" minutes="32.7" orientation="longitude" pageId="6" pageNumber="67" precision="92" value="57.545">57°32.7'E</geoCoordinate>
), 
