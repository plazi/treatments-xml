<tr id="35345004B18B0075F1FDFF6D8DCDCD70" box="[165,1415,254,277]" gridrow="1" pageId="152" pageNumber="153">
<th id="76E53978B18B0075F1FDFF6D89ADCD70" box="[165,487,254,277]" gridcol="0" gridrow="1" pageId="152" pageNumber="153">Basal streak</th>
