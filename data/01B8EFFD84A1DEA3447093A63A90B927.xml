<taxonomicName id="6A0EB42D4422668BF6964216642DBC77" LSID="http://zoobank.org/BD31E11A-6C3A-4D7D-9F9B-88677E9735BB" authority="van Nieukerken &amp; Snyers" class="Insecta" family="Nepticulidae" genus="Stigmella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stigmella molinensis" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Ascomycota" rank="species" species="molinensis">Stigmella molinensis van Nieukerken &amp; Snyers</taxonomicName>
<taxonomicNameLabel id="37C9433022FEBC6D40974145A7CD4326" pageId="17" pageNumber="18">sp. n.</taxonomicNameLabel>
