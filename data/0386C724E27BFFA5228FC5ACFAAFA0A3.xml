<taxonomicName id="4C2F0DB1E27BFFA52335C719FDC1A087" box="[301,549,872,896]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="164" phylum="Arthropoda" rank="species" species="bidentata">
