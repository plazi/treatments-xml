<heading id="9A4B2D2CFFA6BA66FEEBFC2DFDA6FC04" bold="true" box="[310,555,925,949]" centered="true" fontSize="10" level="2" pageId="71" pageNumber="962" reason="2">
<emphasis id="F3C84652FFA6BA66FEEBFC2DFDA6FC04" bold="true" box="[310,555,925,949]" pageId="71" pageNumber="962">
<taxonomicName id="06BCE1C3FFA6BA66FEEBFC2DFE29FC04" authority="Schubart &amp; Ng, 2020" authorityName="Schubart &amp; Ng" authorityYear="2020" box="[310,420,926,949]" class="Malacostraca" family="Sesarmidae" genus="Manarma" kingdom="Animalia" order="Decapoda" pageId="71" pageNumber="962" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="F3C84652FFA6BA66FEEBFC2DFE29FC04" bold="true" box="[310,420,926,949]" italics="true" pageId="71" pageNumber="962">Manarma</emphasis>
</taxonomicName>
