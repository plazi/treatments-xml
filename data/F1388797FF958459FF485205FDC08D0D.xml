<taxonomicName id="BE914D02FF958459FF0F548CFE1A8C01" baseAuthorityName="Burmeister" baseAuthorityYear="1838" box="[208,394,1519,1545]" class="Insecta" family="Philopteridae" genus="Degeeriella" kingdom="Animalia" order="Psocodea" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="rufa">
