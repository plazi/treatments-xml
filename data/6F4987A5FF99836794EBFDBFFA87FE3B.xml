<taxonomicName id="20E04D30FF9A83699524FA5DFDF0F9F6" box="[344,552,1516,1538]" class="Insecta" family="Staphylinidae" genus="Terataki" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="257" phylum="Arthropoda" rank="species" species="badiipennis">
<emphasis id="D594EAA1FF9A83699524FA5DFDF0F9F6" box="[344,552,1516,1538]" italics="true" pageId="6" pageNumber="257">Terataki badiipennis</emphasis>
</taxonomicName>
