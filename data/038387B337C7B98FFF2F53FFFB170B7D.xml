<taxonomicName id="4C2A4D2637C6B98FFB8157D8FB1808EB" box="[1054,1227,410,433]" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" kingdom="Plantae" order="Ericales" pageId="139" pageNumber="140" phylum="Tracheophyta" rank="genus">
