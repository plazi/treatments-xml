<quantity id="4CFE9B72EA322933CFEABA561FD3D808" box="[915,1016,1838,1862]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="15" pageNumber="68" unit="mm" value="1.05">1.05 mm</quantity>
