<subSubSection id="C3776551FC2A443BFF660DBDFB612D88" pageId="5" pageNumber="435" type="etymology">
<paragraph id="8BD236DAFC2A443BFF660DBDFB612D88" blockId="5.[151,1436,151,501]" pageId="5" pageNumber="435">
<emphasis id="B919EAC8FC2A443BFF660DBDFE862DAD" bold="true" box="[199,333,439,464]" pageId="5" pageNumber="435">Etymology.</emphasis>
