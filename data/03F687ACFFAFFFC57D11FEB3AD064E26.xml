<paragraph id="8BE036BAFFA9FFC47D21FEB3AD234901" blockId="20.[264,1324,284,1899]" box="[312,1097,284,310]" pageId="20" pageNumber="21">
<taxonomicName id="4C5F4D39FFA9FFC47D21FEB3A8E94902" authorityName="Rambur" authorityYear="1842" box="[312,387,284,309]" class="Insecta" family="Nemopteridae" genus="Halter" kingdom="Animalia" order="Neuroptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
