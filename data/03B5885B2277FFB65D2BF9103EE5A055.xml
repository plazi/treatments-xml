<caption id="DF6369C52276FFB65E8BFE6A3D71A2D1" ID-DOI="http://doi.org/10.5281/zenodo.5385098" ID-Zenodo-Dep="5385098" httpUri="https://zenodo.org/record/5385098/files/figure.png" pageId="11" pageNumber="12" startId="11.[146,173,462,479]" targetBox="[146,770,147,427]" targetPageId="11">
