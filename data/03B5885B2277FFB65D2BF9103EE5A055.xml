<taxonomicName id="4C1C42CE2276FFB65ED3FE6A3EEEA2F6" authority="Gollner-Scheiding" authorityName="Gollner-Scheiding" box="[202,497,462,479]" class="Insecta" family="Rhopalidae" genus="Niesthrea" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="cassini">
