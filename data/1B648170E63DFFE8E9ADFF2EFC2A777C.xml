<taxonomicName id="54CD4BE5E63DFFE8EA9FFDE5FB707700" box="[933,1178,534,557]" class="Arachnida" family="Trombiculidae" genus="Lacertacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="16" phylum="Arthropoda" rank="species" species="graecus" status="sp. nov.">
<emphasis id="A1B9EC74E63DFFE8EA9FFDE5FB707700" box="[933,1178,534,557]" italics="true" pageId="1" pageNumber="16">Lacertacarus graecus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="BA8A510FE63DFFE8ED9AFDE6FB137700" box="[1184,1273,533,557]" pageId="1" pageNumber="16" rank="species">
<emphasis id="A1B9EC74E63DFFE8ED9AFDE6FB137700" bold="true" box="[1184,1273,533,557]" pageId="1" pageNumber="16">sp. nov.</emphasis>
</taxonomicNameLabel>
