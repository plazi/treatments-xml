<subSubSection id="63BB67EE6364FFCDFF5A4204FB75FE99" box="[198,1152,359,383]" pageId="3" pageNumber="68" type="etymology">
<paragraph id="2B1E34656364FFCDFF5A4204FB75FE99" blockId="3.[151,1436,151,383]" box="[198,1152,359,383]" pageId="3" pageNumber="68">
<emphasis id="19D5E8776364FFCDFF5A4204FEBFFE99" bold="true" box="[198,330,359,383]" pageId="3" pageNumber="68">Etymology:</emphasis>
