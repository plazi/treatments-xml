<taxonomicName id="EDAC4D58FFBCFFA99242FB012D13D1FE" authority="Lotz, 2002" authorityName="Lotz" authorityYear="2002" box="[823,1067,1229,1254]" class="Arachnida" family="Miturgidae" genus="Cheiramiona" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="74" phylum="Arthropoda" rank="species" species="lajuma">
