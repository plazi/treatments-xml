<specimenCode id="9B089EADA805FFDAFF15FE9CFE8E0844" box="[128,307,329,350]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="29" pageNumber="29">AMNH 514436</specimenCode>
