<taxonomicName id="A1BBFFBE6EA4CC06329C5EA1AC22690A" class="Gastropoda" family="Ficidae" genus="Ficus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ficus altissima" order="Littorinimorpha" pageId="12" pageNumber="131" phylum="Mollusca" rank="species" species="altissima">Ficus altissima</taxonomicName>
