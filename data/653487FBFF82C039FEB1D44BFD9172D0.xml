<taxonomicName id="2A9D4D6EFF84C039FE45D0B2FD5072B9" box="[508,711,407,432]" class="Magnoliopsida" family="Fabaceae" genus="Genista" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="58" phylum="Tracheophyta" rank="species" species="tinctoria">
