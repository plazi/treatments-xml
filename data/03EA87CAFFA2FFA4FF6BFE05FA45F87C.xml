<collectingCountry id="F354764CFFA2FFA4FC97FE05FBC8FE01" box="[875,1041,431,457]" name="United States of America" pageId="5" pageNumber="6">United States</collectingCountry>
</emphasis>
</heading>
</paragraph>
<paragraph id="8BFC36DCFFA2FFA4FF6BFE50FACBFDDC" blockId="5.[151,1298,506,532]" box="[151,1298,506,532]" pageId="5" pageNumber="6">
<taxonomicName id="4C434D5FFFA2FFA4FF6BFE50FE10FDDB" authorityName="Byars." authorityYear="1951" box="[151,457,506,531]" class="Insecta" family="Formicidae" genus="Trachymyrmex" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="nogalensis">
