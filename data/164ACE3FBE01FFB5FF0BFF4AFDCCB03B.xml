<taxonomicName id="59E304AABE01FFB5FEB7FF03FD98B5BB" authority="Lilljeborg, 1865" authorityName="Lilljeborg" authorityYear="1865" box="[299,607,222,246]" class="Malacostraca" family="Calliopiidae" genus="Calliopius" kingdom="Animalia" order="Amphipoda" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis id="AC97A33BBE01FFB5FEB7FF03FE58B5BB" box="[299,415,222,246]" italics="true" pageId="38" pageNumber="39">Calliopius</emphasis>
Lilljeborg, 
<number id="282B6D0EBE01FFB5FDB8FF03FD98B5BB" box="[548,607,222,246]" pageId="38" pageNumber="39" value="1865.0">1865</number>
</taxonomicName>
