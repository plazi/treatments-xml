<taxonomicName id="4C011DDAFF90FFFCC864F8D045DEF8E0" authorityName="Stephens" authorityYear="1829" box="[305,426,1858,1883]" class="Insecta" family="Hepialidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="family">Hepialidae</taxonomicName>
