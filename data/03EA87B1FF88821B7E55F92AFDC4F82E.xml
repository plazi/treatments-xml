<taxonomicName id="4C434D24FF88821B7D67F8BCFD64F8E9" authority="Lederer" authorityName="Lederer" box="[524,752,1892,1916]" class="Insecta" family="Pyralidae" genus="Pilocrocis" kingdom="Animalia" order="Lepidoptera" pageId="52" pageNumber="367" phylum="Arthropoda" rank="species" species="ramentalis" subFamily="Spilomelinae">
<emphasis id="B937EAB5FF88821B7D67F8BCFD03F8E9" box="[524,663,1892,1916]" italics="true" pageId="52" pageNumber="367">P. ramentalis</emphasis>
Lederer
</taxonomicName>
, the 
<typeStatus id="54F88805FF88821B7FABF85CFF7AF80F" box="[192,238,1925,1949]" pageId="52" pageNumber="367">type</typeStatus>
