<taxonomicName id="4C434D24FF88821B7DECF8FAFD60F8A8" box="[647,756,1827,1850]" class="Insecta" family="Pyralidae" genus="Pilocrocis" kingdom="Animalia" order="Lepidoptera" pageId="52" pageNumber="367" phylum="Arthropoda" rank="genus" subFamily="Spilomelinae">
<emphasis id="B937EAB5FF88821B7DECF8FAFD60F8A8" box="[647,756,1827,1850]" italics="true" pageId="52" pageNumber="367">Pilocrocis</emphasis>
</taxonomicName>
