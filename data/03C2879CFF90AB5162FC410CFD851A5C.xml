<taxonomicName id="4C6B4D09FF90AB51635E436FFE7C1A5C" box="[354,511,1870,1894]" class="Insecta" family="Apidae" genus="Xylocopa" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="suspecta" subGenus="Neoxylocopa">
