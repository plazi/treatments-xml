<paragraph id="8BE34675E42EFF9DFF5AFF4AAEADFEBD" blockId="3.[151,1436,151,320]" pageId="3" pageNumber="489">
<emphasis id="B9289A67E42EFF9DFF5AFF4AAB41FF4D" bold="true" box="[199,314,151,176]" pageId="3" pageNumber="489">Diagnosis</emphasis>
. Length of body 
<quantity id="4CA4EB90E42EFF9DFD98FF45A8EAFF52" box="[517,657,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" metricValueMax="2.3" metricValueMin="2.1" pageId="3" pageNumber="489" unit="mm" value="2.2" valueMax="2.3" valueMin="2.1">2.1–2.3 mm</quantity>
; clypeus without teeth; female with 
<number id="3D945452E42EFF9DFBA9FF45AE2BFF4D" box="[1076,1104,152,176]" pageId="3" pageNumber="489" value="23.0">23</number>
