<collectingCountry id="F30676543E6FFFDFFC90E0456E029E58" box="[808,870,1095,1120]" name="Cuba" pageId="16" pageNumber="191">Cuba</collectingCountry>
; 
<collectingCountry id="F30676543E6FFFDFFCCFE0456EB19E58" box="[887,981,1095,1120]" name="Jamaica" pageId="16" pageNumber="191">Jamaica</collectingCountry>
; 
<collectingCountry id="F30676543E6FFFDFFC5EE04A69179E58" box="[998,1139,1095,1120]" name="Puerto Rico" pageId="16" pageNumber="191">Puerto Rico</collectingCountry>
; 
<collectingCountry id="F30676543E6FFFDFFB3CE045684E9E58" box="[1156,1322,1095,1120]" name="Virgin Islands" pageId="16" pageNumber="191">Virgin Islands</collectingCountry>
; 
<collectingCountry id="F30676543E6FFFDFFA82E04568FC9E58" box="[1338,1432,1095,1120]" name="Antigua and Barbuda" pageId="16" pageNumber="191">Antigua</collectingCountry>
; 
<collectingCountry id="F30676543E6FFFDFFF2FE06E6C609EBD" box="[151,260,1132,1157]" name="Barbados" pageId="16" pageNumber="191">Barbados</collectingCountry>
; 
<collectingCountry id="F30676543E6FFFDFFEB7E06E6CE49EBD" box="[271,384,1132,1157]" name="Colombia" pageId="16" pageNumber="191">Colombia</collectingCountry>
