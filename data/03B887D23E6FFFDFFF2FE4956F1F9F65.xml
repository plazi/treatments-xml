<collectingCountry id="F30676543E6FFFDFFE57E0456F059E58" box="[495,609,1095,1120]" name="United Kingdom" pageId="16" pageNumber="191">Caledonia</collectingCountry>
Bay; 
<collectingCountry id="F30676543E6FFFDFFD16E0456E7C9E58" box="[686,792,1095,1120]" name="Bahamas" pageId="16" pageNumber="191">Bahamas</collectingCountry>
