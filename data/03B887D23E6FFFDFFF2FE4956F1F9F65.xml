<collectingCountry id="F30676543E6FFFDFFB6AE08D68259E90" box="[1234,1345,1167,1192]" name="Suriname" pageId="16" pageNumber="191">Suriname</collectingCountry>
; French 
<collectingCountry id="F30676543E6FFFDFFF2FE0B66D969EF4" box="[151,242,1204,1228]" name="Guyana" pageId="16" pageNumber="191">Guyana</collectingCountry>
