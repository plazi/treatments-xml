. 
<taxonomicName id="6E8D63EDFFDD0444E7628CE3DB3A342A" authority="Sheng &amp; Li" authorityName="Sheng &amp; Li" authorityYear="2022" box="[328,816,1768,1792]" class="Insecta" family="Ichneumonidae" genus="Megastylus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="457" phylum="Arthropoda" rank="species" species="mentouense" status="sp. nov." subGenus="Dicolus">
