Nutt.
</taxonomicName>
, and 
<taxonomicName id="38DC4D6A0913FFB9FBF85032B1EAF9C1" authority="A. de Candolle" authorityName="A. de Candolle" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="13" pageNumber="26" phylum="Tracheophyta" rank="species" species="wislizenii">
