<taxonomicName id="38DC4D6A0913FFB9FCF35032B3BFF9E4" authority="Nutt." authorityName="Nutt." box="[828,1018,1575,1601]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="13" pageNumber="26" phylum="Tracheophyta" rank="species" species="dumosa">
