<taxonomicName id="38DC4D6A0913FFB9FC4C5011B15EF9E4" authority="Liebm. They" authorityName="Liebm. They" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="13" pageNumber="26" phylum="Tracheophyta" rank="species" species="chrysolepis">
