(
<bibRefCitation id="EFF115FDFFC3FFAEFAE11EEFFF4DF8FC" author="Bousfield, E. L." pageId="1" pageNumber="22" pagination="255 - 293" refId="ref4645" refString="Bousfield, E. L. (1971) Amphipoda of the Bismarck Archipelago and adjacent Indo-Pacific islands (Crustacea). Steenstrupia, 1, 255 - 293." type="journal article" year="1971">Bousfield, 1971</bibRefCitation>
)
</taxonomicName>
; 
<taxonomicName id="4C60138FFFC3FFAEFF141ECAFDAFF8FC" authority="(Bousfield, 1971)" baseAuthorityName="Bousfield" baseAuthorityYear="1971" box="[235,567,1781,1807]" class="Malacostraca" family="Makawidae" genus="Parorchestia" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="22" phylum="Arthropoda" rank="species" species="similis">
