<taxonomicName id="4C514D1EFFA0FFF5F5EAF96545E0F97C" authority="Roger, 1861" authorityName="Roger" authorityYear="1861" box="[151,442,1690,1717]" class="Insecta" family="Formicidae" genus="Leptogenys" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis id="B925EA8FFFA0FFF5F5EAF96545E0F97C" bold="true" box="[151,442,1690,1717]" pageId="33" pageNumber="34">
<emphasis id="B925EA8FFFA0FFF5F5EAF965457AF97D" bold="true" box="[151,288,1690,1716]" italics="true" pageId="33" pageNumber="34">Leptogenys</emphasis>
<bibRefCitation id="EFC04B6CFFA0FFF5F45AF96445E0F97C" author="Roger" box="[295,442,1691,1717]" pageId="33" pageNumber="34" refString="Roger, J. (1861) Die Ponera-artigen Ameisen. (Schluss.) Berliner Entomologische Zeitschrift, 5, 1 - 54." type="journal article" year="1861">
