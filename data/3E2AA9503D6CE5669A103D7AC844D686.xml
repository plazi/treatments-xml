<subSubSection id="A34E3F3EB06FEBFD8613FC81B7466A88" pageId="47" pageNumber="48" type="reference_group">
<paragraph id="5E9EE62E562CB47766F211DD3D0EA3B0" pageId="47" pageNumber="48">
<taxonomicName id="9422D214836E9A47BA239EB2B58A5D9A" class="Gastropoda" family="Camaenidae" genus="Trachia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trachia" order="Stylommatophora" pageId="47" pageNumber="48" phylum="Mollusca" rank="genus">Trachia</taxonomicName>
