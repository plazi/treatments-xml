<taxonomicName id="A3E145338E95B9F3954AB726485AF437" class="Magnoliopsida" family="Hypericaceae" genus="Hypericum" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="713" phylum="Tracheophyta" rank="species" species="veronense">
