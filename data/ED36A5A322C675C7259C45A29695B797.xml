<taxonomicName id="381F41FD0C4DC3637C9D60C22FE5293F" class="Magnoliopsida" family="Hypericaceae" genus="Hypericum" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="713" phylum="Tracheophyta" rank="species" species="perforatum">
