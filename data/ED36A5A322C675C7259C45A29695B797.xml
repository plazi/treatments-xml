<normalizedToken id="766A74643EA04E34CE2911116B3CFF73" originalValue="geflügelt" pageId="null" pageNumber="713">gefluegelt</normalizedToken>
; 
<normalizedToken id="4843A56DF85A1527863E3435381BA468" originalValue="Kronblätter" pageId="null" pageNumber="713">Kronblaetter</normalizedToken>
<normalizedToken id="BD87BF5CD5353E871581C85B5AE7FEF6" originalValue="2½" pageId="null" pageNumber="713">21/2</normalizedToken>
-3mal so lang wie der Kelch; 
<normalizedToken id="E08C22005C617071B8E7C06C6D1351F6" originalValue="Staubblätter" pageId="null" pageNumber="713">Staubblaetter</normalizedToken>
