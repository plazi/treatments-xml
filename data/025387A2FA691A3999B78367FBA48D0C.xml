<taxonomicName id="4DFA4D37FA691A399A9E83BEFBAB8D0D" authorityName="Cambessedes" authorityYear="1832" box="[929,1092,1274,1300]" class="Magnoliopsida" family="Myrtaceae" genus="Myrcia" kingdom="Plantae" order="Myrtales" pageId="13" pageNumber="82" phylum="Tracheophyta" rank="species" species="linearifolia">
