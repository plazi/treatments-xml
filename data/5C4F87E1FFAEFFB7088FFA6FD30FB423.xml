<taxonomicName id="13E64D74FFAEFFB60971F9FCD5FBB0E9" authority="Bavay, 1869" authorityName="Bavay" authorityYear="1869" box="[361,766,1661,1686]" class="Insecta" family="Curculionidae" genus="Platydactylus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="chahoua">
