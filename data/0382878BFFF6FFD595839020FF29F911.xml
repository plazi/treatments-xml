</emphasis>
<taxonomicName id="4C2B4D1EFFF6FFD5942D9094FCD1FA9D" authority="Milne Edwards, 1830" authorityName="Milne Edwards" authorityYear="1830" box="[313,807,1255,1280]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="552" phylum="Arthropoda" rank="species" species="atlanticus">
