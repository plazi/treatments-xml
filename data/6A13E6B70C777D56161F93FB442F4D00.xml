<paragraph id="84C94745DBB1D8F1985D6517D29B13EE" pageId="6" pageNumber="29">
<taxonomicName id="032A9D32453B0005867C44DE9A649C24" authority="X. Y. Ma, K. D. Hyde &amp; Jayawardena" class="Sordariomycetes" family="Glomerellaceae" genus="Colletotrichum" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Colletotrichum cariniferi" order="Glomerellales" pageId="6" pageNumber="29" phylum="Ascomycota" rank="species" species="cariniferi">Colletotrichum cariniferi X.Y. Ma, K.D. Hyde &amp; Jayawardena</taxonomicName>
