<taxonomicName id="4C1BC8A9FF9C3974FB278FA9FAA7FDFE" box="[1266,1382,498,523]" class="Monoplacophora" family="Neopilinidae" genus="Neopilina" kingdom="Animalia" order="Tryblidiida" pageId="3" pageNumber="208" phylum="Mollusca" rank="genus">
