<heading id="6F7CC5581761A94E989DFE6C7AB5FE3D" bold="true" box="[151,758,423,449]" fontSize="11" level="1" pageId="80" pageNumber="81" reason="1">
<emphasis id="06FFAE261761A94E989DFE6C7AB5FE3D" bold="true" box="[151,758,423,449]" pageId="80" pageNumber="81">
<taxonomicName id="F38B09B71761A94E989DFE6C7A1AFE3D" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,601,423,449]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="meyi" status="new species">
<emphasis id="06FFAE261761A94E989DFE6C7936FE3D" bold="true" box="[151,373,423,449]" italics="true" pageId="80" pageNumber="81">Hydropsyche meyi</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D1761A94E9A6DFE6C7AB5FE3D" box="[615,758,423,449]" pageId="80" pageNumber="81" rank="species">new species</taxonomicNameLabel>
