<taxonomicName id="4C3870C0FFA4FFBF3C68F8162C34DB82" authority="Wang, Li &amp; Zhang" authorityName="Wang, Li &amp; Zhang" authorityYear="2021" box="[260,615,2021,2043]" class="Arachnida" family="Lycosidae" genus="Arctosa" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="31" phylum="Arthropoda" rank="species" species="tangguoi" status="sp. nov.">
