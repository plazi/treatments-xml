<taxonomicName id="4C3870C0FFB9FFA23988FEFC2B65DD5F" authorityName="C. L. Koch" authorityYear="1847" box="[1252,1334,271,294]" class="Arachnida" family="Lycosidae" genus="Arctosa" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="34" phylum="Arthropoda" rank="genus">
