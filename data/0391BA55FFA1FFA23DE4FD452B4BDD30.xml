<taxonomicName id="4C3870C0FFB9FFA23F76FEFC2CF2DD5E" box="[538,673,271,295]" class="Arachnida" family="Lycosidae" genus="Arctosa" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="34" phylum="Arthropoda" rank="species" species="tridentata">
