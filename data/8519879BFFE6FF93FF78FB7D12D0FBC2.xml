<typeStatus id="D20B882FFFE0FF93FECCFCC712BCFC31" box="[291,402,872,897]" pageId="28" pageNumber="29">Holotype</typeStatus>
.
</emphasis>
<collectingCountry id="75A7761DFFE0FF93FE4CFCC712CCFC32" box="[419,482,872,898]" name="Malta" pageId="28" pageNumber="29">Male</collectingCountry>
