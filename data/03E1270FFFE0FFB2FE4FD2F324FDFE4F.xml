<taxonomicName id="4C48ED9AFFE0FFB3FE24D7D327E6F98E" authority="Kejval &amp; Cz, 2013" authorityName="Kejval &amp; Cz" authorityYear="2013" box="[417,684,1531,1554]" class="Insecta" family="Anthicidae" genus="Mecynotarsus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="excavatus" status="sp. nov.">
