<collectingCountry id="C09244F5FFFC8B32AC6DFEA9FE2BFE83" box="[359,426,333,359]" name="China" pageId="5" pageNumber="433">China</collectingCountry>
(
<collectingRegion id="7A41CA87FFFC8B32ACB3FEA9FDA3FE8C" box="[441,546,333,360]" country="China" name="Xinjiang Uygur" pageId="5" pageNumber="433">Xinjiang</collectingRegion>
