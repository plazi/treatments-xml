<taxonomicName id="4C557F99B91D9719FD9785899F73F835" authorityName="J. L. Barnard" authorityYear="1965" box="[534,627,1953,1978]" class="Malacostraca" family="Lysianassidae" genus="Azotostoma" kingdom="Animalia" order="Amphipoda" pageId="3" pageNumber="564" phylum="Arthropoda" rank="species" species="fusta">
<emphasis id="B921D808B91D9719FD9785899F73F835" box="[534,627,1953,1978]" italics="true" pageId="3" pageNumber="564">A. fusta</emphasis>
</taxonomicName>
