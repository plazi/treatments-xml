<collectingCountry id="9BA5767DF20A8B06FE9DF8CAFE2DF88D" box="[365,435,1858,1884]" name="Japan" pageId="25" pageNumber="26">Japan</collectingCountry>
, 
<collectingCountry id="9BA5767DF20A8B06FE4FF8CAFDB4F88D" box="[447,554,1858,1884]" name="Australia" pageId="25" pageNumber="26">Australia</collectingCountry>
(
<collectingRegion id="2176F80FF20A8B06FDC9F8CAFC8CF88D" box="[569,786,1858,1884]" country="Australia" name="Western Australia" pageId="25" pageNumber="26">Western Australia</collectingRegion>
).
</paragraph>
