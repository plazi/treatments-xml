<taxonomicName id="5D3A80B165FC85545CBD82D34F258D1F" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. muscorum" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="muscorum" subSpecies="celticus">B. muscorum celticus</taxonomicName>
