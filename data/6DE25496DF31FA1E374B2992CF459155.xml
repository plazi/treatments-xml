<taxonomicName id="C812CFFD60B4F76F39150406D4479D3E" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. muscorum" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="muscorum" subSpecies="agricolae">B. muscorum agricolae</taxonomicName>
