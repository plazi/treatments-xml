<taxonomicName id="F39B99F1A8D401550CB4F846DB84E556" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. muscorum" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="muscorum" subSpecies="scyllonius">B. muscorum scyllonius</taxonomicName>
