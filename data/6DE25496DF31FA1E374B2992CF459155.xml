<taxonomicName id="94267CD89824B856C3E6616B3EB786A2" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. muscorum" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="muscorum" subSpecies="orcadensis">B. muscorum orcadensis</taxonomicName>
