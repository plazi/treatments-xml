<collectingRegion id="499F46A7FFA0FFA503ABE382FBFAFB7D" box="[1015,1082,1196,1216]" country="Spain" name="Madrid" pageId="52" pageNumber="1065">Madrid</collectingRegion>
, Manzanares on &quot; 
<emphasis id="B92F5457FFA0FFA501D4E3EAFDE2FB65" box="[392,546,1220,1240]" italics="true" pageId="52" pageNumber="1065">
<taxonomicName id="4C5BF3C6FFA0FFA501D4E3EAFE1BFB65" box="[392,475,1220,1240]" class="Magnoliopsida" family="Asteraceae" genus="Santolina" kingdom="Plantae" order="Asterales" pageId="52" pageNumber="1065" phylum="Tracheophyta" rank="genus">Santolina</taxonomicName>
