<pageBreakToken id="601C8BCCE89A569F969E44DCFB3B0C3B" pageId="null" pageNumber="34" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="A9E69C0B1C229DAED99110FFA2CF8268" authority="L." class="Magnoliopsida" family="Gentianaceae" genus="Gentiana" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="34" phylum="Tracheophyta" rank="species" species="campestris">
