; of 
<specimenCount id="9D5BB9E7A20F3068FBC9020DFB96FAF7" box="[1117,1136,1360,1382]" pageId="32" pageNumber="39" type="female">♀</specimenCount>
, 23-42 and 
<quantity id="4CA5DF8BA20F3068FD460230FCB0FA16" box="[722,854,1389,1415]" metricMagnitude="-3" metricUnit="m" metricValue="1.85" metricValueMax="2.2" metricValueMin="1.5" pageId="32" pageNumber="39" unit="mm" value="1.85" valueMax="2.2" valueMin="1.5">1.5-2.2 mm</quantity>
