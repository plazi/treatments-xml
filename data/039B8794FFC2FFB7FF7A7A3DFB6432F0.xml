<heading id="D0C581EEFFC2FFB7FF7A7A3DFA0C3431" box="[177,574,616,638]" fontSize="8" level="2" pageId="3" pageNumber="1214" reason="2">
<taxonomicName id="4C324D01FFC2FFB7FF7A7A3DFA0C3431" authority="(SCOPOLI 1772)" baseAuthorityName="SCOPOLI" baseAuthorityYear="1772" box="[177,574,616,638]" class="Insecta" family="Tephritidae" genus="Stemonocera" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="1214" phylum="Arthropoda" rank="species" species="cornuta">
