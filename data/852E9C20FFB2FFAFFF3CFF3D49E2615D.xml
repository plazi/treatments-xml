<quantity id="CA7F80D3FFB3FFAFFB98FA5F49A7610D" box="[1075,1251,1525,1551]" metricMagnitude="2" metricUnit="m" metricValue="9.19" metricValueMax="16.38" metricValueMin="2.0" pageId="65" pageNumber="66" unit="m" value="919.0" valueMax="1638.0" valueMin="200.0">200 to 1638 m</quantity>
