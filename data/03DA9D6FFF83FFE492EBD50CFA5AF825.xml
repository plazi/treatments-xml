<taxonomicName id="4C7357FAFF83FFE4973CD680FA5AF905" baseAuthorityName="Lefebvre" baseAuthorityYear="1842" box="[1344,1435,1719,1740]" class="Insecta" family="Ascalaphidae" genus="Libelloides" kingdom="Animalia" order="Neuroptera" pageId="17" pageNumber="304" phylum="Arthropoda" rank="species" species="latinus">
<emphasis id="B907F06BFF83FFE4973CD680FA5AF905" box="[1344,1435,1719,1740]" italics="true" pageId="17" pageNumber="304">L. latinus</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8BCC2C79FF83FFE492EBD6E3FA5AF8CF" blockId="17.[151,1437,1401,2028]" pageId="17" pageNumber="304">
<keyLead id="30828F4CFF83FFE492EBD6E3FA5AF8CF" pageId="17" pageNumber="304">
