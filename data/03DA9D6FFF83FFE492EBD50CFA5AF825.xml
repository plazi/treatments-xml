<taxonomicName id="4C7357FAFF83FFE49743D63DFA5AF9D6" baseAuthorityName="Angelini" baseAuthorityYear="1827" box="[1343,1435,1546,1567]" class="Insecta" family="Ascalaphidae" genus="Libelloides" kingdom="Animalia" order="Neuroptera" pageId="17" pageNumber="304" phylum="Arthropoda" rank="species" species="siculus">
