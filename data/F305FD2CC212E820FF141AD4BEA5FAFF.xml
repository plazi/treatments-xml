</taxonomicName>
and 
<taxonomicName id="BCAC37B9C212E820FF4A1DABBEA9FAFF" authority="(Willemse, 1961)" baseAuthorityName="Willemse" baseAuthorityYear="1961" box="[157,484,1261,1288]" class="Insecta" family="Tettigoniidae" genus="Platycaedicia" kingdom="Animalia" order="Orthoptera" pageId="28" pageNumber="553" phylum="Arthropoda" rank="species" species="inexpectata">
