<taxonomicName id="0AD764CE66EC6D882D35FE5E8D3DEC2F" baseAuthorityName="Sowerby I" baseAuthorityYear="1832" class="Gastropoda" family="Bothriembryontidae" genus="Plectostylus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="9" pageNumber="10" phylum="Mollusca" rank="species" species="broderipii">
