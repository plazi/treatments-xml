, width 
<quantity id="4CE2E281C930FFE9611A8D77D9E2FBFC" box="[1096,1202,1042,1066]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="14" pageNumber="414" unit="mm" value="0.85">0.85 mm</quantity>
, height 
<quantity id="4CE2E281C930FFE965B48D50DC00FB9B" box="[230,336,1077,1101]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="14" pageNumber="414" unit="mm" value="0.3">0.30 mm</quantity>
, head width 
<quantity id="4CE2E281C930FFE964B58D50DF00FB9B" box="[487,592,1077,1101]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="14" pageNumber="414" unit="mm" value="0.45">0.45 mm</quantity>
, eye group width 
<quantity id="4CE2E281C930FFE966718D50DEDDFB9B" box="[803,909,1077,1101]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="14" pageNumber="414" unit="mm" value="0.35">0.35 mm</quantity>
; abdomen 
<quantity id="4CE2E281C930FFE961408D50D924FB9B" box="[1042,1140,1077,1101]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="14" pageNumber="414" unit="mm" value="1.1">1.10 mm</quantity>
long, 
<quantity id="4CE2E281C930FFE965C78D3DDDAAFBA6" box="[149,250,1112,1136]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="14" pageNumber="414" unit="mm" value="0.6">0.60 mm</quantity>
