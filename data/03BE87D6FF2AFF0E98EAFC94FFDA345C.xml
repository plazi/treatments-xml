<quantity id="4CEF9B25FF2AFF0E9D6AFC04FFDD3414" box="[1303,1436,1014,1041]" metricMagnitude="2" metricUnit="m" metricValue="6.1" metricValueMax="11.95" metricValueMin="0.25" pageId="187" pageNumber="188" unit="m" value="610.0" valueMax="1195.0" valueMin="25.0">25–1,195 m</quantity>
(
<quantity id="4CEF9B25FF2AFF0E98B3FBE9FB123430" box="[206,339,1050,1077]" metricMagnitude="2" metricUnit="m" metricValue="6.100572" metricValueMax="11.951208" metricValueMin="0.249936" pageId="187" pageNumber="188" unit="ft" value="2001.5" valueMax="3921.0" valueMin="82.0">82–3,921 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FF2AFF0E99D1FBE8F8E63430" author="Allen, M. J. &amp; Smith, G. B." box="[428,679,1050,1077]" pageId="187" pageNumber="188" refId="ref259821" refString="Allen, M. J. &amp; Smith, G. B. (1988) Atlas and zoogeography of common fishes in the Bering Sea and northeastern Pacific. NOAA Technical Report NMFS, 66." type="book" year="1988">Allen and Smith 1988</bibRefCitation>
; max.: Parin and Pakhorukov 2003); adults typically at 
<quantity id="4CEF9B25FF2AFF0E9D60FBE8FFDD3430" box="[1309,1436,1050,1077]" metricMagnitude="2" metricUnit="m" metricValue="4.5" metricValueMax="6.0" metricValueMin="3.0" pageId="187" pageNumber="188" unit="m" value="450.0" valueMax="600.0" valueMin="300.0">300–600 m</quantity>
(
<quantity id="4CEF9B25FF2AFF0E98B3FBCCFB21345C" box="[206,352,1086,1113]" metricMagnitude="2" metricUnit="m" metricValue="4.498848" metricValueMax="5.998464" metricValueMin="2.999232" pageId="187" pageNumber="188" unit="ft" value="1476.0" valueMax="1968.0" valueMin="984.0">984–1,968 ft</quantity>
) (
<bibRefCitation id="EF864B31FF2AFF0E990BFBCCF894345C" author="Mecklenburg, C. W. &amp; Sheiko, B. A." box="[374,725,1086,1113]" pageId="187" pageNumber="188" refId="ref300791" refString="Mecklenburg, C. W. &amp; Sheiko, B. A. (2004) Family Stichaeidae Gill 1864 - pricklebacks. California Academy of Sciences, Annotated Checklists of Fishes, No. 35." type="book" year="2004">Mecklenburg and Sheiko 2004</bibRefCitation>
