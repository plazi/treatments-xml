<quantity id="4CEF9B25FF2AFF0E98B3FBCCFB21345C" box="[206,352,1086,1113]" metricMagnitude="2" metricUnit="m" metricValue="4.498848" metricValueMax="5.998464" metricValueMin="2.999232" pageId="187" pageNumber="188" unit="ft" value="1476.0" valueMax="1968.0" valueMin="984.0">984–1,968 ft</quantity>
