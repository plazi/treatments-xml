<taxonomicName id="4C174D43FF2AFF0E9A9AFBCCF991345D" box="[743,976,1086,1112]" class="Actinopterygii" family="Stichaeidae" genus="Lumpenlla" kingdom="Animalia" order="Perciformes" pageId="187" pageNumber="188" phylum="Chordata" rank="species" species="nigricans">
