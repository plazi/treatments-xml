<taxonomicName id="9CC54D1F251CFFC1FF7FEDD4420AFD28" box="[232,588,722,746]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="22" pageNumber="22" phylum="Tracheophyta" rank="subSpecies" species="macrocarpa" subSpecies="elachanta">
