<taxonomicName id="CE8F4D68D77D391CFDA0FE2CFD6699F7" box="[573,677,477,500]" class="Insecta" family="Pseudococcidae" genus="Ferrisia" kingdom="Animalia" order="Hemiptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="virgata">
