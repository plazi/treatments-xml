<taxonomicName id="4C562CA70A3CF549FF68CAB8AB29F93E" box="[151,285,1686,1711]" class="Gastropoda" family="Viviparidae" kingdom="Animalia" pageId="57" pageNumber="58" phylum="Mollusca" rank="family">Viviparidae</taxonomicName>
