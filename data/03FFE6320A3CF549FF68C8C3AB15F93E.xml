<taxonomicName id="4C562CA70A3CF549FE29CA5CA862F91A" box="[470,598,1650,1675]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="57" pageNumber="58" phylum="Mollusca" rank="genus">
<emphasis id="B9228B360A3CF549FE29CA5CA862F91A" box="[470,598,1650,1675]" italics="true" pageId="57" pageNumber="58">Ampullaria</emphasis>
</taxonomicName>
