<collectionCode id="ED39AE08FFCFFFB3FB81FA8AFB993426" box="[1057,1137,1306,1332]" country="Austria" httpUri="http://grbio.org/cool/c7yt-hh77" name="Naturhistorisches Museum, Wien" pageId="116" pageNumber="117">NMW</collectionCode>
