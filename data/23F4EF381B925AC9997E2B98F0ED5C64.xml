<taxonomicName id="9F4BFA2765865795DBEAFAEEF53F91E5" authorityName="Soula" authorityYear="2010" class="Insecta" family="Scarabaeidae" genus="Chalcoplethis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcoplethis kirbyi subsp. misionesensis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="subSpecies" species="kirbyi" subSpecies="misionesensis">
<emphasis id="D518BCDC12E2FF290E77300AF56593FD" italics="true" pageId="0" pageNumber="1">Chalcoplethis kirbyi misionesensis</emphasis>
</taxonomicName>
Soula 2010&quot; (47030871); &quot;Pozo Azul, Misiones Argentine, XII/2009//Allotype 
<taxonomicName id="7F417D742C1358F96342E85DE83D52EF" authorityName="Soula" authorityYear="2010" class="Insecta" family="Scarabaeidae" genus="Chalcoplethis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcoplethis kirbyi subsp. misionesensis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="subSpecies" species="kirbyi" subSpecies="misionesensis">
<emphasis id="1E83FEBF30755379C5182CDE145B8E1B" italics="true" pageId="0" pageNumber="1">Chalcoplethis kirbyi misionesensis</emphasis>
</taxonomicName>
