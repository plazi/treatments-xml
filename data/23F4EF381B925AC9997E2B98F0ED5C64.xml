<taxonomicName id="E5596331FD8A63E1CB2000F0F28050FE" authorityName="Soula" authorityYear="2010" class="Insecta" family="Scarabaeidae" genus="Chalcoplethis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcoplethis kirbyi subsp. misionesensis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="subSpecies" species="kirbyi" subSpecies="misionesensis">
<emphasis id="ADD6A6214EEB4AC400F8DDB00F61518C" italics="true" pageId="0" pageNumber="1">Chalcoplethis kirbyi misionesensis</emphasis>
