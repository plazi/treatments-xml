<taxonomicName id="5BD4B11082E35EA5BCFF316980A45959" authorityName="Soula" authorityYear="2010" class="Insecta" family="Scarabaeidae" genus="Chalcoplethis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chalcoplethis kirbyi subsp. misionesensis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="subSpecies" species="kirbyi" subSpecies="misionesensis">
