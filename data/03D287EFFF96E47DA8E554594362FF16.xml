<emphasis id="B90FEAEBFF96E47CA8B555624012F8C7" bold="true" box="[199,273,1805,1830]" pageId="54" pageNumber="55">Hosts.</emphasis>
<taxonomicName id="4C7B4D7AFF96E47CA96F5561409FF8C7" box="[285,412,1806,1830]" class="Magnoliopsida" family="Asteraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
