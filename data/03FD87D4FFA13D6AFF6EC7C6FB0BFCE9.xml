<number id="3D9C24E5FFA03D6AFE9AC679FB41FEBC" box="[355,372,296,320]" pageId="23" pageNumber="524" value="2.0">2</number>
; metatarsus II pv 
<number id="3D9C24E5FFA03D6AFDC5C679F879FEBC" box="[572,588,296,320]" pageId="23" pageNumber="524" value="2.0">2</number>
, rv 
<number id="3D9C24E5FFA03D6AFD8FC679F8B2FEBC" box="[630,647,296,320]" pageId="23" pageNumber="524" value="2.0">2</number>
; femur III md 
<number id="3D9C24E5FFA03D6AFCD3C679F963FEBC" box="[810,854,296,320]" pageId="23" pageNumber="524" value="0.5" valueMax="1.0" valueMin="0.0">0–1</number>
, pd 
<number id="3D9C24E5FFA03D6AFC7CC679F9A3FEBC" box="[901,918,296,320]" pageId="23" pageNumber="524" value="2.0">2</number>
