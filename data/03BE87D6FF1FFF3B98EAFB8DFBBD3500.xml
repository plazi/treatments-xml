<quantity id="4CEF9B25FF1FFF3B9C45FB8DFE2F349F" box="[1080,1134,1151,1178]" metricMagnitude="-2" metricUnit="m" metricValue="6.0" pageId="142" pageNumber="143" unit="cm" value="6.0">6 cm</quantity>
