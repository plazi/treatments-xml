<quantity id="4CEF9B25FF1FFF3B9C84FB72FF05349F" box="[1273,1348,1152,1178]" metricMagnitude="-2" metricUnit="m" metricValue="7.1" pageId="142" pageNumber="143" unit="cm" value="7.1">7.1 cm</quantity>
