<quantity id="4CEF9B25FF1FFF3B9ACEFB35F94E34E4" box="[691,783,1223,1249]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="142" pageNumber="143" unit="m" value="2000.0">2,000 m</quantity>
(
