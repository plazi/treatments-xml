<quantity id="4CEF9B25FF1FFF3B9D2EFB72FFD5349F" box="[1363,1428,1151,1178]" metricMagnitude="-2" metricUnit="m" metricValue="7.111999999999999" pageId="142" pageNumber="143" unit="in" value="2.8">2.8 in</quantity>
