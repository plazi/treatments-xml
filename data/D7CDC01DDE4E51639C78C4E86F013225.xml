<taxonomicName id="E879103017A293CC8BD723AE35024542" LSID="D7CDC01D-DE4E-5163-9C78-C4E86F013225" authority="Smith" authorityName="Smith" class="Magnoliopsida" family="Tenthredinidae" genus="Leseha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Leseha" order="Hymenoptera" pageId="0" pageNumber="45" phylum="Tracheophyta" rank="genus">Leseha Smith</taxonomicName>
