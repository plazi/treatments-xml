<taxonomicName id="4C784D31FFEAFFFD71E3FD64FC7D1DCC" box="[953,1006,673,694]" class="Gastropoda" family="Ficidae" genus="Ficus" kingdom="Animalia" order="Littorinimorpha" pageId="3" pageNumber="94" phylum="Mollusca" rank="genus">
<emphasis id="B90CEAA0FFEAFFFD71E3FD64FC7D1DCC" box="[953,1006,673,694]" italics="true" pageId="3" pageNumber="94">Ficus</emphasis>
</taxonomicName>
