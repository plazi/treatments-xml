<quantity id="EE421EF8FFF19E532D8CF916FB926FD5" box="[1014,1145,1730,1754]" metricMagnitude="2" metricUnit="m" metricValue="8.905" metricValueMax="8.97" metricValueMin="8.84" pageId="1" pageNumber="60" unit="m" value="890.5" valueMax="897.0" valueMin="884.0">884–897 m</quantity>
