<taxonomicName id="4C6DBBE3FFB68D017B6C02A9FD5C97C2" authority="Jepson et al., 2012: 38" authorityName="Jepson" authorityPageNumber="38" authorityYear="2012" box="[297,764,1513,1535]" class="Insecta" family="Osmylidae" genus="Mesosmylidus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Neuroptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="vulgaris">
