<quantity id="EB5061F5FFCBFFB6FC6D6CF2BC5BFF69" box="[923,1026,189,215]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="31" pageNumber="32" unit="mm" value="0.13">0.13 mm</quantity>
