<quantity id="EB5061F5FFCBFFB6FDA76CF2BAE1FF69" box="[593,696,189,215]" metricMagnitude="-4" metricUnit="m" metricValue="2.9" pageId="31" pageNumber="32" unit="mm" value="0.29">0.29 mm</quantity>
. BL 
<quantity id="EB5061F5FFCBFFB6FD1A6CF3BB38FF69" box="[748,865,188,215]" metricMagnitude="-3" metricUnit="m" metricValue="5.342" pageId="31" pageNumber="32" unit="mm" value="5.342">5.342 mm</quantity>
