<quantity id="EB5061F5FFCBFFB6FBC26CF2BCC2FF69" box="[1076,1179,189,215]" metricMagnitude="-4" metricUnit="m" metricValue="2.4" pageId="31" pageNumber="32" unit="mm" value="0.24">0.24 mm</quantity>
