<quantity id="EB5061F5FFCBFFB6FEE26CF2B934FF69" box="[276,365,189,215]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="31" pageNumber="32" unit="mm" value="0.2">0.2 mm</quantity>
. GW 
<quantity id="EB5061F5FFCBFFB6FE5B6CF2BA4DFF69" box="[429,532,189,215]" metricMagnitude="-4" metricUnit="m" metricValue="2.2" pageId="31" pageNumber="32" unit="mm" value="0.22">0.22 mm</quantity>
