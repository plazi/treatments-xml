<caption id="A2A66620FFBDBB33FF32FBA86D85FB9D" ID-DOI="http://doi.org/10.5281/zenodo.5446203" ID-Zenodo-Dep="5446203" httpUri="https://zenodo.org/record/5446203/files/figure.png" pageId="8" pageNumber="659" startId="8.[144,223,1084,1103]" targetBox="[303,1261,198,1054]" targetPageId="8">
