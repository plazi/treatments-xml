<paragraph id="F0DF930C271F51C47A780B8C3B2BDF5C" pageId="2" pageNumber="75">
<taxonomicName id="577A48900D5B3E7F7519523ECB84EEBF" class="Insecta" family="Curculionidae" genus="Arostropsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arostropsis groehni" order="Coleoptera" pageId="2" pageNumber="75" phylum="Arthropoda" rank="species" species="groehni">Arostropsis groehni</taxonomicName>
