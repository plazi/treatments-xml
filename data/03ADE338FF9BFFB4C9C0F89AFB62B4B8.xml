. 
<emphasis id="B9708E3CFF9BFFB5CBCAF7C4FCBDBC45" bold="true" box="[642,771,2055,2081]" pageId="14" pageNumber="207">
<collectingRegion id="49C09CCCFF9BFFB5CBCAF7C4FD40BC45" box="[642,766,2055,2081]" country="Brazil" name="Sao Paulo" pageId="14" pageNumber="207">São Paulo</collectingRegion>
:
</emphasis>
<location id="8EDB04F5FF9BFFB5CA42F7C4FBB6BC45" LSID="urn:lsid:plazi:treatment:03ADE338FF9BFFB4C9C0F89AFB62B4B8:8EDB04F5FF9BFFB5CA42F7C4FBB6BC45" box="[778,1032,2055,2081]" country="Brazil" name="Sao Jose do Rio Pardo" pageId="14" pageNumber="207" stateProvince="Sao Paulo">São José do Rio Pardo</location>
, 
<date id="FFBA74EEFF9BFFB5CD5CF7C4FB0CBC45" box="[1044,1202,2055,2082]" pageId="14" pageNumber="207" value="2013-04-25">
