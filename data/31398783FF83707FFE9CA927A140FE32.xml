<taxonomicName id="7E904D16FF827071FC84A959A3F0F99E" authority="(Quoy and Gaimard)" baseAuthorityName="Quoy and Gaimard" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="87" pageNumber="748" phylum="Chordata" rank="species" species="undulosus">
