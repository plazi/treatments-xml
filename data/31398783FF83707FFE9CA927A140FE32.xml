<taxonomicName id="7E904D16FF827071FFFAA966A24BF9FE" box="[125,374,1567,1591]" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="87" pageNumber="748" phylum="Chordata" rank="species" species="nudulosus">
