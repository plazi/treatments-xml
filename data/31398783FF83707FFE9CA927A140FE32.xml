(
<taxonomicName id="7E904D16FF827071FDDFA946A1E9F99E" authorityName="Swainson" authorityYear="1839" box="[600,724,1599,1623]" class="Actinopterygii" family="Serranidae" kingdom="Animalia" order="Perciformes" pageId="87" pageNumber="748" phylum="Chordata" rank="family">Serranidae</taxonomicName>
