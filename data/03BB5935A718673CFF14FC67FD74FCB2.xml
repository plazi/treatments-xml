<subSubSection id="C308BBA8A718673DFF44FB01FD4DFB64" box="[199,697,1225,1250]" pageId="20" pageNumber="21" type="etymology">
<paragraph id="8BADE823A718673DFF44FB01FD4DFB64" blockId="20.[151,1436,1117,2005]" box="[199,697,1225,1250]" pageId="20" pageNumber="21">
<emphasis id="B9663431A718673DFF44FB01FEB9FB64" bold="true" box="[199,333,1225,1249]" inLineHeading="true" pageId="20" pageNumber="21" reason="1">Etymology.</emphasis>
