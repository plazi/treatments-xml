<paragraph id="8BAB5A6AFF90FFBA319FF93AFB68E9A4" blockId="8.[151,1209,1692,1715]" box="[151,1209,1692,1715]" pageId="8" pageNumber="485">
<emphasis id="B9608678FF90FFBA319FF93AFE97E9A4" bold="true" box="[151,326,1693,1715]" pageId="8" pageNumber="485">FIGURE 25–26.</emphasis>
<taxonomicName id="4C1421E9FF90FFBA3044F93BFD7FE9A4" authority="Fraser, 1929" authorityName="Fraser" authorityYear="1929" box="[332,686,1692,1714]" class="Insecta" family="Chlorogomphidae" genus="Chloropetalia" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Odonata" pageId="8" pageNumber="485" phylum="Arthropoda" rank="species" species="selysi">
