<emphasis id="B953C24FFF905572FF3B09E9D36A69DC" bold="true" box="[151,334,1966,1988]" pageId="17" pageNumber="268">FIGURES 38–43</emphasis>
. 
<taxonomicName id="4C2765DEFF905572FEF009E8D0AF69DC" authority="Li &amp; Carpenter, 2019" authorityName="Li &amp; Carpenter" authorityYear="2019" box="[348,651,1966,1988]" class="Insecta" family="Vespidae" genus="Parancistrocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="268" phylum="Arthropoda" rank="species" species="abyssicavus" status="sp. nov.">
<emphasis id="B953C24FFF905572FEF009E8D0AF69DC" box="[348,651,1966,1988]" italics="true" pageId="17" pageNumber="268">Parancistrocerus abyssicavus</emphasis>
</taxonomicName>
<emphasis id="B953C24FFF905572FD3F09E9D0C069DC" bold="true" box="[659,740,1966,1988]" pageId="17" pageNumber="268">
<taxonomicNameLabel id="A2607F34FF905572FD3F09E9D0C069DC" box="[659,740,1966,1988]" pageId="17" pageNumber="268" rank="species">sp. nov.</taxonomicNameLabel>
