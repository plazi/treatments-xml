<collectionCode id="97C1A9BBFF88FF95FD735581BCD1FE71" box="[679,766,426,452]" country="USA" name="North Carolina Museum of Natural Sciences" pageId="48" pageNumber="49" type="Museum">NCSM</collectionCode>
