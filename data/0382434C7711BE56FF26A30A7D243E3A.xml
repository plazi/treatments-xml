<collectingCountry id="F33CB2CA7711BE55FCACA0277FEB3847" box="[797,934,1873,1897]" name="Puerto Rico" pageId="9" pageNumber="222">Puerto Rico</collectingCountry>
, 
<collectingCountry id="F33CB2CA7711BE55FC02A02478643847" box="[947,1065,1873,1897]" name="Venezuela" pageId="9" pageNumber="222">Venezuela</collectingCountry>
