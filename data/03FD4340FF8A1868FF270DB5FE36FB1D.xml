<quantity id="4CAC5FB3FF8A1868FDFE0BBCFD37FB71" box="[590,647,1162,1188]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="48" pageNumber="49" unit="km" value="2.0">2 km</quantity>
<superScript id="7C215F1EFF8A1868FD370BBCFD3FFB4D" attach="left" box="[647,655,1162,1176]" fontSize="6" pageId="48" pageNumber="49">2</superScript>
