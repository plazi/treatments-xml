<location id="5E71B685FFEBFFCBFF1A8CF2B0D0C9AB" LSID="urn:lsid:plazi:treatment:D3075148FFEBFFCBFF678FEEB513CC2B:5E71B685FFEBFFCBFF1A8CF2B0D0C9AB" box="[234,392,655,681]" name="Isabela Island" pageId="48" pageNumber="49">Isabela Island</location>
