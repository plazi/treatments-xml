<taxonomicName id="4C174D020065FF829A76FAD6FD6BC359" authorityName="Blahnik &amp; Holzenthal" authorityYear="2011" box="[592,721,1374,1398]" class="Magnoliopsida" family="Apocynaceae" genus="Mortoniella" kingdom="Plantae" order="Gentianales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="species" species="bocaina">
