</emphasis>
<specimenCount id="F884FD2C3B47FFD2FE477AEFFDB666CE" box="[439,529,692,716]" pageId="38" pageNumber="39" type="male">3 males</specimenCount>
. 
<typeStatus id="313988073B47FFD2FDEB7AEFFD0866CE" box="[539,687,692,716]" pageId="38" pageNumber="39">HOLOTYPE</typeStatus>
<materialsCitation id="5EEA3CF83B47FFD2FD467AEFFA8366ED" collectionCode="AEIC" collectorName="S. &amp; J. Peck" country="Ecuador" elevation="680" location="Sto. Domingo" municipality="Pich. Prov." pageId="38" pageNumber="39" specimenCount="681" specimenCount-male="1" typeStatus="holotype">
