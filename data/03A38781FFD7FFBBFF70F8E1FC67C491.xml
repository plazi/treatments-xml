<emphasis id="B97EEA85FFD7FFBEFF70F8E1FC14C11C" bold="true" box="[151,961,1802,1829]" pageId="49" pageNumber="50">
<taxonomicName id="4C0A4D14FFD7FFBEFF70F8E1FCF1C11C" authority="Skelley, Xu &amp; Tang" authorityName="Skelley, Xu &amp; Tang" box="[151,804,1802,1829]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="hiepi" status="new species" subGenus="Strobilophila">
<emphasis id="B97EEA85FFD7FFBEFF70F8E1FEFBC11D" bold="true" box="[151,302,1802,1828]" italics="true" pageId="49" pageNumber="50">Cycadophila</emphasis>
