<taxonomicName id="F0CD4D2EFFC5FF8E6A258BF2343D6ACF" authorityName="Hincks" authorityYear="1955" box="[358,517,455,479]" class="Insecta" family="Haplodiplatyidae" genus="Haplodiplatys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dermaptera" pageId="5" pageNumber="2013" phylum="Arthropoda" rank="genus">Haplodiplatys</taxonomicName>
