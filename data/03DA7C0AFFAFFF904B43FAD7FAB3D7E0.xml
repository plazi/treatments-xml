<taxonomicName id="4C73B69FFFAFFF92487CF935FCB7D7BB" box="[567,776,1732,1757]" class="Malacostraca" family="Archaeoscinidae" genus="Micromimonectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
