<taxonomicName id="4C73B69FFFAFFF92497EF935FBCCD7B8" authority="Woltereck, 1906" authorityName="Woltereck" authorityYear="1906" box="[821,1139,1732,1758]" class="Malacostraca" family="Archaeoscinidae" genus="Micromimonectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="irene">
