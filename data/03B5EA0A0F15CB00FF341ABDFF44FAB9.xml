<taxonomicName id="4C1C209F0F15CB01FF341B6AFDE7F8E2" authority="Iwatsuki, 2013: 83" authorityName="Iwatsuki" authorityPageNumber="83" authorityYear="2013" box="[151,605,1869,1891]" class="Actinopterygii" family="Sparidae" genus="Acanthopagrus" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="73" phylum="Chordata" rank="species" species="arabicus">
<emphasis id="B968870E0F15CB01FF341B6AFE2DF8E2" bold="true" box="[151,407,1869,1891]" italics="true" pageId="2" pageNumber="73">Acanthopagrus arabicus</emphasis>
Iwatsuki, 
<number id="3DD4493B0F15CB01FDA11B6AFD82F8E2" box="[514,568,1869,1891]" pageId="2" pageNumber="73" value="2013.0">2013</number>
: 
<number id="3DD4493B0F15CB01FDE11B6AFDE7F8E2" box="[578,605,1869,1891]" pageId="2" pageNumber="73" value="83.0">83</number>
</taxonomicName>
