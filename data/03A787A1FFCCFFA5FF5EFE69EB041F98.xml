<collectionCode id="ED1FAE72FFCCFFA5FDCAFDF3ED0F1A26" LSID="urn:lsid:biocol.org:col:34697" box="[515,594,582,606]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="8" pageNumber="9">BPBM</collectionCode>
