<taxonomicName id="4C537CF461153A51D03CF9CAFB74F97C" authority="Andre 1980" authorityName="Andre" authorityYear="1980" box="[862,1219,1646,1671]" class="Arachnida" family="Tydeidae" genus="Metatriophtydeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="496" phylum="Arthropoda" rank="genus">
