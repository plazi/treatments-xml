<taxonomicName id="4C537CF461153A51D661F9CAFA22F97C" box="[1283,1429,1646,1671]" class="Arachnida" family="Tydeidae" genus="Triophtydeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="1" pageNumber="496" phylum="Arthropoda" rank="genus">
