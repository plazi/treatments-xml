<taxonomicName id="4C204D39231AC87F044631BAFBF4F90C" authority="Drude" authorityName="Drude" box="[827,1039,1765,1789]" class="Liliopsida" family="Arecaceae" genus="Bactris" kingdom="Plantae" order="Arecales" pageId="2" pageNumber="107" phylum="Tracheophyta" rank="species" species="glaucescens">
