<collectionCode id="D2E728A6FFA2FF95FD8F7D8A6DB526D3" box="[598,677,1484,1508]" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="15" pageNumber="96">MZSP</collectionCode>
