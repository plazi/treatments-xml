<bibRefCitation id="504B99C7D861FFC28AC9F9A24B7DF984" author="Matsumura" box="[411,583,1545,1566]" pageId="1" pageNumber="440" refString="Matsumura, S. (1931) 6000 Illustrated Insects of the Japan-Empire. Toko Shoin, Tokyo, 1496 pp." type="book" year="1931">Matsumura, 1931</bibRefCitation>
); 
<treatmentCitation id="B57BC227D861FFC2890BF9A24BD7F984" author="Heo" box="[601,749,1545,1566]" page="195" pageId="1" pageNumber="440" year="2016">
