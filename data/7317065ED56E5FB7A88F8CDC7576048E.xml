<taxonomicName id="C772A074287131F122DD1C0E0AAEA906" authority="Blume, 1823" authorityName="F. N. Wei" authorityYear="1993" class="Magnoliopsida" family="Magnoliaceae" genus="Manglietia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Manglietia sinoconifera" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="sinoconifera">
<emphasis id="AE67B6A4A0808ADAF2BFD4DA1295E2FA" italics="true" pageId="0" pageNumber="1">Manglietia sinoconifera</emphasis>
