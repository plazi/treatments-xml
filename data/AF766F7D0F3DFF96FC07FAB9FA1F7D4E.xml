<quantity id="E027738E0F3CFF96FC55FD5BFB947B98" box="[969,1064,645,667]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="3" pageNumber="37" unit="mm" value="3.1">3.10 mm</quantity>
