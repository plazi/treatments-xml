(
<bibRefCitation id="0F69BA93FF88FFD2FBFA8BE25F11B092" author="Kriechbaumer" box="[1103,1339,918,943]" pageId="15" pageNumber="250" refString="Kriechbaumer, J. (1854) Hymenopterologisches. Stettiner Entomologische Zeitung, 15, 153 - 158." type="journal article" year="1854">
