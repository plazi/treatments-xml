<heading id="D0ACF863A52DFF8A682BBA7BCF0CFBEA" box="[853,1374,1070,1092]" centered="true" fontSize="9" level="2" pageId="56" pageNumber="155" reason="4">
<taxonomicName id="4C5B348CA52DFF8A682BBA7BCE21FBEA" authorityName="Mengual" authorityYear="2012" box="[853,1139,1070,1091]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="56" pageNumber="166" phylum="Arthropoda" rank="species" species="pintada">
<emphasis id="B92F931DA52DFF8A682BBA7BCE21FBEA" bold="true" box="[853,1139,1070,1091]" italics="true" pageId="56" pageNumber="155">CITROGRAMMA PINTADA</emphasis>
</taxonomicName>
