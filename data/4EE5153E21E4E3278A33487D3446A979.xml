<pageBreakToken id="131C236755123772F3A4ADDCB8534943" pageId="null" pageNumber="550" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="7B2759793C8C752CCA79C3B0DA8AB796" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Anthemis" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="550" phylum="Tracheophyta" rank="species" species="tinctoria">
Anthemis 
