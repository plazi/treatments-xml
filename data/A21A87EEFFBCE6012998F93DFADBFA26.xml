<taxonomicName id="EDB34D7BFFBCE6022B1CF820FD43F828" authorityName="Latreille" authorityYear="1802" box="[531,660,1999,2025]" class="Insecta" family="Nitidulidae" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="548" phylum="Arthropoda" rank="family">Nitidulidae</taxonomicName>
