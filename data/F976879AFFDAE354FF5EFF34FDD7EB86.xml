for 
<taxonomicName id="B6DF4D0FFFDAE354FD92F880FC41EB36" authority="Powell, 1951" authorityName="Powell" authorityYear="1951" box="[603,968,1826,1852]" class="Gastropoda" genus="Brookula" higherTaxonomySource="CoL" kingdom="Animalia" order="Seguenziida" pageId="12" pageNumber="199" phylum="Mollusca" rank="species" species="pfefferi">
