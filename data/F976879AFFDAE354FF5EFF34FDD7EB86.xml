<quantity id="B6279B69FFDAE354FBF8F8E9FB04EB6E" box="[1073,1165,1866,1892]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="12" pageNumber="199" unit="mm" value="2.5">2.5 mm</quantity>
