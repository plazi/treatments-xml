<taxonomicName id="B6DF4D0FFFDAE354FB1BF908FEBFEAE6" authority="Powell, 1951" authorityName="Powell" authorityYear="1951" class="Gastropoda" genus="Brookula" higherTaxonomySource="CoL" kingdom="Animalia" order="Seguenziida" pageId="12" pageNumber="199" phylum="Mollusca" rank="species" species="pfefferi">
