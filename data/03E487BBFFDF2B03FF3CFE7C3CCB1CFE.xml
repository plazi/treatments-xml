<paragraph id="8BF236ADFFDF2B03FF3CFE7C3AC61D40" blockId="12.[151,913,487,513]" box="[151,913,487,513]" pageId="12" pageNumber="13">
<heading id="D0BA81C1FFDF2B03FF3CFE7C3AC61D40" bold="true" box="[151,913,487,513]" fontSize="11" level="1" pageId="12" pageNumber="13" reason="1">
<emphasis id="B939EABFFFDF2B03FF3CFE7C3AC61D40" bold="true" box="[151,913,487,513]" pageId="12" pageNumber="13">
