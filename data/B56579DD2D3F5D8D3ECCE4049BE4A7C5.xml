<taxonomicName id="A4EAF610C1DA779218A17D31E297252D" authorityName="Recluz" authorityYear="1841" class="Gastropoda" family="Neritidae" genus="Neritina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Archaeogastropoda" pageId="1" pageNumber="12" phylum="Mollusca" rank="species" species="anatolica" subGenus="Neritaea">
