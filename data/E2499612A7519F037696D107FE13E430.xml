<taxonomicName id="ADE05C87A7519F037716D107FD2AE53C" authority="Topsent, 1893" authorityName="Topsent" authorityYear="1893" box="[279,743,1089,1116]" class="Demospongiae" family="Microcionidae" genus="Clathria" kingdom="Animalia" order="Poecilosclerida" pageId="2" pageNumber="502" phylum="Porifera" rank="subGenus" subGenus="Axosuberites">
