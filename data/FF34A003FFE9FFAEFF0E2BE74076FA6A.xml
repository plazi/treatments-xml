[i.e. 
<taxonomicName id="B09D6A96FFE9FFAEFC872D4D4644FAAA" authority="(Liu &amp; Xu) Moller &amp; A. Weber" authorityName="Moller &amp; A. Weber" baseAuthorityName="Liu &amp; Xu" box="[798,1315,1357,1380]" class="Magnoliopsida" family="Gesneriaceae" genus="Wentsaiboea" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="luochengensis">
<emphasis id="45E9CD07FFE9FFAEFC872D4D40A8FAAA" box="[798,975,1357,1380]" italics="true" pageId="3" pageNumber="4">P. luochengensis</emphasis>
