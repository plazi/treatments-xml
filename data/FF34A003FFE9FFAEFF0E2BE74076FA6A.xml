<taxonomicName id="B09D6A96FFE9FFAEFDE12D8D406DFA6A" box="[632,778,1421,1444]" class="Magnoliopsida" family="Gesneriaceae" genus="Chirita" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="linearifolia">
<emphasis id="45E9CD07FFE9FFAEFDE12D8D406DFA6A" box="[632,778,1421,1444]" italics="true" pageId="3" pageNumber="4">P. linearifolia</emphasis>
</taxonomicName>
.
