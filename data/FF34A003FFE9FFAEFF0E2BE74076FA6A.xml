<taxonomicName id="B09D6A96FFE9FFAEFE082D6D412AFA4A" box="[401,589,1389,1412]" class="Magnoliopsida" family="Gesneriaceae" genus="Wentsaiboea" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="luochengensis">
<emphasis id="45E9CD07FFE9FFAEFE082D6D412AFA4A" box="[401,589,1389,1412]" italics="true" pageId="3" pageNumber="4">C. luochengensis</emphasis>
</taxonomicName>
to 
<taxonomicName id="B09D6A96FFE9FFAEFDE52D6D418FFA4A" box="[636,744,1389,1412]" class="Magnoliopsida" family="Gesneriaceae" genus="Primulina" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="genus">
<emphasis id="45E9CD07FFE9FFAEFDE52D6D418FFA4A" box="[636,744,1389,1412]" italics="true" pageId="3" pageNumber="4">Primulina</emphasis>
</taxonomicName>
. The new name 
<taxonomicName id="B09D6A96FFE9FFAEFC282D6D478AFA4A" authorityName="W. B. Xu &amp; K. F. Chung" authorityYear="2012" box="[945,1261,1389,1412]" class="Magnoliopsida" family="Gesneriaceae" genus="Primulina" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="pseudolinearifolia">
