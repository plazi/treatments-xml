[i.e. 
<taxonomicName id="B09D6A96FFE9FFAEFDE52D0D40D2FAEA" authority="(Wang) Wang" authorityName="Wang" baseAuthorityName="Wang" box="[636,949,1293,1316]" class="Magnoliopsida" family="Gesneriaceae" genus="Chirita" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="linearifolia">
