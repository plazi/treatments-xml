<paragraph id="4A10293BF5D30B01048EC9A5C8CECE63" pageId="493" pageNumber="494">
Genus 
<taxonomicName id="9E6151284014F5E934BADAED0870D62D" LSID="7210A080-B0EC-4245-B5D3-92516495FAB5" authority="Motschulsky, 1866" authorityName="Motschulsky" authorityYear="1866" class="Insecta" family="Carabidae" genus="Stolonis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Stolonis" order="Coleoptera" pageId="493" pageNumber="494" phylum="Arthropoda" rank="genus">Stolonis Motschulsky, 1866</taxonomicName>
