<taxonomicName id="4C1E4D19FF94CD7DFEFEEE0DB359EFC4" box="[293,554,1655,1677]" class="Diplopoda" family="Anthroleucosomatidae" genus="Georgiosoma" kingdom="Animalia" order="Chordeumatida" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="bicornutum">
