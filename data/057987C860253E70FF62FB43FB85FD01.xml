, 
<taxonomicName id="4AD04D5D60243E73FC14F86CFB5FF82C" box="[993,1152,1976,1999]" class="Insecta" family="Miridae" genus="Dryophilocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="56" phylum="Arthropoda" rank="species" species="kanyukovae">
