, 
<taxonomicName id="4AD04D5D60243E73FF62F808FEFEF810" box="[151,289,2012,2035]" class="Insecta" family="Miridae" genus="Dryophilocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="56" phylum="Arthropoda" rank="species" species="miyamotoi">
