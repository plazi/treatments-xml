<number id="3B1824F960243E73FB78F863FB0DF82D" box="[1165,1234,1975,1998]" pageId="5" pageNumber="56" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
, 
<taxonomicName id="4AD04D5D60243E73FB28F86CFA9AF82C" box="[1245,1349,1976,1999]" class="Insecta" family="Miridae" genus="Dryophilocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="56" phylum="Arthropoda" rank="species" species="lucidus">
