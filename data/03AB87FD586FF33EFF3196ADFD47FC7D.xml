<quantity id="4CFA9B0E586EF33EFB9490D8FB83FF21" box="[1074,1150,152,179]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" pageId="5" pageNumber="327" unit="mm" value="15.0">15 µm</quantity>
