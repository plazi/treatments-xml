<taxonomicName id="905C3DAD988AFF7F1A669D5C05F99747" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33744" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole flavens Roger" lsidName-HNS="Pheidole flavens Roger" order="Hymenoptera" pageNumber="461" phylum="Arthropoda" rank="species" species="flavens">flavens</taxonomicName>
