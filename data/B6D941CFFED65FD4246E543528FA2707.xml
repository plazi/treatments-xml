<taxonomicName id="5981A0353B680EB3E47E269CC29FCB1E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33901" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole nuculiceps Wheeler" lsidName-HNS="Pheidole nuculiceps Wheeler" order="Hymenoptera" pageNumber="461" phylum="Arthropoda" rank="species" species="nuculiceps">nuculiceps</taxonomicName>
