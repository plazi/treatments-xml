<taxonomicName id="1E1388F00B456A7AC1C172DF6E6AFB9D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33912" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole orbica Forel" lsidName-HNS="Pheidole orbica Forel" order="Hymenoptera" pageNumber="461" phylum="Arthropoda" rank="species" species="orbica">orbica</taxonomicName>
, 
<taxonomicName id="DDF4EC34EFD33A9B18CE281B13A7F6F3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181923" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole pholeops Wilson" lsidName-HNS="Pheidole pholeops Wilson" order="Hymenoptera" pageNumber="461" phylum="Arthropoda" rank="species" species="pholeops">pholeops</taxonomicName>
