<collectingCountry id="E8A0B59BB55AEB7DB02110F7FE73F08C" box="[359,468,1211,1237]" name="Indonesia" pageId="5" pageNumber="524">Indonesia</collectingCountry>
(
<collectingRegion id="52733BE9B55AEB7DB0A510F0FDCAF08F" box="[483,621,1212,1238]" country="Indonesia" name="Papua Barat" pageId="5" pageNumber="524">West Papua</collectingRegion>
).
</paragraph>
