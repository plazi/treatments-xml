<collectingRegion id="49C2F827C434B543FE9A8088FEE0BA95" box="[271,359,903,929]" country="China" name="Jiangxi" pageId="11" pageNumber="81">Jiangxi</collectingRegion>
, 
<collectingRegion id="49C2F827C434B543FEEE8088FE5BBA95" box="[379,476,903,929]" country="China" name="Shaanxi" pageId="11" pageNumber="81">Shaanxi</collectingRegion>
