<taxonomicName id="4C0F4D457B1C0362EF60FE39FE31FE50" box="[351,503,404,429]" class="Magnoliopsida" family="Myricaceae" genus="Myrica" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="species" species="rubra">
