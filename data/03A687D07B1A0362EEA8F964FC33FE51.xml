(
<taxonomicName id="4C0F4D457B1C0362EC3AFE38FD49FE50" authorityName="" authorityYear="1817" box="[517,655,404,429]" class="Magnoliopsida" family="Myricaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="family">Myricaceae</taxonomicName>
); 
<emphasis id="B97BEAD47B1C0362EC9DFE39FC98FE50" box="[674,862,404,429]" italics="true" pageId="100" pageNumber="101">Syzygium hancei</emphasis>
(
<taxonomicName id="4C0F4D457B1C0362ED53FE38FC2FFE51" authorityName="" authorityYear="1789" box="[876,1001,404,428]" class="Magnoliopsida" family="Myrtaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Myrtales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
