<paragraph id="C5167BE6B6C0A37CE6FB07C03B1884AE" pageId="null" pageNumber="917">
Soldanelle, 
<normalizedToken id="7FEA1756C18D835CE257B79430B0C943" originalValue="Alpenglöckchen" pageId="null" pageNumber="917">Alpengloeckchen</normalizedToken>
</paragraph>
