<emphasis id="BDF5EAC63677FFE2FCB52E40FC82FE92" box="[869,894,332,355]" italics="true" pageId="5" pageNumber="372">ve</emphasis>
, 
<emphasis id="BDF5EAC63677FFE2FC5C2E40FC63FE92" box="[908,927,332,355]" italics="true" pageId="5" pageNumber="372">si</emphasis>
and 
<emphasis id="BDF5EAC63677FFE2FC072E47FC0DFE92" box="[983,1009,331,355]" italics="true" pageId="5" pageNumber="372">
c 
<number id="394924F33677FFE2FC352E47FC0DFE92" box="[997,1009,331,355]" pageId="5" pageNumber="372" value="1.0">1</number>
</emphasis>
