FEMALE (
<typeStatus id="503A88763677FFE2FE602E0EFDE4FEEB" box="[432,536,258,282]" pageId="5" pageNumber="372">holotype</typeStatus>
). 
<taxonomicName id="48814D573677FFE2FDFB2E0FFD46FEEA" box="[555,698,259,283]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="5" pageNumber="372" phylum="Nematoda" rank="genus">
