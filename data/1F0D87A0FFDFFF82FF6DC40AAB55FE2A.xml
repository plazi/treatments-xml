<taxonomicName id="50A44D35FFDCFF83FE36C44FAD29F89C" box="[460,625,1832,1854]" class="Insecta" family="Empididae" genus="Asymphyloptera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="555" phylum="Arthropoda" rank="genus">
