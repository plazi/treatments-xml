<quantity id="4C9A61985740FF921C93F9DAFDDB6804" box="[447,554,1552,1578]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="2.0" metricValueMin="1.5" pageId="34" pageNumber="35" unit="m" value="17.5" valueMax="20.0" valueMin="15.0">
15– 
