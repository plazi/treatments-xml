<taxonomicName id="39C6B6A3FFEAFFB0FC14768DDC09FC42" authorityName="PACE" authorityYear="1998" box="[927,1137,937,961]" class="Insecta" family="Staphylinidae" genus="Zyras" kingdom="Animalia" order="Coleoptera" pageId="91" pageNumber="104" phylum="Arthropoda" rank="species" species="shaanxiensis">
