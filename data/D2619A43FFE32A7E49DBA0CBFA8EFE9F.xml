(Meigen)
</taxonomicName>
; 
<emphasis id="68BCF747FFE22A794D46A32DFBD1F8BA" bold="true" box="[1034,1068,1868,1892]" pageId="27" pageNumber="28">67:</emphasis>
mine in 
<taxonomicName id="9DC850D6FFE22A794DC6A32DFA61F8BA" box="[1162,1436,1868,1892]" class="Magnoliopsida" family="Caryophyllaceae" genus="Silene" kingdom="Plantae" order="Caryophyllales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="subSpecies" species="latifolia" subSpecies="alba">
