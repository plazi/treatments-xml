<taxonomicName id="9DC850D6FFE22A794B4AA32DFC03F8BA" authority="(Meigen)" baseAuthorityName="Meigen" baseAuthorityYear="1830" box="[518,1022,1868,1892]" class="Insecta" family="Agromyzidae" genus="Amauromyza" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="flavifrons" subGenus="Cephalomyza">
