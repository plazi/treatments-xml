<taxonomicName id="4C4475C6FFC9FFCDFE79FD3D8E64FD3B" authorityName="Zannou, Moraes &amp; Oliveira" authorityYear="2008" box="[473,566,672,697]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="grewiae">
