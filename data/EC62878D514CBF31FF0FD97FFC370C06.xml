<geoCoordinate id="01FF505C514DBF31FB0DD89EFB610D96" box="[1173,1272,690,716]" degrees="22" direction="north" minutes="42" orientation="latitude" pageId="9" pageNumber="178" precision="925" value="22.7">22°42’N</geoCoordinate>
