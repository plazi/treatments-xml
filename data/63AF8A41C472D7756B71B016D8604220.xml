<taxonomicName id="8835B3182233A8EA86C2C0763440C9F9" class="Polychaeta" family="Nephtyidae" genus="Nephtys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nephtys pulchra" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="pulchra">Nephtys pulchra</taxonomicName>
