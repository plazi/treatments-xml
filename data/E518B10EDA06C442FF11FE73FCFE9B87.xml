<quantity id="AA49ADFDDA06C440FD91FC5AFD089D17" box="[535,662,1010,1035]" metricMagnitude="2" metricUnit="m" metricValue="4.17" metricValueMax="5.1" metricValueMin="3.2399999999999998" pageId="12" pageNumber="267" unit="m" value="417.0" valueMax="510.0" valueMin="324.0">324–510 m</quantity>
