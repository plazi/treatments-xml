<taxonomicName id="8AFC4D518277FFCB0FE9CDEA407C6038" box="[412,541,764,790]" class="Insecta" family="Piophilidae" genus="Prochyliza" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="283" phylum="Arthropoda" rank="genus">
