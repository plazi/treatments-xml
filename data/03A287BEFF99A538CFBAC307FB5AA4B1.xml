<taxonomicName id="4C0B4D2BFF98A538CFD4C250FDDEA439" authority="Fabricius" authorityName="Fabricius" authorityYear="1798" box="[249,583,512,538]" class="Malacostraca" family="Penaeidae" genus="Penaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="monodon">
<emphasis id="B97FEABAFF98A538CFD4C250FE48A43A" box="[249,465,512,537]" italics="true" pageId="33" pageNumber="34">Penaeus monodon</emphasis>
