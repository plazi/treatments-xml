<taxonomicName id="4C0B4D2BFF98A538CD10C188FD4BA7D2" authorityName="Reeve" authorityYear="1843" box="[573,722,472,497]" class="Gastropoda" family="Conidae" genus="Conus" kingdom="Animalia" order="Neogastropoda" pageId="33" pageNumber="34" phylum="Mollusca" rank="species" species="inscriptus">
<emphasis id="B97FEABAFF98A538CD10C188FD4BA7D2" box="[573,722,472,497]" italics="true" pageId="33" pageNumber="34">C. inscriptus</emphasis>
</taxonomicName>
