<collectingRegion id="49CFF84AFF98A538CFE9C1E0FEDEA7E9" box="[196,327,432,458]" country="India" name="Tamil Nadu" pageId="33" pageNumber="34">TamilNadu</collectingRegion>
<collectingRegion id="49CFF84AFF98A538CE7FC1E0FE0DA7E9" box="[338,404,432,458]" country="New Zealand" name="West Coast" pageId="33" pageNumber="34">Coast</collectingRegion>
(
<tableCitation id="C6890313FF98A538CE8BC1E0FD9DA7E9" box="[422,516,432,458]" captionStart="TABLE 6" captionStartId="12.[151,236,198,220]" captionText="TABLE 6. Presence () of Conus species and total number of species per sampling site." pageId="33" pageNumber="34">Table 6</tableCitation>
