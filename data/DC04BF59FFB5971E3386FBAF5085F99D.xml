<quantity id="9355A3AAFFB597133192FA725059F9D7" box="[643,818,1530,1554]" metricMagnitude="-3" metricUnit="m" metricValue="1.89" metricValueMax="1.91" metricValueMin="1.87" pageId="5" pageNumber="41" unit="mm" value="1.89" valueMax="1.91" valueMin="1.87">1.87–1.91 mm</quantity>
