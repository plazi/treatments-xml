<taxonomicName id="74814D12D55AFFE2251F0722396A246B" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="287" phylum="Arthropoda" rank="species" species="perissochaeta">
<emphasis id="81F5EA83D55AFFE2251F0722396A246B" italics="true" pageId="30" pageNumber="287">M. perissochaeta</emphasis>
</taxonomicName>
