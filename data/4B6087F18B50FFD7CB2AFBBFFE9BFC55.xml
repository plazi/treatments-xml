<paragraph id="C37636E78B50FFD1CA38FA98FC9EFAF3" blockId="26.[125,1172,1309,1364]" pageId="26" pageNumber="632">
<materialsCitation id="73A13CBA8B50FFD1CA38FA98FC86FAF3" collectingDate="1892-01-22" collectionCode="MHNG" country="Italy" location="Mediterranean" municipality="Island of Capri" pageId="26" pageNumber="632" specimenCode="INVE 34237" specimenCount="1">
