<specimenCode id="936F9E9C8B50FFD1CAA3FA98FE6BFA92" box="[230,386,1309,1333]" collectionCode="INVE" pageId="26" pageNumber="632">INVE 34237</specimenCode>
, 
<location id="C616603C8B50FFD1CBD5FA98FDD6FA92" LSID="urn:lsid:plazi:treatment:4B6087F18B50FFD7CB2AFBBFFE9BFC55:C616603C8B50FFD1CBD5FA98FDD6FA92" box="[400,575,1309,1333]" country="Italy" municipality="Island of Capri" name="Mediterranean" pageId="26" pageNumber="632">Mediterranean</location>
, 
<collectingCountry id="BBDE76778B50FFD1C80BFA98FD6CFA92" box="[590,645,1309,1333]" name="Italy" pageId="26" pageNumber="632">Italy</collectingCountry>
, 
<collectingMunicipality id="2312AC9D8B50FFD1C8D6FA98FCA3FA92" box="[659,842,1309,1333]" pageId="26" pageNumber="632">Island of Capri</collectingMunicipality>
, coll. 
<date id="B77710278B50FFD1C9D6FA98FBB2FA92" box="[915,1115,1309,1333]" pageId="26" pageNumber="632" value="1892-01-22">
