[SAM­H 5080]; C–D. 
<taxonomicName id="F5944D135926FFA3FB61F8A1FEB1F8A4" class="Demospongiae" family="Latrunculiidae" genus="Cyclacanthia" kingdom="Animalia" order="Poecilosclerida" pageId="10" pageNumber="11" phylum="Porifera" rank="species" species="mzimayiensis" status="sp. nov.">
<emphasis id="00E0EA825926FFA3FB61F8A1FEB1F8A4" italics="true" pageId="10" pageNumber="11">C. mzimayiensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="1BD357F95926FFA3FEC5F883FE0BF8A7" box="[376,459,1854,1877]" pageId="10" pageNumber="11" rank="species">sp. nov.</taxonomicNameLabel>
