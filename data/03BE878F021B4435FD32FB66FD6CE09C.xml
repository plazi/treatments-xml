<taxonomicName id="4C174D1A021B4435FCA2FAD0FC62E0FC" authorityName="Meuffels &amp; Grootaert" authorityYear="2004" box="[825,916,898,922]" class="Insecta" family="Dolichopodidae" genus="Teuchophorus" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="173" phylum="Arthropoda" rank="species" species="obliquus">
<emphasis id="B963EA8B021B4435FCA2FAD0FC62E0FC" box="[825,916,898,922]" italics="true" pageId="31" pageNumber="173">obliquus</emphasis>
