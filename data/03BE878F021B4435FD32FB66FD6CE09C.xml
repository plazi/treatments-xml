<taxonomicName id="4C174D1A021B4435FD43FB61FCC1E12D" authorityName="Meuffels &amp; Grootaert" authorityYear="2004" box="[728,823,563,587]" class="Insecta" family="Dolichopodidae" genus="Teuchophorus" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="173" phylum="Arthropoda" rank="species" species="obscurus">
<emphasis id="B963EA8B021B4435FD43FB61FCC1E12D" bold="true" box="[728,823,563,587]" italics="true" pageId="31" pageNumber="173">obscurus</emphasis>
</taxonomicName>
