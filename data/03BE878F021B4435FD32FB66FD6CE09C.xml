<collectingCountry id="F3007609021B4435FDD0FAD0FD44E0FC" box="[587,690,898,922]" name="Thailand" pageId="31" pageNumber="173">Thailand</collectingCountry>
(
<taxonomicName id="4C174D1A021B4435FD5CFAD0FCD0E0FC" authorityName="Meuffels &amp; Grootaert" authorityYear="2004" box="[711,806,898,922]" class="Insecta" family="Dolichopodidae" genus="Teuchophorus" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="173" phylum="Arthropoda" rank="species" species="obscurus">
<emphasis id="B963EA8B021B4435FD5CFAD0FCD0E0FC" box="[711,806,898,922]" italics="true" pageId="31" pageNumber="173">obscurus</emphasis>
</taxonomicName>
