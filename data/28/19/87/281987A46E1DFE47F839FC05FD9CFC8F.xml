<emphasis bold="true" box="[264,782,924,950]" pageId="1" pageNumber="2">
<taxonomicName authority="Marinoni &amp; Knutson" authorityName="Marinoni &amp; Knutson" box="[264,639,924,950]" class="Insecta" family="Sciomyzidae" genus="Neuzina" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
