(
<collectionCode box="[196,252,521,542]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15519" lsid="urn:lsid:biocol.org:col:15519" name="Université Montpellier 2" pageId="15" pageNumber="132" type="Herbarium">MPU</collectionCode>
