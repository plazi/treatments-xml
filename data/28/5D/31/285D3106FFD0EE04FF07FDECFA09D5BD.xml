(
<bibRefCitation author="Pic, M." box="[1337,1429,1071,1093]" pageId="5" pageNumber="106" pagination="172" refId="ref33317" refString="Pic, M. (1902) Description de Coleopteres Nouveaux. Bruchidae de l'Amerique Meridionale. Naturaliste, 24, 172." type="journal article" year="1902">Pic, 1902</bibRefCitation>
)
