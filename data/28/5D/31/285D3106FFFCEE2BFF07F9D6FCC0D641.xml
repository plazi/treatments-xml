, 
<collectingRegion box="[999,1114,1737,1764]" country="Brazil" name="Sao Paulo" pageId="42" pageNumber="143">São Paulo</collectingRegion>
) and 
<collectingCountry box="[1176,1231,1737,1763]" name="Peru" pageId="42" pageNumber="143">Peru</collectingCountry>
