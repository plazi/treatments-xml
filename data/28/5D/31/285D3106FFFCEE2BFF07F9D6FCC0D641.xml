<collectingCountry box="[480,542,1737,1763]" name="Cuba" pageId="42" pageNumber="143">Cuba</collectingCountry>
, 
<collectingCountry box="[553,665,1737,1763]" name="Honduras" pageId="42" pageNumber="143">Honduras</collectingCountry>
, 
<collectingCountry box="[676,744,1737,1763]" name="Brazil" pageId="42" pageNumber="143">Brazil</collectingCountry>
(
<collectingRegion box="[759,909,1737,1763]" country="Brazil" name="Mato Grosso" pageId="42" pageNumber="143">Mato Grosso</collectingRegion>
, 
