<typeStatus box="[363,411,981,1005]" pageId="15" pageNumber="46">type</typeStatus>
specimens (
<tableCitation box="[549,636,980,1005]" captionStart="TABLE 7" captionStartId="15.[151,239,1750,1773]" captionTargetBox="[159,1186,1801,1947]" captionTargetPageId="15" captionText="TABLE 7. Comparison of Rumanian and Turkish specimens of Z. similifoveolatus." httpUri="http://table.plazi.org/id/F4A7B9C94C46FFA2FF4DCDEEFBE2ADD6" pageId="15" pageNumber="46" tableUuid="F4A7B9C94C46FFA2FF4DCDEEFBE2ADD6">
Table 
<number box="[620,636,980,1004]" pageId="15" pageNumber="46" value="7.0">7</number>
</tableCitation>
