<subSubSection pageId="12" pageNumber="43" type="discussion">
<paragraph blockId="12.[151,1437,151,320]" pageId="12" pageNumber="43">
<emphasis bold="true" box="[199,315,260,285]" pageId="12" pageNumber="43">Remarks.</emphasis>
