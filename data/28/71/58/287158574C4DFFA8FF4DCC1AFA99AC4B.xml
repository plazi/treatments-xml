Balogh
</taxonomicName>
, 
<taxonomicName authority="Athias-Henriot" authorityName="Athias-Henriot" authorityYear="1836" box="[438,732,1807,1832]" class="Arachnida" family="Zerconidae" genus="Zercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="36" phylum="Arthropoda" rank="species" species="cabylus">
