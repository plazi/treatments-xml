<subSubSection pageId="7" pageNumber="8" type="discussion">
<paragraph blockId="7.[151,1437,151,788]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[199,335,691,716]" pageId="7" pageNumber="8">Comments.</emphasis>
