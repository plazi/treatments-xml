<number box="[638,669,1614,1639]" pageId="4" pageNumber="588" value="21.0">21</number>
(
<number box="[678,707,1614,1639]" pageId="4" pageNumber="588" value="15.0">15</number>
): 
<number box="[727,759,1614,1639]" pageId="4" pageNumber="588" value="52.0">52</number>
(
