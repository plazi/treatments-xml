<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Senna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Senna" order="Fabales" pageId="0" pageNumber="609" phylum="Tracheophyta" rank="genus">
