<collectingRegion box="[945,1028,1170,1192]" country="Panama" name="Chiriqui" pageId="31" pageNumber="32">Chiriquí</collectingRegion>
; on fallen branch of forest tree, 
<quantity metricMagnitude="3" metricUnit="m" metricValue="1.55" metricValueMax="1.65" metricValueMin="1.45" pageId="31" pageNumber="32" unit="m" value="1550.0" valueMax="1650.0" valueMin="1450.0">1450–1650 m</quantity>
, 
<emphasis box="[221,405,1206,1227]" italics="true" pageId="31" pageNumber="32">
Maxon, 
<collectionCode box="[300,321,1206,1227]" country="Austria" lsid="urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="31" pageNumber="32" type="Herbarium">W</collectionCode>
. R. 5627
</emphasis>
(
<accessionNumber box="[419,529,1206,1228]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/US676072" pageId="31" pageNumber="32">
