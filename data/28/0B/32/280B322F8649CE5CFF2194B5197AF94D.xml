<emphasis box="[352,470,1547,1571]" italics="true" pageId="6" pageNumber="587">
<taxonomicName box="[352,462,1547,1571]" class="Insecta" family="Rhyparochromidae" genus="Vertomannus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="587" phylum="Arthropoda" rank="species" species="tibetanus">tibetanus</taxonomicName>
