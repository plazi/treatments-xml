<number box="[575,619,1074,1095]" pageId="82" pageNumber="83" value="21.9">21.9</number>
%– 
<number box="[650,692,1074,1095]" pageId="82" pageNumber="83" value="24.1">24.1</number>
% TL; dorsal caudal space 
<number box="[959,1002,1074,1095]" pageId="82" pageNumber="83" value="10.4">10.4</number>
%– 
<number box="[1034,1076,1074,1095]" pageId="82" pageNumber="83" value="10.9">10.9</number>
% TL; pectoral-caudal space 
<number box="[1362,1405,1074,1095]" pageId="82" pageNumber="83" value="20.3">20.3</number>
%– 
<number box="[225,269,1103,1124]" pageId="82" pageNumber="83" value="24.4">24.4</number>
