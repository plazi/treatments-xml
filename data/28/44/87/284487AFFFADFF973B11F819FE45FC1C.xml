<quantity box="[168,249,496,520]" metricMagnitude="-2" metricUnit="m" metricValue="5.5" metricValueMax="7.0" metricValueMin="4.0" pageId="9" pageNumber="78" unit="cm" value="5.5" valueMax="7.0" valueMin="4.0">4–7 cm</quantity>
