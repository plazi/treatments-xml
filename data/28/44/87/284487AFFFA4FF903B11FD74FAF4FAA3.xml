<paragraph blockId="14.[810,1420,710,1362]" pageId="14" pageNumber="83">
<emphasis bold="true" box="[810,1067,710,735]" pageId="14" pageNumber="83">Diagnostic characters.</emphasis>
Stems solitary, erect, 
<quantity box="[1308,1376,710,734]" metricMagnitude="0" metricUnit="m" metricValue="7.5" metricValueMax="9.0" metricValueMin="6.0" pageId="14" pageNumber="83" unit="m" value="7.5" valueMax="9.0" valueMin="6.0">6–9 m</quantity>
tall and 
<quantity box="[856,963,745,769]" metricMagnitude="-1" metricUnit="m" metricValue="2.5" metricValueMax="3.0" metricValueMin="2.0" pageId="14" pageNumber="83" unit="cm" value="25.0" valueMax="30.0" valueMin="20.0">20–30 cm</quantity>
in diameter, grey. Leaf costapalmate, 
<quantity metricMagnitude="0" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="14" pageNumber="83" unit="m" value="3.5" valueMax="4.0" valueMin="3.0">3–4 m</quantity>
long; petiole 
<quantity box="[986,1077,779,803]" metricMagnitude="0" metricUnit="m" metricValue="2.25" metricValueMax="2.5" metricValueMin="2.0" pageId="14" pageNumber="83" unit="m" value="2.25" valueMax="2.5" valueMin="2.0">2–2.5 m</quantity>
long, black fibre at base, mar- gin spiny, ca 
<quantity box="[958,1013,813,837]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="14" pageNumber="83" unit="cm" value="2.0">2 cm</quantity>
long; lamina ca 
<quantity box="[1201,1263,813,837]" metricMagnitude="0" metricUnit="m" metricValue="1.3" pageId="14" pageNumber="83" unit="m" value="1.3">1.3 m</quantity>
long. Leaflets 
<quantity box="[810,918,847,871]" metricMagnitude="0" metricUnit="m" metricValue="2.413" metricValueMax="2.54" metricValueMin="2.286" pageId="14" pageNumber="83" unit="in" value="95.0" valueMax="100.0" valueMin="90.0">90–100 in</quantity>
number, ca 
<quantity box="[1051,1118,847,871]" metricMagnitude="-1" metricUnit="m" metricValue="6.0" pageId="14" pageNumber="83" unit="cm" value="60.0">60 cm</quantity>
long, apices bifid, ca 
<quantity box="[1353,1419,847,871]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="14" pageNumber="83" unit="cm" value="30.0">30 cm</quantity>
long, acuminate and dropping. Inflorescence 
<quantity box="[1312,1420,881,905]" metricMagnitude="0" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="14" pageNumber="83" unit="m" value="1.4" valueMax="1.5" valueMin="1.3">1.3–1.5 m</quantity>
long; primary branches 
<quantity box="[1073,1142,915,939]" metricMagnitude="-1" metricUnit="m" metricValue="1.651" metricValueMax="1.778" metricValueMin="1.524" pageId="14" pageNumber="83" unit="in" value="6.5" valueMax="7.0" valueMin="6.0">6–7 in</quantity>
number, 
<quantity box="[1247,1357,915,939]" metricMagnitude="-1" metricUnit="m" metricValue="3.75" metricValueMax="4.5" metricValueMin="3.0" pageId="14" pageNumber="83" unit="cm" value="37.5" valueMax="45.0" valueMin="30.0">30–45 cm</quantity>
long; bracts tubular, 
<quantity box="[986,1100,949,973]" metricMagnitude="-1" metricUnit="m" metricValue="2.25" metricValueMax="2.5" metricValueMin="2.0" pageId="14" pageNumber="83" unit="cm" value="22.5" valueMax="25.0" valueMin="20.0">20–25 cm</quantity>
long, indumentums brown. Fruits ellipsoid, bluish-green, 
<quantity box="[1134,1234,983,1007]" metricMagnitude="-2" metricUnit="m" metricValue="2.25" metricValueMax="2.5" metricValueMin="2.0" pageId="14" pageNumber="83" unit="cm" value="2.25" valueMax="2.5" valueMin="2.0">2–2.5 cm</quantity>
long.
</paragraph>
