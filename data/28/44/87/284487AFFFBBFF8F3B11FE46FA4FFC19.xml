<quantity box="[1057,1161,512,536]" metricMagnitude="-1" metricUnit="m" metricValue="1.4" metricValueMax="1.6" metricValueMin="1.2" pageId="17" pageNumber="86" unit="cm" value="14.0" valueMax="16.0" valueMin="12.0">12–16 cm</quantity>
