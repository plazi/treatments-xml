<quantity id="250AB289DA4AFFD13E04F9749881F929" box="[1081,1171,1711,1736]" metricMagnitude="1" metricUnit="m" metricValue="2.8" metricValueMax="5.5" metricValueMin="0.1" pageId="93" pageNumber="94" unit="m" value="28.0" valueMax="55.0" valueMin="1.0">
