<taxonomicName id="4C264D1DFF9EFFF6DBC9FC9BFD82208F" baseAuthorityName="Gray" baseAuthorityYear="1856" box="[400,535,1996,2021]" class="Reptilia" family="Chelidae" genus="Mesoclemmys" kingdom="Animalia" order="Testudines" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="raniceps">
