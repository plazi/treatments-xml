<paragraph id="982636B4CF11FFBFFF34FABB37674E5C" blockId="11.[151,566,1289,1344]" box="[151,566,1321,1344]" pageId="11" pageNumber="12">
<treatmentCitationGroup id="B889119ACF11FFBFFF34FABB37674E5C" box="[151,566,1321,1344]" pageId="11" pageNumber="12">
<taxonomicName id="5F994D37CF11FFBFFF34FABB37634E5C" authority="Oldroyd, 1972: 255" authorityName="Oldroyd" authorityPageNumber="255" authorityYear="1972" box="[151,562,1321,1344]" class="Insecta" family="Asilidae" genus="Xenomyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="andron">
<emphasis id="AAEDEAA6CF11FFBFFF34FABB34064E5C" box="[151,343,1321,1344]" italics="true" pageId="11" pageNumber="12">Xenomyza andron</emphasis>
<treatmentCitation id="193810A5CF11FFBFFEFDFABB37634E5C" author="Oldroyd" box="[350,562,1321,1344]" page="255" pageId="11" pageNumber="12" year="1972">
<bibRefCitation id="FC084B45CF11FFBFFEFDFABB34AC4E5C" author="Oldroyd" box="[350,509,1321,1344]" pageId="11" pageNumber="18" refString="Oldroyd, H. (1972) Robber flies (Diptera, Asilidae) of the Philippine Islands. Pacific Insects, 14 (2), 201 - 337." type="journal article" year="1972">
Oldroyd, 
<number id="2E512493CF11FFBFFE67FABB34AC4E5C" box="[452,509,1321,1344]" pageId="11" pageNumber="12" value="1972.0">1972</number>
</bibRefCitation>
