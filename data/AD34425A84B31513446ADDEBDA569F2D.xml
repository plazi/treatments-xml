<paragraph id="2E5F5561F1D6B60094EC904C09256925" pageId="0" pageNumber="6900">
<taxonomicName id="06ADEFD615ED7A857CD799483528D680" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. hearseyi" order="Biddulphiales" pageId="0" pageNumber="6900" phylum="Diatomeae" rank="species" species="hearseyi">B. hearseyi</taxonomicName>
