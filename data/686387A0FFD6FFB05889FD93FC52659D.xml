<taxonomicName id="27CA4D35FFD6FFB05A40FCEEFD556205" authorityName="Gonzalez et al." authorityYear="2018" baseAuthorityName="Philippi" baseAuthorityYear="1865" box="[606,742,867,892]" class="Insecta" family="Acroceridae" genus="Lasia" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="aenea">
