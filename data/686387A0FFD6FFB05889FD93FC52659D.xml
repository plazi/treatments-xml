<taxonomicName id="27CA4D35FFD6FFB05A41FC26FC1662BC" authority="(Philippi, 1865)" authorityName="Gonzalez et al." authorityYear="2018" baseAuthorityName="Philippi" baseAuthorityYear="1865" box="[607,933,938,965]" class="Insecta" family="Acroceridae" genus="Lasia" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="aenea">
<emphasis id="D2BEEAA4FFD6FFB05A41FC26FD5A62BD" box="[607,745,939,964]" italics="true" pageId="27" pageNumber="28">Lasia aenea</emphasis>
(
<bibRefCitation id="845B4B47FFD6FFB05AE7FC27FC2E62BC" author="Philippi, R. A." box="[761,925,938,965]" pageId="27" pageNumber="28" pagination="595 - 782" refId="ref363865" refString="Philippi, R. A. (1865) Aufzahlung der chilenischen Diptera. Verhandlungen der Kaiserlich - Koniglichen Zoologisch - Botanischen Gesellschaft in Wien, 15, 595 - 782." type="journal article" year="1865">Philippi, 1865</bibRefCitation>
)
</taxonomicName>
as a junior synonym of 
<taxonomicName id="27CA4D35FFD6FFB05CA0FC26FDB56290" authority="Westwood. Dunning (1877: 261)" authorityName="Westwood. Dunning" authorityPageNumber="261" authorityYear="1877" class="Insecta" family="Acroceridae" genus="Lasia" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="rufipes">
