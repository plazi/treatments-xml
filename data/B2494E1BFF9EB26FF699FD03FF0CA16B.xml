<taxonomicName id="FDE0848EFF9EB26CF705FCCFFBB9A611" authority="Jenkins, 1908" authorityName="Jenkins" authorityYear="1908" box="[844,1117,771,795]" class="Calcarea" family="Achramorphidae" genus="Achramorpha" kingdom="Animalia" order="Leucosolenida" pageId="37" pageNumber="240" phylum="Porifera" rank="genus">
