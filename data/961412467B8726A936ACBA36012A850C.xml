<taxonomicName id="C8AC71EC7560B877C34C42D0D040D94C" authority="Westwood, 1839" authorityName="Westwood" authorityYear="1839" class="Insecta" family="Tenthredinidae" genus="Fenella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Fenella nigrita" order="Hymenoptera" pageId="2" pageNumber="85" phylum="Arthropoda" rank="species" species="nigrita">Fenella nigrita</taxonomicName>
