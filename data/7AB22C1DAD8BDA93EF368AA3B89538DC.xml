, 
<taxonomicName id="E0018890564B78E597352A3C2B9C8B19" class="Gastropoda" family="Plectopylidae" genus="Endothyrella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Endothyrella sowerbyi" order="Stylommatophora" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="sowerbyi">Endothyrella sowerbyi</taxonomicName>
