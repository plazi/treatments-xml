<taxonomicName id="FBEE2FB7A0C4C979CAAB50065FC4BE26" authorityName="Butler" authorityYear="1889" class="Insecta" family="Pyralidae" genus="Pyralis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyralis princeps" order="Lepidoptera" pageId="0" pageNumber="337" phylum="Arthropoda" rank="species" species="princeps">
