<taxonomicName id="DC70125ABDDD24926219C12C42041E6B" authorityName="Butler" authorityYear="1889" class="Insecta" family="Pyralidae" genus="Pyralis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyralis princeps" order="Lepidoptera" pageId="0" pageNumber="337" phylum="Arthropoda" rank="species" species="princeps">
<emphasis id="ED06420B453B63CE96C835B71EED4D3E" italics="true" pageId="0" pageNumber="337">Pyralis princeps</emphasis>
</taxonomicName>
