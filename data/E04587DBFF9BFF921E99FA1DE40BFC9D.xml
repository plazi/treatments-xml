<materialsCitation id="D8843C90FF86FF921FF1FDA7E48CFCED" country="Myanmar" county="Cretaceous" location="Zigras" municipality="Tanai Village on Ledo" pageId="18" pageNumber="19" specimenCount="1" specimenCount-female="1" stateProvince="Kachin" typeStatus="holotype">
<typeStatus id="B757886FFF86FF921FF1FDA7E425FD7D" box="[168,296,636,662]" pageId="18" pageNumber="19">HOLOTYPE</typeStatus>
: 
<specimenCount id="7EEAFD44FF86FF921E6FFDA0E443FD7D" box="[310,334,635,662]" pageId="18" pageNumber="19" type="female">♀</specimenCount>
, (fig. 6B), JZC Bu-60; 
<collectingCounty id="81324E41FF86FF921D0EFDA7E7D4FD7D" box="[599,729,636,662]" pageId="18" pageNumber="19">Cretaceous</collectingCounty>
amber (Cenomanian), 
<collectingCountry id="10FB765DFF86FF921CB7FDA6E16CFD7C" box="[1006,1121,637,663]" name="Myanmar" pageId="18" pageNumber="19">Myanmar</collectingCountry>
, 
