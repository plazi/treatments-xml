<treatmentCitationGroup id="E2A311C1664B47392BFAFA35C78BFA2F" box="[151,1166,1433,1455]" pageId="23" pageNumber="24">
<treatmentCitation id="431210FE664B47392BFAFA35C2E7FA2F" author="Crawford, J. C." box="[151,482,1433,1455]" page="39" pageId="23" pageNumber="24" year="1948">
<taxonomicName id="05B34D6C664B47392BFAFA35C2E7FA2F" authority="Crawford, 1948: 39" authorityName="Crawford" authorityPageNumber="39" authorityYear="1948" box="[151,482,1433,1455]" class="Insecta" family="Phlaeothripidae" genus="Baenothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
<emphasis id="F0C7EAFD664B47392BFAFA35C216FA2F" box="[151,275,1433,1455]" italics="true" pageId="23" pageNumber="24">Baenothrips</emphasis>
<bibRefCitation id="A6224B1E664B47392A74FA35C2E7FA2F" author="Crawford, J. C." box="[281,482,1433,1455]" pageId="23" pageNumber="24" pagination="39 - 40" refId="ref60284" refString="Crawford, J. C. (1948) A new genus of Urothripidae from Guatemala. Proceedings of the Entomological Society of Washington, 50, 39 - 40." type="journal article" year="1948">Crawford, 1948: 39</bibRefCitation>
</taxonomicName>
</treatmentCitation>
