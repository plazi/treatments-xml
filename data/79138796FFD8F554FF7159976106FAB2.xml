<taxonomicName id="36BA4D03FFD8F554FC515ADB60BDFD62" box="[951,1167,657,683]" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="species" species="fulviventris">
