</taxonomicName>
, 
<taxonomicName id="4C6CA1CF664EB436C52F48973CDA22AF" box="[1043,1216,999,1022]" class="Magnoliopsida" family="Brassicaceae" genus="Iberis" kingdom="Plantae" order="Brassicales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="sempervirens">
<emphasis id="B918065E664EB436C52F48973CDA22AF" box="[1043,1216,999,1022]" italics="true" pageId="27" pageNumber="28">I. sempervirens</emphasis>
</taxonomicName>
(
<taxonomicName id="4C6CA1CF664EB436C5EC48963D7E22AE" baseAuthorityName="Korotyaev &amp; Gultekin" baseAuthorityYear="2007" box="[1232,1380,998,1023]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
