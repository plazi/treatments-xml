<taxonomicName id="4C6CA1CF664EB436C0174F7A39AF2572" box="[299,437,1034,1059]" class="Magnoliopsida" family="Resedaceae" kingdom="Plantae" order="Capparales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="family">Resedaceae</taxonomicName>
