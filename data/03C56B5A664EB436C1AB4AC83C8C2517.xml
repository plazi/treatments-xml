<taxonomicName id="4C6CA1CF664EB436C1AB4F7A38F22572" box="[151,232,1034,1059]" class="Magnoliopsida" family="Resedaceae" genus="Reseda" kingdom="Plantae" order="Capparales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E664EB436C1AB4F7A38F22572" box="[151,232,1034,1059]" italics="true" pageId="27" pageNumber="28">Reseda</emphasis>
</taxonomicName>
