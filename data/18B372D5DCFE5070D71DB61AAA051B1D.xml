<taxonomicName id="234C10B2A859D0E7826EC9E5DA536C7B" baseAuthorityName="Garrett" baseAuthorityYear="1872" class="Gastropoda" family="Bothriembryontidae" genus="Callistocharis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="8" pageNumber="9" phylum="Mollusca" rank="species" species="crassilabrum">
<emphasis id="D3EE41F5A148C40BE4FF33E012D117BE" italics="true" pageId="8" pageNumber="9">Callistocharis crassilabrum</emphasis>
