<paragraph id="8BC92654FFA3FF8C43CE5B85FDFEFE86" blockId="2.[151,1437,1313,1665]" box="[199,615,1493,1521]" pageId="2" pageNumber="298">
<emphasis id="B902FA46FFA3FF8C43CE5B85FEF8FE87" bold="true" box="[199,353,1493,1520]" pageId="2" pageNumber="298">Distribution.</emphasis>
<collectingCountry id="F36166C4FFA3FF8C426E5B86FE32FE87" box="[359,427,1494,1520]" name="Brazil" pageId="2" pageNumber="298">Brazil</collectingCountry>
(
<collectingRegion id="49B2E8B6FFA3FF8C42B25B86FDF9FE86" box="[443,608,1494,1521]" country="Brazil" name="Rio de Janeiro" pageId="2" pageNumber="298">Rio de Janeiro</collectingRegion>
