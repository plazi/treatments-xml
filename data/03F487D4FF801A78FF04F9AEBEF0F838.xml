<taxonomicName id="4C5D4D41FF801A78FF04F9D9BFCDF942" authority="Joost, 1981: 188" authorityName="Joost" authorityPageNumber="188" authorityYear="1981" box="[151,716,1657,1679]" class="Insecta" family="Empididae" genus="Wiedemannia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="358" phylum="Arthropoda" rank="species" species="klausnitzeri" subGenus="Philolutra">
<emphasis id="B929EAD0FF801A78FF04F9D9BC20F942" box="[151,289,1657,1679]" italics="true" pageId="7" pageNumber="358">Wiedemannia</emphasis>
(
<emphasis id="B929EAD0FF801A78FEBCF9D9BC96F942" box="[303,407,1657,1679]" italics="true" pageId="7" pageNumber="358">Philolutra</emphasis>
) 
<emphasis id="B929EAD0FF801A78FE35F9D9BF1DF942" box="[422,540,1657,1679]" italics="true" pageId="7" pageNumber="358">klausnitzeri</emphasis>
<treatmentCitation id="0AFC10D3FF801A78FDB0F9DABFCDF942" author="Joost" box="[547,716,1657,1679]" page="188" pageId="7" pageNumber="358" year="1981">
