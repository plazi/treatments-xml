<taxonomicName id="B58B7EB61B38ACEB29D9C0BD90EBA63B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27088" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus putatus Forel" order="Hymenoptera" pageNumber="234" phylum="Arthropoda" rank="species" species="putatus">CAMPONOTUS PUTATUS</taxonomicName>
