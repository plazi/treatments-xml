<taxonomicName id="4C3395F3DD1C047DD3A1F8B2FF24A126" box="[1201,1388,1844,1869]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="deuteronymus">
<emphasis id="B9473262DD1C047DD3A1F8B2FF24A126" box="[1201,1388,1844,1869]" italics="true" pageId="40" pageNumber="41">B. deuteronymus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3395F3DD1C047DD787F8DEFB41A11A" box="[151,265,1880,1905]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="humilis">
<emphasis id="B9473262DD1C047DD787F8DEFB41A11A" box="[151,265,1880,1905]" italics="true" pageId="40" pageNumber="41">B. humilis</emphasis>
</taxonomicName>
(see the key; 
<figureCitation id="1308F2F5DD1C047DD6B4F8DFFBB1A11A" box="[420,505,1880,1905]" captionStart="FIGURE 51. B" captionStartId="37.[151,250,702,724]" captionTargetBox="[157,1436,193,619]" captionTargetId="figure@37.[151,1436,193,619]" captionTargetPageId="37" captionText="FIGURE 51. B. deuteronymus worker visiting Cirsium leo (Asteraceae) in Gansu (PW)." httpUri="https://zenodo.org/record/286587/files/figure.png" pageId="40" pageNumber="41">
Figs 
<number id="3DFBFC57DD1C047DD6CBF8DFFBB1A11A" box="[475,505,1880,1905]" pageId="40" pageNumber="41" value="51.53">51</number>
</figureCitation>
, 
<figureCitation id="1308F2F5DD1C047DD515F8DFF86BA11A" box="[517,547,1880,1905]" captionStart="FIGURES 52 – 53. 52" captionStartId="37.[151,264,1831,1853]" captionTargetBox="[154,1436,1381,1806]" captionTargetId="figure@37.[151,1436,1381,1807]" captionTargetPageId="37" captionText="FIGURES 52 – 53. 52. Map showing the distribution of B. humilis in North China with records as black spots, the province boundaries in dark grey, and all sites with records of all bumblebee species in light grey. 53. B. humilis worker visiting Trifolium pratense (Fabaceae) in Shaanxi." httpUri="https://zenodo.org/record/286588/files/figure.png" pageId="40" pageNumber="41">53</figureCitation>
