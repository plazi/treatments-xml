<quantity id="4CD125EC670F1E7F5EFA6728FDC3FA53" box="[479,558,1402,1426]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="3" pageNumber="844" unit="mm" value="2.1">2.1 mm</quantity>
(
<typeStatus id="549236AB670F1E7F5D196728FD4DFA50" box="[572,672,1402,1426]" pageId="3" pageNumber="844">lectotype</typeStatus>
