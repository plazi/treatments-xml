, 
<taxonomicName id="66EF05A02C8810F119574EF231A0276C" authorityName="Dall" authorityYear="1892" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="157" pageNumber="158" phylum="Mollusca" rank="species" species="habeli">
<emphasis id="604CC9ACF0E2AA82628C7A351C61228D" italics="true" pageId="157" pageNumber="158">Naesiotus habeli</emphasis>
