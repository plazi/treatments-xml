<collectingCountry id="F35B767EFF9B0D3191409836FD0BFE7C" box="[624,687,373,395]" name="Israel" pageId="20" pageNumber="1261">Israel</collectingCountry>
, 
<collectingCountry id="F35B767EFF9B0D3191839836FD53FE7C" box="[691,759,373,395]" name="Egypt" pageId="20" pageNumber="1261">Egypt</collectingCountry>
, 
