<taxonomicName id="2280EEF8FF86FFF730D52B0AFCC2F9C4" box="[708,812,1584,1607]" class="Hydrozoa" family="Bougainvilliidae" genus="Nemopsis" kingdom="Animalia" order="Anthoathecata" pageId="1" pageNumber="297" phylum="Cnidaria" rank="genus">
