<paragraph id="8BDA36B66C3F8A34FF45FE1A46C0FE5D" blockId="19.[189,457,474,500]" box="[189,457,474,500]" pageId="19" pageNumber="20">
<heading id="D09281DA6C3F8A34FF45FE1A46C0FE5D" bold="true" box="[189,457,474,500]" fontSize="11" level="3" pageId="19" pageNumber="20" reason="3">
