<subSubSection id="C3716505FFB6BD43FF4FF9FEFBF3F9C0" box="[199,1095,1605,1629]" pageId="0" pageNumber="67" type="etymology">
<paragraph id="8BD4368EFFB6BD43FF4FF9FEFBF3F9C0" blockId="0.[151,1437,1536,2010]" box="[199,1095,1605,1629]" pageId="0" pageNumber="67">
<emphasis id="B91FEA9CFFB6BD43FF4FF9FEFEF3F9C1" bold="true" box="[199,327,1605,1629]" pageId="0" pageNumber="67">Etymology.</emphasis>
