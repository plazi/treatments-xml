<taxonomicName id="4C654D37FF91FF882E3FE9CFA2D5FC66" baseAuthorityName="Say" baseAuthorityYear="1827" box="[345,669,994,1018]" class="Insecta" family="Anthribidae" genus="Trigonorhinus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="tomentosus">
