<collectingCountry id="F30E07C4FF8F724FFD1FFE0BFCBFFE0E" box="[764,880,444,470]" name="Argentina" pageId="5" pageNumber="6">Argentina</collectingCountry>
and 
<collectingCountry id="F30E07C4FF8F724FFC49FE0BFC26FE0E" box="[938,1001,444,470]" name="Chile" pageId="5" pageNumber="6">Chile</collectingCountry>
(
<figureCitation id="13225BD1FF8F724FFC1AFE0BFB9EFE0E" box="[1017,1105,444,470]" captionStart="FIGURES 30 – 32" captionStartId="21.[264,383,1196,1220]" captionTargetBox="[290,1303,441,1195]" captionTargetId="figure@21.[259,1328,413,1202]" captionTargetPageId="21" captionText="FIGURES 30 – 32. Distribution maps. 30. Microthereva argentiventris (closed circles), M. variventris (arrow). 31. Peralia hermanni. 32. Peralia vittata." httpUri="https://zenodo.org/record/173539/files/figure.png" pageId="5" pageNumber="6">
Fig. 
<number id="3DD15573FF8F724FFBD3FE0BFB9EFE0E" box="[1072,1105,444,470]" pageId="5" pageNumber="6" value="30.0">30</number>
</figureCitation>
).
</paragraph>
