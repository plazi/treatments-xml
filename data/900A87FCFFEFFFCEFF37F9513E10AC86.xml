slide); 
<number id="AE6B24CDFFEFFFCCFC6DF8B439F6AF08" box="[973,987,1866,1892]" pageId="16" pageNumber="17" value="7.0">7</number>
<emphasis id="2AD7EAF8FFEFFFCCFC43F8B53E0DAF08" box="[995,1056,1867,1892]" italics="true" pageId="16" pageNumber="17">large</emphasis>
