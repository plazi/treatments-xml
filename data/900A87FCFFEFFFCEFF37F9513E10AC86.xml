<number id="AE6B24CDFFEFFFCCFEABF8B43B75AF08" box="[267,344,1866,1892]" pageId="16" pageNumber="17" value="13006.0">13006</number>
; 
<collectionCode id="7EB2AE2FFFEFFFCCFEC4F8B43B92AF08" LSID="urn:lsid:biocol.org:col:34871" box="[356,447,1866,1892]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="16" pageNumber="17">USNM</collectionCode>
): 
<number id="AE6B24CDFFEFFFCCFE74F8B43BCEAF08" box="[468,483,1866,1892]" pageId="16" pageNumber="17" value="2.0">2</number>
/ 
<number id="AE6B24CDFFEFFFCCFE4CF8B4382BAF08" box="[492,518,1866,1892]" pageId="16" pageNumber="17" value="11.0">11</number>
cysts + 
<number id="AE6B24CDFFEFFFCCFDCBF8B43857AF08" box="[619,634,1866,1892]" pageId="16" pageNumber="17" value="1.0">1</number>
