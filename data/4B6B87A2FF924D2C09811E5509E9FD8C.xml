<quantity id="043A9B51FF924D2F0A131E1E0BFBF8FD" box="[880,939,1891,1916]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="5" pageNumber="626" unit="m" value="50.0">50 m</quantity>
