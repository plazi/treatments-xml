<taxonomicName id="DEBC4D25FFC3FFBFFF34FF2FE53F4CD2" authority="Pajni &amp; Sood, 1982" authorityName="Pajni &amp; Sood" authorityYear="1982" box="[151,622,221,244]" class="Insecta" family="Curculionidae" genus="Dirodes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="123" phylum="Arthropoda" rank="species" species="flavomaculatus">
