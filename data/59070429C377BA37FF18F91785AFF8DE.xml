<taxonomicName id="16AECEBCC377BA37FDECF94B828EF8DE" authority="Costa" authorityName="Costa" box="[611,914,1782,1808]" class="Insecta" family="Dactylopiidae" genus="Dactylopius" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="coccus">
