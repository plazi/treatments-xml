<specimenCount id="8FABA4875002FFFE58AEF859FB775A0C" box="[1157,1201,1809,1835]" pageId="9" pageNumber="541" type="male">3 ♂</specimenCount>
, 
<specimenCount id="8FABA4875002FFFE5896F859FB215A0C" box="[1213,1255,1809,1835]" pageId="9" pageNumber="541" type="female">2 ♀</specimenCount>
, 
<collectionCode id="FFBCF7CB5002FFFE58DEF859FAFE5A0C" box="[1269,1336,1809,1835]" country="Finland" httpUri="http://grbio.org/cool/2vmj-fp0v" name="Finnish Museum of Natural History" pageId="9" pageNumber="541" type="Museum">MZH</collectionCode>
