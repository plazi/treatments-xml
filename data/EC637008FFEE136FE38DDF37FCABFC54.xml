<typeStatus id="BB717FBCFFEC136FE2D5DAC1FDA6FE42" box="[463,575,459,485]" pageId="31" pageNumber="32">Holotype</typeStatus>
female and 
<typeStatus id="BB717FBCFFEC136FE1CCDAC1FCA7FE42" box="[726,830,459,485]" pageId="31" pageNumber="32">paratype</typeStatus>
female, ex. litter, coastal heathland, Little Target, Beecroft Peninsula (
<geoCoordinate id="01FEA7D9FFEC136FE290DAFBFE6CFDAC" box="[394,501,496,523]" direction="south" orientation="latitude" pageId="31" pageNumber="32" precision="925" value="-35.05">
