<taxonomicName id="4C088CDDFFD8FFAA3CC52142FC955201" authority="Narayanan" authorityName="Narayanan" box="[409,801,1547,1569]" class="Insecta" family="Encyrtidae" genus="Monstranusia" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="267" phylum="Arthropoda" rank="species" species="antennata">
