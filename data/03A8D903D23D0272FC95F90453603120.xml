<taxonomicName id="4C011396D23D0273FAC8F8B454B93449" box="[1377,1467,1856,1879]" class="Bacilli" family="Lactobacillaceae" genus="Limosilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="50" pageNumber="2832" phylum="Firmicutes" rank="species" species="mucosae">
