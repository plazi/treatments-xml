<paragraph id="8B9FB75DFFCC3D583EEEE543FEDABD06" blockId="54.[151,327,1542,1602]" box="[151,279,1542,1566]" pageId="54" pageNumber="55">
<taxonomicName id="4C20CCDEFFCC3D583EEEE543FF11BD05" box="[151,220,1542,1565]" class="Tubothalamea" family="Hauerinidae" genus="Pyrgo" kingdom="Chromista" order="Miliolida" pageId="54" pageNumber="55" phylum="Foraminifera" rank="genus">
