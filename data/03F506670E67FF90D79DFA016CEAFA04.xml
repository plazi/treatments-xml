<taxonomicName id="4C5CCCF20E78FF90D5D1FAF96FB0FA31" authorityName="Forster" authorityYear="1869" box="[387,444,1386,1410]" class="Insecta" family="Figitidae" genus="Dilyta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="131" phylum="Arthropoda" rank="genus">
