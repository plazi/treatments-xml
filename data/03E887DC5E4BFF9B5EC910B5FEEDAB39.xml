<taxonomicName id="4C414D495E48FF9A5DCE12BFFBB8AD52" box="[912,1035,1784,1809]" class="Demospongiae" family="Suberitidae" genus="Suberites" kingdom="Animalia" order="Hadromerida" pageId="34" pageNumber="129" phylum="Porifera" rank="species" species="topsenti">
