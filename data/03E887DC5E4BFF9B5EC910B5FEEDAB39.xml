<taxonomicName id="4C414D495E48FF9A5F701057FE43AE64" box="[302,496,1040,1063]" class="Demospongiae" family="Suberitidae" genus="Suberites" kingdom="Animalia" order="Hadromerida" pageId="34" pageNumber="129" phylum="Porifera" rank="species" species="topsenti">
