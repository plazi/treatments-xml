<taxonomicName id="4C414D495E48FF9A5C09130FFC99AD22" authority="Burton, 1929" authorityName="Burton" authorityYear="1929" box="[599,810,1864,1889]" class="Demospongiae" family="Suberitidae" genus="Suberella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Suberitida" pageId="34" pageNumber="129" phylum="Porifera" rank="species" species="topsenti">
