, 
<taxonomicName id="EB450992E286DB0AFE9147ABC5B0D400" authority="dunkeri" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="51" pageNumber="52" phylum="Mollusca" rank="subSpecies" species="dunkeri" subGenus="Drymaeus" subSpecies="dunkeri">
