<emphasis id="2BC3EACF0B298B7210FBF84F7D23F863" bold="true" box="[151,281,1973,1997]" pageId="3" pageNumber="443">FIGURE 2.</emphasis>
<taxonomicName id="DEB74D5E0B298B72114CF84C7DE2F863" authorityName="Serejo &amp; Sittrop" authorityYear="2009" box="[288,472,1974,1997]" class="Malacostraca" family="Hyalidae" genus="Hyachelia" kingdom="Animalia" order="Amphipoda" pageId="3" pageNumber="443" phylum="Arthropoda" rank="species" species="lowryi" status="sp. nov.">
<emphasis id="2BC3EACF0B298B72114CF84C7DE2F863" box="[288,472,1974,1997]" italics="true" pageId="3" pageNumber="443">Hyachelia lowryi</emphasis>
</taxonomicName>
<emphasis id="2BC3EACF0B298B7211B3F84F7E0EF863" bold="true" box="[479,564,1973,1997]" pageId="3" pageNumber="443">
<taxonomicNameLabel id="30F057B40B298B7211B3F84F7E0EF863" box="[479,564,1973,1997]" pageId="3" pageNumber="443" rank="species">sp. nov.</taxonomicNameLabel>
