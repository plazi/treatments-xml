, 
<collectingMunicipality id="6BD2ACC8FFB8FFB7FD7D35E4FCFF1190" box="[680,804,1162,1187]" pageId="4" pageNumber="157">Klagenfurt</collectingMunicipality>
, 
<collectingCountry id="F31E7622FFB8FFB7FCE735E4FC5E1190" box="[818,901,1162,1187]" name="Austria" pageId="4" pageNumber="157">Austria</collectingCountry>
