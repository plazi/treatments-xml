<taxonomicName id="7999A3B2FF9ED65F6EDFF9ED7E52F96E" authority="(Lamarck, 1816)" baseAuthorityName="Lamarck" baseAuthorityYear="1816" box="[192,517,1657,1675]" class="Hydrozoa" family="Plumulariidae" genus="Cladacanthella" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="799" phylum="Cnidaria" rank="species" species="scabra">
