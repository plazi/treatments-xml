<normalizedToken id="4B61BA2C196AC64B8113F4C30B8CA03E" originalValue="trockenhäutigem" pageId="null" pageNumber="474">trockenhaeutigem</normalizedToken>
, 
<normalizedToken id="BC42BC47C43B09F503C09F2326F7B44E" originalValue="weißem" pageId="null" pageNumber="474">weissem</normalizedToken>
oder 
<normalizedToken id="C44816400819EF7A762E032AB07EAB51" originalValue="bräunlichem" pageId="null" pageNumber="474">braeunlichem</normalizedToken>
Rand
</td>
<td id="96D6BD24CFC4F944F0CC96C9D3ABFA01" align="left" pageId="null" pageNumber="474" valign="bottom">
