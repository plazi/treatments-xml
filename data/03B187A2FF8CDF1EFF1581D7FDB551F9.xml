<quantity id="4CE09B51FF8CDF10FD4581BCFCA8532D" box="[711,784,748,773]" metricMagnitude="2" metricUnit="m" metricValue="5.92" pageId="23" pageNumber="24" unit="m" value="592.0">592 m</quantity>
