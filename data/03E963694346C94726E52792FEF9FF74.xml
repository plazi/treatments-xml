<taxonomicName id="4C40A9FC4346C94427652793FD11F800" authority="Illies, 1963" authorityName="Illies" authorityYear="1963" box="[279,653,1928,1953]" class="Insecta" family="Nemouridae" genus="Nemoura" kingdom="Animalia" order="Plecoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="carpathica">
<emphasis id="B9340E6D4346C94427652793FD9AF800" bold="true" box="[279,518,1928,1953]" italics="true" pageId="33" pageNumber="34">Nemoura carpathica</emphasis>
<bibRefCitation id="EFD1AF8E4346C944247F2792FD11F800" author="Illies" box="[525,653,1928,1953]" pageId="33" pageNumber="34" refString="Illies, J. (1963) Neue Plecopteren aus den Karpathen. Mitteilungen der Schweizerischen entomologischen Gesellschaft, 35 (3 - 4), 288 - 295." type="journal article" year="1963">
Illies, 
<number id="3D88C0584346C94424272792FD11F800" box="[597,653,1928,1953]" pageId="33" pageNumber="34" value="1963.0">1963</number>
</bibRefCitation>
