<taxonomicName id="5B838AC5DE11FFA69021E154FD57FBEA" authorityName="Haglund" authorityYear="1895" box="[570,681,1050,1071]" class="Insecta" family="Miridae" genus="Odoniella" kingdom="Animalia" order="Hemiptera" pageId="94" pageNumber="130" phylum="Arthropoda" rank="genus">
