<taxonomicName id="5B838AC5DE11FFA6933EE154FE74FBEA" authorityName="Distant" authorityYear="1904" box="[293,394,1050,1071]" class="Insecta" family="Miridae" genus="Volkelius" kingdom="Animalia" order="Hemiptera" pageId="94" pageNumber="130" phylum="Arthropoda" rank="genus">Volkelius</taxonomicName>
</emphasis>
