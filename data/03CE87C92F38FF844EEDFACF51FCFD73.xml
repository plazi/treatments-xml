<taxonomicName id="4C674D5C2F3BFF854F34FAF55158FAAA" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" box="[296,594,1301,1322]" class="Insecta" family="Chrysididae" genus="Cleptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="393" phylum="Arthropoda" rank="variety" species="pallipes" variety="androgyna">
