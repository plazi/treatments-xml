<taxonomicName id="FDE0848EFFA0B252F71EFA00FB7DA0EE" authority="Jenkin, 1908" authorityName="Jenkin" authorityYear="1908" box="[855,1177,1484,1508]" class="Calcarea" family="Grantiidae" genus="Leucandra" kingdom="Animalia" order="Leucosolenida" pageId="27" pageNumber="230" phylum="Porifera" rank="species" species="hiberna">
