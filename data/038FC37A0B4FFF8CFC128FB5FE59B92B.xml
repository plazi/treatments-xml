<taxonomicName id="4C2609EF0B48FF8DFAC2888DFF65BFCE" authorityName="Yasunaga &amp; Chérot &amp; Schwartz" authorityYear="2021" class="Insecta" family="Miridae" genus="Sundacapsus" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="149" phylum="Arthropoda" rank="species" species="flavonitidus">
