<taxonomicName id="4C5F0DC6FFF9FFBFFB52DE9AFA71F851" authority="(FAUVEL, 1865)" baseAuthorityName="FAUVEL" baseAuthorityYear="1865" box="[1197,1451,1973,1997]" class="Insecta" family="Staphylinidae" genus="Isomalus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="303" phylum="Arthropoda" rank="species" species="frater">
