<quantity id="4CA7DBA0FFF9FFBFFEAEDD6DFE06FBC5" box="[337,476,1089,1113]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" metricValueMax="3.5" metricValueMin="3.1" pageId="28" pageNumber="303" unit="mm" value="3.3" valueMax="3.5" valueMin="3.1">3.1 – 3.5 mm</quantity>
