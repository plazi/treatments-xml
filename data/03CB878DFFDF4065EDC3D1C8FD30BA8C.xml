<quantity id="4C9A9B7EFFDF4065EC3AD090FE3FBA8C" box="[366,501,1480,1504]" metricMagnitude="-3" metricUnit="m" metricValue="7.75" metricValueMax="8.1" metricValueMin="7.4" pageId="6" pageNumber="439" unit="mm" value="7.75" valueMax="8.1" valueMin="7.4">7.4–8.1 mm</quantity>
