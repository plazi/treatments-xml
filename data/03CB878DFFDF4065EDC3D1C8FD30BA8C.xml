<quantity id="4C9A9B7EFFDF4065EFC9D090FD3CBA8C" box="[669,758,1480,1504]" metricMagnitude="-3" metricUnit="m" metricValue="9.9" pageId="6" pageNumber="439" unit="mm" value="9.9">9.9 mm</quantity>
