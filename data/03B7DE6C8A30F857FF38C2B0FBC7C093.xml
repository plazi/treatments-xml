<number id="3DD67D5D8A0FF857FC33C3FBFC09C4D8" box="[924,960,476,500]" pageId="32" pageNumber="33" value="1.1">1.1</number>
) – 
<number id="3DD67D5D8A0FF857FC4AC3FBFBC8C4D8" box="[997,1025,476,500]" pageId="32" pageNumber="33" value="15.0">15</number>
