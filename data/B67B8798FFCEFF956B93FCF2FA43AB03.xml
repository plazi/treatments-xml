<keyLead id="852395BBFFCEFF956B93FB54FA44AB7D" pageId="1" pageNumber="566">
<number id="881A24A9FFCEFF956B93FB54FF7AAB5A" box="[151,165,1263,1285]" pageId="1" pageNumber="566" value="5.0">5</number>
