<quantity id="4CBB9B42FF9482077CE3FA78FC45FA2B" box="[904,977,1441,1465]" metricMagnitude="2" metricUnit="m" metricValue="2.2" pageId="40" pageNumber="355" unit="m" value="220.0">220 m</quantity>
; Los Gemelos; Media Luna. Deposited in 
<collectionCode id="ED52AE62FF9482077C54FA1BFC07FA48" box="[831,915,1474,1498]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="40" pageNumber="355">AMNH</collectionCode>
, 
<collectionCode id="ED52AE62FF9482077CF5FA1BFC40FA48" box="[926,980,1474,1498]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="40" pageNumber="355">CAS</collectionCode>
, 
<collectionCode id="ED52AE62FF9482077CB4FA1BFBB3FA48" box="[991,1063,1474,1498]" country="Ecuador" httpUri="http://grbio.org/cool/rhcv-mdhf" name="Invertebrate Collection" pageId="40" pageNumber="355">CDRS</collectionCode>
, 
<collectionCode id="ED52AE62FF9482077B59FA1BFBFFFA48" box="[1074,1131,1474,1498]" country="Canada" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="40" pageNumber="355">CNC</collectionCode>
, 
<collectionCode id="ED52AE62FF9482077B1CFA1BFB20FA48" box="[1143,1204,1474,1498]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="40" pageNumber="355">MCZ</collectionCode>
, and 
<collectionCode id="ED52AE62FF9482077B80FA1BFAD4FA48" box="[1259,1344,1474,1498]" country="Switzerland" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="40" pageNumber="355">MHNG</collectionCode>
.
</paragraph>
