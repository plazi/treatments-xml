<taxonomicName id="A6D44D1AFF95FFADFE86C11F1BC8FDBF" authority="T. K. A. Kumar &amp; Manim., Mycotaxon" authorityName="T. K. A. Kumar &amp; Manim., Mycotaxon" box="[285,956,548,575]" class="Agaricomycetes" family="Polyporaceae" genus="Lentinus" kingdom="Fungi" order="Polyporales" pageId="2" pageNumber="289" phylum="Basidiomycota" rank="species" species="bambusinus">
