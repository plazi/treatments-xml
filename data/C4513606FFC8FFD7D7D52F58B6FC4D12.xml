: 
<taxonomicName id="8BF8FC93FFCFFFD5D66B2E87B68E4B8B" authority="Assing, 2014" authorityName="Assing" authorityYear="2014" box="[268,436,1461,1481]" class="Insecta" family="Staphylinidae" genus="Astenus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="419" phylum="Arthropoda" rank="species" species="wunderlei" status="nov.sp.">
