</taxonomicName>
(yellow) and 
<taxonomicName id="B6D7A887FF9A163AEDC7F83581232D83" authorityName="Pauwels &amp; Thongyai &amp; Chantong &amp; Sumontha" authorityYear="2021" box="[1214,1379,1941,1965]" class="Reptilia" family="Colubridae" genus="Oligodon" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="550" phylum="Chordata" rank="species" species="promsombuti">
