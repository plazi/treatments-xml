[
<taxonomicName id="4C319EBF1902FFEE697E94C81BA9FB5B" authority="Mathews" authorityName="Mathews" authorityYear="1923" box="[770,1160,1222,1245]" class="Aves" family="Corcoracidae" genus="Struthidea" kingdom="Animalia" order="Passeriformes" pageId="61" pageNumber="61" phylum="Chordata" rank="subSpecies" species="cinerea" subSpecies="dalyi">
<emphasis id="B945392E1902FFEE697E94C81B35FB5B" box="[770,1044,1222,1245]" italics="true" pageId="61" pageNumber="61">Struthidea cinerea dalyi</emphasis>
