<collectingCountry id="EDB076495619FFD2FECFFC4328C47484" box="[359,426,979,1005]" name="China" pageId="20" pageNumber="323">China</collectingCountry>
(
<collectingRegion id="5763F83B5619FFD2FE11FC432B4E7487" box="[441,544,979,1006]" country="China" name="Guizhou" pageId="20" pageNumber="323">Guizhou</collectingRegion>
