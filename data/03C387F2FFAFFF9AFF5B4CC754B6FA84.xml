<taxonomicName id="4C6A4D67FFAFFF9AFC374B825210FB1B" box="[1019,1128,1184,1207]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="genus">
