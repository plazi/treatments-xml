<taxonomicName id="4C46A882BF48FFB30B2EF83F9D8BACBB" authorityName="Garcia &amp; Fisher" authorityYear="2012" box="[377,464,1975,1996]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="113" pageNumber="114" phylum="Arthropoda" rank="species" species="tsingy">
