<taxonomicName id="4C2C3BA69D4CFFBCCA35FB32FC4C7BC1" authority="(Willd.) Dippel, Handbuch der Laubholzkunde" authorityName="Dippel, Handbuch der Laubholzkunde" baseAuthorityName="Willd." box="[371,1020,1218,1245]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="humilis">
