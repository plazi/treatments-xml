<taxonomicName id="4C2C3BA69D4CFFBCCF4DFB8BFAF57B89" authority="Spach, Annales" authorityName="Spach, Annales" box="[1035,1349,1146,1173]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="pumila">
