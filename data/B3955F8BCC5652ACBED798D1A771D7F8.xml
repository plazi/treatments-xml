<taxonomicName id="A0AA7D7DE143537D5FC09D8B22913527" LSID="B3955F8B-CC56-52AC-BED7-98D1A771D7F8" authority="Zug and Vindum, 2007" authorityName="Zug and Vindum" authorityYear="2007" class="Reptilia" family="Agamidae" genus="Calotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calotes htunwini" order="Squamata" pageId="0" pageNumber="669" phylum="Chordata" rank="species" species="htunwini">Calotes htunwini Zug and Vindum, 2007</taxonomicName>
