, PL 
<quantity id="4CC89B35654EFF90764AC0BAFC72F8B7" box="[883,993,1785,1811]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" pageId="9" pageNumber="10" unit="mm" value="1.15">1.15 mm</quantity>
, PW 
<quantity id="4CC89B35654EFF90711CC0BAFB00F8B7" box="[1061,1171,1785,1811]" metricMagnitude="-3" metricUnit="m" metricValue="1.54" pageId="9" pageNumber="10" unit="mm" value="1.54">1.54 mm</quantity>
