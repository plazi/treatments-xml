<taxonomicName id="BAA4F5B5FFB9D353EF07FCB4FAFAFCB0" authorityName="Watts" authorityYear="2010" box="[1142,1283,833,857]" class="Insecta" family="Scirtidae" genus="Prionocyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="172" phylum="Arthropoda" rank="species" species="tolgaensis">
