<emphasis id="B911EA9F8A66FFADFF5BF927F12AF948" bold="true" box="[151,315,1671,1696]" pageId="2" pageNumber="341">FIGURES 1–3.</emphasis>
<taxonomicName id="4C654D0E8A66FFADFE8EF928F2A7F948" authority="Kral &amp; Jakl" authorityName="Kral &amp; Jakl" authorityYear="2020" box="[322,694,1671,1696]" class="Insecta" family="Geotrupidae" genus="Ceratophyus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="341" phylum="Arthropoda" rank="species" species="amdoensis" status="new species">
<emphasis id="B911EA9F8A66FFADFE8EF928F224F948" box="[322,565,1672,1696]" italics="true" pageId="2" pageNumber="341">Ceratophyus amdoensis</emphasis>
Král &amp; Jákl
</taxonomicName>
, 
<taxonomicNameLabel id="A22257E48A66FFADFD0DF929F32FF948" box="[705,830,1671,1696]" pageId="2" pageNumber="341" rank="species">new species</taxonomicNameLabel>
