; 
<bibRefCitation id="EF89ED94FFE76D3584C5FE33FDC751D2" author="Utinomi" box="[424,593,404,430]" pageId="5" pageNumber="12" refString="Utinomi, H. (1955) Report on the Pycnogonida collected by the Soyo-Maru Expedition made on the continental shelf bordering Japan during the years 1926 - 30. Publications of the Seto Marine Biological Laboratory, 5 (1), 1 - 42" type="journal article" year="1955">
Utinomi 
<number id="3DD08242FFE76D35877FFE33FDC751D2" box="[530,593,404,430]" pageId="5" pageNumber="6" value="1955.0">1955</number>
</bibRefCitation>
); 
<taxonomicName id="4C18EBE6FFE76D358705FE33FCFA51D2" authority="Stock, 1986" authorityName="Stock" authorityYear="1986" box="[616,876,404,430]" class="Pycnogonida" family="Ammotheidae" genus="Heterofragilia" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="major">
