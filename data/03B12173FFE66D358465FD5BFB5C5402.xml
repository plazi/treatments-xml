<quantity id="4CE03D80FFE76D358465FECBFEFD51FA" box="[264,363,364,390]" metricMagnitude="2" metricUnit="m" metricValue="8.705" pageId="5" pageNumber="6" unit="m" value="870.5">870.5 m</quantity>
