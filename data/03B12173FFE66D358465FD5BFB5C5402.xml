<taxonomicName id="4C18EBE6FFE76D358728FE43FD2B5181" box="[581,701,484,509]" class="Pycnogonida" family="Ammotheidae" genus="Heterofragilia" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="hirsuta">
