<quantity id="4CE03D80FFE76D358413FE43FD935182" box="[382,517,484,510]" metricMagnitude="2" metricUnit="m" metricValue="6.65" metricValueMax="6.8" metricValueMin="6.5" pageId="5" pageNumber="6" unit="m" value="665.0" valueMax="680.0" valueMin="650.0">650–680 m</quantity>
