<taxonomicName id="4C18EBE6FFE76D358063FEBBFDAC5122" authority="Hedgpeth, 1943" authorityName="Hedgpeth" authorityYear="1943" class="Pycnogonida" family="Ammotheidae" genus="Heterofragilia" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="fimbriata">
