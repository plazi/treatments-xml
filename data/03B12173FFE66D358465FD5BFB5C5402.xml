<quantity id="4CE03D80FFE76D3584DEFE1BFDCE51AA" box="[435,600,444,470]" metricMagnitude="3" metricUnit="m" metricValue="1.2965" metricValueMax="1.54" metricValueMin="1.053" pageId="5" pageNumber="6" unit="m" value="1296.5" valueMax="1540.0" valueMin="1053.0">1053–1540 m</quantity>
; 
<taxonomicName id="4C18EBE6FFE76D358707FE1BFC2051AA" authority="Stock, 1991" authorityName="Stock" authorityYear="1991" box="[618,950,444,470]" class="Pycnogonida" family="Ammotheidae" genus="Heterofragilia" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="brevicauda">
