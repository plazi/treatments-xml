<emphasis id="D39BC00E9128074CFEA0FAA5FDD7051B" bold="true" box="[264,528,1282,1308]" italics="true" pageId="11" pageNumber="12">Liosomaphis turanica</emphasis>
Narzikulov, 
<number id="57270E3B9128074CFD19FAA6FD2B051C" box="[689,748,1281,1307]" pageId="11" pageNumber="12" value="1960.0">1960</number>
</taxonomicName>
= 
<taxonomicName id="26EF679F9128074CFCA3FAA5FADD051C" authority="Kaltenbach, 1843" authorityName="Kaltenbach" authorityYear="1843" box="[779,1306,1281,1308]" class="Insecta" family="Aphididae" genus="Liosomaphis" kingdom="Animalia" order="Aphidomorpha" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="berberidis">
<emphasis id="D39BC00E9128074CFCA3FAA5FBE2051B" bold="true" box="[779,1061,1282,1308]" italics="true" pageId="11" pageNumber="12">Liosomaphis berberidis</emphasis>
(Kaltenbach, 
<number id="57270E3B9128074CFB7EFAA6FAD5051C" box="[1238,1298,1281,1307]" pageId="11" pageNumber="12" value="1843.0">1843</number>
)
