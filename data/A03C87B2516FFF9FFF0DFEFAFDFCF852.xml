</taxonomicName>
<emphasis id="1AE1EAB6516FFF9FFAD5FD27FA16FCBB" bold="true" box="[1359,1436,757,779]" pageId="2" pageNumber="43">
<taxonomicNameLabel id="01D257CD516FFF9FFAD5FD27FA16FCBB" box="[1359,1436,757,779]" pageId="2" pageNumber="43" rank="species">
sp. 
<date id="5C2B1064516FFF9FFAE8FD27FA16FCBB" box="[1394,1436,757,779]" pageId="2" pageNumber="43">nov.</date>
