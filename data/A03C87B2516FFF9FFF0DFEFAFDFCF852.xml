<number id="9E5D2483516FFF9FFDC2FCC0FDEEFC97" box="[600,612,786,807]" pageId="2" pageNumber="43" value="4.0">4</number>
(sometimes with 
<number id="9E5D2483516FFF9FFC89FCC0FC95FC97" box="[787,799,786,807]" pageId="2" pageNumber="43" value="3.0">3</number>
) palpomeres. Wing 
<quantity id="EF6D9B41516FFF9FFC7CFCC0FBD0FC97" box="[998,1114,786,807]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" metricValueMax="2.2" metricValueMin="2.1" pageId="2" pageNumber="43" unit="mm" value="2.15" valueMax="2.2" valueMin="2.1">2.1–2.2 mm</quantity>
