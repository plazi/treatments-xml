<quantity id="EF6D9B41516FFF9FFCB8FC9DFC1FFCD4" box="[802,917,847,868]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" metricValueMax="2.7" metricValueMin="2.6" pageId="2" pageNumber="43" unit="mm" value="2.65" valueMax="2.7" valueMin="2.6">2.6–2.7 mm</quantity>
