<taxonomicName id="79AE7CDB962EFFB8ED274B1EFE09FEB6" box="[307,468,311,335]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="10" pageNumber="264" phylum="Tracheophyta" rank="species" species="undetermined">
