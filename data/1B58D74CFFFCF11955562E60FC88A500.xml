<caption id="C78E36D2FFFEF11E57942829FCACA50C" ID-DOI="http://doi.org/10.5281/zenodo.4652418" ID-Zenodo-Dep="4652418" httpUri="https://zenodo.org/record/4652418/files/figure.png" pageId="11" pageNumber="1223" startId="11.[213,280,1284,1306]" targetBox="[213,1170,156,1250]" targetPageId="11">
<paragraph id="934E665AFFFEF11E57942829FCACA50C" blockId="11.[213,1170,1284,1364]" pageId="11" pageNumber="1223">
