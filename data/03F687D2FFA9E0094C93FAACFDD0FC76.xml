, WF 
<quantity id="4CA79B21FFABE0094FC0FC8EFB84FC86" box="[964,1102,799,825]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="8.0" metricValueMin="7.0" pageId="10" pageNumber="313" unit="mm" value="0.75" valueMax="0.8" valueMin="0.7">0.7–0.8 mm</quantity>
, EL 
<quantity id="4CA79B21FFABE0094886FC8EFAC6FC86" box="[1154,1292,799,825]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="10" pageNumber="313" unit="mm" value="0.85" valueMax="0.9" valueMin="0.8">0.8–0.9 mm</quantity>
, ML 
<quantity id="4CA79B21FFABE009494CFC8EFF0FFCE2" metricMagnitude="-3" metricUnit="m" metricValue="2.8" metricValueMax="2.9" metricValueMin="2.7" pageId="10" pageNumber="313" unit="mm" value="2.8" valueMax="2.9" valueMin="2.7">2.7–2.9 mm</quantity>
