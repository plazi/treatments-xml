<taxonomicName id="2C1A12B6896B313A9720BC5D0175AF1A" authorityName="Kozlov &amp; Kononova" authorityYear="2004" class="Insecta" family="Scelionidae" genus="Gryon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gryon laetum" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="species" species="laetum">Gryon laetum</taxonomicName>
