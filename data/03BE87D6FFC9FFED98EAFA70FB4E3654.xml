<geoCoordinate id="EE235007FFC9FFED9CF6F9E1FE8B3628" box="[1163,1226,1555,1581]" degrees="1,968" direction="north" orientation="latitude" pageId="88" pageNumber="89" precision="55" value="1.968">1,968</geoCoordinate>
<geoCoordinate id="EE235007FFC9FFED9CB7F9E1FF583628" box="[1226,1305,1555,1581]" degrees="4,101" direction="west" orientation="longitude" pageId="88" pageNumber="89" precision="55" value="-4.101">–4,101</geoCoordinate>
ft
</quantity>
) (
<bibRefCitation id="EF864B31FFC9FFED9D34F9E1FB433654" author="Pietsch" firstAuthor="Pietsch" pageId="88" pageNumber="89" refId="ref312236" refString="Pietsch, T. W. (1974) Osteology and relationships of ceratioid anglerfishes of the family Oneirodidae, with a review of the genus Oneirodes Lutken. Natural History Museum of Los Angeles County Contributions in Science, 18." type="book" year="1974">Pietsch 1974</bibRefCitation>
