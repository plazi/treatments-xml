<emphasis id="B979833B5623BC0DFF72FEF1DC6BFEBB" bold="true" box="[151,320,266,292]" pageId="7" pageNumber="48">
<typeStatus id="54B6E18B5623BC0DFF72FEF1DDFEFEBB" box="[151,213,266,292]" pageId="7" pageNumber="48">Type</typeStatus>
–species.
</emphasis>
<taxonomicName id="4C0D24AA5623BC0DFEADFEF1DF80FEBB" authority="Beekey, 1938" authorityName="Beekey" authorityYear="1938" box="[328,683,266,292]" class="Insecta" family="Scatopsidae" genus="Scatopse" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="48" phylum="Arthropoda" rank="species" species="similis">
