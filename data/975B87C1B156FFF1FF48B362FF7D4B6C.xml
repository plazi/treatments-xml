</taxonomicName>
and 
<taxonomicName id="D8F24D54B157FFF2FC7BB22FFBAD4C3E" box="[932,1069,1551,1576]" class="Insecta" family="Histeridae" genus="Solenopsister" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="amabilis">
