<taxonomicName id="4C514D58FFBE605BF670FF40FD4ADEE3" box="[521,660,225,248]" class="Insecta" family="Drepanidae" genus="Isopsestis" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="naumanni">
