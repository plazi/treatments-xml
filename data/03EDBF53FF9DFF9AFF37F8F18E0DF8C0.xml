<paragraph id="8BFB0E45FF9EFF9AFF67FE2D8812FD10" blockId="102.[151,1437,352,1858]" pageId="102" pageNumber="103">
<materialsCitation id="3B2C0418FF9EFF9AFF67FE2D886EFD10" collectionCode="R" collectorName="Dorsum. Dorsal &amp; With &amp; A. Setae" latitude="-5.2" pageId="102" pageNumber="103" specimenCode="R1" specimenCount="1">
