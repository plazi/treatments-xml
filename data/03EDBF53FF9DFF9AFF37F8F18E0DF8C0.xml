<geoCoordinate id="EE706882FF9EFF9AFF77FDCD8D47FDE8" box="[215,277,592,618]" degrees="6" direction="south" minutes="26" orientation="latitude" pageId="102" pageNumber="103" precision="925" value="-6.4333334">s6 26</geoCoordinate>
(23–28) [26], 
<geoCoordinate id="EE706882FF9EFF9AFE61FDCD8E57FDE8" box="[449,517,592,618]" degrees="2" direction="south" minutes="29" orientation="latitude" pageId="102" pageNumber="103" precision="925" value="-2.4833333">S2 29</geoCoordinate>
(23–33) [29], 
<geoCoordinate id="EE706882FF9EFF9AFD11FDCD8EA7FDE8" box="[689,757,592,618]" degrees="4" direction="south" minutes="31" orientation="latitude" pageId="102" pageNumber="103" precision="925" value="-4.5166664">S4 31</geoCoordinate>
(24–38) [35], 
<geoCoordinate id="EE706882FF9EFF9AFC01FDCD8FB6FDE8" box="[929,996,592,618]" degrees="5" direction="south" minutes="12" orientation="latitude" pageId="102" pageNumber="103" precision="925" value="-5.2">S5 12</geoCoordinate>
