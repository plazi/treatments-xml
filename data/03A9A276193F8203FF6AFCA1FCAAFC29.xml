<taxonomicName id="4C0068E3193F8203FCB8FC69FBE2FC0E" box="[837,1068,943,969]" class="Insecta" family="Pentatomidae" genus="Cosmopepla" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="undetermined">
