<taxonomicName id="4C0068E3193F8203FE36FC15FD49FC2A" authority="Hahn" authorityName="Hahn" authorityYear="1834" box="[459,647,979,1005]" class="Insecta" family="Pentatomidae" genus="Eysarcoris" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
<emphasis id="B974CF72193F8203FE36FC15FD8DFC2B" box="[459,579,979,1004]" italics="true" pageId="16" pageNumber="17">Eysarcoris</emphasis>
Hahn
</taxonomicName>
(tribe 
<taxonomicName id="4C0068E3193F8203FD2FFC15FC96FC29" authorityName="Mulsant &amp; Rey" authorityYear="1866" box="[722,856,979,1006]" class="Insecta" family="Pentatomidae" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="tribe" tribe="Eysarcorini">Eysarcorini</taxonomicName>
