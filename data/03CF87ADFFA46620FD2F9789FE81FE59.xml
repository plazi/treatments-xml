<taxonomicName id="4C664D38FFA46622FD779789FCB5FE3E" authorityName="Simon" authorityYear="1903" box="[750,812,471,497]" class="Arachnida" family="Anyphaenidae" genus="Tafana" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="riveti">
