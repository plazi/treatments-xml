<quantity id="4CCF00EDFFA7FF92FC03B4D6FB8037A5" box="[1014,1071,1410,1437]" metricMagnitude="1" metricUnit="m" metricValue="7.5" pageId="6" pageNumber="107" unit="m" value="75.0">75 m</quantity>
