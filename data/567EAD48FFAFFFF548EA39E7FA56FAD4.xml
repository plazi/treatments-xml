</taxonomicName>
sp. (
<taxonomicName id="19D767DDFFAFFFF44CDD3C40FADCF8F7" box="[1184,1297,1866,1892]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="53" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
) (
<figureCitation id="46EC00DBFFAFFFF44D513C40FA4EF8F7" box="[1324,1411,1866,1892]" captionStart="FIGURE 14" captionStartId="14.[151,255,1808,1832]" captionTargetBox="[153,1433,197,1784]" captionTargetId="figure@14.[153,1433,197,1784]" captionTargetPageId="14" captionText="FIGURE 14. Cecidomyiidae galls: a) blister galls on Copaifera sp., b) globular gall on Copaifera sp., c) cylindrical galls on Serjania mexicana, d) globular galls on Serjania mexicana, e) blister galls on Smilax panamensis, f) blister galls on Machaerium milleflorum, g) globular galls on Hirtella flava, h) galls on Unonopsis sp." httpUri="https://zenodo.org/record/190458/files/figure.png" pageId="10" pageNumber="53">
Fig. 
<number id="681F0E79FFAFFFF44D1B3C40FA4EF8F7" box="[1382,1411,1866,1892]" pageId="10" pageNumber="53" value="14.0">14</number>
</figureCitation>
b) and 
<taxonomicName id="19D767DDFFAFFFF448B53C7BFE69F819" box="[200,420,1905,1930]" class="Magnoliopsida" family="Sapindaceae" genus="Serjania" kingdom="Plantae" order="Sapindales" pageId="10" pageNumber="53" phylum="Tracheophyta" rank="species" species="mexicana">
<emphasis id="ECA3C04CFFAFFFF448B53C7BFE69F819" box="[200,420,1905,1930]" italics="true" pageId="10" pageNumber="53">Serjania mexicana</emphasis>
</taxonomicName>
(
<taxonomicName id="19D767DDFFAFFFF449CF3C7BFD85F818" box="[434,584,1905,1931]" class="Magnoliopsida" family="Sapindaceae" kingdom="Plantae" order="Sapindales" pageId="10" pageNumber="53" phylum="Tracheophyta" rank="family">Sapindaceae</taxonomicName>
) (
<figureCitation id="46EC00DBFFAFFFF44A1C3C7BFD7BF818" box="[609,694,1905,1931]" captionStart="FIGURE 14" captionStartId="14.[151,255,1808,1832]" captionTargetBox="[153,1433,197,1784]" captionTargetId="figure@14.[153,1433,197,1784]" captionTargetPageId="14" captionText="FIGURE 14. Cecidomyiidae galls: a) blister galls on Copaifera sp., b) globular gall on Copaifera sp., c) cylindrical galls on Serjania mexicana, d) globular galls on Serjania mexicana, e) blister galls on Smilax panamensis, f) blister galls on Machaerium milleflorum, g) globular galls on Hirtella flava, h) galls on Unonopsis sp." httpUri="https://zenodo.org/record/190458/files/figure.png" pageId="10" pageNumber="53">
Fig. 
<number id="681F0E79FFAFFFF44AEA3C7BFD7BF818" box="[663,694,1905,1931]" pageId="10" pageNumber="53" value="14.0">14</number>
</figureCitation>
d).
</paragraph>
