<taxonomicName id="19D767DDFFAFFFF44A833C40FC7FF8F7" box="[766,946,1866,1892]" class="Insecta" family="Cecidomyiidae" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="53" phylum="Arthropoda" rank="family">Cecidomyiidae</taxonomicName>
) on 
<taxonomicName id="19D767DDFFAFFFF44B963C41FBAFF8F7" box="[1003,1122,1867,1892]" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="53" phylum="Tracheophyta" rank="genus">
