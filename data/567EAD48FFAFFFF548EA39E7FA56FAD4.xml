<taxonomicName id="19D767DDFFAFFFF44C8E3C92FAA7F822" box="[1267,1386,1944,1969]" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="53" phylum="Tracheophyta" rank="genus">
