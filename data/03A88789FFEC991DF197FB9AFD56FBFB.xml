<quantity id="4CF99B7AFFEF991DF0A7FD26FDC9FD5D" box="[423,513,653,680]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="99" pageNumber="100" unit="mm" value="2.2">2.2 mm</quantity>
