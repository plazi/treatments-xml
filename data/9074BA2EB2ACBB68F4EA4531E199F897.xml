<taxonomicName id="DE67A825520959AF98AFBC5AA3351D89" authorityName="Clairville" authorityYear="1806" class="Hexapoda" family="Carabidae" genus="Badister" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="632" pageNumber="633" phylum="Arthropoda" rank="genus">Badister</taxonomicName>
