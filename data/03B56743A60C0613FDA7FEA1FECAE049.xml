<taxonomicName id="4C1CADD6A60C0613FF46FE22FE6BE594" baseAuthorityName="Moser" baseAuthorityYear="1912" box="[189,393,395,421]" class="Insecta" family="Melolonthidae" genus="Miridiba" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="longula">
