<caption id="DFAD01D72B0CFF8AFF7ED60C537496A5" httpUri="https://zenodo.org/record/1221936/files/figure.png" pageId="37" pageNumber="38" startId="37.[151,250,1909,1931]" targetBox="[193,1397,193,1885]" targetPageId="37">
