<taxonomicName id="47F24D26FF8BAF25FC3EA3C0FBF1666F" box="[960,1105,1520,1544]" class="Arachnida" family="Theraphosidae" genus="Phlogiellus" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="289" phylum="Arthropoda" rank="species" species="daweiensis">
<emphasis id="B286EAB7FF8BAF25FC3EA3C0FBF1666F" box="[960,1105,1520,1544]" italics="true" pageId="0" pageNumber="289">P. daweiensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="47F24D26FF8BAF25FB9EA3C0FB75666F" authorityName="Nunn, West &amp; von Wirth" authorityYear="2016" box="[1120,1237,1520,1544]" class="Arachnida" family="Theraphosidae" genus="Phlogiellus" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="289" phylum="Arthropoda" rank="species" species="pelidnus">
