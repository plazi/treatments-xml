3. 
<taxonomicName id="C1F0D10B9DC0D5969A55F39A31C64174" LSID="BADEB464-181C-59D2-ADC6-E2294A07D016" authority="species group" authorityName="species group" class="Insecta" family="Noteridae" genus="Notomicrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Notomicrus meizon" order="Coleoptera" pageId="0" pageNumber="177" phylum="Arthropoda" rank="species" species="meizon">Notomicrus meizon species group</taxonomicName>
