<taxonomicName id="3EAE5DF41062FFC5AE5B788B99F91FC0" class="Insecta" family="Syrphidae" genus="Myiathropa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="82" phylum="Arthropoda" rank="species" species="fasciata">
<emphasis id="CBDAFA651062FFC5AE5B788B99F91FC0" italics="true" pageId="10" pageNumber="82">Myiathropa fasciata</emphasis>
</taxonomicName>
S. 
<taxonomicName id="3EAE5DF41062FFC5A88C78AA9EFF1FC1" box="[971,1173,446,467]" class="Insecta" family="Syrphidae" genus="Quichuana" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="82" phylum="Arthropoda" rank="variety" species="fasciata" variety="nigriventris">
