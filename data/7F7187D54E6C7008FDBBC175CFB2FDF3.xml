<taxonomicName id="30D84D404E6D700BFE96C719CE58F879" box="[362,591,1895,1921]" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="narathiwat">
