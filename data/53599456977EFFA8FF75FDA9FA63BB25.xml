species: 
<taxonomicName id="1CF05EC3977EFFA8FEC4FDE7FD5BBDAF" authority="Serville, 1831" authorityName="Serville" authorityYear="1831" box="[294,677,618,640]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="maculicollis">
