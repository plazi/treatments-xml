<paragraph id="AC3936E63A4BDD1AB6FDFF63F0C6FF36" blockId="17.[151,1003,151,178]" box="[151,1003,151,178]" pageId="17" pageNumber="18">
<heading id="F771818A3A4BDD1AB6FDFF63F0C6FF36" box="[151,1003,151,178]" fontSize="11" level="2" pageId="17" pageNumber="18" reason="3">
