<location id="2540605D2D44206DFF403F8EC64FFB2E" LSID="urn:lsid:plazi:treatment:A83687902D44206CFF323903C4E1FD4A:2540605D2D44206DFF403F8EC64FFB2E" box="[229,275,1228,1254]" country="Italy" name="The" pageId="10" pageNumber="541">The</location>
<specimenCount id="3699FD0F2D44206DFEBF3F8EC6EBFB2E" box="[282,439,1228,1254]" pageId="10" pageNumber="541" type="female">three females</specimenCount>
