<quantity id="E7679B632D44206DFDD33F73C588FB83" box="[630,724,1073,1099]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="10" pageNumber="541" unit="m" value="1300.0">
<elevation id="ABB2D1B52D44206DFDD33F73C588FB83" box="[630,724,1073,1099]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="10" pageNumber="541" unit="m" value="1300.0">1300 m</elevation>
</quantity>
