<quantity id="E7679B632D44206DFCDC3F49C48EFBEC" box="[889,978,1035,1060]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="10" pageNumber="541" unit="m" value="1300.0">
<elevation id="ABB2D1B52D44206DFCDC3F49C48EFBEC" box="[889,978,1035,1060]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="10" pageNumber="541" unit="m" value="1300.0">1300 m</elevation>
</quantity>
</emphasis>
