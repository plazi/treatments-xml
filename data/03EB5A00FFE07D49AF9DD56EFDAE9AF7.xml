<geoCoordinate id="EE768DD1FFDF7D48AA25D568FB089970" box="[1095,1241,313,340]" degrees="19" direction="north" minutes="54" orientation="latitude" pageId="32" pageNumber="33" precision="15" seconds="05" value="19.901388">19°54’05”N</geoCoordinate>
, 
