<taxonomicName id="4C564D4930315C1AFE53F1322910F8AA" box="[411,588,1812,1835]" class="Insecta" family="Chrysomelidae" genus="Neocrepidodera" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="obscuritarsis">
