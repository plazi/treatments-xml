<taxonomicName id="4C564D4930315C1AFD45F132281EF8AA" box="[653,834,1812,1835]" class="Insecta" family="Chrysomelidae" genus="Asiorestia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="interpunctata">
<emphasis id="B922EAD830315C1AFD45F132281EF8AA" box="[653,834,1812,1835]" italics="true" pageId="4" pageNumber="59">N. interpunctata</emphasis>
