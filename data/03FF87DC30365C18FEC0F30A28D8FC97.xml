<taxonomicName id="4C564D4930315C1AFC57F1322F09F8AA" box="[927,1109,1812,1835]" class="Insecta" family="Chrysomelidae" genus="Neocrepidodera" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="konstantinovi">
<emphasis id="B922EAD830315C1AFC57F1322F09F8AA" box="[927,1109,1812,1835]" italics="true" pageId="4" pageNumber="59">N. konstantinovi</emphasis>
