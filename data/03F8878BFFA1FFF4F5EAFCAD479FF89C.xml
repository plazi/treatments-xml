<paragraph id="8BEE369DFFA1FFF4F5EAFCAD45B6FCA5" blockId="32.[151,492,850,876]" box="[151,492,850,876]" pageId="32" pageNumber="33">
<heading id="D0A681F1FFA1FFF4F5EAFCAD45B6FCA5" bold="true" box="[151,492,850,876]" fontSize="11" level="1" pageId="32" pageNumber="33" reason="1">
<taxonomicName id="4C514D1EFFA1FFF4F5EAFCAD45B6FCA5" authority="Jerdon, 1851" authorityName="Jerdon" authorityYear="1851" box="[151,492,850,876]" class="Insecta" family="Formicidae" genus="Harpegnathos" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="B925EA8FFFA1FFF4F5EAFCAD45B6FCA5" bold="true" box="[151,492,850,876]" pageId="32" pageNumber="33">
<emphasis id="B925EA8FFFA1FFF4F5EAFCAD451EFCA5" bold="true" box="[151,324,850,876]" italics="true" pageId="32" pageNumber="33">Harpegnathos</emphasis>
<bibRefCitation id="EFC04B6CFFA1FFF4F436FCAD45B6FCA5" author="Jerdon" box="[331,492,850,876]" pageId="32" pageNumber="33" refString="Jerdon, T. C. (1851) A catalogue of the species of ants found in southern India. Madras Journal of Literature and Science, 17, 103 - 127." type="journal article" year="1851">
Jerdon, 
<number id="3D9924BAFFA1FFF4F4CCFCAD45B6FCA5" box="[433,492,850,876]" pageId="32" pageNumber="33" value="1851.0">1851</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
