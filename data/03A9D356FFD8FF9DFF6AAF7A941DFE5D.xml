<caption id="DF7F32C8FFDFFF9CFF6AAB259716F879" ID-DOI="http://doi.org/10.5281/zenodo.5062121" ID-Zenodo-Dep="5062121" httpUri="https://zenodo.org/record/5062121/files/figure.png" pageId="4" pageNumber="589" startId="4.[151,264,1912,1935]" targetBox="[175,1389,179,1880]" targetPageId="4">
