[
<emphasis id="1BA3CB1FFFA4FFD9FD24F8F6FCAFE017" bold="true" box="[742,876,1886,1912]" pageId="7" pageNumber="576">new record</emphasis>
]) (
<bibRefCitation id="4D466AFCFFA4FFD9FC4FF8F6FBF2E016" author="Yu, D. S. &amp; van Achterberg, C. &amp; Horstmann, K." box="[909,1073,1886,1913]" pageId="7" pageNumber="576" refId="ref11774" refString="Yu, D. S., van Achterberg, C. &amp; Horstmann, K. (2016) World Ichneumonoidea 2015. Nepean, Ottawa, Ontario. [database on flash-drive]" type="book" year="2016">
Yu 
<emphasis id="1BA3CB1FFFA4FFD9FC77F8F7FC25E017" box="[949,998,1886,1912]" italics="true" pageId="7" pageNumber="576">et al</emphasis>
