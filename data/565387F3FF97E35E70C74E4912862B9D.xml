<quantity id="19029B00FF90E35E73944AEB17332AF8" box="[964,1031,229,255]" metricMagnitude="2" metricUnit="m" metricValue="6.4" pageId="6" pageNumber="49" unit="m" value="640.0">640m</quantity>
