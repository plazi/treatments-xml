<taxonomicName id="4C714D14FFEEFFA4FF3B4AF6FEA2FB04" authority="Zhang &amp; Zhu, 2009" authorityName="Zhang &amp; Zhu" authorityYear="2009" box="[185,300,1192,1217]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="huberi" status="sp. nov.">
<emphasis id="B905EA85FFEEFFA4FF3B4AF6FEA2FB04" box="[185,300,1192,1217]" italics="true" pageId="34" pageNumber="35">P. huberi</emphasis>
</taxonomicName>
<emphasis id="B905EA85FFEEFFA4FEB54AF9FE10FB04" bold="true" box="[311,414,1191,1217]" pageId="34" pageNumber="35">
<taxonomicNameLabel id="A23657FEFFEEFFA4FEB54AF9FE10FB04" box="[311,414,1191,1217]" pageId="34" pageNumber="35" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C714D14FFEEFFA4FE334AF6FDCAFB04" authority="Zhang &amp; Zhu, 2009" authorityName="Zhang &amp; Zhu" authorityYear="2009" box="[433,580,1192,1217]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="songxian" status="sp. nov.">
