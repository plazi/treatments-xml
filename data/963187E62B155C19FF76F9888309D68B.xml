<materialsCitation id="AEF03CAD2B155C19FE6FF9698238D660" ID-GBIF-Occurrence="3319229431" collectionCode="BMNH" location="Cape" pageId="60" pageNumber="61" specimenCount="1" stateProvince="Western Cape" typeStatus="holotype">
