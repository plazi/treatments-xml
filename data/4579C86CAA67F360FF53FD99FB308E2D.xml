</taxonomicName>
from 
<taxonomicName id="0AD002F9AA67F366FD6EFA42FBB68DE8" authority="Grunberg" authorityName="Grunberg" box="[682,1049,1476,1501]" class="Insecta" family="Stratiomyidae" genus="Parapecticus" kingdom="Animalia" order="Diptera" pageId="18" phylum="Arthropoda" rank="species" species="viduatus">
