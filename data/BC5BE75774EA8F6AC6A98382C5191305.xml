<taxonomicName id="1E98BE613F0041E2E0A637EBBEC2AE8F" class="Actinopterygii" family="Ariidae" genus="Bagre" kingdom="Animalia" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Bagre</taxonomicName>
, 
<taxonomicName id="A7FC85A8E4CB77BA0DA4E4397482EDBA" class="Actinopterygii" family="Ariidae" genus="Cathorops" kingdom="Animalia" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Cathorops</taxonomicName>
