<taxonomicName id="08B581E71FA78CBA8806939B0C65BE36" LSID-ZBK="urn:lsid:zoobank.org:act:347CE897-3860-4DA6-85B8-033B2318B81A" class="Actinopterygii" family="Ariidae" genus="Aspistor" kingdom="Animalia" lsidName-ZBK="Aspistor Jordan &amp; Evermann 1898:2763" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Aspistor</taxonomicName>
