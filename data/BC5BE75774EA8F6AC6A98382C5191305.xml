<taxonomicName id="18FC684D70107C5E65AFF7DEC7F4EDC5" class="Actinopterygii" family="Ariidae" genus="Cathorops" kingdom="Animalia" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="species" species="dasycephalus">C. dasycephalus</taxonomicName>
), 
<taxonomicName id="775F52250E0EC2334EDA1BCB45BC2126" LSID-ZBK="urn:lsid:zoobank.org:act:B293B6B1-C1F8-4EA8-AA70-87E0EF542E48" class="Actinopterygii" family="Ariidae" genus="Cochlefelis" kingdom="Animalia" lsidName-ZBK="Cochlefelis Whitley 1941:8" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Cochlefelis</taxonomicName>
, 
<taxonomicName id="1A344C22C3E3831806276765D2FFF67E" LSID-ZBK="urn:lsid:zoobank.org:act:3F1F6AF2-6F38-42C0-9C82-C1468F19FAF0" class="Actinopterygii" family="Ariidae" genus="Hemiarius" kingdom="Animalia" lsidName-ZBK="Hemiarius Bleeker 1862:7, 29" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Hemiarius</taxonomicName>
