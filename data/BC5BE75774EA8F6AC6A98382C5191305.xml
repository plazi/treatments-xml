, 
<taxonomicName id="B4867046EE9CB30DE75804F2840AF1C9" LSID-ZBK="urn:lsid:zoobank.org:act:B293B6B1-C1F8-4EA8-AA70-87E0EF542E48" class="Actinopterygii" family="Ariidae" genus="Cochlefelis" kingdom="Animalia" lsidName-ZBK="Cochlefelis Whitley 1941:8" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Cochlefelis</taxonomicName>
