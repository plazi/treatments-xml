<taxonomicName id="FD4A31D649F80ABC6D39C7A9848CF58D" LSID-ZBK="urn:lsid:zoobank.org:act:D469DDD6-F17E-4DEB-99B7-0C54A27687E2" authority="Ogilby, 1898" authorityName="Ogilby" authorityYear="1898" class="Actinopterygii" family="Ariidae" genus="Pachyula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-ZBK="Pachyula Ogilby 1898:33" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Pachyula</taxonomicName>
