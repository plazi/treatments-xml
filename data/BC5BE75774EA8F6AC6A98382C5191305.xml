<taxonomicName id="CC83A964788DCE0E98A2A3044A73995B" LSID-ZBK="urn:lsid:zoobank.org:act:086E7B0F-D030-44BD-9DCC-8CAEB178CBAA" class="Actinopterygii" family="Ariidae" genus="Notarius" kingdom="Animalia" lsidName-ZBK="Notarius Gill 1863:171" order="Siluriformes" pageNumber="100" phylum="Chordata" rank="genus">Notarius</taxonomicName>
