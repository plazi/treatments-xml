<geoCoordinate id="EE7D5008FFFEFFBAFCACFAC9FC02C1B0" box="[874,927,1405,1431]" degrees="48" direction="south" orientation="latitude" pageId="11" pageNumber="212" precision="55555" value="-48.0">48°S</geoCoordinate>
