<specimenCount id="9D5F40969B51B46AE46E62BBFE712641" box="[256,409,1266,1290]" pageId="4" pageNumber="43" type="generic">
<number id="3D9199389B51B46AE46E62BBFEF42641" box="[256,284,1266,1290]" pageId="4" pageNumber="43" value="30.0">30</number>
