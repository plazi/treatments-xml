<quantity id="4CD8AB92B705AD2FFAE7C6EC1E8736F4" box="[1374,1432,721,745]" metricMagnitude="1" metricUnit="m" metricValue="5.5" pageId="23" pageNumber="24" unit="m" value="55.0">55 m</quantity>
, 
<date id="FF9E20B7B705AD2FFF2EC6C81A503710" box="[151,335,757,781]" pageId="23" pageNumber="24" value="1898-10-18">
<number id="3DE81450B705AD2FFF2EC6C81BAC3710" box="[151,179,757,781]" pageId="23" pageNumber="24" value="18.0">18</number>
