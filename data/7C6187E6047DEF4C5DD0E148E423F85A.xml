<taxonomicName id="33C84D73047FEF4C5C1BE36DE1BCFA4A" baseAuthorityName="McDunnough" baseAuthorityYear="1931" box="[348,496,1402,1424]" class="Insecta" family="Heptageniidae" genus="Nixe" kingdom="Animalia" order="Ephemeroptera" pageId="7" pageNumber="34" phylum="Arthropoda" rank="species" species="rusticalis">
