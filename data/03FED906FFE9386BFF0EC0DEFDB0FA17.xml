<taxonomicName id="4C571393FFE9386BFF0EC0DEFDEEFD0B" authority="(Walker)" baseAuthorityName="Walker" baseAuthorityYear="1848" box="[151,554,730,756]" class="Insecta" family="Chironomidae" genus="Macropelopia" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="decedens">
