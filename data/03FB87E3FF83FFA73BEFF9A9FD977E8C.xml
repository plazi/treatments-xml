<taxonomicName id="4C524D76FF84FFA7384DFEBEFC537D61" box="[821,952,260,285]" class="Insecta" family="Bibionidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="barbicornis">barbicornis</taxonomicName>
