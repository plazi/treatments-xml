<taxonomicName id="4C524D76FF84FFA73A39FEBEFD2C7D61" authority="Linnaeus, 1767" authorityName="Linnaeus" authorityYear="1767" box="[321,711,260,285]" class="Insecta" family="Bibionidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="barbicornis">
