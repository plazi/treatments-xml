<caption id="DF5B667FF75AFFA5A195F820E63DF850" ID-DOI="http://doi.org/10.5281/zenodo.4535012" ID-Zenodo-Dep="4535012" httpUri="https://zenodo.org/record/4535012/files/figure.png" pageId="10" pageNumber="161" startId="10.[132,143,1939,1957]" targetBox="[144,1434,215,1878]" targetPageId="10">
<paragraph id="8B9B36F7F75AFFA5A195F820E63DF850" blockId="10.[132,1457,1939,2025]" pageId="10" pageNumber="161">
