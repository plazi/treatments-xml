<taxonomicName id="4C1F4D714714FFF4F561FBDFFDE1FBE1" authority="Balthasar, 1936" authorityName="Balthasar" authorityYear="1936" box="[293,635,1064,1088]" class="Insecta" family="Trogidae" genus="Trox" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="326" phylum="Arthropoda" rank="species" species="sordidatus">
