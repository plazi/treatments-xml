<taxonomicName id="4C7E3589FF6AFF4CFE1EF8453361FCB3" authorityName="Dechambre" authorityYear="1992" box="[406,541,811,835]" class="Insecta" family="Dynastidae" genus="Aspidolea" kingdom="Animalia" order="Coleoptera" pageId="161" pageNumber="160" phylum="Arthropoda" rank="species" species="bleuzeni">
<emphasis id="B90A9218FF6AFF4CFE1EF8453361FCB3" box="[406,541,811,835]" italics="true" pageId="161" pageNumber="160">A. bleuzeni</emphasis>
</taxonomicName>
