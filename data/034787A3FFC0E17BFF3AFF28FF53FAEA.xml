<typeStatus id="54558817FFC0E17BFBE5FB1DFBDEFB0A" box="[1096,1146,1186,1212]" pageId="7" pageNumber="64">type</typeStatus>
host plant, 
<emphasis id="B99AEAA7FFC0E17BFAAEFB1CFAE1FB0A" box="[1283,1349,1187,1212]" italics="true" pageId="7" pageNumber="64">
<taxonomicName id="4CEE4D36FFC0E17BFAAEFB1CFAE5FB0A" box="[1283,1345,1187,1212]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="7" pageNumber="64" phylum="Chordata" rank="genus">Salix</taxonomicName>
