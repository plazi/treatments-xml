<taxonomicName id="4CEE4D36FFC0E17BFD71FB4CFCCBFABA" box="[732,879,1267,1292]" class="Arachnida" family="Eriophyidae" genus="Aculops" kingdom="Animalia" order="Prostigmata" pageId="7" pageNumber="64" phylum="Arthropoda" rank="species" species="rhodensis">
<emphasis id="B99AEAA7FFC0E17BFD71FB4CFCCBFABA" box="[732,879,1267,1292]" italics="true" pageId="7" pageNumber="64">A. rhodensis</emphasis>
</taxonomicName>
