<taxonomicName id="4CEE4D36FFC0E17BFC3CFAA4FB81FA82" box="[913,1061,1306,1332]" class="Arachnida" family="Eriophyidae" genus="Aculops" kingdom="Animalia" order="Prostigmata" pageId="7" pageNumber="64" phylum="Arthropoda" rank="species" species="rhodensis">
<emphasis id="B99AEAA7FFC0E17BFC3CFAA4FC07FA82" box="[913,931,1307,1332]" italics="true" pageId="7" pageNumber="64">A</emphasis>
