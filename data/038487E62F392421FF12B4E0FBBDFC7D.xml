<taxonomicName id="4C2D4D732F3A2420FE7AB2A2FDC4F80C" authorityName="Kobelt &amp; Mollendorff" authorityYear="1900" box="[511,607,1980,2004]" class="Gastropoda" family="Cyclophoridae" genus="Dicharax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda" pageId="26" pageNumber="27" phylum="Mollusca" rank="genus">
<emphasis id="B959EAE22F3A2420FE7AB2A2FDC4F80C" box="[511,607,1980,2004]" italics="true" pageId="26" pageNumber="27">Dicharax</emphasis>
</taxonomicName>
(?) 
<taxonomicName id="4C2D4D732F3A2420FD0FB2A2FB93F80D" authority="(Godwin-Austen, 1914)" baseAuthorityName="Godwin-Austen" baseAuthorityYear="1914" box="[650,1032,1980,2005]" class="Gastropoda" family="Cyclophoridae" genus="Dicharax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda" pageId="26" pageNumber="27" phylum="Mollusca" rank="species" species="woodthorpei">
<emphasis id="B959EAE22F3A2420FD0FB2A2FC91F80C" box="[650,778,1980,2004]" italics="true" pageId="26" pageNumber="27">woodthorpei</emphasis>
(Godwin-Austen, 1914)
</taxonomicName>
, syntype (NHMUK 1903.7.1.3064); C: 
<taxonomicName id="4C2D4D732F3A2420FF12B2FEFDB7F820" authority="(W. T. Blanford, 1865)" baseAuthorityName="W. T. Blanford" baseAuthorityYear="1865" box="[151,556,2016,2041]" class="Gastropoda" family="Cyclophoridae" genus="Dicharax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda" pageId="26" pageNumber="27" phylum="Mollusca" rank="species" species="politus">
