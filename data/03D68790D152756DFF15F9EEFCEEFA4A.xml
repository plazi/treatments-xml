; 
<quantity id="4C879B63D150756DFEB1FAFFFE26FA4A" box="[307,399,1389,1415]" metricMagnitude="3" metricUnit="m" metricValue="3.2" pageId="15" pageNumber="16" unit="m" value="3200.0">
