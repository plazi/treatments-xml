<heading id="D09781819B2FFF9EFF5C9539212C2036" bold="true" box="[151,523,800,826]" fontSize="11" level="1" pageId="13" pageNumber="264" reason="1">
<emphasis id="B914EAFF9B2FFF9EFF5C9539212C2036" bold="true" box="[151,523,800,826]" pageId="13" pageNumber="264">
<taxonomicName id="4C604D6E9B2FFF9EFF5C953922462036" box="[151,353,800,826]" class="Insecta" family="Ptiloneuridae" genus="Triplocania" kingdom="Animalia" order="Psocodea" pageId="13" pageNumber="264" phylum="Arthropoda" rank="species" species="inpa" status="n. sp.">
<emphasis id="B914EAFF9B2FFF9EFF5C953922462036" bold="true" box="[151,353,800,826]" italics="true" pageId="13" pageNumber="264">Triplocania inpa</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22757849B2FFF9EFEA39539228D2036" box="[360,426,800,826]" pageId="13" pageNumber="264" rank="species">n. sp.</taxonomicNameLabel>
