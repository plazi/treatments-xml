<taxonomicName id="C1CD4D2EFFF8FF8AFB43FA4AFB7E07E7" authorityName="Waterhouse" authorityYear="1879" box="[1159,1271,1421,1442]" class="Insecta" family="Lycidae" genus="Lipernes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" phylum="Arthropoda" rank="genus">
