<taxonomicName id="C1CD4D2EFFF8FF8AFC12FA0DFBD1079A" authorityName="Motschulsky" authorityYear="1861" box="[982,1112,1482,1503]" class="Insecta" family="Lycidae" genus="Lycostomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" phylum="Arthropoda" rank="genus">
