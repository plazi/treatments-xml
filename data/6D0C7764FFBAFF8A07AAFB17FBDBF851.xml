<taxonomicName id="22A5BDF1FFBAFF8907FBF827FE52F819" authority="(Ausserer)" baseAuthorityName="Ausserer" baseAuthorityYear="1875" box="[198,511,1987,2013]" class="Arachnida" family="Theraphosidae" genus="Tliltocatl" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="vagans">
