<taxonomicName id="22A5BDF1FFB9FF8A069EF8D7FDFCF889" authorityName="Pocock" authorityYear="1901" box="[419,593,1843,1869]" class="Arachnida" family="Theraphosidae" genus="Citharacanthus" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
