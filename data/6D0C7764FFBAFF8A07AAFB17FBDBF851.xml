, 
<location id="E07A90A9FFB9FF8A057AFD33FC6EFD35" LSID="urn:lsid:plazi:treatment:6D0C7764FFBAFF8A07AAFB17FBDBF851:E07A90A9FFB9FF8A057AFD33FC6EFD35" box="[583,963,727,753]" country="Costa Rica" name="Rincon de la Vieja National Park" pageId="10" pageNumber="11" stateProvince="Guanacaste Province">Rincon de la Vieja National Park</location>
; 
