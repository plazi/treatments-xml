<taxonomicName id="1C6E605859AD14CA7F7B33C28EEB3379" authority="Motschulsky, 1862" authorityName="Motschulsky" authorityYear="1862" class="Insecta" family="Carabidae" genus="Tachyina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tachyina (Tachyta) picipes" order="Coleoptera" pageId="9" pageNumber="96" phylum="Arthropoda" rank="species" species="picipes" subGenus="Tachyta">Tachyta picipes</taxonomicName>
