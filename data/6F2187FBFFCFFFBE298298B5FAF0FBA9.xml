<taxonomicName id="20884D6EFFCFFFBE289D9900FD99FC81" box="[392,534,770,796]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="43" phylum="Arthropoda" rank="species" species="hargreavesi">
