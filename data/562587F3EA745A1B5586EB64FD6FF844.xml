Notes sur 
<taxonomicName id="198C4D66EA745A1B5436EB67FD3CF98E" authority="(Hiern) Keay" authorityName="Keay" baseAuthorityName="Hiern" box="[303,651,1621,1650]" class="Magnoliopsida" family="Rubiaceae" genus="Oxyanthus" kingdom="Plantae" order="Gentianales" pageId="2" pageNumber="243" phylum="Tracheophyta" rank="species" species="subpunctatus">Oxyanthus subpunctatus</taxonomicName>
</emphasis>
