<collectingCountry id="F34776030367EF791D9D78BCFC1B6AA8" box="[937,986,1999,2025]" name="Iran" pageId="24" pageNumber="92">Iran</collectingCountry>
, the 
<typeStatus id="54EB88310367EF791A3978BCFBFE6AA8" box="[1037,1087,1999,2025]" pageId="24" pageNumber="92">type</typeStatus>
