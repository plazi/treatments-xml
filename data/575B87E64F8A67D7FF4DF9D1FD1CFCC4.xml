<taxonomicName id="18F24D734F8A67D6FC26FC53FBE7FC73" authorityName="Platnick &amp; Baehr" authorityYear="2006" box="[942,1084,994,1017]" class="Arachnida" family="Gnaphosidae" genus="Nomindra" kingdom="Animalia" order="Araneae" pageId="230" pageNumber="231" phylum="Arthropoda" rank="species" species="berrimah">
