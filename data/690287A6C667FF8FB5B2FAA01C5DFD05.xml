. Habitus. 
<emphasis id="D3DFEAA2C664FF89B4B9F86219A1F84C" bold="true" box="[412,425,1976,1997]" pageId="2" pageNumber="53">1</emphasis>
, 
<taxonomicName id="26AB4D33C664FF89B490F8621AEAF84F" authority="Monte" authorityName="Monte" box="[437,738,1976,1998]" class="Insecta" family="Tingidae" genus="Niborskiana" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="53" phylum="Arthropoda" rank="species" species="gracilis">
