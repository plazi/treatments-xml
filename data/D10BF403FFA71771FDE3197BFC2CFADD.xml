<paragraph id="591D4515FFA71770FF091DBBFAFDF854" blockId="18.[187,1398,1963,2025]" pageId="18" pageNumber="122">
<taxonomicName id="9EA23E96FFA71770FF091DBBFE5AF879" authority="Korn &amp; Miao &amp; Bockwinkel, 2022" authorityName="Korn &amp; Miao &amp; Bockwinkel" authorityYear="2022" box="[189,466,1963,1989]" class="Cephalopoda" family="Trigonoceratidae" genus="Vestinautilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nautilida" pageId="18" pageNumber="122" phylum="Mollusca" rank="species" species="concinnus" status="sp. nov.">
