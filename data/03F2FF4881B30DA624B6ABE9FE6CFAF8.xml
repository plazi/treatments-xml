, 
<taxonomicName id="4C5B35DD81B30DA62690AE44FCC7FADF" box="[689,841,1338,1362]" class="Insecta" family="Cerococcidae" genus="Cerococcus" kingdom="Animalia" order="Hemiptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="species" species="artemisiae">
