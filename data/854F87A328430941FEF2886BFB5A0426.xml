<taxonomicName id="CAE64D3628430942FC3A8FA1FAAB0346" authority="Martins &amp; Galileo, 2003" authorityName="Martins &amp; Galileo" authorityYear="2003" box="[967,1357,1534,1558]" class="Insecta" family="Cerambycidae" genus="Anelaphus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="178" phylum="Arthropoda" rank="species" species="sparsus">
