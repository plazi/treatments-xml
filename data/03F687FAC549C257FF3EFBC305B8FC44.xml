<taxonomicName id="4C5F4D6FC54AC256FD7CFADC069CF860" box="[725,976,1968,1992]" class="Gastropoda" family="Palaeozygopleuridae" genus="Cimrmaniela" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Murchisoniina" pageId="6" pageNumber="475" phylum="Mollusca" rank="species" species="smoljaki">
