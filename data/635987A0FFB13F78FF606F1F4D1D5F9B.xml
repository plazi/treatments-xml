<collectingCountry id="93E77626FFB13F78FE9068F44DE35CFA" box="[359,426,1426,1451]" name="China" pageId="2" pageNumber="73">China</collectingCountry>
(
<collectingRegion id="2934F854FFB13F78FE4E68F44E5E5CFA" box="[441,535,1426,1451]" country="China" name="Shaanxi" pageId="2" pageNumber="73">Shaanxi</collectingRegion>
