<subSubSection id="C3716549871CAA0346A0FD88DA4CA92B" pageId="7" pageNumber="8" type="discussion">
<paragraph id="8BD436C2871CAA0346A0FD88DA24A8B1" blockId="7.[189,324,634,660]" box="[189,324,634,660]" pageId="7" pageNumber="8">
<heading id="D09C81AE871CAA0346A0FD88DA24A8B1" bold="true" box="[189,324,634,660]" fontSize="11" level="3" pageId="7" pageNumber="8" reason="3">
