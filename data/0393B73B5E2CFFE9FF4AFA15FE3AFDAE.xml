, 
<taxonomicName id="4C3A7DAE5E2DFFEEFEACF87CFDB4F80F" authority="Nascimento &amp; Heffern, 2018" authorityName="Nascimento &amp; Heffern" authorityYear="2018" box="[369,609,1926,1948]" class="Insecta" family="Cerambycidae" genus="Leptostylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="295" phylum="Arthropoda" rank="species" species="obrienorum" status="sp. nov.">
<emphasis id="B94EDA3F5E2DFFEEFEACF87CFDB4F80F" box="[369,609,1926,1948]" italics="true" pageId="3" pageNumber="295">Leptostylus obrienorum</emphasis>
