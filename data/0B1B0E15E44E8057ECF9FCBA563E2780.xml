<paragraph id="91C686618C575CC283FB1F04CD89D2D6" pageId="12" pageNumber="53">
<taxonomicName id="05E2E612E9405A56E1D73A6C8C9856DE" class="Insecta" family="Curculionidae" genus="Curculio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Curculio nigrinus" order="Coleoptera" pageId="12" pageNumber="53" phylum="Arthropoda" rank="species" species="nigrinus">Curculio nigrinus</taxonomicName>
