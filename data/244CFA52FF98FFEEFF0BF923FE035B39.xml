<taxonomicName id="6BE530C7FF99FFEEFB9FFF51FAFE59C9" authority="Linnaeus" authorityName="Linnaeus" box="[1027,1369,152,178]" class="Actinopterygii" family="Serranidae" genus="Serranus" kingdom="Animalia" order="Perciformes" pageId="17" pageNumber="18" phylum="Chordata" rank="species" species="cabrilla">
<emphasis id="9E919756FF99FFEEFB9FFF51FB7559CA" box="[1027,1234,152,177]" italics="true" pageId="17" pageNumber="18">Serranus cabrilla</emphasis>
