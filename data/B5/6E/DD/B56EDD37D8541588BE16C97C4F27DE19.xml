<taxonomicName authority="Dejean, 1834" authorityName="Dejean" authorityYear="1834" class="Insecta" family="Tenebrionidae" genus="Blacodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Blacodes" order="Coleoptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="genus">Blacodes</taxonomicName>
