<taxonomicName authority="de Laporte, 1840" authorityName="de Laporte" authorityYear="1840" class="Insecta" family="Tenebrionidae" genus="Blenosia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Blenosia" order="Coleoptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="genus">Blenosia</taxonomicName>
