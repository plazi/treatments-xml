<taxonomicName authority="Wilhelm Ferdinand Erichson, 1843" authorityName="Wilhelm Ferdinand Erichson" authorityYear="1843" class="Insecta" family="Tenebrionidae" genus="Emmallus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Emmallus" order="Coleoptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="genus">Emmallus</taxonomicName>
