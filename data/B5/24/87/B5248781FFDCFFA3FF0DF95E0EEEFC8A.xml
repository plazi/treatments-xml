<taxonomicName authorityName="Distant" authorityYear="1906" box="[846,953,151,176]" class="Insecta" family="Cicadidae" genus="Melampsalta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" phylum="Arthropoda" rank="species" species="germaini">germaini</taxonomicName>
