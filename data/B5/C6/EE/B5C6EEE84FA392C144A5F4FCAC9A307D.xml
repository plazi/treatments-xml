<subSubSection type="nomenclature">
<paragraph pageNumber="158">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181653" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole pinicola Wilson" lsidName-HNS="Pheidole pinicola Wilson" order="Hymenoptera" pageNumber="158" phylum="Arthropoda" rank="species" species="pinicola">Pheidole pinicola</taxonomicName>
