<subSubSection pageId="6" pageNumber="103" type="distribution">
<paragraph pageId="6" pageNumber="103">Distribution in Nepal.</paragraph>
<paragraph pageId="6" pageNumber="103">Mustang.</paragraph>
</subSubSection>
<subSubSection pageId="6" pageNumber="103" type="habitat">
<paragraph pageId="6" pageNumber="103">Habitat.</paragraph>
<paragraph pageId="6" pageNumber="103">High mountain steppes and alpine mats, among subalpine shrubs and on rocky ledges.</paragraph>
</subSubSection>
