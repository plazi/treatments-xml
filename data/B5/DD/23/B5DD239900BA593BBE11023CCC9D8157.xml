<paragraph pageId="null" pageNumber="630">
<taxonomicName authority="Hudson" class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="630" phylum="Tracheophyta" rank="species" species="purpurea">
