e 
<taxonomicName box="[466,582,1303,1326]" class="Diplopoda" family="Pseudonannolenidae" genus="Pseudonannolene" kingdom="Animalia" order="Spirostreptida" pageId="9" pageNumber="370" phylum="Arthropoda" rank="species" species="strinatii">
