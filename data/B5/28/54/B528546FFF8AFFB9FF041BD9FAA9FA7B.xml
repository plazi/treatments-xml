<collectingCountry box="[631,720,1347,1373]" name="Panama" pageId="4" pageNumber="205">Panama</collectingCountry>
previously reported from the Canal Zone, 
<collectingRegion box="[1191,1287,1346,1372]" country="Panama" name="Chiriqui" pageId="4" pageNumber="205">Chiriqui</collectingRegion>
, 
<collectingRegion box="[1298,1366,1346,1372]" country="Panama" name="Cocle" pageId="4" pageNumber="205">Coclé</collectingRegion>
, 
<collectingRegion box="[1376,1447,1346,1372]" country="Panama" name="Colon" pageId="4" pageNumber="205">Colon</collectingRegion>
, 
<collectingRegion box="[136,212,1382,1408]" country="Panama" name="Darien" pageId="4" pageNumber="205">Darién</collectingRegion>
and 
<collectingCountry box="[267,356,1382,1408]" name="Panama" pageId="4" pageNumber="205">Panamá</collectingCountry>
provinces (
<bibRefCitation author="Stotler, R. &amp; Salazar Allen, N. &amp; Gradstein, S. R. &amp; McGuinness, W. &amp; Whittemore, A. &amp; Chung, C." box="[486,690,1382,1409]" pageId="4" pageNumber="205" pagination="167 - 195" refId="ref25959" refString="Stotler, R., Salazar Allen, N., Gradstein, S. R., McGuinness, W., Whittemore, A. &amp; Chung, C. (1998) A checklist of the hepatics and anthocerotes of Panama. Tropical Bryology 15: 167 - 195." type="journal article" year="1998">Stotler et al. 1998</bibRefCitation>
