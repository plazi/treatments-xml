<emphasis box="[731,1224,1382,1409]" italics="true" pageId="4" pageNumber="205">
<taxonomicName authorityName="Stephani" authorityYear="1913" baseAuthorityName="Spruce" box="[731,869,1383,1409]" class="Jungermanniopsida" family="Lejeuneaceae" genus="Ceratolejeunea" kingdom="Plantae" order="Porellales" pageId="4" pageNumber="205" phylum="Marchantiophyta" rank="species" species="maritima">C. maritima</taxonomicName>
