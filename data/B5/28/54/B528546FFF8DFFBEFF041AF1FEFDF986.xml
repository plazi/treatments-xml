<collectingCountry box="[739,837,1562,1588]" name="Trinidad and Tobago" pageId="3" pageNumber="204">Trinidad</collectingCountry>
, 
<collectingCountry box="[853,924,1562,1588]" name="Brazil" pageId="3" pageNumber="204">Brazil</collectingCountry>
, 
<collectingCountry box="[940,1055,1562,1588]" name="Venezuela" pageId="3" pageNumber="204">Venezuela</collectingCountry>
(
<bibRefCitation author="Fulford, M. H." box="[1076,1231,1562,1589]" pageId="3" pageNumber="204" pagination="1 - 172" refId="ref21703" refString="Fulford, M. H. (1963) Manual of the Leafy Hepaticae of Latin America - Part I. Memoirs of The New York Botanical Garden 11: 1 - 172." type="journal article" year="1963">Fulford 1963</bibRefCitation>
), 
<collectingCountry box="[1256,1366,1562,1588]" name="Colombia" pageId="3" pageNumber="204">Colombia</collectingCountry>
(
<bibRefCitation author="Uribe M. &amp; Gradstein, S. R." pageId="3" pageNumber="204" pagination="1 - 99" refId="ref26117" refString="Uribe M., J. &amp; Gradstein, S. R. (1998) Catalogue of the Hepaticae and Anthocerotae of Colombia. Bryophytorum Bibliotheca 53: 1 - 99." type="journal article" year="1998">Uribe &amp; Gradstein 1998</bibRefCitation>
