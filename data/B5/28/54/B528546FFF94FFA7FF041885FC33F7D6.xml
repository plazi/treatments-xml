<collectingCountry box="[866,937,2030,2056]" name="Brazil" pageId="26" pageNumber="227">Brazil</collectingCountry>
, 
<collectingCountry box="[947,1068,2030,2056]" name="Costa Rica" pageId="26" pageNumber="227">Costa Rica</collectingCountry>
incl. Cocos Island, and from 
<collectingCountry box="[1392,1451,2030,2056]" name="Cuba" pageId="26" pageNumber="227">Cuba</collectingCountry>
to 
<collectingCountry box="[164,259,2066,2092]" name="Trinidad and Tobago" pageId="26" pageNumber="227">Trinidad</collectingCountry>
