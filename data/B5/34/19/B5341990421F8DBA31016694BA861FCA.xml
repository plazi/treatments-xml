<taxonomicName class="Insecta" family="Dytiscidae" genus="Exocelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exocelina varirata" order="Coleoptera" pageId="19" pageNumber="64" phylum="Arthropoda" rank="species" species="varirata">Exocelina varirata</taxonomicName>
