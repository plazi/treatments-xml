<taxonomicName authorityName="Pallary" authorityYear="1916" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis abbreviata" order="Pteropoda" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="abbreviata">
