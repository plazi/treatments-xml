<taxonomicName authorityName="Noltie" authorityYear="1999" class="Liliopsida" family="Poaceae" genus="Stipa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Stipa milleri" order="Poales" pageId="9" pageNumber="106" phylum="Tracheophyta" rank="species" species="milleri">Stipa milleri</taxonomicName>
