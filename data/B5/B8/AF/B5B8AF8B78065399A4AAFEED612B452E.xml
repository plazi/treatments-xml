<taxonomicName authorityName="L. B. Cai" authorityYear="2003" class="Liliopsida" family="Poaceae" genus="Ptilagrostis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ptilagrostis macrospicula" order="Poales" pageId="9" pageNumber="106" phylum="Tracheophyta" rank="species" species="macrospicula">Ptilagrostis macrospicula</taxonomicName>
