<emphasis italics="true" pageId="0" pageNumber="53">Aesculus</emphasis>
</taxonomicName>
L., 
<taxonomicName class="Magnoliopsida" family="Betulaceae" genus="Alnus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Alnus" order="Fagales" pageId="0" pageNumber="53" phylum="Tracheophyta" rank="genus">
