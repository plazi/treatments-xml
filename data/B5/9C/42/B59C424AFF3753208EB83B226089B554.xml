<taxonomicName class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Abies" order="Pinales" pageId="0" pageNumber="53" phylum="Tracheophyta" rank="genus">
