<taxonomicName class="Magnoliopsida" family="Cornaceae" genus="Cornus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cornus" order="Cornales" pageId="0" pageNumber="53" phylum="Tracheophyta" rank="genus">
