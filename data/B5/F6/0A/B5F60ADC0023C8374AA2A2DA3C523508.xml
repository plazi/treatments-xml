<taxonomicName authority="Achard, 1921" authorityName="Achard" authorityYear="1921" class="Insecta" family="Staphylinidae" genus="Lepteroscapha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lepteroscapha filiformis" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="filiformis">Lepteroscapha filiformis</taxonomicName>
