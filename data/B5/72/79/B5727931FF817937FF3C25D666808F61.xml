<taxonomicName box="[496,612,778,803]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="48" pageNumber="49" phylum="Annelida" rank="genus">
