<taxonomicName authority="Chen, 1938" authorityName="Chen" authorityYear="1938" box="[852,1357,639,664]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="26" pageNumber="27" phylum="Annelida" rank="subSpecies" species="multitheca" subSpecies="multitheca">
