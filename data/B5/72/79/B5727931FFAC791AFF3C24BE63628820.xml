<collectingCountry box="[678,745,953,978]" name="China" pageId="29" pageNumber="30">China</collectingCountry>
(
<collectingRegion box="[765,854,954,978]" country="China" name="Yunnan" pageId="29" pageNumber="30">Yunnan</collectingRegion>
) and 
<collectingCountry box="[927,1038,954,978]" name="Myanmar" pageId="29" pageNumber="30">Myanmar</collectingCountry>
, and known from 
<collectingCountry box="[1262,1329,954,977]" name="Japan" pageId="29" pageNumber="30">Japan</collectingCountry>
; recently reported from 
<collectingCountry box="[313,380,989,1014]" name="China" pageId="29" pageNumber="30">China</collectingCountry>
(
<collectingRegion box="[394,489,989,1014]" country="China" name="Sichuan" pageId="29" pageNumber="30">Sichuan</collectingRegion>
; 
<collectingRegion box="[501,597,989,1014]" country="China" name="Guizhou" pageId="29" pageNumber="30">Guizhou</collectingRegion>
) and 
<collectingCountry box="[661,756,989,1014]" name="Vietnam" pageId="29" pageNumber="30">Vietnam</collectingCountry>
(
<bibRefCitation author="Easton" box="[770,914,990,1014]" pageId="29" pageNumber="30" refString="Easton, E. G. (1981) Japanese earthworms: a synopsis of the Megadrile species (Oligochaeta). Bulletin of the British Museum of Natural History (Zoology), 40 (2), 33 - 65." type="journal article" year="1981">Easton 1981</bibRefCitation>
