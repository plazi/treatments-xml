<taxonomicName box="[268,384,548,573]" class="Oligochaeta" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="68" pageNumber="69" phylum="Annelida" rank="genus">
