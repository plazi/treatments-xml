<taxonomicName box="[987,1103,476,501]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="68" pageNumber="69" phylum="Annelida" rank="genus">
