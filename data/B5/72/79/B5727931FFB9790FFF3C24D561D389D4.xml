, 
<collectingCountry box="[151,262,1298,1322]" name="Myanmar" pageId="8" pageNumber="9">Myanmar</collectingCountry>
, 
<collectingCountry box="[277,395,1297,1322]" name="Cambodia" pageId="8" pageNumber="9">Cambodia</collectingCountry>
, 
