des 
<collectionCode box="[251,285,1039,1060]" pageId="30" pageNumber="1383">HT</collectionCode>
(und ein 
<specimenCount box="[389,410,1044,1065]" pageId="30" pageNumber="1383" type="female">♀</specimenCount>
