(= 
<taxonomicName id="71DF4D7DA556FFE3AC264071FBFD1EF8" authority="(Walker, 1874))" authorityName="" baseAuthorityName="Walker" baseAuthorityYear="1874" box="[789,1120,1645,1668]" class="Insecta" family="Ichneumonidae" genus="Zatypota" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="1173" phylum="Arthropoda" rank="species" species="albicoxa">
