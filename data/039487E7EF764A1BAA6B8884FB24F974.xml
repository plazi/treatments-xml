<paragraph id="8B8236F1EF784A1BABA38900FAD7FA6E" blockId="24.[189,1398,1379,1441]" pageId="24" pageNumber="25">
<taxonomicName id="4C3D4D72EF784A1BABA38900FDC3FAB2" authority="Fagan-Jeffries &amp; Bird &amp; Giannotta &amp; Bradford &amp; Austin, 2022" authorityName="Fagan-Jeffries &amp; Bird &amp; Giannotta &amp; Bradford &amp; Austin" authorityYear="2022" box="[189,576,1379,1405]" class="Insecta" family="Braconidae" genus="Glyptapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="andamookaensis" status="sp. nov.">
