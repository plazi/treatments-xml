<emphasis id="BFF9EAE3324FFFFCFF4DF81AFEC5F87A" bold="true" box="[151,300,1937,1959]" pageId="3" pageNumber="294">FIGURE 3–4.</emphasis>
Habitus of 
<taxonomicName id="4A8D4D72324FFFFCFE79F819FE19F87B" authorityName="Latreille" authorityYear="1797" box="[419,496,1938,1959]" class="Insecta" family="Staphylinidae" genus="Lesteva" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="294" phylum="Arthropoda" rank="genus">
<emphasis id="BFF9EAE3324FFFFCFE79F819FE19F87B" box="[419,496,1938,1959]" italics="true" pageId="3" pageNumber="294">Lesteva</emphasis>
</taxonomicName>
: 3− 
<taxonomicName id="4A8D4D72324FFFFCFDC2F819FD6CF87A" authority="Shavrin, 2014" authorityName="Shavrin" authorityYear="2014" box="[536,645,1937,1959]" class="Insecta" family="Staphylinidae" genus="Lesteva" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="294" phylum="Arthropoda" rank="species" species="ruzickai" status="sp.n.">
<emphasis id="BFF9EAE3324FFFFCFDC2F819FD6CF87A" box="[536,645,1937,1959]" italics="true" pageId="3" pageNumber="294">L. ruzickai</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A4CA5798324FFFFCFD51F81AFD54F87A" box="[651,701,1937,1958]" pageId="3" pageNumber="294" rank="species">sp.n.</taxonomicNameLabel>
, 4− 
<taxonomicName id="4A8D4D72324FFFFCFD38F819FC48F87A" authorityName="Rougemont" authorityYear="2000" box="[738,929,1937,1959]" class="Insecta" family="Staphylinidae" genus="Lesteva" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="294" phylum="Arthropoda" rank="species" species="septemmaculata">
<emphasis id="BFF9EAE3324FFFFCFD38F819FC48F87A" box="[738,929,1937,1959]" italics="true" pageId="3" pageNumber="294">L. septemmaculata</emphasis>
</taxonomicName>
