<taxonomicName id="4C201FC9FFB582460AF86339FED7BB48" authorityName="Volynkin &amp; Černý" authorityYear="2020" baseAuthorityName="Strand" baseAuthorityYear="1917" box="[207,372,1590,1617]" class="Insecta" family="Arctiidae" genus="Miltochrista" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="93" phylum="Arthropoda" rank="species" species="alikangiae">
