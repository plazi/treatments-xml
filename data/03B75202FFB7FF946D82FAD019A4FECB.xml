<taxonomicName id="4C1E9897FFB7FF956D82FA251BE2FA46" authority="Gorochov, 2015" authorityName="Gorochov" authorityYear="2015" box="[151,661,1464,1486]" class="Insecta" family="Rhaphidophoridae" genus="Tamdaotettix" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="341" phylum="Arthropoda" rank="species" species="curvatus" subGenus="Laotettix">
