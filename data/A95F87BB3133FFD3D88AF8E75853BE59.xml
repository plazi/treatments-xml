<taxonomicName id="E6F64D2E3130FFD3D8C3F9B45891B0D9" authority="(Meigen, 1838)" baseAuthorityName="Meigen" baseAuthorityYear="1838" box="[151,509,1663,1686]" class="Insecta" family="Chironomidae" genus="Potthastia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="349" phylum="Arthropoda" rank="species" species="gaedii">
