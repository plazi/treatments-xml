<taxonomicName id="4C694D04FFB6FF8010CE0835FACFF9FB" box="[1216,1342,1580,1605]" class="Insecta" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="518" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
