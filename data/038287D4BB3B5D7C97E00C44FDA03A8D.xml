<taxonomicName id="4C2B4D41BB3D5D7C96CF0A6FFDEE3A3C" authorityName="Lopes" authorityYear="1946" box="[440,563,259,285]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="augusta">
