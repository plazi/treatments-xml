, 
<date id="5F3A10021C76534C56BBFDDD275EE21A" box="[1010,1101,583,609]" pageId="61" pageNumber="62" value="1919-11">xi-1919</date>
; Depok, alt. 
<quantity id="EC7C9B271C76534C5193FDDD2631E21A" box="[1242,1314,583,609]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="61" pageNumber="62" unit="m" value="100.0">100 m</quantity>
, 
<date id="5F3A10021C76534C5067FDDD268BE21A" box="[1326,1432,583,609]" pageId="61" pageNumber="62" value="1918-08">viii-1918</date>
; Mt Tugu, near Cibeber, alt. 
<quantity id="EC7C9B271C76534C5487FDF12136E2FE" box="[462,549,619,645]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="61" pageNumber="62" unit="m" value="1000.0">1000 m</quantity>
, 
<date id="5F3A10021C76534C5778FDF12198E2FE" box="[561,651,619,645]" pageId="61" pageNumber="62" value="1923-06">vi-1923</date>
; Mt Panisian, near Bogor, alt. 
<quantity id="EC7C9B271C76534C56A9FDF1273AE2FE" box="[992,1065,619,645]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="61" pageNumber="62" unit="m" value="700.0">700 m</quantity>
, 
<date id="5F3A10021C76534C517CFDF12784E2FE" box="[1077,1175,619,645]" pageId="61" pageNumber="62" value="1923-12">xii-1923</date>
; Mt Salak, alt. 
<quantity id="EC7C9B271C76534C5009FDF1268BE2FE" box="[1344,1432,619,645]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="61" pageNumber="62" unit="m" value="1000.0">1000 m</quantity>
