<taxonomicName id="68AAD03CACABA0CD4712E216E234E677" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole titanis" order="Hymenoptera" pageId="116" pageNumber="117" phylum="Arthropoda" rank="species" species="titanis">Pheidole titanis</taxonomicName>
(Wheeler) and 
<taxonomicName id="DBA82220825CFA73249B57C4E2320F15" family="Scolytidae" lsidName="" pageId="116" pageNumber="117" rank="family">Scolytidae</taxonomicName>
