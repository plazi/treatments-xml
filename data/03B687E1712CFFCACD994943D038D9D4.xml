<taxonomicName id="4C1F4D74712CFFCAC9E949FAD331D8E3" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Insecta" family="Vespidae" genus="Vespa" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="emarginata">
