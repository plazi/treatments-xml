: 
<number id="3DD724D0712CFFCACE4B49D8D13DD8E0" box="[837,875,1712,1733]" pageId="7" pageNumber="8" value="137.0">137</number>
[erroneously as 
<taxonomicName id="4C1F4D74712CFFCAC91D49D8D749D8E0" authority="De Geer" authorityName="De Geer" box="[1043,1311,1712,1734]" class="Insecta" family="Pompilidae" genus="Sphex" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="maxillosus">
