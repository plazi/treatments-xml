<taxonomicName id="4C504D06FFD8FFDEE0ACF9066C9EF904" authorityName="Kritsky, Boeger &amp; Jegu" authorityYear="1996" box="[993,1175,1732,1758]" class="Monogenea" family="Ancyrocephalidae" genus="Mymarothecium" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dactylogyridea" pageId="4" pageNumber="233" phylum="Platyhelminthes" rank="genus">
