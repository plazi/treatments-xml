<taxonomicName id="AFA35B9CFFD8FFF28A31D52FFBE6CC66" authorityName="de Blauwe &amp; Gordon" authorityYear="2014" box="[885,1032,1365,1389]" class="Gymnolaemata" family="Membranicellariidae" genus="Membranicellaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="5" pageNumber="216" phylum="Bryozoa" rank="species" species="pellucens">
