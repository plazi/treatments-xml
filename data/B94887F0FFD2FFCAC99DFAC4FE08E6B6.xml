<taxonomicName id="F6E14D65FFD2FFCAC9EDFAC4FDE6E532" authority="A. Agassiz, 1872" authorityName="A. Agassiz" authorityYear="1872" box="[229,611,1293,1319]" class="Echinoidea" family="Temnopleuridae" kingdom="Animalia" order="Camarodonta" pageId="2" pageNumber="99" phylum="Echinodermata" rank="family">TEMNOPLEURIDAE A. Agassiz, 1872</taxonomicName>
