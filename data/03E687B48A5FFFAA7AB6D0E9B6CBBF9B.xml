<taxonomicName id="4C4F4D218A50FFAB7E87D1D9B36EBDF0" authority="Luo &amp; Segers, 2020" authorityName="Luo &amp; Segers" authorityYear="2020" box="[1190,1366,1941,1965]" class="Eurotatoria" family="Lepadellidae" genus="Lepadella" kingdom="Animalia" order="Ploima" pageId="8" pageNumber="379" phylum="Rotifera" rank="species" species="weijiai" status="n. sp.">
