<taxonomicName id="4C753F91FFD1985BFB4C2C0E4C1DC589" authorityName="Lelej" authorityYear="1996" box="[1250,1405,876,901]" class="Insecta" family="Mutillidae" genus="Krombeinidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">
