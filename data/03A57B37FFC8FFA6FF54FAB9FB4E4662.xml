<elevation id="00212D12FFC8FFA6FB6BFAF7FAA5457E" box="[1192,1292,1370,1395]" metricMagnitude="1" metricUnit="m" metricValue="6.0" metricValueMax="7.0" metricValueMin="5.0" pageId="13" pageNumber="114" unit="m" value="60.0" valueMax="70.0" valueMin="50.0">
