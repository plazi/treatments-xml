<subSubSection id="F2467CB353F75B09996F511AC8C72C17" type="reference_group">
<paragraph id="4F6263E979E6AE927EFE7938FB4F08CA" pageNumber="132">
<taxonomicName id="10C8B8EF9436290D35F85A38B42616FE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29179" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica pressilabris Nylander" order="Hymenoptera" pageNumber="132" phylum="Arthropoda" rank="species" species="pressilabris">Formica pressilabris Nylander</taxonomicName>
