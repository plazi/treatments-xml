<taxonomicName id="27F690B66E80BBDCA948E7C79D0D3A18" authority="Linnæus, 1758" authorityName="Fabricius" authorityYear="1775" class="Insecta" family="Salticidae" genus="Aranea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aranea pubescens" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pubescens">Aranea pubescens</taxonomicName>
