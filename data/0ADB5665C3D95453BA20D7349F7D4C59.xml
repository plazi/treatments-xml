</taxonomicName>
; Surigao del Norte, Siargao Island, Municipality of Del Carmen, Mahayahay, 
<geoCoordinate id="E9DEE8E4B17F3B330220E227EF5AEB0B" degrees="9.86494" direction="north" orientation="latitude" precision="1" value="9.86494">N9.86494</geoCoordinate>
, 
<geoCoordinate id="1FDABEA3F45A361E4898D91AD012D9BF" degrees="126.03358" direction="east" orientation="longitude" precision="1" value="126.03358">E126.03358</geoCoordinate>
, 82.9
<normalizedToken id="A02B57E65E47BADA3E1A86897471F547" originalValue="±">+/-</normalizedToken>
