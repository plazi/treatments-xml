<number id="3DF924ECE263FF90AFE8A8CBCCC7FBA1" box="[151,188,1951,1975]" pageId="9" pageNumber="10" value="0.3">0.3</number>
, ALE 
<number id="3DF924ECE263FF90AE7AA8CBCD50FBA1" box="[261,299,1951,1975]" pageId="9" pageNumber="10" value="0.2">0.2</number>
