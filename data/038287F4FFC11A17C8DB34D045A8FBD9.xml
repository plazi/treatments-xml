<subSubSection id="C3316569FFC11A17C88931E940FFF88D" box="[197,1253,1472,1496]" pageId="15" pageNumber="16" type="etymology">
<paragraph id="8B9436E2FFC11A17C88931E940FFF88D" blockId="15.[151,1437,356,1676]" box="[197,1253,1472,1496]" pageId="15" pageNumber="16">
<emphasis id="B95FEAF0FFC11A17C88931E94551F88D" bold="true" box="[197,331,1472,1496]" pageId="15" pageNumber="16">Etymology.</emphasis>
