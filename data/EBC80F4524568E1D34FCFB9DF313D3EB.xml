<taxonomicName id="A49EE58E755471F520C5F6047FC6724B" authorityName="Casey" authorityYear="1924" class="Insecta" family="Carabidae" genus="Triplectrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="647" pageNumber="648" phylum="Arthropoda" rank="species" species="paulus">
