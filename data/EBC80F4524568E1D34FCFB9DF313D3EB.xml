<paragraph id="57EC0A3F12B375B5DA657ABD88048AF2" pageId="647" pageNumber="648">
<taxonomicName id="45C3A9514F6824C42C0AB807D6FB905A" authorityName="LeConte" authorityYear="1863" class="Insecta" family="Carabidae" genus="Gynandrotarsus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="647" pageNumber="648" phylum="Arthropoda" rank="species" species="opaculus">Gynandrotarsus opaculus</taxonomicName>
