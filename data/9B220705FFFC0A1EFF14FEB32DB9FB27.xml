<taxonomicName id="D48BCD90FFFC0A1EFEDBFEB32E3AFE7F" authority="Leach, 1818" authorityName="Leach" authorityYear="1818" box="[302,535,374,400]" class="Malacostraca" family="Cirolanidae" genus="Cirolana" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="317" phylum="Arthropoda" rank="genus">
