: Male, 
<date id="FFDE1020FFCEFFBDBCC65BCBFD3875A9" box="[473,641,709,734]" pageId="26" pageNumber="427" value="1997-08-01" valueMax="1997-08-13" valueMin="1997-08-01">
<collectingDate id="EF9AE9C8FFCEFFBDBCC65BCBFD3875A9" box="[473,641,709,734]" pageId="26" pageNumber="427" value="1997-08-01" valueMax="1997-08-13" valueMin="1997-08-01">1.-13.viii.1997</collectingDate>
</date>
, 
<location id="8EBF603BFFCEFFBDBF8C5BCBFCE775A9" LSID="urn:lsid:plazi:treatment:03C987F6FFCEFFBDBD885B54FA847388:8EBF603BFFCEFFBDBF8C5BCBFCE775A9" box="[659,862,709,734]" name="Hawaiian Islands" pageId="26" pageNumber="427">Hawaiian Islands</location>
; O‘ahu, ‘Ewa, suburban area, 
<collectingMethod id="52214EF7FFCEFFBDB9D15BCBFAD875AA" box="[1230,1377,709,734]" pageId="26" pageNumber="427">Malaise trap</collectingMethod>
