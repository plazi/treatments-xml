Material: 
<emphasis id="B914EAF2FFCEFFBDBC155BCBFECE75A9" bold="true" box="[266,375,709,734]" pageId="26" pageNumber="427">
<typeStatus id="54DB8842FFCEFFBDBC155BCBFECE75A9" box="[266,375,709,734]" pageId="26" pageNumber="427">Holotype</typeStatus>
