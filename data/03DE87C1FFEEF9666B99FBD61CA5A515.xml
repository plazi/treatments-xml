<typeStatus id="54CC8875FFEEF9666BC9FAD91953A515" box="[199,258,1365,1389]" pageId="5" pageNumber="6">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C774D54FFEEF9666A67FADA1B5EA515" authority="Preyssler, 1790" authorityName="Preyssler" authorityYear="1790" box="[361,783,1365,1389]" class="Arachnida" family="Scorpionidae" genus="Scorpio" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Scorpiones" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tetrachelatus">
<emphasis id="B903EAC5FFEEF9666A67FADA1A08A515" box="[361,601,1365,1389]" italics="true" pageId="5" pageNumber="6">Scorpio tetrachelatus</emphasis>
Preyssler, 
<number id="3DBF24F0FFEEF96669DAFAD91B5EA515" box="[724,783,1365,1389]" pageId="5" pageNumber="6" value="1790.0">1790</number>
</taxonomicName>
, by subsequent designation of Hoff (
<number id="3DBF24F0FFEEF9666FA4FAD91CB6A515" box="[1194,1255,1365,1389]" pageId="5" pageNumber="6" value="1949.0">1949</number>
