<taxonomicName id="A1DCF0690C3EFF9842705B9AE0B0F466" authorityName="Rudd" authorityYear="1968" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="subFamily" subFamily="Faboideae">Faboideae</taxonomicName>
