<mcCollectionCode id="5E3F46BB170EFF8B0712FF74FDEDFF4E" box="[527,546,152,175]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="5" pageNumber="549">R</mcCollectionCode>
and 
<mcCollectionCode id="5E3F46BB170EFF8B0745FF74FDA5FF4E" box="[600,618,152,175]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="5" pageNumber="549">R</mcCollectionCode>
<subScript id="17962171170EFF8B0776FF4AFDBCFF55" attach="left" box="[619,627,166,180]" fontSize="6" pageId="5" pageNumber="549">1</subScript>
