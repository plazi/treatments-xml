<mcCollectionCode id="5E3F46BB170EFF8B006DFF74FA53FF4E" box="[1392,1436,152,175]" country="Canada" name="McGill University, Redpath Museum" pageId="5" pageNumber="549" type="Museum">RM</mcCollectionCode>
