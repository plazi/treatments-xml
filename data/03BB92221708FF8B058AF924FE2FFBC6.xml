<quantity id="4CEA8ED1170EFF8B0480FF7BFDCBFF4E" box="[413,516,151,176]" metricMagnitude="-4" metricUnit="m" metricValue="9.6" pageId="5" pageNumber="549" unit="mm" value="0.96">0.96 mm</quantity>
