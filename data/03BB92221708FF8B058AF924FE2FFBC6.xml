<mcCollectionCode id="5E3F46BB170EFF8B065BFF74FC96FF4E" box="[838,857,152,175]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="5" pageNumber="549">R</mcCollectionCode>
