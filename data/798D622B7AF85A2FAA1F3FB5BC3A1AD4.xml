</taxonomicName>
, cb = 
<taxonomicName id="2ED313B80D904A179866FE64355392C4" authorityName="Kiew" authorityYear="2011" baseAuthorityName="Ridl." class="Magnoliopsida" family="Gesneriaceae" genus="Codonoboea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Codonoboea breviflora" order="Lamiales" pageId="0" pageNumber="51" phylum="Tracheophyta" rank="species" species="breviflora">
