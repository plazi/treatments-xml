ck = 
<taxonomicName id="5220ED8960A2AC3E2E1A03707FA86FBF" authorityName="Syahida-Emiza, Y. Y. Sam &amp; Siti-Munirah" authorityYear="2020" class="Magnoliopsida" family="Gesneriaceae" genus="Codonoboea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Codonoboea kenaboiensis" order="Lamiales" pageId="0" pageNumber="51" phylum="Tracheophyta" rank="species" species="kenaboiensis">
