<taxonomicName id="59964D40FF8FFFC1C6BD90D75523F8C3" authorityName="Gray" authorityYear="2010" box="[922,1086,1790,1813]" class="Arachnida" family="Atracidae" genus="Hadronyche" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="100" pageNumber="385" phylum="Arthropoda" rank="species" species="lamingtonensis">
