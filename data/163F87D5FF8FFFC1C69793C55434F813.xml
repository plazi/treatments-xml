<quantity id="596E9B26FF8FFFC1C41D90CF5069F92D" box="[314,372,1766,1787]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="100" pageNumber="385" unit="mm" value="1.0">1 mm</quantity>
