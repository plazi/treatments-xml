<typeStatus id="54B7880C2A7179602BDED993F5742AE1" box="[766,864,727,752]" pageId="46" pageNumber="47">holotype</typeStatus>
collected in 
<collectingRegion id="49C8F84C2A7179602AD0D993F28B2AE1" box="[1008,1183,727,752]" country="Panama" name="Cocle" pageId="46" pageNumber="47">Coclé Province</collectingRegion>
, 
