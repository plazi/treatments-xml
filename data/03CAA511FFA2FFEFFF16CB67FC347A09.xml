<taxonomicName id="4C636F84FFA2FFEFFF40C953FEE97A2E" authorityName="Samal" authorityYear="1939" box="[193,369,1847,1874]" class="Insecta" family="Chloroperlidae" genus="Alloperla" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="578" phylum="Arthropoda" rank="species" species="teleckojensis">
