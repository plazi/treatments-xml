(
<taxonomicName id="4C636F84FFA2FFEFFF6FC89BFDE27A75" authority="Zapekina-Dulkeit, 1975" authorityName="Zapekina-Dulkeit" authorityYear="1975" box="[238,634,1775,1802]" class="Insecta" family="Chloroperlidae" genus="Alloperla" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="578" phylum="Arthropoda" rank="species" species="acietata">
