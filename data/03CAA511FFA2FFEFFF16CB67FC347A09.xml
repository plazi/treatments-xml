<taxonomicName id="4C636F84FFA2FFEFFEF3C97FFD4B7A52" authority="(Klapalek, 1923))" authorityName="" baseAuthorityName="Klapalek" baseAuthorityYear="1923" box="[370,723,1811,1838]" class="Insecta" family="Chloroperlidae" genus="Alloperla" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="578" phylum="Arthropoda" rank="species" species="rostellata">
