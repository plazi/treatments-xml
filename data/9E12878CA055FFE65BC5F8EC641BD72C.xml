<taxonomicName id="D1BB4D19A057FFE15A49F9486183D4A5" authority="Wei &amp; Wang &amp; Hayashi &amp; He &amp; Pham, 2020" authorityName="Wei &amp; Wang &amp; Hayashi &amp; He &amp; Pham" authorityYear="2020" box="[283,481,1698,1722]" class="Insecta" family="Cicadidae" genus="Versicolora" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="280" phylum="Arthropoda" rank="species" species="ziyongi" status="sp. nov.">
<emphasis id="24CFEA88A057FFE15A49F9486183D4A5" box="[283,481,1698,1722]" italics="true" pageId="3" pageNumber="280">Versicolora ziyongi</emphasis>
</taxonomicName>
<emphasis id="24CFEA88A057FFE15ABAF9486258D4A5" bold="true" box="[488,570,1698,1722]" pageId="3" pageNumber="280">
<taxonomicNameLabel id="3FFC57F3A057FFE15ABAF9486258D4A5" box="[488,570,1698,1722]" pageId="3" pageNumber="280" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, exuviae. 
<emphasis id="24CFEA88A057FFE159F6F94862D4D4A4" bold="true" box="[676,694,1698,1723]" pageId="3" pageNumber="280">A</emphasis>
. habitus, dorsal view; 
<emphasis id="24CFEA88A057FFE158F2F94863D3D4A5" bold="true" box="[928,945,1698,1722]" pageId="3" pageNumber="280">B</emphasis>
. habitus, lateral view; 
<emphasis id="24CFEA88A057FFE15FCEF94864CCD4A4" bold="true" box="[1180,1198,1698,1723]" pageId="3" pageNumber="280">C</emphasis>
