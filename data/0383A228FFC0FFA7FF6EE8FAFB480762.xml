</taxonomicName>
from 
<taxonomicName id="4C2A68BDFFC3FFA1FDCAEF2AFD6C0408" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[563,739,1863,1887]" class="Gastropoda" family="Ranellidae" genus="Charonia" kingdom="Animalia" order="Littorinimorpha" pageId="26" pageNumber="71" phylum="Mollusca" rank="species" species="lampas">
