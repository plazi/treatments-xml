<table id="F97FC4203E66003677D2FB5194CB985F" box="[159,1420,1164,1872]" gridcols="3" gridrows="10" pageId="16" pageNumber="393">
<tr id="354F34C23E66003677D2FB5194CB9BAD" box="[159,1420,1164,1186]" gridrow="0" pageId="16" pageNumber="393">
<th id="769E5DBE3E66003677D2FB5190279BAD" box="[159,352,1164,1186]" gridcol="0" gridrow="0" pageId="16" pageNumber="393">Female</th>
<th id="769E5DBE3E6600367639FB5192159BAD" box="[372,850,1164,1186]" gridcol="1" gridrow="0" pageId="16" pageNumber="393">
<taxonomicName id="4C7F4D033E66FFD97639FB50930E9BAD" authority="Kim" authorityName="Kim" box="[372,585,1164,1186]" class="Insecta" family="Eumenidae" genus="Polistes" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="393" phylum="Arthropoda" rank="species" species="tenuispunctia">
