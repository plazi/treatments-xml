<taxonomicName id="4C326DC91A43FFDCD32E0C76FDBCFAB0" box="[375,559,1311,1334]" class="Insecta" family="Acrididae" kingdom="Animalia" order="Orthoptera" pageId="29" pageNumber="340" phylum="Arthropoda" rank="subFamily" subFamily="Catantopinae">CATANTOPINAE</taxonomicName>
