<quantity id="C4119B50C57DFFF5FDCAF329FD308B34" box="[592,679,1886,1909]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="262" unit="mm" value="1.6">1.6 mm</quantity>
, crown length 
<quantity id="C4119B50C57DFFF5FF45F309FEA48BD4" box="[223,307,1918,1941]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="5" pageNumber="262" unit="mm" value="0.6">0.6 mm</quantity>
. Pronotum width 
<quantity id="C4119B50C57DFFF5FE76F309FDA98BD4" box="[492,574,1918,1941]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="262" unit="mm" value="1.6">1.6 mm</quantity>
, pronotum length 
