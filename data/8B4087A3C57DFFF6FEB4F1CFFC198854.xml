<quantity id="C4119B50C57DFFF5FEBEF329FE3C8B34" box="[292,427,1886,1909]" metricMagnitude="-3" metricUnit="m" metricValue="4.55" metricValueMax="4.8" metricValueMin="4.3" pageId="5" pageNumber="262" unit="mm" value="4.55" valueMax="4.8" valueMin="4.3">4.3–4.8 mm</quantity>
