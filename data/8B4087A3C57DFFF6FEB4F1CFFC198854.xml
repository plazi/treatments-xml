<quantity id="C4119B50C57DFFF5FD33F349FF538B34" metricMagnitude="-3" metricUnit="m" metricValue="4.4" metricValueMax="4.6" metricValueMin="4.2" pageId="5" pageNumber="262" unit="mm" value="4.4" valueMax="4.6" valueMin="4.2">4.2–4.6 mm</quantity>
