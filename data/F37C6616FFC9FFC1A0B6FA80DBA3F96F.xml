<taxonomicName id="BCD5AC83FFC9FFC1A0B6FA80D99FFA48" authority="Collins &amp; Larsen, 2013" authorityName="Collins &amp; Larsen" authorityYear="2013" box="[151,723,1394,1421]" class="Insecta" family="Hesperiidae" genus="Artitropa" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="325" phylum="Arthropoda" rank="species" species="principetome">
