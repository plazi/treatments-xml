28 
<taxonomicName id="C1FDE465CEAA96D62191AA354A3944DA" class="Insecta" family="Staphylinidae" genus="Lasinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lasinus saoriae" order="Coleoptera" pageId="14" pageNumber="35" phylum="Arthropoda" rank="species" species="saoriae">Lasinus saoriae</taxonomicName>
