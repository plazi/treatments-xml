(
<taxonomicName id="24F550E94742FFC4C3FAE27CFA4CFAAB" box="[1274,1433,1376,1400]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="420" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
