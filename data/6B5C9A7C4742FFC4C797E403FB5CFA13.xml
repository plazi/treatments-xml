<taxonomicName id="24F550E94742FFC4C423E300FCB9FBE6" box="[803,876,1052,1077]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="420" phylum="Arthropoda" rank="genus">
