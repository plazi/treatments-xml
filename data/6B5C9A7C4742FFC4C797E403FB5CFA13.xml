<taxonomicName id="24F550E94742FFC4C3AAE298FE18FA13" authority="(Marsham) (Yu et al. 2012)" authorityName="(Marsham) (Yu et al." authorityYear="2012" class="Insecta" family="Curculionidae" genus="Scolytus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="420" phylum="Arthropoda" rank="species" species="multistriatus">
