<taxonomicName id="24F550E94740FFC6C589E2BBFD37FA13" box="[649,738,1447,1472]" class="Magnoliopsida" family="Ulmaceae" genus="Zelkova" kingdom="Plantae" order="Rosales" pageId="17" pageNumber="418" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F7784740FFC6C589E2BBFD37FA13" box="[649,738,1447,1472]" italics="true" pageId="17" pageNumber="418">Zelkova</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E94740FFC6C5F2E2BBFCB3FA13" box="[754,870,1447,1472]" class="Magnoliopsida" family="Ulmaceae" kingdom="Plantae" order="Rosales" pageId="17" pageNumber="418" phylum="Tracheophyta" rank="family">Ulmaceae</taxonomicName>
