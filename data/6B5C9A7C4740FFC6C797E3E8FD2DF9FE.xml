<taxonomicName id="24F550E94740FFC6C344E2B5FB40FA13" box="[1092,1173,1449,1472]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="17" pageNumber="418" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F7784740FFC6C344E2B5FB40FA13" box="[1092,1173,1449,1472]" italics="true" pageId="17" pageNumber="418">Prunus</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E94740FFC6C3A5E2B4FAC7FA13" box="[1189,1298,1448,1472]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="17" pageNumber="418" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
) (
<bibRefCitation id="8764569B4740FFC6C22AE2BBFF06FA37" author="Michalski" pageId="17" pageNumber="418" refString="Michalski, J. (1973) Revision of the palearctic species of the genus Scolytus Geoffroy (Coleoptera, Scolytidae). Panstwowe Wydawnictwo Naukowe, Warszawa, 214 pp., 49 pls." type="book" year="1973">
Michalski 
<number id="553D394D4740FFC6C797E2D0FF06FA37" box="[151,211,1484,1508]" pageId="17" pageNumber="418" value="1973.0">1973</number>
</bibRefCitation>
). Recorded from 
<taxonomicName id="24F550E94740FFC6C696E2D0FE0AFA36" box="[406,479,1484,1509]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="418" phylum="Arthropoda" rank="genus">
<emphasis id="D181F7784740FFC6C696E2D0FE0AFA36" box="[406,479,1484,1509]" italics="true" pageId="17" pageNumber="418">Ulmus</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="24F550E94740FFC6C514E2D0FD5DFA36" box="[532,648,1484,1509]" class="Magnoliopsida" family="Ulmaceae" kingdom="Plantae" order="Rosales" pageId="17" pageNumber="418" phylum="Tracheophyta" rank="family">Ulmaceae</taxonomicName>
) (
<bibRefCitation id="8764569B4740FFC6C59EE2D0FC5BFA37" author="Modarres" box="[670,910,1484,1509]" pageId="17" pageNumber="418" refString="Modarres Awal, M. (1997) Platypodidae, Scolytidae. In: Modarres Awal, M. (Ed.), List of agricultural pests and their natural enemies in Iran. Ferdowsi University Press, Mashhad, pp. 185, 192 - 194." type="book chapter" year="1997">
Modarres Awal 
<number id="553D394D4740FFC6C453E2D0FC5BFA37" box="[851,910,1484,1508]" pageId="17" pageNumber="418" value="1997.0">1997</number>
</bibRefCitation>
), and 
<taxonomicName id="24F550E94740FFC6C4D2E2D0FA91FA37" authority="Amini et al. 2013" authorityName="Amini et al." authorityYear="2013" box="[978,1348,1484,1509]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="418" phylum="Arthropoda" rank="species" species="minor">
<emphasis id="D181F7784740FFC6C4D2E2D0FBB0FA37" box="[978,1125,1484,1509]" italics="true" pageId="17" pageNumber="418">Ulmus minor</emphasis>
(
<bibRefCitation id="8764569B4740FFC6C373E2D0FAE8FA37" author="Amini" box="[1139,1341,1484,1509]" pageId="17" pageNumber="418" refString="Amini, S., Hosseini, R. &amp; Sohani, M. M. (2013) A faunal study of bark beetles (Coleoptera: Curculionidae: Scolytinae) in Guilan province in North of Iran. Entomofauna, 34, 169 - 176." type="journal article" year="2013">
Amini 
<emphasis id="D181F7784740FFC6C3C4E2D1FB21FA36" box="[1220,1268,1484,1509]" italics="true" pageId="17" pageNumber="418">et al</emphasis>
. 
<number id="553D394D4740FFC6C203E2D0FAE8FA37" box="[1283,1341,1484,1508]" pageId="17" pageNumber="418" value="2013.0">2013</number>
</bibRefCitation>
)
</taxonomicName>
in 
<collectingCountry id="9BE26BFA4740FFC6C268E2D1FA4CFA37" box="[1384,1433,1485,1508]" name="Iran" pageId="17" pageNumber="418">Iran</collectingCountry>
. 
<bibRefCitation id="8764569B4740FFC6C797E2F3FE8CF9DB" author="Michalski" box="[151,345,1519,1544]" pageId="17" pageNumber="418" refString="Michalski, J. (1973) Revision of the palearctic species of the genus Scolytus Geoffroy (Coleoptera, Scolytidae). Panstwowe Wydawnictwo Naukowe, Warszawa, 214 pp., 49 pls." type="book" year="1973">
Michalski (
<number id="553D394D4740FFC6C617E2ECFE84F9DB" box="[279,337,1520,1544]" pageId="17" pageNumber="418" value="1973.0">1973</number>
)
</bibRefCitation>
