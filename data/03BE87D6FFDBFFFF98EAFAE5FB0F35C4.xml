<taxonomicName id="4C174D43FFDBFFFF98EAFAE5F8563537" authority="(Cuvier, 1816)" baseAuthorityName="Cuvier" baseAuthorityYear="1816" box="[151,535,1303,1330]" class="Actinopterygii" family="Regalecidae" genus="Regalecus" kingdom="Animalia" order="Lampriformes" pageId="74" pageNumber="75" phylum="Chordata" rank="species" species="russelii">
<emphasis id="B963EAD2FFDBFFFF98EAFAE5FB283537" bold="true" box="[151,361,1303,1330]" italics="true" pageId="74" pageNumber="75">Regalecus russelii</emphasis>
