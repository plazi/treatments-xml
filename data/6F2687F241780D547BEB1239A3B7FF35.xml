<taxonomicName id="208F4D6741780D557F6611C2A56FF87A" authority="(Pickford, 1975)" authorityName="Pickford" authorityYear="1975" baseAuthorityName="Pickford" baseAuthorityYear="1975" box="[1050,1356,1950,1975]" class="Clitellata" family="Microchaetidae" genus="Kazimierzus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="2" pageNumber="189" phylum="Annelida" rank="species" species="crousi">
