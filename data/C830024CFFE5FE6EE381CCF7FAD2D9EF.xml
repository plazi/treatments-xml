<emphasis id="72ED6F48FFE5FE6EE619C83BFAF0DA31" box="[1295,1323,1292,1316]" italics="true" pageId="14" pageNumber="15">ad</emphasis>
<number id="F651A17DFFE5FE6EE63DC82CFAE8DA3D" box="[1323,1331,1307,1320]" pageId="14" pageNumber="15" value="3.0">
<emphasis id="72ED6F48FFE5FE6EE63DC82CFAE8DA3D" box="[1323,1331,1307,1320]" italics="true" pageId="14" pageNumber="15">3</emphasis>
