<taxonomicName id="4C7BF4D4FFB1AA5ACDB1F866FE04F80E" authority="McLaughlin, 2002" authorityName="McLaughlin" authorityYear="2002" class="Malacostraca" family="Paguridae" genus="Catapaguroides" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="39" phylum="Arthropoda" rank="species" species="foresti">
