<quantity id="4CBD17E3FF8CFFA3FBC8FCDFFBE9D532" box="[1036,1146,844,870]" metricMagnitude="-2" metricUnit="m" metricValue="1.54" pageId="2" pageNumber="23" unit="mm" value="15.4">15.4 mm</quantity>
) (
<collectionCode id="ED5422C3FF8CFFA3FB52FCDFFB46D532" box="[1174,1237,844,870]" httpUri="http://grbio.org/cool/16z4-1tbx" name="Zoological Reference Collection, National University of Singapore" pageId="2" pageNumber="23">ZRC</collectionCode>
), 
<collectingCountry id="F352FA96FF8CFFA3FB28FCDFFA8ED532" box="[1260,1309,844,870]" name="India" pageId="2" pageNumber="23">Gua</collectingCountry>
Giri Putri Cave, 
<quantity id="4CBD17E3FF8CFFA3FF25FCE1FE8ED5D8" box="[225,285,882,908]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="2" pageNumber="23" unit="m" value="50.0">50 m</quantity>
asl, Dusun Karangsari, Desa Suana, Nusa Penida, southeast of Bali, 
<collectingCountry id="F352FA96FF8CFFA3FBFBFCE1FB27D5D8" box="[1087,1204,882,908]" name="Indonesia" pageId="2" pageNumber="23">Indonesia</collectingCountry>
, coll. A. J. Whitten, 
<date id="FFFB9CC6FF8CFFA3FF53FC0AFEDED5E7" box="[151,333,921,947]" pageId="2" pageNumber="23" value="1994-03-21">
