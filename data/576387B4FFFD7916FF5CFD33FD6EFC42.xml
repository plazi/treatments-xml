<taxonomicName id="18CA4D21FFFD7916FF5CFD33FE25FD17" authority="Hood, 1925" authorityName="Hood" authorityYear="1925" box="[151,392,720,747]" class="Insecta" family="Thripidae" genus="Frankliniella" kingdom="Animalia" order="Thysanoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="parvula">
