<taxonomicName id="2DDC4D7BFFE52A1CDD58FB2C88EDB4FE" box="[809,986,1233,1258]" class="Insecta" family="Formicidae" genus="Heteroponera" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="176" phylum="Arthropoda" rank="species" species="pendergrasti">
