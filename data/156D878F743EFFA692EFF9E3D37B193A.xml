<typeStatus id="427F883B743EFFA59348F918D3E41BA9" box="[304,411,1717,1743]" pageId="4" pageNumber="349">Holotype</typeStatus>
, 
<specimenCount id="8BC2FD10743EFFA593DEF91BD3C41BB6" box="[422,443,1718,1744]" pageId="4" pageNumber="349" type="female">♀</specimenCount>
