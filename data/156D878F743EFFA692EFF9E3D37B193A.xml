<subSubSection id="D5DE6512743DFFA692BFFC1CD7111EAD" box="[199,1390,945,972]" pageId="7" pageNumber="352" type="etymology">
<paragraph id="9D7B3699743DFFA692BFFC1CD7111EAD" blockId="7.[151,1437,153,1116]" box="[199,1390,945,972]" pageId="7" pageNumber="352">
<emphasis id="AFB0EA8B743DFFA692BFFC1CD3311EAD" bold="true" box="[199,334,945,971]" pageId="7" pageNumber="352">Etymology.</emphasis>
