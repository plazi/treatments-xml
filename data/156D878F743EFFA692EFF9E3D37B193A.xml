<quantity id="5A3C9B7C743EFFA5900BF918D0B41BB6" box="[627,715,1717,1744]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" pageId="4" pageNumber="349" unit="mm" value="6.1">6.1 mm</quantity>
