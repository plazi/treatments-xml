<number id="F75124D1FFA9FFC5FE5562F3FE63E009" box="[489,510,1785,1809]" pageId="3" pageNumber="32" value="3.0">3</number>
<collectionCode id="2788AE33FFA9FFC5FDA762F2FDEBE008" box="[539,630,1784,1808]" httpUri="http://grbio.org/cool/hp11-qr98" name="Nankai University" pageId="3" pageNumber="32">NKUM</collectionCode>
