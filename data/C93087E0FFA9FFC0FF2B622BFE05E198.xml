<collectionCode id="2788AE33FFA9FFC5FC6762DEFBAAE1F4" box="[987,1079,1748,1772]" httpUri="http://grbio.org/cool/hp11-qr98" name="Nankai University" pageId="3" pageNumber="32">NKUM</collectionCode>
