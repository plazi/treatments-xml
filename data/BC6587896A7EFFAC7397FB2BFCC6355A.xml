<taxonomicName id="F3CC4D1C6A7EFFAA77E6F90FFAE231F9" box="[1254,1400,1698,1723]" class="Arachnida" family="Ctenidae" genus="Chococtenus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="470" phylum="Arthropoda" rank="species" species="otongachi">
<emphasis id="06B8EA8D6A7EFFAA77E6F90FFAE231F9" box="[1254,1400,1698,1723]" italics="true" pageId="19" pageNumber="470">C. otongachi</emphasis>
</taxonomicName>
