<paragraph blockId="2.[151,1436,151,1423]" pageId="2" pageNumber="239">
<emphasis bold="true" box="[151,299,186,209]" pageId="2" pageNumber="239">
<typeStatus box="[151,207,186,209]" pageId="2" pageNumber="239">Type</typeStatus>
locality
</emphasis>
