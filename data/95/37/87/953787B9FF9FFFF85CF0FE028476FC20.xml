<collectingCounty box="[878,1011,823,848]" pageId="16" pageNumber="117">Washington</collectingCounty>
to 
<collectingMunicipality box="[1046,1242,823,848]" pageId="16" pageNumber="117">British Columbia</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:953787B9FF9FFFF85CF0FE028476FC20:18416074FF9FFFF85881FCA180E7FCE8" box="[1254,1436,823,848]" country="Japan" county="Washington" municipality="British Columbia" name="Aleutian Islands" pageId="16" pageNumber="117" stateProvince="Hokkaido">Aleutian Islands</location>
(Alaska) to 
<location LSID="urn:lsid:plazi:treatment:953787B9FF9FFFF85CF0FE028476FC20:18416074FF9FFFF85D47FCCB849FFCCC" box="[288,484,860,884]" country="Japan" county="Washington" municipality="British Columbia" name="Patton Seamount" pageId="16" pageNumber="117" stateProvince="Hokkaido">Patton Seamount</location>
, 
<location LSID="urn:lsid:plazi:treatment:953787B9FF9FFFF85CF0FE028476FC20:18416074FF9FFFF85D95FCCA87E5FCCD" box="[498,670,860,885]" country="Japan" county="Washington" municipality="British Columbia" name="Gulf of Alaska" pageId="16" pageNumber="117" stateProvince="Hokkaido">Gulf of Alaska</location>
and 
<collectingRegion box="[729,842,860,885]" country="Japan" name="Hokkaido" pageId="16" pageNumber="117">Hokkaido</collectingRegion>
, 
<collectingCountry box="[856,920,861,884]" name="Japan" pageId="16" pageNumber="117">Japan</collectingCountry>
<elevation box="[930,1075,860,884]" metricMagnitude="3" metricUnit="m" metricValue="1.2415" metricValueMax="2.373" metricValueMin="0.11" pageId="16" pageNumber="117" unit="m" value="1241.5" valueMax="2373.0" valueMin="110.0">
<quantity box="[930,1075,860,884]" metricMagnitude="3" metricUnit="m" metricValue="1.2415" metricValueMax="2.373" metricValueMin="0.11" pageId="16" pageNumber="117" unit="m" value="1241.5" valueMax="2373.0" valueMin="110.0">110–2373 m</quantity>
