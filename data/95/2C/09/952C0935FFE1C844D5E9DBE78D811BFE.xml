, HW 
<number box="[1317,1431,1235,1259]" pageId="25" pageNumber="26" value="0.905" valueMax="0.94" valueMin="0.87">0.87–0.94</number>
, AnL 
<number box="[212,326,1270,1295]" pageId="25" pageNumber="26" value="1.72" valueMax="1.81" valueMin="1.63">1.63–1.81</number>
, PL 
