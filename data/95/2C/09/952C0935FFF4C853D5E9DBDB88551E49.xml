<emphasis box="[766,902,1338,1359]" italics="true" pageId="13" pageNumber="14">L. gutianense</emphasis>
</taxonomicName>
; 
<emphasis bold="true" box="[916,934,1337,1359]" pageId="13" pageNumber="14">C</emphasis>
— 
<taxonomicName box="[959,1068,1338,1359]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="nannani">
