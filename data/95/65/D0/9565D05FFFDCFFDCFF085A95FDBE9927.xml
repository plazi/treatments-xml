<taxonomicName authorityName="Benson" authorityYear="1860" box="[489,601,563,587]" class="Gastropoda" family="Gastrocoptidae" genus="Clostophis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="367" phylum="Mollusca" rank="genus">
