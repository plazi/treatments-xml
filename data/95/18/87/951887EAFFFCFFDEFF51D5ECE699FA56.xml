<paragraph blockId="63.[151,784,1879,1904]" box="[151,784,1879,1904]" pageId="63" pageNumber="299">
<taxonomicName authority="Sars, 1911 f" authorityName="sensu Sars" authorityYear="1911" box="[151,530,1879,1904]" class="Hexanauplia" family="Miraciidae" genus="Microthalestris" kingdom="Animalia" order="Harpacticoida" pageId="63" pageNumber="299" phylum="Arthropoda" rank="species" species="littoralis">
