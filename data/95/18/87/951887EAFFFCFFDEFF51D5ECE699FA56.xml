<subSubSection pageId="64" pageNumber="300" type="diagnosis">
<paragraph blockId="64.[151,1437,150,681]" pageId="64" pageNumber="300">
<emphasis bold="true" box="[199,452,474,500]" pageId="64" pageNumber="300">Differential diagnosis</emphasis>
