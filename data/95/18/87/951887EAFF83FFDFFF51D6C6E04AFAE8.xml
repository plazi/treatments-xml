<taxonomicName authority="sensu Vervoort (1964)" authorityName="sensu Vervoort" authorityYear="1964" box="[540,879,1633,1657]" class="Hexanauplia" family="Parastenheliidae" genus="Penicillicaris" kingdom="Animalia" order="Harpacticoida" pageId="64" pageNumber="300" phylum="Arthropoda" rank="species" species="penicillata">
