<quantity box="[426,549,1251,1275]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.1" metricValueMin="1.0" pageId="5" pageNumber="474" unit="mm" value="10.5" valueMax="11.0" valueMin="10.0">10–11 mm</quantity>
