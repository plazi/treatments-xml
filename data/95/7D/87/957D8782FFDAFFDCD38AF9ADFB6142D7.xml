<subSubSection box="[151,1115,1684,1706]" pageId="1" pageNumber="404" type="materials_examined">
<paragraph blockId="1.[151,1115,1684,1706]" box="[151,1115,1684,1706]" pageId="1" pageNumber="404">
<emphasis bold="true" box="[151,290,1684,1706]" pageId="1" pageNumber="404">
