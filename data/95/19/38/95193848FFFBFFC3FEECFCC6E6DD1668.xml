<taxonomicName authority="(Calman, 1912)" baseAuthorityName="Calman" baseAuthorityYear="1912" class="Malacostraca" family="Bodotriidae" genus="Sympodomma" kingdom="Animalia" order="Cumacea" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="diomedeae">
