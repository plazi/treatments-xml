; 
<taxonomicName authorityName="Fraser" authorityYear="1960" box="[1121,1219,1051,1074]" class="Insecta" family="Gomphidae" genus="Phyllogomphus" kingdom="Animalia" order="Odonata" pageId="39" pageNumber="270" phylum="Arthropoda" rank="species" species="moundi">
