<emphasis bold="true" box="[843,864,1804,1821]" italics="true" pageId="37" pageNumber="268">47</emphasis>
and 
<emphasis bold="true" box="[901,922,1804,1821]" italics="true" pageId="37" pageNumber="268">54</emphasis>
