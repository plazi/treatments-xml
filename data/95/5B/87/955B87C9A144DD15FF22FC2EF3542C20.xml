</taxonomicName>
from Western Cape Province (UCTES AFR 
<number box="[822,867,764,788]" pageId="50" pageNumber="51" value="950.0">950</number>
, 
<date box="[878,992,764,788]" pageId="50" pageNumber="51" value="1949-03-20">
<number box="[878,930,764,788]" pageId="50" pageNumber="51" value="20.3">20.3</number>
.1949
</date>
, 
<geoCoordinate box="[1004,1098,764,788]" direction="south" orientation="latitude" pageId="50" pageNumber="51" precision="925" value="-34.733334">
34° 
<number box="[1045,1078,764,788]" pageId="50" pageNumber="51" value="44.0">44</number>
’S
