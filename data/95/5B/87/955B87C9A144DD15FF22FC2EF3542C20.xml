, 
<geoCoordinate box="[1110,1206,764,788]" direction="east" orientation="longitude" pageId="50" pageNumber="51" precision="925" value="21.3">
<number box="[1110,1140,764,788]" pageId="50" pageNumber="51" value="21.0">21</number>
° 
