<number id="3DBD24B9FFD8FFB2FC1BFC59066AC3FF" box="[1004,1080,920,947]" pageId="5" pageNumber="41" value="30.15">30.15</number>
; first stigma length 
<number id="3DBD24B9FFD8FFB2FAD6FC59070DC3FE" box="[1313,1375,920,946]" pageId="5" pageNumber="41" value="30.3">30.3</number>
