remote from anterior border, setae 
<emphasis id="72ED6F48FFECFE67E7E6CE09FB27DC40" box="[1264,1276,830,853]" italics="true" pageId="7" pageNumber="8">c</emphasis>
<number id="F651A17DFFECFE67E7EACE7BFADFDC4C" box="[1276,1284,844,857]" pageId="7" pageNumber="8" value="2.0">
<emphasis id="72ED6F48FFECFE67E7EACE7BFADFDC4C" box="[1276,1284,844,857]" italics="true" pageId="7" pageNumber="8">2</emphasis>
