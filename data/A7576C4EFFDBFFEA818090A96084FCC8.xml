<number id="9936CF7FFFDCFFEB83D990A660F1FE29" box="[718,747,392,418]" pageId="4" pageNumber="55" value="31.0">31</number>
<number id="9936CF7FFFDCFFEB83E090A6610DFE29" box="[759,791,392,418]" pageId="4" pageNumber="55" value="16.0">16</number>
’ 
