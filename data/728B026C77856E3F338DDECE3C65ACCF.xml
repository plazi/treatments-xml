<taxonomicName id="1B587F9F5D029F29B2AB0A19C14CC4D0" class="Insecta" family="Coccinellidae" genus="Exochomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exochomus" order="Coleoptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="genus">Exochomus</taxonomicName>
