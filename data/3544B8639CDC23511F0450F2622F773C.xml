<taxonomicName id="518C52397215CE2B0C6C8D5E89EF7ABA" authority="Bhatti" class="Insecta" family="Thripidae" genus="Tenothrips" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tenothrips" order="Thysanoptera" pageId="11" pageNumber="38" phylum="Arthropoda" rank="genus">Tenothrips Bhatti</taxonomicName>
