<taxonomicName id="4C4A8AFFFFE84B2CFC84FF0BFB8AFF38" box="[892,1030,152,177]" class="Insecta" family="Platygastridae" genus="Axea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="yasigi">
