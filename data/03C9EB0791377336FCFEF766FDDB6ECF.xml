<taxonomicName id="4C60219291377338FD3BF066FC596934" authority=", A. R. Gittens" authorityName="A. R. Gittens" box="[685,958,1576,1597]" class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" kingdom="Plantae" order="Ranunculales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
