</taxonomicName>
, 
<taxonomicName id="4C60219291377338FC9EF3F0FC6B6ADA" box="[776,908,1470,1491]" class="Magnoliopsida" family="Asteraceae" genus="Balsamorhiza" kingdom="Plantae" order="Asterales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
<emphasis id="B914860391377338FC9EF3F0FC6B6ADA" box="[776,908,1470,1491]" italics="true" pageId="6" pageNumber="672">Balsamorhiza</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C60219291377338FC0EF3F0FBC66ADA" box="[920,1057,1470,1491]" class="Magnoliopsida" family="Boraginaceae" genus="Lithospermum" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
