</taxonomicName>
, 
<taxonomicName id="4C60219291377338FBBBF3F0FB6A6ADA" box="[1069,1165,1470,1491]" class="Magnoliopsida" family="Apiaceae" genus="Lomatium" kingdom="Plantae" order="Apiales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
<emphasis id="B914860391377338FBBBF3F0FB6A6ADA" box="[1069,1165,1470,1491]" italics="true" pageId="6" pageNumber="672">Lomatium</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C60219291377338FD3BF396FCE66AE4" box="[685,769,1496,1517]" class="Magnoliopsida" family="Hydrophyllaceae" genus="Phacelia" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
<emphasis id="B914860391377338FD3BF396FCE66AE4" box="[685,769,1496,1517]" italics="true" pageId="6" pageNumber="672">Phacelia</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C60219291377338FC9BF396FCDB6AE4" box="[781,828,1496,1517]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
<emphasis id="B914860391377338FC9BF396FCDB6AE4" box="[781,828,1496,1517]" italics="true" pageId="6" pageNumber="672">Rosa</emphasis>
</taxonomicName>
, and 
<taxonomicName id="4C60219291377338FCF9F396FD0F6901" authority="(Linsley and Chemsak 1972)" baseAuthorityName="Linsley and Chemsak" baseAuthorityYear="1972" class="Magnoliopsida" family="Asteraceae" genus="Wyethia" kingdom="Plantae" order="Asterales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
