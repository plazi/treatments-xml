<taxonomicName id="4C60219291377338FD3BF3F0FD1B6ADA" box="[685,764,1470,1491]" class="Magnoliopsida" family="Asteraceae" genus="Achillea" kingdom="Plantae" order="Asterales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
