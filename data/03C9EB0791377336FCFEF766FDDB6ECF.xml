<paragraph id="8BDF5A1191397336FF01F68CFDDD6FDE" blockId="8.[151,638,194,454]" box="[151,570,194,215]" pageId="8" pageNumber="674">
<taxonomicName id="4C60219291397336FF01F68CFDDD6FDE" authority="(Kirby, 1837)" baseAuthorityName="Kirby" baseAuthorityYear="1837" box="[151,570,194,215]" class="Insecta" family="Cerambycidae" genus="Grammoptera" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="674" phylum="Arthropoda" rank="species" species="subargentata">
