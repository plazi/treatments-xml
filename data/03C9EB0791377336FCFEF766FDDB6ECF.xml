<taxonomicName id="4C60219291377338FCBFF013FC92697B" box="[809,885,1629,1650]" class="Magnoliopsida" family="Asteraceae" genus="Cirsium" kingdom="Plantae" order="Asterales" pageId="6" pageNumber="672" phylum="Tracheophyta" rank="genus">
