<taxonomicName id="6AD247ADFFF7FFCAFEB8F926EB15F8E7" authorityName="Rolston" authorityYear="1984" box="[273,526,1770,1796]" class="Insecta" family="Pentatomidae" genus="Euschistus" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="carbonerus">
