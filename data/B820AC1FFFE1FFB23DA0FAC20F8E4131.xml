<taxonomicName id="F789668AFFE0FFB53F16F8C40D2E4771" authority="Laboissiere." authorityName="Laboissiere." box="[545,845,1863,1885]" class="Insecta" family="Chrysomelidae" genus="Oides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="foveicollis">
