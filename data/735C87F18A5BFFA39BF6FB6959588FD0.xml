<quantity id="3C0D9B028A5BFFA39FBBF9B15F528F2C" box="[1242,1374,1620,1645]" metricMagnitude="2" metricUnit="m" metricValue="4.925" metricValueMax="5.55" metricValueMin="4.3" pageId="13" pageNumber="14" unit="m" value="492.5" valueMax="555.0" valueMin="430.0">430–555 m</quantity>
