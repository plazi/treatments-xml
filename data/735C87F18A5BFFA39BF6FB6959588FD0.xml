<quantity id="3C0D9B028A5BFFA399D6F99258F28FCF" box="[695,766,1655,1680]" metricMagnitude="2" metricUnit="m" metricValue="6.89" pageId="13" pageNumber="14" unit="m" value="689.0">689 m</quantity>
