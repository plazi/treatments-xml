<paragraph id="C3BE977B53B497A0CA70CAEF18D2A633">
[[ 
<taxonomicName id="EF4FBBAB598F28D2F64DC8CDC794C5BD" class="Actinopterygii" family="Cichlidae" genus="Pelvicachromis" kingdom="Animalia" order="Perciformes" pageNumber="1" phylum="Chordata" rank="genus">Pelvicachromis</taxonomicName>
