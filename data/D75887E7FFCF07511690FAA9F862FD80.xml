<quantity id="98099B14FFCE075115C4FEC4FBB2FEE9" box="[386,496,321,345]" metricMagnitude="2" metricUnit="m" metricValue="6.35" metricValueMax="7.7" metricValueMin="5.0" pageId="19" pageNumber="352" unit="m" value="635.0" valueMax="770.0" valueMin="500.0">500-770 m</quantity>
