<typeStatus id="54FA8836FFE770412B258214FDA70548" box="[551,605,864,888]" pageId="4" pageNumber="583">Type</typeStatus>
: 
<taxonomicName id="4C414D17FFE770412B6B822BFBE50547" authority="Morley, 1913" authorityName="Morley" authorityYear="1913" box="[617,1055,863,888]" class="Insecta" family="Ichneumonidae" genus="Xanthocampoplex" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="583" phylum="Arthropoda" rank="species" species="orientalis">
