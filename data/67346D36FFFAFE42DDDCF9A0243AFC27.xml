<typeStatus id="30266282FFFAFE41DD8EF9322040FEB1" box="[197,256,1714,1740]" pageId="1" pageNumber="59">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="289DA7A3FFFAFE41DC25F93322B8FEB0" authority="Cassis, Symonds and Tatarnic" authorityName="Cassis, Symonds and Tatarnic" box="[366,1016,1715,1741]" class="Insecta" family="Miridae" genus="Harveycapsus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="59" phylum="Arthropoda" rank="species" species="dimorpha">
