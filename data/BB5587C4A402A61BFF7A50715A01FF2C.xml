<paragraph id="334336D2A402A61AFF7A5071589BF8C1" blockId="38.[151,416,1821,1847]" box="[151,416,1821,1847]" pageId="38" pageNumber="39">
<heading id="680B81BEA402A61AFF7A5071589BF8C1" bold="true" box="[151,416,1821,1847]" fontSize="11" level="1" pageId="38" pageNumber="39" reason="1">
<emphasis id="0188EAC0A402A61AFF7A5071589BF8C1" bold="true" box="[151,416,1821,1847]" pageId="38" pageNumber="39">
<taxonomicName id="F4FC4D51A402A61AFF7A507159D4F8C1" box="[151,239,1821,1847]" class="Arachnida" family="Crotoniidae" genus="Crotonia" kingdom="Animalia" order="Sarcoptiformes" pageId="38" pageNumber="48" phylum="Arthropoda" rank="species" species="raveni">Raveni</taxonomicName>
