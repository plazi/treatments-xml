(
<collectingRegion id="49F9F879FF54E34D6F9E9C14FDDFFAF3" box="[441,517,1356,1381]" country="China" name="Fujian" pageId="80" pageNumber="81">Fujian</collectingRegion>
, 
<collectingRegion id="49F9F879FF54E34D6C379C14FD8DFAF3" box="[528,599,1356,1381]" country="China" name="Hubei" pageId="80" pageNumber="81">Hubei</collectingRegion>
, 
<collectingRegion id="49F9F879FF54E34D6C459C14FD1DFAF3" box="[610,711,1356,1381]" country="China" name="Guangxi" pageId="80" pageNumber="81">Guangxi</collectingRegion>
