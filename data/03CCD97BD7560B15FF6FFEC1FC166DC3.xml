<taxonomicName id="4C6513EED7560B15FE29FCA3FD606D56" authorityName="Linnaeus" authorityYear="1761" box="[465,649,829,855]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="183" phylum="Arthropoda" rank="species" species="viridula">
