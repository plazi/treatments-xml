<taxonomicName id="4C6513EED7560B15FABFFCFEFE376D9F" authority="Mocsary, 1889" authorityName="Mocsary" authorityYear="1889" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="183" phylum="Arthropoda" rank="species" species="consanguinea">
