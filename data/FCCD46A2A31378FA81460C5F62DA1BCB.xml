klein, 
<normalizedToken id="2438435339894080CCE91CCA7C333755" originalValue="regelmäßig" pageId="null" pageNumber="590">regelmaessig</normalizedToken>
und grob 
<normalizedToken id="17ABC1BCC66606F63C6511D5FD644A42" originalValue="gezähnt" pageId="null" pageNumber="590">gezaehnt</normalizedToken>
