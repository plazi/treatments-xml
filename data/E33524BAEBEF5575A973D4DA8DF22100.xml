<taxonomicName id="86F5A358D875379BC8D934A1151F1B75" authorityName="Robinson &amp; Keeley &amp; Skvarla &amp; Chan" authorityYear="2014" baseAuthorityName="Robinson &amp; Keeley &amp; Skvarla &amp; Chan" baseAuthorityYear="2014" class="Magnoliopsida" family="Asteraceae" genus="Vernonia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="8" pageNumber="57" phylum="Tracheophyta" rank="species" species="brachycalyx" status="comb. nov.">
