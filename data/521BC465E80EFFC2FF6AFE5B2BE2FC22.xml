<taxonomicName id="1DB20EF0E80EFFC2FF6AFE5B28E1FDC5" authority="(Felton, 1765)" baseAuthorityName="Felton" baseAuthorityYear="1765" box="[192,686,506,530]" class="Insecta" family="Tetrigidae" genus="Phyllotettix" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="14" phylum="Arthropoda" rank="species" species="rhombeus">
