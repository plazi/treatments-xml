<taxonomicName id="4C2354F7FF98D422FEB6C7EF432AF809" authority="Popovici &amp; Buhl, 2010" authorityName="Popovici &amp; Buhl" authorityYear="2010" box="[303,800,1930,1956]" class="Insecta" family="Platygastridae" genus="Fidiobia" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="hispanica">
