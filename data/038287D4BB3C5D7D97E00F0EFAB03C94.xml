<taxonomicName id="4C2B4D41BB3C5D7D957B0C42FD433C67" authorityName="Aldrich" authorityYear="1916" box="[524,670,1837,1864]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="culminata">
<emphasis id="B95FEAD0BB3C5D7D957B0C42FD433C67" box="[524,670,1837,1864]" italics="true" pageId="45" pageNumber="46">O. culminata</emphasis>
