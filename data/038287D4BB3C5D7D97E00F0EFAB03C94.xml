<taxonomicName id="4C2B4D41BB3C5D7D97970C66FE8E3C03" box="[224,339,1802,1828]" class="Liliopsida" family="Arecaceae" genus="Syagrus" kingdom="Plantae" order="Arecales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="comosa">
