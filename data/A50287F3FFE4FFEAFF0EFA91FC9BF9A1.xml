<collectingRegion id="EF6FF807FFE4FFEAFE02FA4AFE1CFA43" box="[411,505,1484,1509]" country="Guinea-Bissau" name="Cacheu" pageId="9" pageNumber="10">Cacheu</collectingRegion>
