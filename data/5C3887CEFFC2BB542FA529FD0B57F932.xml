<subSubSection id="9C8B6553FFC2BB542FA529B50AA9FA6B" box="[151,931,1482,1509]" pageId="18" pageNumber="391" type="materials_examined">
<paragraph id="D42E36D8FFC2BB542FA529B50AA9FA6B" blockId="18.[151,1437,1482,1725]" box="[151,931,1482,1509]" pageId="18" pageNumber="391">
