<taxonomicName id="0BA44D4BFFD60C7B5C29547D0E953C57" box="[888,1020,1075,1099]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="12" pageNumber="305" phylum="Tracheophyta" rank="subSpecies" species="jacea" subSpecies="jacea">
