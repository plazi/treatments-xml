<taxonomicName id="4C2CE9FE0224FFFDDDDF2701FCE36175" box="[751,858,1046,1069]" class="Insecta" family="Rhyacophilidae" genus="Rhyacophila" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="141" phylum="Arthropoda" rank="species" species="gyamo">
