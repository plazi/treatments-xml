<taxonomicName id="4C7D4D6D2135FFF0FD33DDEBB918FD27" authority="(H. Milne Edwards)" baseAuthorityName="H. Milne Edwards" box="[725,1075,691,718]" class="Malacostraca" family="Raninidae" genus="Raninoides" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis id="B909EAFC2135FFF0FD33DDEBBE78FD24" box="[725,851,691,717]" italics="true" pageId="17" pageNumber="18">Raninoides</emphasis>
