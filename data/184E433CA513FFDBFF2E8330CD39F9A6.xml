<collectionCode id="F6F66AEFA513FFDBFAFF8468CAD7F99A" LSID="urn:lsid:biocol.org:col:34988" box="[1350,1435,1591,1614]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="24" pageNumber="25">MNHN</collectionCode>
(Warning Work Record, Record # 
<number id="262FE00DA513FFDBFDAE8405CD7FF9A6" box="[535,563,1626,1650]" pageId="24" pageNumber="25" value="38.0">38</number>
<number id="262FE00DA513FFDBFD838405CD24F9A6" box="[570,616,1626,1650]" pageId="24" pageNumber="25" value="329.0">329</number>
