<emphasis id="A2932E38A513FFDBFF1A8469CED8F99B" box="[163,404,1590,1615]" italics="true" pageId="24" pageNumber="25">
Aldrichina 
<taxonomicName id="57E789A9A513FFDBFE9B8467CED8F99B" authority="Robineau-Desvoidy, 1830: 438" authorityName="Robineau-Desvoidy" authorityPageNumber="438" authorityYear="1830" box="[290,404,1592,1615]" class="Insecta" family="Calliphoridae" genus="Calliphora" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="peruviana">peruviana</taxonomicName>
