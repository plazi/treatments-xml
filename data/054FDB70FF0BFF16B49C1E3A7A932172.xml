<taxonomicName id="4AE611E5FF0BFF16B49C1E3A7BB22312" authorityName="Prebus" authorityYear="2021" baseAuthorityName="Emery" baseAuthorityYear="1894" box="[498,640,1063,1093]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="204" pageNumber="205" phylum="Arthropoda" rank="species" species="pastinifer">
