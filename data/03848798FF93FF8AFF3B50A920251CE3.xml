<taxonomicName id="4C2D4D0DFF93FF88FBE855BB20711EB6" authorityName="Simon" authorityYear="1884" box="[1134,1315,1952,1978]" class="Arachnida" family="Zodariidae" genus="Storenomorpha" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
