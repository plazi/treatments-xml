<emphasis id="AFAA6E5A8087FF24EDACCC0D18AC0C41" bold="true" box="[199,497,548,573]" pageId="131" pageNumber="132">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA8087FF24EC93CC0C1BD50C41" box="[504,648,548,573]" country="Papua New Guinea" name="New Ireland" pageId="131" pageNumber="132">New Ireland</collectingRegion>
