<taxonomicName id="4C0E4D19E37BFFCB521FBA418159F878" box="[346,797,1907,1929]" class="Insecta" family="Ceratopogonidae" genus="Forcipomyia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="atenasensis" status="sp. n." subGenus="Metaforcipomyia">
