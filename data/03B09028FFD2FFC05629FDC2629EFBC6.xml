<paragraph id="8BA6213EFFD2FFC35629FDA161C2FDB1" blockId="13.[264,519,563,625]" box="[264,421,599,625]" pageId="13" pageNumber="14">
(
<figureCitation id="13223DBBFFD2FFC35630FDA16115FDB1" box="[273,370,599,625]" captionStart="FIGURES 13 – 22" captionStartId="45.[264,382,1790,1814]" captionTargetBox="[266,1276,321,1790]" captionTargetId="figure@45.[253,1307,305,1790]" captionTargetPageId="45" captionText="FIGURES 13 – 22. Aculeus full length. — 13. Carpophthoromyia flavofasciata. — 14. C. radulata. — 15. C. tritea. — 16. C. interrupta. — 17. C. schoutedeni. — 18. C. dividua. — 19. C. tessmanni. — 20. C. pseudotritea. — 21. C. virgata. — 22. C. vittata." httpUri="https://zenodo.org/record/172785/files/figure.png" pageId="13" pageNumber="14">Figs. 13</figureCitation>
, 
<figureCitation id="13223DBBFFD2FFC3565EFDA161FAFDB1" box="[383,413,599,625]" captionStart="FIGURES 28 – 33" captionStartId="46.[264,382,1845,1869]" captionTargetBox="[298,1244,875,1844]" captionTargetId="figure@46.[297,1289,864,1844]" captionTargetPageId="46" captionText="FIGURES 28 – 33. Apical part of aculeus. — 28. Carpophthoromyia flavofasciata. — 29. C. radulata. — 30. C. virgata. — 31. C. interrupta. — 32. C. schoutedeni. — 33. C. tritea." httpUri="https://zenodo.org/record/172787/files/figure.png" pageId="13" pageNumber="14">28</figureCitation>
)
