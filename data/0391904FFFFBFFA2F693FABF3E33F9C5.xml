<collectingRegion id="49FCEFBBFFFBFFA7F6EBF9923D72F9C1" box="[209,299,1547,1569]" country="Mongolia" name="Orhon" pageId="1" pageNumber="254">Orkhon</collectingRegion>
, north-eastern Hangay, 
<collectingRegion id="49FCEFBBFFFBFFA7F478F9923F5FF9C1" box="[578,774,1547,1569]" country="Mongolia" name="Arhangay" pageId="1" pageNumber="254">Arhangay Aimag</collectingRegion>
