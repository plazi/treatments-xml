<quantity id="4CC08CBCFFFBFFA7F6CBFA743D73F9E3" box="[241,298,1517,1539]" metricMagnitude="3" metricUnit="m" metricValue="6.0" pageId="1" pageNumber="254" unit="km" value="6.0">6 km</quantity>
west of 
<collectingMunicipality id="6BE3BB23FFFBFFA7F7AFFA743E13F9E2" box="[405,586,1517,1539]" pageId="1" pageNumber="254">Khotont Somon</collectingMunicipality>
