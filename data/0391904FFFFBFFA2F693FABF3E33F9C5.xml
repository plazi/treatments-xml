<taxonomicName id="4C385ADAFFFBFFA7F2A9FABF392EFADB" authorityName="Smirnov" authorityYear="1992" box="[1171,1399,1318,1339]" class="Branchiopoda" family="Branchiopoda (awaiting allocation)" genus="Archeoxus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Branchiopoda (awaiting allocation)" pageId="1" pageNumber="254" phylum="Arthropoda" rank="species" species="mirabilis">
