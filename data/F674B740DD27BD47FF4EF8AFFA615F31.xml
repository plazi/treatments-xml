<keyLead id="C52CA563DD26BD47FF4EFE62FA665CF1" pageId="1" pageNumber="30">
<number id="C8151471DD26BD47FF4EFE62FF5E5CD1" box="[151,164,440,463]" pageId="1" pageNumber="30" value="4.0">4</number>
