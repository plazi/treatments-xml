<caption id="85DFC6E88A7299296BE38B21FD08F8C2" box="[151,719,1847,1871]" httpUri="https://zenodo.org/record/274855/files/figure.png" pageId="7" pageNumber="20" targetBox="[154,1436,385,1823]" targetPageId="7">
<paragraph id="D11F96608A7299296BE38B21FD08F8C2" blockId="7.[151,719,1847,1871]" box="[151,719,1847,1871]" pageId="7" pageNumber="20">
