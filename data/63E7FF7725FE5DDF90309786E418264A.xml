<paragraph id="37E3CB4C0D86C660774122091EA0C5E5" pageId="0" pageNumber="73">
Three specimens 
<quantity id="6587D64D9B13B518C1F719801DA5EAD7" metricMagnitude="-2" metricUnit="m" metricValue="2.15" metricValueMax="2.4" metricValueMin="1.9" unit="mm" value="21.5" valueMax="24.0" valueMin="19.0">19-24 mm</quantity>
