<quantity id="DC6FC6BFB9FBFF43CBCBFEB1FF083910" box="[158,231,260,284]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="222" pageNumber="223" unit="m" value="500.0">500 m</quantity>
