<taxonomicName id="C7DD559C3783838199BF25B468CFCBE4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26904" authorityName="Forel" authorityYear="1886" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus lubbocki Forel" order="Hymenoptera" pageNumber="60" phylum="Arthropoda" rank="species" species="lubbocki">Camponotus Lubbocki</taxonomicName>
