<taxonomicName id="3673A8C8EDF03FBC6C90DD24F91DEB8D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26904" authorityName="Forel" authorityYear="1886" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus lubbocki Forel" order="Hymenoptera" pageNumber="60" phylum="Arthropoda" rank="subSpecies" species="lubbocki">Camponotus Lubbocki</taxonomicName>
