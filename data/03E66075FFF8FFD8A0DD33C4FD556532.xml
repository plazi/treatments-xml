<geoCoordinate id="EE7BB7A4FFE6FFD8A1E4356CFF5B6370" box="[189,241,611,634]" degrees="5" direction="north" minutes="7" orientation="latitude" pageId="16" pageNumber="327" precision="925" value="5.116667">
<emphasis id="B93B0D71FFE6FFD8A1E4356CFF606370" box="[189,202,611,634]" italics="true" pageId="16" pageNumber="327">n</emphasis>
5 7
</geoCoordinate>
