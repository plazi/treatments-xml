<geoCoordinate id="EE7BB7A4FFE6FFD8A10536EBFF3A60F1" box="[92,144,483,507]" degrees="5" direction="north" minutes="9" orientation="latitude" pageId="16" pageNumber="327" precision="925" value="5.15">
<emphasis id="B93B0D71FFE6FFD8A10536EBFFC360F1" box="[92,105,484,507]" italics="true" pageId="16" pageNumber="327">n</emphasis>
