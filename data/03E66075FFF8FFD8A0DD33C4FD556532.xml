<geoCoordinate id="EE7BB7A4FFE6FFD8A28F368BFBA16091" box="[982,1035,387,411]" degrees="5" direction="north" minutes="6" orientation="latitude" pageId="16" pageNumber="327" precision="925" value="5.1">
<emphasis id="B93B0D71FFE6FFD8A28F368BFC496091" box="[982,995,388,411]" italics="true" pageId="16" pageNumber="327">n</emphasis>
