<geoCoordinate id="EE7BB7A4FFE6FFD8A105350BFF3A6310" box="[92,144,515,539]" degrees="5" direction="north" minutes="7" orientation="latitude" pageId="16" pageNumber="327" precision="925" value="5.116667">
