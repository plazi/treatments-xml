<geoCoordinate id="EE7BB7A4FFE6FFD8A38336CBFCA560D1" box="[730,783,451,475]" degrees="5" direction="north" minutes="9" orientation="latitude" pageId="16" pageNumber="327" precision="925" value="5.15">
