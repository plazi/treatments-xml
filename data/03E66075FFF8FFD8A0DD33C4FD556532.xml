<geoCoordinate id="EE7BB7A4FFE6FFD8A3AC36EBFC8060F0" box="[757,810,483,507]" degrees="5" direction="north" minutes="7" orientation="latitude" pageId="16" pageNumber="327" precision="925" value="5.116667">
