<taxonomicName id="17C6CCFCFFACFFA9FECBFE04BDB82E36" authority="Pallas, 1766" authorityName="Pallas" authorityYear="1766" box="[295,694,500,522]" class="Polychaeta" family="Nereididae" genus="Nereis" kingdom="Animalia" order="Phyllodocida" pageId="37" pageNumber="38" phylum="Annelida" rank="subSpecies" species="cylindraria" subSpecies="belgica">
