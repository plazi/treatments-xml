<quantity id="AA206AF5FFFBFFC8CCEFFDF4FCB8962E" box="[736,831,541,566]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="6" pageNumber="148" unit="mm" value="5.3">5.3 mm</quantity>
