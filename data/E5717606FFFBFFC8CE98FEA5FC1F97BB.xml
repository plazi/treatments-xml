<quantity id="AA206AF5FFFBFFC8CC19FDF4FDD9962E" box="[534,606,541,566]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="6" pageNumber="148" unit="mm" value="7.0">7 mm</quantity>
