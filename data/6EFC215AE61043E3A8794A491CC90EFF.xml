<taxonomicName id="C6DDB2CEF4DE0CFA594ED606D2F30C32" class="Insecta" family="Cerambycidae" genus="Solenoptera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Solenoptera" order="Coleoptera" pageId="201" pageNumber="202" phylum="Arthropoda" rank="genus">Solenoptera</taxonomicName>
