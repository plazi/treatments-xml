<taxonomicName id="11984D098232FFAFFD0FFC67224CFC4B" box="[659,843,904,929]" class="Insecta" family="Chrysomelidae" genus="Paraplotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="239" phylum="Arthropoda" rank="species" species="tsoui">
<emphasis id="E4ECEA988232FFAFFD0FFC67224CFC4B" box="[659,843,904,929]" italics="true" pageId="16" pageNumber="239">Paraplotes tsoui</emphasis>
</taxonomicName>
.
