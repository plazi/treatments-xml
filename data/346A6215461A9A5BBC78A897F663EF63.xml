<taxonomicName id="78026E305F5070C0462D227C65E49DF3" class="Insecta" family="Braconidae" genus="Idiasta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Idiasta" order="Hymenoptera" pageId="28" pageNumber="65" phylum="Arthropoda" rank="genus">Idiasta</taxonomicName>
