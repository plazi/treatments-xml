<quantity id="4CC686B1FFC1C203FE37FB8A6208FB9B" box="[455,570,1058,1085]" metricMagnitude="-3" metricUnit="m" metricValue="4.25" metricValueMax="5.0" metricValueMin="3.5" pageId="6" pageNumber="81" unit="mm" value="4.25" valueMax="5.0" valueMin="3.5">3.5–5 mm</quantity>
