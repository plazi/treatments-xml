<specimenCode id="EF2E003CFFE47524FED0F9ADFE3BE399" box="[298,435,1655,1679]" collectionCode="G" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="20" pageNumber="107" type="Herbarium">G00341502</specimenCode>
