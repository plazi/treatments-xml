, PL 
<quantity id="883DCBD1B906FFBDFD9FFD50FD1E17A7" box="[598,705,674,700]" metricMagnitude="-4" metricUnit="m" metricValue="9.8" pageId="10" pageNumber="38" unit="mm" value="0.98">0.98 mm</quantity>
, PW 
<quantity id="883DCBD1B906FFBDFD37FD50FCB517A7" box="[766,874,674,700]" metricMagnitude="-3" metricUnit="m" metricValue="1.34" pageId="10" pageNumber="38" unit="mm" value="1.34">1.34 mm</quantity>
, HW 
<quantity id="883DCBD1B906FFBDFC65FD50FBC817A7" box="[940,1047,674,700]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="10" pageNumber="38" unit="mm" value="1.6">1.60 mm</quantity>
, Edist 
<quantity id="883DCBD1B906FFBDFBADFD50FB1017A7" box="[1124,1231,674,700]" metricMagnitude="-4" metricUnit="m" metricValue="4.6" pageId="10" pageNumber="38" unit="mm" value="0.46">0.46 mm</quantity>
, Ediam 
<quantity id="883DCBD1B906FFBDFAE4FD50FA4717A7" box="[1325,1432,674,700]" metricMagnitude="-4" metricUnit="m" metricValue="3.9" pageId="10" pageNumber="38" unit="mm" value="0.39">0.39 mm</quantity>
.
</paragraph>
</subSubSection>
