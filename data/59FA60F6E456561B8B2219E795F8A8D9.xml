<taxonomicName id="C3F8BD03342B1613A30CE230CB568142" baseAuthorityName="Martens" baseAuthorityYear="1867" class="Gastropoda" family="Veronicellidae" genus="Valiguna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Valiguna siamensis" order="Systellommatophora" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="siamensis">Valiguna siamensis</taxonomicName>
