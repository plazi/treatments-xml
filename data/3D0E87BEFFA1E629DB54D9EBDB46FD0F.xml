(
<taxonomicName id="72A74D2BFFA1E629DB4CD9EBDA88FF1A" authorityName="Gervais" authorityYear="1853" box="[1044,1223,226,250]" class="Reptilia" family="Mosasauridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="692" phylum="Chordata" rank="subFamily" subFamily="Mosasaurinae">MOSASAURINAE</taxonomicName>
