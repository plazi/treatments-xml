<geoCoordinate id="BAC40498FF9E4369FEE3FEF8124BE072" box="[280,351,267,291]" degrees="13.2" direction="east" orientation="longitude" pageId="15" pageNumber="1706" precision="5555" value="13.2">Ö13.2</geoCoordinate>
