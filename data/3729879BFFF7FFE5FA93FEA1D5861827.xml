<subSubSection id="F79A6506FFF7FFE5FA93FEA1D60C1C32" box="[151,525,294,321]" pageId="18" pageNumber="19" type="nomenclature">
<paragraph id="BF3F368DFFF7FFE5FA93FEA1D60C1C32" blockId="18.[151,525,294,321]" box="[151,525,294,321]" pageId="18" pageNumber="19">
<emphasis id="8DF4EA9FFFF7FFE5FA93FEA1D60C1C32" bold="true" box="[151,525,294,321]" pageId="18" pageNumber="19">
Genus 
<taxonomicName id="78804D0EFFF7FFE5FAEAFEA1D60C1C32" authority="Lutzen, 1968" authorityName="Lutzen" authorityYear="1968" box="[238,525,294,321]" class="Maxillopoda" family="Ascidicolidae" genus="Styelicola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
<emphasis id="8DF4EA9FFFF7FFE5FAEAFEA1D5621C33" bold="true" box="[238,355,294,321]" italics="true" pageId="18" pageNumber="19">Styelicola</emphasis>
<bibRefCitation id="DB114B7CFFF7FFE5FB6FFEA0D60C1C32" author="Lutzen, J." box="[363,525,294,321]" pageId="18" pageNumber="19" pagination="96 - 102" refId="ref142967" refString="Lutzen, J. (1968) Styel ẚccla bahus ẚa n. g., sp. nov. (family Ascidicolidae), a commensal copepod from Styela atlant ẚca and S. gela ẚncsa from the Skagerrak. Crustaceana, Supplement 1 (Studies on Copepoda), pp. 96 - 102. [https: // www. jstor. org / stable / 25027368]" type="journal article" year="1968">Lützen, 1968</bibRefCitation>
