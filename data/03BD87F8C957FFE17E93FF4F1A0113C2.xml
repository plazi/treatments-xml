<taxonomicName id="4C144D6DC957FFE07EB0FF4F1F1B10C3" authorityName="Southern" authorityYear="1921" box="[180,338,151,177]" class="Polychaeta" family="Serpulidae" genus="Ficopomatus" kingdom="Animalia" order="Sabellida" pageId="40" pageNumber="41" phylum="Annelida" rank="genus">
<emphasis id="B960EAFCC957FFE07EB0FF4F1F1B10C3" bold="true" box="[180,338,151,177]" italics="true" pageId="40" pageNumber="41">Ficopomatus</emphasis>
</taxonomicName>
