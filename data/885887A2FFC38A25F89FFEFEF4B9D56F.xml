<taxonomicName id="C7F14D37FFC38A25F54EFCDBF472DAC2" authorityName="K. Andersen" authorityYear="1905" box="[2690,3063,890,929]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="291" phylum="Chordata" rank="species" species="eloquens">
