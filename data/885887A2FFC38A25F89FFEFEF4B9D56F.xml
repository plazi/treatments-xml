<paragraph id="004E36B4FFC38A25F54EFD0BF4B6DD6E" blockId="12.[2689,3490,520,1090]" pageId="12" pageNumber="291">
<taxonomicName id="C7F14D37FFC38A25F54EFD0BF380DBB2" authorityName="K. Andersen" authorityYear="1905" box="[2690,3077,682,721]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="291" phylum="Chordata" rank="species" species="eloquens">
