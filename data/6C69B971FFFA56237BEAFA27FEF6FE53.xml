<collectionCode id="82D190A2FFFA56227A6EF922FEC4F948" box="[275,327,1690,1716]" country="Russia" httpUri="http://grbio.org/cool/z2y8-2jfr" name="Russian Academy of Sciences, Zoological Institute, Zoological Museum" pageId="2" pageNumber="548" type="Museum">ZIN</collectionCode>
)
