: 
<taxonomicName id="59C5B6EC7D6BE203660EA9FBA6E52FF9" class="Sordariomycetes" family="Ceratostomataceae" genus="Sphaeroderma" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Sphaeroderma freyi" order="Melanosporales" pageId="44" pageNumber="45" phylum="Ascomycota" rank="species" species="freyi">Sphaeroderma freyi</taxonomicName>
