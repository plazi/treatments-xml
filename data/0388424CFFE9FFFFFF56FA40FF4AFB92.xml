<taxonomicName id="4C2188D9FFEEFFFFFD1BFC1DFCE8FC03" authorityName="Niedbala" authorityYear="2000" box="[730,837,927,951]" class="Arachnida" family="Phthiracaridae" genus="Hoplophthiracarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageId="5" pageNumber="181" phylum="Arthropoda" rank="species" species="similis">
