<taxonomicName id="4C2188D9FFEEFFFFFE4BFD5BFCCDFD4A" authority="Niedbala, 2000" authorityName="Niedbala" authorityYear="2000" box="[394,864,741,766]" class="Arachnida" family="Phthiracaridae" genus="Hoplophthiracarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageId="5" pageNumber="181" phylum="Arthropoda" rank="species" species="similis">
