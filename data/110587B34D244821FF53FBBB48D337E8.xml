<taxonomicName id="5EAC4D264D3B4821FD46FED0485E3459" authorityName=": Gomez" authorityYear="2014" box="[642,815,295,321]" class="Demospongiae" family="Microcionidae" genus="Clathria" kingdom="Animalia" order="Poecilosclerida" pageId="48" pageNumber="49" phylum="Porifera" rank="species" species="echinata" subGenus="Clathria">
