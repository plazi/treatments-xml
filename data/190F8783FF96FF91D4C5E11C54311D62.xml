<emphasis id="A3D2EA87FF97FF96D5D0E0FC55361AC7" box="[386,628,1707,1729]" italics="true" pageId="75" pageNumber="76">Holopothrips porrosati</emphasis>
</taxonomicName>
and 
<taxonomicName id="56A64D16FF97FF96D6E7E0FB54721AC7" box="[693,816,1708,1729]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="signatus">
<emphasis id="A3D2EA87FF97FF96D6E7E0FB54721AC7" box="[693,816,1708,1729]" italics="true" pageId="75" pageNumber="76">H. signatus</emphasis>
</taxonomicName>
. (221) 
<taxonomicName id="56A64D16FF97FF96D7DBE0FC533E1AC7" box="[905,1148,1707,1729]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="porrosati">
