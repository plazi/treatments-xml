<taxonomicName id="6AD247ADFFCEFFF3FEBBFECFEB2AFEFF" authorityName="Breddin" authorityYear="1905" box="[274,561,259,285]" class="Insecta" family="Pentatomidae" genus="Brachystethus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="improvisus">
