(
<emphasis id="AD230A2CF710E47E40F41C12F22F2146" bold="true" pageId="0" pageNumber="69">A</emphasis>
); 10 
<normalizedToken id="34B87C46DC11D09069330BDC9D76EDB3" originalValue="μm">μm</normalizedToken>
(
