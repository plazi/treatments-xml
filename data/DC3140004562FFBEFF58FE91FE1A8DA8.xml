<elevation id="DFB516254562FFBBFF22FD79FF108EC7" box="[174,242,736,760]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="27" pageNumber="58" unit="m" value="750.0">750 m</elevation>
