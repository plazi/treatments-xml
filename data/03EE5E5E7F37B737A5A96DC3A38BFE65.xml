<taxonomicName id="4C4794CB7F34B737A1C46FEFA4FEFF39" authorityName="Cranston. According" authorityYear="2017" box="[1274,1420,189,215]" class="Insecta" family="Chironomidae" genus="Yarrhpelopia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="413" phylum="Arthropoda" rank="genus">
