<subSubSection id="CCE3E8C2F16B070FFF0E5A27FACEFDA3" lastPageId="9" lastPageNumber="104" pageId="8" pageNumber="103" type="diagnosis">
<paragraph id="8446BB49F16B070EFF0E5A27FCB8F86C" blockId="8.[151,1436,1876,2008]" pageId="8" pageNumber="103">
<emphasis id="B68D675BF16B070EFF0E5A27FEA1F8D6" bold="true" box="[199,321,1876,1901]" pageId="8" pageNumber="103">Diagnosis.</emphasis>
<taxonomicName id="43F9C0CAF16B070EFE855A27FD65F8D7" authority="Cai, 2017" authorityName="Cai" authorityYear="2017" box="[332,645,1876,1900]" class="Adenophorea" family="Sphaerolaimidae" genus="Parasphaerolaimus" kingdom="Animalia" order="Monhysterida" pageId="8" pageNumber="103" phylum="Nematoda" rank="species" species="jintiani" status="n. sp.">
<emphasis id="B68D675BF16B070EFE855A27FD65F8D7" box="[332,645,1876,1900]" italics="true" pageId="8" pageNumber="103">Parasphaerolaimus jintiani</emphasis>
</taxonomicName>
<taxonomicNameLabel id="ADBEDA20F16B070EFD595A27FD33F8D6" box="[656,723,1876,1901]" pageId="8" pageNumber="103" rank="species">
<emphasis id="B68D675BF16B070EFD595A27FD33F8D6" bold="true" box="[656,723,1876,1901]" pageId="8" pageNumber="103">n. sp.</emphasis>
</taxonomicNameLabel>
