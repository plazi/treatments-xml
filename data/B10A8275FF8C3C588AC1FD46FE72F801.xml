<taxonomicName id="FEA348E0FF8C3C588AC1FCDEFE65FCCA" authority="Broun, 1914: 258" authorityName="Broun" authorityPageNumber="258" authorityYear="1914" box="[151,509,868,893]" class="Insecta" family="Chrysomelidae" genus="Aphilon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="impressa">
