Papp, 
<quantity id="4CF79B2AE928FFB33A54FCB8FEAD233A" box="[283,326,834,853]" metricMagnitude="1" metricUnit="m" metricValue="5.0952399999999995" pageId="3" pageNumber="284" unit="in" value="2006.0">2006</quantity>
