; 
<emphasis id="B95AD64DFFB23D12FE3BCF9FFE58FEBF" bold="true" box="[433,452,242,259]" pageId="10" pageNumber="461">D.</emphasis>
, 
<taxonomicName id="4C2E71DCFFB23D12FE44CF9FFD8BFEBF" authorityName="Brandt" authorityYear="1878" box="[462,535,242,259]" class="Mammalia" family="Rhinocerotidae" genus="Dihoplus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="10" pageNumber="461" phylum="Chordata" rank="genus">
