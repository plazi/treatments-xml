<taxonomicName id="4C2E71DCFFB23D12FEA5CF9FFE3AFEBF" authorityName="Gray" authorityYear="1868" box="[303,422,242,259]" class="Mammalia" family="Rhinocerotidae" genus="Ceratotherium" kingdom="Animalia" order="Perissodactyla" pageId="10" pageNumber="461" phylum="Chordata" rank="genus">
