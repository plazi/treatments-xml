<taxonomicName id="4C5087B9FFA4FFE40BB233DEFEBEF909" class="Insecta" family="Syrphidae" genus="Phytomia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="species" species="pubipennis">
<emphasis id="B9242028FFA4FFE40BB233DEFEBEF909" italics="true" pageId="19" pageNumber="220">P. pubipennis</emphasis>
