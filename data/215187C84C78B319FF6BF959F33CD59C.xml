<taxonomicName id="6EF84D5D4C78B318FB6AF900F627D26C" authority="Bernhauer, 1921" authorityName="Bernhauer" authorityYear="1921" class="Insecta" family="Staphylinidae" genus="Trigonopselaphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="elvirae">
