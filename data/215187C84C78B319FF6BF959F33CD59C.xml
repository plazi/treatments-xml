<taxonomicName id="6EF84D5D4C78B318FC4CF89AF226D200" authority="Bernhauer, 1921" authorityName="Bernhauer" authorityYear="1921" box="[944,1395,1870,1892]" class="Insecta" family="Staphylinidae" genus="Trigonopselaphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="elvirae">
