<taxonomicName id="6EF84D5D4C78B318FCA7F926F487D26C" authorityName="Kirby" authorityYear="1837" box="[859,978,1778,1800]" class="Insecta" family="Staphylinidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="subTribe" subTribe="Philonthina">Philonthina</taxonomicName>
