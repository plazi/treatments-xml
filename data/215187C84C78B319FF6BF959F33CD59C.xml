<taxonomicName id="6EF84D5D4C78B318FD39F901F4BCD38E" authority="Bernhauer, 1921" authorityName="Bernhauer" authorityYear="1921" box="[709,1001,1748,1770]" class="Insecta" family="Staphylinidae" genus="Prionopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="genus">
