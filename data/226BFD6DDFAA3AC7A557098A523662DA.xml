<taxonomicName id="2321D50DB34778AF390046B15A49EEE4" authority="Hulcr &amp; Cognato, 2010" authorityName="Hulcr &amp; Cognato" authorityYear="2010" class="Insecta" family="Curculionidae" genus="Planiculus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Planiculus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Planiculus</taxonomicName>
