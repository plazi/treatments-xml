<taxonomicName id="CAA721BCFF84FFC2EE9BF96E917003EE" authority="(Hamilton)" baseAuthorityName="Hamilton" baseAuthorityYear="1822" box="[151,513,1728,1755]" class="Actinopterygii" family="Toxotidae" genus="Toxotes" kingdom="Animalia" order="Perciformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="chatareus">
<emphasis id="3FD3862DFF84FFC2EE9BF96E917003EE" bold="true" box="[151,513,1728,1755]" pageId="16" pageNumber="17">
<emphasis id="3FD3862DFF84FFC2EE9BF96E920103EF" bold="true" box="[151,368,1729,1755]" italics="true" pageId="16" pageNumber="17">Toxotes chatareus</emphasis>
