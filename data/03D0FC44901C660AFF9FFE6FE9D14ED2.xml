, qui specimen descriptum ante plures anos communicavit [= rare in 
<collectingCountry id="F36E0DC2901C660AFD4BFDACE8EC4F56" box="[677,759,542,561]" name="Denmark" pageId="67" pageNumber="178">Denmark</collectingCountry>
, 
<collectorName id="268C2884901C660AFD10FDADE9794F56" box="[766,866,542,562]" pageId="67" pageNumber="178">Mr. Staeger</collectorName>
