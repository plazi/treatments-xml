. 
<typeStatus id="54C2F3F0901C660AFE17FDB6E8574F72" box="[505,588,516,535]" pageId="67" pageNumber="178">HOLOTYPE</typeStatus>
: 
<specimenCount id="9D7F86DB901C660AFDB8FE4CE87D4F70" box="[598,614,510,535]" pageId="67" pageNumber="178" type="female">♀</specimenCount>
, 
<collectingCountry id="F36E0DC2901C660AFD81FDB6E8DB4F70" box="[623,704,516,535]" name="Denmark" pageId="67" pageNumber="178">Denmark</collectingCountry>
, ‘ 
<location id="8EA61B89901C660AFD25FDB6E9594F70" LSID="urn:lsid:plazi:treatment:03D0FC44901C660AFF9FFE6FE9D14ED2:8EA61B89901C660AFD25FDB6E9594F70" box="[715,834,516,535]" country="Denmark" name="Hab. in Dania" pageId="67" pageNumber="178">Hab. in Dania</location>
rarissime, 
