<taxonomicName id="4C414D48CC5EFFC8FE99FD4134AAFD58" authority="Alvarado, 2020" authorityName="Alvarado" authorityYear="2020" box="[280,437,658,683]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="192" phylum="Arthropoda" rank="species" species="pusaq" status="sp. nov.">
<emphasis id="B935EAD9CC5EFFC8FE99FD4134AAFD58" box="[280,437,658,683]" italics="true" pageId="11" pageNumber="192">Mnioes pusaq</emphasis>
</taxonomicName>
<emphasis id="B935EAD9CC5EFFC8FE3DFD42370AFD58" bold="true" box="[444,533,657,683]" pageId="11" pageNumber="192">
