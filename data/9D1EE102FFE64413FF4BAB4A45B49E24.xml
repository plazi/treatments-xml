<taxonomicName id="D2B72B97FFE64413FDDBA81D43C89E4B" box="[519,754,525,549]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="11" pageNumber="72" phylum="Arthropoda" rank="species" species="escalerai">
