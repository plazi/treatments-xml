<taxonomicName id="D2B72B97FFE64413FB79A81E445F9E4B" box="[1189,1381,525,549]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="11" pageNumber="72" phylum="Arthropoda" rank="species" species="catalana" subGenus="Dicampa">
