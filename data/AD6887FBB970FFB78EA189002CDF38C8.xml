<number id="930924CAB970FFB78B068AD12C1238D6" box="[1328,1366,1707,1728]" pageId="10" pageNumber="543" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
% TL); body without bands......................................................... 
<taxonomicName id="E2C14D6EB970FFB78A7D8AB32D8838C8" box="[1099,1228,1737,1758]" class="Elasmobranchii" family="Rhinobatidae" genus="Platyrhina" kingdom="Animalia" order="Rajiformes" pageId="10" pageNumber="543" phylum="Chordata" rank="species" species="hyugaensis">
