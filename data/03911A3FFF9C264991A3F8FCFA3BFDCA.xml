<taxonomicName id="4C38D0AAFF9C2648954BF861FA3BF87C" authority="Belokobylskij, 1994" authorityName="Belokobylskij" authorityYear="1994" box="[1151,1436,1980,2002]" class="Insecta" family="Braconidae" genus="Tamdaona" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="491" phylum="Arthropoda" rank="species" species="janus">
