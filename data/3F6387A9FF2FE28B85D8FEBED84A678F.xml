<taxonomicName id="70CA4D3CFF2FE28B84D8FE65DF2564E4" box="[407,555,438,461]" class="Hydrozoa" family="Aeginidae" genus="Aegina" kingdom="Animalia" order="Narcomedusae" pageId="194" pageNumber="195" phylum="Cnidaria" rank="species" species="rosea">
