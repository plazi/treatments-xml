<taxonomicName id="79F04D1ED532FFFB6C74C49EFE50FD47" box="[343,488,686,709]" class="Arachnida" family="Scutoverticidae" genus="Scutovertex" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="43" phylum="Arthropoda" rank="species" species="arenocolus">
