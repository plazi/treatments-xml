<taxonomicName id="5BD24D100B57FF81FF74FBEAFE9FFB9B" box="[201,333,1067,1091]" class="Insecta" family="Curculionidae" genus="Caneorhinus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="gravidus">
