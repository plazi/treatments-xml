<taxonomicName id="5BD24D100B57FF81FE9BFBB2FE7AFB53" box="[294,424,1139,1163]" class="Insecta" family="Curculionidae" genus="Caneorhinus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="gravidus">
<emphasis id="AEA6EA810B57FF81FE9BFBB2FE7AFB53" box="[294,424,1139,1163]" italics="true" pageId="10" pageNumber="11">C. gravidus</emphasis>
</taxonomicName>
