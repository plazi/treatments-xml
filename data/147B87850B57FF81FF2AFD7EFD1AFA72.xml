<taxonomicName id="5BD24D100B57FF81FC09FBEAFBB2FB9B" box="[948,1120,1067,1091]" class="Insecta" family="Curculionidae" genus="Cylindrorrhinus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="confusaneus">
