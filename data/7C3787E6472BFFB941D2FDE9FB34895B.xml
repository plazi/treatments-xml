<collectingCountry id="8C897660472AFFB94022FE8FFE64897F" box="[359,426,297,323]" name="China" pageId="3" pageNumber="208">China</collectingCountry>
(
<collectingRegion id="365AF812472AFFB940FCFE8FFDDB897F" box="[441,533,297,323]" country="China" name="Yunnan" pageId="3" pageNumber="208">Yunnan</collectingRegion>
