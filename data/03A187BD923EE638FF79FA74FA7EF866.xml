<date id="FFB6106B923EE638FDBAF9A6FD97F9C6" box="[596,629,1617,1640]" pageId="6" pageNumber="65">
<number id="3DC0248C923EE638FDBAF9A6FD83F9C6" box="[596,609,1617,1640]" pageId="6" pageNumber="65" value="2.0">2</number>
x
