<quantity id="34249B37FFBE3F359973B327ECC25B7E" box="[1065,1160,592,618]" metricMagnitude="3" metricUnit="m" metricValue="4.1" pageId="7" pageNumber="42" unit="m" value="4100.0">4100 m</quantity>
, 
<date id="87621012FFBE3F3599CDB327ED5E5B7E" box="[1175,1300,592,618]" pageId="7" pageNumber="42" value="1984-06-28">
<number id="451424F5FFBE3F3599CDB327ECFC5B7E" box="[1175,1206,592,618]" pageId="7" pageNumber="42" value="28.0">28</number>
