<taxonomicName id="98D9F91088E699F686EDF59A305E4552" authorityName="Ubagan &amp; Lee &amp; Kim &amp; Shin" authorityYear="2020" class="Asteroidea" family="Echinasteridae" genus="Henricia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="epiphysialis" order="Spinulosida" pageId="0" pageNumber="1" phylum="Echinodermata" rank="species" species="epiphysialis">epiphysialis</taxonomicName>
