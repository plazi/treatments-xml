<taxonomicName id="52C9C5EB0C1DEAFECD27B6090BCB3C1D" class="Bivalvia" family="Nuculanidae" genus="Ledella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ledella" order="Nuculanida" pageId="8" pageNumber="9" phylum="Mollusca" rank="genus">Ledella</taxonomicName>
