<quantity id="AD76A8D91B0A6409FC623C7B3353B34A" box="[975,1067,794,820]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="34" pageNumber="35" unit="mm" value="1.2">1.2 mm</quantity>
