<quantity id="AD76A8D91B0A6409FD1E3C7B3464B34A" box="[691,796,794,820]" metricMagnitude="-2" metricUnit="m" metricValue="3.05" pageId="34" pageNumber="35" unit="mm" value="30.5">30.5 mm</quantity>
