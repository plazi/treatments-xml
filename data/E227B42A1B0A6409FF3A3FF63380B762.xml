<quantity id="AD76A8D91B0A6409FE013C7B3570B34A" box="[428,520,794,820]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="34" pageNumber="35" unit="mm" value="0.9">0.9 mm</quantity>
