<quantity id="AD76A8D91B0A6409FA8E3D9337BDB34A" metricMagnitude="-2" metricUnit="m" metricValue="1.31" metricValueMax="2.17" metricValueMin="0.45" pageId="34" pageNumber="35" unit="mm" value="13.1" valueMax="21.7" valueMin="4.5">4.5 – 21.7 mm</quantity>
(mean: 
<quantity id="AD76A8D91B0A6409FE883C7B36F5B34A" box="[293,397,794,820]" metricMagnitude="-2" metricUnit="m" metricValue="1.14" pageId="34" pageNumber="35" unit="mm" value="11.4">11.4 mm</quantity>
