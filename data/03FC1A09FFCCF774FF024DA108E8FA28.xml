species: 
<taxonomicName id="4C55D09CFFCCF774FEA54B810A03FB08" authority="Burmeister, 1847" authorityName="Burmeister" authorityYear="1847" box="[359,842,1251,1275]" class="Insecta" family="Dynastidae" genus="Heterogomphus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="18" phylum="Arthropoda" rank="species" species="julus">
<emphasis id="B921770DFFCCF774FEA54B810B22FB08" box="[359,619,1251,1275]" italics="true" pageId="19" pageNumber="18">Heterogomphus julus</emphasis>
<bibRefCitation id="EFC4D6EEFFCCF774FDB34B810A03FB08" author="Burmeister, H. C. C." box="[625,842,1251,1275]" pageId="19" pageNumber="18" refId="ref23787" refString="Burmeister, H. C. C. 1847. Handbuch der Entomologie. (Coleoptera, Lamellicornia Xylophila et Pectinicornia) Vol. 5. T. C. F. Enslin; Berlin, Germany. 584 p." type="book" year="1847">Burmeister, 1847</bibRefCitation>
