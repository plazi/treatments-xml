A large 
<taxonomicName id="4C0569BEFFA8FF892E9D15BAFDD0FD4A" authorityName="Casey" authorityYear="1914" box="[415,550,716,738]" class="Insecta" family="Carabidae" genus="Celiamorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="29" phylum="Arthropoda" rank="genus">
