<quantity id="4CAA8FF83803595131F0E41AC066154D" box="[737,839,1619,1644]" metricMagnitude="-4" metricUnit="m" metricValue="7.6" pageId="3" pageNumber="580" unit="mm" value="0.76">0.76 mm</quantity>
, PW 
<quantity id="4CAA8FF8380359513095E41DC0CB154D" box="[900,1002,1620,1644]" metricMagnitude="-3" metricUnit="m" metricValue="1.14" pageId="3" pageNumber="580" unit="mm" value="1.14">1.14 mm</quantity>
