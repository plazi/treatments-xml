<taxonomicName id="27CA4D35FEF5FE935A96FC06FC8862DD" authorityName="Rondani" authorityYear="1859" box="[648,827,907,932]" class="Insecta" family="Tachinidae" genus="Exorista" kingdom="Animalia" order="Diptera" pageId="312" pageNumber="313" phylum="Arthropoda" rank="species" species="crinita">
