<typeStatus id="54B09A8FDA35E734BD22FEBCFE379858" box="[373,472,360,383]" pageId="3" pageNumber="60">Holotype</typeStatus>
Ə, 
<collectingCountry id="F31C64BDDA35E734BE55FEBCFDA99858" box="[514,582,360,383]" name="China" pageId="3" pageNumber="60">China</collectingCountry>
: Yunnan Province, Yingjiang County, Tongbiguan, 
<date id="FFB502EDDA35E734B827FEBCFB1B9858" box="[1136,1268,360,383]" pageId="3" pageNumber="60" value="2011-06-03">
<number id="3DC3360ADA35E734B827FEBCFB929858" box="[1136,1149,360,383]" pageId="3" pageNumber="60" value="3.0">3</number>
