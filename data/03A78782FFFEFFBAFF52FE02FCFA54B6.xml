/ 
<taxonomicName id="4C0E4D17FFFEFFBBFD53FC53FCB25699" box="[662,773,998,1023]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="392" phylum="Arthropoda" rank="species" species="pennardsi">
<emphasis id="B97AEA86FFFEFFBBFD53FC53FCB25699" box="[662,773,998,1023]" italics="true" pageId="4" pageNumber="392">pennardsi</emphasis>
</taxonomicName>
/ Det.: X. Mengual 
<number id="3DC624B3FFFEFFBBFC23FC52FB945699" box="[998,1059,999,1023]" pageId="4" pageNumber="392" value="2013.0">2013</number>
’ [
<specimenCount id="9D08FD1DFFFEFFBBFBFCFC52FB345699" box="[1081,1155,998,1023]" pageId="4" pageNumber="392" type="male">
<number id="3DC624B3FFFEFFBBFBFCFC52FBF15699" box="[1081,1094,999,1023]" pageId="4" pageNumber="392" value="1.0">1</number>
