female
</specimenCount>
, RMNH; 
<specimenCount id="9D08FD1DFFFEFFBBFA48FC52FF675145" pageId="4" pageNumber="392" type="male">
<number id="3DC624B3FFFEFFBBFA48FC52FA2C5699" box="[1421,1435,999,1023]" pageId="4" pageNumber="392" value="1.0">1</number>
