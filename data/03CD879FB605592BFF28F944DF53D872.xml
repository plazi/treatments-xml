<taxonomicName id="4C644D0AB602592BFD87FC80D98DDF1A" authorityName="" authorityYear="1789" box="[568,690,836,860]" class="Magnoliopsida" family="Lauraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Laurales" pageId="12" pageNumber="263" phylum="Magnoliopsida" rank="family">Lauraceae</taxonomicName>
