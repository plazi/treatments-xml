<taxonomicName id="E785E6833F25FF9AFDFAF9D6ADFBF9AE" authority="Franklin." authorityName="Franklin." box="[619,936,1622,1644]" class="Insecta" family="Thripidae" genus="Ctenothrips" kingdom="Animalia" order="Thysanoptera" pageId="0" pageNumber="273" phylum="Arthropoda" rank="species" species="bridwelli">
