<taxonomicName id="4C2F4D433C597456FC781B81FAD9F911" box="[1003,1313,1757,1782]" class="Insecta" family="Reduviidae" genus="Penthikocleptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="89" phylum="Arthropoda" rank="species" species="australicus">
