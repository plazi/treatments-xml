<taxonomicName id="4C7BB7F02621FFD061BAC8A0FF41FF12" authority="Riedel, 2020" authorityName="Riedel" authorityYear="2020" class="Insecta" family="Eulophidae" genus="Ichneumon" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="378" phylum="Arthropoda" rank="species" species="megextensorius" status="spec. nov.">
<emphasis id="B90F10612621FFD061BAC8A0FF41FF12" italics="true" pageId="13" pageNumber="378">Ichneumon megextensorius</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23CAD1A2621FFD065EEC882FEDAFF12" box="[192,288,1525,1546]" pageId="13" pageNumber="378" rank="species">spec. nov.</taxonomicNameLabel>
, 
<emphasis id="B90F10612621FFD06406C882FECEFF12" bold="true" box="[296,308,1525,1546]" pageId="13" pageNumber="378">7</emphasis>
. 
<taxonomicName id="4C7BB7F02621FFD06411C882FDDAFF12" authority="Riedel, 2020" authorityName="Riedel" authorityYear="2020" box="[319,544,1525,1546]" class="Insecta" family="Eulophidae" genus="Ichneumon" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="378" phylum="Arthropoda" rank="species" species="pilocephalus" status="spec. nov.">
