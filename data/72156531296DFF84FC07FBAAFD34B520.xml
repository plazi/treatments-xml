<taxonomicName id="3DBCAFA4296CFF84FE87FEAAFE50B520" authorityName="Dana" authorityYear="1852" box="[306,433,317,341]" class="Malacostraca" family="Diogenidae" genus="Clibanarius" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="28" phylum="Arthropoda" rank="genus">
