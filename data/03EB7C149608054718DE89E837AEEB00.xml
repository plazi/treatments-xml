<taxonomicName id="4C42B681960805471D5C8CE537AEEA8A" box="[1301,1435,1177,1199]" class="Insecta" family="Diprionidae" genus="Macrodiprion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="540" phylum="Arthropoda" rank="genus">
