<keyLead id="30B36E379608054718DE8C0037AEEA8A" pageId="3" pageNumber="540">
<number id="3D8ADF259608054718DE8C003293EAB7" box="[151,166,1148,1170]" pageId="3" pageNumber="540" value="6.0">6</number>
