<taxonomicName id="4C461DB22F1FEC04FF02ABB9FD0FFCE7" authority="Moraes &amp; Duarte, 2015" authorityName="Moraes &amp; Duarte" authorityYear="2015" box="[151,744,760,787]" class="Insecta" family="Arctiidae" genus="Dysschema" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="286" phylum="Arthropoda" rank="species" species="amapoarum">
