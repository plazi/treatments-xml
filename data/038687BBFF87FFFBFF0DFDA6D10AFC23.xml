<quantity id="4CD79B48FF81FFFBFDAAFE2AD17BFE31" box="[555,599,448,474]" metricMagnitude="-1" metricUnit="m" metricValue="1.524" pageId="61" pageNumber="62" unit="in" value="6.0">6 in</quantity>
