</taxonomicName>
<number id="7101B978FFB2FFFBFD16242AFD11D259" box="[705,729,1601,1623]" pageId="5" pageNumber="46" value="45.0">45</number>
—male pygofer and subgenital plate, lateral view; 
<number id="7101B978FFB2FFFBFB09242AFB30D259" box="[1246,1272,1601,1623]" pageId="5" pageNumber="46" value="46.0">46</number>
—aedeagus, lateral view; 
<number id="7101B978FFB2FFFBFED42408FED4D276" box="[259,284,1635,1656]" pageId="5" pageNumber="46" value="47.0">47</number>
—same, ventral view; 
<number id="7101B978FFB2FFFBFDD42409FDD4D276" box="[515,540,1634,1656]" pageId="5" pageNumber="46" value="48.0">48</number>
