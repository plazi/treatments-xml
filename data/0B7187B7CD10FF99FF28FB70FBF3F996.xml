<paragraph id="836736A1CD10FF9BFF28FA25FE81FA18" blockId="5.[151,1437,1441,2006]" pageId="5">
<emphasis id="B1ACEAB3CD10FF9BFF28FA25FEC1FA7C" bold="true" box="[151,286,1441,1466]" pageId="5">Etymology.</emphasis>
<taxonomicName id="44D84D22CD10FF9BFE99FA25FE5AFA7C" box="[294,389,1441,1466]" class="Insecta" family="Gomphidae" genus="Burmagomphus" kingdom="Animalia" order="Odonata" pageId="5" phylum="Arthropoda" rank="species" species="dentatus">
<emphasis id="B1ACEAB3CD10FF9BFE99FA25FE5AFA7C" box="[294,389,1441,1466]" italics="true" pageId="5">dentatus</emphasis>
