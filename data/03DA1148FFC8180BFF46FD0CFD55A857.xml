<taxonomicName id="4C73DBDDFFC8180BFF67FD0CFD4FACA6" authority="Sergeev et Pokivajlov, 1992" authorityName="Sergeev et Pokivajlov" authorityYear="1992" box="[182,680,686,712]" class="Insecta" family="Tettigoniidae" genus="Pravdiniana" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
