<heading id="9F6EA228FFF7FFE4BB30FBA93A34FB93" bold="true" box="[351,556,1112,1136]" centered="true" fontSize="10" level="3" pageId="19" pageNumber="410" reason="2">
<emphasis id="F6EDC956FFF7FFE4BB30FBA93A34FB93" bold="true" box="[351,556,1112,1136]" pageId="19" pageNumber="410">
The 
<taxonomicName id="03996EC7FFF7FFE4BBFDFBA939C7FB8C" authorityName="Platnick &amp; Sedgwick" authorityYear="1984" box="[402,479,1112,1135]" class="Arachnida" family="Liphistiidae" genus="Liphistius" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="410" phylum="Arthropoda" rank="species" species="tioman">
<emphasis id="F6EDC956FFF7FFE4BBFDFBA939C7FB8C" bold="true" box="[402,479,1112,1135]" italics="true" pageId="19" pageNumber="410">tioman</emphasis>
</taxonomicName>
