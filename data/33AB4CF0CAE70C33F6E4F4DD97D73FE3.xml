<paragraph id="7696C950CD23562156E1136D41D1965B" pageId="1" pageNumber="2">
<taxonomicName id="423565F9CFC68B321BC1ACB894CA5A19" authority="Schmarda, 1861" authorityName="Schmarda" authorityYear="1861" class="Oligochaeta" family="Megascolecidae" genus="Perichaeta" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Perichaeta bicincta" order="Haplotaxida" pageId="1" pageNumber="2" phylum="Annelida" rank="species" species="bicincta">Perichaeta bicincta</taxonomicName>
