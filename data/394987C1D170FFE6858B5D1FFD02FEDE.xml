<taxonomicName id="76E04D54D170FFE587BB5DA4FB7AFD76" authority="Franz, 1975: 273" authorityName="Franz" authorityPageNumber="273" authorityYear="1975" box="[679,1189,641,663]" class="Insecta" family="Staphylinidae" genus="Palaeoscydmaenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="68" phylum="Arthropoda" rank="species" species="australiensis">
<emphasis id="8394EAC5D170FFE587BB5DA4FC2FFD76" box="[679,1008,641,663]" italics="true" pageId="5" pageNumber="68">Palaeoscydmaenus australiensis</emphasis>
