<taxonomicName id="E3A7FE88FF3E83CEFF4C59C5FF73FE46" box="[136,227,421,445]" class="Magnoliopsida" family="Brassicaceae" genus="Graellsia" kingdom="Plantae" order="Brassicales" pageId="131" pageNumber="131" phylum="Tracheophyta" rank="genus">
