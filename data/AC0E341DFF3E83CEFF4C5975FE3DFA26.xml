<taxonomicName id="E3A7FE88FF3E83CEFD8E59A5FD35FE26" box="[586,677,453,477]" class="Magnoliopsida" family="Brassicaceae" genus="Graellsia" kingdom="Plantae" order="Brassicales" pageId="131" pageNumber="131" phylum="Tracheophyta" rank="genus">
