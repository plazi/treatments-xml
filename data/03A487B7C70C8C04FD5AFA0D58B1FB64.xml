<materialsCitation id="3B653CFCC70D8C07FD4CFCE15DA9FCB2" box="[761,1207,854,878]" location="Francke" municipality="Lourenco" pageId="125" pageNumber="125" specimenCount="1">
troglobie (
<collectingMunicipality id="6BD6ACDBC70D8C07FCD9FCE15AC6FCB2" box="[876,984,854,878]" pageId="125" pageNumber="125">Lourenço</collectingMunicipality>
