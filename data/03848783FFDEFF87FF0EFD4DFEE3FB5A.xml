species
</emphasis>
. 
<taxonomicName id="4C2D4D16FFDEFF87FEB3FC17FC92FC3A" authority="W. Horn, 1934" authorityName="W. Horn" authorityYear="1934" box="[298,861,928,953]" class="Insecta" family="Carabidae" genus="Prothyma" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="centropunctata" subGenus="Megalomma">
