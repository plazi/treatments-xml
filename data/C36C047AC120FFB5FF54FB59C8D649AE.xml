species:
</emphasis>
<taxonomicName id="8CC5CEEFC120FFB5FEAFF9BAC8C8493D" authority="Ekins, Benayahu &amp; McFadden" authorityName="Ekins, Benayahu &amp; McFadden" authorityYear="2022" box="[364,914,1565,1591]" class="Anthozoa" family="Xeniidae" genus="Latissimia" kingdom="Animalia" order="Alcyonacea" pageId="9" pageNumber="430" phylum="Cnidaria" rank="species" species="opalia" status="sp. nov.">
<emphasis id="79B1697EC120FFB5FEAFF9BAC975493D" box="[364,559,1565,1591]" italics="true" pageId="9" pageNumber="430">Latissimia opalia</emphasis>
Ekins, Benayahu &amp; McFadden
</taxonomicName>
, 
<emphasis id="79B1697EC120FFB5FC5EF9B9C8AD493D" bold="true" box="[925,1015,1565,1591]" pageId="9" pageNumber="430">
<taxonomicNameLabel id="6282D405C120FFB5FC5EF9B9C8AD493D" box="[925,1015,1565,1591]" pageId="9" pageNumber="430" rank="species">sp. nov.</taxonomicNameLabel>
