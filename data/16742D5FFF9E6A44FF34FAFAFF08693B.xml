<taxonomicName id="59DDE7CAFF836A44FEF7FDD0FDA368AB" authorityName="Stevens" authorityYear="2011" box="[340,608,620,644]" class="Insecta" family="Braconidae" genus="Therophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="aalvikorum">
