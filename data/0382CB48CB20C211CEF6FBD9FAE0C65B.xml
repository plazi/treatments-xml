(
<collectingRegion id="49EFB4BCCB20C211CDD6F8FEFBCDC60B" box="[951,1093,1813,1839]" country="Costa Rica" name="Guanacaste" pageId="60" pageNumber="61">Guanacaste</collectingRegion>
