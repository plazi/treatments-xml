<heading id="D0F481D6FFC56707FF2CC1D0290F8675" bold="true" box="[151,579,1772,1797]" fontSize="10" level="2" pageId="6" pageNumber="7" reason="2">
<emphasis id="B977EAA8FFC56707FF2CC1D0290F8675" bold="true" box="[151,579,1772,1797]" pageId="6" pageNumber="7">
On 
<taxonomicName id="4C034D39FFC56707FF78C1D0290F8675" authority="Casey, 1924" authorityName="Casey" authorityYear="1924" box="[195,579,1772,1797]" class="Insecta" family="Cerambycidae" genus="Prionus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="subGenus" subGenus="Prionellus">
<emphasis id="B977EAA8FFC56707FF78C1D02A538675" bold="true" box="[195,287,1772,1797]" italics="true" pageId="6" pageNumber="7">Prionus</emphasis>
(
<emphasis id="B977EAA8FFC56707FE94C1D02AEB8675" bold="true" box="[303,423,1772,1797]" italics="true" pageId="6" pageNumber="7">Prionellus</emphasis>
) 
<bibRefCitation id="EF924B4BFFC56707FE0CC1D0290F8675" author="Casey" box="[439,579,1772,1797]" pageId="6" pageNumber="7" refString="Casey, T. L. (1924) Additions to the known Coleoptera of North America. Memoirs on the Coleoptera, 11, 1 - 347." type="journal article" year="1924">
Casey, 
<number id="3DCB249DFFC56707FDB0C1D0290F8675" box="[523,579,1772,1797]" pageId="6" pageNumber="7" value="1924.0">1924</number>
</bibRefCitation>
</taxonomicName>
</emphasis>
