<taxonomicName id="4C364D08026DFFCA53ECFA2FFA4670C8" authority="(Am Stein)" authorityName="Baez &amp; Oromi" authorityYear="2010" baseAuthorityName="Am Stein" baseAuthorityYear="1860" box="[1224,1431,1448,1471]" class="Insecta" family="Muscidae" genus="Limnophora" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="124" phylum="Arthropoda" rank="species" species="tigrina">
