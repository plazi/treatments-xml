<taxonomicName id="8FEE4D41FF8CFFA7FAB6FD1560DAC98B" box="[1318,1433,690,716]" class="Reptilia" kingdom="Animalia" order="Squamata" pageId="101" pageNumber="102" phylum="Chordata" rank="order">Squamata</taxonomicName>
