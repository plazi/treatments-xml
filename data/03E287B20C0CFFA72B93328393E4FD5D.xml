<taxonomicName id="4C4B4D270C0CFFA42E86306C9679F96C" authorityName="Boulenger" authorityYear="1906" class="Actinopterygii" family="Claroteidae" genus="Auchenoglanis" kingdom="Animalia" order="Siluriformes" pageId="21" pageNumber="46" phylum="Chordata" rank="species" species="tanganicanus">
