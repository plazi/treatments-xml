</taxonomicName>
<taxonomicNameLabel id="1ABCF15FDB37FFEAFDC3640EFDABE864" box="[546,625,1622,1645]" pageId="1" pageNumber="448" rank="species">sp. nov.</taxonomicNameLabel>
<emphasis id="018F4C24DB37FFEAFD96640DFD5EE864" bold="true" box="[631,644,1621,1645]" pageId="1" pageNumber="448">1</emphasis>
, 
<typeStatus id="EC402E94DB37FFEAFD71640EFD33E867" box="[656,745,1622,1646]" pageId="1" pageNumber="448">holotype</typeStatus>
female, lateral habitus, scale bar = 
<quantity id="F4033DD3DB37FFEAFBB0640EFB55E864" box="[1105,1167,1622,1646]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="448" unit="mm" value="1.0">1 mm</quantity>
; 
<emphasis id="018F4C24DB37FFEAFB78640DFB7CE864" bold="true" box="[1177,1190,1621,1645]" pageId="1" pageNumber="448">2</emphasis>
, head, frontal view, surface sculpturing not indicated; 
<emphasis id="018F4C24DB37FFEAFE306421FE04E898" bold="true" box="[465,478,1657,1681]" pageId="1" pageNumber="448">3</emphasis>
