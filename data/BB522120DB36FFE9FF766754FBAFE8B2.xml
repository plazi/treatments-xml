<collectionCode id="55EA08F3DB36FFEBFF7F65BBFF34E9F2" box="[158,238,2019,2043]" country="Sweden" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34994" lsid="urn:lsid:biocol.org:col:34994" name="Lund University" pageId="0" pageNumber="447">MZLU</collectionCode>
