<paragraph id="4843369B3B72FFE93A55FB78FCCDFB1C" blockId="3.[113,1088,1182,1269]" box="[113,833,1182,1205]" pageId="3" pageNumber="112">
<emphasis id="7A88EA893B72FFE93A55FB78FE89FB1C" bold="true" box="[113,261,1182,1205]" pageId="3" pageNumber="112">
<typeStatus id="974788393B72FFE93A55FB78FF25FB1C" box="[113,169,1182,1205]" pageId="3" pageNumber="112">Type</typeStatus>
