<number id="3DDA425EFFE3FFAB017CFD495516A1B7" box="[329,345,713,739]" pageId="7" pageNumber="8" value="3.5">3</number>
</figureCitation>
, 
<figureCitation id="13294CFCFFE3FFAB0152FD49553AA1B7" box="[359,373,713,739]" captionStart="FIGURE 5" captionStartId="14.[264,368,1244,1268]" captionTargetBox="[417,1170,320,1225]" captionTargetId="figure@14.[417,1171,320,1225]" captionTargetPageId="14" captionText="FIGURE 5. Posterior views of the exaggerated male furculae. A. T. ocampensis, B. T. ebanoverde; C, T. guajataca; D, T. cerropunta; E, T. cayey; F, T. luquillensis." httpUri="https://zenodo.org/record/156981/files/figure.png" pageId="7" pageNumber="8">5</figureCitation>
E, 
<number id="3DDA425EFFE3FFAB01A3FD4955E9A1B7" box="[406,422,713,739]" pageId="7" pageNumber="8" value="6.0">6</number>
B, 
<number id="3DDA425EFFE3FFAB01F2FD49564CA1B7" box="[455,515,713,739]" pageId="7" pageNumber="8" value="9.18">9, 18</number>
, 
<number id="3DDA425EFFE3FFAB0225FD495662A1B7" box="[528,557,713,739]" pageId="7" pageNumber="8" value="19.0">19</number>
