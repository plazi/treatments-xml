<typeStatus id="54946EC9FFA86F58FCC07C8EFC63FD2A" box="[812,905,677,701]" pageId="0" pageNumber="600">holotype</typeStatus>
of 
<taxonomicName id="4C2FABE8FFA86F58FC477C8EFB4CFD2A" authority="Kaur &amp; Garima &amp; Pandher, 2020" authorityName="Kaur &amp; Garima &amp; Pandher" authorityYear="2020" box="[939,1190,677,701]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="0" pageNumber="600" phylum="Arthropoda" rank="species" species="gangtokensis" status="new species">
