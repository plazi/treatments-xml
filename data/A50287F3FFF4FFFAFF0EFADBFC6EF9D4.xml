<taxonomicName id="EAAB4D66FFF4FFFAFF58FADBFCF3FAD0" authority="Wallengren, 1857" authorityName="Wallengren" authorityYear="1857" box="[193,790,1373,1398]" class="Insecta" family="Lycaenidae" genus="Pseudonacaduba" kingdom="Animalia" order="Lepidoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="subSpecies" species="sichela" subSpecies="sichela">
