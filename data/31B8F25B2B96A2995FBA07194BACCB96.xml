<taxonomicName id="E9AA26DF936EDBBD24A4CE0044BD8D05" LSID="http://zoobank.org/E98CD31C-4FC2-469C-995B-CB9AFBC9B0C5" authority="Sharkey" authorityName="Sharkey" authorityYear="2018" class="Insecta" family="Braconidae" genus="Zosteragathis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zosteragathis chaiyaphumensis" order="Hymenoptera" pageId="0" pageNumber="225" phylum="Arthropoda" rank="species" species="chaiyaphumensis" status="sp. n.">Zosteragathis chaiyaphumensis Sharkey</taxonomicName>
