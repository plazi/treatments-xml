<keyLead id="30919595FFFBFFDAC0ADFA8181EE9131" box="[151,1436,1362,1383]" pageId="20" pageNumber="21">
- 
<taxonomicName id="4C604D23FFFBFFDAC0DBFA81855E9131" box="[225,300,1362,1383]" class="Insecta" family="Eccoptarthridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="subFamily" subFamily="Carinae">Carinae</taxonomicName>
