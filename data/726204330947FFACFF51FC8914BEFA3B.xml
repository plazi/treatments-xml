<emphasis id="C8BF69370947FFACFF51FC891723FF36" bold="true" box="[151,546,151,178]" pageId="3" pageNumber="343">
<taxonomicName id="3DCBCEA60947FFACFF51FC8914BCFF35" authority="Feng &amp; Dietrich &amp; Zhang, 2018" authorityName="Feng &amp; Dietrich &amp; Zhang" authorityYear="2018" box="[151,445,151,177]" class="Insecta" family="Cicadellidae" genus="Sochinsogonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="343" phylum="Arthropoda" rank="species" species="radicalia" status="sp. nov.">
<emphasis id="C8BF69370947FFACFF51FC8914BCFF35" bold="true" box="[151,445,151,177]" italics="true" pageId="3" pageNumber="343">Sochinsogonia radicalia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="D38CD44C0947FFACFE02FC861723FF36" box="[452,546,152,178]" pageId="3" pageNumber="343" rank="species">sp. nov.</taxonomicNameLabel>
