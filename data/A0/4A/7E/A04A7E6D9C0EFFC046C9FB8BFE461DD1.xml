<taxonomicName box="[293,447,1028,1050]" class="Gastropoda" family="Plakobranchidae" genus="Elysia" kingdom="Animalia" order="Sacoglossa" pageId="66" pageNumber="67" phylum="Mollusca" rank="species" species="evelinae">
<emphasis box="[293,447,1028,1050]" italics="true" pageId="66" pageNumber="67">
Elysia 
<taxonomicName box="[363,447,1028,1050]" pageId="66" pageNumber="67" rank="species" species="evelinae">evelinae</taxonomicName>
</emphasis>
</taxonomicName>
