</figureCitation>
)................................................................................................. 
<number box="[1423,1435,1536,1558]" pageId="28" pageNumber="479" value="2.0">2</number>
</keyLead>
</paragraph>
<paragraph blockId="28.[151,1436,1507,1793]" pageId="28" pageNumber="479">
<keyLead pageId="28" pageNumber="479">
