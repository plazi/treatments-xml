<taxonomicName class="Eudicots" family="Aizoaceae" genus="Carpobrotus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Caryophyllales" pageId="1" pageNumber="96" phylum="Magnoliophyta" rank="genus">
<emphasis italics="true" pageId="1" pageNumber="96">Carpobrotus</emphasis>
</taxonomicName>
sp. and &quot;large pink mesem&quot;; 
<taxonomicName genus="Apiaceae" lsidName="" pageId="1" pageNumber="96" rank="genus">Apiaceae</taxonomicName>
(
<taxonomicName class="Magnoliopsida" family="Apiaceae" genus="Capnophyllum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Apiales" pageId="1" pageNumber="96" phylum="Tracheophyta" rank="species" species="africanum">
<emphasis italics="true" pageId="1" pageNumber="96">Capnophyllum africanum</emphasis>
</taxonomicName>
(L.) W. D. J. Koch) and 
<taxonomicName genus="Asteraceae" lsidName="" pageId="1" pageNumber="96" rank="genus">Asteraceae</taxonomicName>
(
<taxonomicName class="Dicotyledoneae" family="Asteraceae" genus="Senecio" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Campanulales" pageId="1" pageNumber="96" phylum="Angiospermae" rank="genus">
