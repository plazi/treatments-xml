<paragraph blockId="17.[121,1089,166,1672]" pageId="17" pageNumber="40">
<taxonomicName authorityName="Yamaguti" authorityYear="1952" box="[166,282,1610,1635]" class="Trematoda" family="Opecoelidae" genus="Macvicaria" kingdom="Animalia" order="Plagiorchiida" pageId="17" pageNumber="40" phylum="Platyhelminthes" rank="species" species="synagris">
