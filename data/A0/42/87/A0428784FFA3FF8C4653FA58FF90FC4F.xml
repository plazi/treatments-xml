(
<quantity box="[1042,1152,1019,1046]" metricMagnitude="-3" metricUnit="m" metricValue="1.71" pageId="3" pageNumber="4" unit="mm" value="1.71">1.71 mm</quantity>
, 
<quantity box="[1164,1273,1019,1046]" metricMagnitude="-3" metricUnit="m" metricValue="2.18" pageId="3" pageNumber="4" unit="mm" value="2.18">2.18 mm</quantity>
