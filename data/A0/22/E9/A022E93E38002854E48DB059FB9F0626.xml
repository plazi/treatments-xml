<emphasis bold="true" box="[151,350,1899,1923]" pageId="18" pageNumber="49">FIGURES 36–41.</emphasis>
<taxonomicName authority="Herting" authorityName="Herting" box="[356,724,1900,1923]" class="Insecta" family="Tachinidae" genus="Rossimyiops" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="49" phylum="Arthropoda" rank="species" species="subapertus">
<taxonomicName box="[356,614,1900,1923]" class="Insecta" family="Tachinidae" genus="Rossimyiops" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="49" phylum="Arthropoda" rank="species" species="subapertus">
