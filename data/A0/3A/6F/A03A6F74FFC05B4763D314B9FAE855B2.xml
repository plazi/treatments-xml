<emphasis bold="true" box="[773,838,1932,1957]" pageId="33" pageNumber="184">
<taxonomicNameLabel box="[773,838,1932,1957]" pageId="33" pageNumber="184" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
(
<number box="[856,884,1933,1957]" pageId="33" pageNumber="184" value="18.0">18</number>
% and 
<number box="[968,997,1933,1957]" pageId="33" pageNumber="184" value="13.0">13</number>
%), 
<taxonomicName box="[1044,1241,1933,1956]" class="Aves" family="Rallidae" genus="Rallus" kingdom="Animalia" order="Gruiformes" pageId="33" pageNumber="184" phylum="Chordata" rank="species" species="montivagorum" status="n. sp.">
<emphasis box="[1044,1241,1933,1956]" italics="true" pageId="33" pageNumber="184">R. montivagorum</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1251,1316,1932,1957]" pageId="33" pageNumber="184">
<taxonomicNameLabel box="[1251,1316,1932,1957]" pageId="33" pageNumber="184" rank="species">n. sp.</taxonomicNameLabel>
