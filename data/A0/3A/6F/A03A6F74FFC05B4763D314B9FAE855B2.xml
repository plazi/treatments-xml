<taxonomicNameLabel box="[947,1019,1896,1921]" pageId="33" pageNumber="184" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
(
<number box="[1044,1074,1897,1921]" pageId="33" pageNumber="184" value="22.0">22</number>
% and 
<number box="[1169,1198,1897,1921]" pageId="33" pageNumber="184" value="14.0">14</number>
%, respectively), 
<taxonomicName class="Aves" family="Rallidae" genus="Rallus" kingdom="Animalia" order="Gruiformes" pageId="33" pageNumber="184" phylum="Chordata" rank="species" species="adolfocaesaris" status="n. sp.">
<emphasis italics="true" pageId="33" pageNumber="184">R. adolfocaesaris</emphasis>
</taxonomicName>
<emphasis bold="true" box="[325,390,1932,1957]" pageId="33" pageNumber="184">
<taxonomicNameLabel box="[325,390,1932,1957]" pageId="33" pageNumber="184" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
(
<number box="[408,437,1933,1957]" pageId="33" pageNumber="184" value="23.0">23</number>
% and 
<number box="[520,550,1933,1957]" pageId="33" pageNumber="184" value="10.0">10</number>
%), 
<taxonomicName box="[596,764,1933,1956]" class="Aves" family="Rallidae" genus="Rallus" kingdom="Animalia" order="Gruiformes" pageId="33" pageNumber="184" phylum="Chordata" rank="species" species="carvaoensis" status="n. sp.">
<emphasis box="[596,764,1933,1956]" italics="true" pageId="33" pageNumber="184">R. carvaoensis</emphasis>
