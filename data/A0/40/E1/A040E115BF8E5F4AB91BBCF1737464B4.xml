<taxonomicName class="Dothideomycetes" family="Lamiaceae" genus="Tectona" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Tectona grandis" order="Lamiales" pageId="0" pageNumber="65" phylum="Ascomycota" rank="species" species="grandis">
