<taxonomicName authorityName="Jendek &amp; Nakládal" authorityYear="2019" box="[560,669,1950,1974]" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="289" phylum="Arthropoda" rank="species" species="vendibilis">
