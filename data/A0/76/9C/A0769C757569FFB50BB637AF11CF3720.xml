<quantity box="[848,927,889,914]" metricMagnitude="2" metricUnit="m" metricValue="8.54" pageId="16" pageNumber="17" unit="m" value="854.0">854 m</quantity>
; 
<date box="[943,1129,889,914]" pageId="16" pageNumber="17" value="2012-08-11">11 August 2012</date>
—Six individuals; trawl; - 38.0083 lat. -54.4178 long. (station 11); 
<quantity box="[616,707,926,950]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="16" pageNumber="17" unit="m" value="1200.0">1200 m</quantity>
