<paragraph blockId="1.[151,1436,1472,1607]" pageId="1" pageNumber="576">
<emphasis bold="true" box="[151,274,1472,1498]" pageId="1" pageNumber="576">Diagnosis.</emphasis>
<taxonomicName authorityName="H. Aspock &amp; U. Aspock" authorityYear="1968" box="[281,479,1472,1498]" class="Insecta" family="Raphidiidae" genus="Mongoloraphidia" kingdom="Animalia" order="Raphidioptera" pageId="1" pageNumber="576" phylum="Arthropoda" rank="genus">
<emphasis box="[281,479,1472,1498]" italics="true" pageId="1" pageNumber="576">Mongoloraphidia</emphasis>
</taxonomicName>
