<number id="3DE20D02C86EFFB3D633FF3EFBA4F1C7" box="[1040,1054,188,212]" pageId="14" pageNumber="15" value="4.0">4</number>
juveniles
</specimenCount>
(SB 
<date id="FF9439E5C86EFFB3D6EFFF3EFAF4F1C6" box="[1228,1358,188,213]" pageId="14" pageNumber="15" value="1251" valueMax="1254">
<number id="3DE20D02C86EFFB3D6EFFF3EFAF4F1C6" box="[1228,1358,188,213]" pageId="14" pageNumber="15" value="1252.5" valueMax="1254.0" valueMin="1251.0">1251–1254</number>
</date>
), 
<accessionNumber id="947982C6C86EFFB3D747FF3EFF5BF1EB" httpUri="http://www.uniprot.org/uniprot/SMF63850" pageId="14" pageNumber="15">
