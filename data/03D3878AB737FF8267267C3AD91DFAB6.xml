<taxonomicName id="4C7A4D1FB737FF8267F67E71DF51FCC5" authorityName="Leach" authorityYear="1815" box="[674,801,835,862]" class="Insecta" family="Dytiscidae" kingdom="Animalia" order="Coleoptera" pageId="102" pageNumber="103" phylum="Arthropoda" rank="family">Dytiscidae</taxonomicName>
) from 
<collectingCountry id="F36D760CB737FF8266207E71DFDAFCC6" box="[884,938,835,861]" name="Italy" pageId="102" pageNumber="103">Italy</collectingCountry>
