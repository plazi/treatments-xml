<taxonomicName id="4C704D72FFE1FF9C30B1FF203DF1FF50" baseAuthorityName="Wiedemann" baseAuthorityYear="1819" box="[491,695,151,172]" class="Insecta" family="Dolichopodidae" genus="Chrysosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="vittatum">
