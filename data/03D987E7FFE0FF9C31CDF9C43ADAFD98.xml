<collectingCountry id="F3677661FFE1FF9C32F1FF583CA7FEF8" box="[939,993,239,260]" name="India" pageId="17" pageNumber="18">India</collectingCountry>
] … 
<taxonomicName id="4C704D72FFE1FF9C3551FF583ADAFEF8" authority="(Wiedemann, 1819)" authorityName="Wiedemann" authorityYear="1819" baseAuthorityName="Wiedemann" baseAuthorityYear="1819" box="[1035,1436,239,260]" class="Insecta" family="Dolichopodidae" genus="Chrysosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="vittatum">
