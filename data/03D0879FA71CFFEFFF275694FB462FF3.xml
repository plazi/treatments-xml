<taxonomicName id="4C794D0AA71CFFEFFF7F5694FEEF2DE6" box="[207,314,151,177]" class="Insecta" family="Dolichopodidae" genus="Campsicnemus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="36" phylum="Arthropoda" rank="species" species="englundi">
