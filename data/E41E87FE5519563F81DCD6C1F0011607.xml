<quantity id="AB4F9B0D551956308420D236F1DB161B" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="23" pageNumber="24" unit="mm" value="3.7">3.70 mm</quantity>
, 
<emphasis id="5EC3EAFA55195630819BD20DF1C5161A" box="[208,219,1844,1867]" italics="true" pageId="23" pageNumber="24">s</emphasis>
= 0.24, 
<emphasis id="5EC3EAFA551956308073D20DF058161A" box="[312,326,1844,1867]" italics="true" pageId="23" pageNumber="24">n</emphasis>
= 6), width 
<quantity id="AB4F9B0D55195630809AD20AF32A161B" box="[465,564,1843,1867]" metricMagnitude="-3" metricUnit="m" metricValue="2.11" pageId="23" pageNumber="24" unit="mm" value="2.11">2.11 mm</quantity>
to 
<quantity id="AB4F9B0D55195630831CD20BF3A5161B" box="[599,699,1842,1867]" metricMagnitude="-3" metricUnit="m" metricValue="2.85" pageId="23" pageNumber="24" unit="mm" value="2.85">2.85 mm</quantity>
(
