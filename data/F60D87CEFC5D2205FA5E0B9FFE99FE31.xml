<paragraph id="7E1B36D8FC5D2202FA5E0B9FFDDFFD51" blockId="3.[272,594,736,792]" box="[272,594,736,760]" pageId="3" pageNumber="22">
<heading id="255381B4FC5D2202FA5E0B9FFDDFFD51" bold="true" box="[272,594,736,760]" centered="true" fontSize="10" level="2" pageId="3" pageNumber="22" reason="2">
<emphasis id="4CD0EACAFC5D2202FA5E0B9FFDDFFD51" bold="true" box="[272,594,736,760]" pageId="3" pageNumber="22">
<taxonomicName id="B9A44D5BFC5D2202FA5E0B9FFE77FD51" authority="Hong &amp; James, 2011" authorityName=", new" authorityYear="2011" box="[272,506,736,760]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="3" pageNumber="19" phylum="Annelida" rank="species" species="thaii" status="new">
<emphasis id="4CD0EACAFC5D2202FA5E0B9FFE49FD51" bold="true" box="[272,452,736,760]" italics="true" pageId="3" pageNumber="22">Pheretima thaii,</emphasis>
<taxonomicNameLabel id="57E357B1FC5D2202FA820B9FFE77FD51" box="[460,506,736,760]" pageId="3" pageNumber="22" rank="species">new</taxonomicNameLabel>
</taxonomicName>
