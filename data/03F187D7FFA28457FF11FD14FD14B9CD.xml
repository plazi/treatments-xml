<subSubSection id="C342654AFFA28454FF11FCFDFEB7BA6A" box="[151,366,860,882]" pageId="41" pageNumber="42" type="materials_examined">
<paragraph id="8BE736C1FFA28454FF11FCFDFEB7BA6A" blockId="41.[151,1375,798,882]" box="[151,366,860,882]" pageId="41" pageNumber="42">
