<typeStatus id="2C6E4384E11CFFC2FDECFDA6FD213A25" box="[629,682,548,571]" pageId="2" pageNumber="3">Type</typeStatus>
genus: 
<taxonomicName id="34D586A5E11CFFC2FD65FDA6FBAE3A25" authority="Berlese, 1910" authorityName="Berlese" authorityYear="1910" box="[764,1061,548,571]" class="Arachnida" family="Homocaligidae" genus="Homocaligus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
