<number id="451DEF01E11CFFC2FE64FDA6FDBD3A25" box="[509,566,548,571]" pageId="2" pageNumber="3" value="1969.0">1969</number>
</bibRefCitation>
: 
<number id="451DEF01E11CFFC2FDD9FDA6FDE03A25" box="[576,619,548,571]" pageId="2" pageNumber="3" value="712.0">712</number>
</treatmentCitation>
