<location id="8EA701F9986C7070C7BAFB10FCEAFB17" LSID="urn:lsid:plazi:treatment:03D1E634986C7070C5CDFBC4FBB9F870:8EA701F9986C7070C7BAFB10FCEAFB17" box="[767,824,1170,1191]" country="Venezuela" municipality="Type" name="Tovar" pageId="63" pageNumber="64" stateProvince="Merida">Tovar</location>
