<taxonomicName id="4C0D4D21406AFFB846D7BB97FE5D01BC" box="[396,458,1184,1208]" class="Insecta" family="Mantispidae" genus="Plega" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="4" pageNumber="279" phylum="Arthropoda" rank="genus">
