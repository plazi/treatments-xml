<quantity id="DA7C18CAFFBAFFABFE98E835FE33FEF9" box="[380,457,301,324]" metricMagnitude="-2" metricUnit="m" metricValue="3.3" pageId="48" pageNumber="279" unit="mm" value="33.0">33 mm</quantity>
