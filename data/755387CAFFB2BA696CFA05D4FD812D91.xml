<heading id="A60D81B0FFB2BA696CFA05D4FEFA2BAA" bold="true" box="[151,327,401,427]" fontSize="11" level="1" pageId="1" pageNumber="137" reason="1">
<emphasis id="CF8EEACEFFB2BA696CFA05D4FEFA2BAA" bold="true" box="[151,327,401,427]" pageId="1" pageNumber="137">
<taxonomicName id="3AFA4D5FFFB2BA696CFA05D4FF4B2BAA" box="[151,246,401,427]" class="Insecta" family="Pentatomidae" genus="Edessa" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="137" phylum="Arthropoda" rank="species" species="viridula">
<emphasis id="CF8EEACEFFB2BA696CFA05D4FF4B2BAA" bold="true" box="[151,246,401,427]" italics="true" pageId="1" pageNumber="137">viridula</emphasis>
</taxonomicName>
