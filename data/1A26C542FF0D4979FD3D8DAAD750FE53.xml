</taxonomicName>
(
<taxonomicName id="558F0FD7FF0D4979FCE88C33D7CCFE7E" box="[888,979,372,393]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="740" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
