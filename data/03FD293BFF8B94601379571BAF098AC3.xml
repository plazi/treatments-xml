<quantity id="4CAC35C8FF8B946710505788A9DE89F4" box="[545,653,399,425]" metricMagnitude="-3" metricUnit="m" metricValue="2.35" pageId="2" pageNumber="3" unit="mm" value="2.35">2.35 mm</quantity>
