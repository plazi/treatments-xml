<emphasis id="B91C1500FF94FFD1FF7CF893BE59F8CD" bold="true" box="[132,261,1852,1871]" pageId="4" pageNumber="349">FIGURES 6-7:</emphasis>
<taxonomicName id="4C68B291FF94FFD1FEF3F893BEEEF8CC" authorityName="Vanin" authorityYear="2008" box="[267,434,1852,1871]" class="Insecta" family="Curculionidae" genus="Pacholenus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="349" phylum="Arthropoda" rank="species" species="monteiroi" status="sp. nov.">
<emphasis id="B91C1500FF94FFD1FEF3F893BEEEF8CC" box="[267,434,1852,1871]" italics="true" pageId="4" pageNumber="349">Pacholenus monteiroi</emphasis>
</taxonomicName>
