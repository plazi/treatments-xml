<taxonomicName id="4C68B291FF94FFD1FC5EFBD7BBAEFB12" authority="Spreng" authorityName="Spreng" box="[934,1266,1144,1168]" class="Magnoliopsida" family="Myrtaceae" genus="Calyptranthes" higherTaxonomySource="GBIF" kingdom="Plantae" order="Myrtales" pageId="4" pageNumber="349" phylum="Tracheophyta" rank="species" species="brasiliensis">
