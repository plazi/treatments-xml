<taxonomicName id="4C654D71FFBA4365FD3FC2468B7EF8AD" box="[682,839,1828,1853]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="longispinus">
