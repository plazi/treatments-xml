<taxonomicName id="4C7F1887FFB93D73A8D5F975E157F919" authority="Chamberlin" authorityName="Chamberlin" authorityYear="1957" box="[407,633,1754,1776]" class="Diplopoda" family="Dalodesmidae" genus="Tsagonus" higherTaxonomySource="CoL" kingdom="Animalia" order="Polydesmida" pageId="16" pageNumber="523" phylum="Arthropoda" rank="genus">
