<taxonomicName id="76937CE1DF3D6366FD91FBD9FBA40F03" authority="(Boulenger, 1911)" baseAuthorityName="Boulenger" baseAuthorityYear="1911" box="[526,1055,1107,1132]" class="Actinopterygii" family="Claroteidae" genus="Parauchenoglanis" kingdom="Animalia" order="Siluriformes" pageId="24" pageNumber="798" phylum="Chordata" rank="species" species="ngamensis">
