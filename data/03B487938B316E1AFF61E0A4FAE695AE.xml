<taxonomicName id="4C1D4D068B316E1DFBCFE2DBFB3B928D" authorityName="Linnaeus" authorityYear="1753" box="[1081,1193,1982,2008]" class="Magnoliopsida" family="Solanaceae" genus="Capsicum" kingdom="Plantae" order="Solanales" pageId="3" pageNumber="530" phylum="Tracheophyta" rank="genus">
