<materialsCitation id="13873CEB9E74FFBCFFC97604FE6B1572" ID-GBIF-Occurrence="3059739125" collectionCode="AMNH" collectorName="Eichhorn" country="Iceland" county="Mussau Island" location="In" municipality="The" pageId="112" pageNumber="112" specimenCode="AMNH 722158-722164" specimenCount="1" typeStatus="paratype">
