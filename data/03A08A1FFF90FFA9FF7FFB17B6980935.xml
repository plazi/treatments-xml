<taxonomicName id="4C09408AFF8EFFA9FE28FB32B70B091F" box="[448,558,1278,1303]" class="Insecta" family="Chironomidae" genus="Tanypus" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="42" phylum="Arthropoda" rank="species" species="kraatzi">
