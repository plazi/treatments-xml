<taxonomicName id="4C09408AFF90FFB7FF7FFB17B78808FD" authority="Niitsuma (2001)" authorityName="Niitsuma" authorityYear="2001" box="[151,685,1243,1269]" class="Insecta" family="Chironomidae" genus="Tanypus" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="40" phylum="Arthropoda" rank="species" species="formosanus">
<emphasis id="B97DE71BFF90FFB7FF7FFB17B78808FD" bold="true" box="[151,685,1243,1269]" pageId="15" pageNumber="40">
<emphasis id="B97DE71BFF90FFB7FF7FFB17B4B608FD" bold="true" box="[151,403,1243,1269]" italics="true" pageId="15" pageNumber="40">Tanypus formosanus</emphasis>
sensu 
<bibRefCitation id="EF9846F8FF90FFB7FE0EFB17B78808FD" author="Niitsuma" box="[486,685,1243,1269]" pageId="15" pageNumber="40" refString="Niitsuma, H. (2001) The immature stages of Tanypus formosanus (Kieffer) (Diptera: Chironomidae). Species Diversity, 6, 65 - 72." type="journal article" year="2001">
Niitsuma (
<number id="3DC1292EFF90FFB7FD8FFB17B78008FD" box="[615,677,1243,1269]" pageId="15" pageNumber="40" value="2001.0">2001</number>
)
</bibRefCitation>
