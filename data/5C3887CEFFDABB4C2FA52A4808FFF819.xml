7. 
<taxonomicName id="13914D5BFFDABB4C2F812A480BA5F9DE" authority="(Norman, 1868)" baseAuthorityName="Norman" baseAuthorityYear="1868" box="[179,687,1591,1618]" class="Malacostraca" family="Dexaminidae" genus="Guernea" kingdom="Animalia" order="Amphipoda" pageId="10" pageNumber="383" phylum="Arthropoda" rank="species" species="coalita" subGenus="Guernea">
