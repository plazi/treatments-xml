<taxonomicName id="4C3B4D0E734AAB1843E2FC1DFE8AFC7D" authority="Patton, 1879" authorityName="Patton" authorityYear="1879" box="[160,286,973,998]" class="Insecta" family="Apidae" genus="Synhalonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="genus">
