<taxonomicName id="DB827887FFF0E224FE88D99B7561FC28" authority="Dejean" authorityName="Dejean" box="[374,588,922,948]" class="Insecta" family="Carabidae" genus="Colliuris" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="rufipes">
