, 
<location id="DE0660376003FFFD895661EA413C8C60" LSID="urn:lsid:plazi:treatment:537087FA6003FFFD8BCA6170401D88E0:DE0660376003FFFD895661EA413C8C60" box="[545,636,1513,1537]" county="Landkreis Markisch-Oderland" latitude="52.535" longLatPrecision="64" longitude="14.204" municipality="Muncheberg" name="Trebnitz" pageId="5" pageNumber="388" stateProvince="Brandenburg">Trebnitz</location>
, 
<geoCoordinate id="BEED502B6003FFFD89E661EA41B48C60" box="[657,756,1513,1537]" degrees="52.535" direction="north" orientation="latitude" pageId="5" pageNumber="388" precision="55" value="52.535">52.535°N</geoCoordinate>
