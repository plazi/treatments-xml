<taxonomicName id="4C39BBCFFFA9EB1BFC8F8C9B438CF940" authorityName="Thomas" authorityYear="1901" box="[809,923,1729,1750]" class="Mammalia" family="Vespertilionidae" genus="Laephotis" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="1127" phylum="Chordata" rank="genus">
