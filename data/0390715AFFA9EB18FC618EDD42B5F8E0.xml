<taxonomicName id="4C39BBCFFFA9EB1BFB678C9B4556F941" authorityName="Blumenbach" authorityYear="1779" box="[1217,1345,1729,1751]" class="Mammalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="1127" phylum="Chordata" rank="order">Chiroptera</taxonomicName>
report (
<bibRefCitation id="EFA8BDBDFFA9EB1BFC968CBA442CF960" author="AfricanBats NPC" box="[816,1083,1760,1782]" pageId="13" pageNumber="1127" refId="ref20682" refString="AfricanBats NPC. 2019. African Chiroptera report 2019. Pretoria: AfricanBats NPC. Available at: https: // africanbats. org / publication / african-chiroptera-report- 2019 /" type="url" year="2019">AfricanBats NPC, 2019</bibRefCitation>
