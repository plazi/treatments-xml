<quantity id="4CE547C20423FFB3FE74FB0EFCBB7028" box="[416,506,1270,1294]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="4" pageNumber="5" unit="mm" value="0.8">0.8 mm</quantity>
