, Fanjing Shan, 
<quantity id="4CC3779D4536572D27CEFD60FBEFFD00" box="[975,1082,727,748]" metricMagnitude="3" metricUnit="m" metricValue="1.1" metricValueMax="1.3" metricValueMin="0.9" pageId="40" pageNumber="745" unit="m" value="1100.0" valueMax="1300.0" valueMin="900.0">900-1300m</quantity>
, 
<geoCoordinate id="EE0FBCBF4536572D24DEFD58FEFBFCE8" box="[223,302,745,772]" degrees="27" direction="north" minutes="54" orientation="latitude" pageId="40" pageNumber="745" precision="925" value="27.9">27o54’N</geoCoordinate>
