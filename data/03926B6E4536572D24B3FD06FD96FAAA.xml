<date id="FF85FCB84536572D25EAFD59FDB9FCE8" box="[491,620,750,772]" pageId="40" pageNumber="745" value="2014-06-05" valueMax="2014-06-11" valueMin="2014-06-05">5.-11.VI.2014</date>
