<taxonomicName id="4C1504E39F73FFF2FD3EFF08FB8680EC" authority="DISTANT, 1908 IN THIS STUDY" authorityName="Distant" authorityYear="1908" box="[642,1098,214,236]" class="Insecta" family="Cicadellidae" genus="Bundera" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="386" phylum="Arthropoda" rank="genus">
