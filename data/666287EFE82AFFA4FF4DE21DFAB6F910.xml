and some 
<taxonomicName id="29CB4D7AE82BFFA4FB4CE6BEFA06FBC0" authority="Scudder, 1862" authorityName="Scudder" authorityYear="1862" box="[1174,1437,1012,1036]" class="Insecta" family="Tetrigidae" genus="Tettigidea" kingdom="Animalia" order="Orthoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="DCBFEAEBE82BFFA4FB4CE6BEFB61FBC0" box="[1174,1274,1012,1036]" italics="true" pageId="47" pageNumber="48">Tettigidea</emphasis>
