<quantity id="3C0D9B028A6CFF949E7CFEAE5F6E8822" box="[1309,1378,331,355]" metricMagnitude="-1" metricUnit="m" metricValue="2.2" pageId="58" pageNumber="59" unit="cm" value="22.0">22 cm</quantity>
