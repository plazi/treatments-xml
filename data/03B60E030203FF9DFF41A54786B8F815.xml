<taxonomicName id="4C1FC4960203FF9CFF41A5478756F84E" authorityName="Ricker" authorityYear="1952" box="[151,269,1903,1929]" class="Insecta" family="Nemouridae" genus="Podmosta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="0" pageNumber="293" phylum="Arthropoda" rank="genus">
