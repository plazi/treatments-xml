<subSubSection id="F4C8BBD3FFC24A31FF13F905CBE6F899" pageId="22" pageNumber="473" type="etymology">
<paragraph id="BC6DE858FFC24A31FF13F905CBE6F899" blockId="22.[151,1437,1554,2011]" pageId="22" pageNumber="473">
<emphasis id="8EA6344AFFC24A31FF13F905C91CF944" bold="true" box="[199,334,1735,1758]" pageId="22" pageNumber="473">Etymology.</emphasis>
