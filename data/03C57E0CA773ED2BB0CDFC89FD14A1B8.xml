<quantity id="4C9462FFA773ED2BB392FC1EFBBEA6D0" box="[968,1032,961,986]" metricMagnitude="2" metricUnit="m" metricValue="5.3" pageId="15" pageNumber="16" unit="m" value="530.0">530m</quantity>
