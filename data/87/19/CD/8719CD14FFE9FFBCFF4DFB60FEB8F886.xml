</taxonomicName>
, 
<taxonomicName authority="Beklemischev, 1951" authorityName="Beklemischev" authorityYear="1951" class="Rhabditophora" family="Macrostomidae" genus="Macrostomum" kingdom="Animalia" order="Macrostomida" pageId="7" pageNumber="127" phylum="Platyhelminthes" rank="species" species="quiritium">
