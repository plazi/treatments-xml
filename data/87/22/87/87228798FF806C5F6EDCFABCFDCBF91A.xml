<paragraph blockId="10.[192,1440,1582,1894]" box="[240,905,1678,1702]" pageId="10" pageNumber="9">
<emphasis bold="true" box="[240,311,1678,1702]" pageId="10" pageNumber="9">Body</emphasis>
elongate, parallel-sided, convex; length, 
<quantity box="[798,901,1678,1702]" metricMagnitude="-3" metricUnit="m" metricValue="2.12" pageId="10" pageNumber="9" unit="mm" value="2.12">2.12mm</quantity>
