</taxonomicName>
; and 
<taxonomicName authority="Suwa and Mawatari, 1998" authorityName="Suwa and Mawatari" authorityYear="1998" box="[125,601,232,256]" class="Gymnolaemata" family="Microporellidae" genus="Microporella" kingdom="Animalia" order="Cheilostomatida" pageId="89" pageNumber="1134" phylum="Bryozoa" rank="species" species="trigonellata">
<emphasis box="[125,286,232,256]" italics="true" pageId="89" pageNumber="1134">M. trigonellata</emphasis>
<bibRefCitation author="Suwa T &amp; Mawatari SF" box="[294,601,232,256]" pageId="89" pageNumber="1134" pagination="895 - 922" refId="ref57903" refString="Suwa T, Mawatari SF. 1998. Revision of seven species of Microporella (Bryozoa, Cheilostomatida) from Hokkaido, Japan, using new taxonomic characters. Journal of Natural History 32: 895 - 922." type="journal article" year="1998">Suwa and Mawatari, 1998</bibRefCitation>
</taxonomicName>
