<taxonomicName genus="Mesembryanthema" lsidName="" pageId="17" pageNumber="18" rank="genus">Mesembryanthema</taxonomicName>
(
<taxonomicName class="Magnoliopsida" family="Aizoaceae" genus="Prenia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Caryophyllales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="pallens">Prenia pallens</taxonomicName>
