<taxonomicName authority="Yamaki" authorityName="Yamaki" box="[373,844,822,846]" class="Magnoliopsida" family="Aristolochiaceae" genus="Asiasarum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Piperales" pageId="3" pageNumber="298" phylum="Tracheophyta" rank="variety" species="sieboldii" variety="versicolor">
