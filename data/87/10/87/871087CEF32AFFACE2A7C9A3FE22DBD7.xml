<taxonomicName authority="C. S. Yook &amp; J. G. Kim" authorityName="C. S. Yook &amp; J. G. Kim" authorityYear="1996" class="Magnoliopsida" family="Aristolochiaceae" form="chungbuensis" genus="Asiasarum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Piperales" pageId="3" pageNumber="298" phylum="Tracheophyta" rank="form" species="sieboldii">
