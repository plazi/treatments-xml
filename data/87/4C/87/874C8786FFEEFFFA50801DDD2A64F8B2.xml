(
<bibRefCitation author="Collingwood, C. A." box="[359,565,1818,1840]" journalOrPublisher="Fauna of Saudi Arabia" pageId="15" pageNumber="548" pagination="230 - 302" part="7" refId="ref13290" refString="Collingwood, C. A. 1985. Hymenoptera: Fam. Formicidae of Saudi Arabia. Fauna of Saudi Arabia, 7: 230 - 302." title="Hymenoptera: Fam. Formicidae of Saudi Arabia" type="journal article" year="1985">Collingwood 1985</bibRefCitation>
