<collectingCountry box="[529,583,1787,1808]" name="Saudi Arabia" pageId="15" pageNumber="548">KSA</collectingCountry>
, 
<collectingCountry box="[597,682,1787,1809]" name="Kuwait" pageId="15" pageNumber="548">Kuwait</collectingCountry>
, 
<collectingCountry box="[697,767,1787,1808]" name="Oman" pageId="15" pageNumber="548">Oman</collectingCountry>
