<subSubSection pageId="7" pageNumber="540" type="distribution">
<paragraph blockId="7.[144,772,180,911]" pageId="7" pageNumber="540">
<emphasis bold="true" box="[181,499,426,448]" pageId="7" pageNumber="540">
<emphasis bold="true" box="[181,492,426,448]" italics="true" pageId="7" pageNumber="540">Geographic distribution</emphasis>
.
</emphasis>
