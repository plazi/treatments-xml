<collectingCountry box="[519,604,519,541]" name="Kuwait" pageId="7" pageNumber="540">Kuwait</collectingCountry>
, 
<collectingCountry box="[626,697,519,540]" name="Oman" pageId="7" pageNumber="540">Oman</collectingCountry>
, 
<collectingCountry box="[717,771,519,540]" name="United Arab Emirates" pageId="7" pageNumber="540">UAE</collectingCountry>
(
<bibRefCitation author="Collingwood, C. A. &amp; Agosti, D." box="[151,505,550,572]" journalOrPublisher="Fauna of Arabia" pageId="7" pageNumber="540" pagination="300 - 385" part="15" refId="ref13318" refString="Collingwood, C. A. and Agosti, D. 1996. Formicidae (Insecta: Hymenoptera) of Saudi Arabia (Part 2). Fauna of Arabia, 15: 300 - 385." title="Formicidae (Insecta: Hymenoptera) of Saudi Arabia (Part 2)" type="journal article" year="1996">Collingwood and Agosti 1996</bibRefCitation>
