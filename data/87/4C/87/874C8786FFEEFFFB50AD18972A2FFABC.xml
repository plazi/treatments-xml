<collectingCountry box="[391,495,1049,1071]" name="Palestine" pageId="14" pageNumber="547">Palestine</collectingCountry>
and is known from Levant: 
<collectingCountry box="[199,267,1080,1101]" name="Egypt" pageId="14" pageNumber="547">Egypt</collectingCountry>
, 
<collectingCountry box="[283,389,1079,1101]" name="Palestine" pageId="14" pageNumber="547">Palestine</collectingCountry>
, 
<collectingCountry box="[405,472,1079,1101]" name="Israel" pageId="14" pageNumber="547">Israel</collectingCountry>
, 
<collectingCountry box="[488,569,1079,1101]" name="Jordan" pageId="14" pageNumber="547">Jordan</collectingCountry>
, 
<collectingCountry box="[585,645,1079,1101]" name="Syria" pageId="14" pageNumber="547">Syria</collectingCountry>
(
<bibRefCitation author="Borowiec L &amp; S. Salata" journalOrPublisher="Annals of the Upper Silesian Museum in Bytom, Entomology" pageId="14" pageNumber="547" pagination="1 - 26" part="29" refId="ref13011" refString="Borowiec L, S. Salata. 2020. Review of ants (Hymenoptera: Formicidae) from Jordan. Annals of the Upper Silesian Museum in Bytom, Entomology, 29: 1 - 26." title="Review of ants (Hymenoptera: Formicidae) from Jordan" type="journal article" year="2020">Borowiec and Salata 2020</bibRefCitation>
); Arabian Peninsula: 
<collectingCountry box="[612,665,1110,1131]" name="Saudi Arabia" pageId="14" pageNumber="547">KSA</collectingCountry>
, 
<collectingCountry box="[684,767,1110,1132]" name="Kuwait" pageId="14" pageNumber="547">Kuwait</collectingCountry>
, 
<collectingCountry box="[144,214,1141,1162]" name="Oman" pageId="14" pageNumber="547">Oman</collectingCountry>
, 
<collectingCountry box="[234,312,1141,1162]" name="Yemen" pageId="14" pageNumber="547">Yemen</collectingCountry>
