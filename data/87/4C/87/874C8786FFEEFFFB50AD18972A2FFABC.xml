<subSubSection pageId="14" pageNumber="547" type="distribution">
<paragraph blockId="14.[144,771,403,1378]" pageId="14" pageNumber="547">
<emphasis bold="true" box="[181,476,1017,1039]" pageId="14" pageNumber="547">
<emphasis bold="true" box="[181,469,1017,1039]" italics="true" pageId="14" pageNumber="547">Geographic distribution</emphasis>
.
</emphasis>
