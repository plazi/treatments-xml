<subSubSection pageId="null" pageNumber="559" type="nomenclature">
<paragraph pageId="null" pageNumber="559">
<taxonomicName authority="L." class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="559" phylum="Tracheophyta" rank="species" species="alopecuroides">
Astragalus 
<normalizedToken originalValue="alopecuroídes" pageId="null" pageNumber="559">alopecuroides</normalizedToken>
<authorityName pageId="null" pageNumber="559">L.</authorityName>
</taxonomicName>
</paragraph>
