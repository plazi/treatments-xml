spp., ♀, holotypes, habitus: 1— 
<taxonomicName box="[748,891,1250,1272]" class="Insecta" family="Ichneumonidae" genus="Diaparsis" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="538" phylum="Arthropoda" rank="species" species="kanyawara" status="sp. nov.">
<emphasis box="[748,891,1250,1272]" italics="true" pageId="2" pageNumber="538">D. kanyawara</emphasis>
</taxonomicName>
<emphasis bold="true" box="[897,977,1250,1271]" pageId="2" pageNumber="538">
<taxonomicNameLabel box="[897,977,1250,1271]" pageId="2" pageNumber="538" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; 2— 
<taxonomicName box="[1030,1145,1251,1272]" class="Insecta" family="Ichneumonidae" genus="Diaparsis" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="538" phylum="Arthropoda" rank="species" species="sinuator" status="sp. nov.">
<emphasis box="[1030,1145,1251,1272]" italics="true" pageId="2" pageNumber="538">D. sinuator</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1151,1232,1250,1271]" pageId="2" pageNumber="538">
<taxonomicNameLabel box="[1151,1232,1250,1271]" pageId="2" pageNumber="538" rank="species">sp. nov.</taxonomicNameLabel>
