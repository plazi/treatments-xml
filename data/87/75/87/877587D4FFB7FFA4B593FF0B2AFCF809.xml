(
<emphasis bold="true" box="[284,399,152,178]" italics="true" pageId="2" pageNumber="538">Diaparsis</emphasis>
) 
<emphasis bold="true" box="[416,549,152,178]" italics="true" pageId="2" pageNumber="538">kanyawara</emphasis>
Khalaim &amp; Sääksjärvi
</taxonomicName>
, 
<taxonomicNameLabel box="[853,947,152,178]" pageId="2" pageNumber="538" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
