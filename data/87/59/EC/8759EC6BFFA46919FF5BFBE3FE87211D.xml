<taxonomicName authorityName="Cameron" authorityYear="1932" box="[580,721,1197,1223]" class="Insecta" family="Staphylinidae" genus="Barypalpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="581" phylum="Arthropoda" rank="species" species="ruficornis">
