<subSubSection pageId="3" pageNumber="543" type="nomenclature">
<paragraph blockId="3.[151,480,151,178]" box="[151,480,151,178]" pageId="3" pageNumber="543">
<heading bold="true" box="[151,480,151,178]" fontSize="11" level="1" pageId="3" pageNumber="543" reason="1">
<emphasis bold="true" box="[151,480,151,178]" pageId="3" pageNumber="543">
The 
<taxonomicName box="[207,305,151,177]" class="Insecta" family="Carabidae" genus="Orthogonius" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="543" phylum="Arthropoda" rank="species" species="lateralis">
