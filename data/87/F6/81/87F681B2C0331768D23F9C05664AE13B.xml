<taxonomicName authority="Motschulsky, 1857" authorityName="Motschulsky" authorityYear="1857" class="Insecta" family="Carabidae" genus="Trigonognatha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trigonognatha" order="Coleoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="genus">Trigonognatha</taxonomicName>
