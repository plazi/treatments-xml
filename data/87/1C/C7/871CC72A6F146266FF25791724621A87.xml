), other setae shorter (
<emphasis box="[244,258,192,215]" italics="true" pageId="22" pageNumber="553">p</emphasis>
<emphasis box="[258,266,206,219]" italics="true" pageId="22" pageNumber="553">
<number box="[258,266,206,219]" pageId="22" pageNumber="553" value="2.0">2</number>
</emphasis>
= 
<number box="[295,325,191,215]" pageId="22" pageNumber="553" value="65.0">65</number>
, 
<emphasis box="[337,351,192,215]" italics="true" pageId="22" pageNumber="553">p</emphasis>
<emphasis box="[350,358,206,219]" italics="true" pageId="22" pageNumber="553">
