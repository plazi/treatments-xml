<number id="2E4224ACFFD2CB67A0CC44CFFC0966A0" box="[928,965,1450,1474]" pageId="13" pageNumber="414" value="2.7">2.7</number>
, 
<number id="2E4224ACFFD2CB67A0BD44CCFC3B66A0" box="[977,1015,1449,1474]" pageId="13" pageNumber="414" value="5.4">5.4</number>
, 
<number id="2E4224ACFFD2CB67A76F44CCFBE566A0" box="[1027,1065,1449,1474]" pageId="13" pageNumber="414" value="5.6">5.6</number>
