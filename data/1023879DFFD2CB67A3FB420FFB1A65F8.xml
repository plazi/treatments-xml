<number id="2E4224ACFFD2CB67A16444CCFDE266A0" box="[520,558,1449,1474]" pageId="13" pageNumber="414" value="5.8">5.8</number>
, 
<number id="2E4224ACFFD2CB67A15644CFFD9366A0" box="[570,607,1450,1474]" pageId="13" pageNumber="414" value="2.8">2.8</number>
, 
<number id="2E4224ACFFD2CB67A10044CFFD5E66A0" box="[620,658,1450,1474]" pageId="13" pageNumber="414" value="4.9">4.9</number>
