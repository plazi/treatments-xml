<taxonomicName id="32814D118004FA61FD3C59B02770FD16" box="[681,894,691,717]" class="Insecta" family="Pyralidae" genus="Epischnia" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="363" phylum="Arthropoda" rank="species" species="rutilella">
