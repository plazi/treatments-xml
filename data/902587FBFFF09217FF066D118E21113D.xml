, C) 
<taxonomicName id="DF8C4D6EFFF39216FD9E6E4E8EDE1772" baseAuthorityName="Blatchley" baseAuthorityYear="1910" box="[555,654,612,630]" class="Insecta" family="Mordellidae" genus="Mordellina" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="701" phylum="Arthropoda" rank="species" species="testacea">
<emphasis id="2AF8EAFFFFF39216FD9E6E4E8EDE1772" box="[555,654,612,630]" italics="true" pageId="2" pageNumber="701">M. testacea</emphasis>
</taxonomicName>
, D) 
<taxonomicName id="DF8C4D6EFFF39216FF7F6E568D22178A" authority="Steury &amp; Steiner, 2020" authorityName="Steury &amp; Steiner" authorityYear="2020" box="[202,370,635,654]" class="Insecta" family="Mordellidae" genus="Mordellina" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="701" phylum="Arthropoda" rank="species" species="washingtonensis" status="new species">
<emphasis id="2AF8EAFFFFF39216FF7F6E568D22178A" box="[202,370,635,654]" italics="true" pageId="2" pageNumber="701">M. washingtonensis</emphasis>
