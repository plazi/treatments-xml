<geoCoordinate id="26A15046C3268739FC821C3EFC16FAA3" box="[883,949,1308,1332]" degrees="0.093" direction="north" orientation="latitude" pageId="28" pageNumber="3571" precision="55" value="0.093">0.093</geoCoordinate>
