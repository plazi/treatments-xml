<materialsCitation id="F3FD3CDCC3268739FF7B1D9FFEB0FA23" collectionCode="HW, PW, MW" latitude="0.093" longLatPrecision="78" longitude="-0.103" pageId="28" pageNumber="3571" specimenCode="HW 2.11, 2.29" specimenCount="1">
