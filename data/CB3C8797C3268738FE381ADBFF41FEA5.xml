<taxonomicName id="84954D02C3268739FEF41D9FFEDFFB42" authorityName="Ansari" authorityYear="1955" box="[261,380,1213,1237]" class="Insecta" family="Philopteridae" genus="Columbicola" kingdom="Animalia" order="Psocodea" pageId="28" pageNumber="3571" phylum="Arthropoda" rank="species" species="theresae">
