</taxonomicName>
- 
<location id="052E606FFFE58A03FD29FB41FB9BDC64" LSID="urn:lsid:plazi:treatment:885887A2FFE68A03F84FEFCAF52ADA38:052E606FFFE58A03FD29FB41FB9BDC64" box="[741,1054,1248,1287]" municipality="Horsfield" name="Malay Peninsula" pageId="42" pageNumber="321" stateProvince="Sumatera Utara">
<emphasis id="3285EAA6FFE58A03FD29FB41FCD3DC64" box="[741,854,1248,1287]" italics="true" pageId="42" pageNumber="321">Malay</emphasis>
Peninsula
</location>
(including Langkawi and Tioman Is), 
<collectingRegion id="C235F856FFE58A03FF64FAB7FECFDC5E" box="[168,330,1302,1341]" country="Indonesia" name="Sumatera Utara" pageId="42" pageNumber="321">Sumatra</collectingRegion>
