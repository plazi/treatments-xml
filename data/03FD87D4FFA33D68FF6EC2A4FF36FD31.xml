<number id="3D9C24E5FFA33D69FCD9C1BAF961F8F8" box="[800,852,1771,1796]" pageId="20" pageNumber="521" value="2.65">2.65</number>
, width 
<number id="3D9C24E5FFA33D69FC4FC1BAF9DFF8F8" box="[950,1002,1771,1796]" pageId="20" pageNumber="521" value="1.55">1.55</number>
. Abdomen length 
<number id="3D9C24E5FFA33D69FB37C1BAFF37F8FF" box="[1230,1282,1771,1795]" pageId="20" pageNumber="521" value="2.7">2.70</number>
, width 
<number id="3D9C24E5FFA33D69FA9DC1BAFFADF8F8" box="[1380,1432,1771,1796]" pageId="20" pageNumber="521" value="1.45">1.45</number>
. Chelicera length 
<number id="3D9C24E5FFA33D69FEAFC05EFBBFF8DB" box="[342,394,1807,1831]" pageId="20" pageNumber="521" value="1.9">1.90</number>
