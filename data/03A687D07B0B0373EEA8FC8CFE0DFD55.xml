<quantity id="4CF79B237B0B0375EC51FBD0FD00FB69" box="[622,710,1148,1172]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="115" pageNumber="116" unit="mm" value="1.9">1.9 mm</quantity>
, width 
