<taxonomicName id="E732B2824F58B2468ED21AF1C4A12ECF" class="Insecta" family="Cicadellidae" genus="Sweta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sweta hallucinata" order="Hemiptera" pageId="0" pageNumber="35" phylum="Arthropoda" rank="species" species="hallucinata">Sweta hallucinata</taxonomicName>
