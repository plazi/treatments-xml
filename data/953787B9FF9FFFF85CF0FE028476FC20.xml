<materialsCitation id="ADF63CF2FF9FFFF85CA0FCA18702FCE8" ID-GBIF-Occurrence="1316556634" box="[199,633,823,848]" location="California" pageId="16" pageNumber="117" specimenCount="1" stateProvince="California">
