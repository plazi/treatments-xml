<emphasis id="FBB0B219AB415603FDD4968C935F089F" box="[528,610,1442,1461]" italics="true" pageId="10" pageNumber="15">R. euopta</emphasis>
</taxonomicName>
; 29, 34, 39, 
<taxonomicName id="0EC41588AB415603FD16968C9202089F" baseAuthorityName="Fabricius" baseAuthorityYear="1798" box="[722,831,1442,1461]" class="Insecta" family="Pentatomidae" genus="Runibia" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="15" phylum="Arthropoda" rank="species" species="perspicua">
