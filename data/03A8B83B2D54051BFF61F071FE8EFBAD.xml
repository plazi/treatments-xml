<taxonomicName id="4C0172AE2D54051BFB12F0E4FAFFFC01" box="[1252,1361,904,928]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="10" pageNumber="79" phylum="Tracheophyta" rank="genus">
