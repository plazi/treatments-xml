<taxonomicName id="4C1363E56A49F56DFE1FFE71887BFDA1" authority="Błażewicz-Paszkowycz, 2007" authorityName="Błażewicz-Paszkowycz" authorityYear="2007" box="[398,539,498,523]" class="Malacostraca" family="Typhlotanaidae" genus="Typhlotanais" kingdom="Animalia" order="Tanaidacea" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="andeepae" status="n. sp.">
