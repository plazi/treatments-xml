</taxonomicName>
(
<taxonomicName id="4C124D40834AAD11FE09E794FDF7FF31" box="[506,590,216,238]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="720" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
