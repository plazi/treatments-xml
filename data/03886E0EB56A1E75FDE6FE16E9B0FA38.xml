<taxonomicName id="4C21A49BB56A1E75FBF7FC6EE993FDCE" authorityName="Simone &amp; Casati" authorityYear="2013" box="[1038,1232,931,957]" class="Gastropoda" family="Orthalicidae" genus="Rhinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="24" pageNumber="25" phylum="Mollusca" rank="species" species="gilbertus">
