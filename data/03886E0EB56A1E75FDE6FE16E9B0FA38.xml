<taxonomicName id="4C21A49BB56A1E75FC71FC05E94CFD92" authorityName="Simone &amp; Casati" authorityYear="2013" box="[904,1039,968,994]" class="Gastropoda" family="Orthalicidae" genus="Cyclodontina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="24" pageNumber="25" phylum="Mollusca" rank="species" species="capivara">
