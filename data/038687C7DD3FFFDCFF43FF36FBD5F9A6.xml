of 
<collectingRegion id="49EBF833DD3FFFDCFDC5FE02FD72FE67" box="[626,677,457,478]" country="Poland" name="Lodzkie" pageId="0" pageNumber="734">Lodz</collectingRegion>
, 
<collectingCounty id="62F14E5DDD3FFFDCFD06FE02FCD4FE67" box="[689,771,457,478]" pageId="0" pageNumber="734">Banacha</collectingCounty>
12/16 90-237 
<collectingRegion id="49EBF833DD3FFFDCFDDBFE28FD4BFE40" box="[620,668,483,505]" country="Poland" name="Lodzkie" pageId="0" pageNumber="734">Łódź</collectingRegion>
, 
<collectingCountry id="F3387641DD3FFFDCFD1DFE2FFCDDFE40" box="[682,778,484,505]" name="Poland" pageId="0" pageNumber="734">POLAND</collectingCountry>
