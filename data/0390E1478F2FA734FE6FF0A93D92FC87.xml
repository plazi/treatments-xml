<quantity id="4CC1FDB48F2FA735FDA8F1A43EC6FE1C" box="[512,640,465,488]" metricMagnitude="-3" metricUnit="m" metricValue="4.45" metricValueMax="4.5" metricValueMin="4.4" pageId="64" pageNumber="61" unit="mm" value="4.45" valueMax="4.5" valueMin="4.4">4.4–4.5 mm</quantity>
