<taxonomicName id="2ACF6B60738861FB2C8C271B27C82793" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia" order="Zingiberales" pageId="153" pageNumber="154" phylum="Tracheophyta" rank="genus">Heliconia</taxonomicName>
sp. (
<taxonomicName id="1CB58CFAE6A19E3CAF400C043BAF8A7D" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="153" pageNumber="154" rank="genus">Heliconiaceae</taxonomicName>
), 
<taxonomicName id="D6C32ACB1888C8FCE81760D8953051FA" class="Liliopsida" family="Marantaceae" genus="Calathea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Calathea insignis" order="Zingiberales" pageId="153" pageNumber="154" phylum="Tracheophyta" rank="species" species="insignis">Calathea insignis</taxonomicName>
Hort. and Bull. (
<bibRefCitation id="49F53AE58D74A167455CB4C2F2B801FF" pageId="153" pageNumber="154">Staines 1996</bibRefCitation>
); 
<taxonomicName id="9AC675046CCACF3B37783FCFCFAFBF00" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia crotalifera" order="Coleoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="crotalifera">Cephaloleia crotalifera</taxonomicName>
S. Watson, 
<taxonomicName id="C577875AFF96A94ED7EF7A0717521165" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia lutea" order="Coleoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="lutea">Cephaloleia lutea</taxonomicName>
G. Mey. (
<taxonomicName id="250C4F2B0906E923959BDCD47BFDA487" genus="Marantaceae" lsidName="Marantaceae" pageId="153" pageNumber="154" rank="genus">Marantaceae</taxonomicName>
) (
<bibRefCitation id="F1F9B23D5DC2B89E9B130BB677ED0E4E" author="Garcia-Robledo, C" journalOrPublisher="PLoS ONE" pageId="287" pageNumber="288" title="Tropical plant-herbivore networks: Reconstructing species interactions using DNA barcodes." url="10.1371/journal.pone.0052967" volume="8" year="2013 a">
<normalizedToken id="9CC16C8FE90BA7161233ED41DB0FFDCC" originalValue="García–Robledo">Garcia-Robledo</normalizedToken>
et al. 2013a
</bibRefCitation>
).
</paragraph>
</subSubSection>
