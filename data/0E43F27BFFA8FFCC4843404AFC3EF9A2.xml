<taxonomicName id="41EA38EEFFA8FFCC48B640D0FB72FA36" baseAuthorityName="Erxleben" baseAuthorityYear="1777" box="[984,1204,1493,1517]" class="Mammalia" family="Cervidae" genus="Mazama" kingdom="Animalia" order="Artiodactyla" pageId="4" pageNumber="213" phylum="Chordata" rank="species" species="americana">
