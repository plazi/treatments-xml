<date id="6621106A5D225601FD2E8CDAFCF6B7BC" box="[719,868,2004,2028]" pageId="5" pageNumber="118" value="1989-08-12">
<number id="A457248D5D225601FD2E8CDAFD79B7BC" box="[719,747,2004,2028]" pageId="5" pageNumber="118" value="12.0">12</number>
