<collectingCountry id="A3A6C1C1FF7664531968FAF2C667FC76" box="[822,890,1365,1389]" name="Austria" pageId="9" pageNumber="40">Austria</collectingCountry>
(
<bibRefCitation id="BF20FCA0FF76645319DAFAF2C6E2FC76" author="KIEFFER V. I." box="[900,1023,1365,1389]" pageId="9" pageNumber="40" pagination="257 - 267" refId="ref10495" refString="KIEFFER V. I. 1898. - About a new and previously known cynipides. Wiener Entomologische Zeitung 17: 257 - 267 (in German). Kieffer J. J. 1901. Revision of Onychiinae. Bulletin de la Societe Entomologique de France 7: 161 (in French)." type="journal article" year="1898">Kieffer 1898</bibRefCitation>
); 
<collectingCountry id="A3A6C1C1FF7664531E51FAF2C141FC77" box="[1039,1116,1365,1388]" name="Norway" pageId="9" pageNumber="40">Norway</collectingCountry>
(
<bibRefCitation id="BF20FCA0FF7664531E39FAF2C1EFFC76" author="HEDICKE V. I." box="[1127,1266,1365,1389]" pageId="9" pageNumber="40" pagination="74 - 76" refId="ref10467" refString="HEDICKE V. I. 1930. - Contributions to the knowledge of cynipides. Deutsche Entomologische Zeitschrift 85: 74 - 76 (in German)." type="journal article" year="1930">Hedicke 1930</bibRefCitation>
), 
<collectingCountry id="A3A6C1C1FF7664531F5FFAF2C06BFC76" box="[1281,1398,1365,1389]" name="Netherlands" pageId="9" pageNumber="40">Netherlands</collectingCountry>
