<taxonomicName id="19F34D1E930E6227FB51F8D4FAFCF8FB" authority="Geoffroy, 1762" authorityName="Geoffroy" authorityYear="1762" box="[1268,1397,1880,1903]" class="Insecta" family="Stratiomyidae" genus="Stratiomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
