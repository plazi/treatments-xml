<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:132459" class="Insecta" family="Formicidae" genus="Lasius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lasius interjectus Mayr" lsidName-HNS="Lasius interjectus Mayr" order="Hymenoptera" pageNumber="888" phylum="Arthropoda" rank="species" species="interjectus">L. interjectus</taxonomicName>
