<collectingRegion box="[848,1020,809,831]" isEnumeration="true" pageId="1" pageNumber="569">Alaska, Michigan</collectingRegion>
, &amp; 
<collectingRegion box="[1055,1128,809,831]" country="Canada" name="Ontario" pageId="1" pageNumber="569">Ontario</collectingRegion>
.......... 
<taxonomicName authority="(Fisher)" authorityName="Fisher" baseAuthorityName="Fisher" baseAuthorityYear="1938" box="[1254,1434,809,831]" class="Insecta" family="Keroplatidae" genus="Rocetelion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="569" phylum="Arthropoda" rank="species" species="fenestrale">
<emphasis box="[1254,1349,810,831]" italics="true" pageId="1" pageNumber="569">fenestrale</emphasis>
