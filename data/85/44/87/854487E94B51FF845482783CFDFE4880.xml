<geoCoordinate box="[460,603,188,213]" direction="east" orientation="longitude" pageId="3" pageNumber="514" precision="1" value="60.88639">60.88639° E</geoCoordinate>
, 
<collectingDate box="[615,737,188,213]" pageId="3" pageNumber="514" value="2017-06-23">
<date box="[615,737,188,213]" pageId="3" pageNumber="514" value="2017-06-23">23.vi.2017</date>
</collectingDate>
, 
<specimenCount box="[750,888,188,213]" pageId="3" pageNumber="514" type="generic">2 specimens</specimenCount>
[
<collectionCode box="[902,991,189,212]" pageId="3" pageNumber="514">RMBH</collectionCode>
: voucher nos. biv279_1 and biv279_2], 
