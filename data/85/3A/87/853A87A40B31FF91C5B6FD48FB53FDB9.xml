<paragraph blockId="28.[151,439,674,700]" box="[151,439,674,700]" pageId="28" pageNumber="29">
<heading bold="true" box="[151,439,674,700]" fontSize="11" level="1" pageId="28" pageNumber="29" reason="1">
<emphasis bold="true" box="[151,439,674,700]" pageId="28" pageNumber="29">
<taxonomicName box="[151,350,674,700]" class="Insecta" family="Tettigoniidae" genus="Pandanagraecia" kingdom="Animalia" order="Orthoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[151,350,674,700]" italics="true" pageId="28" pageNumber="29">Pandanagraecia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[357,439,674,700]" pageId="28" pageNumber="29" rank="genus">n. gen.</taxonomicNameLabel>
</emphasis>
