°0' 
<number box="[344,402,2010,2036]" pageId="14" pageNumber="15" value="51.8">51.8</number>
''S
</geoCoordinate>
, 
<geoCoordinate box="[439,614,2010,2036]" direction="east" orientation="longitude" pageId="14" pageNumber="15" precision="1" value="145.41302">
<number box="[439,483,2010,2036]" pageId="14" pageNumber="15" value="145.0">145</number>
° 
