<paragraph blockId="4.[151,366,1754,1780]" box="[151,366,1754,1780]" pageId="4" pageNumber="5">
<heading bold="true" box="[151,366,1754,1780]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<emphasis bold="true" box="[151,366,1754,1780]" pageId="4" pageNumber="5">
<taxonomicName box="[151,276,1754,1780]" class="Insecta" family="Tettigoniidae" genus="Ingrischia" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus" status="gen. n.">
<emphasis bold="true" box="[151,276,1754,1780]" italics="true" pageId="4" pageNumber="5">Ingrischia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[284,366,1754,1780]" pageId="4" pageNumber="5" rank="genus">gen. n.</taxonomicNameLabel>
