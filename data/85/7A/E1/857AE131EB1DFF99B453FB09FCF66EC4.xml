<taxonomicName authority="Domning, 1989" authorityName="Domning" authorityYear="1989" class="Mammalia" family="Dugongidae" genus="Xenosiren" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sirenia" pageId="5" pageNumber="6" phylum="Chordata" rank="genus">
