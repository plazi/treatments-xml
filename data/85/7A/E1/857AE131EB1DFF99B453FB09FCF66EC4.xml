<emphasis box="[516,658,1669,1695]" italics="true" pageId="5" pageNumber="6">Eotheroides</emphasis>
Palmer, 1899
</taxonomicName>
, 
<taxonomicName authority="De Zigno, 1887" authorityName="De Zigno" authorityYear="1887" box="[829,1175,1668,1695]" class="Mammalia" family="Dugongidae" genus="Prototherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sirenia" pageId="5" pageNumber="6" phylum="Chordata" rank="genus">
<emphasis box="[829,988,1669,1695]" italics="true" pageId="5" pageNumber="6">Prototherium</emphasis>
De Zigno, 1887
</taxonomicName>
, 
<taxonomicName authority="Andrews, 1902" authorityName="Andrews" authorityYear="1902" class="Mammalia" family="Dugongidae" genus="Eosiren" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sirenia" pageId="5" pageNumber="6" phylum="Chordata" rank="genus">
