<taxonomicName authority="Sp." authorityName="Sp." box="[498,648,764,788]" class="Actinopterygii" family="Ambassidae" genus="Ambassis" kingdom="Animalia" order="Perciformes" pageId="14" pageNumber="15" phylum="Chordata" rank="genus">
