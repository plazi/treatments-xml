<number box="[1111,1156,511,535]" pageId="7" pageNumber="80" value="373.0">373</number>
, dL of palp segments: P- 
<number box="[151,165,548,572]" pageId="7" pageNumber="80" value="1.0">1</number>
<number box="[173,203,548,572]" pageId="7" pageNumber="80" value="36.0">36</number>
, P- 
<number box="[241,254,548,572]" pageId="7" pageNumber="80" value="2.0">2</number>
<number box="[263,293,548,572]" pageId="7" pageNumber="80" value="98.0">98</number>
, P- 
<number box="[332,344,548,572]" pageId="7" pageNumber="80" value="3.0">3</number>
<number box="[353,383,548,572]" pageId="7" pageNumber="80" value="75.0">75</number>
