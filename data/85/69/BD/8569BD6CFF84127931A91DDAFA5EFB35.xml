<quantity box="[389,547,503,525]" metricMagnitude="0" metricUnit="m" metricValue="2.9" metricValueMax="3.2" metricValueMin="2.6" pageId="21" pageNumber="507" unit="mm" value="2900.0" valueMax="3200.0" valueMin="2600.0">
