<taxonomicName authorityName="Lacepede" authorityYear="1799" box="[382,652,503,527]" class="Mammalia" family="Hominidae" genus="Pongo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="35" pageNumber="2086" phylum="Chordata" rank="subSpecies" species="pygmaeus" subSpecies="borneo">
