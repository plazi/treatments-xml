<emphasis box="[851,965,659,683]" italics="true" pageId="10" pageNumber="249">L. baynesi</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1019,1151,659,682]" class="Reptilia" family="Scincidae" genus="Lerista" kingdom="Animalia" order="Squamata" pageId="10" pageNumber="249" phylum="Chordata" rank="species" species="picturata">
