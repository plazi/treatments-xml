<quantity box="[210,378,437,464]" metricMagnitude="-2" metricUnit="m" metricValue="1.68" metricValueMax="1.78" metricValueMin="1.58" pageId="28" pageNumber="75" unit="mm" value="16.8" valueMax="17.8" valueMin="15.8">15.8-17.8 mm</quantity>
