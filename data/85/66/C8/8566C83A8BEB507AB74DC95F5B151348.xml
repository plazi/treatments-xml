<emphasis bold="true" pageId="4" pageNumber="5">A</emphasis>
<taxonomicName authorityName="Hartmann-Schroeder" authorityYear="1985" class="Polychaeta" family="Nereididae" genus="Ceratonereis" kingdom="Animalia" lsidName="Ceratonereis singularis subsp. australis" order="Phyllodocida" pageId="4" pageNumber="5" phylum="Annelida" rank="subSpecies" species="singularis" subSpecies="australis">
<emphasis italics="true" pageId="4" pageNumber="5">Ceratonereis singularis australis</emphasis>
</taxonomicName>
, male 
<emphasis bold="true" pageId="4" pageNumber="5">B</emphasis>
<taxonomicName authorityName="Hartmann-Schroeder" authorityYear="1985" class="Polychaeta" family="Nereididae" genus="Ceratonereis" kingdom="Animalia" lsidName="Ceratonereis singularis subsp. australis" order="Phyllodocida" pageId="4" pageNumber="5" phylum="Annelida" rank="subSpecies" species="singularis" subSpecies="australis">
