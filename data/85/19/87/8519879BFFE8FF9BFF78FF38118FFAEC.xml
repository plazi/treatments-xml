</emphasis>
<typeStatus box="[286,393,1307,1332]" pageId="20" pageNumber="21">
<emphasis box="[286,393,1307,1332]" italics="true" pageId="20" pageNumber="21">Holotype</emphasis>
</typeStatus>
. 
<collectingCountry box="[408,471,1306,1332]" name="Malta" pageId="20" pageNumber="21">Male</collectingCountry>
, 
<collectingCountry box="[484,639,1306,1332]" name="Malaysia" pageId="20" pageNumber="21">MALAYSIA</collectingCountry>
, Pahang, Bukit Fraser, 
<quantity box="[912,999,1306,1332]" metricMagnitude="3" metricUnit="m" metricValue="1.225296" pageId="20" pageNumber="21" unit="ft" value="4020.0">4020 ft</quantity>
, Malaise in forest 
<date box="[1217,1395,1306,1332]" pageId="20" pageNumber="21" value="1997-03-14" valueMax="1997-03-17" valueMin="1997-03-14">
<number box="[1217,1292,1306,1332]" pageId="20" pageNumber="21" value="15.5" valueMax="17.0" valueMin="14.0">14â€“17</number>
.III. 
<number box="[1332,1395,1306,1332]" pageId="20" pageNumber="21" value="1997.0">1997</number>
</date>
, H. Hippa, M. Jaschhof, B. Viklund (in 
<collectionCode box="[570,660,1346,1372]" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="20" pageNumber="21">SMNH</collectionCode>
