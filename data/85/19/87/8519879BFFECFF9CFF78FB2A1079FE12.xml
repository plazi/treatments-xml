. 
<collectingCountry box="[411,474,352,378]" name="Malta" pageId="19" pageNumber="20">Male</collectingCountry>
, 
<collectingCountry box="[488,643,352,378]" name="Malaysia" pageId="19" pageNumber="20">MALAYSIA</collectingCountry>
, Pahang, Genting Highlands, Awana, 
<quantity box="[1096,1184,352,378]" metricMagnitude="3" metricUnit="m" metricValue="1.024128" pageId="19" pageNumber="20" unit="ft" value="3360.0">3360 ft</quantity>
, Malaise in forest 
<date pageId="19" pageNumber="20" value="1997-03-09" valueMax="1997-03-22" valueMin="1997-03-09">
