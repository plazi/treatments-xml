<geoCoordinate direction="north" orientation="latitude" precision="1" value="35.111">35°06'39.6&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="25.809">25°48'32.4&quot;E</geoCoordinate>
