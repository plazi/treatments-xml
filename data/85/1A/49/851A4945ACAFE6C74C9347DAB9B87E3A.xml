<tbody pageId="null" pageNumber="567">
<tr pageId="null" pageNumber="567">
<td align="left" pageId="null" pageNumber="567" valign="top">
1. 
<normalizedToken originalValue="Blätter" pageId="null" pageNumber="567">Blaetter</normalizedToken>
mit einem 
<normalizedToken originalValue="endständigen" pageId="null" pageNumber="567">endstaendigen</normalizedToken>
Dorn
</td>
<td align="left" pageId="null" pageNumber="567" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="567" phylum="Tracheophyta" rank="species" species="sempervirens">
<italic pageId="null" pageNumber="567">A. sempervirens</italic>
</taxonomicName>
(Nr. 1)
</td>
