<taxonomicName id="3EA24D227F2AB363FBBBF88C90C1C93A" box="[1051,1159,1819,1844]" class="Magnoliopsida" family="Urticaceae" genus="Cecropia" kingdom="Plantae" order="Rosales" pageId="9" pageNumber="10" phylum="Tracheophyta" rank="genus">
<emphasis id="CBD6EAB37F2AB363FBBBF88C90C1C93A" box="[1051,1159,1819,1844]" italics="true" pageId="9" pageNumber="10">Cecropia</emphasis>
