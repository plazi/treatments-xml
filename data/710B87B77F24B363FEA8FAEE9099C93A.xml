<quantity id="3E5A9B447F2AB363FBBDF95D90C3C8EA" box="[1053,1157,1738,1764]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" pageId="9" pageNumber="10" unit="mm" value="2.65">2.65 mm</quantity>
long; females measure 
