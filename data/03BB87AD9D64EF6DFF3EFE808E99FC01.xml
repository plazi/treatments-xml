<taxonomicName id="4C124D389D64EF6DFB8AFCE28E9EFCD4" authority="Yang, Zhu and Hu" authorityName="Yang, Zhu and Hu" box="[1059,1435,780,801]" class="Insecta" family="Lauxaniidae" genus="Homoneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="518" phylum="Arthropoda" rank="species" species="polyacantha" subGenus="Homoneura">
