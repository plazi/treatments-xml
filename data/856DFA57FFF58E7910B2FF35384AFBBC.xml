species: 
<taxonomicName id="CAC430C2FFF58E791103FE683944FE31" authority="Bolivar, 1887" authorityName="Bolivar" authorityYear="1887" box="[294,630,384,406]" class="Insecta" family="Tetrigidae" genus="Systolederus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="490" phylum="Arthropoda" rank="species" species="haani">
