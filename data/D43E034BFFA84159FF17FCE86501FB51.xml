<taxonomicName id="9B97C9DEFFA84159FEFAFB476415FB76" authority="Klimaszewski and Webster" authorityName="Klimaszewski and Webster" authorityYear="2008" box="[353,781,1152,1178]" class="Insecta" family="Staphylinidae" genus="Gnypeta" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="64" phylum="Arthropoda" rank="species" species="minuta">
