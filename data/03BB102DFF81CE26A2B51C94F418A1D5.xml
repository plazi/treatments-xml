<date id="FFAC87FBFF81CE26A2B51834F677A100" box="[151,286,1303,1328]" pageId="55" pageNumber="56" value="2004-08-06">
<number id="3DDAB31CFF81CE26A2B51834F7CCA100" box="[151,165,1303,1328]" pageId="55" pageNumber="56" value="6.0">6</number>
.VIII. 
<number id="3DDAB31CFF81CE26A2C21834F677A100" box="[224,286,1303,1328]" pageId="55" pageNumber="56" value="2004.0">2004</number>
</date>
, Min Wang &amp; Xiao-Ling Fan leg. (
<collectionCode id="ED0339FEFF81CE26A08C183BF596A100" box="[686,767,1304,1328]" pageId="55" pageNumber="56">SCAU</collectionCode>
); 
<typeStatus id="54A91F99FF81CE26A131183BF411A100" box="[787,888,1304,1328]" pageId="55" pageNumber="56">Paratype</typeStatus>
, 
<specimenCount id="9D146AB2FF81CE26A1A6183BF4BAA100" box="[900,979,1303,1328]" pageId="55" pageNumber="56" type="male">
<number id="3DDAB31CFF81CE26A1A6183BF4FBA100" box="[900,914,1304,1328]" pageId="55" pageNumber="56" value="1.0">1</number>
