but 
<date id="FFAC87FBFF81CE26A72E183BF2FEA100" box="[1292,1431,1304,1328]" pageId="55" pageNumber="56" value="2009-07-23">
<number id="3DDAB31CFF81CE26A72E183BF240A100" box="[1292,1321,1304,1328]" pageId="55" pageNumber="56" value="23.0">23</number>
.VII. 
<number id="3DDAB31CFF81CE26A778183BF2FEA100" box="[1370,1431,1304,1328]" pageId="55" pageNumber="56" value="2009.0">2009</number>
