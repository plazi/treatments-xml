<taxonomicName id="4C0D0EB58A68FB559BCE26231CA9F8EB" authority="Griffini. Bollettino Musei di Zoologia" authorityName="Griffini. Bollettino Musei di Zoologia" authorityYear="1893" box="[221,944,1810,1836]" class="Insecta" family="Stenopelmatidae" genus="Stenopelmatus" kingdom="Animalia" order="Orthoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="calcaratus">
