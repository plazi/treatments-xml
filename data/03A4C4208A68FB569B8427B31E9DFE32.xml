<materialsCitation id="3B657F6B8A68FB569B8426231BA0FF13" ID-GBIF-Occurrence="3026613326" collectionCode="MRSNT, MNHN" country="Mexico" county="Jerusalem" lastPageId="26" lastPageNumber="27" location="Universita di Torino" municipality="New" pageId="25" pageNumber="26" specimenCount="1" typeStatus="holotype">
