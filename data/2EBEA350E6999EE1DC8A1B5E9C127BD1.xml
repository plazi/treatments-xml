<paragraph id="D7975C2A4D8569B4439ABE43E0734D45" pageId="3" pageNumber="102">
<taxonomicName id="D7AA2C229EF86356DEA81C7921A53FEB" LSID="http://bioguid.osu.edu/osuc_concepts:7847" authority="Kozlov" class="Insecta" family="Platygastridae" genus="Pyrgaspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyrgaspis" order="Hymenoptera" pageId="3" pageNumber="102" phylum="Arthropoda" rank="genus">Pyrgaspis Kozlov</taxonomicName>
