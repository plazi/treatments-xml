(
<collectorName id="26EE1A99FFADFFC4FF66FB8B2598CB2C" box="[252,434,1052,1076]" pageId="37" pageNumber="2037">Coll. Générale</collectorName>
), 
<collectorName id="26EE1A99FFADFFC4FE52FB8B262DCB2C" box="[456,519,1052,1076]" pageId="37" pageNumber="2037">Paris</collectorName>
</materialsCitation>
