<collectionCode id="ED0AE78AFFADFFC4FD3DFBCB272CCB6C" box="[679,774,1116,1140]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="37" pageNumber="2037">MNHN</collectionCode>
