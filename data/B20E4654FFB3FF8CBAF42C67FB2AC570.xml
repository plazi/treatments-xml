<collectingCounty id="D3798FCEFFB2FF8CB81F2A1FFC8CC7A4" box="[636,771,369,395]" pageId="25" pageNumber="26">Department</collectingCounty>
, 
<collectingCountry id="42B0B7D2FFB2FF8CB96D2A1FFCCAC7A4" box="[782,837,369,395]" name="Peru" pageId="25" pageNumber="26">Peru</collectingCountry>
</materialsCitation>
