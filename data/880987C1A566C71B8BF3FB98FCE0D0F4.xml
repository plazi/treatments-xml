<paragraph id="001F36D7A566C71A8BF3F9B4FC95D774" blockId="4.[136,1452,1133,1624]" box="[136,853,1602,1624]" pageId="4" pageNumber="291">
– 
<taxonomicName id="C7A04D54A566C71A8BEFF9B4FE13D774" authority="Gussone (1844: 808)" authorityName="Gussone" authorityPageNumber="808" authorityYear="1844" box="[148,467,1602,1624]" class="Magnoliopsida" family="Linaceae" genus="Linum" kingdom="Plantae" order="Malpighiales" pageId="4" pageNumber="291" phylum="Tracheophyta" rank="species" species="collinum">
