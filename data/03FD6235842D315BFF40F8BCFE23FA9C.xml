<caption id="DF2B83AB842A3158FF40F8E6FDADF803" httpUri="https://zenodo.org/record/3238916/files/figure.png" pageId="13" pageNumber="264" startId="13.[151,250,1898,1923]" targetBox="[151,1436,200,1856]" targetPageId="13">
