<taxonomicName id="4C48BFEE5A57FFB0FEB1FA08A791FA1A" authority="Smith" authorityName="Smith" box="[280,486,1485,1511]" class="Insecta" family="Stephanidae" genus="Foenatopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
