<taxonomicName id="4C5632E85460FF23FCBD9801059AFDCB" box="[768,897,584,607]" class="Insecta" family="Miridae" genus="Angerianus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="583" phylum="Arthropoda" rank="genus">
<emphasis id="B92295795460FF23FCBD9801059AFDCB" box="[768,897,584,607]" italics="true" pageId="2" pageNumber="583">Angerianus</emphasis>
</taxonomicName>
