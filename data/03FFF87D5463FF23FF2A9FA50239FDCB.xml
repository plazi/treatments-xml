<taxonomicName id="4C5632E85460FF23FC0598010207FDF4" box="[952,1052,584,608]" class="Insecta" family="Miridae" genus="Fingulus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="583" phylum="Arthropoda" rank="genus">
<emphasis id="B92295795460FF23FC0598010207FDF4" box="[952,1052,584,608]" italics="true" pageId="2" pageNumber="583">Fingulus</emphasis>
