<paragraph id="BBA71938CCFD0002DCE9327E3FA52330" pageId="0" pageNumber="27">
<emphasis id="872786AA453E16E11C2B894AEE6A9240" bold="true" pageId="0" pageNumber="27">Origin</emphasis>
