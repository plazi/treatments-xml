<taxonomicName id="AFD9A596123CFFB1FAECFA75FDC7FA05" authority="(Menard de la Groye, 1811)" authorityName="Menard de la Groye" authorityYear="1811" baseAuthorityName="Menard de la Groye" baseAuthorityYear="1811" class="Gastropoda" family="Ringiculidae" genus="Ringicula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="70" pageNumber="71" phylum="Mollusca" rank="species" species="auriculata">
