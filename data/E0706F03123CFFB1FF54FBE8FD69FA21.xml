<taxonomicName id="AFD9A596123CFFB1FEC4FA3DFD6DFA21" authority="Monterosato, 1877" authorityName="Monterosato" authorityYear="1877" box="[263,712,1484,1509]" class="Gastropoda" family="Ringiculidae" genus="Ringicula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="70" pageNumber="71" phylum="Mollusca" rank="species" species="conformis">
