<taxonomicName id="4C174D43FFA2FF8698EAFEF0F80F3118" authority="(Pennant, 1784)" baseAuthorityName="Pennant" baseAuthorityYear="1784" box="[151,590,258,285]" class="Magnoliopsida" family="Euphorbiaceae" genus="Mallotus" kingdom="Plantae" order="Malpighiales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="species" species="catervarius">
<emphasis id="B963EAD2FFA2FF8698EAFEF0FBCB3118" bold="true" box="[151,394,258,285]" italics="true" pageId="51" pageNumber="52">Mallotus catervarius</emphasis>
