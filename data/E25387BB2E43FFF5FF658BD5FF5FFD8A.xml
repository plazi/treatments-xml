<geoCoordinate id="0FCE506A2E43FFF5FEBA8906FE0AFDFB" box="[297,408,516,540]" degrees="4" direction="west" minutes="35" orientation="longitude" pageId="13" pageNumber="38" precision="15" seconds="49" value="-4.596945">4°35’49”W</geoCoordinate>
