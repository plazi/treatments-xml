<paragraph id="4A7A853BFFE4FFD3FF54F939CE31F8E6" blockId="0.[151,605,1768,1795]" box="[151,605,1768,1795]" pageId="0" pageNumber="182">
<heading id="11323257FFE4FFD3FF54F939CE31F8E6" bold="true" box="[151,605,1768,1795]" fontSize="11" level="1" pageId="0" pageNumber="182" reason="1">
<emphasis id="78B15929FFE4FFD3FF54F939CE31F8E6" bold="true" box="[151,605,1768,1795]" pageId="0" pageNumber="182">
Genus 
<taxonomicName id="8DC5FEB8FFE4FFD3FF2DF939CD8BF8E6" authority="Wang &amp; Zhou &amp; Chang, 2020" authorityName="Wang &amp; Zhou &amp; Chang" authorityYear="2020" box="[238,487,1768,1795]" class="Insecta" family="Tettigoniidae" genus="Pseudophlugiolopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="0" pageNumber="182" phylum="Arthropoda" rank="genus" status="gen. nov.">
