<geoCoordinate id="30EFBC5BC6A2C294035E899968543FD4" degrees="39" direction="north" minutes="57" orientation="latitude" precision="1" seconds="47.49" value="39.963192">39°57'47.49&quot;N</geoCoordinate>
), from branches of 
<taxonomicName id="302B7DDE6C6E4AF29DC87C96E67F204C" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Juglans mandshurica" order="Fagales" pageId="0" pageNumber="97" phylum="Tracheophyta" rank="species" species="mandshurica">
<emphasis id="417954378C7E7AA849E8303363F293F7" italics="true" pageId="0" pageNumber="97">Juglans mandshurica</emphasis>
