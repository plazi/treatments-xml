<taxonomicName id="75B84D40FF861878CE16F8D6FA71DD0F" authority="Dumeril, 1861" authorityName="Dumeril" authorityYear="1861" box="[1034,1436,1875,1899]" class="Elasmobranchii" family="Myliobatidae" genus="Aetobatus" kingdom="Animalia" order="Myliobatiformes" pageId="1" pageNumber="436" phylum="Chordata" rank="species" species="latirostris">
