<geoCoordinate id="EE0894C8FFC54E0E71A8F9E2826DCE77" box="[946,1076,1583,1608]" degrees="117" direction="east" minutes="57" orientation="longitude" pageId="36" pageNumber="967" precision="15" seconds="04" value="117.95111">117°57′04″E</geoCoordinate>
