<geoCoordinate id="EE0894C8FFC54E0E7130F9E285FBCE77" box="[810,930,1583,1608]" degrees="05" direction="north" minutes="49" orientation="latitude" pageId="36" pageNumber="967" precision="15" seconds="36" value="5.826667">05°49′36″N</geoCoordinate>
