<emphasis id="B95F7A3C8C7CFFF4B4A4FADE0E21C044" bold="true" box="[151,342,1311,1334]" pageId="3" pageNumber="25">FIGURES 17–25.</emphasis>
<taxonomicName id="4C2BDDAD8C7CFFF4B56DFAE00C00C044" authority="Qin and Liu" authorityName="Qin and Liu" box="[350,887,1311,1334]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="25" phylum="Arthropoda" rank="species" species="biprocessa" status="sp. nov." subGenus="Matsumurasca">
