and 
<taxonomicName id="9B894D38FF9588632F57E574C0F9F9E4" authorityName="Fedosov &amp; Malcolm &amp; Terryn &amp; Gorson &amp; Modica &amp; Holford &amp; Puillandre" authorityYear="2019" box="[1144,1237,1627,1646]" class="Gastropoda" family="Terebridae" genus="Myurellopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="19" pageNumber="20" phylum="Mollusca" rank="genus">
