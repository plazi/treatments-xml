Genera 
<taxonomicName id="9B894D38FF95886328DBE574C067F9E4" authorityName="Fedosov &amp; Malcolm &amp; Terryn &amp; Gorson &amp; Modica &amp; Holford &amp; Puillandre" authorityYear="2019" box="[1012,1099,1627,1646]" class="Gastropoda" family="Terebridae" genus="Maculauger" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="19" pageNumber="20" phylum="Mollusca" rank="genus">
