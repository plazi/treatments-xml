<taxonomicName id="5D11D4997C5B174738BBB30134B2E1E7" LSID="http://zoobank.org/EEBE0D21-510C-470E-B1E6-73D18F387BEC" class="Arachnida" family="Galumnidae" genus="Pergalumna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pergalumna paraboliviana" order="Sarcoptiformes" pageId="2" pageNumber="89" phylum="Arthropoda" rank="species" species="paraboliviana">Pergalumna paraboliviana</taxonomicName>
