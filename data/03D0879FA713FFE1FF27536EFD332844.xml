<heading id="D08E81E5A713FFE0FF27536EFD0828D0" bold="true" box="[151,733,1389,1415]" fontSize="11" level="1" pageId="4" pageNumber="29" reason="1">
<emphasis id="B90DEA9BA713FFE0FF27536EFD0828D0" bold="true" box="[151,733,1389,1415]" pageId="4" pageNumber="29">
<taxonomicName id="4C794D0AA713FFE0FF27536EFD9528D0" authority="Evenhuis" authorityName="Evenhuis" box="[151,576,1389,1415]" class="Insecta" family="Dolichopodidae" genus="Campsicnemus" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="29" phylum="Arthropoda" rank="species" species="gladiator" status="new species">
<emphasis id="B90DEA9BA713FFE0FF27536EFE1628D0" bold="true" box="[151,451,1389,1415]" italics="true" pageId="4" pageNumber="29">Campsicnemus gladiator</emphasis>
Evenhuis
</taxonomicName>
, 
<taxonomicNameLabel id="A23E57E0A713FFE0FDFD536EFD0828D0" box="[589,733,1389,1415]" pageId="4" pageNumber="29" rank="species">new species</taxonomicNameLabel>
