<materialsCitation id="C7E1C43BFD50FFBCBA7DF8EB798811FA" ID-GBIF-Occurrence="2332527321" collectingDate="1959-10" collectionCode="RMCA" collectorName="J. Vadon" country="Madagascar" location="The" municipality="East African islands" pageId="2" pageNumber="391" specimenCount="1" specimenCount-male="1" stateProvince="Antsiranana" typeStatus="paratype">
