<taxonomicName id="4C484D37931BA8576FEBFC562279C1A5" authority="Meigen, 1830" authorityName="Meigen" authorityYear="1830" box="[151,368,942,969]" class="Insecta" family="Bibionidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="scripta" subGenus="Vestiplex">
<emphasis id="B93CEAA6931BA8576FEBFC5623EFC1A5" bold="true" box="[151,230,942,969]" italics="true" pageId="61" pageNumber="62">Tipula</emphasis>
(
<emphasis id="B93CEAA6931BA8576F8BFC572207C1A5" bold="true" box="[247,270,943,969]" italics="true" pageId="61" pageNumber="62">V.</emphasis>
