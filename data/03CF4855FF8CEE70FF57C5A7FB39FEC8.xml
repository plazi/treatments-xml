species: 
<taxonomicName id="4C6682C0FF8CEE71FEE6C5E0FDA3F949" authority="Gaud, 1962" authorityName="Gaud" authorityYear="1962" box="[294,601,1743,1765]" class="Arachnida" family="Proctophyllodidae" genus="Pterodectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="wolffi">
<emphasis id="B9122551FF8CEE71FEE6C5E0FE27F949" box="[294,477,1743,1765]" italics="true" pageId="30" pageNumber="31">Pterodectes wolffi</emphasis>
<bibRefCitation id="EFF784B2FF8CEE71FE23C5E0FDA3F949" author="Gaud" box="[483,601,1743,1765]" pageId="30" pageNumber="31" refString="Gaud, J. (1962) Sarcoptiformes plumicoles (Analgesoidea) parasites d'oiseaux de l'Ile Rennell. The Natural History of Rennell Island, British Solomon Islands, 4, 31 - 51." type="journal article" year="1962">
