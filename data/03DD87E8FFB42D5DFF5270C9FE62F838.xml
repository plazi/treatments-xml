<emphasis id="B900EAECFFB42D5DFF5270C9FE5DF916" bold="true" box="[151,404,1663,1690]" pageId="20" pageNumber="21">
Species group 
<taxonomicName id="4C744D7DFFB42D5DFE8E7036FE5DF916" baseAuthorityName="Berla" baseAuthorityYear="1959" box="[331,404,1663,1689]" class="Arachnida" family="Proctophyllodidae" genus="Platyacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="minor">
