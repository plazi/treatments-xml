<geoCoordinate id="E2B03E321DC5BFFF325DA5D694DA6BCD" direction="north" orientation="latitude" precision="925" value="10.433333">10°26'N</geoCoordinate>
, 
<geoCoordinate id="6B0BE15070E9AC11C2332913EE155004" direction="west" orientation="longitude" precision="925" value="-84.01667">84°01'W</geoCoordinate>
