<collectionCode id="ED5FAE018924FF80FF248E06C2ADFB3A" box="[181,245,1149,1175]" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="34" pageNumber="35" type="Museum">ZMB</collectionCode>
