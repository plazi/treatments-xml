<location id="8E91601F8924FF80FEA48E2EC3C8FBC2" LSID="urn:lsid:plazi:treatment:03E787D28924FF80FF0688AEC1A6FB3A:8E91601F8924FF80FEA48E2EC3C8FBC2" box="[309,400,1109,1135]" county="Hotha" municipality="These" name="Dobson" pageId="34" pageNumber="35" stateProvince="Yunnan">Dobson</location>
