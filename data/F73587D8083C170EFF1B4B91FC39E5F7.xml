<taxonomicName id="B89C4D4D083F170EFE82494EFE46E530" box="[270,415,584,608]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="55" phylum="Arthropoda" rank="genus">
