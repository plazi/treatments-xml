<heading id="D0F4822F8005FFBF89DFFA291029FA7F" box="[959,1309,1427,1454]" centered="true" fontSize="11" level="2" pageId="6" pageNumber="13" reason="2">
<taxonomicName id="4C034EC08005FFBF89DFFA291029FA7F" authority="Leidy, 1852" authorityName="Leidy" authorityYear="1852" box="[959,1309,1427,1454]" class="Mammalia" family="Basilosauridae" genus="Pontogeneus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="6" pageNumber="13" phylum="Chordata" rank="species" species="priscus">
<emphasis id="B977E9518005FFBF89DFFA2911A0FA7C" bold="true" box="[959,1172,1427,1453]" italics="true" pageId="6" pageNumber="13">Pontogeneus priscus</emphasis>
<bibRefCitation id="EF9248B28005FFBF8EFBFA291029FA7F" author="LEIDY J." box="[1179,1309,1427,1454]" pageId="6" pageNumber="13" pagination="52" refId="ref100971" refString="LEIDY J. 1852. - Description of Pontogeneus priscus. Proceedings of the Academy of Natural Sciences of Philadelphia 6: 52." type="journal article" year="1852">Leidy, 1852</bibRefCitation>
