<treatmentCitationGroup id="AB311186FFD8FFF025FDF9ED0FC1F9B5" box="[192,1016,1614,1639]" pageId="6" pageNumber="689">
<taxonomicName id="4C214D2BFFD8FFF025FDF9ED0E46F9B5" box="[192,639,1614,1639]" class="Arachnida" family="Phytoseiidae" genus="Amblydromella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="689" phylum="Arthropoda" rank="species" species="buccalis" subGenus="Amblydromella">
