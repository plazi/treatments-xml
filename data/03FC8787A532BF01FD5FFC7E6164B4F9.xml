<taxonomicName id="4C554D12A532BF01FADCFB3465A8B5D0" authorityName="C.Linnaeus" authorityYear="1753" box="[1291,1415,1185,1210]" class="Equisetopsida" family="Equisetaceae" genus="Equisetum" kingdom="Plantae" order="Equisetales" pageId="36" pageNumber="37" phylum="Tracheophyta" rank="genus">
