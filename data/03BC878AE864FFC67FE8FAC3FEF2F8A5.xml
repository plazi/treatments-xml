<paragraph id="8BAA369CE864FFC67FE8F99AFEF2F8A5" blockId="38.[151,1437,1376,1798]" pageId="38" pageNumber="439">
<emphasis id="B961EA8EE864FFC67FE8F99AFEB0F9F0" bold="true" box="[151,335,1592,1619]" pageId="38" pageNumber="439">Type material:</emphasis>
<materialsCitation id="3B7D3CC1E864FFC67E21F99AFC0DF91D" collectionCode="NHML" pageId="38" pageNumber="439" specimenCode="NHML010694500, NHML010709952" specimenCount="4" typeStatus="syntype">
