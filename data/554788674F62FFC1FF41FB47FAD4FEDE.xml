<taxonomicNameLabel id="F4A958184F64FFC2FB08F836FAD6F85F" box="[1246,1384,1974,1997]" pageId="13" pageNumber="14" rank="species">new species</taxonomicNameLabel>
, and 
<taxonomicName id="1AEE42F24F64FFC2FF41F859FDACF87D" authority="Bartalucci." authorityName="Bartalucci." box="[151,530,2008,2032]" class="Insecta" family="Tiphiidae" genus="Anthobosca" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="micromeria">
<emphasis id="EF9AE5634F64FFC2FF41F859FE29F862" box="[151,407,2009,2032]" italics="true" pageId="13" pageNumber="14">Anthobosca micromeria</emphasis>
Bartalucci.
</taxonomicName>
</paragraph>
</subSubSection>
