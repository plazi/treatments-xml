<taxonomicName id="4C2808B8FF814F19FDE6F841FDB878C9" authorityName="Wasmann" authorityYear="1896" box="[535,636,1980,1999]" class="Insecta" family="Tenebrionidae" genus="Xenotermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="345" phylum="Arthropoda" rank="genus">
