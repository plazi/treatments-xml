; width, 
<quantity id="CA02C9DE2D40FFF5FF71FE9DAF2EFEE7" box="[192,353,342,369]" metricMagnitude="-2" metricUnit="m" metricValue="1.16" metricValueMax="1.19" metricValueMin="1.13" pageId="27" pageNumber="74" unit="mm" value="11.6" valueMax="11.9" valueMin="11.3">11.3-11.9 mm</quantity>
; thickness, 
<quantity id="CA02C9DE2D40FFF5FE6DFE9DAC3FFEE7" box="[476,624,342,369]" metricMagnitude="-2" metricUnit="m" metricValue="1.03" metricValueMax="1.0699999999999998" metricValueMin="0.99" pageId="27" pageNumber="74" unit="mm" value="10.3" valueMax="10.7" valueMin="9.9">9.9-10.7 mm</quantity>
.
</paragraph>
