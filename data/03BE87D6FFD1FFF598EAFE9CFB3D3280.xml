<quantity id="4CEF9B25FFD1FFF59DFDFE29FB64321C" metricMagnitude="2" metricUnit="m" metricValue="6.55" metricValueMax="13.1" metricValueMin="0.0" pageId="64" pageNumber="65" unit="m" value="655.0" valueMax="1310.0" valueMin="0.0">0– 1,310 m</quantity>
