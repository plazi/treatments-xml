<subSubSection id="00F95D8E3A2BCDDD4560C027B29F833C" pageId="118" pageNumber="119" type="nomenclature">
<paragraph id="D39197CC1B839A33E4533ED249B63AAA" pageId="118" pageNumber="119">
<taxonomicName id="2AFE18005D8662B29761EF0A5C4652CD" LSID="DE306AC0-5F25-504A-B12F-F265898B67C2" authority="(Burm. f.) Merr." authorityName="(Burm. f.) Merr." class="Magnoliopsida" family="Meliaceae" genus="Sandoricum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Sandoricum koetjape" order="Sapindales" pageId="118" pageNumber="119" phylum="Tracheophyta" rank="species" species="koetjape">Sandoricum koetjape (Burm.f.) Merr.</taxonomicName>
