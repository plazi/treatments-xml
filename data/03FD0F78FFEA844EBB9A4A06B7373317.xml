, 
<date id="FFEA98AEFFEA844EB90B4566B1CA3857" box="[601,715,2074,2099]" pageId="5" pageNumber="6" value="2010-03-03">03.iii.2010</date>
, 
<specimenCount id="9D5275E7FFEA844EBB0C4546B3803836" box="[94,129,2103,2131]" pageId="5" pageNumber="6" type="female">1 ♀</specimenCount>
