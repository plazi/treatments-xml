<collectionCode id="2C82AE15FFA9FFC2FF11F8DE1CEEF899" LSID="urn:lsid:biocol.org:col:34871" box="[151,232,1870,1894]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="26" pageNumber="27">USNM</collectionCode>
