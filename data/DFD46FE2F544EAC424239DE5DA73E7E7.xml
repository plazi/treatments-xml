<taxonomicName id="8854E779150AB729B27BF567CF6A0DD9" authority="Jacoby, 1886" authorityName="Jacoby" authorityYear="1886" class="Insecta" family="Chrysomelidae" genus="Madurasia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Madurasia undulatovittata" order="Coleoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="undulatovittata">Madurasia undulatovittata</taxonomicName>
