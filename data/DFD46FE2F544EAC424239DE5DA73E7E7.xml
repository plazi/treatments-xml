<taxonomicName id="8D2EB1E5E1E401519FFE8BB2C69F1B38" authority="Jacoby, 1886" authorityName="Jacoby" authorityYear="1886" class="Insecta" family="Chrysomelidae" genus="Madurasia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Madurasia andamanica" order="Coleoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="andamanica">Madurasia andamanica</taxonomicName>
