<location id="AB1D6042FFFDFFDDFE0EFEA1CADEFEC5" LSID="urn:lsid:plazi:treatment:266B878FFFFEFFDDFF05FF64C8E2FE1C:AB1D6042FFFDFFDDFE0EFEA1CADEFEC5" box="[441,480,285,307]" county="Habitat" municipality="Phanologie" name="Auf" pageId="6" pageNumber="465" stateProvince="Peloponnisos">Auf</location>
der 
<collectingRegion id="6C06F87BFFFDFFDDFDA2FEA1C9B4FEC5" box="[533,650,285,307]" country="Greece" name="Peloponnisos" pageId="6" pageNumber="465">Peloponnes</collectingRegion>
ausschliesslich montane 
<location id="AB1D6042FFFDFFDDFC3AFEA1C8C2FEC5" LSID="urn:lsid:plazi:treatment:266B878FFFFEFFDDFF05FF64C8E2FE1C:AB1D6042FFFDFFDDFC3AFEA1C8C2FEC5" box="[909,1020,285,307]" county="Habitat" municipality="Phanologie" name="Nachweise" pageId="6" pageNumber="465" stateProvince="Peloponnisos">Nachweise</location>
(
<quantity id="693A9B7CFFFDFFDDFBB9FEA1CBC2FEA6" metricMagnitude="3" metricUnit="m" metricValue="1.25" metricValueMax="1.7" metricValueMin="0.8" pageId="6" pageNumber="465" unit="m" value="1250.0" valueMax="1700.0" valueMin="800.0">
<elevation id="25EFD1AAFFFDFFDDFBB9FEA1CBC2FEA6" metricMagnitude="3" metricUnit="m" metricValue="1.25" metricValueMax="1.7" metricValueMin="0.8" pageId="6" pageNumber="465" unit="m" value="1250.0" valueMax="1700.0" valueMin="800.0">800- 1700m</elevation>
</quantity>
