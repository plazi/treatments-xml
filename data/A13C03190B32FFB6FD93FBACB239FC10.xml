<paragraph id="292AB20F0B3CFFB6FF32FCBEB239FC10" blockId="8.[189,1399,878,940]" pageId="8" pageNumber="9">
<taxonomicName id="EE95C98C0B3CFFB6FF32FCBEB6F7FC35" box="[189,441,879,905]" class="Magnoliopsida" family="Gesneriaceae" genus="Cyrtandra" kingdom="Plantae" order="Lamiales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="ferruginea">
