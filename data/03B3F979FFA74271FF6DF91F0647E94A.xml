'W
</geoCoordinate>
, 
<quantity id="4CE2E58AFFA54270FF6DFF07040DEB81" box="[151,280,223,248]" metricMagnitude="2" metricUnit="m" metricValue="2.38" metricValueMax="2.52" metricValueMin="2.2399999999999998" pageId="86" pageNumber="87" unit="m" value="238.0" valueMax="252.0" valueMin="224.0">224–252 m</quantity>
, 
<date id="FFA46EAFFFA54270FEDEFF3804A8EB8E" box="[292,445,224,248]" pageId="86" pageNumber="87" value="1992-05-12">
