<misspelling id="4AAA7B5AC28D2DC1CF35CEC28F929E1B" corrections="[Ljava.lang.String;@180c06e" pageNumber="314" type="misspelled">Sarawak</misspelling>
</collectingRegion>
(
<collectorName id="CF2C786D9ED7B538D35BEBDBF0F4C8A6" pageId="35" pageNumber="314">
<tnCandidate id="491A3A7E908FFB9740A05694AA49C720" pageId="35" pageNumber="314">Dyak</tnCandidate>
s.n.,
</collectorName>
<date id="509152BDE5EC1B8D0B61248CDE80CB78" pageId="35" pageNumber="314" value="1908-12">
<collectingDate id="ECBAD04B1169BC5C134933952622D8CA" pageId="35" pageNumber="314" value="1908-12">
<misspelling id="50E0A34C6E4671213BB6BBC92672DEC5" corrections="[Ljava.lang.String;@1d99133" pageNumber="314" type="misspelled">Dec</misspelling>
