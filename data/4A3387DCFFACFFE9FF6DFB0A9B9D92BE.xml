<taxonomicName id="059A4D49FFACFFE9FDFCF9339D9F92BE" authorityName="Quinn" authorityYear="1991" box="[518,654,1681,1706]" class="Gastropoda" family="Solariellidae" genus="Lamellitrochus" higherTaxonomySource="CoL" kingdom="Animalia" order="Trochida" pageId="18" pageNumber="419" phylum="Mollusca" rank="species" species="carinatus">
