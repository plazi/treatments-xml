28 
<taxonomicName id="4C3D69CC2F618E18029AFBEBFE9FFBFA" box="[176,286,1066,1088]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="182" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
