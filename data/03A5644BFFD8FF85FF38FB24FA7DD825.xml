<taxonomicName id="4C0CAEDEFFDAFF85FD5AFA62FCA7DAA5" box="[674,890,1475,1499]" class="Insecta" family="Odontoceridae" genus="Psilotreta" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="undetermined">
