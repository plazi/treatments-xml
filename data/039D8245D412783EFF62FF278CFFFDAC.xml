<collectingCountry id="F32373C3D412783EFC5AFE658B60FE50" box="[943,1057,469,494]" name="Argentina" pageId="12" pageNumber="513">Argentina</collectingCountry>
, 
<collectingCountry id="F32373C3D412783EFBC1FE658BC8FE50" box="[1076,1161,469,494]" name="Bolivia" pageId="12" pageNumber="513">Bolivia</collectingCountry>
, 
<collectingCountry id="F32373C3D412783EFB6EFE658B9EFE50" box="[1179,1247,469,494]" name="Brazil" pageId="12" pageNumber="513">Brazil</collectingCountry>
(
<bibRefCitation id="EFA54EA2D412783EFB00FE658AD6FE50" author="Metcalf" box="[1269,1431,469,494]" pageId="12" pageNumber="513" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
