<taxonomicName id="4C5E12E4AF735D5AFEF3AB5A7434DF07" authorityName="Sharp" authorityYear="1887" box="[363,600,1305,1328]" class="Insecta" family="Staphylinidae" genus="Megarthrus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="120" phylum="Arthropoda" rank="species" species="solitarius">
