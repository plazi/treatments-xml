<taxonomicName id="4C154D375D77FFACFE8DB8EB356D86FA" authority="Collin, 1960" authorityName="Collin" authorityYear="1960" box="[339,790,1930,1956]" class="Insecta" family="Hybotidae" genus="Crossopalpus" kingdom="Animalia" order="Diptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="hirsutipes">
