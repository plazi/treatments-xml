<taxonomicName id="36096E05EFC705923D43ADE94B0C6C34" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24838" class="Insecta" family="Formicidae" genus="Leptogenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptogenys Roger" lsidName-HNS="Leptogenys Roger" order="Hymenoptera" pageNumber="6" phylum="Arthropoda" rank="genus">Leptogenys</taxonomicName>
