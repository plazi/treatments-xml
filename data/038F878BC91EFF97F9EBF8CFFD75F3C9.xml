Caribbean Sea (
<bibRefCitation id="EFB74B6CC91DFF97FB63FE4DFD52F3B5" author="Opresko, D. M." box="[543,715,404,431]" pageId="14" pageNumber="15" pagination="209 - 240" refId="ref48958" refString="Opresko, D. M. (2004) Revision of the Antipatharia (Cnidaria: Anthozoa). Part IV. Establishment of a new family, Aphanipathidae. Zoologische Mededelingen, 78, 209 - 240." type="journal article" year="2004">Opresko, 2004</bibRefCitation>
; 
<bibRefCitation id="EFB74B6CC91DFF97FBA4FE4DFCF5F3B5" author="Loiola, L. L." box="[728,876,404,431]" pageId="14" pageNumber="15" pagination="253 - 264" refId="ref47648" refString="Loiola, L. L. (2007) Black corals (Cnidaria: Antipatharia) from Brazil: an overview. Bulletin of Marine Science, 81 (3), 253 - 264." type="journal article" year="2007">Loiola, 2007</bibRefCitation>
; 
<bibRefCitation id="EFB74B6CC91DFF97FA06FE4DFBBAF3B5" author="Opresko, D. M." box="[890,1059,404,431]" pageId="14" pageNumber="15" pagination="359 - 363" refId="ref49123" refString="Opresko, D. M. (2009) Antipatharia (Cnidaria) of the Gulf of Mexico. Gulf of Mexico Origin, Waters, and Biota: Biodiversity, 1, 359 - 363." type="journal article" year="2009">Opresko, 2009</bibRefCitation>
) and off 
<collectingCountry id="F331760DC91DFF97FDF2FE4DFB4BF3B4" box="[1166,1234,404,430]" name="Brazil" pageId="14" pageNumber="15">Brazil</collectingCountry>
(fig. 35) (
<bibRefCitation id="EFB74B6CC91DFF97FC37FE4DFF4DF3C9" author="Loiola, L. L." pageId="14" pageNumber="15" pagination="253 - 264" refId="ref47648" refString="Loiola, L. L. (2007) Black corals (Cnidaria: Antipatharia) from Brazil: an overview. Bulletin of Marine Science, 81 (3), 253 - 264." type="journal article" year="2007">Loiola, 2007</bibRefCitation>
); from 
<quantity id="4CDE9B78C91DFF97F859FE60FEF5F3C9" box="[293,364,441,467]" metricMagnitude="2" metricUnit="m" metricValue="1.29" pageId="14" pageNumber="15" unit="m" value="129.0">129 m</quantity>
to 
<quantity id="4CDE9B78C91DFF97F8F3FE60FE4FF3C9" box="[399,470,441,467]" metricMagnitude="2" metricUnit="m" metricValue="4.9399999999999995" pageId="14" pageNumber="15" unit="m" value="494.0">494 m</quantity>
depths (
<bibRefCitation id="EFB74B6CC91DFF97FB49FE61FD79F3C9" author="Opresko, D. M." box="[565,736,440,467]" pageId="14" pageNumber="15" pagination="359 - 363" refId="ref49123" refString="Opresko, D. M. (2009) Antipatharia (Cnidaria) of the Gulf of Mexico. Gulf of Mexico Origin, Waters, and Biota: Biodiversity, 1, 359 - 363." type="journal article" year="2009">Opresko, 2009</bibRefCitation>
).
</paragraph>
