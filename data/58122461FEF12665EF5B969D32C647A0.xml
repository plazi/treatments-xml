<taxonomicName id="91EDED853E72435CDEB671034C2720C5" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia" order="Zingiberales" pageId="155" pageNumber="156" phylum="Tracheophyta" rank="genus">Heliconia</taxonomicName>
sp. (
<taxonomicName id="F07B18968DF2D7DBA42E1B1D8295B20B" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="155" pageNumber="156" rank="genus">Heliconiaceae</taxonomicName>
).
</paragraph>
</subSubSection>
