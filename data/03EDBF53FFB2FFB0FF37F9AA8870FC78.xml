<subSubSection id="C35E5DCEFFB4FFB0FF67FD728C8DFCB0" pageId="76" pageNumber="77" type="etymology">
<paragraph id="8BFB0E45FFB4FFB0FF67FD728C8DFCB0" blockId="76.[151,1436,152,1018]" pageId="76" pageNumber="77">
<emphasis id="B930D257FFB4FFB0FF67FD728D1EFC8B" bold="true" box="[199,332,751,777]" pageId="76" pageNumber="77">Etymology</emphasis>
