<taxonomicName id="4C4475C6FFB4FFB0FE75FD6D8EF4FC8B" authorityName="Zannou, Moraes &amp; Oliveira" authorityYear="2008" box="[469,678,752,777]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="neohartlandrowei">
