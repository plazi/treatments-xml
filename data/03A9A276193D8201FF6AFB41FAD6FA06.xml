<subSubSection id="C31A40EB193D8201FF3AFB6CFD87FB2D" pageId="18" pageNumber="19" type="distribution">
<paragraph id="8BBF1360193D8201FF3AFB6CFD87FB2D" blockId="18.[151,1437,1159,1473]" pageId="18" pageNumber="19">
<emphasis id="B974CF72193D8201FF3AFB6CFEADFB03" bold="true" box="[199,355,1194,1220]" pageId="18" pageNumber="19">Distribution:</emphasis>
<collectingCountry id="F31753F0193D8201FE91FB6DFDC6FB02" box="[364,520,1195,1221]" name="United States of America" pageId="18" pageNumber="19">United States</collectingCountry>
