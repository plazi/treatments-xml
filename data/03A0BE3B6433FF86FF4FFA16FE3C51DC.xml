<taxonomicName id="4C0974AE6433FF86FF36FA16FDCF52F0" authority="Kaup, 1829" authorityName="Kaup" authorityYear="1829" box="[238,526,1408,1434]" class="Mammalia" family="Vespertilionidae" genus="Pipistrellus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Chiroptera" pageId="57" pageNumber="58" phylum="Chordata" rank="genus">
