<quantity id="4CDF9B3F2947FFE5FAA8E37EFA3948D6" box="[1406,1524,1390,1419]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" pageId="7" pageNumber="6" unit="mm" value="1.75">1.75 mm</quantity>
