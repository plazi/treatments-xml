<taxonomicName id="4C0D6692FF97FFEDFE57E8E3FCBD828F" box="[497,768,672,697]" class="Aves" family="Thraupidae" genus="Tachyphonus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="64" phylum="Chordata" rank="species" species="luctuosus">
