<taxonomicName id="9ABBE28EFFC36844D0D8FEE1FF30FE6D" box="[151,234,364,390]" class="Entognatha" family="Neelidae" genus="Neelus" kingdom="Animalia" order="Collembola" pageId="20" pageNumber="71" phylum="Arthropoda" rank="genus">
