<taxonomicName id="DD478754C52E9786DDEA9FDE677E69A8" class="Gastropoda" family="Streptaxidae" genus="Perrottetia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Perrottetia aquilonaria" order="Stylommatophora" pageId="8" pageNumber="49" phylum="Mollusca" rank="species" species="aquilonaria">Perrottetia aquilonaria</taxonomicName>
