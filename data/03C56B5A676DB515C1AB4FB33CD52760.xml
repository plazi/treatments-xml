<taxonomicName id="4C6CA1CF676DB515C5184D6A3C7E2760" box="[1060,1124,1562,1585]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="312" pageNumber="313" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E676DB515C5184D6A3C7E2760" box="[1060,1124,1562,1585]" italics="true" pageId="312" pageNumber="313">Picea</emphasis>
