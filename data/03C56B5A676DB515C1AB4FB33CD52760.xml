<taxonomicName id="4C6CA1CF676DB515C5B54D693CDD2763" box="[1161,1223,1561,1586]" class="Pinopsida" family="Pinaceae" genus="Abies" kingdom="Plantae" order="Pinales" pageId="312" pageNumber="313" phylum="Tracheophyta" rank="genus">
