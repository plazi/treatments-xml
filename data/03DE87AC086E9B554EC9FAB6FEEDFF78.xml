<taxonomicName id="4C774D3908689B564C57F8B1FCB2F89D" authorityName="Yang &amp; Du &amp; Zhu" authorityYear="2021" box="[521,778,1816,1840]" class="Insecta" family="Leuctridae" genus="Rhopalopsole" kingdom="Animalia" order="Plecoptera" pageId="5" pageNumber="148" phylum="Arthropoda" rank="species" species="trichotoma">
