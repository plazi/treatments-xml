<taxonomicName id="F7884D23FFA1FFF10B3EF68B8A38F6FC" authority="Leach." box="[1336,1573,2382,2420]" class="Insecta" family="Lestidae" genus="Lestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="364" phylum="Arthropoda" rank="genus">
