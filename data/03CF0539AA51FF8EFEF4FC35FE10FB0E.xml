<taxonomicName id="4C66CFACAA51FF8EFD0FFBA8FC94FBE5" authorityName="Evans" authorityYear="1970" box="[755,850,1116,1141]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="napo">
