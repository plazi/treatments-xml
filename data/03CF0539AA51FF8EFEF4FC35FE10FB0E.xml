<collectingCountry id="F371F4BFAA51FF8EFB1BFBA8FEFBFB0E" name="Costa Rica" pageId="27" pageNumber="28">Costa Rica</collectingCountry>
to 
<collectingCountry id="F371F4BFAA51FF8EFE9FFB70FE14FB0E" box="[355,466,1156,1182]" name="Paraguay" pageId="27" pageNumber="28">Paraguay</collectingCountry>
