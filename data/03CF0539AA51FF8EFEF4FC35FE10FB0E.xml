<taxonomicName id="4C66CFACAA51FF8EFC73FBA8FBE0FBE5" baseAuthorityName="Kieffer" baseAuthorityYear="1910" box="[911,1062,1116,1141]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="punctatus">
