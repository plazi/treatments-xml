<paragraph id="8BB17B0B8566145CE7C6FBFFFE3A03F0" blockId="12.[151,484,1148,1174]" box="[151,484,1148,1174]" pageId="12" pageNumber="357">
<heading id="D0F9CC678566145CE7C6FBFFFE3A03F0" bold="true" box="[151,484,1148,1174]" fontSize="11" level="1" pageId="12" pageNumber="357" reason="1">
<emphasis id="B97AA7198566145CE7C6FBFFFE3A03F0" bold="true" box="[151,484,1148,1174]" pageId="12" pageNumber="357">
Genus 
<taxonomicName id="4C0E00888566145CE7BCFBFFFEB003F0" box="[237,366,1148,1174]" class="Elasmobranchii" family="Potamotrygonidae" genus="Telatrygon" kingdom="Animalia" order="Myliobatiformes" pageId="12" pageNumber="357" phylum="Chordata" rank="genus" status="gen. nov.">
<emphasis id="B97AA7198566145CE7BCFBFFFEB003F0" bold="true" box="[237,366,1148,1174]" italics="true" pageId="12" pageNumber="357">Telatrygon</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2491A628566145CE627FBFFFE3A03F0" box="[374,484,1148,1174]" pageId="12" pageNumber="357" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
