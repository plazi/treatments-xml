<taxonomicName id="4C6C4D76FFD2FF89FF66CCB271A9FB65" box="[151,450,1227,1251]" class="Malacostraca" family="Callianassidae" genus="Lepidophthalmus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="581" phylum="Arthropoda" rank="species" species="bocourti">
