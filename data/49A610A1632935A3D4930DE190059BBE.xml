<taxonomicName id="E124CF776907D6D1FF606949CBF943BA" class="Gastropoda" family="Megaspiridae" genus="Thaumastus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thaumastus (Thaumastus) flori" order="Stylommatophora" pageId="31" pageNumber="32" phylum="Mollusca" rank="species" species="flori" subGenus="Thaumastus">Thaumastus (Thaumastus) flori</taxonomicName>
