<taxonomicName id="4C684D02437CC717FBD3FEEEFAB0031E" authority="Westwood" authorityName="Westwood" box="[1143,1393,359,380]" class="Insecta" family="Ptychopteridae" genus="Bittacomorpha" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
