<taxonomicName id="4C684D02437CC717FC13FB9DFA5A064B" authority="Kang, Wang &amp; Yang" authorityName="Kang, Wang &amp; Yang" authorityYear="2012" box="[951,1435,1044,1065]" class="Insecta" family="Ptychopteridae" genus="Bittacomorphella" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="gongshana">
