<taxonomicName id="4C3605DFFFB0FF80285F6DB4FC31FF74" authority="Russell" authorityName="Russell" authorityYear="1965" box="[634,967,150,176]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="species" species="dispersus">
<emphasis id="B942A24EFFB0FF80285F6DB4FC86FF74" box="[634,880,150,176]" italics="true" pageId="5" pageNumber="550">Aleurodicus dispersus</emphasis>
Russell
</taxonomicName>
(
<taxonomicName id="4C3605DFFFB0FF8029F16DB4FBA6FF74" box="[980,1104,150,176]" class="Insecta" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
: 
<taxonomicName id="4C3605DFFFB0FF802E726DB4FB11FF75" authorityName="Westwood" authorityYear="1840" box="[1111,1255,150,177]" class="Insecta" family="Aleyrodidae" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="family">Aleyrodidae</taxonomicName>
); 
<taxonomicName id="4C3605DFFFB0FF802EDD6DB5FEC2FF11" authority="(Cockerell)" baseAuthorityName="Cockerell" baseAuthorityYear="1893" class="Insecta" family="Pseudococcidae" genus="Ferrisia" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="species" species="virgata">
<emphasis id="B942A24EFFB0FF802EDD6DB5FF5BFF10" italics="true" pageId="5" pageNumber="550">Ferrisia virgata</emphasis>
(Cockerell)
</taxonomicName>
(
<taxonomicName id="4C3605DFFFB0FF802B646D98FE4BFF10" box="[321,445,186,212]" class="Insecta" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
: 
<taxonomicName id="4C3605DFFFB0FF802BED6D98FD76FF11" box="[456,640,186,213]" class="Insecta" family="Pseudococcidae" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="family">Pseudococcidae</taxonomicName>
); associated with mealybugs infesting coffee, garden plants, hedgerows and avenue trees such as 
<taxonomicName id="4C3605DFFFB0FF802B946DFCFD37FF3C" box="[433,705,222,248]" class="Magnoliopsida" family="Bignoniaceae" genus="Spathodea" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="550" phylum="Tracheophyta" rank="species" species="campanulata">
