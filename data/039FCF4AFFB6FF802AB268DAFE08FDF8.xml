<taxonomicName id="4C3605DFFFB0FF8029156C20FC3FFED9" box="[816,969,258,285]" class="Insecta" family="Coccinellidae" genus="Horniolus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="550" phylum="Arthropoda" rank="species" species="undetermined">
