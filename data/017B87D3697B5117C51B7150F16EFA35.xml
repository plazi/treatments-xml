<quantity id="4E2A9B20697B5117C68A71D3F174FC73" box="[745,860,950,974]" metricMagnitude="-2" metricUnit="m" metricValue="1.871" pageId="35" pageNumber="2545" unit="mm" value="18.71">18.71 mm</quantity>
