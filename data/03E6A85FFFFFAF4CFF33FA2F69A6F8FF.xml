<subSubSection id="C3554AC2FFFFAF4CFF33F9F86F2DF973" lastPageId="12" lastPageNumber="13" pageId="11" pageNumber="12" type="materials_examined">
<paragraph id="8BF01949FFFFAF4BFF33F9F86F48F9BE" blockId="11.[151,1353,1594,1618]" box="[151,1353,1594,1618]" pageId="11" pageNumber="12">
<emphasis id="B93BC55BFFFFAF4BFF33F9F86AF3F9BE" bold="true" box="[151,242,1594,1618]" pageId="11" pageNumber="12">Type:—</emphasis>
<materialsCitation id="3B271314FFFFAF4BFF56F9F96F44F9BE" box="[242,1349,1595,1618]" collectionCode="SHTU" country="China" location="Kalakule Lake" municipality="Kunlun Mountains" pageId="11" pageNumber="12" specimenCount="1" typeStatus="holotype">
<collectingCountry id="F35859D9FFFFAF4BFF56F9F96B4BF9BE" box="[242,330,1595,1618]" name="China" pageId="11" pageNumber="12">CHINA</collectingCountry>
. 
<location id="8E904F92FFFFAF4BFEF0F9F96BF3F9BE" LSID="urn:lsid:plazi:treatment:03E6A85FFFFFAF4CFF33FA2F69A6F8FF:8E904F92FFFFAF4BFEF0F9F96BF3F9BE" box="[340,498,1595,1618]" country="China" municipality="Kunlun Mountains" name="Kalakule Lake" pageId="11" pageNumber="12">Kalakule Lake</location>
