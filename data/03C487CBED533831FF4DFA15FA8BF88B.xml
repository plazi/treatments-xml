<paragraph id="8BD236DDED553831FC9AF98CFA8BF88B" blockId="516.[810,1452,1601,1866]" pageId="516" pageNumber="517">
<taxonomicName id="4C6D4D5EED553831FC9AF98CFBDDF99A" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[863,1048,1601,1627]" class="Ascidiacea" family="Didemnidae" genus="Notoixys" kingdom="Animalia" order="Enterogona" pageId="516" pageNumber="517" phylum="Chordata" rank="species" species="hirsuta" status="gen. et sp. nov.">
<emphasis id="B919EACFED553831FC9AF98CFBDDF99A" box="[863,1048,1601,1627]" italics="true" pageId="516" pageNumber="517">Notoixys hirsuta</emphasis>
</taxonomicName>
<emphasis id="B919EACFED553831FBE4F98CFB16F99D" bold="true" box="[1057,1235,1602,1628]" pageId="516" pageNumber="517">
<taxonomicNameLabel id="A22A57B4ED553831FBE4F98CFB16F99D" box="[1057,1235,1602,1628]" pageId="516" pageNumber="517" rank="species">gen. et sp. nov.</taxonomicNameLabel>
</emphasis>
