<caption id="DF566662FFC6491DFF66D4C277AEF9D1" box="[151,1273,1534,1558]" httpUri="https://zenodo.org/record/183238/files/figure.png" pageId="29" pageNumber="30" targetBox="[152,1360,188,1533]" targetPageId="29">
<paragraph id="8B9636EAFFC6491DFF66D4C277AEF9D1" blockId="29.[151,1273,1534,1558]" box="[151,1273,1534,1558]" pageId="29" pageNumber="30">
<emphasis id="B95DEAF8FFC6491DFF66D4C27271F9D2" bold="true" box="[151,294,1534,1558]" pageId="29" pageNumber="30">FIGURE 38.</emphasis>
Distribution map of 
<taxonomicName id="4C294D69FFC6491DFDF4D4C271CFF9D1" box="[517,664,1534,1557]" class="Insecta" family="Protoneuridae" genus="Epipotoneura" kingdom="Animalia" order="Odonata" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis id="B95DEAF8FFC6491DFDF4D4C271CFF9D1" box="[517,664,1534,1557]" italics="true" pageId="29" pageNumber="30">Epipotoneura</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C294D69FFC6491DFD3DD4C270B7F9D1" box="[716,992,1534,1557]" class="Insecta" family="Protoneuridae" genus="Epipleoneura" kingdom="Animalia" order="Odonata" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="demarmelsi">
<emphasis id="B95DEAF8FFC6491DFD3DD4C270B7F9D1" box="[716,992,1534,1557]" italics="true" pageId="29" pageNumber="30">Epipleoneura demarmelsi</emphasis>
</taxonomicName>
