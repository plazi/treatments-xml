<taxonomicName id="4C16BABF953DFFFAF879FA85760BFB69" box="[1291,1382,1280,1307]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="56" pageNumber="57" phylum="Tracheophyta" rank="species" species="laeta">
<emphasis id="B9621D2E953DFFFAF879FA85760BFB69" box="[1291,1382,1280,1307]" italics="true" pageId="56" pageNumber="57">Q. laeta</emphasis>
</taxonomicName>
, and from galls induced by undetermined species of 
<taxonomicName id="4C16BABF953DFFFAFFCAFAA17062FB4C" authorityName="Kinsey" authorityYear="1930" box="[696,783,1317,1343]" class="Insecta" family="Cynipidae" genus="Atrusca" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">
<emphasis id="B9621D2E953DFFFAFFCAFAA17062FB4C" box="[696,783,1317,1343]" italics="true" pageId="56" pageNumber="57">Atrusca</emphasis>
</taxonomicName>
on 
<taxonomicName id="4C16BABF953DFFFAFE4FFAA170F4FB4D" box="[829,921,1316,1343]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="56" pageNumber="57" phylum="Tracheophyta" rank="species" species="laeta">
<emphasis id="B9621D2E953DFFFAFE4FFAA1703CFB4C" box="[829,849,1317,1343]" italics="true" pageId="56" pageNumber="57">Q</emphasis>
. 
<emphasis id="B9621D2E953DFFFAFE13FAA070F4FB4D" box="[865,921,1316,1342]" italics="true" pageId="56" pageNumber="57">laeta</emphasis>
</taxonomicName>
