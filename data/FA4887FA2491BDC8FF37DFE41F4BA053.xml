<quantity id="B5199B092491BDC8FC27DC1F1F3DA175" box="[903,1026,605,628]" metricMagnitude="2" metricUnit="m" metricValue="7.5" metricValueMax="8.0" metricValueMin="7.0" pageId="143" pageNumber="144" unit="m" value="750.0" valueMax="800.0" valueMin="700.0">700–800 m</quantity>
