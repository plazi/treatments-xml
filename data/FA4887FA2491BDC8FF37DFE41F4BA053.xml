<quantity id="B5199B092491BDC8FD3FDC1F19EAA175" box="[671,725,605,628]" metricMagnitude="3" metricUnit="m" metricValue="5.0" pageId="143" pageNumber="144" unit="km" value="5.0">5 km</quantity>
