<taxonomicName id="4C6ACC83FFB05556B425F97FFB7F3EB0" box="[1129,1164,1750,1771]" class="Insecta" family="Tarachodidae" genus="Iris" kingdom="Animalia" order="Mantodea" pageId="3" pageNumber="104" phylum="Arthropoda" rank="genus">Iris</taxonomicName>
