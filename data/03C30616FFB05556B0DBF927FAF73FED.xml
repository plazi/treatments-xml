<taxonomicName id="4C6ACC83FFB05556B1E0F97CFD2E3EB0" authority="Dietze, 1906" authorityName="Dietze" authorityYear="1906" box="[428,733,1749,1771]" class="Insecta" family="Geometridae" genus="Eupithecia" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="104" phylum="Arthropoda" rank="species" species="opistographata">
