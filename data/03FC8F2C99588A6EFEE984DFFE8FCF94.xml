<taxonomicName id="4C5545B999588A6FFE268296FDB9CBAD" authorityName="Bedel" authorityYear="1884" box="[474,602,2024,2045]" class="Insecta" family="Curculionidae" genus="Anchonidium" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="38" phylum="Arthropoda" rank="genus">
