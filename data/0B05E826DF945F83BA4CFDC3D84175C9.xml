<taxonomicName id="78262D15BE41379F65BC58EBCC2AA740" class="Gastropoda" family="Ariophantidae" genus="Macrochlamys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Macrochlamys" order="Stylommatophora" pageId="2" pageNumber="3" phylum="Mollusca" rank="genus">
