<taxonomicName id="452DDFC62B93DA028C9BD1C894B589F4" class="Gastropoda" family="Ariophantidae" genus="Macrochlamys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Macrochlamys" order="Stylommatophora" pageId="2" pageNumber="3" phylum="Mollusca" rank="genus">
