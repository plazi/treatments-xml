<taxonomicName id="13392D2F2F6FCA43F477082BD8C9F952" authority="Keifer, 1951" authorityName="Keifer" authorityYear="1951" class="Arachnida" family="Diptilomiopidae" genus="Diptacus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Diptacus mengdaensis" order="Trombidiformes" pageId="19" pageNumber="66" phylum="Arthropoda" rank="species" species="mengdaensis">Diptacus mengdaensis</taxonomicName>
