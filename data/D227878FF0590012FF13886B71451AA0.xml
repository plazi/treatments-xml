<quantity id="9D769B7CF059001DFB818A5D710D1961" box="[1029,1126,1569,1594]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" metricValueMax="6.0" metricValueMin="5.0" pageId="7" pageNumber="378" unit="mm" value="5.5" valueMax="6.0" valueMin="5.0">5–6 mm</quantity>
