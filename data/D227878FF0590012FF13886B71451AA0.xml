<taxonomicName id="9D8E4D1AF0560012FF138869749A1B73" authority="Schluter, 1976" authorityName="Schluter" authorityYear="1976" box="[151,497,1045,1067]" class="Insecta" family="Bombyliidae" genus="Glabellula" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="379" phylum="Arthropoda" rank="species" species="kuehnei">
<emphasis id="68FAEA8BF0560012FF13886974331B73" box="[151,344,1045,1067]" italics="true" pageId="8" pageNumber="379">Glabellula kuehnei</emphasis>
Schlüter, 
<number id="EC4624BEF0560012FE3A8869749A1B73" box="[446,497,1045,1067]" pageId="8" pageNumber="379" value="1976.0">1976</number>
