<taxonomicName id="4C6A4D1EFFBF5044FE78FDA28308FDC6" authority="COPE, 1879" authorityName="COPE" authorityYear="1879" box="[477,845,588,614]" class="Mammalia" family="Hyracodontidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="8" pageNumber="9" phylum="Chordata" rank="family">HYRACODONTIDAE COPE, 1879</taxonomicName>
</heading>
</paragraph>
<paragraph id="8BD5369DFFBF5044FFC9FD6A8123FD3E" blockId="8.[108,1213,644,1194]" box="[108,358,644,670]" pageId="8" pageNumber="9">
<taxonomicName id="4C6A4D1EFFBF5044FFC9FD6A8123FD3E" authority="Cope, 1880" authorityName="Cope" authorityYear="1880" box="[108,358,644,670]" class="Mammalia" family="Hyracodontidae" genus="Triplopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="8" pageNumber="9" phylum="Chordata" rank="genus">
