<taxonomicName id="DB743789B76D3A35E74D4E849C63FFDE" class="Magnoliopsida" family="Asteraceae" genus="Aster" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="544" phylum="Tracheophyta" rank="species" species="salignus">
<italic id="7F17E9DB12A247946673516B2D32CDC8" pageId="null" pageNumber="544">A. salignus</italic>
