<taxonomicName id="757292E08FF35F3170E5BC9BD58B77AC" class="Magnoliopsida" family="Asteraceae" genus="Aster" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="544" phylum="Tracheophyta" rank="genus">Aster</taxonomicName>
