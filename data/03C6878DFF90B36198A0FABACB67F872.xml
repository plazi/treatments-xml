<emphasis id="B91BEA89FF90B36198A0FABACB73FABF" bold="true" box="[151,553,1358,1384]" pageId="5" pageNumber="440">
<taxonomicName id="4C6F4D18FF90B36198A0FABAC89FFABF" authority="Fang &amp; Xing, 2018" authorityName="Fang &amp; Xing" authorityYear="2018" box="[151,453,1358,1384]" class="Insecta" family="Cicadellidae" genus="Longicornus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="440" phylum="Arthropoda" rank="species" species="biprocessus" status="sp. nov.">
<emphasis id="B91BEA89FF90B36198A0FABAC89FFABF" bold="true" box="[151,453,1358,1384]" italics="true" pageId="5" pageNumber="440">Longicornus biprocessus</emphasis>
