2015
</bibRefCitation>
; current record), 
<collectingCountry id="E1BB76354D2B4831FCADF99748A23362" box="[873,979,1632,1658]" name="Bermuda" pageId="32" pageNumber="33">Bermuda</collectingCountry>
, 
<collectingCountry id="E1BB76354D2B4831FC1AF9974F363362" box="[990,1095,1632,1658]" name="Bahamas" pageId="32" pageNumber="33">Bahamas</collectingCountry>
, 
<collectingCountry id="E1BB76354D2B4831FB90F9964F093363" box="[1108,1144,1633,1659]" name="United States of America" pageId="32" pageNumber="33">US</collectingCountry>
