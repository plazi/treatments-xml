<taxonomicName id="705243BD494359B231EA591B3F98476F" authority="Cognato, Smith &amp; Beaver, 2020" authorityName="Cognato, Smith &amp; Beaver" authorityYear="2020" class="Insecta" family="Curculionidae" genus="Fraudatrix" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Fraudatrix" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Fraudatrix</taxonomicName>
