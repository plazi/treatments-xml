<subSubSection id="B1B5652DFFC6FF8844EB09AF112FDB2F" pageId="4" pageNumber="579" type="diagnosis">
<paragraph id="F91036A6FFC6FF8844EB09AF112FDB2F" blockId="4.[151,1437,152,630]" pageId="4" pageNumber="579">
<emphasis id="CBDBEAB4FFC6FF8844EB09AF125AD991" bold="true" box="[199,314,152,179]" pageId="4" pageNumber="579">Diagnosis</emphasis>
. Forewing length is 
<quantity id="3E579B43FFC6FF88460A09AE11C0D991" box="[550,672,153,179]" metricMagnitude="-2" metricUnit="m" metricValue="1.85" metricValueMax="1.9" metricValueMin="1.8" pageId="4" pageNumber="579" unit="mm" value="18.5" valueMax="19.0" valueMin="18.0">18–19 mm</quantity>
