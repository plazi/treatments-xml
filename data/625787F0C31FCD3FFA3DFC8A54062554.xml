<taxonomicName id="2DFE4D65C31ECD3FFA58FD2456CF2362" baseAuthorityName="Becker" baseAuthorityYear="1906" box="[264,432,767,790]" class="Insecta" family="Bombyliidae" genus="Anthrax" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="608" phylum="Arthropoda" rank="species" species="dentatus">
<emphasis id="D88AEAF4C31ECD3FFA58FD2456CF2362" box="[264,432,767,790]" italics="true" pageId="11" pageNumber="608">Anthrax dentatus</emphasis>
