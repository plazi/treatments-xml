<taxonomicName id="3CD64D4F0438FFDAC6E6FA58FE94F990" box="[151,299,1524,1548]" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="556" phylum="Arthropoda" rank="genus">
<emphasis id="C9A2EADE0438FFDAC6E6FA58FE94F990" box="[151,299,1524,1548]" italics="true" pageId="4" pageNumber="556">Phenacoccus</emphasis>
</taxonomicName>
species (
<taxonomicName id="3CD64D4F0438FFDAC7E7FA58FD38F991" authority="Signoret" authorityName="Signoret" box="[406,647,1524,1549]" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="556" phylum="Arthropoda" rank="species" species="aceris">
