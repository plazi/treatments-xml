<taxonomicName id="4C6E4D76A81ADE3A11E5FDD20D0E573C" authority="Fianco, 2021" authorityName="Fianco" authorityYear="2021" box="[1211,1365,582,609]" class="Insecta" family="Phaneropteridae" genus="Anaulacomera" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="47" phylum="Arthropoda" rank="species" species="mediastina" status="sp. n.">
