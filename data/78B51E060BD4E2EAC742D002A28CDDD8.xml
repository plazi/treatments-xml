<bibRefCitation id="D2202F36EE130CB43A47161A2771BBAC" author="Pereira, FS" journalOrPublisher="Arquivos do Museu Nacional Rio de Janeiro" pageId="275" pageNumber="276" pagination="459 - 467" title="Especies novas de Scarabaeidae Brasileiros." volume="42" year="1954 b">Pereira (1954b</bibRefCitation>
: 464), characters in key; 
<bibRefCitation id="B004CAE78A798908B9965C7FD2D28F53" author="Vulcano, MA" journalOrPublisher="Atas do Simposio sobre a Biota Amazonica" pageId="279" pageNumber="280" pagination="533 - 603" title="Sinopse dos Passalidae e Scarabaeidae s. str. da regiao amazonica (Insecta, Coleoptera)." volume="5" year="1967">Vulcano and Pereira (1967</bibRefCitation>
: 584), characters in key; 
<bibRefCitation id="05BC982D9B2EC75109C6F2B5735E8091" author="Carvajal, V" journalOrPublisher="Quito, Ecuado" pageId="264" pageNumber="265" title="Escarabajos del Ecuador. Principales generos. Instituto de Ciencias Biologicas. Escuela Politecnica Nacional. Serie Entomologia, Nro. 1." year="2011">Carvajal et al. (2011</bibRefCitation>
