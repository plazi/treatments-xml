<quantity id="4CC89B67FFFFF707FC886659FC08FAE1" box="[863,955,1376,1400]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="10" pageNumber="536" unit="m" value="1300.0">1300 m</quantity>
, 
