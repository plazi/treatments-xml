<taxonomicName id="82B54D17D244FFC3FF146BC3998AF930" box="[248,464,1692,1716]" class="Insecta" family="Phaneropteridae" genus="Deflorita" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="588" phylum="Arthropoda" rank="species" species="deflorita">
