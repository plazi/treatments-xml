<mcCollectionCode id="9885C9B9FFD4FFF6FDCDFEB1C4B7AD20" box="[603,667,259,283]" country="Netherlands" httpUri="http://grbio.org/cool/47nz-wbm0" name="National Museum of Natural History, Naturalis" pageId="15" pageNumber="416" type="Museum">RGM</mcCollectionCode>
