<taxonomicName id="BD8940B7CF630DF4B7C0EB59D41522C6" authorityName="Yang" authorityYear="1996" class="Insecta" family="Eupelmidae" genus="Metapelma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metapelma beijingense" order="Hymenoptera" pageId="0" pageNumber="53" phylum="Arthropoda" rank="species" species="beijingense">
<emphasis id="ACA3455B0953357AA22EBA0EF455F82B" italics="true" pageId="0" pageNumber="53">Metapelma beijingense</emphasis>
