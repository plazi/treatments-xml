<collectingRegion id="3B3B93E41A0BF4533CDC486B1EC3F881" box="[457,645,1889,1915]" country="United States of America" name="North Carolina" pageId="27" pageNumber="28">
North 
<taxonomicName id="3EFF26851A0BF4533F0D486B1EC3F881" authority=": Haywood Co., Balsam Mtn. picnic" authorityName="Haywood Co., Balsam Mtn. Picnic" box="[536,645,1889,1915]" class="Magnoliopsida" family="Styracaceae" genus="Halesia" kingdom="Plantae" order="Ericales" pageId="27" pageNumber="27" phylum="Tracheophyta" rank="species" species="carolina">Carolina</taxonomicName>
