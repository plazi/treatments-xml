W Oviedo, 
<quantity id="A62B9B0FFFEDFF86FAFD55B1149C31FC" box="[1375,1431,152,178]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="10" pageNumber="37" unit="m" value="10.0">10m</quantity>
; arid thorn forest; 
<date id="156D102AFFEDFF86FEC155E913203194" box="[355,555,192,218]" pageId="10" pageNumber="37" value="1991-11-28" valueMax="1991-12-04" valueMin="1991-11-28">
<number id="D71B24CDFFEDFF86FEC155E9108A3194" box="[355,385,192,218]" pageId="10" pageNumber="37" value="28.0">28</number>
