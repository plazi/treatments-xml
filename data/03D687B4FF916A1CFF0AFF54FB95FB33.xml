<taxonomicName id="4C7F4D21FF916A1CFCA0FBE7FC7EFBF4" authorityName="Gordon and Hanley" authorityYear="2020" box="[874,968,1123,1147]" class="Insecta" family="Coccinellidae" genus="Calloeneis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="sheri">
