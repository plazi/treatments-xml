<quantity id="4C970FED6A77FFE202CCFCCA7B69FCEB" box="[1007,1078,776,798]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="20" pageNumber="953" unit="m" value="1100.0">1100m</quantity>
