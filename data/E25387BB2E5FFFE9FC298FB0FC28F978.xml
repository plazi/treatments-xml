<date id="1E44106D2E5FFFE9FBDB8D15FB29F9C8" box="[1096,1211,1559,1583]" pageId="17" pageNumber="42" value="2008-06-20">20.VI.2008</date>
(
<collectionCode id="0CEBAE682E5FFFE9FB5B8D15FA97F9C8" box="[1224,1285,1559,1583]" pageId="17" pageNumber="42">GBIF</collectionCode>
