<keyLead id="30E91C4DFFABD212D2A6FB20FA3953EA" pageId="1" pageNumber="388">
<number id="3DD0AD5FFFABD212D2A6FB20FF0753F5" box="[151,165,1196,1217]" pageId="1" pageNumber="388" value="6.0">6</number>
