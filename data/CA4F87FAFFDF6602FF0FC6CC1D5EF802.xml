<taxonomicName id="85E64D6FFFDF6600FBBFC41A1988F8FB" authority="Xu &amp; Li, 2008" authorityName="Xu &amp; Li" authorityYear="2008" box="[1063,1436,1806,1830]" class="Arachnida" family="Agelenidae" genus="Draconarius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="0" pageNumber="397" phylum="Arthropoda" rank="species" species="pseudocoreanus">
