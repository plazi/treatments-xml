<taxonomicName id="4C1EBEE7FFC3C319FE2607278E28FAAA" box="[459,570,1329,1352]" class="Aves" family="Tytonidae" genus="Tyto" kingdom="Animalia" order="Strigiformes" pageId="6" pageNumber="57" phylum="Chordata" rank="species" species="almae">
