<taxonomicName id="4C2901BE04541507FEB74C15FE03FDA2" authorityName="Deshayes" authorityYear="1834" box="[404,488,572,598]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="93" pageNumber="94" phylum="Mollusca" rank="genus">
