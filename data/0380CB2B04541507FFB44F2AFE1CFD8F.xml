<taxonomicName id="4C2901BE04541507FB6D4C15FAF7FDA2" authorityName="Gould" authorityYear="1861" box="[1102,1308,572,598]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="93" pageNumber="94" phylum="Mollusca" rank="species" species="interfilata">
