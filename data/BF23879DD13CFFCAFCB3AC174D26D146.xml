Carusioidea + 
<taxonomicName id="F08A4D08D13CFFD5FC60AC174B67D2BF" baseAuthorityName="Alifanov" baseAuthorityYear="2000" box="[960,1073,624,646]" class="Reptilia" family="Anguidae" kingdom="Animalia" order="Squamata" pageId="111" pageNumber="112" phylum="Chordata" rank="family">Anguidae</taxonomicName>
