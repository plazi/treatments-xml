<number id="D219D26B6372FF8AFE5B442CFE3DF9CC" box="[399,443,1570,1596]" pageId="30" pageNumber="31" value="760.0">760</number>
long, 
<number id="D219D26B6372FF8AFDDC442CFDB2F9CC" box="[520,564,1570,1596]" pageId="30" pageNumber="31" value="590.0">590</number>
