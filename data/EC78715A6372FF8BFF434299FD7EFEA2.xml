long; 
<emphasis id="56A51C5E6372FF8AFE414445FE31F994" box="[405,439,1611,1636]" italics="true" pageId="30" pageNumber="31">
st 
<number id="D219D26B6372FF8AFE7D4445FE31F994" box="[425,439,1611,1636]" pageId="30" pageNumber="31" value="3.0">3</number>
</emphasis>
