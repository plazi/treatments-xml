<taxonomicName id="8FEE4D41FF09FF22FC90F94D6167CC43" authorityName="Walker, Pegram and Keirans" authorityYear="1995" box="[768,1060,1770,1796]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="224" pageNumber="225" phylum="Arthropoda" rank="species" species="interventus">
