<taxonomicName id="8FEE4D41FF09FF22FE46F8A967A6CC6F" authorityName="in Saratsiotis" authorityYear="1977" box="[470,741,1806,1832]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="224" pageNumber="225" phylum="Arthropoda" rank="species" species="lunulatus">
