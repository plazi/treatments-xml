<typeStatus id="54C2479FFF9CC515FF77FB23FE81FA53" box="[161,297,1405,1431]" pageId="16" pageNumber="17">Lectotypus</typeStatus>
, Ƥ, 
<taxonomicName id="4C7982BEFF9CC515FE8DFB23FD65FA53" authority="Petri., Gultekin" authorityName="Petri., Gultekin" box="[347,717,1405,1431]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="colchicus">
