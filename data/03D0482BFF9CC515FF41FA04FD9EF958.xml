<typeStatus id="54C2479FFF9CC515FF77FB06FE8AFAB6" box="[161,290,1368,1394]" pageId="16" pageNumber="17">Holotypus</typeStatus>
, 
<number id="3DB1EB1AFF9CC515FEF8FB06FEC4FAB6" box="[302,364,1368,1394]" pageId="16" pageNumber="17" value="1904.0">1904</number>
, 
<taxonomicName id="4C7982BEFF9CC515FEAFFB06FDDCFAB6" authority="Petri" authorityName="Petri" box="[377,628,1368,1394]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="colchicus">
<emphasis id="B90D252FFF9CC515FEAFFB06FD98FAB5" box="[377,560,1368,1393]" italics="true" pageId="16" pageNumber="17">Lixus colchicus</emphasis>
