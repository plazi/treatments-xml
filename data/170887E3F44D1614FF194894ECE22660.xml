<number id="296924D2F44D1614FBFF4996EBB123E0" box="[1137,1197,405,429]" pageId="11" pageNumber="12" value="2011.0">2011</number>
</date>
, Ant. 
<number id="296924D2F44D1614FB674996EA1F23E0" box="[1257,1283,405,429]" pageId="11" pageNumber="12" value="10.0">10</number>
