<taxonomicName id="4C264D7CFFB6542C9AAF32DFFD6DFACA" authority="Huang" authorityName="Huang" authorityYear="1994" box="[446,764,1272,1297]" class="Insecta" family="Aphelinidae" genus="Coccobius" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="366" phylum="Arthropoda" rank="species" species="abdominis">
