<taxonomicName id="4C264D7CFFB6542C9B9E32DFFED3FACB" authorityName="Huang" authorityYear="1994" box="[143,322,1272,1296]" class="Insecta" family="Aphelinidae" genus="Coccobius" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="366" phylum="Arthropoda" rank="species" species="chaoi">
