<taxonomicName id="FBDD4D4880158166FBEFFB2DFC281F8D" box="[649,898,1184,1208]" class="Secernentea" family="Hoplolaimidae" genus="Hoplolaimus" kingdom="Animalia" order="Tylenchida" pageId="5" pageNumber="6" phylum="Nematoda" rank="species" species="galeatus">
