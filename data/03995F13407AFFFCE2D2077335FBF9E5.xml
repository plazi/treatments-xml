<taxonomicName id="4C309586407AFFFCE2D20773361DFA34" box="[151,311,1400,1426]" class="Eurotatoria" family="Notommatidae" genus="Cephalodella" kingdom="Animalia" order="Ploima" pageId="33" pageNumber="184" phylum="Rotifera" rank="genus">
