<taxonomicName id="E2BD71B89D161C4632A419F4FE11FA54" authority="Pritchard" authorityName="Pritchard" box="[238,497,1449,1475]" class="Insecta" family="Cecidomyiidae" genus="Acoenonia" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="290" phylum="Arthropoda" rank="genus">
