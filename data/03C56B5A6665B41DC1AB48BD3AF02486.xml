</taxonomicName>
, 
<taxonomicName id="4C6CA1CF6665B41DC1D14EEB397624E2" baseAuthorityName="Solodovnikova" baseAuthorityYear="1963" box="[237,364,1435,1459]" class="Magnoliopsida" family="Fabaceae" genus="Genista" kingdom="Plantae" order="Fabales" pageId="48" pageNumber="49" phylum="Tracheophyta" rank="species" species="tinctoria">
