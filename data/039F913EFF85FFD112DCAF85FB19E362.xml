<taxonomicName id="4C365BABFF85FFD110C2AED5FD76E0DB" authorityName="Cook" authorityYear="1958" box="[649,758,1509,1534]" class="Insecta" family="Scatopsidae" genus="Psectrosciara" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="253" phylum="Arthropoda" rank="species" species="serrata">
