<quantity id="4C9FB5EC8C21B91AA3EB64F8FDD6F7F8" box="[444,526,664,687]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="6" pageNumber="7" unit="mm" value="5.5">5.5 mm</quantity>
) .................................................................................................................................. 
