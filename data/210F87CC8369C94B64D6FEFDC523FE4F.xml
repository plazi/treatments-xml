<taxonomicName id="6EA64D598369C94B60C2FE6DC3A1FE6B" authorityName="J.C.Fabricius" authorityYear="1794" box="[1155,1262,439,465]" class="Pycnogonida" family="Nymphonidae" genus="Nymphon" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="4" pageNumber="551" phylum="Arthropoda" rank="genus">
