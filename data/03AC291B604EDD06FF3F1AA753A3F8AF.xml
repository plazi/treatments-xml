<quantity id="4CFD35E86049DD06FDA91FB75046FC2A" box="[513,606,972,998]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="5" pageNumber="39" unit="mm" value="1.2">1.2 mm</quantity>
; ultimate section of CuA 
<number id="3DCD8A2A6049DD06FC2A1FA75192FC26" box="[898,906,988,1002]" pageId="5" pageNumber="39" value="1.0">1</number>
