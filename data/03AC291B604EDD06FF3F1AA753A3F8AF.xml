<quantity id="4CFD35E86049DD06FF691FA752F0FC2A" box="[193,232,972,1002]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="5" pageNumber="39" unit="in" value="2.0">2 in</quantity>
