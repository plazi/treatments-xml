<taxonomicName id="25E24D23FF84AF53FEE1F9890B66E828" authority="Kakui &amp; Tomioka, 2018" authorityName="Kakui &amp; Tomioka" authorityYear="2018" box="[367,531,1564,1588]" class="Malacostraca" family="Anarthruridae" genus="Tsuranarthrura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="62" phylum="Arthropoda" rank="genus" status="gen. nov.">
