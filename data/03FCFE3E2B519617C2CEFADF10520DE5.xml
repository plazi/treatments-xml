<taxonomicName id="4C5534AB2B509618C09EFB24165308CE" authority="Smetana, 2010" authorityName="Smetana" authorityYear="2010" box="[1003,1089,1217,1240]" class="Insecta" family="Staphylinidae" genus="Tasgius" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="152" phylum="Arthropoda" rank="species" species="pugio" status="sp. nov.">
<emphasis id="B921933A2B509618C09EFB2411E808CE" box="[1003,1018,1217,1240]" italics="true" pageId="7" pageNumber="152">T</emphasis>
. 
<emphasis id="B921933A2B509618C770FB24165308CE" box="[1029,1089,1217,1240]" italics="true" pageId="7" pageNumber="152">pugio</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2122E412B509618C304FB0412D608EE" box="[113,196,1249,1272]" pageId="7" pageNumber="152" rank="species">sp. nov.</taxonomicNameLabel>
