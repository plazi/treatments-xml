<taxonomicName id="4C5534AB2B509618C258FBC713D10822" authority="Smetana, 2010" authorityName="Smetana" authorityYear="2010" box="[301,451,1058,1076]" class="Insecta" family="Staphylinidae" genus="Tasgius" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="152" phylum="Arthropoda" rank="species" species="congener" status="sp. nov.">
