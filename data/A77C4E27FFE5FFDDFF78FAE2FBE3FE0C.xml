<taxonomicName id="E8D584B2FFE5FFDCFE3FF8F5FB30F8CF" box="[464,613,1848,1873]" class="Gastropoda" family="Pyramidellidae" genus="Kongsrudia" kingdom="Animalia" pageId="2" pageNumber="3" phylum="Mollusca" rank="genus">
<emphasis id="1DA12323FFE5FFDCFE3FF8F5FB30F8CF" box="[464,613,1848,1873]" italics="true" pageId="2" pageNumber="3">Kongsrudia</emphasis>
</taxonomicName>
: 
<taxonomicName id="E8D584B2FFE5FFDCFD97F8F5FDE4F8CC" authority="Dautzenberg, 1913" authorityName="Dautzenberg" authorityYear="1913" box="[632,1201,1848,1874]" class="Gastropoda" family="Pyramidellidae" genus="Pyrgulina" kingdom="Animalia" pageId="2" pageNumber="3" phylum="Mollusca" rank="species" species="approximans">
