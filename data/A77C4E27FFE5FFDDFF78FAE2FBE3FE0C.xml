<taxonomicName id="E8D584B2FFE4FFDDFF59FEE0F87FFED8" box="[182,298,301,326]" class="Gastropoda" family="Pyramidellidae" genus="Pyrgulina" kingdom="Animalia" pageId="3" pageNumber="4" phylum="Mollusca" rank="genus">
