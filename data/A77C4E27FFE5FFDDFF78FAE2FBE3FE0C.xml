</emphasis>
<taxonomicName id="E8D584B2FFE5FFDCFE9EF96EFA34F922" authority="Dautzenberg, 1910" authorityName="Dautzenberg" authorityYear="1910" box="[369,865,1698,1724]" class="Gastropoda" family="Pyramidellidae" genus="Actaeopyramis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Entomotaeniata" pageId="2" pageNumber="3" phylum="Mollusca" rank="species" species="gruveli">
