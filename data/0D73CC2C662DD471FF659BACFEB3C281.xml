<taxonomicName id="42DA06B9662DD477FEE59BACFDA1C471" authority="Barkalov" authorityName="Barkalov" authorityYear="2002" box="[279,554,1519,1546]" class="Insecta" family="Syrphidae" genus="Convocheila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="105" phylum="Arthropoda" rank="genus">
