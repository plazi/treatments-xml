<taxonomicName id="ADCD4D04FFA2FFCCEF85FE91FDD7FEDB" authority=", Madrid" authorityName="Madrid" box="[416,554,296,323]" class="Aves" family="Psittacidae" genus="Eos" kingdom="Animalia" order="Psittaciformes" pageId="3" pageNumber="581" phylum="Chordata" rank="genus">Eos, Madrid</taxonomicName>
