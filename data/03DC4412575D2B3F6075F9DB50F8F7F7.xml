<collectingCountry id="F362B594575B2B3F6284FCA45532F1DE" box="[213,330,879,903]" name="Argentina" pageId="4" pageNumber="1861">Argentina</collectingCountry>
, 
<collectingCountry id="F362B594575B2B3F6304FCA455E0F1DE" box="[341,408,879,903]" name="Brazil" pageId="4" pageNumber="1861">Brazil</collectingCountry>
