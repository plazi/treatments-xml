<quantity id="4CDA8B9FFF8CFFE9CAEBCCB7165DCBA9" box="[677,770,1426,1453]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="7" pageNumber="101" unit="mm" value="3.8">3.8 mm</quantity>
