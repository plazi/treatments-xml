<taxonomicName id="558F0FD7FF09497DFD3D8931D7C1FB7C" authority="Schaeffer." authorityName="Schaeffer." box="[685,990,1142,1163]" class="Insecta" family="Cerambycidae" genus="Neoclytus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="744" phylum="Arthropoda" rank="species" species="magnus">
<emphasis id="A0FBA846FF09497DFD3D8931D7C1FB7C" bold="true" box="[685,990,1142,1163]" pageId="6" pageNumber="744">
<emphasis id="A0FBA846FF09497DFD3D8931D774FB7C" bold="true" box="[685,875,1142,1163]" italics="true" pageId="6" pageNumber="744">Neoclytus magnus</emphasis>
Schaeffer.
</emphasis>
</taxonomicName>
New larval host
</heading>
</paragraph>
<paragraph id="92307454FF09497DFD5889D6D70AFB51" blockId="6.[685,1173,1142,1432]" box="[712,789,1169,1190]" pageId="6" pageNumber="744">records.</paragraph>
