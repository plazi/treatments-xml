<taxonomicName id="4C524D45D40D2E5DF22EF899FA505BF4" authority="Crasquin, 2010" authorityName="Crasquin" authorityYear="2010" box="[1103,1453,1905,1931]" class="Ostracoda" family="Bairdiidae" genus="Bairdia" kingdom="Animalia" order="Podocopida" pageId="11" pageNumber="662" phylum="Arthropoda" rank="species" species="bassoni">
