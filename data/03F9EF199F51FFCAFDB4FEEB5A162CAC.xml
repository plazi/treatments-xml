<taxonomicName id="4C50258C9F5EFFCAFDFFFD4259FA2CCF" authorityName="Champion" authorityYear="1924" box="[541,710,674,700]" class="Insecta" family="Limnichidae" genus="Acontosceles" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="hydroporoides">
