<taxonomicName id="4C4C4D16FFDFFF8D4714FBAFFD13FBD2" authority="Karsch 1890: 122" authorityName="Karsch" authorityPageNumber="122" authorityYear="1890" box="[313,729,1108,1133]" class="Insecta" family="Cicadidae" genus="Ligymolpa" kingdom="Animalia" order="Hemiptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="madegassa">
