<taxonomicName id="4C434D520A07A55581434375FE054F04" authority="Vilkamaa &amp; Suuronen, 2019" authorityName="Vilkamaa &amp; Suuronen" authorityYear="2019" box="[281,465,1639,1660]" class="Insecta" family="Sciaridae" genus="Bradysia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="157" phylum="Arthropoda" rank="species" species="conflexa" status="sp. n.">
