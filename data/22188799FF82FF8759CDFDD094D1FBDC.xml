<collectingCountry id="D2A6761FFF82FF87583DFBCB97D6FBDC" box="[359,429,1116,1140]" name="China" pageId="8" pageNumber="9">China</collectingCountry>
, 
<collectingCountry id="D2A6761FFF82FF8758E2FBCA9784FBDC" box="[440,511,1117,1140]" name="South Korea" pageId="8" pageNumber="9">Korea</collectingCountry>
, 
<collectingCountry id="D2A6761FFF82FF875B50FBCA9437FBDC" box="[522,588,1117,1140]" name="Japan" pageId="8" pageNumber="9">Japan</collectingCountry>
, 
