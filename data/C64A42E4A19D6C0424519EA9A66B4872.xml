<paragraph id="56A760C6505E01D25A06E51975DFAA0B" pageId="31" pageNumber="32">Type species.</paragraph>
<paragraph id="7E9E82410B94D8DD1F9A330FC3FB4063" pageId="31" pageNumber="32">
<taxonomicName id="2EB0988C0CACFC60B23C875904D727E5" class="Insecta" family="Curculionidae" genus="Laparocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Laparocerus inaequalis" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="inaequalis">Laparocerus inaequalis</taxonomicName>
