<paragraph id="8BB9252AFFF1FF8F0087524EFB9F59D9" blockId="34.[151,1437,319,1928]" pageId="34" pageNumber="194">
<emphasis id="B972F938FFF1FF8F0087524EFE705932" bold="true" box="[199,507,571,596]" pageId="34" pageNumber="194">
<typeStatus id="54BD9B88FFF1FF8F0087524EFE895932" box="[199,258,572,596]" pageId="34" pageNumber="194">Type</typeStatus>
