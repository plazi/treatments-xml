<paragraph id="3A77C2D885B0395D1E30D7C02249352C" pageNumber="192">
etymology Gr 
<taxonomicName id="D9DE367EF0A2E8F161FD869BEB6F4C41" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181670" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Pheidole gagates Wilson" order="Hymenoptera" pageNumber="192" phylum="Arthropoda" rank="species" species="gagates">gagates</taxonomicName>
