<taxonomicName id="29904D53FFA5A603B7F9FA4DFCC2FA6A" authority="Rathbun, 1878" authorityName="Rathbun" authorityYear="1878" box="[406,775,1463,1490]" class="Malacostraca" family="Inachoididae" genus="Collodes" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="armatus">
