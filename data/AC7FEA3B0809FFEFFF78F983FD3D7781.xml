<emphasis id="16A2873F0808FFECFD36F910FD26707E" bold="true" box="[729,754,1774,1798]" pageId="5" pageNumber="6">C.</emphasis>
<taxonomicName id="E3D620AE0808FFECFD14F911FC5C707E" authority="Yasunaga, 2022" authorityName="Yasunaga" authorityYear="2022" box="[763,904,1774,1798]" class="Insecta" family="Miridae" genus="Eremophylus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="nakagawai" status="n. sp.">
