<collectionCode id="ED740CABFFBC1443FE8AF3A4FEE7E2EC" LSID="urn:lsid:biocol.org:col:14518" box="[281,335,1195,1219]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14518" name="University of Helsinki" pageId="9" pageNumber="160">HEL</collectionCode>
