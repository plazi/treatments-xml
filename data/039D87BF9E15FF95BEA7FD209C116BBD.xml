<taxonomicName id="4C344D2A9E14FF91BEDDFAE09CAA68B1" authority="Bruce, 2009" authorityName="Bruce" authorityYear="2009" box="[246,394,1398,1419]" class="Malacostraca" family="Serolidae" genus="Myopiarolis" kingdom="Animalia" order="Isopoda" pageId="25" pageNumber="42" phylum="Arthropoda" rank="species" species="koro" status="sp. n.">
