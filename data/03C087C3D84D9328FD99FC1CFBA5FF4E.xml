</taxonomicName>
<taxonomicName id="4C694D56D84E9328FDF1FB94FD01FD4D" box="[636,746,1092,1111]" class="Insecta" family="Dytiscidae" genus="Ilybius" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="54" phylum="Arthropoda" rank="species" species="biguttulus">
