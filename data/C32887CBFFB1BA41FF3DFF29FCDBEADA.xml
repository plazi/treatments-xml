<paragraph id="4B3E36DDFFB1BA42FF3DFF29FDB7EADA" blockId="33.[151,602,222,282]" box="[151,602,222,248]" pageId="33" pageNumber="34">
<heading id="107681B1FFB1BA42FF3DFF29FDB7EADA" bold="true" box="[151,602,222,248]" fontSize="11" level="1" pageId="33" pageNumber="34" reason="1">
<taxonomicName id="8C814D5EFFB1BA42FF3DFF29FDB7EADA" authority="de Winton, 1902" authorityName="de Winton" authorityYear="1902" box="[151,602,222,248]" class="Mammalia" family="Muridae" genus="Gerbillus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="33" pageNumber="34" phylum="Chordata" rank="species" species="andersoni">
