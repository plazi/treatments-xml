<emphasis id="B9480707A34F7F5FFB9CFE3B8F21D89C" box="[1132,1149,405,428]" italics="true" pageId="34" pageNumber="335">B</emphasis>
.) 
<emphasis id="B9480707A34F7F5FFB68FE3B8FADD89C" box="[1176,1265,405,428]" italics="true" pageId="34" pageNumber="335">minutus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27BBA7CA34F7F5FFB0CFE3A8E1CD89D" box="[1276,1344,404,429]" pageId="34" pageNumber="335" rank="species">
<emphasis id="B9480707A34F7F5FFB0CFE3A8E1CD89D" bold="true" box="[1276,1344,404,429]" pageId="34" pageNumber="335">sp. n.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="4C3CA096A34F7F5FFAA1FE3B8A42D8E0" authority="Schmidt, 2017" authorityName="Schmidt" authorityYear="2017" class="Insecta" family="Carabidae" genus="Trichotichnus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="335" phylum="Arthropoda" rank="species" species="ganeshensis" status="sp. n." subGenus="Bottchrus">
<emphasis id="B9480707A34F7F5FFAA1FE3B8E3AD89C" box="[1361,1382,405,428]" italics="true" pageId="34" pageNumber="335">T.</emphasis>
(
<emphasis id="B9480707A34F7F5FFA8AFE3B8ED7D89C" box="[1402,1419,405,428]" italics="true" pageId="34" pageNumber="335">B</emphasis>
.) 
<emphasis id="B9480707A34F7F5FFF67FE198A42D8E0" box="[151,286,439,464]" italics="true" pageId="34" pageNumber="335">ganeshensis</emphasis>
</taxonomicName>
<emphasis id="B9480707A34F7F5FFED5FE198A30D8E0" bold="true" box="[293,364,439,464]" pageId="34" pageNumber="335">
<taxonomicNameLabel id="A27BBA7CA34F7F5FFED5FE198A3AD8E0" box="[293,358,439,464]" pageId="34" pageNumber="335" rank="species">sp. n.</taxonomicNameLabel>
,
</emphasis>
and 
<taxonomicName id="4C3CA096A34F7F5FFE54FE178915D8E0" authority="Schmidt, 2017" authorityName="Schmidt" authorityYear="2017" box="[420,585,439,464]" class="Insecta" family="Carabidae" genus="Trichotichnus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="335" phylum="Arthropoda" rank="species" species="pusillus" status="sp. n." subGenus="Bottchrus">
<emphasis id="B9480707A34F7F5FFE54FE178AEBD8E0" box="[420,439,441,464]" italics="true" pageId="34" pageNumber="335">T.</emphasis>
(
<emphasis id="B9480707A34F7F5FFE39FE178A86D8E0" box="[457,474,441,464]" italics="true" pageId="34" pageNumber="335">B</emphasis>
.) 
