<taxonomicName id="4D8D4D2AFFC6FF84FD67FE68FCEDFE41" authorityName="Krefft" authorityYear="1871" box="[724,859,393,419]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="27" pageNumber="203" phylum="Nematoda" rank="genus">gnathosoma</taxonomicName>
