<quantity id="4D759B4CFFC6FF84FEC0FE68FE1AFE41" box="[371,428,393,419]" metricMagnitude="0" metricUnit="m" metricValue="2.2352" pageId="27" pageNumber="228" unit="in" value="88.0">88 in</quantity>
male (
<typeStatus id="5536880BFFC6FF84FE4BFE68FDD7FE46" box="[504,609,393,420]" pageId="27" pageNumber="228">holotype</typeStatus>
