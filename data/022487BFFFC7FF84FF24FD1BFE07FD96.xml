<quantity id="4D759B4CFFC6FF84FE82FE87FECCFE63" box="[305,378,358,385]" metricMagnitude="0" metricUnit="m" metricValue="4.191" pageId="27" pageNumber="228" unit="in" value="165.0">165 in</quantity>
male (
<typeStatus id="5536880BFFC6FF84FE79FE86FD85FE60" box="[458,563,359,386]" pageId="27" pageNumber="228">holotype</typeStatus>
). 
<taxonomicName id="4D8D4D2AFFC6FF84FDF5FE86FD62FE63" authorityName="Krefft" authorityYear="1871" box="[582,724,359,385]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="27" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
length 
<quantity id="4D759B4CFFC6FF84FC98FE86FCC2FE63" box="[811,884,359,385]" metricMagnitude="0" metricUnit="m" metricValue="4.7498" pageId="27" pageNumber="228" unit="in" value="187.0">187 in</quantity>
male (
<typeStatus id="5536880BFFC6FF84FC70FE86FB9DFE60" box="[963,1067,359,386]" pageId="27" pageNumber="228">holotype</typeStatus>
), 
<quantity id="4D759B4CFFC6FF84FBF3FE86FB77FE63" box="[1088,1217,359,385]" metricMagnitude="0" metricUnit="m" metricValue="4.953" metricValueMax="5.08" metricValueMin="4.826" pageId="27" pageNumber="228" unit="in" value="195.0" valueMax="200.0" valueMin="190.0">190–200 in</quantity>
female, 
<quantity id="4D759B4CFFC6FF84FA96FE86FAD8FE63" box="[1317,1390,359,385]" metricMagnitude="0" metricUnit="m" metricValue="2.54" pageId="27" pageNumber="228" unit="in" value="100.0">100 in</quantity>
LV. 
<taxonomicName id="4D8D4D2AFFC6FF84FF24FE68FE93FE41" authorityName="Krefft" authorityYear="1871" box="[151,293,393,419]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="27" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
