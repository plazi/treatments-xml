<taxonomicName id="01C360C44E00B957FEFAFD56FE2D2F31" authority="Baba &amp; Ahyong &amp; Schnabel, 2018" authorityName="Baba &amp; Ahyong &amp; Schnabel" authorityYear="2018" box="[347,483,762,787]" class="Malacostraca" family="Sternostylidae" genus="Sternostylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="81" phylum="Arthropoda" rank="genus" status="new genus">
