and 
<taxonomicName id="12C34D156D255268FC22582D8FFAFCB1" authorityName="Riedel" authorityYear="2001" box="[963,985,794,876]" class="Insecta" family="Attelabidae" genus="Euops" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="1224" phylum="Arthropoda" rank="species" species="ibelensis">
