<paragraph id="8B9436E2FFC41A12C8DB324340A7FBF0" blockId="10.[151,1437,1641,2032]" pageId="10" pageNumber="11">
<emphasis id="B95FEAF0FFC41A12C8DB324344FBFBD7" bold="true" box="[151,225,1642,1666]" pageId="10" pageNumber="11">
<typeStatus id="54908840FFC41A12C8DB324344C0FBD7" box="[151,218,1642,1666]" pageId="10" pageNumber="11">Types</typeStatus>
