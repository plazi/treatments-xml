<taxonomicName id="49EE98D4FF8AFFBDFB0E05AFFDE1FBCA" authority="(Duges, 1881)" authorityName="Duges" authorityYear="1881" baseAuthorityName="Duges" baseAuthorityYear="1881" class="Insecta" family="Meloidae" genus="Lytta" kingdom="Animalia" order="Coleoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="subSpecies" species="biguttata" subGenus="Paralytta" subSpecies="bivirgata">
