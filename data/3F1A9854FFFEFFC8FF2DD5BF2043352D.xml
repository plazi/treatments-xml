<taxonomicName id="70B352C1FFFAFFC8FE9FD0C225B23444" baseAuthorityName="Westwood" baseAuthorityYear="1837" box="[293,499,1020,1044]" class="Insecta" family="Pentatomidae" genus="Piezodorus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="449" phylum="Arthropoda" rank="species" species="guildinii">
