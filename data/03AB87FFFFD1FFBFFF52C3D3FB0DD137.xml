; 
<bibRefCitation id="EF934B18FFD6FFBEFA82C264FE10D70D" author="Chamberlin and Hoffman" firstAuthor="Chamberlin" pageId="12" pageNumber="11" pagination="1 - 236" refId="ref25774" refString="Chamberlin, R. V., and R. L. Hoffman. 1958. Checklist of the millipeds of North America. United States National Museum Bulletin 212: 1 - 236." type="journal article" year="1958">Chamberlin and Hoffman 1958</bibRefCitation>
; 
<bibRefCitation id="EF934B18FFD6FFBEFE3EC204FDEAD70D" author="Hoffman" box="[428,603,1872,1896]" firstAuthor="Hoffman" pageId="12" pageNumber="11" refId="ref27457" refString="Hoffman, R. L. 1980 (1979). Classification of the Diplopoda. Museum d'Histoire Naturelle; Geneve, Switzerland. 237 p." type="book" year="1980">Hoffman 1980</bibRefCitation>
, 
<bibRefCitation id="EF934B18FFD6FFBEFDF6C204FD13D70D" author="Hoffman" box="[612,674,1872,1896]" firstAuthor="Hoffman" pageId="12" pageNumber="11" pagination="1 - 584" refId="ref27613" refString="Hoffman, R. L. 1999. Checklist of the millipeds of North and Middle America. Virginia Museum of Natural History Special Publication Number 8: 1 - 584." type="book chapter" year="1999">1999</bibRefCitation>
; 
<bibRefCitation id="EF934B18FFD6FFBEFD3EC204FCE9D70D" author="Shelley" box="[684,856,1872,1896]" firstAuthor="Shelley" pageId="12" pageNumber="11" pagination="27 - 54" refId="ref30963" refString="Shelley, R. M. 2000 a. Revision of the milliped subfamily Desmoninae (Polydesmida: Sphaeriodesmidae). Myriapodologica 6 (5): 27 - 54." type="journal article" year="2000" yearSuffix="a">Shelley 2000a</bibRefCitation>
); 
<taxonomicName id="4C024D6AFFD6FFBEFCF9C204FAB4D70D" authority="(Chelodesmoidea)" baseAuthorityName="Chelodesmoidea" box="[875,1285,1872,1896]" class="Diplopoda" family="Chelodesmidae" kingdom="Animalia" order="Polydesmida" pageId="12" pageNumber="11" phylum="Arthropoda" rank="family">Chelodesmidae (Chelodesmoidea)</taxonomicName>
, represented by 
<taxonomicName id="4C024D6AFFD6FFBEFF70C224FDD8D7ED" authority="Mauries" authorityName="Mauries" box="[226,617,1904,1928]" class="Diplopoda" family="Chelodesmidae" genus="Cantabrodesmus" kingdom="Animalia" order="Polydesmida" pageId="12" pageNumber="11" phylum="Arthropoda" rank="species" species="lorioli">
<emphasis id="B976EAFBFFD6FFBEFF70C224FE4CD7ED" box="[226,509,1904,1928]" italics="true" pageId="12" pageNumber="11">Cantabrodesmus lorioli</emphasis>
Mauriès
</taxonomicName>
, in caves in northern 
<collectingCountry id="F3157679FFD6FFBEFCF4C224FC1CD7ED" box="[870,941,1904,1928]" name="Spain" pageId="12" pageNumber="11">Spain</collectingCountry>
(
<bibRefCitation id="EF934B18FFD6FFBEFC28C224FBD3D7EC" author="Mauries" box="[954,1122,1904,1929]" firstAuthor="Mauries" pageId="12" pageNumber="11" pagination="117 - 124" refId="ref28412" refString="Mauries, J. - P. 1971. Diplopodes epiges et cavernicoles de Pyrenees espagnoles et des Monts Cantabriques. VI. Polydesmides. Bulletin de la Societe d'Histoire Naturelle de Toulouse 107 (1 - 2): 117 - 124." type="journal article" year="1971">Mauriès 1971</bibRefCitation>
, 
<bibRefCitation id="EF934B18FFD6FFBEFBFEC225FB18D7EC" author="Mauries" box="[1132,1193,1905,1929]" firstAuthor="Mauries" pageId="12" pageNumber="11" pagination="53 - 63" refId="ref28456" refString="Mauries, J. - P. 1974. Interet phylogenie et biogeographique de quelques diplopodes recemment decrit du nord de l'Espagne. Symposia of the Zoological Society of London 32: 53 - 63." type="journal article" year="1974">1974</bibRefCitation>
; 
<bibRefCitation id="EF934B18FFD6FFBFFB21C225FF4DD097" author="Luque and Labrada" firstAuthor="Luque" lastPageId="13" lastPageNumber="12" pageId="12" pageNumber="11" pagination="51 - 60" refId="ref28203" refString="Luque, C. G., and L. Labrada. 2017. New localities of the species Cantabrodesmus lorioli Mauries, 1971 in Cantabria, Spain (Polydesmida, Diplopoda, Chelodesmidae). Boletin de la Real Sociedad Espanola de Historia Natural Seccion Biologica 111: 51 - 60." type="journal article" year="2017">Luque and Labrada 2017</bibRefCitation>
