<number id="427964753A73FFCDFD6FE235A00EFD8C" box="[746,797,583,607]" pageId="2" pageNumber="45" value="0.32">0.32</number>
/ 
<number id="427964753A73FFCDFCA3E235A04BFD8C" box="[806,856,583,607]" pageId="2" pageNumber="45" value="2.38">2.38</number>
± 
