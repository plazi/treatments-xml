<taxonomicName id="33B10DD13A73FFCDFE19E2E2A119FD7B" box="[412,522,656,680]" class="Insecta" family="Cerambycidae" genus="Eucharitolus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="45" phylum="Arthropoda" rank="species" species="depressus">
