<taxonomicName id="3FEE9FCB6A64FFC9FD14F9F20B7E2901" baseAuthorityName="Bosquet" baseAuthorityYear="1852" box="[677,887,1596,1620]" class="Ostracoda" family="Xestoleberididae" genus="Xestoleberis" kingdom="Animalia" order="Podocopida" pageId="42" pageNumber="951" phylum="Arthropoda" rank="species" species="subglobosa">
