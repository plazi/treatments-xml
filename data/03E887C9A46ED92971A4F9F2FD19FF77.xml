<subSubSection id="C35B6554A46ED92A71A4F8B3FEBEF897" lastPageId="17" lastPageNumber="16" pageId="16" pageNumber="15" type="diagnosis">
<paragraph id="8BFE36DFA46ED92B71A4F8B3FB87F8D7" blockId="16.[192,1440,1827,1883]" pageId="16" pageNumber="15">
<emphasis id="B935EACDA46ED92B71A4F8B3FEE1F8B7" bold="true" box="[192,328,1827,1851]" pageId="16" pageNumber="15">Diagnosis</emphasis>
