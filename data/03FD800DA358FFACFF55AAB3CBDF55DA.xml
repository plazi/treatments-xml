<quantity id="4CAC9CFEA358FFACFC7AAD7CCA4C5557" box="[952,1039,1113,1134]" metricMagnitude="-2" metricUnit="m" metricValue="1.26" pageId="2" pageNumber="110" unit="mm" value="12.6">12.6 mm</quantity>
. 
<emphasis id="B920ED09A358FFACFBDAAD7CCAF15557" bold="true" box="[1048,1202,1113,1134]" pageId="2" pageNumber="110">
South 
<collectingCountry id="F343718BA358FFACFB98AD7CCAF15557" box="[1114,1202,1113,1134]" name="United States of America" pageId="2" pageNumber="110">America</collectingCountry>
</emphasis>
.................. 
<number id="3D9C233CA358FFACFA4DAD7CCBD85557" box="[1423,1435,1113,1134]" pageId="2" pageNumber="110" value="2.0">2</number>
