<emphasis id="B920ED09A358FFACFDCAADB1CC945590" bold="true" box="[520,727,1172,1193]" pageId="2" pageNumber="110">
<collectingCountry id="F343718BA358FFACFDCAADB1CC1C5590" box="[520,607,1172,1193]" name="Ecuador" pageId="2" pageNumber="110">Ecuador</collectingCountry>
