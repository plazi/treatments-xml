(
<figureCitation id="ABEF3495FFF33E2D20A7CC4C6A31FE5E" box="[159,292,369,394]" captionStart="FIGURE 1" captionStartId="2.[151,250,1942,1964]" captionTargetBox="[395,1127,195,1904]" captionTargetId="figure-70@2.[395,1127,195,1907]" captionTargetPageId="2" captionText="FIGURE 1. Adults of Helictophanes and Cyphophanes. A. Helictophanes flava, n. sp. (holotype male). B. H. flava, n. sp. (paratype female). C. Cyphophanes khitchakutensis, n. sp. (holotype male). D. C. khitchakutensis, n. sp. (paratype female) (scale bar=2 mm)." figureDoi="http://doi.org/10.5281/zenodo.4928202" httpUri="https://zenodo.org/record/4928202/files/figure.png" pageId="6" pageNumber="133">Figs. 1Câ€“D</figureCitation>
, 
<figureCitation id="ABEF3495FFF33E2D2117CC4C6A97FE5E" box="[303,386,369,394]" captionStart="FIGURE 2" captionStartId="3.[151,250,1428,1450]" captionTargetBox="[172,1426,211,1364]" captionTargetId="figure-202@3.[151,1436,193,1406]" captionTargetPageId="3" captionText="FIGURE 2. Labial palpi and wing venation. A-B. Labial palpi; A. Helictophanes flava, n. sp. B. Cyphophanes khitchakutensis, n. sp. C-D. Wing venation; C. H. flava, n. sp. D. C. khitchakutensis, n. sp." figureDoi="http://doi.org/10.5281/zenodo.4928204" httpUri="https://zenodo.org/record/4928204/files/figure.png" pageId="6" pageNumber="133">2B, 2D</figureCitation>
, 
<figureCitation id="ABEF3495FFF33E2D21B6CC4C6AA4FE5E" box="[398,433,369,394]" captionStart="FIGURE 3" captionStartId="4.[151,250,1953,1975]" captionTargetBox="[208,1382,214,1871]" captionTargetId="figure-47@4.[167,1419,193,1932]" captionTargetPageId="4" captionText="FIGURE 3. Male genitalia. A. Helictophanes flava, n. sp. (holotype male). B. Cyphophanes khitchakutensis, n. sp. (holotype male)." figureDoi="http://doi.org/10.5281/zenodo.4928206" httpUri="https://zenodo.org/record/4928206/files/figure.png" pageId="6" pageNumber="133">3B</figureCitation>
, 
<figureCitation id="ABEF3495FFF33E2D2185CC4C6ACAFE5E" box="[445,479,369,394]" captionStart="FIGURE 4" captionStartId="5.[151,250,1800,1822]" captionTargetBox="[160,1387,208,1730]" captionTargetId="figure-121@5.[151,1436,193,1778]" captionTargetPageId="5" captionText="FIGURE 4. Female genitalia. A. Helictophanes flava, n. sp. (paratype female). B. Cyphophanes khitchakutensis, n. sp. (paratype female)." figureDoi="http://doi.org/10.5281/zenodo.4928210" httpUri="https://zenodo.org/record/4928210/files/figure.png" pageId="6" pageNumber="133">4B</figureCitation>
