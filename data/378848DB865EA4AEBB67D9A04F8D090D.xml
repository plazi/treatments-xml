<taxonomicName id="71EFC237C67BBDDFB479D5E90AF59A4B" class="Insecta" family="Curculionidae" genus="Rhyncholus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhyncholus" order="Coleoptera" pageId="259" pageNumber="260" phylum="Arthropoda" rank="genus">Rhyncholus</taxonomicName>
