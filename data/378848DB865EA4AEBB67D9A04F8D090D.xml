<taxonomicName id="74C489F317D71EC0D2B638B878351001" class="Insecta" family="Curculionidae" genus="Rhyncolus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhyncolus" order="Coleoptera" pageId="259" pageNumber="260" phylum="Arthropoda" rank="genus">Rhyncolus</taxonomicName>
