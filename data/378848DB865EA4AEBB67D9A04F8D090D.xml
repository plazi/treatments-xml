<taxonomicName id="8E52EA43C0CBD7A0320FD2AC928856EC" class="Insecta" family="Curculionidae" genus="Rhyncolus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhyncolus" order="Coleoptera" pageId="259" pageNumber="260" phylum="Arthropoda" rank="genus">Rhyncolus</taxonomicName>
