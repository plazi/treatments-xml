<paragraph id="8BC83699FFC5A76FFF1EF8810F07E5BA" blockId="0.[146,1431,1644,1941]" pageId="0" pageNumber="57">
<emphasis id="B903EA8BFFC5A76FFF1EF8810B74E541" bold="true" box="[193,300,1876,1902]" pageId="0" pageNumber="57">Affinity:</emphasis>
― 
<taxonomicName id="4C774D1AFFC5A76FFE95F88008CFE541" authorityName="Szelag &amp; Vladimirov" authorityYear="2013" box="[330,663,1877,1902]" class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="0" pageNumber="57" phylum="Tracheophyta" rank="species" species="crinitopannosum">
