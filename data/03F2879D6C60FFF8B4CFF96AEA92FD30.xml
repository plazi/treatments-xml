<taxonomicName id="4C5B4D086C61FFF8B4CFFE53EA51FDAC" box="[151,339,511,537]" class="Magnoliopsida" family="Plumbaginaceae" kingdom="Plantae" order="Caryophyllales" pageId="71" pageNumber="72" phylum="Tracheophyta" rank="family">Plumbaginaceae</taxonomicName>
