<number id="3DA12481AE70FFFAFEB9F86FD9D4F863" box="[305,424,2012,2038]" pageId="13" pageNumber="14" value="172.25" valueMax="215.5" valueMin="129.0">
129- 
<emphasis id="B91DEAB4AE70FFFAFEEEF86FD9D4F863" box="[358,424,2012,2037]" italics="true" pageId="13" pageNumber="14">215.5</emphasis>
</number>
- 
<number id="3DA12481AE70FFFAFE38F86FD9A2F860" box="[432,478,2012,2038]" pageId="13" pageNumber="14" value="293.0">293</number>
<date id="FFD71066AE70FFFAFE6EF86FDA71F860" box="[486,525,2012,2038]" pageId="13" pageNumber="14">
x 
<number id="3DA12481AE70FFFAFE75F86FDA71F860" box="[509,525,2012,2038]" pageId="13" pageNumber="14" value="6.85" valueMax="7.7" valueMin="6.0">6</number>
</date>
