<taxonomicName id="A6DB4D714C477A0B1A95FF5FFAEEC8C0" box="[1247,1371,189,210]" class="Mammalia" family="Lophiomerycidae" genus="Lophiomeryx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Artiodactyla" pageId="4" pageNumber="5" phylum="Chordata" rank="genus">
