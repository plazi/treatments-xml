</taxonomicName>
, and 
<taxonomicName id="279A51E50056BA53FC1CFA0AFB1CFA17" authority="Das &amp; Bastawade" authorityName="Das &amp; Bastawade" box="[921,1270,1428,1454]" class="Arachnida" family="Opilioacaridae" genus="Indiacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opilioacarida" pageId="1" pageNumber="24" phylum="Arthropoda" rank="genus">
