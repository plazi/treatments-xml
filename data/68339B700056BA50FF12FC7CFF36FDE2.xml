<taxonomicName id="279A51E50056BA53FD1BFA0AFCB3FA14" box="[670,857,1428,1453]" class="Arachnida" family="Opilioacaridae" genus="Phalangiacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opilioacarida" pageId="1" pageNumber="24" phylum="Arthropoda" rank="genus">
