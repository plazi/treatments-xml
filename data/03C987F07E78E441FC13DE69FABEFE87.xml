<taxonomicName id="4C604D657E7FE441FB53DA8BFABDFF07" authorityName="Rioja" authorityYear="1955" box="[1197,1300,186,210]" class="Malacostraca" family="Platyarthridae" genus="Trichorhina" kingdom="Animalia" order="Isopoda" pageId="13" pageNumber="252" phylum="Arthropoda" rank="species" species="vandeli">
