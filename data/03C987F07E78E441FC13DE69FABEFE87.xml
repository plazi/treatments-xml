<taxonomicName id="4C604D657E7FE441FF3FDD6AFE91F8A7" authorityName="Arcangeli" authorityYear="1936" box="[193,312,1882,1906]" class="Malacostraca" family="Platyarthridae" genus="Trichorhina" kingdom="Animalia" order="Isopoda" pageId="13" pageNumber="252" phylum="Arthropoda" rank="species" species="silvestrii">
