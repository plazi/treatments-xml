<taxonomicName id="4C604D657E7FE441FDE9DCAAFD3FF967" authorityName="Budde-Lund" authorityYear="1893" baseAuthorityName="Budde-Lund" baseAuthorityYear="1893" box="[535,662,1690,1714]" class="Malacostraca" family="Platyarthridae" genus="Trichorhina" kingdom="Animalia" order="Isopoda" pageId="13" pageNumber="252" phylum="Arthropoda" rank="species" species="papillosa">
<emphasis id="B914EAF47E7FE441FDE9DCAAFD3FF967" box="[535,662,1690,1714]" italics="true" pageId="13" pageNumber="252">T. papillosa</emphasis>
