<taxonomicName id="4C604D657E7FE441FE7EDFEAFD90FA27" authorityName="Souza &amp; Araújo &amp; Campos-Filho" authorityYear="2011" box="[384,569,1498,1522]" class="Malacostraca" family="Platyarthridae" genus="Trichorhina" kingdom="Animalia" order="Isopoda" pageId="13" pageNumber="252" phylum="Arthropoda" rank="species" species="myrmecophila">
