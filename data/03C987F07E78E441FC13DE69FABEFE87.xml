<taxonomicName id="4C604D657E7FE441FDB5DDAAFD6FF867" authorityName="Arcangeli" authorityYear="1936" box="[587,710,1946,1970]" class="Malacostraca" family="Platyarthridae" genus="Trichorhina" kingdom="Animalia" order="Isopoda" pageId="13" pageNumber="252" phylum="Arthropoda" rank="species" species="silvestrii">
<emphasis id="B914EAF47E7FE441FDB5DDAAFD6FF867" box="[587,710,1946,1970]" italics="true" pageId="13" pageNumber="252">T. silvestrii</emphasis>
</taxonomicName>
). From 
<taxonomicName id="4C604D657E7FE441FCC5DAABFC0DFF67" authorityName="Rioja" authorityYear="1955" box="[827,932,154,178]" class="Malacostraca" family="Platyarthridae" genus="Trichorhina" kingdom="Animalia" order="Isopoda" pageId="13" pageNumber="252" phylum="Arthropoda" rank="species" species="vandeli">
