<specimenCount id="5CEAFD3AFFB54F02FABEF9D042E5FB73" box="[1401,1436,1155,1179]" pageId="83" pageNumber="84" type="female">1♀</specimenCount>
(leg. 
<collectorName id="E7195365FFB54F02FF17F9F54648FB57" box="[208,305,1190,1215]" pageId="83" pageNumber="84">Escalera</collectorName>
) (
<collectionCode id="2CFDAE76FFB54F02FE80F9F546FDFB57" box="[327,388,1190,1215]" country="Germany" httpUri="http://grbio.org/cool/jp8y-i5nv" name="Museum f�r Naturkunde" pageId="83" pageNumber="84">MfN</collectionCode>
)
</materialsCitation>
