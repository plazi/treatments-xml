<quantity id="4CCDA6F4FFE3FFF1A880F9975C52F922" box="[928,1035,1662,1688]" metricMagnitude="-4" metricUnit="m" metricValue="2.4" pageId="24" pageNumber="25" unit="mm" value="0.24">0.24 mm</quantity>
