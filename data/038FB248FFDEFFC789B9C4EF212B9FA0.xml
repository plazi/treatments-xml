<taxonomicName id="4C2678DDFFC0FFC78B8EC73B256F9F30" authority="Choudhury &amp; Perryman, 2003" authorityName="Choudhury &amp; Perryman" authorityYear="2003" box="[672,1317,295,320]" class="Chromadorea" family="Cystidicolidae" genus="Spinitectus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="113" pageNumber="114" phylum="Nematoda" rank="species" species="macrospinosus">
