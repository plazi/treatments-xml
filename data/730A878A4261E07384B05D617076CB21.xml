<quantity id="3C5B9B794260E07387505DC871D4C81D" box="[564,659,1273,1297]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="1" pageNumber="158" unit="mm" value="0.85">0.85 mm</quantity>
