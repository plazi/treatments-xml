<collectionCode id="ED04AE7C7A31FFBF272EFD437033FCEA" box="[832,890,756,777]" country="Viet Nam" httpUri="http://grbio.org/cool/tj07-dt2h" name="Institute of Tropical Biology" pageId="1" pageNumber="146" type="Herbarium">VNM</collectionCode>
<specimenCode id="DBB39EC27A31FFBF2711FD437752FCEA" box="[895,1051,756,777]" collectionCode="VNM" country="Viet Nam" httpUri="http://grbio.org/cool/tj07-dt2h" name="Institute of Tropical Biology" pageId="1" pageNumber="146" type="Herbarium">VNM00024328</specimenCode>
)
