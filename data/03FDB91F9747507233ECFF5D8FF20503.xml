<collectingCountry id="F3434899974D50723636F9858FF60453" box="[1357,1432,1615,1637]" name="Mexico" pageId="13" pageNumber="289">Mexico</collectingCountry>
, Central 
<collectingCountry id="F3434899974D50723256F9A68B1104B4" box="[301,383,1644,1666]" name="United States of America" pageId="13" pageNumber="289">America</collectingCountry>
...................................................................................... 
