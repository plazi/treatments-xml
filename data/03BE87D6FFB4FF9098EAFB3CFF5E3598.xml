<taxonomicName id="4C174D43FFB4FF9098BBFAADF81D357C" authority="Garman, 1899" authorityName="Garman" authorityYear="1899" box="[198,604,1375,1401]" class="Actinopterygii" family="Congridae" genus="Chiloconger" kingdom="Animalia" order="Anguilliformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="obtusus">
<emphasis id="B963EAD2FFB4FF9098BBFAADFBF0357C" box="[198,433,1375,1401]" italics="true" pageId="37" pageNumber="38">Chiloconger obtusus</emphasis>
