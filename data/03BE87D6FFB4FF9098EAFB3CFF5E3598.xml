<quantity id="4CEF9B25FFB4FF9099EFFAE5FBA33534" box="[402,482,1303,1329]" metricMagnitude="3" metricUnit="m" metricValue="3.218688" pageId="37" pageNumber="38" unit="mi" value="2.0">2 miles</quantity>
