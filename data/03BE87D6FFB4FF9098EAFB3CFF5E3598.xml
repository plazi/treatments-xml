<geoCoordinate id="EE235007FFB4FF909941FAE5FBC03534" box="[316,385,1303,1329]" degrees="49" direction="west" orientation="longitude" pageId="37" pageNumber="38" precision="55555" value="-49.0">49°W</geoCoordinate>
