<taxonomicName id="4C174D43FFB4FF909CD2FAADFB3D3598" authority="(Garman, 1899)" baseAuthorityName="Garman" baseAuthorityYear="1899" class="Actinopterygii" family="Congridae" genus="Paraconger" kingdom="Animalia" order="Anguilliformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="dentatus">
