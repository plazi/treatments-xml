<emphasis id="B963EAD2FFB4FF909CE5FA71FF5E3598" box="[1176,1311,1411,1437]" italics="true" pageId="37" pageNumber="38">
<taxonomicName id="4C174D43FFB4FF909CE5FA71FF5A3598" baseAuthorityName="Garman" baseAuthorityYear="1899" box="[1176,1307,1411,1437]" class="Actinopterygii" family="Congridae" genus="Paraconger" kingdom="Animalia" order="Anguilliformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="dentatus">C. dentatus</taxonomicName>
