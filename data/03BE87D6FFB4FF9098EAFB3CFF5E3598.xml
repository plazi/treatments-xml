<quantity id="4CEF9B25FFB4FF909C36FB3DFECE34EC" box="[1099,1167,1231,1257]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="37" pageNumber="38" unit="cm" value="30.0">30 cm</quantity>
