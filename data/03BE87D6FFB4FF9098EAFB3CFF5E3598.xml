<geoCoordinate id="EE235007FFB4FF9098B2FAE4FB733534" box="[207,306,1302,1329]" degrees="25" direction="north" minutes="18" orientation="latitude" pageId="37" pageNumber="38" precision="925" value="25.3">25°18’N</geoCoordinate>
