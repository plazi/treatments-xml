<geoCoordinate id="EE235007FFB4FF909A61FB01F83F3508" box="[540,638,1267,1293]" degrees="24" direction="north" minutes="11" orientation="latitude" pageId="37" pageNumber="38" precision="925" value="24.183332">24°11’N</geoCoordinate>
