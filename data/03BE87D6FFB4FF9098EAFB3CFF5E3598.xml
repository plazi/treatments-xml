<taxonomicName id="4C174D43FFB4FF909A11FAADFE2C357C" authority="Myers and Wade, 1941" authorityName="Myers and Wade" authorityYear="1941" box="[620,1133,1375,1401]" class="Actinopterygii" family="Congridae" genus="Chiloconger" kingdom="Animalia" order="Anguilliformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="labiatus">
<emphasis id="B963EAD2FFB4FF909A11FAADF91A357C" box="[620,859,1375,1401]" italics="true" pageId="37" pageNumber="38">Chiloconger labiatus</emphasis>
