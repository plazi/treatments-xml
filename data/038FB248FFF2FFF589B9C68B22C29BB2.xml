<taxonomicName id="4C2678DDFFF2FFF589B9C68B22A79EC2" authority="Moravec &amp; Arai, 1971" authorityName="Moravec &amp; Arai" authorityYear="1971" box="[151,749,152,178]" class="Secernentea" family="Thelaziidae" genus="Rhabdochona" kingdom="Animalia" order="Spirurida" pageId="67" pageNumber="68" phylum="Nematoda" rank="species" species="canadensis">
