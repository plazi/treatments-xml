<caption id="DF1B4CFB575A3C06FF05FB51B8DDAC83" httpUri="https://zenodo.org/record/253246/files/figure.png" pageId="34" pageNumber="135" targetBox="[182,1402,441,1239]" targetPageId="34">
<paragraph id="8BDB1C73575A3C06FF05FB51B8DDAC83" blockId="34.[151,1436,1251,1426]" pageId="34" pageNumber="135">
