<quantity id="4CCC9B0AFF99FFCCFC403D24ABB7FAAD" box="[943,999,1336,1355]" metricMagnitude="2" metricUnit="m" metricValue="2.5" pageId="10" pageNumber="665" unit="m" value="250.0">250 m</quantity>
, 
<date id="FF8A102FFF99FFCCFC003D24AC32FAAD" box="[1007,1122,1336,1355]" pageId="10" pageNumber="665" value="1997-01-15" valueMax="1997-01-18" valueMin="1997-01-15">15.-18.i.1997</date>
