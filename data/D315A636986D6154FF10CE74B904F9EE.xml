<taxonomicName id="9CBC6CA3986D6154FDF4CEB8BBCAFC02" authority="Jaloszynski, 2009 a" authorityName="Jaloszynski" authorityYear="2009" box="[627,971,904,929]" class="Insecta" family="Staphylinidae" genus="Trurlia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="559" phylum="Arthropoda" rank="species" species="insana">
