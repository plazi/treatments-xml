<collectingRegion id="49C57FE477ECFF17FC5CA5BC944AF96A" box="[991,1079,1751,1776]" country="Mexico" name="Mexico" pageId="187" pageNumber="188">Mexico</collectingRegion>
</collectingCountry>
, 
<collectingRegion id="49C57FE477ECFF17FBCBA5BC94E1F96A" box="[1096,1180,1751,1776]" country="United States of America" name="Florida" pageId="187" pageNumber="188">Florida</collectingRegion>
