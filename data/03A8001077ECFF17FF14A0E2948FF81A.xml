<quantity id="4CF91CE377ECFF17FDAEA5979206F88E" box="[557,635,1788,1813]" metricMagnitude="1" metricUnit="m" metricValue="8.85" pageId="187" pageNumber="188" unit="m" value="88.5">88.5 m</quantity>
depth (
<collectingCountry id="F316F19677ECFF17FD4DA597935AF88E" box="[718,807,1788,1812]" name="Guyana" pageId="187" pageNumber="188">Guyana</collectingCountry>
Shelf 
<quantity id="4CF91CE377ECFF17FCF2A5979391F88E" box="[881,1004,1788,1813]" metricMagnitude="1" metricUnit="m" metricValue="6.225" metricValueMax="8.85" metricValueMin="3.6" pageId="187" pageNumber="188" unit="m" value="62.25" valueMax="88.5" valueMin="36.0">36–88.5 m</quantity>
