<taxonomicName id="738C94F125CEA11AEDFE5B4FD5D22C70" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139355" class="Insecta" family="Formicidae" genus="Leptogenys" kingdom="Animalia" lsidName="Leptogenys (Lobopelta) conigera r. adlerzi Forel" lsidName-HNS="Leptogenys (Lobopelta) conigera r. adlerzi Forel" order="Hymenoptera" pageNumber="65" phylum="Arthropoda" rank="subSpecies" species="conigera" subGenus="Lobopelta" subSpecies="adlerzi">Leptogenys (Lobopelta) conigera Mayr, r. Adlerzi</taxonomicName>
<taxonomicNameLabel id="C6963232BDD83AB7B448412FB181779C" rank="subSpecies">nov. st.</taxonomicNameLabel>
</paragraph>
