<emphasis id="0CBFEAA9FF85D648FF6FF890FEA9F8EE" bold="true" box="[151,289,1834,1856]" pageId="21" pageNumber="538">FIGURE 13.</emphasis>
<taxonomicName id="F9CB4D38FF85D648FED0F890FE5EF891" authority="Esquete &amp; Cunha, 2018" authorityName="Esquete &amp; Cunha" authorityYear="2018" box="[296,470,1834,1856]" class="Malacostraca" family="Nototanaidae" genus="Gamboa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tanaidacea" pageId="21" pageNumber="538" phylum="Arthropoda" rank="species" species="henrieti" status="sp. nov.">
<emphasis id="0CBFEAA9FF85D648FED0F890FE5EF891" box="[296,470,1834,1856]" italics="true" pageId="21" pageNumber="538">Gamboa henrieti</emphasis>
</taxonomicName>
<taxonomicNameLabel id="178C57D2FF85D648FE26F890FDB8F8EE" box="[478,560,1834,1855]" pageId="21" pageNumber="538" rank="species">
<emphasis id="0CBFEAA9FF85D648FE26F890FDB8F8EE" bold="true" box="[478,560,1834,1855]" pageId="21" pageNumber="538">sp. nov.</emphasis>
