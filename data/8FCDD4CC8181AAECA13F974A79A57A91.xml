<normalizedToken id="9E48F33058E3ECE2BFA31BB43AE929D0" originalValue="trouvée">trouvee</normalizedToken>
<normalizedToken id="FB2AA48959BF2FCEDA8DF1C7FAAFA0A6" originalValue="à">a</normalizedToken>
<normalizedToken id="139FF12864DF75DB6870A8C7DE25AA52" originalValue="Antananarivô">Antananarivo</normalizedToken>
(
<normalizedToken id="F837BBF21DAE1B21364E4BEC256839F4" originalValue="Rév">Rev</normalizedToken>
. 
<normalizedToken id="4DF4ADD4D86632F2329CAA1E71C4D70A" originalValue="Père">Pere</normalizedToken>
<normalizedToken id="FDE85D3334B1F31185AE5C424BB5BB22" originalValue="Camboué">Camboue</normalizedToken>
) et 
