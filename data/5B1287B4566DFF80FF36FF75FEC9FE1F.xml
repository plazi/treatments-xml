<taxonomicName id="14BB4D21566DFF80FD3BFE88FA2BFE54" authority="(Hammer 1962 a) in Puerto Montt and Cueva" authorityName="in Puerto Montt and Cueva" baseAuthorityName="Hammer" baseAuthorityYear="1962" box="[666,1436,361,386]" class="Arachnida" family="Oppiidae" genus="Oppia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="198" phylum="Arthropoda" rank="species" species="multicorrugata">
