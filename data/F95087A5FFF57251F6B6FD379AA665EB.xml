<taxonomicName id="B6F94D30FFEA7252F770FC669A7C66F9" authority="Mahunka, 1983: 391" authorityName="Mahunka" authorityPageNumber="391" authorityYear="1983" box="[92,625,979,1001]" class="Arachnida" family="Protoribatidae" genus="Protoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="16" pageNumber="1253" phylum="Arthropoda" rank="species" species="serratomarginatus">
