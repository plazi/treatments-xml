<collectingRegion id="B33DF851FFE87250F7E1FB3599576188" box="[205,346,1152,1176]" country="Mongolia" name="Ulaanbaatar" pageId="18" pageNumber="1255">Ulaanbaatar</collectingRegion>
, litter of larch forest (
<taxonomicName id="B6F94D30FFE87250F55AFB349B106188" box="[630,797,1152,1176]" class="Pinopsida" family="Pinaceae" genus="Larix" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="1255" phylum="Tracheophyta" rank="species" species="sibiricus">
<emphasis id="438DEAA1FFE87250F55AFB349B106188" box="[630,797,1152,1176]" italics="true" pageId="18" pageNumber="1255">Larix sibiricus</emphasis>
</taxonomicName>
), 47ss55¾N, 106ss30¾E, 
<date id="05471073FFE87250F7ACFB15990761A8" box="[128,266,1184,1208]" pageId="18" pageNumber="1255" value="1996-03">March 1996</date>
; 
<specimenCount id="67FFFD3AFFE87250F630FB1599BC61A8" box="[284,433,1184,1208]" pageId="18" pageNumber="1255" type="generic">14 specimens</specimenCount>
: Mt. Zaan Terelj, District Erdene, 
<collectingRegion id="B33DF851FFE87250F461FB159C0361A8" box="[845,1038,1184,1208]" inferredBySuffix="true" pageId="18" pageNumber="1255">Central Province</collectingRegion>
, litter of larch forest (
<taxonomicName id="B6F94D30FFE87250F674FB7499F061C8" box="[344,509,1216,1240]" class="Pinopsida" family="Pinaceae" genus="Larix" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="1255" phylum="Tracheophyta" rank="species" species="sibiricus">
