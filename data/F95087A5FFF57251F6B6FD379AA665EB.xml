<quantity id="B6019B56FFE87250F4CDFBD59C1B6168" box="[993,1046,1120,1144]" metricMagnitude="3" metricUnit="m" metricValue="6.0" pageId="18" pageNumber="1255" unit="km" value="6.0">6 km</quantity>
