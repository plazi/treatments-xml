<typeStatus id="54AC8830FFF0326BFE8DFB904E993543" box="[358,457,1088,1111]" pageId="2" pageNumber="67">Holotype</typeStatus>
female, 
<collectingCountry id="F3007602FFF0326BFDCDFB904D2F3543" box="[550,639,1088,1111]" name="China" pageId="2" pageNumber="67">CHINA</collectingCountry>
