<date id="FFA91052FFF0326BFC6FFB904CA13543" box="[900,1009,1088,1111]" pageId="2" pageNumber="67" value="1982-06-15">
<number id="3DDF24B5FFF0326BFC6FFB904CE63543" box="[900,950,1088,1111]" pageId="2" pageNumber="67" value="6.15">6.15</number>
