<taxonomicName id="4C6D1DFBB82C9D13FBD0F8BFFAA5F8F1" authority="Yue &amp; Li" authorityName="Yue &amp; Li" authorityYear="2022" box="[1036,1352,1857,1879]" class="Insecta" family="Crabronidae" genus="Lestica" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="397" phylum="Arthropoda" rank="species" species="breviantennata" status="sp. nov.">
