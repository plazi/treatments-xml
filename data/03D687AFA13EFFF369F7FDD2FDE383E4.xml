<taxonomicName id="4C7F4D3AA13EFFF16E93FB63FCEE874C" authority="Fallen, 1814" authorityName="Fallen" authorityYear="1814" class="Insecta" family="Anthocoridae" genus="Anthocoris" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="376" phylum="Arthropoda" rank="genus">
<emphasis id="B90BEAABA13EFFF16E93FB63FAD386AC" box="[1241,1358,1254,1277]" italics="true" pageId="1" pageNumber="376">Anthocoris</emphasis>
<bibRefCitation id="EFEE4B48A13EFFF16F19FB63FCEE874C" author="FALLEN C. F." pageId="1" pageNumber="376" refId="ref49457" refString="FALLEN C. F. 1814: Specimen novam Hemiptem disponendi methodum exhibens. Berling, Lund, 26 pp." type="book" year="1814">Fallén, 1814</bibRefCitation>
</taxonomicName>
(Northern Hemisphere, with a few exceptions), 
<emphasis id="B90BEAABA13EFFF16976FAA3FC36876C" box="[828,939,1318,1341]" italics="true" pageId="1" pageNumber="376">Arnulphus</emphasis>
Distant, 1904 (
<collectingCountry id="F3687629A13EFFF16E1AFAA3FB20876C" box="[1104,1213,1318,1341]" name="Myanmar" pageId="1" pageNumber="376">Myanmar</collectingCountry>
), 
<taxonomicName id="4C7F4D3AA13EFFF16E9AFAA3FC61870C" authority="McAtee &amp; Malloch, 1925" authorityName="McAtee &amp; Malloch" authorityYear="1925" class="Insecta" family="Anthocoridae" genus="Coccivora" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="376" phylum="Arthropoda" rank="genus">
<emphasis id="B90BEAABA13EFFF16E9AFAA3FAA2876C" box="[1232,1343,1318,1341]" italics="true" pageId="1" pageNumber="376">Coccivora</emphasis>
McAtee &amp; Malloch, 1925
</taxonomicName>
(
<collectingCountry id="F3687629A13EFFF16E47FAC3FBD5870C" box="[1037,1096,1350,1373]" name="United States of America" pageId="1" pageNumber="376">USA</collectingCountry>
), 
<emphasis id="B90BEAABA13EFFF16E14FAC3FB69870C" box="[1118,1268,1350,1373]" italics="true" pageId="1" pageNumber="376">Compsobiella</emphasis>
<bibRefCitation id="EFEE4B48A13EFFF16EB4FAC3FA01870C" author="POPPIUS B." box="[1278,1436,1350,1373]" pageId="1" pageNumber="376" pagination="1 - 43" refId="ref58375" refString="POPPIUS B. 1909: Beitrage zur Kenntnis der Anthocoriden. Acta Societatis Scientiarum Fennicae 37 (9): 1 - 43." type="journal article" year="1909">Poppius, 1909</bibRefCitation>
