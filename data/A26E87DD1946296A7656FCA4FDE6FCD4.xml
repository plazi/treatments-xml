</taxonomicName>
and 
<taxonomicName id="EDC74D4819462965779AF985FBA8F9BB" authorityName="Friese" authorityYear="1926" box="[968,1130,1604,1628]" class="Insecta" family="Halictidae" genus="Megalopta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="597" phylum="Arthropoda" rank="species" species="nitidicollis">
