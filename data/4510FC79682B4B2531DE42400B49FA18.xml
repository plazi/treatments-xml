<taxonomicName id="0AB936EC682B4B25307042DF0D98FC10" authority="Simon, 1895" authorityName="Simon" authorityYear="1895" box="[313,652,965,989]" class="Arachnida" family="Tetragnathidae" genus="Atelidea" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="spinosa">
