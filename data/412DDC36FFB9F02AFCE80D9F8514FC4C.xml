<paragraph id="C93B6D20FFB9F02AFCE80D9F858AFF43" blockId="7.[861,1504,201,1006]" box="[861,1194,201,226]" pageId="7" pageNumber="979">
<taxonomicName id="0E8416A3FFB9F02AFCE80D9F8560FF43" authority="L. Gaut." authorityName="L. Gaut." box="[861,1088,201,226]" class="Magnoliopsida" family="Sapotaceae" genus="Bemangidia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ericales" pageId="7" pageNumber="979" phylum="Tracheophyta" rank="genus" tribus="Tseboneae">
<emphasis id="FBF0B132FFB9F02AFCE80D9F82C1FF40" bold="true" box="[861,993,201,225]" italics="true" pageId="7" pageNumber="979">Bemangidia</emphasis>
L. Gaut.
</taxonomicName>
, 
<emphasis id="FBF0B132FFB9F02AFBFF0D9C858AFF43" bold="true" box="[1098,1194,202,226]" pageId="7" pageNumber="979">
<taxonomicNameLabel id="E0C30C49FFB9F02AFBFF0D9C858AFF43" box="[1098,1194,202,226]" pageId="7" pageNumber="979">gen. nov.</taxonomicNameLabel>
