<quantity id="4CF79B33FFFFFFD6C0D5FE8AFD74FE8B" box="[684,738,339,359]" metricMagnitude="2" metricUnit="m" metricValue="4.9" pageId="16" pageNumber="171" unit="m" value="490.0">490m</quantity>
