<quantity id="4CF79B33FFFFFFD6C16EFEC4FCD8FEDD" box="[791,846,285,305]" metricMagnitude="2" metricUnit="m" metricValue="4.85" pageId="16" pageNumber="171" unit="m" value="485.0">485m</quantity>
