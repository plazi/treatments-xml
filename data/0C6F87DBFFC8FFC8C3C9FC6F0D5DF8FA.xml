<taxonomicName id="43C64D4EFFCEFFC8C009F9040AB5F951" authorityName="Krapp-Schickel &amp; Myers" authorityYear="2006" box="[855,999,1711,1736]" class="Malacostraca" family="Aoridae" genus="Bemlos" kingdom="Animalia" order="Amphipoda" pageId="37" pageNumber="257" phylum="Arthropoda" rank="species" species="clypeatus">
