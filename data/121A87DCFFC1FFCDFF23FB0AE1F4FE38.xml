<taxonomicName id="5DB34D49FFC1FFCCFB9AFA9DE481FACA" box="[1070,1212,1377,1402]" class="Insecta" family="Psychodidae" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="165" phylum="Arthropoda" rank="family">Psychodidae</taxonomicName>
