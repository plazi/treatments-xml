<subSubSection id="C30F0D842D36FFB5FF26B8A3BFF2F932" box="[151,712,1676,1702]" pageId="5" pageNumber="122" type="description">
<paragraph id="8BAA5E0F2D36FFB5FF26B8A3BFF2F932" blockId="5.[151,712,1639,1702]" box="[151,712,1676,1702]" pageId="5" pageNumber="122">
(males unknown in 
<taxonomicName id="4C15258C2D36FFB5FEC4B8A2BCCFF932" authorityName="Morice" authorityYear="1897" box="[373,501,1676,1702]" class="Insecta" family="Crabronidae" genus="Dinetus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="122" phylum="Arthropoda" rank="species" species="cereolus">
