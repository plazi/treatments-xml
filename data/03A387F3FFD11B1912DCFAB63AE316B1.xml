<subSubSection id="C310656EFFD11B19128CF9643AE316B1" pageId="2" pageNumber="249" type="etymology">
<paragraph id="8BB536E5FFD11B19128CF9643AE316B1" blockId="2.[151,1437,1369,1790]" pageId="2" pageNumber="249">
<emphasis id="B97EEAF7FFD11B19128CF9643F331695" bold="true" box="[199,334,1729,1754]" pageId="2" pageNumber="249">Etymology.</emphasis>
