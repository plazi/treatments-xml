<quantity id="4CF59B1F4103FFA1FF00F991237BF9B8" box="[238,327,1549,1570]" metricMagnitude="-3" metricUnit="m" metricValue="1.08" pageId="13" pageNumber="319" unit="mm" value="1.08">1.08 mm</quantity>
