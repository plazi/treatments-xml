<emphasis id="3FF4215E4420FFD4FF5CFB5CFCDAFB13" bold="true" box="[151,780,1155,1182]" pageId="0" pageNumber="193">
“ 
<taxonomicName id="CA8086CF4420FFD4FF6DFB5CFDC1FB13" authority="L. Syst. Nat." authorityName="L. Syst. Nat." box="[166,535,1155,1182]" class="Anthozoa" family="Alcyoniidae" genus="Alcyonium" kingdom="Animalia" order="Alcyonacea" pageId="0" pageNumber="193" phylum="Cnidaria" rank="species" species="bursa">
