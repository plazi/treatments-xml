<pageBreakToken id="610DE7631376A08B3D4AB326FB9378A5" pageId="null" pageNumber="274" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="CBFE68C2E53080024D52EEB9F5100CC7" authority="L." class="Liliopsida" family="Poaceae" genus="Phleum" kingdom="Plantae" order="Poales" pageId="null" pageNumber="274" phylum="Tracheophyta" rank="species" species="pratense">
