<emphasis id="C5D47B09D16BFFE03AF1B8B2FD59F95D" box="[739,754,1735,1760]" italics="true" pageId="1" pageNumber="54">h</emphasis>
<number id="4168B53CD16BFFE03AE0B8A2FD51F958" box="[754,762,1751,1765]" pageId="1" pageNumber="54" value="2.0">2</number>
