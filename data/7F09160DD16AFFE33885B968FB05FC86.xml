<emphasis id="C5D47B09D16BFFE03A41B8B2FDCBF95D" box="[595,608,1735,1760]" italics="true" pageId="1" pageNumber="54">c</emphasis>
<number id="4168B53CD16BFFE03A72B8A2FDC3F958" box="[608,616,1751,1765]" pageId="1" pageNumber="54" value="1.0">1</number>
