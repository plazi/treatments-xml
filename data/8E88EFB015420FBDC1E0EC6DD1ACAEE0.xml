<taxonomicName id="D2FECB8C4E6440C058C688383F490BA2" baseAuthorityName="Moure" baseAuthorityYear="1945" class="Insecta" family="Colletidae" genus="Zikanapis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="2" pageNumber="65" phylum="Arthropoda" rank="species" species="tucumana">
