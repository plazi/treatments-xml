<taxonomicName id="012179EE6C736E130C656DDD51D9F55F" baseAuthorityName="Moure" baseAuthorityYear="1945" class="Insecta" family="Colletidae" genus="Zikanapis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="2" pageNumber="65" phylum="Arthropoda" rank="species" species="tucumana">
