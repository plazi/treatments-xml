<taxonomicName id="7365FE45B8082F46F823E1354138F088" class="Insecta" family="Carabidae" genus="Sarothrocrepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sarothrocrepis" order="Coleoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="genus">Sarothrocrepis</taxonomicName>
