<taxonomicName id="4C2A4D155422FFE21937B62EBB339632" box="[287,576,864,886]" class="Insecta" family="Carabidae" genus="Foveocrotaphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="86" phylum="Arthropoda" rank="species" species="burmanicus" status="n. sp.">
