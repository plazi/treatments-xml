<taxonomicName id="F5A14D257F387D1A29926A37B740FED9" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[482,864,308,333]" class="Gymnolaemata" family="Bugulidae" genus="Bugula" kingdom="Animalia" order="Cheilostomatida" pageId="21" pageNumber="1079" phylum="Bryozoa" rank="species" species="neritina">
