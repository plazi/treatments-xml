<taxonomicName id="F5A14D257F387D1A2AF76A56B03BFEF9" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[647,1051,340,365]" class="Hydrozoa" family="Campanulariidae" genus="Obelia" kingdom="Animalia" order="Leptothecata" pageId="21" pageNumber="1079" phylum="Cnidaria" rank="species" species="dichotoma">
