<taxonomicName id="4A1BBF45CF939F79907EF9FE169D095A" authority="Schmidt, 1862" authorityName="Schmidt" authorityYear="1862" class="Demospongiae" family="Dictyodendrillidae" genus="Ditela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ditela nitens" order="Dendroceratida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="nitens">Ditela nitens</taxonomicName>
