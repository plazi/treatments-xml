<taxonomicName id="178EEEC771B8542B8EC6678A7C6F7863" LSID-ZBK="urn:lsid:zoobank.org:act:F589893C-76A9-4820-A8B9-B794837B3FB9" class="Actinopterygii" family="Ariidae" genus="Nedystoma" kingdom="Animalia" lsidName-ZBK="Nedystoma Ogilby 1898:32" order="Siluriformes" pageNumber="69" phylum="Chordata" rank="genus">Nedystoma</taxonomicName>
