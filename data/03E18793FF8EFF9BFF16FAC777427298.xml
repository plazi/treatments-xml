<number id="3D8024A2FF88FF9BFCC6FE9F7027713C" box="[839,894,268,294]" pageId="29" pageNumber="30" value="14.5">14.5</number>
; Va: 
<number id="3D8024A2FF88FF9BFC36FE9F7086713C" box="[951,991,268,294]" pageId="29" pageNumber="30" value="4.2">4.2</number>
