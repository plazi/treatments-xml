<number id="3D8024A2FF88FF9BFD88FEA171657156" box="[521,572,306,332]" pageId="29" pageNumber="30" value="16.1">16.1</number>
(in 
<typeStatus id="54F38827FF88FF9BFDEDFEA171857156" box="[620,732,306,332]" pageId="29" pageNumber="30">Holotype</typeStatus>
, HBUMM-05432-specimen 
<number id="3D8024A2FF88FF9BFBADFEA177637156" box="[1068,1082,306,332]" pageId="29" pageNumber="30" value="1.0">1</number>
