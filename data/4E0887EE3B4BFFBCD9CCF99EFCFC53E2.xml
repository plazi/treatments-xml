</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="01A14D7B3B48FFBDD849FAF7FDA154D1" authority="Tan 2009" authorityName="Tan" authorityYear="2009" box="[274,540,1996,2021]" class="Insecta" family="Bittacidae" genus="Terrobittacus" kingdom="Animalia" order="Mecoptera" pageId="2" pageNumber="67" phylum="Arthropoda" rank="genus">
