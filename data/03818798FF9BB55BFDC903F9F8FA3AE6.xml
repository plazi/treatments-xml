<taxonomicName id="4C284D0DFFA5B55AFDBD0314F9D13D7E" box="[527,729,1316,1340]" class="Magnoliopsida" family="Fabaceae" genus="Teline" kingdom="Plantae" order="Fabales" pageId="32" pageNumber="428" phylum="Tracheophyta" rank="species" species="stenopetala">
