<taxonomicName id="11D34D6E803BD609FE7DFC239D308397" authorityName="Bloch" authorityYear="1795" box="[441,606,973,998]" class="Actinopterygii" family="Muraenidae" genus="Gymnothorax" kingdom="Animalia" order="Anguilliformes" pageId="58" pageNumber="59" phylum="Chordata" rank="genus">
