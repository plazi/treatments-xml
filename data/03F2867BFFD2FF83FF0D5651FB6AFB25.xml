<taxonomicName id="4C5B4CEEFFD3FF83FDED530BFE9BFB85" authority="(LENGERSDORF)" baseAuthorityName="LENGERSDORF" baseAuthorityYear="1930" class="Insecta" family="Sciaridae" genus="Pseudosciara" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="180" phylum="Arthropoda" rank="species" species="thoracica">
