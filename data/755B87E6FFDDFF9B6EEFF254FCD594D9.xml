<taxonomicName id="3AF24D73FFDDFF9B6EEFF254FD209534" authority="Solier, 1848" authorityName="Solier" authorityYear="1848" box="[428,693,1106,1128]" class="Insecta" family="Tenebrionidae" genus="Eleodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="424" phylum="Arthropoda" rank="species" species="goryi">
<emphasis id="CF86EAE2FFDDFF9B6EEFF254FD209534" bold="true" box="[428,693,1106,1128]" pageId="11" pageNumber="424">
<emphasis id="CF86EAE2FFDDFF9B6EEFF254FDA19534" bold="true" box="[428,564,1106,1128]" italics="true" pageId="11" pageNumber="424">Eleodes goryi</emphasis>
<bibRefCitation id="99634B01FFDDFF9B6D7FF254FD209534" author="Solier" box="[572,693,1106,1128]" pageId="11" pageNumber="424" pagination="146 - 370" refId="ref12149" refString="Solier, A. J. 1848. Essai sur les collapterides. 14 e tribu - Blapsites. Baudi et Truqui Studi Entomologi 2: 146 - 370." type="journal article" year="1848">Solier, 1848</bibRefCitation>
