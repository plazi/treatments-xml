<taxonomicName id="D3F5427B404ED7C275E160893735039E" class="Insecta" family="Nemonychidae" genus="Mecomacer" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecomacer" order="Coleoptera" pageId="240" pageNumber="241" phylum="Arthropoda" rank="genus">Mecomacer</taxonomicName>
