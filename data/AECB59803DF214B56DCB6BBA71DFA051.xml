<taxonomicName id="86A17583F4FEF7180DC04CEBF46F05A4" authority="Voss, 1952" authorityName="Voss" authorityYear="1952" class="Insecta" family="Nemonychidae" genus="Rhynchitoplesius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhynchitoplesius" order="Coleoptera" pageId="240" pageNumber="241" phylum="Arthropoda" rank="genus">Rhynchitoplesius</taxonomicName>
