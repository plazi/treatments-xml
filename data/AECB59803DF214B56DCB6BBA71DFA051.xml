<taxonomicName id="BBADEBB5127C0C260A00B9A8249E1EE6" class="Insecta" family="Nemonychidae" genus="Mecomacer" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecomacer" order="Coleoptera" pageId="240" pageNumber="241" phylum="Arthropoda" rank="genus">Mecomacer</taxonomicName>
