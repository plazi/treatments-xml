depth with 
<taxonomicName id="70CA4D3CFF8CE2288719FE9FDE61644C" authority="Bale, 1884" authorityName="Bale" authorityYear="1884" box="[598,879,332,357]" class="Hydrozoa" family="Plumulariidae" genus="Halicornaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="97" pageNumber="98" phylum="Cnidaria" rank="species" species="longicauda">
