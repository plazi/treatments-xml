<taxonomicName id="70CA4D3CFF8DE22981D3F84ADC5762FF" authority="Meneghini, 1845" authorityName="Meneghini" authorityYear="1845" class="Hydrozoa" family="Aglaopheniidae" genus="Aglaophenia" kingdom="Animalia" order="Leptothecata" pageId="96" pageNumber="97" phylum="Cnidaria" rank="species" species="elongata">
