<treatmentCitationGroup id="60F211F2CC21FF82E8DD06290EA3F3F0" pageId="55" pageNumber="56">
<taxonomicName id="87E24D5FCC21FF82E8DD06290D74F3D7" authority="Pena Cantero, 1998: 78" authorityName="Pena Cantero" authorityPageNumber="78" authorityYear="1998" box="[151,619,1646,1668]" class="Hydrozoa" family="Halopterididae" genus="Schizotricha" kingdom="Animalia" order="Leptothecata" pageId="55" pageNumber="56" phylum="Cnidaria" rank="species" species="falcata">
