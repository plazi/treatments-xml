<number id="3DB92A0AB173CC352635FEA24F01FEBC" box="[1330,1395,259,281]" pageId="3" pageNumber="508" value="32.36">32, 36</number>
, 
<number id="3DB92A0AB173CC35267AFEA24FEAFEBC" box="[1405,1432,259,281]" pageId="3" pageNumber="508" value="38.0">38</number>
