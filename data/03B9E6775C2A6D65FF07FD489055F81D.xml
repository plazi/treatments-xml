<taxonomicName id="4C102CE25C2A6D65FE68FAA29084FAF9" box="[504,686,1374,1399]" class="Diplopoda" family="Aphelidesmidae" kingdom="Animalia" order="Polydesmida" pageId="48" pageNumber="49" phylum="Arthropoda" rank="family">Aphelidesmidae</taxonomicName>
and other unidentified 
<taxonomicName id="4C102CE25C2A6D65FC24FAA29662FAF9" box="[948,1096,1374,1399]" class="Diplopoda" kingdom="Animalia" order="Polydesmida" pageId="48" pageNumber="49" phylum="Arthropoda" rank="order">Polydesmida</taxonomicName>
