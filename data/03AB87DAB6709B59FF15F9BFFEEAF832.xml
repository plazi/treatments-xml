<collectingRegion id="49C6F82EB6709B59FCBFF822FC70F80F" box="[829,942,1974,1999]" country="Japan" name="Hokkaido" pageId="8" pageNumber="423">Hokkaido</collectingRegion>
, 
<collectingCountry id="F315765CB6709B59FC3DF823FBDCF80E" box="[959,1026,1975,1998]" name="Japan" pageId="8" pageNumber="423">Japan</collectingCountry>
; in the subtidal zone on red algae 
<taxonomicName id="4C024D4FB6709B59FF15F84EFF21F833" authorityName="J.V.Lamouroux" authorityYear="1813" box="[151,255,2010,2035]" class="Florideophyceae" family="Gelidiaceae" genus="Gelidium" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gelidiales" pageId="8" pageNumber="423" phylum="Rhodophyta" rank="genus">
<emphasis id="B976EADEB6709B59FF15F84EFF21F833" box="[151,255,2010,2035]" italics="true" pageId="8" pageNumber="423">Gelidium</emphasis>
</taxonomicName>
