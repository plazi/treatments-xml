<subSubSection id="7A96A596FFBAFFF6FF4B4CDAFC1AFD4A" box="[199,966,671,698]" pageId="7" pageNumber="30" type="etymology">
<paragraph id="3233F61DFFBAFFF6FF4B4CDAFC1AFD4A" blockId="7.[151,1436,151,818]" box="[199,966,671,698]" pageId="7" pageNumber="30">
<emphasis id="00F82A0FFFBAFFF6FF4B4CDAFE8FFD49" bold="true" box="[199,339,671,697]" pageId="7" pageNumber="30">Etymology.</emphasis>
