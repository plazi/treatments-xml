<subSubSection id="7A96A596FFBBFFF7FF1B4B08FCEBFA97" box="[151,823,1357,1383]" pageId="6" pageNumber="29" type="nomenclature">
<paragraph id="3233F61DFFBBFFF7FF1B4B08FCEBFA97" blockId="6.[151,823,1357,1418]" box="[151,823,1357,1383]" pageId="6" pageNumber="29">
<heading id="697B4171FFBBFFF7FF1B4B08FCEBFA97" bold="true" box="[151,823,1357,1383]" fontSize="11" level="1" pageId="6" pageNumber="29" reason="1">
<emphasis id="00F82A0FFFBBFFF7FF1B4B08FCEBFA97" bold="true" box="[151,823,1357,1383]" pageId="6" pageNumber="29">
2. 
<taxonomicName id="F58C8D9EFFBBFFF7FF384B08FD05FA97" authority="Fu-Ming &amp; Mao &amp; Chang, 2007" authorityName="Fu-Ming &amp; Mao &amp; Chang" authorityYear="2007" box="[180,729,1357,1383]" class="Insecta" family="Tettigoniidae" genus="Pseudokuzicus" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="29" phylum="Arthropoda" rank="species" species="quadridentatus" status="sp.nov." subGenus="Similkuzicus">
