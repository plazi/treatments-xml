<geoCoordinate id="EE315033FFF0FF86FCBCFA823AF4FABF" box="[835,915,1285,1307]" degrees="01.53" direction="south" orientation="latitude" pageId="75" pageNumber="75" precision="555" value="-1.53">01.53S</geoCoordinate>
, 
<geoCoordinate id="EE315033FFF0FF86FC61FA823A9CFABF" box="[926,1019,1285,1307]" degrees="136.14" direction="east" orientation="longitude" pageId="75" pageNumber="75" precision="555" value="136.14">136.14E</geoCoordinate>
