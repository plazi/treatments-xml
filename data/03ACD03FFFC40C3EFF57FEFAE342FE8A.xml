<taxonomicName id="4C051AAAFFC70C3FFEB2FBFFE187FB48" authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[336,496,1142,1163]" class="Insecta" family="Staphylinidae" genus="Lobrathium" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="339" phylum="Arthropoda" rank="species" species="pedes" status="sp. n.">
<emphasis id="B971BD3BFFC70C3FFEB2FBFFE187FB48" box="[336,496,1142,1163]" italics="true" pageId="38" pageNumber="339">Lobrathium pedes</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2420040FFC70C3FFE1BFBFEE258FB4F" box="[505,559,1143,1164]" pageId="38" pageNumber="339" rank="species">sp. n.</taxonomicNameLabel>
: habitus (
<emphasis id="B971BD3BFFC70C3FFD70FBFEE2C4FB4B" bold="true" box="[658,691,1143,1160]" pageId="38" pageNumber="339">134</emphasis>
); forebody (
<emphasis id="B971BD3BFFC70C3FFCCDFBFEE338FB4B" bold="true" box="[815,847,1143,1160]" pageId="38" pageNumber="339">135</emphasis>
); antenna (
<emphasis id="B971BD3BFFC70C3FFC26FBFEE392FB4B" bold="true" box="[964,997,1143,1160]" pageId="38" pageNumber="339">136</emphasis>
); male tergite VIII (
<emphasis id="B971BD3BFFC70C3FFF2DFB1DE086FB66" bold="true" box="[207,241,1172,1189]" pageId="38" pageNumber="339">137</emphasis>
); male sternite VII (
<emphasis id="B971BD3BFFC70C3FFE53FB1DE1A5FB66" bold="true" box="[433,466,1172,1189]" pageId="38" pageNumber="339">138</emphasis>
); male sternite VIII (
<emphasis id="B971BD3BFFC70C3FFD79FB1DE2CBFB66" bold="true" box="[667,700,1172,1189]" pageId="38" pageNumber="339">139</emphasis>
); aedeagus in lateral and in ventral view (
<emphasis id="B971BD3BFFC70C3FFBDDFB1DE4FBFB67" bold="true" box="[1087,1164,1172,1188]" pageId="38" pageNumber="339">140-141</emphasis>
); female tergite VIII (
<emphasis id="B971BD3BFFC70C3FFE68FB38E1DCFB02" bold="true" box="[394,427,1201,1217]" pageId="38" pageNumber="339">142</emphasis>
); female sternite VIII (
<emphasis id="B971BD3BFFC70C3FFD71FB38E2C3FB01" bold="true" box="[659,692,1201,1218]" pageId="38" pageNumber="339">143</emphasis>
); female tergites IX-X (
<emphasis id="B971BD3BFFC70C3FFC42FB38E3B6FB02" bold="true" box="[928,961,1201,1217]" pageId="38" pageNumber="339">144</emphasis>
