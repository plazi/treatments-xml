<taxonomicName id="4C6BA095FFDEFF8AFDF8FCBEFD4E7E24" box="[524,670,808,832]" class="Insecta" family="Curculionidae" genus="Staminodeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="1738" phylum="Arthropoda" rank="genus">Staminodeus</taxonomicName>
