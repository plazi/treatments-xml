<paragraph id="FF27D21622F489A6F3BC48CECFA112D9" pageId="101" pageNumber="102">
<taxonomicName id="DA77071197C63A456849A2245E9B7DC7" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus edithae" order="Hymenoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="edithae">Euplectrus edithae</taxonomicName>
