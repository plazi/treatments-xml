<subSubSection id="ACE6654FD95D2E60FF74FC0EFB98FC3A" box="[199,948,908,933]" pageId="39" pageNumber="40" type="etymology">
<paragraph id="E44336C4D95D2E60FF74FC0EFB98FC3A" blockId="39.[151,1437,151,933]" box="[199,948,908,933]" pageId="39" pageNumber="40">
