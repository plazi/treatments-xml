<collectingRegion id="EF6FF807FFF6FFF8FDB5FF11FD6CFF16" box="[556,649,151,176]" country="Guinea-Bissau" name="Quinara" pageId="27" pageNumber="28">Quinara</collectingRegion>
, 
<collectingRegion id="EF6FF807FFF6FFF8FD0CFF11FD13FF16" box="[661,758,151,176]" country="Guinea-Bissau" name="Tombali" pageId="27" pageNumber="28">Tombali</collectingRegion>
. 
<emphasis id="1FDFEAF7FFF6FFF8FD66FF11FC8FFF16" bold="true" box="[767,874,151,176]" pageId="27" pageNumber="28">Authors:</emphasis>
