<taxonomicName id="4C24379FFF98E26FFD50FC98FB6DC821" authority="Velip &amp; Rivonker, 2014" authorityName="Velip &amp; Rivonker" authorityYear="2014" box="[714,1238,860,885]" class="Malacostraca" family="Hexapodidae" genus="Hexapus" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="126" phylum="Arthropoda" rank="species" species="bidentatus">
