<taxonomicName id="8374D0DF3F6638511B2DD7B99EE9D0C7" authorityName="Foerster" authorityYear="1854" class="Insecta" family="Tenthredinidae" genus="Nematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nematus respondens" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="respondens">Nematus respondens</taxonomicName>
