<paragraph id="5FC8EAD61EA5508F46DCD07E38094D32" pageId="0" pageNumber="187">
<taxonomicName id="23A192311769700D4CF4A1C01CA2C4C7" authorityName="Thomson" authorityYear="1888" class="Insecta" family="Tenthredinidae" genus="Nematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nematus nitens" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="nitens">Nematus nitens</taxonomicName>
