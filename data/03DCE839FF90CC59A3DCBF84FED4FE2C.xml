<taxonomicName id="4C7522ACFF91CC56A255BF69FD5CF993" authority="Johansson" authorityName="Johansson" authorityYear="2021" box="[286,666,1566,1590]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="species" species="apocalypticus" status="sp. nov.">
