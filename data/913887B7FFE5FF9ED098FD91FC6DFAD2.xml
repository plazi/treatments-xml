<taxonomicName id="DE914D22FFE5FF9ED58DFC2BFE62FBD6" authority="Roux, 1978" authorityName="Roux" authorityYear="1978" class="Crinoidea" family="Bathycrinidae" genus="Paraconocrinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="27" pageNumber="78" phylum="Echinodermata" rank="species" species="handiaensis">
