<collectingRegion id="D9098FF7FD0FFFC2FCAC0AE1B54AFA0B" box="[803,862,1509,1525]" country="China" name="Sichuan" pageId="1" pageNumber="452">Sichuan</collectingRegion>
<location id="1E1217CEFD0FFFC2FCEB0AE1B6D9F9F4" LSID="urn:lsid:plazi:treatment:9364F003FD0FFFC0FE380C73B421FBCB:1E1217CEFD0FFFC2FCEB0AE1B6D9F9F4" country="China" name="Department of Science" pageId="1" pageNumber="452" stateProvince="Sichuan">Department of Science</location>
