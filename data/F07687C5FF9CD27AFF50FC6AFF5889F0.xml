<taxonomicName id="BFDF4D50FF9CD27AFD89FADBFCB7891C" box="[590,774,1336,1362]" class="Malacostraca" family="Penaeidae" genus="Penaeus" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="304" phylum="Arthropoda" rank="species" species="subtilis">
