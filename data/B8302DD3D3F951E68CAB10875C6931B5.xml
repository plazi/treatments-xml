<taxonomicName id="56D99E63C50713EBBCFB7E1518C65D58" authorityName="Regel &amp; Schmalh" authorityYear="1879" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus subhirsutus" order="Solanales" pageId="176" pageNumber="177" phylum="Tracheophyta" rank="species" species="subhirsutus">
