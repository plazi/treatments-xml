(n = 
<number id="3DFEAC6CFF96FFAA7CC1C2D6FE4E2880" box="[432,464,654,678]" pageId="12" pageNumber="13" value="22.0">22</number>
): HB = 
<number id="3DFEAC6CFF96FFAA7F58C2D6FD132880" box="[553,653,654,678]" pageId="12" pageNumber="13" value="150.0" valueMax="190.0" valueMin="110.0">110–190</number>
, T = 
<number id="3DFEAC6CFF96FFAA7FB6C2D6FCB22880" box="[711,812,654,678]" pageId="12" pageNumber="13" value="225.0" valueMax="245.0" valueMin="205.0">205–245</number>
, HF = 
<number id="3DFEAC6CFF96FFAA7E09C2D6FC5F2880" box="[888,961,654,678]" pageId="12" pageNumber="13" value="26.0" valueMax="30.0" valueMin="22.0">22–30</number>
, E = 
<number id="3DFEAC6CFF96FFAA7E8AC2D6FBDD2880" box="[1019,1091,654,678]" pageId="12" pageNumber="13" value="26.5" valueMax="34.0" valueMin="19.0">19–34</number>
, W = 
<number id="3DFEAC6CFF96FFAA79F6C2D6FB412880" box="[1159,1247,654,678]" pageId="12" pageNumber="13" value="105.5" valueMax="140.0" valueMin="71.0">71–140</number>
