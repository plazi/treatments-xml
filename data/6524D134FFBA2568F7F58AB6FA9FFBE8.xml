<taxonomicName id="2A8D1BA1FFB9256EF6CE8EE0FDD2F803" box="[428,581,1921,1942]" class="Insecta" family="Cerambycidae" genus="Mimonemophas" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="597" phylum="Arthropoda" rank="genus">
<emphasis id="DFF9BC30FFB9256EF6CE8EE0FDD2F803" box="[428,581,1921,1942]" italics="true" pageId="2" pageNumber="597">Mimonemophas</emphasis>
</taxonomicName>
spp. 1–4, 7–9. 
<taxonomicName id="2A8D1BA1FFB9256EF5B58EE0FC65F803" authority="Breuning." authorityName="Breuning." box="[727,1010,1921,1942]" class="Insecta" family="Cerambycidae" genus="Mimonemophas" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="597" phylum="Arthropoda" rank="species" species="quadrifasciatus">
