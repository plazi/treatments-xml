<taxonomicName id="22B3C482FFBCD47A485CFDBD3FBC8BE9" box="[1141,1269,626,651]" class="Insecta" family="Chrysomelidae" genus="Serraphula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="debiasei" status="sp. n.">
<emphasis id="D7C76313FFBCD47A485CFDBD3FBC8BE9" box="[1141,1269,626,651]" italics="true" pageId="12" pageNumber="13">S. debiasei</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CCF4DE68FFBCD47A48D5FDBD3E738BEE" box="[1276,1338,626,652]" pageId="12" pageNumber="13" rank="species">sp. n.</taxonomicNameLabel>
