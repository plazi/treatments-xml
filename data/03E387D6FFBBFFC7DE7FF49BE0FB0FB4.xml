, 
<taxonomicName id="4C4A4D43FFBBFFC7D87DF388E0F70FB4" authority="(S. Moricand, 1846)" baseAuthorityName="S. Moricand" baseAuthorityYear="1846" class="Gastropoda" family="Simpulopsidae" genus="Simpulopsis" kingdom="Animalia" order="Stylommatophora" pageId="23" pageNumber="80" phylum="Mollusca" rank="species" species="rufovirens" subGenus="Simpulopsis">
