<typeStatus id="400756963746FB4324FDF9E55933F9ED" box="[220,266,1622,1646]" pageId="10" pageNumber="11">type</typeStatus>
of range (
<taxonomicName id="58BC93B73746FB432558F9E55A66F9ED" baseAuthorityName="Fieber" baseAuthorityYear="1861" box="[377,607,1622,1646]" class="Insecta" family="Miridae" genus="Adelphocoris" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="detritus">
