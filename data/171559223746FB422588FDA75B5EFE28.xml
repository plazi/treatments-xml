<figureCitation id="0787F4B13746FB4327BEF9E55BC9F9EE" box="[927,1008,1622,1646]" captionStart="Fig" captionStartId="10.[122,153,1516,1535]" captionTargetBox="[138,1132,1009,1504]" captionTargetId="figure-312@10.[126,1147,994,1512]" captionTargetPageId="10" captionText="Fig. 6. Distribution of Adelphocoris detritus (A) and Criocoris nigripes (B) in Poland; ● – literature data, ● – confirmed data." figureDoi="http://doi.org/10.5281/zenodo.6252000" httpUri="https://zenodo.org/record/6252000/files/figure.png" pageId="10" pageNumber="11">Fig. 6B</figureCitation>
, 
<taxonomicName id="58BC93B73746FB4327DCF9E558E3F90E" baseAuthorityName="Stal" baseAuthorityYear="1858" class="Insecta" family="Miridae" genus="Eurycolpus" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="flaveolus">
<emphasis id="ADC834263746FB4327DCF9E558E3F90E" italics="true" pageId="10" pageNumber="11">Eurycolpus flaveolus</emphasis>
</taxonomicName>
and 
<taxonomicName id="58BC93B73746FB432536F9C659D1F90F" baseAuthorityName="Fieber" baseAuthorityYear="1859" box="[279,488,1653,1676]" class="Insecta" family="Berytidae" genus="Berytinus" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="signoreti">
