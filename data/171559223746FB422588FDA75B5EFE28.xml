<taxonomicName id="58BC93B73746FB43276EF9C65C49F90E" baseAuthorityName="Schrank" baseAuthorityYear="1758" box="[847,1136,1653,1677]" class="Insecta" family="Miridae" genus="Macrotylus" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="quadrilineatus">
