<taxonomicName id="58BC93B73747FB42245BFF0F596DFF57" baseAuthorityName="Fieber" baseAuthorityYear="1861" box="[122,340,188,212]" class="Insecta" family="Miridae" genus="Stenodema" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="sericans">
