<treatmentCitationGroup id="AB442200FFDAFFEACEECF8A1FCC25FA5" box="[151,894,1910,1934]" pageId="35" pageNumber="36">
<taxonomicName id="4C547EADFFDAFFEACEECF8A1FE3F5FA5" authorityName="Kuhlmann" authorityYear="2007" box="[151,387,1910,1934]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="xuechengensis">
