<taxonomicName id="4C547EADFFDFFFEFCA9EFF08FAE958D2" baseAuthorityName="Fourcroy" baseAuthorityYear="1785" box="[1253,1365,223,249]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="fodiens">
