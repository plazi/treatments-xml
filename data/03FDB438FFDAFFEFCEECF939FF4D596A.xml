<emphasis id="B920D93CFFDFFFEFCEBCFF08FE8758D2" bold="true" box="[199,315,223,249]" pageId="38" pageNumber="39">Remarks.</emphasis>
<taxonomicName id="4C547EADFFDFFFEFCF3AFF08FDF858D2" authorityName="Kuhlmann" authorityYear="2007" box="[321,580,223,249]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="xuechengensis">
