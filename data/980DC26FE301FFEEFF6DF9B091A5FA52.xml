<quantity id="D75CDE9CE300FFEEFB1AFCB39698FCF1" box="[1207,1268,834,858]" metricMagnitude="3" metricUnit="m" metricValue="6.0" pageId="43" pageNumber="42" unit="km" value="6.0">6 km</quantity>
