<taxonomicName id="4C0B9BC6FF9FFFA096F7FA4C550A2189" box="[1264,1380,1477,1498]" class="Rhabditophora" family="Meidiamidae" genus="Meidiama" kingdom="Animalia" order="Seriata" pageId="1" pageNumber="760" phylum="Platyhelminthes" rank="genus">
