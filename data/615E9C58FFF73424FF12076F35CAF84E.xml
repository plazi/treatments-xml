<emphasis id="DB83F15CFFF43424FF1201223748FB59" bold="true" box="[151,274,1141,1163]" pageId="2" pageNumber="595">FIGURE 1.</emphasis>
<taxonomicName id="2EF756CDFFF43424FE9D012237F3FB59" authority="Myers &amp; Trivedi &amp; Gosavi &amp; Vachhrajani, 2017" authorityName="Myers &amp; Trivedi &amp; Gosavi &amp; Vachhrajani" authorityYear="2017" box="[280,425,1141,1163]" class="Malacostraca" family="Hyalidae" genus="Parhyale" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="595" phylum="Arthropoda" rank="species" species="piloi" status="sp. nov.">
<emphasis id="DB83F15CFFF43424FE9D012237F3FB59" box="[280,425,1141,1163]" italics="true" pageId="2" pageNumber="595">Parhyale piloi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="C0B04C27FFF43424FE2A0121345AFB59" box="[431,512,1142,1163]" pageId="2" pageNumber="595" rank="species">
<emphasis id="DB83F15CFFF43424FE2A0121345AFB59" bold="true" box="[431,512,1142,1163]" pageId="2" pageNumber="595">sp. nov.</emphasis>
