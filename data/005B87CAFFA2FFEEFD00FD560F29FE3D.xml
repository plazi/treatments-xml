from 
<location id="8D2D6007FFA2FFE0FEE0FCB30AEBFCF9" LSID="urn:lsid:plazi:treatment:005B87CAFFA2FFEEFD00FD560F29FE3D:8D2D6007FFA2FFE0FEE0FCB30AEBFCF9" box="[353,510,819,841]" country="Venezuela" county="Platnick" municipality="Shear" name="Puerto Cabello" pageId="7" pageNumber="245">Puerto Cabello</location>
