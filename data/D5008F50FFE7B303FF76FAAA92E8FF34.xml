<quantity id="9A5193A3FFE7B302FB8FF81F92ECF81E" box="[1134,1221,1977,2003]" metricMagnitude="3" metricUnit="m" metricValue="2.6" pageId="14" pageNumber="25" unit="m" value="2600.0">2600 m</quantity>
