<taxonomicName id="4C4A4D60C200AF67C204D7AFFD4640FE" authority="PIC" authorityName="PIC" authorityYear="1914" box="[544,666,1448,1468]" class="Insecta" family="Ichneumonidae" genus="Platylabus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="524" phylum="Arthropoda" rank="species" species="lativentris">
<emphasis id="B93EEAF1C200AF67C204D7AFFDAB40F8" box="[544,631,1448,1468]" italics="true" pageId="3" pageNumber="524">lativentris</emphasis>
PIC
</taxonomicName>
<typeStatus id="54F18841C200AF67C2BBD7AFFD1A40F8" box="[671,710,1448,1468]" pageId="3" pageNumber="524">type</typeStatus>
