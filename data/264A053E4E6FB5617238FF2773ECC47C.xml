(MNHN-B 28708), cl 
<quantity id="691B19CD4E6FB56772C7FA8C736FC507" box="[975,1063,1383,1406]" metricMagnitude="-3" metricUnit="m" metricValue="8.7" pageId="67" pageNumber="566" unit="mm" value="8.7">8.7 mm</quantity>
, cw 
<quantity id="691B19CD4E6FB5677569FA8C73F1C507" box="[1121,1209,1383,1406]" metricMagnitude="-3" metricUnit="m" metricValue="9.3" pageId="67" pageNumber="566" unit="mm" value="9.3">9.3 mm</quantity>
