<paragraph id="970336F5FF84FFFEFB10FE2F1752FD90" blockId="12.[125,700,159,1776]" pageId="12" pageNumber="213">
reported from 
<taxonomicName id="50BC4D76FF84FFFEF9C1FE0D1414FE88" box="[183,326,359,382]" class="Aves" kingdom="Animalia" order="Passeriformes" pageId="12" pageNumber="213" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
(
<taxonomicName id="50BC4D76FF84FFFEF822FE0D1496FE88" authorityName="Illiger" authorityYear="1811" box="[340,452,359,382]" class="Aves" family="Passeridae" kingdom="Animalia" order="Passeriformes" pageId="12" pageNumber="213" phylum="Chordata" rank="family">Passeridae</taxonomicName>
, 
<taxonomicName id="50BC4D76FF84FFFEF8B9FE0D1769FE88" authorityName="Vigors" authorityYear="1825" box="[463,571,359,382]" class="Aves" family="Alaudidae" kingdom="Animalia" order="Passeriformes" pageId="12" pageNumber="213" phylum="Chordata" rank="family">Alaudidae</taxonomicName>
, 
<taxonomicName id="50BC4D76FF84FFFEFB30FE0D17E5FE88" authorityName="Illiger" authorityYear="1811" box="[582,695,359,382]" class="Aves" family="Estrildidae" kingdom="Animalia" order="Passeriformes" pageId="12" pageNumber="213" phylum="Chordata" rank="family">Estrildidae</taxonomicName>
, 
<taxonomicName id="50BC4D76FF84FFFEF90BFEE215A6FE69" authorityName="Richmond" authorityYear="1908" box="[125,244,392,415]" class="Aves" family="Prunellidae" kingdom="Animalia" order="Passeriformes" pageId="12" pageNumber="213" phylum="Chordata" rank="family">Prunellidae</taxonomicName>
