<paragraph id="8BCF36D1FFCFFFC2AFB85222FB94FEB4" blockId="5.[140,1107,1539,1672]" lastBlockId="6.[140,1108,167,1003]" lastPageId="6" lastPageNumber="55" pageId="5" pageNumber="54">
<materialsCitation id="3B183C8CFFCFFFC1AFB85222FE54F916" ID-GBIF-Occurrence="2608993580" location="Figs" municipality="Head" pageId="5" pageNumber="54" specimenCount="1" typeStatus="paratype">
<typeStatus id="54CB8873FFCFFFC1AFB85222FE85F9FA" box="[185,282,1610,1636]" pageId="5" pageNumber="54">Paratype</typeStatus>
female (slide mounted, 
<location id="8EAF600AFFCFFFC1AD425222FDEFF9FA" LSID="urn:lsid:plazi:treatment:03D987C7FFC9FFC2AF8D5618FD17FC74:8EAF600AFFCFFFC1AD425222FDEFF9FA" box="[579,624,1610,1636]" municipality="Head" name="Figs" pageId="5" pageNumber="54">Figs</location>
