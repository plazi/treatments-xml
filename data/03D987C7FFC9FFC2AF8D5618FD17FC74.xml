and 
<collectionCode id="ED61AE14FFCCFFC2AECB54A2FE77FF7A" box="[458,488,202,228]" country="Norway" name="Mykotektet, National Veterinary Institute" pageId="6" pageNumber="55">VI</collectionCode>
