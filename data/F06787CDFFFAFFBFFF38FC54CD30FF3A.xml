<taxonomicName id="BFCE4D58FFFAFFBAFA28F8F3CE42F8F0" authority="Skoracki and Dabert, 2000" authorityName="Skoracki and Dabert" authorityYear="2000" class="Aves" family="Syringophilidae" genus="Syringophilopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="1" pageNumber="35" phylum="Chordata" rank="species" species="albicollisi">
