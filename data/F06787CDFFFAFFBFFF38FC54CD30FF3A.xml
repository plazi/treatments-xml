<emphasis id="4ABAEAC9FFFAFFBAFF75FADACC8AFA8E" box="[218,244,1323,1347]" italics="true" pageId="1" pageNumber="35">
<number id="CE0624FCFFFAFFBAFF75FADACC96FA8E" box="[218,232,1323,1347]" pageId="1" pageNumber="35" value="3.0">3</number>
