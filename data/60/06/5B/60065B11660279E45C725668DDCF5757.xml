<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia" order="Fabales" pageId="4" pageNumber="97" phylum="Tracheophyta" rank="genus">Acacia</taxonomicName>
