<emphasis box="[764,915,296,320]" italics="true" pageId="18" pageNumber="67">G. nambulica</emphasis>
</taxonomicName>
, and 
<taxonomicName baseAuthorityName="Blyth" baseAuthorityYear="1860" box="[982,1086,296,320]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="18" pageNumber="67" phylum="Chordata" rank="species" species="notata">
