<emphasis box="[1173,1326,296,319]" italics="true" pageId="18" pageNumber="67">G. namyaensi</emphasis>
</taxonomicName>
; 
<quantity box="[1343,1403,295,320]" metricMagnitude="-1" metricUnit="m" metricValue="7.366" pageId="18" pageNumber="67" unit="in" value="29.0">29 in</quantity>
<taxonomicName authorityName="Kullander &amp; Fang" authorityYear="2004" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="18" pageNumber="67" phylum="Chordata" rank="species" species="poecilura">
