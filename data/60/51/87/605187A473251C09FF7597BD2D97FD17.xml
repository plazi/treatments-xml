; 
<quantity box="[403,504,295,320]" metricMagnitude="-1" metricUnit="m" metricValue="9.906" metricValueMax="10.16" metricValueMin="9.652" pageId="18" pageNumber="67" unit="in" value="39.0" valueMax="40.0" valueMin="38.0">38–40 in</quantity>
<taxonomicName box="[514,678,296,320]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="18" pageNumber="67" phylum="Chordata" rank="species" species="chakpiensis">
