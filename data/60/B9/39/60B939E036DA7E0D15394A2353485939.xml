<paragraph pageId="57" pageNumber="58">
Type locality, 880 m, 
<geoCoordinate direction="north" orientation="latitude" precision="5" value="41.5138">41.5138°N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="5" value="20.3142">20.3142°E</geoCoordinate>
