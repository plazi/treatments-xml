<paragraph blockId="13.[227,492,1937,1964]" box="[227,492,1937,1964]" pageId="13" pageNumber="14">
<collectionCode box="[227,290,1938,1964]" pageId="13" pageNumber="14">BODY</collectionCode>
<collectionCode box="[298,380,1942,1961]" pageId="13" pageNumber="14">LENGTH</collectionCode>
