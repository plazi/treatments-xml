<paragraph blockId="8.[227,492,351,378]" box="[227,492,351,378]" pageId="8" pageNumber="9">
<collectionCode box="[227,290,352,378]" pageId="8" pageNumber="9">BODY</collectionCode>
<collectionCode box="[298,380,356,375]" pageId="8" pageNumber="9">LENGTH</collectionCode>
. 
<quantity box="[395,488,351,378]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="8" pageNumber="9" unit="mm" value="2.2">2.2 mm</quantity>
