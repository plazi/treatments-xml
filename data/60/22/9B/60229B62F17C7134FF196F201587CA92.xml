<taxonomicName authorityName="Sjostedt" authorityYear="1913" box="[748,914,1125,1151]" class="Insecta" family="Phaneropteridae" genus="Morgenia" kingdom="Animalia" order="Orthoptera" pageId="32" pageNumber="433" phylum="Arthropoda" rank="species" species="rubricornis">
