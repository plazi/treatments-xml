<paragraph blockId="1.[151,609,989,1050]" box="[151,381,1023,1050]" pageId="1" pageNumber="58">
(
<figureCitation box="[159,271,1023,1050]" captionStart-0="FIGURES 1 – 3" captionStart-1="FIGURES 4 – 6" captionStartId-0="2.[151,273,1079,1103]" captionStartId-1="3.[151,269,1027,1051]" captionTargetBox-0="[470,1111,232,1022]" captionTargetBox-1="[379,1209,214,994]" captionTargetId-0="figure@2.[344,1238,179,1072]" captionTargetId-1="figure@3.[370,1217,197,1007]" captionTargetPageId-0="2" captionTargetPageId-1="3" captionText-0="FIGURES 1 – 3. Eumantispa paraharmandi sp. nov. Male. 1. terminalia, lateral view; 2. terminalia, dorsal view; 3. terminalia, ventral view. Abbreviations: ect, ectoproct; t, tergite; st, sternite." captionText-1="FIGURES 4 – 6. Eumantispa paraharmandi sp. nov. Male. 4. genitalia, ventral view; 5. genitalia, lateral view. Female. 6. spermatheca. Abbreviations: fc, fertilization canal; gcx, gonocoxites; gnc, gonarcus; m, mediuncus; ml, median lobe; pm, pseudopenal membrane; pp, pseudopenis." httpUri-0="https://zenodo.org/record/199091/files/figure.png" httpUri-1="https://zenodo.org/record/199093/files/figure.png" pageId="1" pageNumber="58">
Figs. 
<number box="[226,241,1024,1050]" pageId="1" pageNumber="58" value="1.0">1</number>
<emphasis bold="true" box="[240,255,1023,1049]" pageId="1" pageNumber="58">–</emphasis>
6
</figureCitation>
, 
<figureCitation box="[284,316,1024,1050]" captionStart="FIGURE 15" captionStartId="8.[151,257,1887,1911]" captionText="FIGURE 15. Eumantispa paraharmandi sp. nov. a. fore leg of female, inner side; b. fore leg of male, inner side; c. dorsal view." pageId="1" pageNumber="58">15</figureCitation>
, 
<figureCitation box="[328,357,1024,1050]" captionStart="FIGURE 19" captionStartId="10.[151,255,1150,1174]" captionTargetBox="[181,1416,233,1066]" captionTargetId="figure@10.[151,1436,194,1129]" captionTargetPageId="10" captionText="FIGURE 19. Head, anterior view. a. Eumantispa paraharmandi sp. nov. b. Eumantispa harmandi, male. c. Eumantispa harmandi, female. d. Eumantispa tibetana. e. Eumantispa fuscicolla." httpUri="https://zenodo.org/record/199099/files/figure.png" pageId="1" pageNumber="58">
<number box="[328,357,1024,1050]" pageId="1" pageNumber="58" value="19.0">19</number>
</figureCitation>
