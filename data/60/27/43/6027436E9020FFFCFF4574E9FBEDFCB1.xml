(mic) on leaves of 
<taxonomicName box="[267,415,655,681]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="4" pageNumber="408" phylum="Chordata" rank="species" species="integra">
<emphasis box="[267,415,655,681]" italics="true" pageId="4" pageNumber="408">Salix integra</emphasis>
</taxonomicName>
[
<taxonomicName box="[432,559,654,681]" class="Magnoliopsida" family="Salicaceae" kingdom="Plantae" order="Malpighiales" pageId="4" pageNumber="408" phylum="Tracheophyta" rank="family">Salicaceae</taxonomicName>
], 
