<taxonomicName authorityName="Kudo" authorityYear="1991" box="[996,1111,1903,1927]" class="Insecta" family="Thripidae" genus="Hydatothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="409" phylum="Arthropoda" rank="species" species="elaeagni">
