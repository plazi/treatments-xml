<paragraph pageId="0" pageNumber="73">
<taxonomicName class="Gastropoda" family="Discodorididae" genus="Jorunna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Jorunna" order="Cephalaspidea" pageId="0" pageNumber="73" phylum="Mollusca" rank="genus">
