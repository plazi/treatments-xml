<paragraph blockId="13.[401,1242,1794,1817]" box="[401,1242,1794,1817]" pageId="13" pageNumber="14">
<taxonomicName box="[401,562,1794,1817]" class="Malacostraca" family="Amathillopsidae" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="14" phylum="Arthropoda" rank="subFamily" subFamily="Parepimeriinae">Parepimeriinae</taxonomicName>
(
<taxonomicName box="[729,931,1794,1817]" class="Malacostraca" family="Amathillopsidae" genus="Parepimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="perda">
