<typeStatus box="[198,269,701,727]" pageId="3" pageNumber="66">Types</typeStatus>
. 
<typeStatus box="[295,407,701,727]" pageId="3" pageNumber="66">Holotype</typeStatus>
</emphasis>
ɗ 
<collectingCountry box="[463,548,701,727]" name="Spain" pageId="3" pageNumber="66">SPAIN</collectingCountry>
, Andalucia, Cortija del Alcazar, 
<quantity box="[981,1136,701,727]" metricMagnitude="3" metricUnit="m" metricValue="1.095" metricValueMax="1.2" metricValueMin="0.99" pageId="3" pageNumber="66" unit="m" value="1095.0" valueMax="1200.0" valueMin="990.0">990-1200 m</quantity>
, 
<geoCoordinate box="[1158,1296,701,727]" direction="north" orientation="latitude" pageId="3" pageNumber="66" precision="15" value="36.920834">
N 
<number box="[1182,1214,701,727]" pageId="3" pageNumber="66" value="36.0">36</number>
° 
<number box="[1224,1255,701,727]" pageId="3" pageNumber="66" value="55.0">55</number>
' 
<number box="[1258,1287,701,727]" pageId="3" pageNumber="66" value="15.0">15</number>
&quot;
</geoCoordinate>
