W 
<number box="[1347,1364,701,727]" pageId="3" pageNumber="66" value="4.0">4</number>
° 
<number box="[1373,1388,701,727]" pageId="3" pageNumber="66" value="5.0">5</number>
' 
<number box="[1391,1421,701,727]" pageId="3" pageNumber="66" value="18.0">18</number>
&quot;
</geoCoordinate>
, 
<date box="[151,265,741,767]" pageId="3" pageNumber="66" value="2006-06-09">
