and 
<taxonomicName box="[620,736,1791,1814]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
