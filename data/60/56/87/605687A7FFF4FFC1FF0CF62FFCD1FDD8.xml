<collectingCountry box="[1330,1435,1482,1506]" name="Jamaica" pageId="6" pageNumber="5">Jamaica</collectingCountry>
, 
<collectingCountry box="[192,336,1514,1538]" name="Puerto Rico" pageId="6" pageNumber="5">Puerto Rico</collectingCountry>
