<subSubSection lastPageId="13" lastPageNumber="12" pageId="12" pageNumber="11" type="biology_ecology">
<paragraph blockId="12.[192,1440,1615,1929]" pageId="12" pageNumber="11">
<emphasis bold="true" box="[192,276,1616,1640]" pageId="12" pageNumber="11">Hosts.</emphasis>
<taxonomicName box="[281,546,1615,1639]" class="Magnoliopsida" family="Apocynaceae" genus="Allamanda" kingdom="Plantae" order="Gentianales" pageId="12" pageNumber="11" phylum="Tracheophyta" rank="species" species="cathartica">
<emphasis box="[281,546,1615,1639]" italics="true" pageId="12" pageNumber="11">Allamanda cathartica</emphasis>
