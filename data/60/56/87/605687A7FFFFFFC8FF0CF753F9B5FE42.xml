<paragraph blockId="13.[192,624,854,879]" box="[192,624,854,879]" pageId="13" pageNumber="12">
<heading bold="true" box="[192,624,854,879]" fontSize="10" level="2" pageId="13" pageNumber="12" reason="3">
<taxonomicName authority="Whalley, 1976" authorityName="Whalley" authorityYear="1976" box="[192,624,854,879]" class="Insecta" family="Thyrididae" genus="Banisia" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="12" phylum="Arthropoda" rank="species" species="argutula">
<emphasis bold="true" box="[192,624,854,879]" pageId="13" pageNumber="12">
<emphasis bold="true" box="[192,430,854,878]" italics="true" pageId="13" pageNumber="12">Banisia argutula</emphasis>
<bibRefCitation author="Whalley, P. E. S." bookContentInfo="194 p." box="[437,624,854,879]" journalOrPublisher="British Museum Natural History. The Gresham Press; London" pageId="13" pageNumber="12" refId="ref18191" refString="Whalley, P. E. S. 1976. Tropical leaf moths: a monograph of the subfamily Striglininae (Lepidoptera: Thyrididae). British Museum Natural History. The Gresham Press; London. 194 p. + 68 pls." title="Tropical leaf moths: a monograph of the subfamily Striglininae (Lepidoptera: Thyrididae)" type="book" year="1976">Whalley, 1976</bibRefCitation>
</emphasis>
</taxonomicName>
