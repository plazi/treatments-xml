<collectingCountry box="[929,1054,541,563]" name="Madagascar" pageId="4" pageNumber="829">Madagascar</collectingCountry>
; in 
<collectingCountry box="[178,247,570,591]" name="France" pageId="4" pageNumber="829">France</collectingCountry>
