D i s t r i b u t i o n i n T u r k e y 
<collectingCountry box="[618,697,1508,1530]" name="Turkey" pageId="3" pageNumber="828">Anatolia</collectingCountry>
(
<bibRefCitation author="HORVATH G" box="[727,898,1508,1530]" pageId="3" pageNumber="828" pagination="21 - 30" refId="ref4759" refString="HORVATH G. (1883): Heteroptera Anatolica in regione Brussae collecta. - Termeszettudomanyi Fuzetek 7: 21 - 30." type="journal article" year="1883">HORVATH 1883</bibRefCitation>
); 
<collectingRegion box="[924,993,1508,1530]" country="Turkey" name="Artvin" pageId="3" pageNumber="828">Artvin</collectingRegion>
, 
<collectingRegion box="[1011,1081,1509,1530]" country="Turkey" name="Konya" pageId="3" pageNumber="828">Konya</collectingRegion>
, Ni de (
<bibRefCitation author="ONDER F" box="[254,389,1537,1559]" pageId="3" pageNumber="828" pagination="1 - 20" refId="ref4914" refString="ONDER F. (1980): Preliminary list of the Turkish Reduviidae (Heteroptera). - Journal of the Agricultural Faculty of Ege University 17: 1 - 20." type="journal article" year="1980">ÖNDER 1980</bibRefCitation>
