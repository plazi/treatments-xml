<taxonomicName authorityName="Wongkamhaeng &amp; Dumrongrojwattana &amp; Sumitrakij &amp; Keetapithchayakul" authorityYear="2022" family="Protorchestiidae" genus="Thailandorchestia" kingdom="Animalia" lsidName="Thailandorchestia rhizophila" order="Amphipoda" pageId="0" pageNumber="139" rank="species" species="rhizophila">
