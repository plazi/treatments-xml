from elevations of 
<quantity metricMagnitude="3" metricUnit="m" metricValue="2.525" metricValueMax="2.67" metricValueMin="2.38" pageId="5" pageNumber="240" unit="m" value="2525.0" valueMax="2670.0" valueMin="2380.0">2380– 2670 m</quantity>
