(Simon, 
<number box="[364,424,1287,1311]" pageId="1" pageNumber="236" value="1897.0">1897</number>
)
</taxonomicName>
, 
<taxonomicName authority="F.O. Pickard-Cambridge, 1902" authorityName="F.O. Pickard-Cambridge" authorityYear="1902" box="[446,900,1286,1311]" class="Arachnida" family="Ctenidae" genus="Anahita" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="236" phylum="Arthropoda" rank="species" species="feai">
<emphasis box="[446,522,1287,1311]" italics="true" pageId="1" pageNumber="236">A. feai</emphasis>
