<quantity box="[1258,1390,582,607]" metricMagnitude="2" metricUnit="m" metricValue="4.38" metricValueMax="7.0" metricValueMin="1.76" pageId="13" pageNumber="588" unit="m" value="438.0" valueMax="700.0" valueMin="176.0">176–700 m</quantity>
