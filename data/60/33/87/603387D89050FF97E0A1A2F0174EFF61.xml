, 
<collectingCountry box="[677,744,152,175]" name="Japan" pageId="8" pageNumber="583">Japan</collectingCountry>
, East and 
<collectingRegion box="[873,938,151,176]" country="Maldives" name="Dhekunu" pageId="8" pageNumber="583">South</collectingRegion>
<collectingCountry box="[951,1018,151,176]" name="China" pageId="8" pageNumber="583">China</collectingCountry>
