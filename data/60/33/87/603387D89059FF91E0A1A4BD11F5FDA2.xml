<collectingRegion box="[359,424,510,535]" country="Maldives" name="Dhekunu" pageId="14" pageNumber="589">South</collectingRegion>
<collectingCountry box="[431,498,510,535]" name="China" pageId="14" pageNumber="589">China</collectingCountry>
Sea, 
<collectingCountry box="[560,686,510,535]" name="Philippines" pageId="14" pageNumber="589">Philippines</collectingCountry>
and 
<collectingCountry box="[740,852,510,535]" name="Indonesia" pageId="14" pageNumber="589">Indonesia</collectingCountry>
; at depths of 
<quantity box="[1001,1143,510,535]" metricMagnitude="2" metricUnit="m" metricValue="6.75" metricValueMax="12.5" metricValueMin="1.0" pageId="14" pageNumber="589" unit="m" value="675.0" valueMax="1250.0" valueMin="100.0">100–1250 m</quantity>
(
<bibRefCitation author="Li" box="[1157,1264,510,535]" pageId="14" pageNumber="589" refString="Li, X. (2006 a) Additional Pandalid shrimps from the South China Sea (Crustacea: Decapoda: Caridea), with description of a new species. Raffles Bulletin of Zoology, 54, 361 - 372." type="journal article" year="2006" yearSuffix="a">Li 2006a</bibRefCitation>
).
</paragraph>
