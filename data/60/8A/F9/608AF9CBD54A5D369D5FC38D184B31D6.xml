<paragraph pageId="0" pageNumber="1">
<taxonomicName authority="C.M.Tian &amp; N.Jiang, 2021" authorityName="C. M. Tian &amp; N. Jiang" authorityYear="2021" class="Sordariomycetes" family="Pseudomelanconidaceae" genus="Micromelanconis" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Micromelanconis kaihuiae" order="Diaporthales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="kaihuiae">
