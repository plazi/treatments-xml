<taxonomicName id="E28774FD2041507E5EEAC8BCD369E85A" class="Liliopsida" family="Marantaceae" genus="Calathea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Calathea inocephala" order="Zingiberales" pageId="101" pageNumber="102" phylum="Tracheophyta" rank="species" species="inocephala">Calathea inocephala</taxonomicName>
(Kuntze) H. Kennedy, 
<taxonomicName id="5B3AA0799601D7F0147D6004F8544ABD" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia lutea" order="Coleoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="lutea">Cephaloleia lutea</taxonomicName>
Schult. (
<taxonomicName id="B15350269E567AD791C4822B1E54A712" genus="Marantaceae" lsidName="Marantaceae" pageId="101" pageNumber="102" rank="genus">Marantaceae</taxonomicName>
), and 
<taxonomicName id="2036C850E3B3B9DC3C3DD208B13E8343" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia stricta" order="Zingiberales" pageId="101" pageNumber="102" phylum="Tracheophyta" rank="species" species="stricta">Heliconia stricta</taxonomicName>
Huber (
<taxonomicName id="E763BC1FA19B7DF2132D8E1CEFD99402" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="101" pageNumber="102" rank="genus">Heliconiaceae</taxonomicName>
).
</paragraph>
</subSubSection>
<subSubSection id="4EDB2F8165423DFA9AC458BD04D5A35A" pageId="101" pageNumber="102" type="distribution">
<paragraph id="D63DE4E7701B19801472B2A68C367AD4" pageId="101" pageNumber="102">Distribution.</paragraph>
