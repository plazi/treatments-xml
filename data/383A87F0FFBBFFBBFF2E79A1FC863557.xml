<collectingCountry id="C8847676FFBBFFBBFF0D7E2CFF4134F6" box="[183,249,1146,1167]" name="United States of America" pageId="1" pageNumber="106">U.S.A.</collectingCountry>
in 
<collectingRegion id="7257F804FFBBFFBBFEA27E2CFEEC34F6" box="[280,340,1146,1167]" country="United States of America" name="Maine" pageId="1" pageNumber="106">Maine</collectingRegion>
(Dearborn and Donahue 1993), 
<collectingRegion id="7257F804FFBBFFBBFD367E2CFC9034F6" box="[652,808,1146,1167]" country="United States of America" name="New Hampshire" pageId="1" pageNumber="106">New Hampshire</collectingRegion>
(
<bibRefCitation id="D4024B17FFBBFFBBFC8E7E2CFC7E34F6" author="Chandler" box="[820,966,1146,1167]" pageId="1" pageNumber="106" refId="ref3493" refString="Chandler, D. S. 2001. University of New Hampshire Insect and Arachnid Collections. http: // colsa 1. unh. edu: 591 / unhinsects. htm (accessed 10 January 2006)." type="url" year="2001">Chandler 2001</bibRefCitation>
), and from 
<collectingRegion id="7257F804FFBBFFBBFF4A7EC2FEC134D0" box="[240,377,1172,1193]" country="United States of America" name="Massachusetts" pageId="1" pageNumber="106">Massachusetts</collectingRegion>
