<taxonomicName id="4C104D3BFFB8FFDB09B2FF7CFBEAFF34" authorityName=": Townes &amp; Townes" authorityYear="1966" box="[964,1122,188,212]" class="Insecta" family="Ichneumonidae" genus="Toechorychus" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="brevicaudis">
