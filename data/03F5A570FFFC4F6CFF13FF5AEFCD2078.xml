<collectingCountry id="F34B54F6FFFA4F6CFD4FFB55E80F21C4" box="[715,804,1235,1259]" name="Japan" pageId="5" pageNumber="197">JAPAN</collectingCountry>
: Hokkaido, Kitasanjo-nishi, Chuo-ku, Sapporo-shi, Hokkaido, 
<date id="FFE232A6FFFA4F6CFE97FB70EAB62020" box="[275,413,1270,1295]" pageId="5" pageNumber="197" value="2005-08-13">
<number id="3D940641FFFA4F6CFE97FB70EA052020" box="[275,302,1270,1295]" pageId="5" pageNumber="197" value="13.0">13</number>
.viii. 
<number id="3D940641FFFA4F6CFEDBFB70EAB62020" box="[351,413,1270,1295]" pageId="5" pageNumber="197" value="2005.0">2005</number>
</date>
