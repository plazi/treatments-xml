<geoCoordinate id="6EE1500AFF85C06BFC5FFC49FB95FC14" box="[963,1116,950,976]" degrees="13" direction="east" minutes="35.968" orientation="longitude" pageId="17" pageNumber="329" precision="1" value="13.599466">13°35.968’E</geoCoordinate>
