<taxonomicName id="4C3B4D76B0089855F266521AFCA8FC16" authority="(Koch, 1839)" baseAuthorityName="Koch" baseAuthorityYear="1839" box="[624,887,871,897]" class="Arachnida" family="Ameroseiidae" genus="Kleemannia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="474" phylum="Arthropoda" rank="species" species="pavida">
<emphasis id="B94FEAE7B0089855F266521AFD5CFC17" box="[624,643,871,896]" italics="true" pageId="1" pageNumber="474">K</emphasis>
