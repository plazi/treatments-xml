<taxonomicName id="4C3B4D76B0089855F104523EFD0BFCCA" authority="Bregetova, 1977" authorityName="Bregetova" authorityYear="1977" box="[274,724,835,861]" class="Arachnida" family="Ameroseiidae" genus="Ameroseiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="474" phylum="Arthropoda" rank="species" species="stepposa">
<emphasis id="B94FEAE7B0089855F104523EFDD0FCCB" box="[274,527,835,861]" italics="true" pageId="1" pageNumber="474">Ameroseiella stepposa</emphasis>
<bibRefCitation id="EFAA4B04B0089855F201523EFD0BFCCA" author="Bregetova, N. G." box="[535,724,835,861]" pageId="1" pageNumber="474" pagination="483 - 554" refId="ref9534" refString="Bregetova, N. G. (1977) Family Laelaptidae Berlese, 1892. In: Ghilyarov, M. S. &amp; Bregetova, N. G. (Eds.), Key to the Soil Inhabiting Mites. Mesostigmata. Nauka, Leningrad, pp. 483 - 554. [in Russian]" type="book chapter" year="1977">Bregetova, 1977</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C3B4D76B0089855F2F6523EFBA1FCC9" authority="(Berlese, 1916 a)" baseAuthorityName="Berlese" baseAuthorityYear="1916" box="[736,1150,835,862]" class="Arachnida" family="Ameroseiidae" genus="Kleemannia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="474" phylum="Arthropoda" rank="species" species="tenella">
<emphasis id="B94FEAE7B0089855F2F6523EFC63FCCA" box="[736,956,835,861]" italics="true" pageId="1" pageNumber="474">Kleemannia tenella</emphasis>
(
<bibRefCitation id="EFAA4B04B0089855F3DA523EFBA8FCC9" author="Berlese, A." box="[972,1143,835,862]" pageId="1" pageNumber="474" pagination="19 - 67" refId="ref9447" refString="Berlese, A. (1916 a) Centuria seconda di Acari nuovi. Redia, 12, 19 - 67." type="journal article" year="1916">Berlese, 1916a</bibRefCitation>
)
</taxonomicName>
[= 
<taxonomicName id="4C3B4D76B0089855F489523EFEC9FC16" authority="Karg, 1971" authorityName="Karg" authorityYear="1971" class="Arachnida" family="Ameroseiidae" genus="Ameroseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="474" phylum="Arthropoda" rank="species" species="fimetorum">
