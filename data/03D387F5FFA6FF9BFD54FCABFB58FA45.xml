<emphasis id="B90EEAF1FFA6FF9BFD6BFB7AFCEBFB0B" bold="true" box="[731,829,1222,1243]" pageId="9" pageNumber="199">Remarks.</emphasis>
<taxonomicName id="4C7A4D60FFA6FF9BFCF8FB7AFB86FB0B" baseAuthorityName="Spaeth" baseAuthorityYear="1913" box="[840,1104,1222,1243]" class="Insecta" family="Chrysomelidae" genus="Notosacantha" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="199" phylum="Arthropoda" rank="species" species="poeciloptera">
