<taxonomicName id="4C6DE5A20C7B7305FE08C9B6BE58D9DD" box="[505,611,152,176]" class="Actinopterygii" family="Syngnathidae" genus="Hippocampus" kingdom="Animalia" order="Syngnathiformes" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="denise">
