<figureCitation id="8626454910232DCC20315BD5C382846D" captionStart="Figure 1" captionStartId="F1" captionText="Figure 1. Corybas papillatus Inuthai, Chantanaorr. &amp; Suddee A, B plants in natural habitat, on humus associated with Bazzania sp. and Acroporium sp. C plant with immature fruit D, E flowers D front view E lateral view F immature flower G underground parts with tubers. Photographs by W. Kiewbang." figureDoi="10.3897/phytokeys.183.71167.figure1" httpUri="https://binary.pensoft.net/fig/595910" pageId="0" pageNumber="1">1A-C</figureCitation>
), such as 
<taxonomicName id="AAC8DF5D12E241552FFA6B66ADAA4B9F" authorityName="Mitten" authorityYear="1868" class="Bryopsida" family="Sematophyllaceae" genus="Acroporium" kingdom="Plantae" lsidName="Acroporium" order="Hypnales" pageId="0" pageNumber="1" phylum="Bryophyta" rank="genus">
<emphasis id="F54DD2C481BD3263DD2AE62649949FBF" italics="true" pageId="0" pageNumber="1">Acroporium</emphasis>
</taxonomicName>
sp., 
<taxonomicName id="0822B340A9B9EE9772F74B40C48BC1CA" authorityName="S.F.Gray" authorityYear="1821" class="Jungermanniopsida" family="Lepidoziaceae" genus="Bazzania" kingdom="Plantae" lsidName="Bazzania" order="Jungermanniales" pageId="0" pageNumber="1" phylum="Marchantiophyta" rank="genus">
<emphasis id="AE60649B2D1DA0D140D0051C7F327C63" italics="true" pageId="0" pageNumber="1">Bazzania</emphasis>
</taxonomicName>
sp., and 
<taxonomicName id="5F1C45C0AD46404F7A69FA2BDE50AE9B" class="Jungermanniopsida" family="Schistochilaceae" genus="Schistochila" kingdom="Plantae" lsidName="Schistochila aligera" order="Jungermanniales" pageId="0" pageNumber="1" phylum="Marchantiophyta" rank="species" species="aligera">
<emphasis id="227007E58FE3F9C9DBD03977E32E53EC" italics="true" pageId="0" pageNumber="1">Schistochila aligera</emphasis>
</taxonomicName>
