<paragraph blockId="7.[151,1436,226,437]" pageId="7" pageNumber="65">
<materialsCitation location="Carmo do Rio Claro" pageId="7" pageNumber="65" specimenCount="1" stateProvince="Minas Gerais, Espirito Santo">
