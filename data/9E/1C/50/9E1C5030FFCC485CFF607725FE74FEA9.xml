<subSubSection box="[199,1419,1892,1917]" pageId="10" pageNumber="491" type="etymology">
<paragraph blockId="10.[151,1436,1892,2025]" box="[199,1419,1892,1917]" pageId="10" pageNumber="491">
<emphasis bold="true" box="[199,337,1892,1917]" pageId="10" pageNumber="491">Etymology:</emphasis>
