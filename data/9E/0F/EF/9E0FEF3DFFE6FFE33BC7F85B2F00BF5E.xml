<subSubSection pageId="0" pageNumber="445" type="nomenclature">
<paragraph blockId="0.[151,447,1938,1964]" box="[151,447,1938,1964]" pageId="0" pageNumber="445">
<heading bold="true" box="[151,447,1938,1964]" fontSize="11" level="1" pageId="0" pageNumber="445" reason="1">
<emphasis bold="true" box="[151,447,1938,1964]" pageId="0" pageNumber="445">
<taxonomicName box="[151,331,1938,1964]" class="Insecta" family="Ceratopogonidae" genus="Afrostilobezzia" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="445" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,331,1938,1964]" italics="true" pageId="0" pageNumber="445">Afrostilobezzia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[337,447,1938,1964]" pageId="0" pageNumber="445" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="0.[151,881,2008,2030]" box="[151,881,2008,2030]" pageId="0" pageNumber="445">
Type-species: 
<taxonomicName box="[296,546,2008,2029]" class="Insecta" family="Ceratopogonidae" genus="Afrostilobezzia" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="445" phylum="Arthropoda" rank="species" species="clastrieri" status="sp. nov.">
<emphasis box="[296,546,2008,2029]" italics="true" pageId="0" pageNumber="445">Afrostilobezzia clastrieri</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[552,633,2008,2030]" pageId="0" pageNumber="445" rank="species">
<emphasis bold="true" box="[552,633,2008,2030]" pageId="0" pageNumber="445">sp. nov.</emphasis>
</taxonomicNameLabel>
