<quantity box="[523,612,1823,1850]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" pageId="3" pageNumber="344" unit="mm" value="9.5">9.5 mm</quantity>
; head— 
<quantity box="[708,793,1823,1850]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="3" pageNumber="344" unit="mm" value="1.3">1.3 mm</quantity>
, thorax— 
<quantity box="[906,994,1823,1850]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="3" pageNumber="344" unit="mm" value="3.4">3.4 mm</quantity>
; abdomen— 
<quantity box="[1138,1226,1823,1850]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="3" pageNumber="344" unit="mm" value="4.8">4.8 mm</quantity>
; ovipositor length: full— 
<quantity box="[220,306,1859,1886]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="3" pageNumber="344" unit="mm" value="3.8">3.8 mm</quantity>
, protruding beyond abdominal apex part— 
<quantity box="[792,878,1860,1886]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="3" pageNumber="344" unit="mm" value="1.8">1.8 mm</quantity>
; fore wing length— 
<quantity box="[1103,1190,1859,1886]" metricMagnitude="-3" metricUnit="m" metricValue="7.9" pageId="3" pageNumber="344" unit="mm" value="7.9">7.9 mm</quantity>
, width— 
<quantity box="[1295,1383,1859,1886]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="3" pageNumber="344" unit="mm" value="2.9">2.9 mm</quantity>
