</taxonomicName>
, 
<taxonomicName authority="(Van Beneden, 1844 b)" authorityName="Van Beneden" authorityYear="1844" baseAuthorityName="Van Beneden" baseAuthorityYear="1844" box="[330,703,1541,1568]" class="Hydrozoa" family="Bougainvilliidae" genus="Bougainvillia" kingdom="Animalia" order="Anthoathecata" pageId="16" pageNumber="17" phylum="Cnidaria" rank="species" species="ramosa">
