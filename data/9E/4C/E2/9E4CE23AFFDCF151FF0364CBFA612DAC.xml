<taxonomicName authority="(Forbes, 1841)" authorityName="Forbes" authorityYear="1841" baseAuthorityName="Forbes" baseAuthorityYear="1841" class="Hydrozoa" family="Bougainvilliidae" genus="Bougainvillia" kingdom="Animalia" order="Anthoathecata" pageId="16" pageNumber="17" phylum="Cnidaria" rank="species" species="britannica">
<emphasis box="[1289,1437,1505,1531]" italics="true" pageId="16" pageNumber="17">B. britannica</emphasis>
