, as 
<taxonomicName authorityName="Wurtz &amp; Roback" authorityYear="1955" box="[403,664,1259,1285]" class="Hydrozoa" family="Cordylophoridae" genus="Cordylophora" kingdom="Animalia" order="Anthoathecata" pageId="15" pageNumber="16" phylum="Cnidaria" rank="species" species="lacustris">
