<collectingCountry box="[1035,1136,581,607]" name="Haiti" pageId="10" pageNumber="11">Tortugas</collectingCountry>
, 
<geoCoordinate box="[1147,1282,579,606]" degrees="25" direction="north" minutes="16.83" orientation="latitude" pageId="10" pageNumber="11" precision="9" value="25.2805">25°16.83’N</geoCoordinate>
