<taxonomicName authority="Bukejs, 2017" authorityName="Bukejs" authorityYear="2017" box="[337,610,1901,1923]" class="Insecta" family="Anthicidae" genus="Ischalia" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="235" phylum="Arthropoda" rank="species" species="danieli" status="sp. nov." subGenus="Telnovia">
