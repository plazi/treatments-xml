<taxonomicName box="[382,442,463,488]" class="Mollusca" family="Mollusca" genus="Atlas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca" pageId="2" pageNumber="3" phylum="Mollusca" rank="genus">
