<paragraph blockId="15.[151,552,938,964]" box="[151,552,938,964]" pageId="15" pageNumber="100">
<heading bold="true" box="[151,552,938,964]" fontSize="11" level="1" pageId="15" pageNumber="100" reason="1">
<taxonomicName authority="Kieffer, 1898" authorityName="Kieffer" authorityYear="1898" box="[151,552,938,964]" class="Insecta" family="Cecidomyiidae" genus="Dicerura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="100" phylum="Arthropoda" rank="species" species="scirpicola">
<emphasis bold="true" box="[151,552,938,964]" pageId="15" pageNumber="100">
<emphasis bold="true" box="[151,384,938,964]" italics="true" pageId="15" pageNumber="100">Dicerura scirpicola</emphasis>
<bibRefCitation box="[392,552,938,964]" pageId="15" pageNumber="100" refId="ref11034" refString="Kieffer, J. - J. (1898) Synopse des cecidomyies d'Europe et d'Algerie decrites jusqu'a ce jour. Bulletin de la Societe d'HiStoire Naturelle de Metz, Series 2, 8, 1 - 64." type="journal article">Kieffer, 1898</bibRefCitation>
</emphasis>
