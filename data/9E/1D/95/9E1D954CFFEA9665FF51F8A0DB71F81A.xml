<taxonomicName authorityName="Spungis" authorityYear="1979" box="[1211,1331,1905,1929]" class="Insecta" family="Cecidomyiidae" genus="Dicerura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="100" phylum="Arthropoda" rank="species" species="dentata">
<emphasis box="[1211,1331,1905,1929]" italics="true" pageId="15" pageNumber="100">D. dentata</emphasis>
</taxonomicName>
