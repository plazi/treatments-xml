<taxonomicName authority="Spungis, 1987" authorityName="Spungis" authorityYear="1987" box="[151,577,1409,1435]" class="Insecta" family="Cecidomyiidae" genus="Dicerura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="98" phylum="Arthropoda" rank="species" species="unidentata">
