<taxonomicName baseAuthorityName="Ohaus" baseAuthorityYear="1930" box="[265,386,227,251]" class="Insecta" family="Rutelidae" genus="Chrysina" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="pehlkei">
<emphasis box="[265,386,227,251]" italics="true" pageId="8" pageNumber="7">C. pehlkei</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Moron" baseAuthorityYear="1990" box="[444,583,227,251]" class="Insecta" family="Rutelidae" genus="Chrysina" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="centralis">
<emphasis box="[444,583,227,251]" italics="true" pageId="8" pageNumber="7">C. centralis</emphasis>
