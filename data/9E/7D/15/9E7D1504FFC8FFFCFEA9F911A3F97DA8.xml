<paragraph blockId="8.[192,1440,195,251]" pageId="8" pageNumber="7">
<taxonomicName authorityName="Monzon" authorityYear="2010" box="[192,401,195,219]" class="Insecta" family="Rutelidae" genus="Chrysina" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="hawksi">
