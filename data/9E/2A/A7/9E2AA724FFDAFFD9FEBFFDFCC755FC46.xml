., 
<emphasis box="[951,1107,953,978]" italics="true" pageId="12" pageNumber="13">pogonogynus</emphasis>
<emphasis bold="true" box="[1114,1242,953,979]" pageId="12" pageNumber="13">comb. rev.</emphasis>
, 
<emphasis italics="true" pageId="12" pageNumber="13">pubescens</emphasis>
<emphasis bold="true" box="[335,464,993,1019]" pageId="12" pageNumber="13">comb. rev.</emphasis>
, and 
<emphasis box="[528,646,993,1018]" italics="true" pageId="12" pageNumber="13">subglaber</emphasis>
