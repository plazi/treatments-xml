<subSubSection box="[151,918,610,632]" pageId="1" pageNumber="332" type="materials_examined">
<paragraph blockId="1.[151,918,610,632]" box="[151,918,610,632]" pageId="1" pageNumber="332">
<emphasis bold="true" box="[151,290,610,632]" pageId="1" pageNumber="332">
