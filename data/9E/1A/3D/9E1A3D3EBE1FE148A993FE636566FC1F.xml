<taxonomicName box="[160,249,454,480]" class="Insecta" family="Noctuidae" genus="Generic" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="44" pageNumber="777" phylum="Arthropoda" rank="genus">Generic</taxonomicName>
diagnosis
</emphasis>
