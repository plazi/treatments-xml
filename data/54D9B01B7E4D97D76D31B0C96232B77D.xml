25. 
<taxonomicName id="900582120635EF9925BA37458CF24A51" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34920" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName="Polyrhachis laevigata Smith" lsidName-HNS="Polyrhachis laevigata Smith" order="Hymenoptera" pageNumber="400" phylum="Arthropoda" rank="species" species="laevigata">Polyrhachis laevigata (Smith)</taxonomicName>
