<subSubSection id="9CAE656EFF814974ED8A726EFB94FE0D" box="[199,1054,366,391]" pageId="7" pageNumber="70" type="etymology">
<paragraph id="D40B36E5FF814974ED8A726EFB94FE0D" blockId="7.[151,1437,258,571]" box="[199,1054,366,391]" pageId="7" pageNumber="70">
<emphasis id="E6C0EAF7FF814974ED8A726EFEC4FE0D" bold="true" box="[199,334,366,391]" pageId="7" pageNumber="70">Etymology.</emphasis>
