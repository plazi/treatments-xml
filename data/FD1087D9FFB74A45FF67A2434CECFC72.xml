<taxonomicName id="B2B94D4CFFB74A46FE16A0FC4D33F812" authority="Bleeker 1853" authorityName="Bleeker" authorityYear="1853" box="[486,828,1949,1975]" class="Actinopterygii" family="Gobiidae" genus="Gnatholepis" kingdom="Animalia" order="Perciformes" pageId="17" pageNumber="18" phylum="Chordata" rank="species" species="cauerensis">
