<collectionCode id="13A8AE0AFFB74A46FDA7A2DC4CA8FA72" LSID="urn:lsid:biocol.org:col:34840" box="[599,679,1469,1495]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34840" name="Staatliches Museum fuer Naturkund Stuttgart" pageId="17" pageNumber="18">SMNS</collectionCode>
<number id="C37124E8FFB74A46FD5EA2DC4CF8FA72" box="[686,759,1469,1495]" pageId="17" pageNumber="18" value="16034.0">16034</number>
(
<number id="C37124E8FFB74A46FCF8A2DC4D16FA72" box="[776,793,1469,1495]" pageId="17" pageNumber="18" value="2.0">2</number>
, Funchal).
</paragraph>
