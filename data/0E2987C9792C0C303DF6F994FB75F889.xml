; clypeal index 
<number id="304824F8792C0C313FA4F82AFCE7F85A" box="[709,832,1944,1970]" pageId="28" pageNumber="29" value="1.5" valueMax="1.75" valueMin="1.25">1.25–1.75</number>
; MOQ index 
<number id="304824F8792C0C313E84F82AFBC9F85A" box="[997,1134,1944,1970]" pageId="28" pageNumber="29" value="10.355" valueMax="11.17" valueMin="9.54">9.54–11.17</number>
