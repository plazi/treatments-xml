<taxonomicName id="41804D5C792D0C303C05F9F2FDBFF9BF" box="[356,536,1600,1623]" class="Arachnida" family="Gnaphosidae" genus="Zelotes" kingdom="Animalia" order="Araneae" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="zekharya" status="n. sp.">
<emphasis id="B4F4EACD792D0C303C05F9F2FDBFF9BF" box="[356,536,1600,1623]" italics="true" pageId="29" pageNumber="30">
Zelotes 
<taxonomicName id="41804D5C792D0C303CD9F9F2FDBFF9BF" box="[440,536,1600,1623]" pageId="29" pageNumber="30" rank="species" species="zekharya" status="n. sp.">zekharya</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="B4F4EACD792D0C303F7FF9F2FDFDF9B0" bold="true" box="[542,602,1600,1624]" pageId="29" pageNumber="30">
<taxonomicNameLabel id="AFC757B6792D0C303F7FF9F2FDFDF9B0" box="[542,602,1600,1624]" pageId="29" pageNumber="30" rank="species">n. sp.</taxonomicNameLabel>
