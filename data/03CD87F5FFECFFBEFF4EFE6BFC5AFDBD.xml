<paragraph id="8BDB36E3FFECFFBFFF4EFDDAFC2CFD22" blockId="8.[151,993,546,682]" box="[151,993,618,646]" pageId="8" pageNumber="9">
<emphasis id="B910EAF1FFECFFBFFF4EFDDAFEFFFD21" bold="true" box="[151,306,618,645]" pageId="8" pageNumber="9">
<emphasis id="B910EAF1FFECFFBFFF4EFDDAFEE4FD21" bold="true" box="[151,297,618,645]" italics="true" pageId="8" pageNumber="9">Type locality</emphasis>
