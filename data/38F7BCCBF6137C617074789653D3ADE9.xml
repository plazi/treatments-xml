<taxonomicName id="89C1886468F4CC55C341B4D04C8C0902" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:37027" class="Insecta" family="Formicidae" genus="Tetramorium" genus.bestMatchDistance="48" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" kingdom="Animalia" lsidName-HNS="Tetramorium guineense (Bernard)" order="Hymenoptera" pageNumber="85" phylum="Arthropoda" rank="species" species="guineense">T. guineense</taxonomicName>
