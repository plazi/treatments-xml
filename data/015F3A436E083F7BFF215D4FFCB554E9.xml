<typeStatus id="564D35F76E083F7BFEC15DFDFE0C545C" box="[375,431,1777,1801]" pageId="6" pageNumber="207">Type</typeStatus>
locality: “Turkestan”, “Saty-Ashu” [=? 
<collectingCountry id="F1E1CBC56E083F7BFCCE5DFDFC58545C" box="[888,1019,1777,1801]" name="Kyrgyzstan" pageId="6" pageNumber="207">Kyrgyzstan</collectingCountry>
