<collectingRegion id="49871F8C453DFC10FDED769FFD17FCD2" box="[528,689,758,782]" country="United States of America" name="Pennsylvania" pageId="4" pageNumber="173">Pennsylvania</collectingRegion>
. 
<location id="8E9C87B5453DFC10FD42769FFD71FCD1" LSID="urn:lsid:plazi:treatment:03EA6078453DFC10FE837507FCBBFC63:8E9C87B5453DFC10FD42769FFD71FCD1" box="[703,727,758,781]" county="Fifteenmile Creek" municipality="Old Big Mountain" name="In" pageId="4" pageNumber="173" stateProvince="Bedford">In</location>
<collectingRegion id="49871F8C453DFC10FD1F769FFEB8FCED" country="United States of America" name="West Virginia" pageId="4" pageNumber="173">West Virginia</collectingRegion>
<taxonomicName id="4C43AAED453DFC10FED57773FE0FFCED" authorityName="Surdick" authorityYear="2004" box="[296,425,793,817]" class="Insecta" family="Chloroperlidae" genus="Sweltsa" kingdom="Animalia" order="Plecoptera" pageId="4" pageNumber="173" phylum="Arthropoda" rank="species" species="palearata">
