<location id="8E9C87B5453DFC10FDA777EDFD07FC40" LSID="urn:lsid:plazi:treatment:03EA6078453DFC10FE837507FCBBFC63:8E9C87B5453DFC10FDA777EDFD07FC40" box="[602,673,900,924]" county="Fifteenmile Creek" municipality="Old Big Mountain" name="North" pageId="4" pageNumber="173" stateProvince="Bedford">North</location>
