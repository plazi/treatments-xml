<taxonomicName id="4C43AAED453DFC10FD5C76C6FCB8FD1B" authorityName="Surdick" authorityYear="2004" box="[673,798,687,711]" class="Insecta" family="Chloroperlidae" genus="Sweltsa" kingdom="Animalia" order="Plecoptera" pageId="4" pageNumber="173" phylum="Arthropoda" rank="species" species="palearata">
