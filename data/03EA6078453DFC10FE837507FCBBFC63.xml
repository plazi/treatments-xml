<taxonomicName id="4C43AAED453DFC10FDE476E2FD6FFD7F" box="[537,713,651,675]" class="Insecta" family="Chloroperlidae" genus="Sweltsa" kingdom="Animalia" order="Plecoptera" pageId="4" pageNumber="173" phylum="Arthropoda" rank="species" species="undetermined">
