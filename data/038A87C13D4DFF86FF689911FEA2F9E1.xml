<taxonomicName id="4C234D543D4DFF86FE229B7EFC94F9CA" box="[477,785,1592,1617]" class="Eutardigrada" family="Macrobiotidae" genus="Macrobiotus" kingdom="Animalia" order="Parachela" pageId="19" pageNumber="20" phylum="Tardigrada" rank="species" species="harmsworthi">
