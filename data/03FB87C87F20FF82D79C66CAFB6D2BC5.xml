<taxonomicName id="4C524D5D7F20FF83D40D6774FB892E63" authority="Mateos, 2011" authorityName="Mateos" authorityYear="2011" box="[774,1062,1320,1345]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="10" pageNumber="398" phylum="Arthropoda" rank="species" species="barbulus">
