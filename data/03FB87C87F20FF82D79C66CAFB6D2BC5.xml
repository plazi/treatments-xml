<taxonomicName id="4C524D5D7F20FF83D7C76680FE612FD4" box="[204,462,1245,1270]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="10" pageNumber="398" phylum="Arthropoda" rank="species" species="lignorum">
