<taxonomicName id="4C4147821F66FFDFFF18F8672C58F875" authorityName="Constant" authorityYear="2018" baseAuthorityName="Walker" baseAuthorityYear="1870" box="[189,312,1988,2014]" class="Insecta" family="Fulgoridae" genus="Birdantis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="collaris">
