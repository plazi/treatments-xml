<taxonomicName id="4C4147821F66FFDFFD29F8672E83F874" authority="(Schmidt, 1926)" authorityName="Constant" authorityYear="2018" baseAuthorityName="Schmidt" baseAuthorityYear="1926" box="[652,995,1988,2015]" class="Insecta" family="Fulgoridae" genus="Birdantis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="trilineata">
