<paragraph id="8BC836C38522FF915991FAC35435FA87" blockId="16.[151,797,1303,1363]" box="[151,378,1338,1363]" pageId="16" pageNumber="467">
(
<figureCitation id="134C2A468522FF915999FAC355A8FA87" box="[159,231,1338,1363]" captionStart="FIGURES 4 – 11" captionStartId="38.[151,264,1745,1768]" captionTargetBox="[181,1434,225,1688]" captionTargetId="figure@38.[151,1435,195,1688]" captionTargetPageId="38" captionText="FIGURES 4 – 11. Aedeagus in Deltochilum (Aganhyboma). 4. D. titovidaurrei new species. 5. D. cupreicolle. 6. D. viridescens new status. 7. D. viridicatum new species. 8. D. amandaarcanjoae new species. 9. D. trisignatum. 10. D. cangalha new species. and D. alpercata new species. 11. D. finestriatum new species. Figures scale = 1 mm." httpUri="https://zenodo.org/record/244797/files/figure.png" pageId="16" pageNumber="467">
Figs 
<number id="3DBF24E48522FF9159D1FAC255A8FA87" box="[215,231,1339,1363]" pageId="16" pageNumber="467" value="4.42">4</number>
</figureCitation>
, 42, 
<number id="3DBF24E48522FF91581BFAC2543DFA87" box="[285,370,1339,1363]" pageId="16" pageNumber="467" value="90.1">90, 100</number>
)
