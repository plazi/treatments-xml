[[ 
<taxonomicName id="5F076ACF5B48530532D8EB70C6E98445" class="Actinopterygii" family="Trichomycteridae" genus="Trichomycterus" higherTaxonomySource="CoL" kingdom="Animalia" order="Siluriformes" pageNumber="50" phylum="Chordata" rank="genus">Trichomycterus</taxonomicName>
]]
</paragraph>
