<subSubSection id="113CF7146F5E0C605B2A224C5B1A264C" pageId="12" pageNumber="359" type="reference_group">
<paragraph id="6CF3EEF407C20A11EAA107106368A0AD" pageId="12" pageNumber="359">
<taxonomicName id="328248AE536522C6C6648ECC8076AEF8" authority="Le Peletier &amp; Audinet-Serville, 1828" authorityName="Le Peletier &amp; Audinet-Serville" authorityYear="1828" class="Insecta" family="Tenthredinidae" genus="Empria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Empria quadrimaculata" order="Hymenoptera" pageId="12" pageNumber="359" phylum="Arthropoda" rank="species" species="quadrimaculata">Empria quadrimaculata</taxonomicName>
