<taxonomicName id="A7F323F16A7515E334ACFAC49A542656" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax" order="Coleoptera" pageId="134" pageNumber="135" phylum="Arthropoda" rank="genus">Mecyclothorax</taxonomicName>
