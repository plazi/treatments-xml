<taxonomicName id="3E00217A11D2F15BEB840159B3146F00" lsidName="subternus" pageId="134" pageNumber="135" rank="species" species="subternus">subternus</taxonomicName>
/ ♂ #40 / det. J.K. Liebherr 2014 // HOLOTYPE / 
<taxonomicName id="1CB445CA84DE67857D4E3829A3A213F9" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax" order="Coleoptera" pageId="134" pageNumber="135" phylum="Arthropoda" rank="genus">Mecyclothorax</taxonomicName>
/ 
<taxonomicName id="D19683D30DCC68F707C16D428706DF14" lsidName="subternus" pageId="134" pageNumber="135" rank="species" species="subternus">subternus</taxonomicName>
