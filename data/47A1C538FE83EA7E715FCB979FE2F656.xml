<taxonomicName id="68DC25BC22DF71CD47C2F47BB0A9CB74" class="Polypodiopsida" family="Cibotiaceae" genus="Cibotium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cibotium" order="Cyatheales" pageId="134" pageNumber="135" phylum="Tracheophyta" rank="genus">Cibotium</taxonomicName>
