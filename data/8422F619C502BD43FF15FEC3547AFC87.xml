<typeStatus id="D330F9ADC502BD43FF45FD58506AFCBB" box="[199,258,756,782]" pageId="18" pageNumber="249">Type</typeStatus>
locality:
</emphasis>
<collectingCountry id="749C079FC502BD43FEF2FD5950BEFCBA" box="[368,470,757,783]" name="Uruguay" pageId="18" pageNumber="249">Uruguay</collectingCountry>
: 
<collectingRegion id="CE4F89EDC502BD43FE63FD58539AFCBB" box="[481,754,756,782]" country="Uruguay" name="Montevideo" pageId="18" pageNumber="249">Montevideo department</collectingRegion>
