, Apr. 869 [
<date id="FFFF8ABAFFD0FF95FD76FD73F158FCCD" box="[681,818,757,783]" pageId="2" pageNumber="58" value="1869-04">
<collectingDate id="EFBB7352FFD0FF95FD76FD73F158FCCD" box="[681,818,757,783]" pageId="2" pageNumber="58" value="1869-04">April 1869</collectingDate>
</date>
], 
<emphasis id="B9357068FFD0FF95FC9AFD72F1ACFCCC" box="[837,966,756,782]" italics="true" pageId="2" pageNumber="58">PančiĆ s.n.</emphasis>
(
