<quantity id="4CEF9B25FF8BFFAF990CFCEDFBEA333C" box="[369,427,799,825]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="27" unit="m" value="10.0">10 m</quantity>
