<subSubSection id="34D1E5A7FFC7FFDBC9E753F42F844AD6" pageId="8" pageNumber="43" type="etymology">
<paragraph id="7C74B62CFFC7FFDBC9E753F42F844AD6" blockId="8.[151,1127,1313,1560]" pageId="8" pageNumber="43">
<emphasis id="4EBF6A3EFFC7FFDBC9E753F42F0A4AF6" bold="true" box="[151,280,1473,1496]" pageId="8" pageNumber="43">Etymology.</emphasis>
