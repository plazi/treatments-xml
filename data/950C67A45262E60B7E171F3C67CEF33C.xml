<taxonomicName id="A76D7DC0F83912BB7554ED0A15A2F220" class="Demospongiae" family="Thorectidae" genus="Cacospongia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cacospongia mollior" order="Dictyoceratida" pageId="37" pageNumber="38" phylum="Porifera" rank="species" species="mollior">Cacospongia mollior</taxonomicName>
