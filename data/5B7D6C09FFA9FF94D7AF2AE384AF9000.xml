<number id="651CCF38FFA9FF95D6C22D4182409771" box="[506,537,1302,1326]" pageId="20" pageNumber="463" value="22.0">22</number>
˚ 
<number id="651CCF38FFA9FF95D5182D4182679771" box="[544,574,1302,1326]" pageId="20" pageNumber="463" value="26.0">26</number>
’ 
<number id="651CCF38FFA9FF95D57A2D4182DC9771" box="[578,645,1302,1326]" pageId="20" pageNumber="463" value="15.63">15.63</number>
” S 
<number id="651CCF38FFA9FF95D5822D4182BF9771" box="[698,742,1302,1326]" pageId="20" pageNumber="463" value="116.0">116</number>
