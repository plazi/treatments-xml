<paragraph id="8BDC9760FB637A2DFF30FDA7FB45FDAA" blockId="24.[151,1436,151,769]" box="[197,1184,570,595]" pageId="24" pageNumber="25">
Two 
<typeStatus id="54D829C2FB637A2DFF08FDA6FE71FDAA" box="[253,404,570,594]" pageId="24" pageNumber="25">paralectotype</typeStatus>
