<taxonomicName id="827D87ED83BF2462CB2E188B3B890C1C" authority="Zinn" class="Liliopsida" family="Orchidaceae" genus="Epipactis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="611" phylum="Tracheophyta" rank="genus">
