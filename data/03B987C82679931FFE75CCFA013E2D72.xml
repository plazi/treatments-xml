<taxonomicName id="4C104D5D2679931FFE06CCFA074028A6" authorityName="ESSIGI" authorityYear="2006" box="[511,770,213,236]" class="Insecta" family="Aphididae" genus="Anomalosiphum" kingdom="Animalia" order="Aphidomorpha" pageId="6" pageNumber="245" phylum="Arthropoda" rank="genus">
