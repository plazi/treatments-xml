<taxonomicName id="CB2DFE6CAA7D194942A6015D278179F4" class="Arachnida" family="Gnaphosidae" genus="Gnaphosa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gnaphosa esyunini" order="Araneae" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="esyunini">Gnaphosa esyunini</taxonomicName>
