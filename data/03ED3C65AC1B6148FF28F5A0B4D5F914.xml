<emphasis id="B9305161AC1A6148FF28F725B73AFBB9" bold="true" box="[151,286,1116,1140]" pageId="3" pageNumber="36">FIGURE 1.</emphasis>
<taxonomicName id="4C44F6F0AC1A6148FE99F725B47FFBBE" authority="Meyer" authorityName="Meyer" box="[294,603,1116,1139]" class="Arachnida" family="Tetranychidae" genus="Peltanobia" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="erasmusi">
