; 
<bibRefCitation id="E8144B5AFFD9FFB6FD4FB9B0770BFB3A" author="Shear" box="[740,876,1243,1269]" pageId="1" pageNumber="58" refString="Shear, W. A. (1972) Studies in the milliped order Chordeumida (Diplopoda): A revision of the family Cleidogonidae and a reclassification of the order Chordeumida in the New World. Bulletin of the Museum of Comparative Zoology, 144 (4), 151 - 352." type="journal article" year="1972">
Shear 
<number id="3A4D248CFFD9FFB6FC85B9B0770BFB3A" box="[814,876,1243,1269]" pageId="1" pageNumber="54" value="1972.1981">1972</number>
</bibRefCitation>
, 
<bibRefCitation id="E8144B5AFFD9FFB6FCDCB9B077D3FB3A" author="Shear" box="[887,948,1243,1269]" pageId="1" pageNumber="58" refString="Shear, W. A. (1981) The milliped family Tingupidae (Diplopoda, Chordeumatida, Brannerioidea). American Museum Novitates, 2715, 1 - 20." type="journal article" year="1981">1981</bibRefCitation>
