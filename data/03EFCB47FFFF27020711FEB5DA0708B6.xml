<paragraph id="8BF97A51FFFF27050711FEB5DAFF0A37" blockId="3.[317,1489,295,2197]" box="[583,1234,296,332]" pageId="3" pageNumber="84">
<heading id="D0B1CD3DFFFF27050711FEB5DAFF0A37" box="[583,1234,296,332]" centered="true" fontSize="12" level="1" pageId="3" pageNumber="84" reason="4">
<emphasis id="B932A643FFFF27050711FEB5DD940A37" box="[583,953,296,332]" italics="true" pageId="3" pageNumber="84">
<taxonomicName id="4C4601D2FFFF27050711FEB5DD9E0A37" authorityName="Marsh" authorityYear="1890" box="[583,947,296,332]" class="Reptilia" family="Ornithomimidae" genus="Ornithomimus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="3" pageNumber="84" phylum="Chordata" rank="species" species="velox" status="gen. et sp. nov.">Ornithomimus velox</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A2011B38FFFF2705069FFEB5DAFF0A37" box="[969,1234,296,332]" pageId="3" pageNumber="84" rank="species">gen. et sp. nov.</taxonomicNameLabel>
</heading>
</paragraph>
