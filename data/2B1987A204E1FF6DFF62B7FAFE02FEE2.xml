<taxonomicName id="64B04D3704FFFF6DFE17B377FD85FEB1" box="[482,601,272,297]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="species" species="deleoni">
