<taxonomicName id="64B04D3704FFFF6DFE28B35FFDA3FEC9" baseAuthorityName="Germar" baseAuthorityYear="1821" box="[477,639,312,337]" class="Insecta" family="Cicadellidae" genus="Homalodisca" kingdom="Animalia" order="Hemiptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="species" species="vitripennis">
