<taxonomicName id="3DE04D63FF9F2404FF7EBD23FD6D3A5C" authority="(Edgbastonia) Ponder, 2008" authorityName="Ponder" authorityYear="2008" box="[151,650,858,884]" class="Gastropoda" family="Hydrobiidae" genus="Edgbastonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="25" pageNumber="26" phylum="Mollusca" rank="subGenus" subGenus="Edgbastonia">
