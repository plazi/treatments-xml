<taxonomicName id="4C7D4D11A916FF8BFF3511C2FE58F85D" authority="Wiles, 1992" authorityName="Wiles" authorityYear="1992" box="[151,476,1965,1990]" class="Arachnida" family="Aturidae" genus="Albiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="vanimoensis">
