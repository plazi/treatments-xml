<taxonomicName id="4C1B1C897E296D2EFE97B676FD53CD3F" authority="Jeannel, 1910" authorityName="Jeannel" authorityYear="1910" box="[322,683,1630,1656]" class="Insecta" family="Leiodidae" genus="Antrocharidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="110" phylum="Arthropoda" rank="genus">
<emphasis id="B96FBB187E296D2EFE97B676FE02CD3F" bold="true" box="[322,506,1630,1656]" italics="true" pageId="6" pageNumber="110">Antrocharidius</emphasis>
