<taxonomicName id="4C3D4D5BFF93E47B95083C1DA775855F" authority="Latreille" authorityName="Latreille" box="[535,770,583,610]" class="Insecta" family="Pompilidae" genus="Ceropales" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="218" phylum="Arthropoda" rank="subGenus" subGenus="Ceropales">
