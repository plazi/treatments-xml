<taxonomicName id="4C414D18FFADFFA25B22F960B0B2F967" authorityName="Reuter" authorityYear="1904" box="[1327,1441,1743,1764]" class="Insecta" family="Miridae" genus="Nanniella" kingdom="Animalia" order="Hemiptera" pageId="64" pageNumber="622" phylum="Arthropoda" rank="genus">
