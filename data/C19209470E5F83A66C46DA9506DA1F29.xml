<taxonomicName id="5AF6D819FE83B73C810A99358B0A9AD0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:224162" class="Insecta" family="Formicidae" genus="Acromyrmex" genus.completionEvidence="missing" genus.completionRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Acromyrmex homalops (Emery)" order="Hymenoptera" pageNumber="233" phylum="Arthropoda" rank="species" species="homalops">homalops</taxonomicName>
