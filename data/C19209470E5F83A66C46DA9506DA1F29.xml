<normalizedToken id="EDBEDE12B3923BC15C40E5513DEC9923" originalValue="à">a</normalizedToken>
<taxonomicName id="D3BF4851CC5ED8CE4F7D66124BD12792" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24723" class="Insecta" family="Formicidae" genus="Acromyrmex" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Acromyrmex coronatus (Fabricius)" order="Hymenoptera" pageNumber="233" phylum="Arthropoda" rank="species" species="coronatus">A. coronatus F. Elle</taxonomicName>
