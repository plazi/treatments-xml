<taxonomicName id="57BC4D0BCD48FFCDFCB071A7FBD8B9C1" authorityName="Radford" authorityYear="1957" box="[820,1101,428,452]" class="Arachnida" family="Trombiculidae" genus="Trombicula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="palestinensis">
