<typeStatus id="4F07882ACD48FFCDFD8471A7FDFDB9C1" box="[512,616,428,452]" pageId="52" pageNumber="53">Holotype</typeStatus>
and 
<typeStatus id="4F07882ACD48FFCDFD2571A7FC99B9C1" box="[673,780,428,452]" pageId="52" pageNumber="53">paratypes</typeStatus>
