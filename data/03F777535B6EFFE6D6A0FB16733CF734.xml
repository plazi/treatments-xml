<taxonomicName id="4C5EBDC65B6EFFE6D6A0FB167271F5F6" authority="Wright, 1870" authorityName="Wright" authorityYear="1870" box="[151,584,1235,1261]" class="Maxillopoda" family="Pennellidae" genus="Pennella" kingdom="Animalia" order="Siphonostomatoida" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="orthogorisci">
<emphasis id="B92A1A575B6EFFE6D6A0FB167271F5F6" bold="true" box="[151,584,1235,1261]" pageId="25" pageNumber="26">
<emphasis id="B92A1A575B6EFFE6D6A0FB1671A5F5F6" bold="true" box="[151,412,1235,1261]" italics="true" pageId="25" pageNumber="26">Pennella orthogorisci</emphasis>
<bibRefCitation id="EFCFBBB45B6EFFE6D793FB167271F5F6" author="Wright" box="[420,584,1235,1261]" pageId="25" pageNumber="26" refString="Wright, E. P. (1870) On a new species of the genus Pennella. Annals of the Magazine of Natural History, 4 (5), 43 - 47. https: // doi. org / 10.1080 / 00222937008696103" type="journal article" year="1870">Wright, 1870</bibRefCitation>
