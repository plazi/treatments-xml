<taxonomicName id="C0D135881618CB4FC1201F4FFCF5D644" authority="Noodt, 1952" authorityName="Noodt" authorityYear="1952" box="[556,791,1046,1073]" class="Maxillopoda" family="Laophontidae" genus="Pilifera" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
