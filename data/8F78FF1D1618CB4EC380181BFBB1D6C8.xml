<taxonomicName id="C0D135881618CB4FC27818F5FD76D1B2" authority="Philippi, 1840" authorityName="Philippi" authorityYear="1840" box="[372,660,941,967]" class="Maxillopoda" family="Laophontidae" genus="Laophonte" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
<emphasis id="35A592191618CB4FC27818F5FE07D1B2" box="[372,485,941,967]" italics="true" pageId="16" pageNumber="17">Laophonte</emphasis>
<bibRefCitation id="634033FA1618CB4FC2E118F5FD76D1B2" author="Philippi A" box="[493,660,941,967]" pageId="16" pageNumber="17" pagination="188 - 190" refId="ref20024" refString="Philippi A (1840) Zoologische Bemerkungen (Fortsetzung). IV. Kurze Charakteristik mehrerer neuer Genera aus der Familie der Copepoden. Archiv fur Naturgeschichte 6: 188 - 190." type="journal article" year="1840">Philippi, 1840</bibRefCitation>
</taxonomicName>
: 
<taxonomicName id="C0D135881618CB4FC19318F5FC12D1B2" authority="Boeck, 1873" authorityName="Boeck" authorityYear="1873" box="[671,1008,941,967]" class="Maxillopoda" family="Laophontidae" genus="Laophonte" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="koreni">
<emphasis id="35A592191618CB4FC19318F5FCB8D1B2" box="[671,858,941,967]" italics="true" pageId="16" pageNumber="17">Laophonte koreni</emphasis>
<bibRefCitation id="634033FA1618CB4FC06E18F5FC12D1B2" author="Boeck A" box="[866,1008,941,967]" pageId="16" pageNumber="17" pagination="35 - 60" refId="ref17768" refString="Boeck A (1873) Nye Slaegter og Arter af Saltvands-Copepoder. Forhandlinger i Videnskabsselskabet i Kristiania 1872: 35 - 60." type="journal article" year="1873">Boeck, 1873</bibRefCitation>
</taxonomicName>
(
<typeStatus id="D86AF0A91618CB4FC0F318F5FBD3D1B2" box="[1023,1073,941,967]" pageId="16" pageNumber="17">type</typeStatus>
by original designation), 
<taxonomicName id="C0D135881618CB4FC28B1889FD7DD19F" authority="T. Scott, 1903" authorityName="T. Scott" authorityYear="1903" box="[391,671,976,1003]" class="Maxillopoda" family="Laophontidae" genus="Laophonte" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis id="35A592191618CB4FC28B1889FE13D19F" box="[391,497,976,1002]" italics="true" pageId="16" pageNumber="17">L. gracilis</emphasis>
T. 
<bibRefCitation id="634033FA1618CB4FC1141888FD7DD19F" author="Scott T" box="[536,671,976,1002]" pageId="16" pageNumber="17" pagination="109 - 135" refId="ref20637" refString="Scott T (1903) On some new and rare Crustacea collected at various times in connection with the investigations of the Fishery Board for Scotland. Reports of the Fishery Board for Scotland, Edinburgh 21 (3): 109 - 135." type="journal article" year="1903">Scott, 1903</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="C0D135881618CB4FC1A11889FC51D19F" authority="Sars, 1908 a" authorityName="Sars" authorityYear="1908" box="[685,947,976,1002]" class="Maxillopoda" family="Laophontidae" genus="Laophonte" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="proxima">
<emphasis id="35A592191618CB4FC1A11889FCC5D19F" box="[685,807,977,1002]" italics="true" pageId="16" pageNumber="17">L. proxima</emphasis>
<bibRefCitation id="634033FA1618CB4FC0231888FC51D19F" author="Sars GO" box="[815,947,976,1002]" pageId="16" pageNumber="17" pagination="241 - 256" refId="ref20226" refString="Sars GO (1908 a) Copepoda Harpacticoida. Parts XXI &amp; XXII. Laophontidae (continued). An account of the Crustacea of Norway, with short descriptions and figures of all the species 5: 241 - 256 + pls. 161 - 176." type="journal article" year="1908">Sars, 1908a</bibRefCitation>
</taxonomicName>
