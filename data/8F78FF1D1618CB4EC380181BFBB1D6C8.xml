<taxonomicName id="C0D135881618CB4FC1C81F62FC41D621" box="[708,931,1082,1108]" class="Maxillopoda" family="Laophontidae" genus="Pseudonychocamptus" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
