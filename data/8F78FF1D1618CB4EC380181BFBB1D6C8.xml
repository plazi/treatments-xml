<taxonomicName id="C0D135881618CB4FC16318ACFD3BD678" authorityName="T. Scott" authorityYear="1903" box="[623,729,1011,1037]" class="Maxillopoda" family="Laophontidae" genus="Laophonte" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="gracilis">
