<taxonomicName id="4C673D9CFFD4121572520BEC9AAAF90F" box="[617,731,1696,1720]" class="Arachnida" family="Euphthiracaridae" genus="Euphthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="53" phylum="Arthropoda" rank="species" species="longisetus">
