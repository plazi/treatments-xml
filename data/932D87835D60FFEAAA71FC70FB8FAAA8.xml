<typeStatus id="C43F88375D61FFEAAA44FD10FE01AF40" box="[317,424,764,790]" pageId="13" pageNumber="14">lectotype</typeStatus>
and the 
<typeStatus id="C43F88375D61FFEAA975FD10FD1CAF40" box="[524,693,764,790]" pageId="13" pageNumber="14">paralectotypes</typeStatus>
