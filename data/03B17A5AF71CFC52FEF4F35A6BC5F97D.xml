H 
<quantity id="4CE066A9F71DFC52FF16F0CA6B1DFD58" box="[217,376,667,694]" metricMagnitude="-3" metricUnit="m" metricValue="1.575" metricValueMax="1.65" metricValueMin="1.5" pageId="15" pageNumber="16" unit="mm" value="1.575" valueMax="1.65" valueMin="1.5">1.5–1.65 mm</quantity>
