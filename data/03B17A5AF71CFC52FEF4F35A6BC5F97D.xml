<subSubSection id="C30298C7F71DFC52FF72F03D682FFD58" box="[189,586,620,694]" pageId="15" pageNumber="16" type="description">
<paragraph id="8BA7CB4CF71DFC52FF72F03D6B2BFD6B" blockId="15.[189,334,620,646]" box="[189,334,620,646]" pageId="15" pageNumber="16">
<heading id="D0EF7C20F71DFC52FF72F03D6B2BFD6B" bold="true" box="[189,334,620,646]" fontSize="11" level="3" pageId="15" pageNumber="16" reason="3">
