<paragraph id="C8299451FFA9FA20FF89C894FEDBE4A5" blockId="19.[88,614,1682,1753]" pageId="19" pageNumber="20">
<superScript id="3FE33919FFA9FA20FF89C894FFE8E4E2" attach="right" box="[98,106,1682,1694]" fontSize="5" pageId="19" pageNumber="20">2</superScript>
