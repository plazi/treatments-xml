<location id="CD49C28AFFA9FA20FFB3C803FF48E461" LSID="urn:lsid:plazi:treatment:403F2547FFA9FA27FF91CA17FDD7E068:CD49C28AFFA9FA20FFB3C803FF48E461" box="[88,202,1541,1565]" county="Fazenda Sertao do Formoso" municipality="Jaborandi" name="Moreover" pageId="19" pageNumber="20" stateProvince="Goias">Moreover</location>
