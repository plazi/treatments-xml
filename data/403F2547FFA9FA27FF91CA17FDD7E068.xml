<typeStatus id="172D2AF3FFA9FA20FE4AC846FE53E424" box="[417,465,1600,1624]" pageId="19" pageNumber="20">type</typeStatus>
series. 
<location id="CD49C28AFFA9FA20FDCCC846FDE7E424" LSID="urn:lsid:plazi:treatment:403F2547FFA9FA27FF91CA17FDD7E068:CD49C28AFFA9FA20FDCCC846FDE7E424" box="[551,613,1600,1624]" county="Fazenda Sertao do Formoso" municipality="Jaborandi" name="More" pageId="19" pageNumber="20" stateProvince="Goias">More</location>
