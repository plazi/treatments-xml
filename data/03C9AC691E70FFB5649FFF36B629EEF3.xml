<taxonomicName id="4C6066FC1E71FFB565A4FE73B622EDAF" authority="Lee &amp; Forges &amp; Ng, 2019" authorityName="Lee &amp; Forges &amp; Ng" authorityYear="2019" box="[428,517,477,503]" class="Malacostraca" family="Epialtidae" genus="Crocydocinus" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="ewok" status="n. sp.">
<emphasis id="B914C16D1E71FFB565A4FE73B598EDAF" box="[428,447,477,503]" italics="true" pageId="31" pageNumber="32">C</emphasis>
. 
<emphasis id="B914C16D1E71FFB565C3FE73B622EDAF" box="[459,517,477,503]" italics="true" pageId="31" pageNumber="32">ewok</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2277C161E71FFB56603FE73B66FEDAF" box="[523,584,477,503]" pageId="31" pageNumber="32" rank="species">
<emphasis id="B914C16D1E71FFB56603FE73B66FEDAF" bold="true" box="[523,584,477,503]" pageId="31" pageNumber="32">n. sp.</emphasis>
