<taxonomicName id="4C6066FC1E71FFB56051FE73B109EDAF" authority="Lee &amp; Forges &amp; Ng, 2019" authorityName="Lee &amp; Forges &amp; Ng" authorityYear="2019" box="[1113,1326,477,503]" class="Malacostraca" family="Epialtidae" genus="Crocydocinus" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="porg" status="n. sp.">
