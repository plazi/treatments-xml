<taxonomicName id="4C6066FC1E71FFB5671BFE73B7A9EDAF" authority="Lee &amp; Forges &amp; Ng, 2019" authorityName="Lee &amp; Forges &amp; Ng" authorityYear="2019" box="[787,910,477,503]" class="Malacostraca" family="Epialtidae" genus="Crocydocinus" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="panglao" status="n. sp.">
<emphasis id="B914C16D1E71FFB5671BFE73B701EDAF" box="[787,806,477,503]" italics="true" pageId="31" pageNumber="32">C</emphasis>
