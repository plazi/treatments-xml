<taxonomicName id="7BB3B6D3A729FFF3E2FEBFC4FD51E159" authority="Aguiar et Ramos, 2011" authorityName="Aguiar et Ramos" authorityYear="2011" box="[151,725,1778,1805]" class="Insecta" family="Ichneumonidae" genus="Digonocryptus" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="sautatus">
