<quantity id="4CA08BC3FFA0FFDF4EF5FA33FEE3F9AA" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="4" unit="mm" value="6.0">6 mm</quantity>
