<quantity id="4CA08BC3FFA0FFDF4F58FAB9FB9CFA39" box="[1085,1141,1391,1412]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="4" unit="mm" value="6.0">6 mm</quantity>
