(
<taxonomicName id="4C02CAD10F74F30DFCD7C40AFC92FBCF" box="[785,875,1048,1073]" class="Magnoliopsida" family="Lamiaceae" genus="Mentha" kingdom="Plantae" order="Lamiales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="genus">
