<taxonomicName id="4EF444E5FF92FF9DFD650132FD5AB87A" authorityName="Distant" authorityYear="1903" box="[642,758,557,583]" class="Insecta" family="Reduviidae" genus="Gerbelius" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis id="BB80E374FF92FF9DFD650132FD5AB87A" box="[642,758,557,583]" italics="true" pageId="25" pageNumber="26">Gerbelius</emphasis>
</taxonomicName>
