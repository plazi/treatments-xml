<taxonomicName id="4EF444E5FF92FF9DFAE30115FAD4B819" authorityName="Distant" authorityYear="1903" box="[1284,1400,522,548]" class="Insecta" family="Reduviidae" genus="Gerbelius" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
