<materialsCitation id="56EE3C9DFF11FF42FE9BFF4DFEC9FBEC" ID-GBIF-Occurrence="2849085607" collectingDate="2011-09-05" collectionCode="MB" country="Lebanon" location="New" pageId="147" pageNumber="148" specimenCount="1">
<location id="E359601BFF11FF42FE9BFF4DFEA6FBA4" LSID="urn:lsid:plazi:treatment:6E2F87D6FF10FF42FF2EF819FDA3FAB8:E359601BFF11FF42FE9BFF4DFEA6FBA4" box="[290,343,153,179]" country="Lebanon" name="New" pageId="147" pageNumber="148">New</location>
