<subSubSection id="B1B5652DFFC6FF8844EB0F7D13BFDFAB" pageId="4" pageNumber="579" type="etymology">
<paragraph id="F91036A6FFC6FF8844EB0F7D13BFDFAB" blockId="4.[151,1437,845,1673]" pageId="4" pageNumber="579">
<emphasis id="CBDBEAB4FFC6FF8844EB0F7D1226DF46" bold="true" box="[199,326,1610,1636]" pageId="4" pageNumber="579">Etymology</emphasis>
