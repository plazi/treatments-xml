<collectingCountry id="81B87636FFC6FF8847E20F101771DF63" box="[974,1041,1575,1601]" name="China" pageId="4" pageNumber="579">China</collectingCountry>
(western 
<collectingRegion id="3B6BF844FFC6FF8840520F1F1624DF60" box="[1150,1348,1575,1602]" country="China" name="Yunnan" pageId="4" pageNumber="579">Yunnan Province</collectingRegion>
