<collectingCountry id="81B87636FFC6FF8845830F101173DF63" box="[431,531,1575,1601]" name="Thailand" pageId="4" pageNumber="579">Thailand</collectingCountry>
(
<collectingRegion id="3B6BF844FFC6FF88460D0F101074DF60" box="[545,788,1575,1602]" country="Thailand" name="Chiang Mai" pageId="4" pageNumber="579">Chiang Mai Province</collectingRegion>
