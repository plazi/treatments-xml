<collectorName id="26CA5323FFCC595AFCC4F8F8D9FC863B" box="[819,918,1826,1849]" pageId="0" pageNumber="596">L. Vivoff</collectorName>
leg. (
<collectionCode id="ED2EAE30FFCC595AFC25F8F8DE42863B" box="[978,1064,1826,1849]" country="Argentina" httpUri="http://grbio.org/cool/pyng-6456" name="Museo Argentino de Ciencias Naturales Bernardino Rivadavia" pageId="0" pageNumber="596" type="Museum">MACN</collectionCode>
