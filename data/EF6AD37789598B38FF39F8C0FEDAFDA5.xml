<paragraph id="677C626189598B3AFF39F8C0FEFCF8F9" blockId="17.[151,426,1865,1927]" box="[151,313,1865,1892]" pageId="17" pageNumber="476">
<heading id="3C34D50D89598B3AFF39F8C0FEFCF8F9" bold="true" box="[151,313,1865,1892]" fontSize="11" level="1" pageId="17" pageNumber="476" reason="1">
<emphasis id="55B7BE7389598B3AFF39F8C0FEFCF8F9" bold="true" box="[151,313,1865,1892]" pageId="17" pageNumber="476">
<taxonomicName id="A0C319E289598B3AFF39F8C0FECBF8FE" authorityName="Nicoll" authorityYear="1907" box="[151,270,1865,1892]" class="Trematoda" family="Philophthalmidae" genus="Parorchis" kingdom="Animalia" order="Plagiorchiida" pageId="17" pageNumber="476" phylum="Platyhelminthes" rank="genus">
<emphasis id="55B7BE7389598B3AFF39F8C0FECBF8FE" bold="true" box="[151,270,1865,1892]" italics="true" pageId="17" pageNumber="476">Parorchis</emphasis>
</taxonomicName>
sp.
</emphasis>
