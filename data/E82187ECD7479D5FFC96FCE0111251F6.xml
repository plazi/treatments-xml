<specimenCount id="768EFD73D7479D5FFBDBFB27108D52E9" box="[1127,1225,1186,1210]" pageId="3" pageNumber="794" type="adult">two adult</specimenCount>
females (
<collectionCode id="0699AE3FD7479D5FFA97FB2711CF52E9" box="[1323,1419,1186,1210]" pageId="3" pageNumber="794">USMHC</collectionCode>
2443, 
<collectionCode id="0699AE3FD7479D5FFCD5FB4117D8528F" box="[873,924,1220,1244]" pageId="3" pageNumber="794">SVL</collectionCode>
<quantity id="A7709B1FD7479D5FFC1FFB4117B55288" box="[931,1009,1220,1244]" metricMagnitude="-2" metricUnit="m" metricValue="5.3" pageId="3" pageNumber="794" unit="mm" value="53.0">53 mm</quantity>
; 
<collectionCode id="0699AE3FD7479D5FFC40FB41101E528F" box="[1020,1114,1220,1244]" pageId="3" pageNumber="794">USMHC</collectionCode>
2444, 
<collectionCode id="0699AE3FD7479D5FFB18FB411093528F" box="[1188,1239,1220,1244]" pageId="3" pageNumber="794">SVL</collectionCode>
<quantity id="A7709B1FD7479D5FFB62FB41116F528F" box="[1246,1323,1220,1244]" metricMagnitude="-2" metricUnit="m" metricValue="4.4" pageId="3" pageNumber="794" unit="mm" value="44.0">44 mm</quantity>
