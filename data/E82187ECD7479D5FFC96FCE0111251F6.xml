<quantity id="A7709B1FD7479D5FFBB0FB27101C52E9" box="[1036,1112,1186,1210]" metricMagnitude="-2" metricUnit="m" metricValue="5.4" pageId="3" pageNumber="794" unit="mm" value="54.0">54 mm</quantity>
