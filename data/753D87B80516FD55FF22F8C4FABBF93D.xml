<taxonomicName id="3A944D2D0517FD55FB94FB7DFB66FB05" box="[1057,1192,1161,1184]" class="Malacostraca" family="Hymenosomatidae" genus="Culexisoma" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="497" phylum="Arthropoda" rank="species" species="ginowan">
