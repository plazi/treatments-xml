<taxonomicName id="4C704F95FFC6FFCC55DCFC09125464F3" box="[151,278,902,927]" class="Crinoidea" family="Septocrinidae" genus="Rouxicrinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="3" pageNumber="52" phylum="Echinodermata" rank="species" species="vestitus">
