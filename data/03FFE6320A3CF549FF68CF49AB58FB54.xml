<treatmentCitationGroup id="AB46700A0A3CF549FF38CFA2ABE6FC59" pageId="57" pageNumber="58">
<taxonomicName id="4C562CA70A3CF549FF38CFA2A889FC34" authority="Linnaeus, 1767: 1245" authorityName="Linnaeus" authorityPageNumber="1245" authorityYear="1767" box="[199,701,908,933]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="57" pageNumber="58" phylum="Mollusca" rank="species" species="lusitanica">
<emphasis id="B9228B360A3CF549FF38CFA2ABBFFC34" box="[199,395,908,933]" italics="true" pageId="57" pageNumber="58">Helix lusitanica</emphasis>
<treatmentCitation id="0AF771350A3CF549FE5BCFA2A889FC34" author="Linnaeus" box="[420,701,908,933]" page="1245" pageId="57" pageNumber="58" year="1767">
<bibRefCitation id="EFC72AD50A3CF549FE5BCFA2A852FC34" author="Linnaeus" box="[420,614,908,933]" pageId="57" pageNumber="58" refString="Linnaeus, C. (1767) Systema naturae per regna tria naturae, secundum classes, ordines, genera, species, cum characteribus, differentiis, synonymis, locis. Tom. I. Pars II. Editio duodecima, reformata. L. Salvii, Holmiae [= Stockholm], 794 + [37] pp. [pp. 533 - 1327 + [1 - 37], 14 June; Evenhuis 2006: 147]" type="journal article" year="1767">
Linnaeus, 
<number id="3D9E45030A3CF549FDD5CFA2A852FC34" box="[554,614,908,933]" pageId="57" pageNumber="58" value="1767.0">1767</number>
</bibRefCitation>
: 
<number id="3D9E45030A3CF549FD7DCFA2A889FC34" box="[642,701,908,933]" pageId="57" pageNumber="58" value="1245.0">1245</number>
</treatmentCitation>
</taxonomicName>
. 
<emphasis id="B9228B360A3CF549FD25CFA3A9B2FC34" box="[730,902,908,933]" italics="true" pageId="57" pageNumber="58">
<typeStatus id="54EDE9860A3CF549FD25CFA3A93AFC35" box="[730,782,909,932]" pageId="57" pageNumber="58">Type</typeStatus>
material
</emphasis>
—possible 
<typeStatus id="54EDE9860A3CF549FBE9CFA3AE46FC34" box="[1046,1138,909,933]" pageId="57" pageNumber="58">syntype</typeStatus>
: LSL (
<number id="3D9E45030A3CF549FB1BCFA2AEC4FC35" box="[1252,1264,908,932]" pageId="57" pageNumber="58" value="1.0">1</number>
spm.). 
<emphasis id="B9228B360A3CF549FA98CFA3AADFFC59" italics="true" pageId="57" pageNumber="58">
