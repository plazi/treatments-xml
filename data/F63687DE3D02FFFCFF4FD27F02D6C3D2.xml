<emphasis id="4CEBEADA3D03FFFFFF1FD5EE0010C2B2" bold="true" box="[199,321,151,176]" pageId="33" pageNumber="334">Diagnosis.</emphasis>
<taxonomicName id="B99F4D4B3D03FFFFFE91D5EE00A0C2B2" authority="Bustamante &amp; Ruiz, 2017" authorityName="Bustamante &amp; Ruiz" authorityYear="2017" box="[329,497,151,176]" class="Arachnida" family="Salticidae" genus="Thiodina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="334" phylum="Arthropoda" rank="species" species="firme" status="sp. nov.">
