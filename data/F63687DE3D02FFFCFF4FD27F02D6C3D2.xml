<taxonomicName id="B99F4D4B3D03FFFFFD0CD5EE0205C2B2" authorityName="Roewer" authorityYear="1951" box="[724,852,151,176]" class="Arachnida" family="Salticidae" genus="Thiodina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="334" phylum="Arthropoda" rank="species" species="nicoleti">
<emphasis id="4CEBEADA3D03FFFFFD0CD5EE0205C2B2" box="[724,852,151,176]" italics="true" pageId="33" pageNumber="334">Th. nicoleti</emphasis>
</taxonomicName>
, 
<taxonomicName id="B99F4D4B3D03FFFFFCBBD5EE0288C2B2" authority="Bustamante &amp; Ruiz, 2017" authorityName="Bustamante &amp; Ruiz" authorityYear="2017" box="[867,985,151,176]" class="Arachnida" family="Salticidae" genus="Thiodina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="334" phylum="Arthropoda" rank="species" species="perian" status="sp. nov.">
<emphasis id="4CEBEADA3D03FFFFFCBBD5EE0288C2B2" box="[867,985,151,176]" italics="true" pageId="33" pageNumber="334">Th. perian</emphasis>
</taxonomicName>
<taxonomicNameLabel id="57D857A13D03FFFFFC39D5EE056DC2B2" box="[993,1084,151,176]" pageId="33" pageNumber="334" rank="species">
<emphasis id="4CEBEADA3D03FFFFFC39D5EE056DC2B2" bold="true" box="[993,1084,151,176]" pageId="33" pageNumber="334">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="B99F4D4B3D03FFFFFBACD5EE05BEC2B2" authorityName="Bustamante &amp; Ruiz" authorityYear="2017" baseAuthorityName="Bustamante &amp; Ruiz" baseAuthorityYear="2017" box="[1140,1263,151,176]" class="Arachnida" family="Salticidae" genus="Cyllodania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="334" phylum="Arthropoda" rank="species" species="minuta" status="comb. nov.">
<emphasis id="4CEBEADA3D03FFFFFBACD5EE05BEC2B2" box="[1140,1263,151,176]" italics="true" pageId="33" pageNumber="334">Th. minuta</emphasis>
</taxonomicName>
<taxonomicNameLabel id="57D857A13D03FFFFFB2ED5EE0429C2B2" box="[1270,1400,151,176]" pageId="33" pageNumber="334">
<emphasis id="4CEBEADA3D03FFFFFB2ED5EE0429C2B2" bold="true" box="[1270,1400,151,176]" pageId="33" pageNumber="334">comb. nov.</emphasis>
</taxonomicNameLabel>
