<taxonomicName id="4C0525C1A06EFF9CFD58F8D46F614E0B" box="[749,794,1857,1882]" class="Insecta" family="Cicadellidae" genus="Regalana" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="242" phylum="Arthropoda" rank="species" species="uni" status="sp. nov.">Uni</taxonomicName>
