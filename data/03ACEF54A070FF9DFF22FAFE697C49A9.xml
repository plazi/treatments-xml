<subSubSection id="C31F0DC9A06EFF9CFF72F8D46DE84E2F" pageId="16" pageNumber="241" type="etymology">
<paragraph id="8BBA5E42A06EFF9CFF72F8D46DE84E2F" blockId="16.[151,1436,1641,2026]" pageId="16" pageNumber="241">
<emphasis id="B9718250A06EFF9CFF72F8D46D354E0B" bold="true" box="[199,334,1857,1882]" pageId="16" pageNumber="241">Etymology.</emphasis>
