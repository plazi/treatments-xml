<taxonomicName id="3BDDB5E22264FF95FF44FC6F14E4FC5B" authority="Spanish" authorityName="Spanish" box="[151,506,973,997]" class="Actinopterygii" family="Lutjanidae" genus="Lutjanus" kingdom="Animalia" order="Perciformes" pageId="28" pageNumber="29" phylum="Chordata" rank="species" species="carponotatus">
