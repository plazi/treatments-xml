<taxonomicName id="F16AB86B72F961D1626CB5D28486E81F" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="178" pageNumber="179" phylum="Mollusca" rank="species" species="zoographicus" subGenus="Drymaeus">
