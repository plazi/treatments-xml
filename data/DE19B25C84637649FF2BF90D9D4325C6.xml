<taxonomicName id="91B078C984637657FF2BF90D9D2F22F2" authority="Muesebeck" authorityName="Muesebeck" box="[151,592,1701,1728]" class="Insecta" family="Braconidae" genus="Cremnops" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="crassifemur">
