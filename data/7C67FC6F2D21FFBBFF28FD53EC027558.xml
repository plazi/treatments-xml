<collectingCountry id="8CD90DE92D20FFBBFC98FB7CEC3E7528" box="[807,870,512,538]" name="India" pageId="19" pageNumber="20">India</collectingCountry>
, 
<collectingRegion id="360A839B2D20FFBBFCCCFB7CEC9F7528" box="[883,967,512,538]" country="India" name="Assam" pageId="19" pageNumber="20">Assam</collectingRegion>
