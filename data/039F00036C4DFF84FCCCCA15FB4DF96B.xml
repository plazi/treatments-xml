<taxonomicName id="4C36CA966C4DFF83FBC8C89EFB79F97E" authority="Melika. Life" authorityName="Melika. Life" authorityYear="2003" box="[1078,1268,1692,1712]" class="Insecta" family="Cynipidae" genus="Salviela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
