<taxonomicName id="6183B088FFE2E07DFD96626EFC8FFCA2" box="[587,838,844,869]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="infulae">
