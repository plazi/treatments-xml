<taxonomicName id="0EE1188704C2FF41FD30FF2C758B10CC" authorityName="$ Herouard" authorityYear="1902" box="[660,921,225,251]" class="Holothuroidea" family="Psychropotidae" genus="Psychropotes" kingdom="Animalia" order="Elasipodida" pageId="146" pageNumber="147" phylum="Echinodermata" rank="species" species="kerhervei">
<emphasis id="FB95BF1604C2FF41FD30FF2C758B10CC" box="[660,921,225,251]" italics="true" pageId="146" pageNumber="147">Psychropotes kerhervei</emphasis>
</taxonomicName>
