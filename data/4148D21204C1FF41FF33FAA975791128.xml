<geoCoordinate id="ACD505C304C1FF42FC8DF81A759E17C5" box="[809,908,2007,2034]" degrees="39" direction="north" minutes="54" orientation="latitude" pageId="145" pageNumber="146" precision="925" value="39.9">39°54’N</geoCoordinate>
