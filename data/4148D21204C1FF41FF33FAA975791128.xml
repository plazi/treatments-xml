<taxonomicName id="0EE1188704C2FF41FD62FECB75771128" authorityName="Theel" authorityYear="1882" box="[710,869,262,287]" class="Holothuroidea" family="Psychropotidae" genus="Psychropotes" kingdom="Animalia" order="Elasipodida" pageId="146" pageNumber="147" phylum="Echinodermata" rank="species" species="semperiana">
