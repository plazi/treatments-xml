<taxonomicName id="4C7A4D61FFE1574F2C3EF913FAD2F94E" box="[1094,1300,1744,1769]" class="Magnoliopsida" family="Anacardiaceae" genus="Mangifera" kingdom="Plantae" order="Sapindales" pageId="1" pageNumber="2" phylum="Tracheophyta" rank="species" species="indica">
