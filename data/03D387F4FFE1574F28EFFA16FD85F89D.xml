<taxonomicName id="4C7A4D61FFE1574F2914F913FE39F94E" box="[364,511,1744,1769]" class="Insecta" family="Phlaeothripidae" genus="Bactrothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
