<taxonomicName id="4C7A4D61FFE1574F2AE7F913FC87F94D" box="[671,833,1744,1770]" class="Insecta" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="order">Thysanoptera</taxonomicName>
