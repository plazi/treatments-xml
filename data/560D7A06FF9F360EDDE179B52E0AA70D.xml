<taxonomicName id="19A4B093FF9F360EDF677D232E94A76D" authorityName="Asche" authorityYear="1990" box="[866,993,1042,1066]" class="Insecta" family="Delphacidae" genus="Vizcaya" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="612" phylum="Arthropoda" rank="species" species="vindaloa">
