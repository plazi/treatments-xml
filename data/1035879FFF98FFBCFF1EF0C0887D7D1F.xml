<taxonomicName id="5F9C4D0AFF98FFBCFDD8F55D89687E69" box="[520,604,1350,1369]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="330" phylum="Arthropoda" rank="subFamily" subFamily="Paussinae">Paussinae</taxonomicName>
