(
<figureCitation id="1349D497AA58FFCDFE79FAF7FD994EE7" box="[422,543,1401,1427]" captionStart-0="FIGURES 1 – 8" captionStart-1="FIGURES 9 – 17" captionStart-2="FIGURES 18 – 24" captionStartId-0="1.[151,258,1736,1758]" captionStartId-1="3.[151,258,1573,1595]" captionStartId-2="4.[151,258,1527,1549]" captionTargetBox-0="[323,1266,362,1710]" captionTargetBox-1="[337,1260,195,1547]" captionTargetBox-2="[332,1255,208,1482]" captionTargetId-0="figure@1.[311,1275,354,1715]" captionTargetId-1="figure@3.[318,1269,191,1552]" captionTargetId-2="figure@4.[311,1275,191,1506]" captionTargetPageId-0="1" captionTargetPageId-1="3" captionTargetPageId-2="4" captionText-0="FIGURES 1 – 8. Transvenosus nigrodorsalis sp. n., male: 1. Habitus, dorsal view; 2. Habitus, lateral view; 3. Face; 4. Pygofer side, lateral view; 5. Genital capsule, lateral view; 6. Subgenital plate, ventral view; 7. Aedeagus, lateral view; 8. Aedeagus, connective and style, ventral view." captionText-1="FIGURES 9 – 17. Transvenosus signuma (Li &amp; Webb, 1996), n. comb., male: 9. Habitus, dorsal view; 10. Habitus, lateral view; 11. Face; 12. Pygofer side, lateral view; 13. Genital capsule, lateral view; 14. Subgenital plate, ventral view; 15. Aedeagus, lateral view; 16. Aedeagus, connective and style, ventral view; 17. Front femur, anterior view." captionText-2="FIGURES 18 – 24. Transvenosus nigrodorsalis sp. n., female: 18. Habitus, dorsal view; 19. Face; 20. sternite VII; 21. Apex of first valvula; 22. First valvula; 23. Second valvula; 24. Third valvula." httpUri-0="https://zenodo.org/record/244451/files/figure.png" httpUri-1="https://zenodo.org/record/244452/files/figure.png" httpUri-2="https://zenodo.org/record/244453/files/figure.png" pageId="0" pageNumber="595">
Figs 
<number id="3DBADA35AA58FFCDFE3BFAF7FD994EE7" box="[484,543,1401,1427]" pageId="0" pageNumber="595" value="12.5" valueMax="24.0" valueMin="1.0">1–24</number>
</figureCitation>
)
