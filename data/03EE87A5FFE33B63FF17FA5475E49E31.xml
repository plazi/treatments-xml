), and 
<taxonomicName id="4C474D30FFE43B63FE18FE7E765899BE" authority="SONG &amp; LI, 2013" authorityName="SONG &amp; LI" authorityYear="2013" class="Insecta" family="Staphylinidae" genus="Pella" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="37" phylum="Arthropoda" rank="species" species="maoershanensis">
<emphasis id="B933EAA1FFE43B63FE18FE7E75CD99DE" box="[475,669,416,440]" italics="true" pageId="4" pageNumber="37">P. maoershanensis</emphasis>
