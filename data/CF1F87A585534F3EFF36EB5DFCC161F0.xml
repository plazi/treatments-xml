<paragraph id="470936B385534F3EFF36EC4EFE45630A" blockId="4.[151,1437,1020,1140]" box="[151,419,1116,1140]" pageId="4" pageNumber="42">
<emphasis id="75C2EAA185534F3EFF36EC4EFEFA630A" bold="true" box="[151,284,1116,1140]" pageId="4" pageNumber="42">MycoBank:</emphasis>
<accessionNumber id="58E5AB5085534F3EFE83EC4FFE45630A" box="[290,419,1117,1140]" pageId="4" pageNumber="42">
<collectionCode id="21A7AE7685534F3EFE83EC4FFEAD630A" box="[290,331,1117,1140]" country="Portugal" name="Universidade de Lisboa, Museu Bocage" pageId="4" pageNumber="42">MB</collectionCode>
