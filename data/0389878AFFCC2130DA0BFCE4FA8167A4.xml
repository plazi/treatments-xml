Basonym: 
<taxonomicName id="4C204D1FFFCC2130DA9CFBBAFAD66622" authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[939,1401,1068,1093]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="wuchangensis">
