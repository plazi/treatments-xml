<subSubSection id="56C02B2D1A96C10FD13C75E2D35D7A32" pageId="2" pageNumber="57" type="nomenclature">
<paragraph id="DD768FAFD1892D4FAA0FFB7E419AB298" pageId="2" pageNumber="57">
<taxonomicName id="7A1C51AEE6F611123D202CC1800CC3B4" LSID="http://zoobank.org/E01181D5-08DB-4651-98F7-7DFFE6106BB2" class="Insecta" family="Histeridae" genus="Malagasyprinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Malagasyprinus" order="Coleoptera" pageId="2" pageNumber="57" phylum="Arthropoda" rank="genus">Malagasyprinus</taxonomicName>
