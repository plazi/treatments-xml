The examined specimen had the following measurements: BL 
<quantity id="04419B2D9B68E8300182BEDFFD0C8BEF" box="[944,1038,1688,1714]" metricMagnitude="-3" metricUnit="m" metricValue="5.7" pageId="4" pageNumber="25" unit="mm" value="5.7">5.7 mm</quantity>
, BH 
<quantity id="04419B2D9B68E830067EBEDFFDA88BEF" box="[1100,1194,1688,1714]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="4" pageNumber="25" unit="mm" value="2.5">2.5 mm</quantity>
, BW 
<quantity id="04419B2D9B68E83006C2BEDFFC4C8BEF" box="[1264,1358,1688,1714]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="4" pageNumber="25" unit="mm" value="3.3">3.3 mm</quantity>
, 
<collectionCode id="A5A8AE0D9B68E8300769BEDFFC9E8BEF" box="[1371,1436,1688,1714]" httpUri="http://grbio.org/cool/qxtm-hypi" name="Tasmanian Museum and Art Gallery" pageId="4" pageNumber="25">TMH</collectionCode>
<quantity id="04419B2D9B68E83002A5BEF9F9F78B85" box="[151,245,1726,1752]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="4" pageNumber="25" unit="mm" value="1.1">1.1 mm</quantity>
, TMW 
<quantity id="04419B2D9B68E8300360BEF9F8B28B85" box="[338,432,1726,1752]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="4" pageNumber="25" unit="mm" value="1.2">1.2 mm</quantity>
, MTH 
<quantity id="04419B2D9B68E8300034BEF9FB668B85" box="[518,612,1726,1752]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="4" pageNumber="25" unit="mm" value="2.1">2.1 mm</quantity>
, 
<collectionCode id="A5A8AE0D9B68E8300043BEF9FBCF8B85" box="[625,717,1726,1752]" pageId="4" pageNumber="25">TMHM</collectionCode>
<quantity id="04419B2D9B68E83000E7BEF9FA318B85" box="[725,819,1726,1752]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="25" unit="mm" value="0.9">0.9 mm</quantity>
, ED 
<quantity id="04419B2D9B68E830015DBEF9FACF8B85" box="[879,973,1726,1752]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="4" pageNumber="25" unit="mm" value="0.6">0.6 mm</quantity>
, IOD 
<quantity id="04419B2D9B68E8300625BEF9FD778B85" box="[1047,1141,1726,1752]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="4" pageNumber="25" unit="mm" value="1.6">1.6 mm</quantity>
, 
<collectingCountry id="BBAE76589B68E83006B3BEF9FDB78B85" box="[1153,1205,1726,1752]" name="India" pageId="4" pageNumber="25">IND</collectingCountry>
<quantity id="04419B2D9B68E830068CBEF9FC1E8B85" box="[1214,1308,1726,1752]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="4" pageNumber="25" unit="mm" value="1.3">1.3 mm</quantity>
, ODW 
<quantity id="04419B2D9B68E8300745BEF9F9C88BA2" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="4" pageNumber="25" unit="mm" value="1.7">1.7 mm</quantity>
, TAL 
<quantity id="04419B2D9B68E830032BBEA2F8888BA2" box="[281,394,1765,1791]" metricMagnitude="-2" metricUnit="m" metricValue="1.13" pageId="4" pageNumber="25" unit="mm" value="11.3">11.3 mm</quantity>
, TL 
<quantity id="04419B2D9B68E83003F4BEA2FB3A8BA2" box="[454,568,1765,1791]" metricMagnitude="-2" metricUnit="m" metricValue="1.71" pageId="4" pageNumber="25" unit="mm" value="17.1">17.1 mm</quantity>
, TN 
<number id="757124EF9B68E8300045BEA2FB9B8BA2" box="[631,665,1765,1791]" pageId="4" pageNumber="25" value="90.0">90</number>
