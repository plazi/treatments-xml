<paragraph id="FB37F50CFFE0FFC113A878D2C6B47135" blockId="21.[151,1436,151,428]" pageId="21" pageNumber="22">
<materialsCitation id="4BE0FF51FFE0FFC113A878D2C6887135" collectingDate="1949-01-04" collectionCode="MCZC" collectorName="J. C. Bradley" country="Democratic Republic of the Congo" location="Its" municipality="Epulu" pageId="21" pageNumber="22" specimenCount="2" typeStatus="holotype">
