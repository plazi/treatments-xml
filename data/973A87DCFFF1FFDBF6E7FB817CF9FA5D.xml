<materialsCitation id="AFFB3C97FFF0FFDBF70FFD3D7C37FCC6" ID-GBIF-Occurrence="2244539061" collectingDate="1990-06-26" collectionCode="NSMT" collectorName="Oshidomari-guchi &amp; Rishiri Is. &amp; A. Shinohara" elevation="800" location="Mt. Rishiri" pageId="5" pageNumber="176" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
