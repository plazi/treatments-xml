(
<collectingRegion id="DD57F828FFF0FFDBF4AAFC717D3DFC31" box="[730,883,945,972]" country="China" name="Heilongjiang" pageId="5" pageNumber="176">Heilongjiang</collectingRegion>
), 
<collectingCountry id="6784765AFFF0FFDBF5F6FC717D88FC36" box="[902,966,945,971]" name="Japan" pageId="5" pageNumber="176">Japan</collectingCountry>
