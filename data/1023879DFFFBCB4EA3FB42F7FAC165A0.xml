<taxonomicName id="5F8A4D08FFFBCB4EA09747CFFB8865A0" box="[1019,1092,1706,1730]" class="Arachnida" family="Sparassidae" genus="Curicaberis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="36" pageNumber="437" phylum="Arthropoda" rank="species" species="potosi" status="sp. nov.">Potosí</taxonomicName>
