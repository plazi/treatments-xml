LOCALITY: ‘‘America’’; restricted to 
<collectingCountry id="F320F0F14B78D86CFF1432D0B648FE6A" box="[163,262,398,421]" name="Suriname" pageId="44" pageNumber="45">Surinam</collectingCountry>
