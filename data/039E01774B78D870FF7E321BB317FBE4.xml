<taxonomicName id="4C37CBE24B64D870FBA9307DB7A4FC9D" authority=", MNFS" authorityName="MNFS" class="Mammalia" family="Didelphidae" genus="Philander" kingdom="Animalia" order="Didelphimorphia" pageId="48" pageNumber="49" phylum="Chordata" rank="species" species="opossum">
