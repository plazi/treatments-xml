<taxonomicName id="4C2238BEB45CFF93FEF8D65CFDDCFCA2" authorityName="Coccoidea" baseAuthorityName="Bouche" baseAuthorityYear="1844" box="[353,518,799,825]" class="Insecta" family="Pseudococcidae" genus="Spilococcus" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="517" phylum="Arthropoda" rank="species" species="mamillariae">
