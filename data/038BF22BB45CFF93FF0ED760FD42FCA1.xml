<taxonomicName id="4C2238BEB45CFF93FACAD7F0FF11FD69" authorityName="de Jussieu" authorityYear="1789" class="Magnoliopsida" family="Cactaceae" kingdom="Plantae" order="Caryophyllales" pageId="16" pageNumber="517" phylum="Tracheophyta" rank="family">Cactaceae</taxonomicName>
