<taxonomicName id="4C42D3FC0302FFE1B6C8E184CE8BFA6F" authority="Stimpson, 1857" authorityName="Stimpson" authorityYear="1857" box="[491,903,1499,1523]" family="Stylochidae" kingdom="Animalia" order="Polycladida" pageId="3" pageNumber="2090" phylum="Platyhelminthes" rank="family">
<emphasis id="B936746D0302FFE1B6C8E184CFC9FA6F" bold="true" box="[491,709,1499,1523]" pageId="3" pageNumber="2090">STYLOCHIDAE</emphasis>
Stimpson, 1857
</taxonomicName>
