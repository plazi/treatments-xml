<specimenCode id="9B089EADA87CFFA3FFC8FB33FEA80DE1" box="[93,277,1254,1275]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="100" pageNumber="100">AMNH 522773</specimenCode>
