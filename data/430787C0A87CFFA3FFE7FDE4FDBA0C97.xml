<date id="BF101016A87CFFA3FE94FB5BFE4B0DB9" box="[257,502,1166,1188]" pageId="100" pageNumber="100" value="1899-02-20">
<collectingDate id="AF54E9FEA87CFFA3FE94FB5BFE4B0DB9" box="[257,502,1166,1188]" pageId="100" pageNumber="100" value="1899-02-20">20 February 1899</collectingDate>
</date>
, Cherrie no. 12017
</materialsCitation>
; 
<materialsCitation id="7BC63C8BA87CFFA3FF7AFB7EFDD50DC5" collectingDate="1899-02-28" collectionCode="AMNH" pageId="100" pageNumber="100" specimenCode="AMNH 522772" specimenCount="1" typeStatus="holotype">
