<collectionCode id="ADBFAE13A87CFFA3FE56FAADFDBE0C97" box="[451,515,1400,1421]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="100" pageNumber="100">SMF</collectionCode>
