<taxonomicName id="C84A74B862244C5EEB527A4653F7D18A" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Castanopsis" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">
