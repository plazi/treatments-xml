<taxonomicName id="EDEA2882FFB86B34FB8EFC2FB1FD029D" box="[1133,1205,918,942]" class="Arachnida" family="Phytoseiidae" genus="Neoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="625" phylum="Arthropoda" rank="species" species="teke">
