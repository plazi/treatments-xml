<taxonomicName id="4841C8A3FA083C2A00852840EADAC68F" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" infraspecific-rank="var." kingdom="Animalia" lsidName="Hedychridium roseum var. caputaureum" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="variety" species="roseum" variety="caputaureum">Hedychridium roseum var. caputaureum</taxonomicName>
