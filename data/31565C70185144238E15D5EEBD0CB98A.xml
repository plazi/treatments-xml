<subSubSection id="6DD04780CFD8A03DB1BBD428068D4778" pageId="null" pageNumber="291" type="nomenclature">
<paragraph id="F2227825F4DCF5EE5E37035B97A28139" pageId="null" pageNumber="291">
Artengruppe des 
<taxonomicName id="7ABD186B8A889D991C8F327C37F384F6" authority="L." class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="291" phylum="Tracheophyta" rank="species" species="palustre">
Galium 
<normalizedToken id="C7E6629700560BBE1E56F20D34ADEA2A" originalValue="palústre" pageId="null" pageNumber="291">palustre</normalizedToken>
<authorityName id="3971F4ADE2879DDD8070E8862627B3A4" pageId="null" pageNumber="291">L.</authorityName>
