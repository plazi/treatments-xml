<paragraph id="2A4BFC16D656763F7BF3FB7F7FF8FB42" blockId="26.[151,1437,1162,1764]" box="[151,463,1162,1188]" pageId="26" pageNumber="227">
<emphasis id="18802004D656763F7BF3FB7F7ECBFB45" bold="true" box="[151,252,1162,1187]" pageId="26" pageNumber="227">Stations.</emphasis>
SC 
<number id="9C3CEE31D656763F7A4DFB797F00FB42" box="[297,311,1164,1188]" pageId="26" pageNumber="227" value="7.0">7</number>
; SS 
<number id="9C3CEE31D656763F7A00FB797FB6FB42" box="[356,385,1164,1188]" pageId="26" pageNumber="227" value="10.0">10</number>
; SS 
<number id="9C3CEE31D656763F7ACAFB797FFCFB42" box="[430,459,1164,1188]" pageId="26" pageNumber="227" value="12.0">12</number>
