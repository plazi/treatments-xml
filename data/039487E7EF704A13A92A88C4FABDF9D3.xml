<taxonomicName id="4C3D4D72EF704A13A92A88C4FCD4FB0D" authorityName="Fagan-Jeffries, Bird &amp; Austin" authorityYear="2022" box="[564,855,1191,1218]" class="Insecta" family="Braconidae" genus="Glyptapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="albigena">
