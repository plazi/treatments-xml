<taxonomicName id="4C5EE9CBFFF6FFBDFC10E7EB0C1CFB6D" authority="Promdam &amp; Nabhitabhata &amp; Ng, 2014" authorityName="Promdam &amp; Nabhitabhata &amp; Ng" authorityYear="2014" box="[981,1289,1166,1189]" class="Malacostraca" family="Potamidae" genus="Nakhonsimon" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="496" phylum="Arthropoda" rank="species" species="ramromensis" status="new species">
