<taxonomicName id="5A4B23E1FC641BE2C6EAABBACF72C981" LSID="http://zoobank.org/61AA9C32-4368-47D8-98DD-DF7FA8A279CC" class="Actinopterygii" family="Mormyridae" genus="Cryptomyrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cryptomyrus ogoouensis" order="Osteoglossiformes" pageId="13" pageNumber="130" phylum="Chordata" rank="species" species="ogoouensis">Cryptomyrus ogoouensis</taxonomicName>
