<date id="FFBD106BE132FFCC2E30FAE8826B5402" box="[604,790,1297,1318]" pageId="3" pageNumber="266" value="2003-08-23" valueMax="2003-08-27" valueMin="2003-08-23">23–27 August 2003</date>
<emphasis id="B977EAB9E132FFCC2F72FAE882565403" bold="true" box="[798,811,1297,1319]" pageId="3" pageNumber="266">5</emphasis>
<taxonomicName id="4C034D28E132FFCC2F5FFAE8852D5403" authority="(Schaus)" authorityName="Sullivan" authorityYear="2010" baseAuthorityName="Sullivan" baseAuthorityYear="2010" box="[819,1104,1297,1319]" class="Insecta" family="Geometridae" genus="Campatonema" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="266" phylum="Arthropoda" rank="species" species="lineata">
