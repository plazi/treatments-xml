<emphasis id="B977EAB9E132FFCC2E73FB0883515423" bold="true" box="[543,556,1265,1287]" pageId="3" pageNumber="266">4</emphasis>
<taxonomicName id="4C034D28E132FFCC2E58FB08822D5423" authority="(Schaus)" authorityName="Sullivan" authorityYear="2010" baseAuthorityName="Sullivan" baseAuthorityYear="2010" box="[564,848,1265,1287]" class="Insecta" family="Geometridae" genus="Campatonema" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="266" phylum="Arthropoda" rank="species" species="lineata">
