<taxonomicName id="4C034D28E132FFCC2CCCFB2880A855C3" authority="Sullivan" authorityName="Sullivan" authorityYear="2010" box="[160,469,1233,1255]" class="Insecta" family="Geometridae" genus="Campatonema" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="266" phylum="Arthropoda" rank="species" species="yanayacua">
