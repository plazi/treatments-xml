<taxonomicName id="4C034D28E132FFCC2ED6FB6882A75583" authority="Sullivan" authorityName="Sullivan" authorityYear="2010" box="[698,986,1169,1191]" class="Insecta" family="Geometridae" genus="Campatonema" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="266" phylum="Arthropoda" rank="species" species="tapantia">
