<quantity box="[305,434,1934,1959]" metricMagnitude="2" metricUnit="m" metricValue="1.575" metricValueMax="1.6" metricValueMin="1.55" pageId="48" pageNumber="49" unit="m" value="157.5" valueMax="160.0" valueMin="155.0">155−160 m</quantity>
