ERICHSON, 1834 (
<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[363,478,1073,1095]" class="Arachnida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="1510" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
, 
<taxonomicName authorityName="Latreille" authorityYear="1802" box="[491,639,1073,1095]" class="Insecta" family="Tenebrionidae" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="1510" phylum="Arthropoda" rank="family">Tenebrionidae</taxonomicName>
) and 
