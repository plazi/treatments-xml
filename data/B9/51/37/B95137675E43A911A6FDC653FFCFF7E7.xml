<taxonomicName class="Insecta" family="Dolichopodidae" genus="Sinosciapus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinosciapus tianmushanus" order="Diptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="species" species="tianmushanus">Sinosciapus tianmushanus</taxonomicName>
