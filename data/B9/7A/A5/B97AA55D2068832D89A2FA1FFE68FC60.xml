<collectingCountry box="[725,792,438,464]" name="Spain" pageId="6" pageNumber="89">Spain</collectingCountry>
, País Valenciano, province Castelló, town Burriana, lat. 
<geoCoordinate box="[151,329,474,501]" degrees="39" direction="north" minutes="52" orientation="latitude" pageId="6" pageNumber="89" precision="1" seconds="32.10" value="39.87558">39°52’32.10” N</geoCoordinate>
&amp; long. 
<geoCoordinate box="[424,581,474,501]" degrees="0" direction="west" minutes="3" orientation="longitude" pageId="6" pageNumber="89" precision="1" seconds="27.87" value="-0.057741668">0°3’27.87” W</geoCoordinate>
