from 
<collectingCountry box="[955,1074,1698,1722]" name="Cape Verde" pageId="6" pageNumber="131">Cape Verde</collectingCountry>
based on a specimen at 
<collectionCode box="[1317,1333,1698,1721]" pageId="6" pageNumber="131">P</collectionCode>
[
<specimenCode box="[1344,1469,1697,1721]" pageId="6" pageNumber="131">P00088662</specimenCode>
</materialsCitation>
] (
<figureCitation box="[832,897,1730,1754]" captionStart="Fig" captionStartId="7.[114,144,1915,1935]" captionTargetId="figure-16@7.[269,1297,276,1891]" captionTargetPageId="7" captionText="Fig. 3.– Holotype of Cyphia stheno Webb at P." figureDoi="http://doi.org/10.5281/zenodo.6327732" httpUri="https://zenodo.org/record/6327732/files/figure.png" pageId="6" pageNumber="131">Fig. 3</figureCitation>
