<paragraph id="8BF836E8FFEBFFEC91F6FF2AFDA5A123" blockId="24.[151,1437,151,1221]" pageId="24" pageNumber="325">
<emphasis id="B933EAFAFFEBFFEC91F6FF2AFE8DA08E" bold="true" box="[199,373,151,176]" pageId="24" pageNumber="325">
<typeStatus id="54FC884AFFEBFFEC91F6FF2AFEFAA08E" box="[199,258,151,176]" pageId="24" pageNumber="325">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54FC884AFFEBFFEC904DFF2AFE1DA08E" box="[380,485,151,176]" pageId="24" pageNumber="325">Holotype</typeStatus>
male (
<collectionCode id="ED56AE2DFFEBFFEC9303FF25FD72A08E" LSID="urn:lsid:biocol.org:col:34871" box="[562,650,152,176]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="24" pageNumber="325">USNM</collectionCode>
