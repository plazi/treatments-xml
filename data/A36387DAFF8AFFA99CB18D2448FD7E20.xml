<taxonomicName id="ECCA4D4FFF8AFFA99F538ABD4D657DD0" box="[885,1042,1470,1495]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="doipuiense">
