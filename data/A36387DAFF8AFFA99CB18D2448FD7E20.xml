<taxonomicName id="ECCA4D4FFF8AFFA99E208ABD4BC87DD0" box="[518,703,1470,1495]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="inthanonense">
