<taxonomicName id="ECCA4D4FFF8AFFA998748ABD4DB37DD0" box="[1106,1220,1470,1495]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="yuphae">
