<quantity id="4CA59B27FF811A79FD23FA95BE3CFA80" box="[688,829,1333,1358]" metricMagnitude="2" metricUnit="m" metricValue="9.095" metricValueMax="14.59" metricValueMin="3.6" pageId="6" pageNumber="357" unit="m" value="909.5" valueMax="1459.0" valueMin="360.0">360–1459 m</quantity>
