<taxonomicName id="4C5D4D41FF811A79FDBCFB4EBFC5FAC8" box="[559,708,1261,1285]" class="Insecta" family="Empididae" genus="Wiedemannia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="357" phylum="Arthropoda" rank="species" species="fallaciosa">
