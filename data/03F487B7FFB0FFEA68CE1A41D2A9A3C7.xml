</taxonomicName>
; 
<taxonomicName id="4C5D4D22FFB0FFEA6B8F1961D6E3A2A7" authority="D'Addabbo Gallo, Grimaldi" authorityName="D'Addabbo Gallo, Grimaldi" class="Heterotardigrada" family="Halechiniscidae" genus="Raiarctus" kingdom="Animalia" order="Arthrotardigrada" pageId="23" pageNumber="424" phylum="Tardigrada" rank="species" species="variabilis">
<emphasis id="B929EAB3FFB0FFEA6B8F1961D2A5A283" box="[982,1210,618,643]" italics="true" pageId="23" pageNumber="424">Raiarctus variabilis</emphasis>
