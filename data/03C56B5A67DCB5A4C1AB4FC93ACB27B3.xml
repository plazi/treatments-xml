</taxonomicName>
and 
<taxonomicName id="4C6CA1CF67DCB5A4C4184DD63D7827EC" box="[1316,1378,1702,1725]" class="Magnoliopsida" family="Polygonaceae" genus="Emex" kingdom="Plantae" order="Polygonales" pageId="393" pageNumber="394" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E67DCB5A4C4184DD63D7827EC" box="[1316,1378,1702,1725]" italics="true" pageId="393" pageNumber="394">Emex</emphasis>
</taxonomicName>
spp. (
<taxonomicName id="4C6CA1CF67DCB5A4C1A24DB9392727B3" box="[158,317,1737,1762]" class="Magnoliopsida" family="Polygonaceae" kingdom="Plantae" order="Polygonales" pageId="393" pageNumber="394" phylum="Tracheophyta" rank="family">Polygonaceae</taxonomicName>
