<taxonomicName id="4C6CA1CF67DCB5A4C5A64DD63CFD27EC" box="[1178,1255,1702,1725]" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Polygonales" pageId="393" pageNumber="394" phylum="Tracheophyta" rank="genus">
