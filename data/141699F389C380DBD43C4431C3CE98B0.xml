<taxonomicName id="2C8E090B32DC28A2BBCE7D9F60D81DE7" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Abies grandis" order="Pinales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="species" species="grandis">Abies grandis</taxonomicName>
