</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFE72775FFFBFD5EE73081FF" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2000" box="[1156,1281,715,739]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="hinkleri">
