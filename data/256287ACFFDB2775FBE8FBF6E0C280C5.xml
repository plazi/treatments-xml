<taxonomicName id="6ACB4D39FFE72775F8EFFD7DE1C1801A" authorityName="Watts &amp; Humphreys" authorityYear="2006" box="[912,1008,751,774]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="raeae">
<emphasis id="9FBFEAA8FFE72775F8EFFD7DE1C1801A" box="[912,1008,751,774]" italics="true" pageId="34" pageNumber="35">L. raeae</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFE72775FF7EFD7DE6F0801A" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="1999" box="[1025,1217,750,774]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="windarraensis">
