<emphasis id="9FBFEAA8FFE72775FE68FD5EE75E81FF" box="[1303,1391,715,739]" italics="true" pageId="34" pageNumber="35">L. leysi</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFE72775FEFAFD5EE329801A" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="masonensis">
