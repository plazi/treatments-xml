</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFE72775F9ABFD5EE19281FF" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" box="[724,931,715,739]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="fridaywellensis">
