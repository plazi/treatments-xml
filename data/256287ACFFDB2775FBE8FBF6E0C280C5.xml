<taxonomicName id="6ACB4D39FFE72775F860FD7DE1B1801A" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="1999" box="[799,896,750,774]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="pulpa">
<emphasis id="9FBFEAA8FFE72775F860FD7DE1B1801A" box="[799,896,750,774]" italics="true" pageId="34" pageNumber="35">L. pulpa</emphasis>
