</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFE72775F906FD7DE13F801A" authorityName="Watts &amp; Humphreys" authorityYear="2009" box="[633,782,750,774]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="ordinarius">
