<paragraph id="F851E4486A5EFFF3FFC0F9C40A8E29F6" blockId="16.[111,647,1515,1700]" pageId="16" pageNumber="925">
<taxonomicName id="3FEE9FCB6A5EFFF3FFC0F9C4098F2971" authorityName="Apostolescu" authorityYear="1955" box="[113,390,1546,1572]" class="Ostracoda" family="Cytherideidae" genus="Clithrocytheridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="16" pageNumber="925" phylum="Arthropoda" rank="species" species="fornicata">
